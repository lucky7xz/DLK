{"textgrid.poem.44563": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Einem Soldaten", "genre": "verse", "period": "N.A.", "pub_year": 1848, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch und erhaben steht des Lebens Baum", "tokens": ["Hoch", "und", "er\u00b7ha\u00b7ben", "steht", "des", "Le\u00b7bens", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und breitet in den Luftkreis seine \u00c4ste,", "tokens": ["Und", "brei\u00b7tet", "in", "den", "Luft\u00b7kreis", "sei\u00b7ne", "\u00c4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Gr\u00fcn und Gold ergl\u00e4nzt der breite Raum,", "tokens": ["In", "Gr\u00fcn", "und", "Gold", "er\u00b7gl\u00e4nzt", "der", "brei\u00b7te", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und singend freun sich ungebetne G\u00e4ste.", "tokens": ["Und", "sin\u00b7gend", "freun", "sich", "un\u00b7ge\u00b7bet\u00b7ne", "G\u00e4s\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von Bl\u00fct und Frucht sind seine Zweige schwer,", "tokens": ["Von", "Bl\u00fct", "und", "Frucht", "sind", "sei\u00b7ne", "Zwei\u00b7ge", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er l\u00e4\u00dft den \u00dcberflu\u00df zu Boden fallen,", "tokens": ["Er", "l\u00e4\u00dft", "den", "\u00dc\u00b7berf\u00b7lu\u00df", "zu", "Bo\u00b7den", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und alles lagert froh sich um ihn her,", "tokens": ["Und", "al\u00b7les", "la\u00b7gert", "froh", "sich", "um", "ihn", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "PRF", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df er Genu\u00df und reiche Labung allen.", "tokens": ["Da\u00df", "er", "Ge\u00b7nu\u00df", "und", "rei\u00b7che", "La\u00b7bung", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "ADJA", "NN", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch nur die eine H\u00e4lfte gl\u00e4nzt im Licht,", "tokens": ["Doch", "nur", "die", "ei\u00b7ne", "H\u00e4lf\u00b7te", "gl\u00e4nzt", "im", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und gilt daher als Baum in jedem Munde,", "tokens": ["Und", "gilt", "da\u00b7her", "als", "Baum", "in", "je\u00b7dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "KOUS", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die zweite H\u00e4lfte sieht dein Auge nicht,", "tokens": ["Die", "zwei\u00b7te", "H\u00e4lf\u00b7te", "sieht", "dein", "Au\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weil sie sich birgt in tiefsten Bodens Grunde.", "tokens": ["Weil", "sie", "sich", "birgt", "in", "tiefs\u00b7ten", "Bo\u00b7dens", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dort saugt sie ein den erdgebornen Saft", "tokens": ["Dort", "saugt", "sie", "ein", "den", "erd\u00b7ge\u00b7bor\u00b7nen", "Saft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und treibt ihn in die lichte, bunte H\u00f6he,", "tokens": ["Und", "treibt", "ihn", "in", "die", "lich\u00b7te", ",", "bun\u00b7te", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie gibt den Halt, des Widerstandes Kraft,", "tokens": ["Sie", "gibt", "den", "Halt", ",", "des", "Wi\u00b7der\u00b7stan\u00b7des", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Damit dem Sturm das Laubdach widerstehe.", "tokens": ["Da\u00b7mit", "dem", "Sturm", "das", "Laub\u00b7dach", "wi\u00b7der\u00b7ste\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So schlie\u00dft sich in sich selbst der stolze Bau,", "tokens": ["So", "schlie\u00dft", "sich", "in", "sich", "selbst", "der", "stol\u00b7ze", "Bau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PRF", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nach oben Fortschritt, Wechsel und das Neue,", "tokens": ["Nach", "o\u00b7ben", "Fort\u00b7schritt", ",", "Wech\u00b7sel", "und", "das", "Neu\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "$,", "NN", "KON", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Wurzel st\u00e4tig, fest und altergrau,", "tokens": ["Die", "Wur\u00b7zel", "st\u00e4\u00b7tig", ",", "fest", "und", "al\u00b7ter\u00b7grau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dasselbe, was beim Menschen hei\u00dft: die Treue.", "tokens": ["Das\u00b7sel\u00b7be", ",", "was", "beim", "Men\u00b7schen", "hei\u00dft", ":", "die", "Treu\u00b7e", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Treu jedem Wort, das Mann dem Manne gab,", "tokens": ["Treu", "je\u00b7dem", "Wort", ",", "das", "Mann", "dem", "Man\u00b7ne", "gab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Treu jener Wahrheit, die mit uns geboren,", "tokens": ["Treu", "je\u00b7ner", "Wahr\u00b7heit", ",", "die", "mit", "uns", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$,", "PRELS", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Lande treu, das Wiege uns und Grab,", "tokens": ["Dem", "Lan\u00b7de", "treu", ",", "das", "Wie\u00b7ge", "uns", "und", "Grab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "PPER", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem F\u00fcrsten treu, dem wir den Eid geschworen.", "tokens": ["Dem", "F\u00fcrs\u00b7ten", "treu", ",", "dem", "wir", "den", "Eid", "ge\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Uns hat der Sturm gesch\u00fcttelt letztes Jahr", "tokens": ["Uns", "hat", "der", "Sturm", "ge\u00b7sch\u00fct\u00b7telt", "letz\u00b7tes", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und abgestreift die Bl\u00fcten und die Fr\u00fcchte,", "tokens": ["Und", "ab\u00b7ge\u00b7streift", "die", "Bl\u00fc\u00b7ten", "und", "die", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An denen nichts als unser D\u00fcnkel wahr,", "tokens": ["An", "de\u00b7nen", "nichts", "als", "un\u00b7ser", "D\u00fcn\u00b7kel", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "KOKOM", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nach kurzer Frist, so ging der Baum zunichte.", "tokens": ["Nach", "kur\u00b7zer", "Frist", ",", "so", "ging", "der", "Baum", "zu\u00b7nich\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Allein die Wurzel hielt. Was Worte leer", "tokens": ["Al\u00b7lein", "die", "Wur\u00b7zel", "hielt", ".", "Was", "Wor\u00b7te", "leer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "$.", "PWS", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geraubt den weisheitstrunknen andern St\u00e4nden,", "tokens": ["Ge\u00b7raubt", "den", "weis\u00b7heits\u00b7trunk\u00b7nen", "an\u00b7dern", "St\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das hielt ein einzger fest. Es war das Heer,", "tokens": ["Das", "hielt", "ein", "einz\u00b7ger", "fest", ".", "Es", "war", "das", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJD", "PTKVZ", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Herzen treu und stark in seinen H\u00e4nden.", "tokens": ["Im", "Her\u00b7zen", "treu", "und", "stark", "in", "sei\u00b7nen", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Sie ri\u00df nicht der Versuchung Stimme fort,", "tokens": ["Sie", "ri\u00df", "nicht", "der", "Ver\u00b7su\u00b7chung", "Stim\u00b7me", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Pflicht entgegen setzten sie dem Wahne,", "tokens": ["Die", "Pflicht", "ent\u00b7ge\u00b7gen", "setz\u00b7ten", "sie", "dem", "Wah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie h\u00f6rten nur des F\u00fchrers ernstes Wort", "tokens": ["Sie", "h\u00f6r\u00b7ten", "nur", "des", "F\u00fch\u00b7rers", "erns\u00b7tes", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sahen nur die unbefleckte Fahne.", "tokens": ["Und", "sa\u00b7hen", "nur", "die", "un\u00b7be\u00b7fleck\u00b7te", "Fah\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "So steht der Baum in neuverj\u00fcngtem Saft,", "tokens": ["So", "steht", "der", "Baum", "in", "neu\u00b7ver\u00b7j\u00fcng\u00b7tem", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den sturmgebeugten Wipfel hoch erhoben,", "tokens": ["Den", "sturm\u00b7ge\u00b7beug\u00b7ten", "Wip\u00b7fel", "hoch", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und halten wird ihn auch der Wurzel Kraft,", "tokens": ["Und", "hal\u00b7ten", "wird", "ihn", "auch", "der", "Wur\u00b7zel", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Beliebts dem Sturm, von anderw\u00e4rts zu toben.", "tokens": ["Be\u00b7liebts", "dem", "Sturm", ",", "von", "an\u00b7der\u00b7w\u00e4rts", "zu", "to\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "APPR", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}