{"textgrid.poem.56267": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Lebewohl", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer sollte fragen: wie's geschah?", "tokens": ["Wer", "soll\u00b7te", "fra\u00b7gen", ":", "wie's", "ge\u00b7schah", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "VVINF", "$.", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es geht auch andern eben so.", "tokens": ["Es", "geht", "auch", "an\u00b7dern", "e\u00b7ben", "so", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich freute mich, als ich dich sah,", "tokens": ["Ich", "freu\u00b7te", "mich", ",", "als", "ich", "dich", "sah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du warst, als du mich sahst, auch froh.", "tokens": ["Du", "warst", ",", "als", "du", "mich", "sahst", ",", "auch", "froh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der erste Gru\u00df, den ich dir bot,", "tokens": ["Der", "ers\u00b7te", "Gru\u00df", ",", "den", "ich", "dir", "bot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Macht' uns auf einmal beide reich;", "tokens": ["Macht'", "uns", "auf", "ein\u00b7mal", "bei\u00b7de", "reich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du wurdest, als ich kam, so rot,", "tokens": ["Du", "wur\u00b7dest", ",", "als", "ich", "kam", ",", "so", "rot", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du wurdest, als ich ging, so bleich.", "tokens": ["Du", "wur\u00b7dest", ",", "als", "ich", "ging", ",", "so", "bleich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun kam ich auch Tag aus, Tag ein,", "tokens": ["Nun", "kam", "ich", "auch", "Tag", "aus", ",", "Tag", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "PTKVZ", "$,", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es ging uns beiden durch den Sinn;", "tokens": ["Es", "ging", "uns", "bei\u00b7den", "durch", "den", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei Regen und bei Sonnenschein", "tokens": ["Bei", "Re\u00b7gen", "und", "bei", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schwand bald der Sommer uns dahin.", "tokens": ["Schwand", "bald", "der", "Som\u00b7mer", "uns", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PPER", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir haben uns die Hand gedr\u00fcckt,", "tokens": ["Wir", "ha\u00b7ben", "uns", "die", "Hand", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um nichts gelacht, um nichts geweint,", "tokens": ["Um", "nichts", "ge\u00b7lacht", ",", "um", "nichts", "ge\u00b7weint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PIS", "VVPP", "$,", "KOUI", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gequ\u00e4lt einander und begl\u00fcckt,", "tokens": ["Ge\u00b7qu\u00e4lt", "ein\u00b7an\u00b7der", "und", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und haben's redlich auch gemeint.", "tokens": ["Und", "ha\u00b7ben's", "red\u00b7lich", "auch", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da kam der Herbst, der Winter gar,", "tokens": ["Da", "kam", "der", "Herbst", ",", "der", "Win\u00b7ter", "gar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schwalbe zog, nach altem Brauch,", "tokens": ["Die", "Schwal\u00b7be", "zog", ",", "nach", "al\u00b7tem", "Brauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und: lieben? \u2013 lieben immerdar? \u2013", "tokens": ["Und", ":", "lie\u00b7ben", "?", "\u2013", "lie\u00b7ben", "im\u00b7mer\u00b7dar", "?", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "$.", "VVINF", "$.", "$(", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es wurde kalt, es fror uns auch.", "tokens": ["Es", "wur\u00b7de", "kalt", ",", "es", "fror", "uns", "auch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich werde gehn ins fremde Land,", "tokens": ["Ich", "wer\u00b7de", "gehn", "ins", "frem\u00b7de", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du sagst mir h\u00f6flich: Lebe wohl!", "tokens": ["Du", "sagst", "mir", "h\u00f6f\u00b7lich", ":", "Le\u00b7be", "wohl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$.", "VVIMP", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich k\u00fcsse h\u00f6flich dir die Hand,", "tokens": ["Ich", "k\u00fcs\u00b7se", "h\u00f6f\u00b7lich", "dir", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nun ist alles, wie es soll.", "tokens": ["Und", "nun", "ist", "al\u00b7les", ",", "wie", "es", "soll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}