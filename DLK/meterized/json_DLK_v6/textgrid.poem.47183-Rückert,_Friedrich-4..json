{"textgrid.poem.47183": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie haben das Herz aus der Brust mir genommen", "tokens": ["Sie", "ha\u00b7ben", "das", "Herz", "aus", "der", "Brust", "mir", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "PPER", "VVPP"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und haben's gelegt in ein Grab;", "tokens": ["Und", "ha\u00b7ben's", "ge\u00b7legt", "in", "ein", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Das Leben, es ist mir abhanden gekommen,", "tokens": ["Das", "Le\u00b7ben", ",", "es", "ist", "mir", "ab\u00b7han\u00b7den", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Es ist mir gegangen hinab.", "tokens": ["Es", "ist", "mir", "ge\u00b7gan\u00b7gen", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Ihr Seufzer beklommen,", "tokens": ["Ihr", "Seuf\u00b7zer", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ihr Augen umschwommen,", "tokens": ["Ihr", "Au\u00b7gen", "um\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Wie seid ihr entkommen?", "tokens": ["Wie", "seid", "ihr", "ent\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Ich gab", "tokens": ["Ich", "gab"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Euch alle ja mit ihr ins Grab.", "tokens": ["Euch", "al\u00b7le", "ja", "mit", "ihr", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADV", "APPR", "PPOSAT", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}