{"textgrid.poem.42111": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich atme Dich mit Sehnsucht, s\u00fc\u00dfer Duft.", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich atme Dich mit Sehnsucht, s\u00fc\u00dfer Duft.", "tokens": ["Ich", "at\u00b7me", "Dich", "mit", "Sehn\u00b7sucht", ",", "s\u00fc\u00b7\u00dfer", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo Du verschwebst, ging aller Fr\u00fchling enden,", "tokens": ["Wo", "Du", "ver\u00b7schwebst", ",", "ging", "al\u00b7ler", "Fr\u00fch\u00b7ling", "en\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo Du verhauchst, da weht von Schatten-W\u00e4nden", "tokens": ["Wo", "Du", "ver\u00b7hauchst", ",", "da", "weht", "von", "Schat\u00b7ten\u00b7W\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herbstlichen Atems die bereifte Luft.", "tokens": ["Herbst\u00b7li\u00b7chen", "A\u00b7tems", "die", "be\u00b7reif\u00b7te", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Ich schmecke Dich mit Andacht, edles Brot.", "tokens": ["Ich", "schme\u00b7cke", "Dich", "mit", "An\u00b7dacht", ",", "ed\u00b7les", "Brot", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo Du gebrichst, gebricht es aller F\u00fclle,", "tokens": ["Wo", "Du", "ge\u00b7brichst", ",", "ge\u00b7bricht", "es", "al\u00b7ler", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo Du ausgehst, da steigt aus ihrer H\u00fclle", "tokens": ["Wo", "Du", "aus\u00b7gehst", ",", "da", "steigt", "aus", "ih\u00b7rer", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von \u00dcberflu\u00df die ungeme\u00dfne Not.", "tokens": ["Von", "\u00dc\u00b7berf\u00b7lu\u00df", "die", "un\u00b7ge\u00b7me\u00df\u00b7ne", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich f\u00fchle Dich mit Angst, geliebter Leib.", "tokens": ["Ich", "f\u00fch\u00b7le", "Dich", "mit", "Angst", ",", "ge\u00b7lieb\u00b7ter", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Dich verlor, die Hand, wird irrer Schwere", "tokens": ["Die", "Dich", "ver\u00b7lor", ",", "die", "Hand", ",", "wird", "ir\u00b7rer", "Schwe\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "ART", "NN", "$,", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tasten ringsum und tasten in die Leere", "tokens": ["Tas\u00b7ten", "ring\u00b7sum", "und", "tas\u00b7ten", "in", "die", "Lee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Nach allen Dings unfa\u00dfbarem Verbleib.", "tokens": ["Nach", "al\u00b7len", "Dings", "un\u00b7fa\u00df\u00b7ba\u00b7rem", "Ver\u00b7bleib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}}, "stanza.4": {"line.1": {"text": "Ich h\u00f6re Dich, o naher Stimme Sang.", "tokens": ["Ich", "h\u00f6\u00b7re", "Dich", ",", "o", "na\u00b7her", "Stim\u00b7me", "Sang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo Du verstummst, wird jeder Laut in Schweigen", "tokens": ["Wo", "Du", "ver\u00b7stummst", ",", "wird", "je\u00b7der", "Laut", "in", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VAFIN", "PIS", "APPR", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hinsterben und vergeblich tief im Neigen", "tokens": ["Hins\u00b7ter\u00b7ben", "und", "ver\u00b7geb\u00b7lich", "tief", "im", "Nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ADJD", "ADJD", "APPRART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Das Ohr sich m\u00fchn nach einem kleinen Klang.", "tokens": ["Das", "Ohr", "sich", "m\u00fchn", "nach", "ei\u00b7nem", "klei\u00b7nen", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich sehe Dich mit Inbrunst, gro\u00dfes Licht,", "tokens": ["Ich", "se\u00b7he", "Dich", "mit", "In\u00b7brunst", ",", "gro\u00b7\u00dfes", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geleucht der Weite, Glanz aus tausend Fernen.", "tokens": ["Ge\u00b7leucht", "der", "Wei\u00b7te", ",", "Glanz", "aus", "tau\u00b7send", "Fer\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo Du verbleichst, kehrt unter blinden Sternen", "tokens": ["Wo", "Du", "ver\u00b7bleichst", ",", "kehrt", "un\u00b7ter", "blin\u00b7den", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Dunkel das verl\u00f6schende Gesicht.", "tokens": ["In", "Dun\u00b7kel", "das", "ver\u00b7l\u00f6\u00b7schen\u00b7de", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}