{"dta.poem.21291": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "XiII .  \n  Die Schule.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1752", "urn": "urn:nbn:de:kobv:b4-200905199117", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch tiefe Seufzer bl\u00f6der Lust", "tokens": ["Durch", "tie\u00b7fe", "Seuf\u00b7zer", "bl\u00f6\u00b7der", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erkl\u00e4rte Damie alle Triebe", "tokens": ["Er\u00b7kl\u00e4r\u00b7te", "Da\u00b7mie", "al\u00b7le", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Seiner Liebe;", "tokens": ["Sei\u00b7ner", "Lie\u00b7be", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Doch r\u00fchrt er nicht der Sch\u00f6nen Brust.", "tokens": ["Doch", "r\u00fchrt", "er", "nicht", "der", "Sch\u00f6\u00b7nen", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es konnt ihm durch sein Gold ja gl\u00fccken;", "tokens": ["Es", "konnt", "ihm", "durch", "sein", "Gold", "ja", "gl\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch spart\u2019 er dieses, und verlohr:", "tokens": ["Doch", "spart'", "er", "die\u00b7ses", ",", "und", "ver\u00b7lohr", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O der Thor!", "tokens": ["O", "der", "Thor", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Man mu\u00df ihn in die Schule schicken.", "tokens": ["Man", "mu\u00df", "ihn", "in", "die", "Schu\u00b7le", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach liebte meine Phyllis mich!", "tokens": ["Ach", "lieb\u00b7te", "mei\u00b7ne", "Phyl\u00b7lis", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seufzt Damon, seine Z\u00e4rtlichkeiten", "tokens": ["Seufzt", "Da\u00b7mon", ",", "sei\u00b7ne", "Z\u00e4rt\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Anzudeuten.", "tokens": ["An\u00b7zu\u00b7deu\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Und Phyllis sagt: Erkl\u00e4re dich!", "tokens": ["Und", "Phyl\u00b7lis", "sagt", ":", "Er\u00b7kl\u00e4\u00b7re", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Allein, bey ihren s\u00fcssen Blicken,", "tokens": ["Al\u00b7lein", ",", "bey", "ih\u00b7ren", "s\u00fcs\u00b7sen", "Bli\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bringt Damon weiter nichts hervor:", "tokens": ["Bringt", "Da\u00b7mon", "wei\u00b7ter", "nichts", "her\u00b7vor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O der Thor!", "tokens": ["O", "der", "Thor", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Man mu\u00df ihn in die Schule schicken.", "tokens": ["Man", "mu\u00df", "ihn", "in", "die", "Schu\u00b7le", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Am Abend weid\u2019 ich bey dem Bach;", "tokens": ["Am", "A\u00b7bend", "weid'", "ich", "bey", "dem", "Bach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Polydor! scherzt Adelheide;", "tokens": ["Mein", "Po\u00b7ly\u00b7dor", "!", "scherzt", "A\u00b7del\u00b7hei\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ich weide,", "tokens": ["Wo", "ich", "wei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Da, rath ich, schleiche mir nicht nach.", "tokens": ["Da", ",", "rath", "ich", ",", "schlei\u00b7che", "mir", "nicht", "nach", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie nicht so str\u00e4flich zu ber\u00fccken,", "tokens": ["Sie", "nicht", "so", "str\u00e4f\u00b7lich", "zu", "be\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verspricht und h\u00e4lt ihr Polydor:", "tokens": ["Ver\u00b7spricht", "und", "h\u00e4lt", "ihr", "Po\u00b7ly\u00b7dor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O der Thor!", "tokens": ["O", "der", "Thor", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Man mu\u00df ihn in die Schule schicken.", "tokens": ["Man", "mu\u00df", "ihn", "in", "die", "Schu\u00b7le", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Schwindel, aber nur zum Spa\u00df,", "tokens": ["Ein", "Schwin\u00b7del", ",", "a\u00b7ber", "nur", "zum", "Spa\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Befiel Dorinen, als ihr Lehrer", "tokens": ["Be\u00b7fiel", "Do\u00b7ri\u00b7nen", ",", "als", "ihr", "Leh\u00b7rer"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Verehrer,", "tokens": ["Und", "Ver\u00b7eh\u00b7rer", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Der steife Cleon, bey ihr sa\u00df.", "tokens": ["Der", "stei\u00b7fe", "Cle\u00b7on", ",", "bey", "ihr", "sa\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Unwissend selbst sie zu erquicken", "tokens": ["Un\u00b7wis\u00b7send", "selbst", "sie", "zu", "er\u00b7qui\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Rief er die Mutter schnell hervor:", "tokens": ["Rief", "er", "die", "Mut\u00b7ter", "schnell", "her\u00b7vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O der Thor!", "tokens": ["O", "der", "Thor", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Man mu\u00df ihn in die Schule schicken.", "tokens": ["Man", "mu\u00df", "ihn", "in", "die", "Schu\u00b7le", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Melander, den die Schreibsucht qu\u00e4lt,", "tokens": ["Me\u00b7lan\u00b7der", ",", "den", "die", "Schreib\u00b7sucht", "qu\u00e4lt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Glaubt, weil der Reim ihm treu verbleibet,", "tokens": ["Glaubt", ",", "weil", "der", "Reim", "ihm", "treu", "ver\u00b7blei\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df er schreibet,", "tokens": ["Da\u00df", "er", "schrei\u00b7bet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Und da\u00df ihm keine Muse fehlt.", "tokens": ["Und", "da\u00df", "ihm", "kei\u00b7ne", "Mu\u00b7se", "fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch er kann den Apoll entz\u00fccken;", "tokens": ["Auch", "er", "kann", "den", "A\u00b7poll", "ent\u00b7z\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Auch er singt mit in seinem Chor:", "tokens": ["Auch", "er", "singt", "mit", "in", "sei\u00b7nem", "Chor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "O der Thor!", "tokens": ["O", "der", "Thor", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Man mu\u00df ihn in die Schule schicken.", "tokens": ["Man", "mu\u00df", "ihn", "in", "die", "Schu\u00b7le", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein Witzling liest den Arouet,", "tokens": ["Ein", "Witz\u00b7ling", "liest", "den", "Ar\u00b7ou\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und r\u00e4th ihm, Worte, Reime, Zeilen", "tokens": ["Und", "r\u00e4\u00b7th", "ihm", ",", "Wor\u00b7te", ",", "Rei\u00b7me", ",", "Zei\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mehr zu feilen,", "tokens": ["Mehr", "zu", "fei\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Vor allen in dem Mahomet.", "tokens": ["Vor", "al\u00b7len", "in", "dem", "Ma\u00b7ho\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Wie \u00fcbt er sich an Meisterst\u00fccken!", "tokens": ["Wie", "\u00fcbt", "er", "sich", "an", "Meis\u00b7ter\u00b7st\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie steigt sein leichter Ruhm empor:", "tokens": ["Wie", "steigt", "sein", "leich\u00b7ter", "Ruhm", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O der Thor!", "tokens": ["O", "der", "Thor", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Man mu\u00df ihn in die Schule schicken.", "tokens": ["Man", "mu\u00df", "ihn", "in", "die", "Schu\u00b7le", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}