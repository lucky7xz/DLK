{"textgrid.poem.65756": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "30", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich in schweigender Gedanken Rat", "tokens": ["Wenn", "ich", "in", "schwei\u00b7gen\u00b7der", "Ge\u00b7dan\u00b7ken", "Rat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erinnrung des Vergangnen traulich lade,", "tokens": ["E\u00b7rinn\u00b7rung", "des", "Ver\u00b7gang\u00b7nen", "trau\u00b7lich", "la\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Beseufzend was entflohn mir nie mehr naht,", "tokens": ["Be\u00b7seuf\u00b7zend", "was", "ent\u00b7flohn", "mir", "nie", "mehr", "naht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PWS", "VVFIN", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Neu klagend alte Weh'n versunkner Lebenspfade:", "tokens": ["Neu", "kla\u00b7gend", "al\u00b7te", "Weh'n", "ver\u00b7sun\u00b7kner", "Le\u00b7ben\u00b7spfa\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Dann netz' ich wohl versiechte Augenlider", "tokens": ["Dann", "netz'", "ich", "wohl", "ver\u00b7siech\u00b7te", "Au\u00b7gen\u00b7li\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Um teure Freund' in Todesnacht geh\u00fcllt;", "tokens": ["Um", "teu\u00b7re", "Freund'", "in", "To\u00b7des\u00b7nacht", "ge\u00b7h\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es weinen, l\u00e4ngst erstickt, der Liebe Schmerzen wieder,", "tokens": ["Es", "wei\u00b7nen", ",", "l\u00e4ngst", "er\u00b7stickt", ",", "der", "Lie\u00b7be", "Schmer\u00b7zen", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "ADV", "VVPP", "$,", "ART", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Gram um manch dahingeschwunden Bild.", "tokens": ["Der", "Gram", "um", "manch", "da\u00b7hin\u00b7ge\u00b7schwun\u00b7den", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dann kann ich leiden um vergangnes Leid,", "tokens": ["Dann", "kann", "ich", "lei\u00b7den", "um", "ver\u00b7gang\u00b7nes", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die tr\u00fcbe Summe vorbeklagter Klagen", "tokens": ["Die", "tr\u00fc\u00b7be", "Sum\u00b7me", "vor\u00b7be\u00b7klag\u00b7ter", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Von Weh zu Weh ziehn mit Betr\u00fcbsamkeit,", "tokens": ["Von", "Weh", "zu", "Weh", "ziehn", "mit", "Be\u00b7tr\u00fcb\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Sie zahlend wie noch niemals abgetragen.", "tokens": ["Sie", "zah\u00b7lend", "wie", "noch", "nie\u00b7mals", "ab\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KOKOM", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Doch, teurer Freund! gedenk' ich dein dabei,", "tokens": ["Doch", ",", "teu\u00b7rer", "Freund", "!", "ge\u00b7denk'", "ich", "dein", "da\u00b7bei", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$.", "VVFIN", "PPER", "PPOSAT", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ersetzt ist alles, und ich atme frei.", "tokens": ["Er\u00b7setzt", "ist", "al\u00b7les", ",", "und", "ich", "at\u00b7me", "frei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "$,", "KON", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}