{"dta.poem.18936": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "15.  \n  Von H: Gl\u00e4tzlin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df jhn das alter nicht graw mach/", "tokens": ["Da\u00df", "jhn", "das", "al\u00b7ter", "nicht", "graw", "mach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "PTKNEG", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weisst Gl\u00e4tzlin ein bewehrte sach/", "tokens": ["Weisst", "Gl\u00e4tz\u00b7lin", "ein", "be\u00b7wehr\u00b7te", "sach", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein artzney voll geheimer tugent:", "tokens": ["Ein", "artz\u00b7ney", "voll", "ge\u00b7hei\u00b7mer", "tu\u00b7gent", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er ist kahl worden in der jugent.", "tokens": ["Er", "ist", "kahl", "wor\u00b7den", "in", "der", "ju\u00b7gent", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VAPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}