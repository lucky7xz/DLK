{"dta.poem.7129": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In welch empfindliches Vergn\u00fcgen kann uns nicht", "tokens": ["In", "welch", "emp\u00b7find\u00b7li\u00b7ches", "Ver\u00b7gn\u00fc\u00b7gen", "kann", "uns", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN", "VMFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein angenehmer Traum versetzen!", "tokens": ["Ein", "an\u00b7ge\u00b7neh\u00b7mer", "Traum", "ver\u00b7set\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kein w\u00fcrckliches Erg\u00f6tzen", "tokens": ["Kein", "w\u00fcrck\u00b7li\u00b7ches", "Er\u00b7g\u00f6t\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durchdringt die Seele so. Ein solches Freuden-Licht,", "tokens": ["Durch\u00b7dringt", "die", "See\u00b7le", "so", ".", "Ein", "sol\u00b7ches", "Freu\u00b7den\u00b7Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$.", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als wie uns offt im Traum bestrahlet,", "tokens": ["Als", "wie", "uns", "offt", "im", "Traum", "be\u00b7strah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bestrahlt uns wachend nie. Ein \u00fcberirdisch Wesen", "tokens": ["Be\u00b7strahlt", "uns", "wa\u00b7chend", "nie", ".", "Ein", "\u00fc\u00b7be\u00b7rir\u00b7disch", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "$.", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wird uns im Traum bisweilen vorgemahlet,", "tokens": ["Wird", "uns", "im", "Traum", "bis\u00b7wei\u00b7len", "vor\u00b7ge\u00b7mah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zumahl, wenn unser Leib durch Kranckheit abgezehrt", "tokens": ["Zu\u00b7mahl", ",", "wenn", "un\u00b7ser", "Leib", "durch", "Kran\u00b7ck\u00b7heit", "ab\u00b7ge\u00b7zehrt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Die zur Gesundheit dient, nun anf\u00e4ngt zu genesen,", "tokens": ["Die", "zur", "Ge\u00b7sund\u00b7heit", "dient", ",", "nun", "an\u00b7f\u00e4ngt", "zu", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+--+--", "measure": "iambic.penta.invert"}, "line.10": {"text": "Und die Gefahr entweicht und aufgeh\u00f6rt,", "tokens": ["Und", "die", "Ge\u00b7fahr", "ent\u00b7weicht", "und", "auf\u00b7ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der Kranckheit-Wust verjagt, vertrieben, ausgef\u00fchrt,", "tokens": ["Der", "Kranck\u00b7heit\u00b7Wust", "ver\u00b7jagt", ",", "ver\u00b7trie\u00b7ben", ",", "aus\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Seele, da der Feind besieget, triumphiert.", "tokens": ["Die", "See\u00b7le", ",", "da", "der", "Feind", "be\u00b7sie\u00b7get", ",", "tri\u00b7um\u00b7phiert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hiedurch nun, da das Blut aufs neue circuliert;", "tokens": ["Hie\u00b7durch", "nun", ",", "da", "das", "Blut", "aufs", "neu\u00b7e", "cir\u00b7cu\u00b7liert", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "$,", "KOUS", "ART", "NN", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wird C\u00f6rper, Seel und Geist aufs lieblichste ger\u00fchrt,", "tokens": ["Wird", "C\u00f6r\u00b7per", ",", "Seel", "und", "Geist", "aufs", "lieb\u00b7lichs\u00b7te", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$,", "NN", "KON", "NN", "APPRART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und inniglich erfrischt, erquickt, ja fast verkl\u00e4rt:", "tokens": ["Und", "in\u00b7nig\u00b7lich", "er\u00b7frischt", ",", "er\u00b7quickt", ",", "ja", "fast", "ver\u00b7kl\u00e4rt", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "VVPP", "$,", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Es wird auf solche Art der Seelen", "tokens": ["Es", "wird", "auf", "sol\u00b7che", "Art", "der", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ein \u00fcberschwengliches Vergn\u00fcgen nimmer fehlen.", "tokens": ["Ein", "\u00fc\u00b7bersc\u00b7hweng\u00b7li\u00b7ches", "Ver\u00b7gn\u00fc\u00b7gen", "nim\u00b7mer", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nur liegt es blo\u00df daran; ein solches Tr\u00e4umen w\u00e4hrt", "tokens": ["Nur", "liegt", "es", "blo\u00df", "da\u00b7ran", ";", "ein", "sol\u00b7ches", "Tr\u00e4u\u00b7men", "w\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PAV", "$.", "ART", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nur gar zu kurtze Zeit.", "tokens": ["Nur", "gar", "zu", "kurt\u00b7ze", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Des laugen Schlaffs, des Todes Traum hingegen,", "tokens": ["Des", "lau\u00b7gen", "Schlaffs", ",", "des", "To\u00b7des", "Traum", "hin\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Wird nicht mehr Lieblichkeit und W\u00fcrcklichkeit nur hegen;", "tokens": ["Wird", "nicht", "mehr", "Lieb\u00b7lich\u00b7keit", "und", "W\u00fcrck\u00b7lich\u00b7keit", "nur", "he\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PIAT", "NN", "KON", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Nein, auch von l\u00e4nger Daur und seeliger noch seyn.", "tokens": ["Nein", ",", "auch", "von", "l\u00e4n\u00b7ger", "Daur", "und", "see\u00b7li\u00b7ger", "noch", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "APPR", "ADJA", "NN", "KON", "ADJD", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}