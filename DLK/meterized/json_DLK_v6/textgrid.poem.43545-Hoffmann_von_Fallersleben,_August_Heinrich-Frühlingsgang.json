{"textgrid.poem.43545": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsgang", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Papa will ins Webicht", "tokens": ["Pa\u00b7pa", "will", "ins", "We\u00b7bicht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Kleiner Franz will auch mit geh'n.", "tokens": ["Klei\u00b7ner", "Franz", "will", "auch", "mit", "geh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["NE", "NE", "VMFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jetzo zieh' ich Stiefel an,", "tokens": ["Jet\u00b7zo", "zieh'", "ich", "Stie\u00b7fel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sollst seh'n, da\u00df ich auch geh'n kann.", "tokens": ["Sollst", "seh'n", ",", "da\u00df", "ich", "auch", "geh'n", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber sag' mir doch, Papa!", "tokens": ["A\u00b7ber", "sag'", "mir", "doch", ",", "Pa\u00b7pa", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sag' mir, was machen wir da?", "tokens": ["Sag'", "mir", ",", "was", "ma\u00b7chen", "wir", "da", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "++-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "\u00bbwollen seh'n, ob der Wald ist gr\u00fcn,", "tokens": ["\u00bb", "wol\u00b7len", "seh'n", ",", "ob", "der", "Wald", "ist", "gr\u00fcn", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "VVINF", "$,", "KOUS", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Seh'n, ob die lieben Blumen bl\u00fch'n.\u00ab", "tokens": ["Seh'n", ",", "ob", "die", "lie\u00b7ben", "Blu\u00b7men", "bl\u00fch'", "n.", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "abbreviation", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "NE", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Mach' doch geschwind, Papa!", "tokens": ["Mach'", "doch", "ge\u00b7schwind", ",", "Pa\u00b7pa", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ei ich wollt', ich w\u00e4r' schon da!", "tokens": ["Ei", "ich", "wollt'", ",", "ich", "w\u00e4r'", "schon", "da", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VMFIN", "$,", "PPER", "VAFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}