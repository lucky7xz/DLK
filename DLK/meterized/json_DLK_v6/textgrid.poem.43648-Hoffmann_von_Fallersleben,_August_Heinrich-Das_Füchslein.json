{"textgrid.poem.43648": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das F\u00fcchslein", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer ist in unser H\u00fchnerhaus", "tokens": ["Wer", "ist", "in", "un\u00b7ser", "H\u00fch\u00b7ner\u00b7haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eben doch gegangen?", "tokens": ["E\u00b7ben", "doch", "ge\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wer will sich dort zu einem Schmaus", "tokens": ["Wer", "will", "sich", "dort", "zu", "ei\u00b7nem", "Schmaus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PRF", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eine Henne fangen?", "tokens": ["Ei\u00b7ne", "Hen\u00b7ne", "fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "F\u00fcchslein, F\u00fcchslein, mach dich fort!", "tokens": ["F\u00fcchs\u00b7lein", ",", "F\u00fcchs\u00b7lein", ",", "mach", "dich", "fort", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcchslein, sag, was willst du dort", "tokens": ["F\u00fcchs\u00b7lein", ",", "sag", ",", "was", "willst", "du", "dort"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "PWS", "VMFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch bei unsern H\u00fchnern?", "tokens": ["Doch", "bei", "un\u00b7sern", "H\u00fch\u00b7nern", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Das F\u00fcchslein aber bleibet da", "tokens": ["Das", "F\u00fcchs\u00b7lein", "a\u00b7ber", "blei\u00b7bet", "da"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ohne Furcht und Bangen;", "tokens": ["Oh\u00b7ne", "Furcht", "und", "Ban\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch eh' er's selber sich versah,", "tokens": ["Doch", "eh'", "er's", "sel\u00b7ber", "sich", "ver\u00b7sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird mein Fuchs gefangen.", "tokens": ["Wird", "mein", "Fuchs", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "F\u00fcchslein, F\u00fcchslein, bangt dir nicht?", "tokens": ["F\u00fcchs\u00b7lein", ",", "F\u00fcchs\u00b7lein", ",", "bangt", "dir", "nicht", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcchslein, jetzo vors Gericht!", "tokens": ["F\u00fcchs\u00b7lein", ",", "jet\u00b7zo", "vors", "Ge\u00b7richt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Denn du bist gefangen.", "tokens": ["Denn", "du", "bist", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Dem Fuchse ward zur Stelle dort", "tokens": ["Dem", "Fuch\u00b7se", "ward", "zur", "Stel\u00b7le", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gleich sein Recht verliehen:", "tokens": ["Gleich", "sein", "Recht", "ver\u00b7lie\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Er mu\u00dfte nach des Richters Wort", "tokens": ["Er", "mu\u00df\u00b7te", "nach", "des", "Rich\u00b7ters", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seinen Pelz ausziehen.", "tokens": ["Sei\u00b7nen", "Pelz", "aus\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "F\u00fcchslein, F\u00fcchslein, wohlgemuth!", "tokens": ["F\u00fcchs\u00b7lein", ",", "F\u00fcchs\u00b7lein", ",", "wohl\u00b7ge\u00b7muth", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcchslein, traun, es ruht sich gut", "tokens": ["F\u00fcchs\u00b7lein", ",", "traun", ",", "es", "ruht", "sich", "gut"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVINF", "$,", "PPER", "VVFIN", "PRF", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf des K\u00fcrschners Stange!", "tokens": ["Auf", "des", "K\u00fcrschners", "Stan\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}