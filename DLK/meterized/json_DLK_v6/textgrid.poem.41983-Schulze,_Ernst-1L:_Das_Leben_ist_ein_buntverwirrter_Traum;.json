{"textgrid.poem.41983": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Leben ist ein buntverwirrter Traum;", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Leben ist ein buntverwirrter Traum;", "tokens": ["Das", "Le\u00b7ben", "ist", "ein", "bunt\u00b7ver\u00b7wirr\u00b7ter", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Dunkel liegt die Zeit, die uns entschwunden,", "tokens": ["Im", "Dun\u00b7kel", "liegt", "die", "Zeit", ",", "die", "uns", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Schleier deckt der Zukunft ferne Stunden,", "tokens": ["Ein", "Schlei\u00b7er", "deckt", "der", "Zu\u00b7kunft", "fer\u00b7ne", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und selbst das Jetzt erkennt die Seele kaum.", "tokens": ["Und", "selbst", "das", "Jetzt", "er\u00b7kennt", "die", "See\u00b7le", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADV", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verworren fliehn mit ungewissem Schweben", "tokens": ["Ver\u00b7wor\u00b7ren", "fliehn", "mit", "un\u00b7ge\u00b7wis\u00b7sem", "Schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Daseyns Bilder unserm Blick vorbei;", "tokens": ["Des", "Da\u00b7seyns", "Bil\u00b7der", "un\u00b7serm", "Blick", "vor\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wir w\u00e4hlen nicht was gut und n\u00fctzlich sey,", "tokens": ["Wir", "w\u00e4h\u00b7len", "nicht", "was", "gut", "und", "n\u00fctz\u00b7lich", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Kein festes Ziel entdeckt sich unserm Streben;", "tokens": ["Kein", "fes\u00b7tes", "Ziel", "ent\u00b7deckt", "sich", "un\u00b7serm", "Stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Zufrieden mit dem bunten Mancherley,", "tokens": ["Zu\u00b7frie\u00b7den", "mit", "dem", "bun\u00b7ten", "Man\u00b7cher\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Womit Geschick und Zufall uns umweben,", "tokens": ["Wo\u00b7mit", "Ge\u00b7schick", "und", "Zu\u00b7fall", "uns", "um\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Durchirren wir, gleich Tr\u00e4umenden, das Leben,", "tokens": ["Durch\u00b7ir\u00b7ren", "wir", ",", "gleich", "Tr\u00e4u\u00b7men\u00b7den", ",", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADV", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Bald auf dem Fittig s\u00fc\u00dfer Schw\u00e4rmerei,", "tokens": ["Bald", "auf", "dem", "Fit\u00b7tig", "s\u00fc\u00b7\u00dfer", "Schw\u00e4r\u00b7me\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Bald stumm und ernst und bald mit scheuem Beben,", "tokens": ["Bald", "stumm", "und", "ernst", "und", "bald", "mit", "scheu\u00b7em", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und f\u00fchlen erst, wenn aus der W\u00fcstenei", "tokens": ["Und", "f\u00fch\u00b7len", "erst", ",", "wenn", "aus", "der", "W\u00fcs\u00b7te\u00b7nei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der Welt uns sch\u00f6n're Genien erheben,", "tokens": ["Der", "Welt", "uns", "sch\u00f6n'\u00b7re", "Ge\u00b7ni\u00b7en", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Das Spiel sey aus und unser Traum vorbei.", "tokens": ["Das", "Spiel", "sey", "aus", "und", "un\u00b7ser", "Traum", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sobald der Mensch des Lebens Hauch empfindet,", "tokens": ["So\u00b7bald", "der", "Mensch", "des", "Le\u00b7bens", "Hauch", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bem\u00fcht er sich in jenes Buch zu sehn,", "tokens": ["Be\u00b7m\u00fcht", "er", "sich", "in", "je\u00b7nes", "Buch", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das ihm den Zweck der bunten Tr\u00e4ume k\u00fcndet;", "tokens": ["Das", "ihm", "den", "Zweck", "der", "bun\u00b7ten", "Tr\u00e4u\u00b7me", "k\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er sucht nach Licht und w\u00e4hnt es zu ersp\u00e4hn.", "tokens": ["Er", "sucht", "nach", "Licht", "und", "w\u00e4hnt", "es", "zu", "er\u00b7sp\u00e4hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sein Geist verlacht die Fessel, die ihn bindet,", "tokens": ["Sein", "Geist", "ver\u00b7lacht", "die", "Fes\u00b7sel", ",", "die", "ihn", "bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Schon glaubt er den verborgnen Rath ergr\u00fcndet,", "tokens": ["Schon", "glaubt", "er", "den", "ver\u00b7borg\u00b7nen", "Rath", "er\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und hascht im Wahn die Wahrheit schon am Saum:", "tokens": ["Und", "hascht", "im", "Wahn", "die", "Wahr\u00b7heit", "schon", "am", "Saum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Doch ach, umsonst! der falsche Schein entschwindet,", "tokens": ["Doch", "ach", ",", "um\u00b7sonst", "!", "der", "fal\u00b7sche", "Schein", "ent\u00b7schwin\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ADV", "$.", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und was er sieht, es ist ein neuer Traum!", "tokens": ["Und", "was", "er", "sieht", ",", "es", "ist", "ein", "neu\u00b7er", "Traum", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zu gl\u00fccklich ist, wer auf dem Pilgerwege", "tokens": ["Zu", "gl\u00fcck\u00b7lich", "ist", ",", "wer", "auf", "dem", "Pil\u00b7ger\u00b7we\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VAFIN", "$,", "PWS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mehr Sonnenschein als wilden Sturm empfing,", "tokens": ["Mehr", "Son\u00b7nen\u00b7schein", "als", "wil\u00b7den", "Sturm", "emp\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer h\u00e4ufiger durch bl\u00fchende Gehege", "tokens": ["Wer", "h\u00e4u\u00b7fi\u00b7ger", "durch", "bl\u00fc\u00b7hen\u00b7de", "Ge\u00b7he\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Als durch den Sand verdorrter W\u00fcsten ging;", "tokens": ["Als", "durch", "den", "Sand", "ver\u00b7dorr\u00b7ter", "W\u00fcs\u00b7ten", "ging", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wem in dem Buch, wo die geno\u00dfnen Freuden", "tokens": ["Wem", "in", "dem", "Buch", ",", "wo", "die", "ge\u00b7no\u00df\u00b7nen", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verzeichnet stehn, kein g\u00e4nzlich leerer Raum", "tokens": ["Ver\u00b7zeich\u00b7net", "stehn", ",", "kein", "g\u00e4nz\u00b7lich", "lee\u00b7rer", "Raum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "VVINF", "$,", "PIAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Entgegenstarrt, und wer beim sp\u00e4ten Scheiden", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7starrt", ",", "und", "wer", "beim", "sp\u00e4\u00b7ten", "Schei\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "PWS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Noch rufen kann: Es war ein sch\u00f6ner Traum!", "tokens": ["Noch", "ru\u00b7fen", "kann", ":", "Es", "war", "ein", "sch\u00f6\u00b7ner", "Traum", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Allein verzeih die wehmuthsvolle Mine,", "tokens": ["Al\u00b7lein", "ver\u00b7zeih", "die", "weh\u00b7muths\u00b7vol\u00b7le", "Mi\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit welcher jetzt die Muse dir erscheint,", "tokens": ["Mit", "wel\u00b7cher", "jetzt", "die", "Mu\u00b7se", "dir", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als st\u00e4nde sie auf einer Trauerb\u00fchne,", "tokens": ["Als", "st\u00e4n\u00b7de", "sie", "auf", "ei\u00b7ner", "Trau\u00b7er\u00b7b\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um die ein Schwarm von bangen H\u00f6rern weint.", "tokens": ["Um", "die", "ein", "Schwarm", "von", "ban\u00b7gen", "H\u00f6\u00b7rern", "weint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Fort mit dem Ernst! Im holden Feenlande,", "tokens": ["Fort", "mit", "dem", "Ernst", "!", "Im", "hol\u00b7den", "Fe\u00b7en\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Wo noch der Lenz uns Rosenkr\u00e4nze flicht,", "tokens": ["Wo", "noch", "der", "Lenz", "uns", "Ro\u00b7sen\u00b7kr\u00e4n\u00b7ze", "flicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ist Sorg' und Gram die schlimmste Contrebande,", "tokens": ["Ist", "Sor\u00b7g'", "und", "Gram", "die", "schlimms\u00b7te", "Cont\u00b7re\u00b7ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und d\u00fcstrer Ernst im gr\u00e4mlichen Gewande", "tokens": ["Und", "d\u00fcst\u00b7rer", "Ernst", "im", "gr\u00e4m\u00b7li\u00b7chen", "Ge\u00b7wan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ein Prediger, der in der W\u00fcste spricht.", "tokens": ["Ein", "Pre\u00b7di\u00b7ger", ",", "der", "in", "der", "W\u00fcs\u00b7te", "spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Was k\u00fcmmern uns die finstern Gr\u00fcbeleyen,", "tokens": ["Was", "k\u00fcm\u00b7mern", "uns", "die", "fins\u00b7tern", "Gr\u00fc\u00b7be\u00b7le\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Womit der Mensch den Keim der Lust zerst\u00f6rt?", "tokens": ["Wo\u00b7mit", "der", "Mensch", "den", "Keim", "der", "Lust", "zer\u00b7st\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Mag sich in Thor des finstern Mi\u00dfmuths freuen,", "tokens": ["Mag", "sich", "in", "Thor", "des", "fins\u00b7tern", "Mi\u00df\u00b7muths", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Mag er das Gl\u00fcck, als w\u00e4r' es S\u00fcnde, scheuen,", "tokens": ["Mag", "er", "das", "Gl\u00fcck", ",", "als", "w\u00e4r'", "es", "S\u00fcn\u00b7de", ",", "scheu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,", "KOKOM", "VAFIN", "PPER", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Wer Grillen sucht, der ist der Grillen werth;", "tokens": ["Wer", "Gril\u00b7len", "sucht", ",", "der", "ist", "der", "Gril\u00b7len", "werth", ";"], "t