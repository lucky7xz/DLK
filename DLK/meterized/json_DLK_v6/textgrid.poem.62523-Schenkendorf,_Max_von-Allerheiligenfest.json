{"textgrid.poem.62523": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Allerheiligenfest", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00e4umt' ich ewig doch den Traum,", "tokens": ["Tr\u00e4umt'", "ich", "e\u00b7wig", "doch", "den", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der mir diese Nacht erschienen,", "tokens": ["Der", "mir", "die\u00b7se", "Nacht", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00e4h' ich offen stets den Raum,", "tokens": ["S\u00e4h'", "ich", "of\u00b7fen", "stets", "den", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die Himmelsmaien gr\u00fcnen!", "tokens": ["Wo", "die", "Him\u00b7mels\u00b7mai\u00b7en", "gr\u00fc\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Garten, der hier bl\u00fcht,", "tokens": ["Gar\u00b7ten", ",", "der", "hier", "bl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "B\u00e4chlein, die entspringen,", "tokens": ["B\u00e4ch\u00b7lein", ",", "die", "ent\u00b7sprin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wunderbares Lied,", "tokens": ["Wun\u00b7der\u00b7ba\u00b7res", "Lied", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Das ich h\u00f6rte klingen.", "tokens": ["Das", "ich", "h\u00f6r\u00b7te", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Blumen, roth und wei\u00df und blau,", "tokens": ["Blu\u00b7men", ",", "roth", "und", "wei\u00df", "und", "blau", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "VVFIN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hatten diese Flur umzogen", "tokens": ["Hat\u00b7ten", "die\u00b7se", "Flur", "um\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "NN", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die allerreinste Frau", "tokens": ["Und", "die", "al\u00b7ler\u00b7reins\u00b7te", "Frau"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sa\u00df auf einem Sternenbogen;", "tokens": ["Sa\u00df", "auf", "ei\u00b7nem", "Ster\u00b7nen\u00b7bo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Englein schwebten da", "tokens": ["En\u00b7glein", "schweb\u00b7ten", "da"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Gleich wie Bl\u00fctenflocken;", "tokens": ["Gleich", "wie", "Bl\u00fc\u00b7ten\u00b7flo\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "L\u00e4uten fern und nah", "tokens": ["L\u00e4u\u00b7ten", "fern", "und", "nah"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Wie von hellen Glocken!", "tokens": ["Wie", "von", "hel\u00b7len", "Glo\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Priester, M\u00f6nch und Ritterheld", "tokens": ["Pries\u00b7ter", ",", "M\u00f6nch", "und", "Rit\u00b7ter\u00b7held"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gingen traulich auf und nieder;", "tokens": ["Gin\u00b7gen", "trau\u00b7lich", "auf", "und", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den B\u00fcschen, auf dem Feld", "tokens": ["In", "den", "B\u00fc\u00b7schen", ",", "auf", "dem", "Feld"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sa\u00dfen Frauen hin und wieder;", "tokens": ["Sa\u00b7\u00dfen", "Frau\u00b7en", "hin", "und", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "KON", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kindlein fromm und mild", "tokens": ["Kin\u00b7dlein", "fromm", "und", "mild"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Sah ich Blumen pfl\u00fccken,", "tokens": ["Sah", "ich", "Blu\u00b7men", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Bald ein Kreuzesbild,", "tokens": ["Bald", "ein", "Kreu\u00b7zes\u00b7bild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Bald ihr Haar zu schm\u00fccken.", "tokens": ["Bald", "ihr", "Haar", "zu", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Jeder trug ein wei\u00dfes Kleid;", "tokens": ["Je\u00b7der", "trug", "ein", "wei\u00b7\u00dfes", "Kleid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Viele doch mit rothen Kr\u00e4nzen", "tokens": ["Vie\u00b7le", "doch", "mit", "ro\u00b7then", "Kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schienen vor den Andern weit", "tokens": ["Schie\u00b7nen", "vor", "den", "An\u00b7dern", "weit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem reinsten Licht zu gl\u00e4nzen.", "tokens": ["In", "dem", "reins\u00b7ten", "Licht", "zu", "gl\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie des Abends Glut,", "tokens": ["Wie", "des", "A\u00b7bends", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Leuchtend als wie Kerzen,", "tokens": ["Leuch\u00b7tend", "als", "wie", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "KOKOM", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Dunkelroth wie Blut", "tokens": ["Dun\u00b7kel\u00b7roth", "wie", "Blut"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KOKOM", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "War die Blum' am Herzen.", "tokens": ["War", "die", "Blum'", "am", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Einer, welchen ich gefragt,", "tokens": ["Ei\u00b7ner", ",", "wel\u00b7chen", "ich", "ge\u00b7fragt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWAT", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Schaar der Sch\u00f6nen, Frommen,", "tokens": ["Aus", "der", "Schaar", "der", "Sch\u00f6\u00b7nen", ",", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat mir treu Bericht gesagt,", "tokens": ["Hat", "mir", "treu", "Be\u00b7richt", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo sie Alle hergekommen.", "tokens": ["Wo", "sie", "Al\u00b7le", "her\u00b7ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus der Tr\u00fcbsal Noth,", "tokens": ["Aus", "der", "Tr\u00fcb\u00b7sal", "Noth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Aus der Glut und Aschen", "tokens": ["Aus", "der", "Glut", "und", "A\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Ward so wei\u00df und roth", "tokens": ["Ward", "so", "wei\u00df", "und", "roth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "VVFIN", "KON", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Ihr Gewand gewaschen.", "tokens": ["Ihr", "Ge\u00b7wand", "ge\u00b7wa\u00b7schen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Pl\u00f6tzlich scholl ein heller Klang", "tokens": ["Pl\u00f6tz\u00b7lich", "scholl", "ein", "hel\u00b7ler", "Klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lockend aus den gr\u00fcnen Zweigen,", "tokens": ["Lo\u00b7ckend", "aus", "den", "gr\u00fc\u00b7nen", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die ganze Schaar verschlang", "tokens": ["Und", "die", "gan\u00b7ze", "Schaar", "ver\u00b7schlang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich in einen frohen Reigen.", "tokens": ["Sich", "in", "ei\u00b7nen", "fro\u00b7hen", "Rei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach es war ein Tanz,", "tokens": ["Ach", "es", "war", "ein", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Wie sich Sterne drehen,", "tokens": ["Wie", "sich", "Ster\u00b7ne", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Solch ein heller Glanz,", "tokens": ["Solch", "ein", "hel\u00b7ler", "Glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Solch ein lindes Wehen!", "tokens": ["Solch", "ein", "lin\u00b7des", "We\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Aber nun der Herr erschien,", "tokens": ["A\u00b7ber", "nun", "der", "Herr", "er\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Geliebte, Sch\u00f6nste, Eine,", "tokens": ["Der", "Ge\u00b7lieb\u00b7te", ",", "Sch\u00f6ns\u00b7te", ",", "Ei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lagen All' auf ihren Knie'n,", "tokens": ["La\u00b7gen", "All'", "auf", "ih\u00b7ren", "Knie'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle sah er an,", "tokens": ["Al\u00b7le", "sah", "er", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Gr\u00fc\u00dft' sie Schwestern, Br\u00fcder,", "tokens": ["Gr\u00fc\u00dft'", "sie", "Schwes\u00b7tern", ",", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Segnend schwand er dann", "tokens": ["Seg\u00b7nend", "schwand", "er", "dann"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Aus den Blicken wieder.", "tokens": ["Aus", "den", "Bli\u00b7cken", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "O der \u00fcbergro\u00dfen Freud',", "tokens": ["O", "der", "\u00fc\u00b7ber\u00b7gro\u00b7\u00dfen", "Freud'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche nicht ist auszusagen,", "tokens": ["Wel\u00b7che", "nicht", "ist", "aus\u00b7zu\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O der Zier und Herrlichkeit,", "tokens": ["O", "der", "Zier", "und", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Welche Gottes Heil'ge tragen!", "tokens": ["Wel\u00b7che", "Got\u00b7tes", "Heil'\u00b7ge", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aller Heil'gen Tag,", "tokens": ["Al\u00b7ler", "Heil'\u00b7gen", "Tag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Welchen Gott gegeben,", "tokens": ["Wel\u00b7chen", "Gott", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Da\u00df er laben mag,", "tokens": ["Da\u00df", "er", "la\u00b7ben", "mag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Uns im l\u00e4ngsten leben!", "tokens": ["Uns", "im", "l\u00e4ngs\u00b7ten", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Himmelan die Augen klar,", "tokens": ["Him\u00b7me\u00b7lan", "die", "Au\u00b7gen", "klar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Himmelan das Herz gehoben,", "tokens": ["Him\u00b7me\u00b7lan", "das", "Herz", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df wir mit der Heil'gen Schaar", "tokens": ["Da\u00df", "wir", "mit", "der", "Heil'\u00b7gen", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Unsern Hirt und Meister loben!", "tokens": ["Un\u00b7sern", "Hirt", "und", "Meis\u00b7ter", "lo\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwester, gib die Hand,", "tokens": ["Schwes\u00b7ter", ",", "gib", "die", "Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Denn auf gleichen Wegen", "tokens": ["Denn", "auf", "glei\u00b7chen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Ziehn wir ", "tokens": ["Ziehn", "wir"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}}}}}