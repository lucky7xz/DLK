{"textgrid.poem.53742": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "An Ernst Toller", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist fein raus.", "tokens": ["Du", "bist", "fein", "raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Nicht einen Tag geschenkt.", "tokens": ["Nicht", "ei\u00b7nen", "Tag", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wobei man an Herrn von Arco denkt.", "tokens": ["Wo\u00b7bei", "man", "an", "Herrn", "von", "Ar\u00b7co", "denkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Sei gegr\u00fc\u00dft! Du kamst ans Licht!", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", "!", "Du", "kamst", "ans", "Licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$.", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Herr Ebert k\u00fcmmert sich um dich nicht.", "tokens": ["Herr", "E\u00b7bert", "k\u00fcm\u00b7mert", "sich", "um", "dich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PRF", "APPR", "PPER", "PTKNEG", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Er mag sich nicht mit Bayern schlagen.", "tokens": ["Er", "mag", "sich", "nicht", "mit", "Bay\u00b7ern", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und da hat er auch nichts zu sagen.", "tokens": ["Und", "da", "hat", "er", "auch", "nichts", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Vor den Rechtsausschu\u00df gingst du? Gar nicht schlecht.", "tokens": ["Vor", "den", "Rechts\u00b7aus\u00b7schu\u00df", "gingst", "du", "?", "Gar", "nicht", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$.", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Da findest du alles \u2013 nur kein Recht.", "tokens": ["Da", "fin\u00b7dest", "du", "al\u00b7les", "\u2013", "nur", "kein", "Recht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$(", "ADV", "PIAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Wer Gefangene schindet, ist der nicht ehrlos?", "tokens": ["Wer", "Ge\u00b7fan\u00b7ge\u00b7ne", "schin\u00b7det", ",", "ist", "der", "nicht", "ehr\u00b7los", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "VAFIN", "ART", "PTKNEG", "ADJD", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Herr Held ist ein Held; die sind ja wehrlos.", "tokens": ["Herr", "Held", "ist", "ein", "Held", ";", "die", "sind", "ja", "wehr\u00b7los", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ART", "NN", "$.", "ART", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Sag es laut! Gott gab dir den Schrei.", "tokens": ["Sag", "es", "laut", "!", "Gott", "gab", "dir", "den", "Schrei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$.", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sag es \u2013 du warst ja mit dabei!", "tokens": ["Sag", "es", "\u2013", "du", "warst", "ja", "mit", "da\u00b7bei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "PPER", "VAFIN", "ADV", "APPR", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und denk auch an ihn, der im Ungemach,", "tokens": ["Und", "denk", "auch", "an", "ihn", ",", "der", "im", "Un\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "$,", "PRELS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "an ihn:", "tokens": ["an", "ihn", ":"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.3": {"text": "an Felix Fechenbach.", "tokens": ["an", "Fe\u00b7lix", "Fe\u00b7chen\u00b7bach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}