{"textgrid.poem.33538": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1902, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Wiesen und in frommen Winden m\u00f6gen", "tokens": ["In", "Wie\u00b7sen", "und", "in", "from\u00b7men", "Win\u00b7den", "m\u00f6\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Friedliche heitre Menschen selig gleiten.", "tokens": ["Fried\u00b7li\u00b7che", "heit\u00b7re", "Men\u00b7schen", "se\u00b7lig", "glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wir aber, morsch und l\u00e4ngst vergiftet, l\u00f6gen", "tokens": ["Wir", "a\u00b7ber", ",", "morsch", "und", "l\u00e4ngst", "ver\u00b7gif\u00b7tet", ",", "l\u00f6\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "$,", "ADJD", "KON", "ADV", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Uns selbst was vor beim In-die-Himmel-Schreiten.", "tokens": ["Uns", "selbst", "was", "vor", "beim", "In\u00b7die\u00b7Him\u00b7mel\u00b7Schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PWS", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In fremden St\u00e4dten treib ich ohne Ruder.", "tokens": ["In", "frem\u00b7den", "St\u00e4d\u00b7ten", "treib", "ich", "oh\u00b7ne", "Ru\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hohl sind die fremden Tage und wie Kreide.", "tokens": ["Hohl", "sind", "die", "frem\u00b7den", "Ta\u00b7ge", "und", "wie", "Krei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "KON", "PWAV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du, mein Berlin, du Opiumrausch, du Luder.", "tokens": ["Du", ",", "mein", "Ber\u00b7lin", ",", "du", "O\u00b7pium\u00b7rausch", ",", "du", "Lu\u00b7der", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NE", "$,", "PPER", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur wer die Sehnsucht kennt, wei\u00df, was ich leide.", "tokens": ["Nur", "wer", "die", "Sehn\u00b7sucht", "kennt", ",", "wei\u00df", ",", "was", "ich", "lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}