{"dta.poem.15600": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So gl\u00fccklich war ich, und so sorglich es zu bleiben,", "tokens": ["So", "gl\u00fcck\u00b7lich", "war", "ich", ",", "und", "so", "sorg\u00b7lich", "es", "zu", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "$,", "KON", "ADV", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So w\u00fcnschend nur mich im gewohnten Gleis zu treiben;", "tokens": ["So", "w\u00fcn\u00b7schend", "nur", "mich", "im", "ge\u00b7wohn\u00b7ten", "Gleis", "zu", "trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PPER", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df ich nicht wagt' im Schritt zu eilen noch zu s\u00e4umen,", "tokens": ["Da\u00df", "ich", "nicht", "wagt'", "im", "Schritt", "zu", "ei\u00b7len", "noch", "zu", "s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "APPRART", "NN", "PTKZU", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch irgend ein Ger\u00e4th von seinem Platz zu r\u00e4umen;", "tokens": ["Noch", "ir\u00b7gend", "ein", "Ge\u00b7r\u00e4\u00b7th", "von", "sei\u00b7nem", "Platz", "zu", "r\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Aus Furcht, es m\u00f6cht' im Takt das Gl\u00fcck die St\u00f6rung sp\u00fcren,", "tokens": ["Aus", "Furcht", ",", "es", "m\u00f6cht'", "im", "Takt", "das", "Gl\u00fcck", "die", "St\u00f6\u00b7rung", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "VMFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und kleine Aenderung zu einer gr\u00f6\u00dfern f\u00fchren.", "tokens": ["Und", "klei\u00b7ne", "A\u00b7en\u00b7de\u00b7rung", "zu", "ei\u00b7ner", "gr\u00f6\u00b7\u00dfern", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}