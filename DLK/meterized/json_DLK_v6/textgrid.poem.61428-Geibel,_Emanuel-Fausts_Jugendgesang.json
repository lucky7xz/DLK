{"textgrid.poem.61428": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Fausts Jugendgesang", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch Klippen, die im Fr\u00fchrot baden,", "tokens": ["Durch", "Klip\u00b7pen", ",", "die", "im", "Fr\u00fch\u00b7rot", "ba\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch schwarzer T\u00e4ler Einsamkeit", "tokens": ["Durch", "schwar\u00b7zer", "T\u00e4\u00b7ler", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hinzieh' ich auf entlegnen Pfaden,", "tokens": ["Hin\u00b7zieh'", "ich", "auf", "ent\u00b7leg\u00b7nen", "Pfa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Geister nur sind mein Geleit.", "tokens": ["Und", "Geis\u00b7ter", "nur", "sind", "mein", "Ge\u00b7leit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Herz, das im Gew\u00fchl verdorrte,", "tokens": ["Mein", "Herz", ",", "das", "im", "Ge\u00b7w\u00fchl", "ver\u00b7dorr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier f\u00fchlt sich's heimatlich erwacht,", "tokens": ["Hier", "f\u00fchlt", "sich's", "hei\u00b7mat\u00b7lich", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Wildnis lehrt mich ernste Worte,", "tokens": ["Die", "Wild\u00b7nis", "lehrt", "mich", "erns\u00b7te", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und R\u00e4tsel deutet mir die Nacht.", "tokens": ["Und", "R\u00e4t\u00b7sel", "deu\u00b7tet", "mir", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und du, o Sturm, wenn laut im Grimme", "tokens": ["Und", "du", ",", "o", "Sturm", ",", "wenn", "laut", "im", "Grim\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,", "KOUS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Tosen durch die Kl\u00fcfte bricht,", "tokens": ["Dein", "To\u00b7sen", "durch", "die", "Kl\u00fcf\u00b7te", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir ist's wie eines Bruders Stimme,", "tokens": ["Mir", "ist's", "wie", "ei\u00b7nes", "Bru\u00b7ders", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Mut und Kraft ins Herz mir spricht;", "tokens": ["Die", "Mut", "und", "Kraft", "ins", "Herz", "mir", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Wogen, die zu Tal ihr brauset,", "tokens": ["Ihr", "Wo\u00b7gen", ",", "die", "zu", "Tal", "ihr", "brau\u00b7set", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Fichten an des Sturzes Rand,", "tokens": ["Ihr", "Fich\u00b7ten", "an", "des", "Stur\u00b7zes", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich wei\u00df es, was ihr sch\u00e4umt und sauset,", "tokens": ["Ich", "wei\u00df", "es", ",", "was", "ihr", "sch\u00e4umt", "und", "sau\u00b7set", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Denn ich, auch ich bin euch verwandt.", "tokens": ["Denn", "ich", ",", "auch", "ich", "bin", "euch", "ver\u00b7wandt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADV", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Tr\u00e4nkst du nicht mich auch, Mutter Erde,", "tokens": ["Tr\u00e4nkst", "du", "nicht", "mich", "auch", ",", "Mut\u00b7ter", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PPER", "ADV", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deiner Milch aus heil'ger Brust?", "tokens": ["Mit", "dei\u00b7ner", "Milch", "aus", "heil'\u00b7ger", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erziehst du, da\u00df gest\u00e4hlt ich werde,", "tokens": ["Er\u00b7ziehst", "du", ",", "da\u00df", "ge\u00b7st\u00e4hlt", "ich", "wer\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "VVPP", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich nicht durch Kampf zu jeder Lust?", "tokens": ["Mich", "nicht", "durch", "Kampf", "zu", "je\u00b7der", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Neigst du den Blick, den strahlend hellen,", "tokens": ["Neigst", "du", "den", "Blick", ",", "den", "strah\u00b7lend", "hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht, Vater \u00c4ther, zu mir her", "tokens": ["Nicht", ",", "Va\u00b7ter", "\u00c4\u00b7ther", ",", "zu", "mir", "her"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "$,", "NN", "NN", "$,", "APPR", "PPER", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und zeigst mir meine Spielgesellen", "tokens": ["Und", "zeigst", "mir", "mei\u00b7ne", "Spiel\u00b7ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In Berg und Luft, in Wald und Meer?", "tokens": ["In", "Berg", "und", "Luft", ",", "in", "Wald", "und", "Meer", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Den Geier seh' ich einsam schweben,", "tokens": ["Den", "Gei\u00b7er", "seh'", "ich", "ein\u00b7sam", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und mein Gedanke holt ihn ein,", "tokens": ["Und", "mein", "Ge\u00b7dan\u00b7ke", "holt", "ihn", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wolke Dunstbild seh' ich weben,", "tokens": ["Der", "Wol\u00b7ke", "Dunst\u00b7bild", "seh'", "ich", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihr verhaltner Groll ist mein.", "tokens": ["Und", "ihr", "ver\u00b7halt\u00b7ner", "Groll", "ist", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn erl\u00f6st dann in den Schl\u00fcnden", "tokens": ["Und", "wenn", "er\u00b7l\u00f6st", "dann", "in", "den", "Schl\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Donner springt von Hang zu Hang,", "tokens": ["Der", "Don\u00b7ner", "springt", "von", "Hang", "zu", "Hang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann jauchzt's in meiner Seele Gr\u00fcnden,", "tokens": ["Dann", "jauchzt's", "in", "mei\u00b7ner", "See\u00b7le", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und meine Brust wird voll Gesang.", "tokens": ["Und", "mei\u00b7ne", "Brust", "wird", "voll", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O Blitzeslodern, Felsenk\u00fchle,", "tokens": ["O", "Blit\u00b7zes\u00b7lo\u00b7dern", ",", "Fel\u00b7sen\u00b7k\u00fch\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Sturm und Waldnacht, nehmt mich hin,", "tokens": ["O", "Sturm", "und", "Wald\u00b7nacht", ",", "nehmt", "mich", "hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie ich ganz mich euer f\u00fchle,", "tokens": ["Und", "wie", "ich", "ganz", "mich", "eu\u00b7er", "f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "PPER", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gebt Liebesantwort meinem Sinn!", "tokens": ["Gebt", "Lie\u00b7bes\u00b7ant\u00b7wort", "mei\u00b7nem", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In euern F\u00fcllen untergehen", "tokens": ["In", "eu\u00b7ern", "F\u00fcl\u00b7len", "un\u00b7ter\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00dft dieses Herzens Einzelschlag,", "tokens": ["La\u00dft", "die\u00b7ses", "Her\u00b7zens", "Ein\u00b7zel\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bis ich von eures Odems Wehen", "tokens": ["Bis", "ich", "von", "eu\u00b7res", "O\u00b7dems", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein eigen Lied nicht scheiden mag!", "tokens": ["Mein", "ei\u00b7gen", "Lied", "nicht", "schei\u00b7den", "mag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}