{"textgrid.poem.66153": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Die Aussicht", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n ist's, von des Thr\u00e4nenberges H\u00f6hen", "tokens": ["Sch\u00f6n", "ist's", ",", "von", "des", "Thr\u00e4\u00b7nen\u00b7ber\u00b7ges", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gott auf seiner Erde wandeln sehen,", "tokens": ["Gott", "auf", "sei\u00b7ner", "Er\u00b7de", "wan\u00b7deln", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wo sein Odem die Gesch\u00f6pfe k\u00fc\u00dft.", "tokens": ["Wo", "sein", "O\u00b7dem", "die", "Ge\u00b7sch\u00f6p\u00b7fe", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Auen sehen, drauf Natur, die treue,", "tokens": ["Au\u00b7en", "se\u00b7hen", ",", "drauf", "Na\u00b7tur", ",", "die", "treu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PAV", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Eingekleidet in des Himmels Bl\u00e4ue,", "tokens": ["Ein\u00b7ge\u00b7klei\u00b7det", "in", "des", "Him\u00b7mels", "Bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Schreitet, und wo Milch und Honig flie\u00dft!", "tokens": ["Schrei\u00b7tet", ",", "und", "wo", "Milch", "und", "Ho\u00b7nig", "flie\u00dft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "PWAV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6n ist's, in des Thr\u00e4nenberges L\u00fcften", "tokens": ["Sch\u00f6n", "ist's", ",", "in", "des", "Thr\u00e4\u00b7nen\u00b7ber\u00b7ges", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "B\u00e4ume sehn, in silberwei\u00dfen D\u00fcften,", "tokens": ["B\u00e4u\u00b7me", "sehn", ",", "in", "sil\u00b7ber\u00b7wei\u00b7\u00dfen", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die der K\u00e4fer wonnesummend trinkt;", "tokens": ["Die", "der", "K\u00e4\u00b7fer", "won\u00b7ne\u00b7sum\u00b7mend", "trinkt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Stra\u00dfe sehn im weiten Lande,", "tokens": ["Und", "die", "Stra\u00b7\u00dfe", "sehn", "im", "wei\u00b7ten", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Menschenwimmelnd, wie vom Silbersande", "tokens": ["Men\u00b7schen\u00b7wim\u00b7melnd", ",", "wie", "vom", "Sil\u00b7ber\u00b7san\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "PWAV", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sie, der Milchstra\u00df' gleich am Himmel, blinkt.", "tokens": ["Sie", ",", "der", "Milch\u00b7stra\u00df'", "gleich", "am", "Him\u00b7mel", ",", "blinkt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ADV", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Und der Neckar blau vor\u00fcberziehend,", "tokens": ["Und", "der", "Ne\u00b7ckar", "blau", "vor\u00b7\u00fc\u00b7ber\u00b7zie\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In dem Gold der Abendsonne gl\u00fchend,", "tokens": ["In", "dem", "Gold", "der", "A\u00b7bend\u00b7son\u00b7ne", "gl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ist dem Sp\u00e4herblicke Himmelslust;", "tokens": ["Ist", "dem", "Sp\u00e4\u00b7her\u00b7bli\u00b7cke", "Him\u00b7mels\u00b7lust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und den Wein, des siechen Wandrers Leben,", "tokens": ["Und", "den", "Wein", ",", "des", "sie\u00b7chen", "Wand\u00b7rers", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wachsen sehn an m\u00fctterlichen Reben,", "tokens": ["Wach\u00b7sen", "sehn", "an", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ist Entz\u00fccken f\u00fcr des Dichters Brust.", "tokens": ["Ist", "Ent\u00b7z\u00fc\u00b7cken", "f\u00fcr", "des", "Dich\u00b7ters", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Aber, armer Mann, du bist gefangen;", "tokens": ["A\u00b7ber", ",", "ar\u00b7mer", "Mann", ",", "du", "bist", "ge\u00b7fan\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kannst du trunken an der Sch\u00f6nheit hangen?", "tokens": ["Kannst", "du", "trun\u00b7ken", "an", "der", "Sch\u00f6n\u00b7heit", "han\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nichts auf dieser sch\u00f6nen Welt ist dein!", "tokens": ["Nichts", "auf", "die\u00b7ser", "sch\u00f6\u00b7nen", "Welt", "ist", "dein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PDAT", "ADJA", "NN", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Alles, alles ist in tiefer Trauer", "tokens": ["Al\u00b7les", ",", "al\u00b7les", "ist", "in", "tie\u00b7fer", "Trau\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "$,", "PIS", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Auf der weiten Erde; denn die Mauer", "tokens": ["Auf", "der", "wei\u00b7ten", "Er\u00b7de", ";", "denn", "die", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "KON", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Meiner Veste schlie\u00dft mich Armen ein!", "tokens": ["Mei\u00b7ner", "Ves\u00b7te", "schlie\u00dft", "mich", "Ar\u00b7men", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Doch herab von meinem Thr\u00e4nenberge", "tokens": ["Doch", "her\u00b7ab", "von", "mei\u00b7nem", "Thr\u00e4\u00b7nen\u00b7ber\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seh' ich dort den Moderplatz der S\u00e4rge;", "tokens": ["Seh'", "ich", "dort", "den", "Mo\u00b7der\u00b7platz", "der", "S\u00e4r\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hinter einer Kirche streckt er sich", "tokens": ["Hin\u00b7ter", "ei\u00b7ner", "Kir\u00b7che", "streckt", "er", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PRF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Gr\u00fcner als die andern Pl\u00e4tze alle:", "tokens": ["Gr\u00fc\u00b7ner", "als", "die", "an\u00b7dern", "Pl\u00e4t\u00b7ze", "al\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "PIS", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ach! herab von meinem hohen Walle", "tokens": ["Ach", "!", "her\u00b7ab", "von", "mei\u00b7nem", "ho\u00b7hen", "Wal\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Seh' ich keinen sch\u00f6nern Platz f\u00fcr mich!", "tokens": ["Seh'", "ich", "kei\u00b7nen", "sch\u00f6\u00b7nern", "Platz", "f\u00fcr", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}