{"textgrid.poem.40123": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsahnung", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Felder liegen wei\u00df;", "tokens": ["Die", "Fel\u00b7der", "lie\u00b7gen", "wei\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "wohin ich schau'", "tokens": ["wo\u00b7hin", "ich", "schau'"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "ins fahle Nebelgrau,", "tokens": ["ins", "fah\u00b7le", "Ne\u00b7bel\u00b7grau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "scheint Schnee und Eis.", "tokens": ["scheint", "Schnee", "und", "Eis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Doch da \u2013 ein Sonnenstrahl", "tokens": ["Doch", "da", "\u2013", "ein", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$(", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "bricht durch den Flor", "tokens": ["bricht", "durch", "den", "Flor"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "und zieht den Blick empor", "tokens": ["und", "zieht", "den", "Blick", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "mit Einem Mal,", "tokens": ["mit", "Ei\u00b7nem", "Mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "und von der Erden", "tokens": ["und", "von", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "ringt jung ein Duft", "tokens": ["ringt", "jung", "ein", "Duft"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "sich durch die Luft: \u2013", "tokens": ["sich", "durch", "die", "Luft", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "will's ", "tokens": ["will's"], "token_info": ["word"], "pos": ["VMFIN"], "meter": "-", "measure": "single.down"}}}}}