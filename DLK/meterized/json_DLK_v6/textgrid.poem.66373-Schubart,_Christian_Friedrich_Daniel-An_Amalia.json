{"textgrid.poem.66373": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An Amalia", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amalia reizend wie Cypria war,", "tokens": ["A\u00b7ma\u00b7lia", "rei\u00b7zend", "wie", "Cyp\u00b7ria", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "KOKOM", "NE", "VAFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Als sie eine Welle des Meeres gebar.", "tokens": ["Als", "sie", "ei\u00b7ne", "Wel\u00b7le", "des", "Mee\u00b7res", "ge\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie schl\u00fcpfte aus silbernem Schaume empor,", "tokens": ["Sie", "schl\u00fcpf\u00b7te", "aus", "sil\u00b7ber\u00b7nem", "Schau\u00b7me", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Begr\u00fc\u00dft von der G\u00f6tter olympischem Chor.", "tokens": ["Be\u00b7gr\u00fc\u00dft", "von", "der", "G\u00f6t\u00b7ter", "o\u00b7lym\u00b7pi\u00b7schem", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+++-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Nun stand am Gestade das himmlische Kind,", "tokens": ["Nun", "stand", "am", "Ge\u00b7sta\u00b7de", "das", "himm\u00b7li\u00b7sche", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es spielte in goldenen Locken der Wind;", "tokens": ["Es", "spiel\u00b7te", "in", "gol\u00b7de\u00b7nen", "Lo\u00b7cken", "der", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und ihren wei\u00dfschimmernden H\u00fcften entschwebt", "tokens": ["Und", "ih\u00b7ren", "wei\u00df\u00b7schim\u00b7mern\u00b7den", "H\u00fcf\u00b7ten", "ent\u00b7schwebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der G\u00fcrtel, aus zauberischem Liebreiz gewebt.", "tokens": ["Der", "G\u00fcr\u00b7tel", ",", "aus", "zau\u00b7be\u00b7ri\u00b7schem", "Lieb\u00b7reiz", "ge\u00b7webt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Amalia, schau in der G\u00f6ttin dein Bild!", "tokens": ["A\u00b7ma\u00b7lia", ",", "schau", "in", "der", "G\u00f6t\u00b7tin", "dein", "Bild", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "So himmlisch geschaffen, so l\u00e4chelnd, so mild;", "tokens": ["So", "himm\u00b7lisch", "ge\u00b7schaf\u00b7fen", ",", "so", "l\u00e4\u00b7chelnd", ",", "so", "mild", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "So still in der Gr\u00f6\u00dfe, so hoch in der Ruh',", "tokens": ["So", "still", "in", "der", "Gr\u00f6\u00b7\u00dfe", ",", "so", "hoch", "in", "der", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "So reizend im Schleier der Sch\u00f6nheit bist du.", "tokens": ["So", "rei\u00b7zend", "im", "Schlei\u00b7er", "der", "Sch\u00f6n\u00b7heit", "bist", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ART", "NN", "VAFIN", "PPER", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Du bist zwar an Liebreiz der Cypria gleich,", "tokens": ["Du", "bist", "zwar", "an", "Lieb\u00b7reiz", "der", "Cyp\u00b7ria", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Doch nicht so empfindsam, f\u00fcr Liebe so weich;", "tokens": ["Doch", "nicht", "so", "emp\u00b7find\u00b7sam", ",", "f\u00fcr", "Lie\u00b7be", "so", "weich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADJD", "$,", "APPR", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "So schmelzend, wenn Liebe F\u00fchlenden spricht:", "tokens": ["So", "schmel\u00b7zend", ",", "wenn", "Lie\u00b7be", "F\u00fch\u00b7len\u00b7den", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "NN", "NN", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Amalia \u2013 leider! so bist du noch nicht!", "tokens": ["A\u00b7ma\u00b7lia", "\u2013", "lei\u00b7der", "!", "so", "bist", "du", "noch", "nicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADV", "$.", "ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Oft hab' ich's empfunden, oft hab' ich's gef\u00fchlt,", "tokens": ["Oft", "hab'", "ich's", "emp\u00b7fun\u00b7den", ",", "oft", "hab'", "ich's", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVPP", "$,", "ADV", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df Qualen der Liebe das Herz mir zerw\u00fchlt.", "tokens": ["Da\u00df", "Qua\u00b7len", "der", "Lie\u00b7be", "das", "Herz", "mir", "zer\u00b7w\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich schaurte, und wies dir mein blutendes Herz;", "tokens": ["Ich", "schaur\u00b7te", ",", "und", "wies", "dir", "mein", "blu\u00b7ten\u00b7des", "Herz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Doch bliebst du, Amalia, h\u00e4rter als Erz.", "tokens": ["Doch", "bliebst", "du", ",", "A\u00b7ma\u00b7lia", ",", "h\u00e4r\u00b7ter", "als", "Erz", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NE", "$,", "ADJD", "KOKOM", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Wenn Liebe von zitternden Lippen mir scholl,", "tokens": ["Wenn", "Lie\u00b7be", "von", "zit\u00b7tern\u00b7den", "Lip\u00b7pen", "mir", "scholl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN", "PPER", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wenn blutend die flehende Z\u00e4hre mir quoll;", "tokens": ["Wenn", "blu\u00b7tend", "die", "fle\u00b7hen\u00b7de", "Z\u00e4h\u00b7re", "mir", "quoll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "So flohst du der Liebe geheiligtes Band,", "tokens": ["So", "flohst", "du", "der", "Lie\u00b7be", "ge\u00b7hei\u00b7lig\u00b7tes", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und botst mir als k\u00e4ltere ", "tokens": ["Und", "botst", "mir", "als", "k\u00e4l\u00b7te\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "ADJA"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Als Freundin? Amalia, Freundschaft ist gut", "tokens": ["Als", "Freun\u00b7din", "?", "A\u00b7ma\u00b7lia", ",", "Freund\u00b7schaft", "ist", "gut"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "$.", "NE", "$,", "NN", "VAFIN", "ADJD"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bei wachsenden Jahren, und k\u00e4lterem Blut:", "tokens": ["Bei", "wach\u00b7sen\u00b7den", "Jah\u00b7ren", ",", "und", "k\u00e4l\u00b7te\u00b7rem", "Blut", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Doch strahlend wie du in der Bl\u00fcthengestalt \u2013", "tokens": ["Doch", "strah\u00b7lend", "wie", "du", "in", "der", "Bl\u00fc\u00b7then\u00b7ge\u00b7stalt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Ihr Himmel, wie ist da die Freundschaft so kalt!", "tokens": ["Ihr", "Him\u00b7mel", ",", "wie", "ist", "da", "die", "Freund\u00b7schaft", "so", "kalt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VAFIN", "ADV", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Nur Liebe, nur Liebe erweckst du in mir,", "tokens": ["Nur", "Lie\u00b7be", ",", "nur", "Lie\u00b7be", "er\u00b7weckst", "du", "in", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die heilige Flamme, wie lodert sie dir!", "tokens": ["Die", "hei\u00b7li\u00b7ge", "Flam\u00b7me", ",", "wie", "lo\u00b7dert", "sie", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "O la\u00df dich erweichen, Amalia, sprich:", "tokens": ["O", "la\u00df", "dich", "er\u00b7wei\u00b7chen", ",", "A\u00b7ma\u00b7lia", ",", "sprich", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "VVINF", "$,", "NE", "$,", "ADJD", "$."], "meter": "++--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "\u00bbmein Busen empfindet ", "tokens": ["\u00bb", "mein", "Bu\u00b7sen", "emp\u00b7fin\u00b7det"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}