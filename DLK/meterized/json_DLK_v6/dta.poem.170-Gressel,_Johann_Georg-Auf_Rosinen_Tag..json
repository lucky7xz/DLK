{"dta.poem.170": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf   Rosinen   Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Als das Ende meiner Pein/", "tokens": ["Als", "das", "En\u00b7de", "mei\u00b7ner", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der mir durchgebrochnen Wangen/", "tokens": ["Der", "mir", "durch\u00b7ge\u00b7broch\u00b7nen", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem Allmanach zu seyn/", "tokens": ["In", "dem", "All\u00b7ma\u00b7nach", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich j\u00fcngst suchte mit Verlangen.", "tokens": ["Ich", "j\u00fcngst", "such\u00b7te", "mit", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sah von selbten Tag nicht fern", "tokens": ["Sah", "von", "selb\u00b7ten", "Tag", "nicht", "fern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PTKNEG", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich ", "tokens": ["Ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Gleich dacht\u2019 ich/ an diesen Stern", "tokens": ["Gleich", "dacht'", "ich", "/", "an", "die\u00b7sen", "Stern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "APPR", "PDAT", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "Mag vielleicht dein Heyl wol hangen.", "tokens": ["Mag", "viel\u00b7leicht", "dein", "Heyl", "wol", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Gl\u00fccklich die ", "tokens": ["Gl\u00fcck\u00b7lich", "die"], "token_info": ["word", "word"], "pos": ["ADJD", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "So ", "tokens": ["So"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Ein gevierter ", "tokens": ["Ein", "ge\u00b7vier\u00b7ter"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Strahlt den Zwillingen zur Seiten.", "tokens": ["Strahlt", "den", "Zwil\u00b7lin\u00b7gen", "zur", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Gelt/ sagt hoffend ich bey mir/", "tokens": ["Gelt", "/", "sagt", "hof\u00b7fend", "ich", "bey", "mir", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "VVPP", "PPER", "APPR", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Es wird dir nach Wunsch gelingen/", "tokens": ["Es", "wird", "dir", "nach", "Wunsch", "ge\u00b7lin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.16": {"text": "Die Gesundheit wiederbringen.", "tokens": ["Die", "Ge\u00b7sund\u00b7heit", "wie\u00b7der\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Was geschicht! die Pein sich bricht/", "tokens": ["Was", "ge\u00b7schicht", "!", "die", "Pein", "sich", "bricht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "$.", "ART", "NN", "PRF", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Alle Marter drauf vergehen/", "tokens": ["Al\u00b7le", "Mar\u00b7ter", "drauf", "ver\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PAV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Und ich kan an diesem Licht/", "tokens": ["Und", "ich", "kan", "an", "die\u00b7sem", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "PDAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Meine Liebste wieder sehen.", "tokens": ["Mei\u00b7ne", "Liebs\u00b7te", "wie\u00b7der", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "O der ", "tokens": ["O", "der"], "token_info": ["word", "word"], "pos": ["NE", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "So ", "tokens": ["So"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.23": {"text": "Blo\u00df ihr Anblick Lindrung schafft/", "tokens": ["Blo\u00df", "ihr", "An\u00b7blick", "Lind\u00b7rung", "schafft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Und der Nahme Schmertzen leget.", "tokens": ["Und", "der", "Nah\u00b7me", "Schmert\u00b7zen", "le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Hier mu\u00df eine Phantasey", "tokens": ["Hier", "mu\u00df", "ei\u00b7ne", "Phan\u00b7ta\u00b7sey"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Von der K\u00f6nge Cur verbleichen/", "tokens": ["Von", "der", "K\u00f6n\u00b7ge", "Cur", "ver\u00b7blei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.28": {"text": "Thut weit besser Wunder-Zeichen.", "tokens": ["Thut", "weit", "bes\u00b7ser", "Wun\u00b7der\u00b7Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJD", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.29": {"text": "Kan dann in Abwesenheit", "tokens": ["Kan", "dann", "in", "Ab\u00b7we\u00b7sen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.30": {"text": "Mich ", "tokens": ["Mich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.31": {"text": "M\u00f6chte ich wol allezeit", "tokens": ["M\u00f6ch\u00b7te", "ich", "wol", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Diese Aertztin bey mir haben!", "tokens": ["Die\u00b7se", "A\u00b7ertz\u00b7tin", "bey", "mir", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "PPER", "VAFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.33": {"text": "Doch zu fr\u00fch mit dem Begehr/", "tokens": ["Doch", "zu", "fr\u00fch", "mit", "dem", "Be\u00b7gehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKA", "ADJD", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Kan die Frucht wol abgenommen", "tokens": ["Kan", "die", "Frucht", "wol", "ab\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Werden von den Meyern/ ehr", "tokens": ["Wer\u00b7den", "von", "den", "Me\u00b7yern", "/", "ehr"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "$(", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Jhre Zeit und Erndte kommen?", "tokens": ["Ih\u00b7re", "Zeit", "und", "Ernd\u00b7te", "kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Har! mit Gott und mit der Zeit/", "tokens": ["Har", "!", "mit", "Gott", "und", "mit", "der", "Zeit", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "NN", "KON", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Werden schon die milden Reben/", "tokens": ["Wer\u00b7den", "schon", "die", "mil\u00b7den", "Re\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.39": {"text": "Jhrer Trauben S\u00fc\u00dfigkeit/", "tokens": ["Ih\u00b7rer", "Trau\u00b7ben", "S\u00fc\u00b7\u00dfig\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Denen Schnittern g\u00fctig geben.", "tokens": ["De\u00b7nen", "Schnit\u00b7tern", "g\u00fc\u00b7tig", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Inde\u00df wil das edle Licht", "tokens": ["In\u00b7de\u00df", "wil", "das", "ed\u00b7le", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.43": {"text": "Bis der Tod die Augen bricht/", "tokens": ["Bis", "der", "Tod", "die", "Au\u00b7gen", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Und in nichts mich wird verkehren.", "tokens": ["Und", "in", "nichts", "mich", "wird", "ver\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "PPER", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Inde\u00df m\u00fcsse mehr und mehr/", "tokens": ["In\u00b7de\u00df", "m\u00fcs\u00b7se", "mehr", "und", "mehr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "KON", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Sch\u00f6nste euer Gl\u00fccke gr\u00fcnen/", "tokens": ["Sch\u00f6ns\u00b7te", "eu\u00b7er", "Gl\u00fc\u00b7cke", "gr\u00fc\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Alsdenn habe ich die Ehr/", "tokens": ["Als\u00b7denn", "ha\u00b7be", "ich", "die", "Ehr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.48": {"text": "Eure Sch\u00f6nheit zu bedienen.", "tokens": ["Eu\u00b7re", "Sch\u00f6n\u00b7heit", "zu", "be\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}