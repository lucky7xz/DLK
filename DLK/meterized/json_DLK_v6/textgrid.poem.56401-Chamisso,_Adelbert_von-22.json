{"textgrid.poem.56401": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "22", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da hub Thrym an,", "tokens": ["Da", "hub", "Th\u00b7rym", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PTKVZ", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.2": {"text": "Der Herrscher der Riesen:", "tokens": ["Der", "Herr\u00b7scher", "der", "Rie\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "\u00bbauf! Auf! ihr Riesen,", "tokens": ["\u00bb", "auf", "!", "Auf", "!", "ihr", "Rie\u00b7sen", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "APPR", "$.", "PPOSAT", "NN", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.4": {"text": "Bereitet die B\u00e4nke,", "tokens": ["Be\u00b7rei\u00b7tet", "die", "B\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Nun f\u00fchrt mir Fraya,", "tokens": ["Nun", "f\u00fchrt", "mir", "Fray\u00b7a", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die Frau, herein.\u00ab", "tokens": ["Die", "Frau", ",", "her\u00b7ein", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PTKVZ", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}