{"textgrid.poem.52276": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Ungarn", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun flackert durch die Heide", "tokens": ["Nun", "fla\u00b7ckert", "durch", "die", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Lagerfeuer Brand;", "tokens": ["Der", "La\u00b7ger\u00b7feu\u00b7er", "Brand", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun blitzt die krumme Schneide", "tokens": ["Nun", "blitzt", "die", "krum\u00b7me", "Schnei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In des Magyaren Hand;", "tokens": ["In", "des", "Ma\u00b7gy\u00b7a\u00b7ren", "Hand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun l\u00e4\u00dft er seine Herde,", "tokens": ["Nun", "l\u00e4\u00dft", "er", "sei\u00b7ne", "Her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nun schwingt er sich zu Pferde,", "tokens": ["Nun", "schwingt", "er", "sich", "zu", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nun lehnt er am Verhau;", "tokens": ["Nun", "lehnt", "er", "am", "Ver\u00b7hau", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und vor dem Eisensporn'gen", "tokens": ["Und", "vor", "dem", "Ei\u00b7sen\u00b7sporn'\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Aufrauscht das Lied der zorn'gen", "tokens": ["Auf\u00b7rauscht", "das", "Lied", "der", "zorn'\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Donau, der Heidefrau.", "tokens": ["Do\u00b7nau", ",", "der", "Hei\u00b7de\u00b7frau", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie jauchzt in ihren Borden,", "tokens": ["Sie", "jauchzt", "in", "ih\u00b7ren", "Bor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie schwillt vor Stolz und Wut:", "tokens": ["Sie", "schwillt", "vor", "Stolz", "und", "Wut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbgl\u00fcck auf, ihr braunen Horden,", "tokens": ["\u00bb", "gl\u00fcck", "auf", ",", "ihr", "brau\u00b7nen", "Hor\u00b7den", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du hei\u00dfes Ungarblut!", "tokens": ["Du", "hei\u00b7\u00dfes", "Un\u00b7gar\u00b7blut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr Hirten und ihr J\u00e4ger,", "tokens": ["Ihr", "Hir\u00b7ten", "und", "ihr", "J\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr wilden Zimbalschl\u00e4ger,", "tokens": ["Ihr", "wil\u00b7den", "Zim\u00b7bal\u00b7schl\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ihr Geiger unverzagt!", "tokens": ["Ihr", "Gei\u00b7ger", "un\u00b7ver\u00b7zagt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ihr, die ihr als die letzten", "tokens": ["Ihr", ",", "die", "ihr", "als", "die", "letz\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "KOUS", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Zur Schlacht mit dem zerfetzten", "tokens": ["Zur", "Schlacht", "mit", "dem", "zer\u00b7fetz\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Panier der Freiheit jagt!", "tokens": ["Pa\u00b7nier", "der", "Frei\u00b7heit", "jagt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Verraten allenthalben,", "tokens": ["Ver\u00b7ra\u00b7ten", "al\u00b7len\u00b7thal\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verraten und schimpfiert,", "tokens": ["Ver\u00b7ra\u00b7ten", "und", "schimp\u00b7fiert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Habt ihr es auf die Falben", "tokens": ["Habt", "ihr", "es", "auf", "die", "Fal\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Rappen euch salviert!", "tokens": ["Und", "Rap\u00b7pen", "euch", "sal\u00b7viert", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vom Ro\u00df emporgehalten,", "tokens": ["Vom", "Ro\u00df", "em\u00b7por\u00b7ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bluteis in seinen Falten,", "tokens": ["Blu\u00b7teis", "in", "sei\u00b7nen", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So tr\u00e4gt es der Magyar;", "tokens": ["So", "tr\u00e4gt", "es", "der", "Ma\u00b7gyar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "So l\u00e4\u00dft er breit es fliegen,", "tokens": ["So", "l\u00e4\u00dft", "er", "breit", "es", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "So l\u00e4\u00dft er es mit Siegen", "tokens": ["So", "l\u00e4\u00dft", "er", "es", "mit", "Sie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Einweihn das neue Jahr!", "tokens": ["Ein\u00b7weihn", "das", "neu\u00b7e", "Jahr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Seht her doch, ihr nach Westen!", "tokens": ["Seht", "her", "doch", ",", "ihr", "nach", "Wes\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das trotzig mit der festen", "tokens": ["Das", "trot\u00b7zig", "mit", "der", "fes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Stahlhand am Aufruhr h\u00e4lt!", "tokens": ["Stahl\u00b7hand", "am", "Auf\u00b7ruhr", "h\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Im fernen w\u00fcsten Osten,", "tokens": ["Im", "fer\u00b7nen", "w\u00fcs\u00b7ten", "Os\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Freiheit Au\u00dfenposten,", "tokens": ["Der", "Frei\u00b7heit", "Au\u00b7\u00dfen\u00b7pos\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die schlagen jetzt die Schlacht,", "tokens": ["Die", "schla\u00b7gen", "jetzt", "die", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die, hei\u00df zur\u00fcck sich w\u00e4lzend,", "tokens": ["Die", ",", "hei\u00df", "zu\u00b7r\u00fcck", "sich", "w\u00e4l\u00b7zend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "PTKVZ", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auch euch zu Freien macht!", "tokens": ["Auch", "euch", "zu", "Frei\u00b7en", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "H\u00f6rt ihr der H\u00f6rner Gellen,", "tokens": ["H\u00f6rt", "ihr", "der", "H\u00f6r\u00b7ner", "Gel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "H\u00f6rt ihr der Rosse Trab,", "tokens": ["H\u00f6rt", "ihr", "der", "Ros\u00b7se", "Trab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seht ihr die blut'gen Wellen?", "tokens": ["Seht", "ihr", "die", "blut'\u00b7gen", "Wel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ist der Kampf bei Raab!", "tokens": ["Das", "ist", "der", "Kampf", "bei", "Ra\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Vorw\u00e4rts, ihr zottigen Streiter!\u00ab \u2013", "tokens": ["Vor\u00b7w\u00e4rts", ",", "ihr", "zot\u00b7ti\u00b7gen", "Strei\u00b7ter", "!", "\u00ab", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "$,", "PPOSAT", "ADJA", "NN", "$.", "$(", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "So klingt der Donau Schrei;", "tokens": ["So", "klingt", "der", "Do\u00b7nau", "Schrei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So w\u00e4lzt sie sich mit Grollen", "tokens": ["So", "w\u00e4lzt", "sie", "sich", "mit", "Grol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hinab durch ihre Schollen", "tokens": ["Hin\u00b7ab", "durch", "ih\u00b7re", "Schol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Zur schl\u00e4frigen T\u00fcrkei.", "tokens": ["Zur", "schl\u00e4f\u00b7ri\u00b7gen", "T\u00fcr\u00b7kei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}