{"textgrid.poem.57101": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Ofen schnauft als wie ein Hund", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ofen schnauft als wie ein Hund", "tokens": ["Der", "O\u00b7fen", "schnauft", "als", "wie", "ein", "Hund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOUS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "im Traum.", "tokens": ["im", "Traum", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Es f\u00e4hrt der Wind in seinen Schlund", "tokens": ["Es", "f\u00e4hrt", "der", "Wind", "in", "sei\u00b7nen", "Schlund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "vom Raum ...", "tokens": ["vom", "Raum", "..."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "von Sternen, fernen, angegl\u00fcht,", "tokens": ["von", "Ster\u00b7nen", ",", "fer\u00b7nen", ",", "an\u00b7ge\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJA", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der Wind ...", "tokens": ["der", "Wind", "..."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Es lauscht ihm liebend mein Gem\u00fct,", "tokens": ["Es", "lauscht", "ihm", "lie\u00b7bend", "mein", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein Kind.", "tokens": ["ein", "Kind", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Er kommt wohl noch aus Abendluft", "tokens": ["Er", "kommt", "wohl", "noch", "aus", "A\u00b7ben\u00b7dluft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "daher,", "tokens": ["da\u00b7her", ","], "token_info": ["word", "punct"], "pos": ["PAV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "in seinem Mantel h\u00e4ngt noch Duft", "tokens": ["in", "sei\u00b7nem", "Man\u00b7tel", "h\u00e4ngt", "noch", "Duft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "vom Meer,", "tokens": ["vom", "Meer", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "noch letztes Gold vom Sonnenrund", "tokens": ["noch", "letz\u00b7tes", "Gold", "vom", "Son\u00b7nen\u00b7rund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "am Saum ...", "tokens": ["am", "Saum", "..."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Der Ofen schnauft als wie ein Hund", "tokens": ["Der", "O\u00b7fen", "schnauft", "als", "wie", "ein", "Hund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOUS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "im Traum ...", "tokens": ["im", "Traum", "..."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$("], "meter": "-+", "measure": "iambic.single"}}}}}