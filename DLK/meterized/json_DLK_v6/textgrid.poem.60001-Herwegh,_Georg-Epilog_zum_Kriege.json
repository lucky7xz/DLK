{"textgrid.poem.60001": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Epilog zum Kriege", "genre": "verse", "period": "N.A.", "pub_year": 1871, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Germania, der Sieg ist dein!", "tokens": ["Ger\u00b7ma\u00b7nia", ",", "der", "Sieg", "ist", "dein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Fahnen wehn, die Glocken klingen,", "tokens": ["Die", "Fah\u00b7nen", "wehn", ",", "die", "Glo\u00b7cken", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Elsa\u00df ist dein und Lotharingen;", "tokens": ["El\u00b7sa\u00df", "ist", "dein", "und", "Lo\u00b7tha\u00b7rin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du sprichst: \u00bbJetzt mu\u00df der Bau gelingen,", "tokens": ["Du", "sprichst", ":", "\u00bb", "Jetzt", "mu\u00df", "der", "Bau", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bald holen wir den letzten Stein.\u00ab", "tokens": ["Bald", "ho\u00b7len", "wir", "den", "letz\u00b7ten", "Stein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gest\u00fctzt auf deines Schwertes Knauf,", "tokens": ["Ge\u00b7st\u00fctzt", "auf", "dei\u00b7nes", "Schwer\u00b7tes", "Knauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lobst du in frommen Telegrammen", "tokens": ["Lobst", "du", "in", "from\u00b7men", "Te\u00b7le\u00b7gram\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Herrn, von dem die Herren stammen,", "tokens": ["Den", "Herrn", ",", "von", "dem", "die", "Her\u00b7ren", "stam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und aus Zerst\u00f6rung, Tod und Flammen", "tokens": ["Und", "aus", "Zer\u00b7st\u00f6\u00b7rung", ",", "Tod", "und", "Flam\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Steigt hei\u00df dein Dank zum Himmel auf.", "tokens": ["Steigt", "hei\u00df", "dein", "Dank", "zum", "Him\u00b7mel", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nach vierundzwanzig Schlachten liegt", "tokens": ["Nach", "vie\u00b7rund\u00b7zwan\u00b7zig", "Schlach\u00b7ten", "liegt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Feind am Boden, \u00fcberwunden;", "tokens": ["Der", "Feind", "am", "Bo\u00b7den", ",", "\u00fc\u00b7berw\u00b7un\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bis in die Stadt voll Blut und Wunden,", "tokens": ["Bis", "in", "die", "Stadt", "voll", "Blut", "und", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die keinen Retterarm gefunden,", "tokens": ["Die", "kei\u00b7nen", "Ret\u00b7ter\u00b7arm", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Brichst du dir Bahn \u2013 du hast gesiegt!", "tokens": ["Brichst", "du", "dir", "Bahn", "\u2013", "du", "hast", "ge\u00b7siegt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "NN", "$(", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schwarz, wei\u00df und rot! um ", "tokens": ["Schwarz", ",", "wei\u00df", "und", "rot", "!", "um"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "VVFIN", "KON", "ADJD", "$.", "KOUI"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Vereinigt stehen S\u00fcd und Norden;", "tokens": ["Ver\u00b7ei\u00b7nigt", "ste\u00b7hen", "S\u00fcd", "und", "Nor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist im ruhmgekr\u00f6nten Morden", "tokens": ["Du", "bist", "im", "ruhm\u00b7ge\u00b7kr\u00f6n\u00b7ten", "Mor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das erste Land der Welt geworden:", "tokens": ["Das", "ers\u00b7te", "Land", "der", "Welt", "ge\u00b7wor\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Germania, mir graut vor dir!", "tokens": ["Ger\u00b7ma\u00b7nia", ",", "mir", "graut", "vor", "dir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mir graut vor dir, ich glaube fast,", "tokens": ["Mir", "graut", "vor", "dir", ",", "ich", "glau\u00b7be", "fast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du, in argen Wahn versunken,", "tokens": ["Da\u00df", "du", ",", "in", "ar\u00b7gen", "Wahn", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit falscher Gr\u00f6\u00dfe suchst zu prunken", "tokens": ["Mit", "fal\u00b7scher", "Gr\u00f6\u00b7\u00dfe", "suchst", "zu", "prun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und da\u00df du, gottesgnadentrunken,", "tokens": ["Und", "da\u00df", "du", ",", "got\u00b7tes\u00b7gna\u00b7den\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das Menschenrecht vergessen hast.", "tokens": ["Das", "Men\u00b7schen\u00b7recht", "ver\u00b7ges\u00b7sen", "hast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Schon, lenkt ein Kaiser dich am Zaum,", "tokens": ["Schon", ",", "lenkt", "ein", "Kai\u00b7ser", "dich", "am", "Zaum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein strammer, strenger Zepterhalter.", "tokens": ["Ein", "stram\u00b7mer", ",", "stren\u00b7ger", "Zep\u00b7ter\u00b7hal\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hofbarden singen ihre Psalter", "tokens": ["Hof\u00b7bar\u00b7den", "sin\u00b7gen", "ih\u00b7re", "Psal\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem auferstandnen Mittelalter,", "tokens": ["Dem", "auf\u00b7er\u00b7stand\u00b7nen", "Mit\u00b7tel\u00b7al\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und 89 wird ein Traum.", "tokens": ["Und", "89", "wird", "ein", "Traum", "."], "token_info": ["word", "number", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Ein Traum? Du sahst, wie Frankreich fiel", "tokens": ["Ein", "Traum", "?", "Du", "sahst", ",", "wie", "Fran\u00b7kreich", "fiel"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN", "$,", "PWAV", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch einen C\u00e4sar, sahst die S\u00fchne", "tokens": ["Durch", "ei\u00b7nen", "C\u00e4\u00b7sar", ",", "sahst", "die", "S\u00fch\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vollzogen auf der Schreckensb\u00fchne \u2013", "tokens": ["Voll\u00b7zo\u00b7gen", "auf", "der", "Schre\u00b7ckens\u00b7b\u00fch\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Deutschland, gedeihe, wachse, gr\u00fcne,", "tokens": ["Deutschland", ",", "ge\u00b7dei\u00b7he", ",", "wach\u00b7se", ",", "gr\u00fc\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "VVFIN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gel\u00e4utert durch dies Trauerspiel!", "tokens": ["Ge\u00b7l\u00e4u\u00b7tert", "durch", "dies", "Trau\u00b7er\u00b7spiel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PDS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}