{"textgrid.poem.63981": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Vorfr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weltanfassende, fr\u00f6hliche Dummheit,", "tokens": ["Welt\u00b7an\u00b7fas\u00b7sen\u00b7de", ",", "fr\u00f6h\u00b7li\u00b7che", "Dumm\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sprie\u00dfendes Singen seimigen Grases,", "tokens": ["Sprie\u00b7\u00dfen\u00b7des", "Sin\u00b7gen", "sei\u00b7mi\u00b7gen", "Gra\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wohligschelmisch Gew\u00f6lk.", "tokens": ["Woh\u00b7lig\u00b7schel\u00b7misch", "Ge\u00b7w\u00f6lk", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Weicher Schalmeient\u00f6ne,", "tokens": ["Wei\u00b7cher", "Schal\u00b7mei\u00b7en\u00b7t\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Sinniges Gr\u00fcbchen.", "tokens": ["Sin\u00b7ni\u00b7ges", "Gr\u00fcb\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Am markig umwundenen Knie,", "tokens": ["Am", "mar\u00b7kig", "um\u00b7wun\u00b7de\u00b7nen", "Knie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Ziehet's spielend hin:", "tokens": ["Zie\u00b7het's", "spie\u00b7lend", "hin", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Fromm in Sonne,", "tokens": ["Fromm", "in", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Atmende Auen.", "tokens": ["At\u00b7men\u00b7de", "Au\u00b7en", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Reime und Ma\u00dfe,", "tokens": ["Rei\u00b7me", "und", "Ma\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Tabulatur der St\u00e4nde", "tokens": ["Ta\u00b7bu\u00b7la\u00b7tur", "der", "St\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Gez\u00e4hlt am peinlich", "tokens": ["Ge\u00b7z\u00e4hlt", "am", "pein\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "PTKA", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Gekr\u00fcmmten Finger \u2013", "tokens": ["Ge\u00b7kr\u00fcmm\u00b7ten", "Fin\u00b7ger", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Das ist vor\u00fcber.", "tokens": ["Das", "ist", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Bl\u00f6de zwinkernd", "tokens": ["Bl\u00f6\u00b7de", "zwin\u00b7kernd"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Putzt die stechenden Brillengl\u00e4ser", "tokens": ["Putzt", "die", "ste\u00b7chen\u00b7den", "Bril\u00b7len\u00b7gl\u00e4\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Heisere Gescheitheit.", "tokens": ["Hei\u00b7se\u00b7re", "Ge\u00b7schei\u00b7theit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Melodische Seele der Welt,", "tokens": ["Me\u00b7lo\u00b7di\u00b7sche", "See\u00b7le", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Fr\u00fchling, Schalmei,", "tokens": ["Fr\u00fch\u00b7ling", ",", "Schal\u00b7mei", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Spiele, spiele uns alle hin", "tokens": ["Spie\u00b7le", ",", "spie\u00b7le", "uns", "al\u00b7le", "hin"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "PIS", "PTKVZ"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "In alles Sch\u00f6nheit tanzendes Leben.", "tokens": ["In", "al\u00b7les", "Sch\u00f6n\u00b7heit", "tan\u00b7zen\u00b7des", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "In das muntere Gesetz", "tokens": ["In", "das", "mun\u00b7te\u00b7re", "Ge\u00b7setz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Alle Sterne strahlenden", "tokens": ["Al\u00b7le", "Ster\u00b7ne", "strah\u00b7len\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Liebenden Reigens.", "tokens": ["Lie\u00b7ben\u00b7den", "Rei\u00b7gens", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Warum kommen nur die Menschen nicht,", "tokens": ["Wa\u00b7rum", "kom\u00b7men", "nur", "die", "Men\u00b7schen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wollen sie nicht?", "tokens": ["Wol\u00b7len", "sie", "nicht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Nun \u2013", "tokens": ["Nun", "\u2013"], "token_info": ["word", "punct"], "pos": ["ADV", "$("], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Und die spatzschreienden Hecken", "tokens": ["Und", "die", "spatz\u00b7schrei\u00b7en\u00b7den", "He\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Und die paarenden Tiere sagen:", "tokens": ["Und", "die", "paa\u00b7ren\u00b7den", "Tie\u00b7re", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Die Welt geht weiter.", "tokens": ["Die", "Welt", "geht", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Auf vermoderter Triebe Rost", "tokens": ["Auf", "ver\u00b7mo\u00b7der\u00b7ter", "Trie\u00b7be", "Rost"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Immer wieder nachquillend", "tokens": ["Im\u00b7mer", "wie\u00b7der", "nach\u00b7quil\u00b7lend"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVPP"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.10": {"text": "Tauender Teufel bereuender Frost.", "tokens": ["Tau\u00b7en\u00b7der", "Teu\u00b7fel", "be\u00b7reu\u00b7en\u00b7der", "Frost", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf der gr\u00fcnen weiteblauen", "tokens": ["Auf", "der", "gr\u00fc\u00b7nen", "wei\u00b7te\u00b7blau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Himmelswiese", "tokens": ["Him\u00b7mels\u00b7wie\u00b7se"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Dauern hin, spielend versonnen,", "tokens": ["Dau\u00b7ern", "hin", ",", "spie\u00b7lend", "ver\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "ADJD", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Weltverlorne Lichtungen,", "tokens": ["Welt\u00b7ver\u00b7lor\u00b7ne", "Lich\u00b7tun\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAT", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Locken r\u00f6tlich tr\u00e4umende Kindesk\u00f6pfe.", "tokens": ["Lo\u00b7cken", "r\u00f6t\u00b7lich", "tr\u00e4u\u00b7men\u00b7de", "Kin\u00b7des\u00b7k\u00f6p\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.6": {"text": "Gelbes rotes strotzendes Feuer", "tokens": ["Gel\u00b7bes", "ro\u00b7tes", "strot\u00b7zen\u00b7des", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Roter Blumen.", "tokens": ["Ro\u00b7ter", "Blu\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Blitzelt auf br\u00e4unlichen St\u00e4ndern", "tokens": ["Blit\u00b7zelt", "auf", "br\u00e4un\u00b7li\u00b7chen", "St\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.9": {"text": "Suchend wach ...", "tokens": ["Su\u00b7chend", "wach", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Entgilbender Himmel \u2013", "tokens": ["Ent\u00b7gil\u00b7ben\u00b7der", "Him\u00b7mel", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Ist es nicht w\u00e4rmer schon oben?", "tokens": ["Ist", "es", "nicht", "w\u00e4r\u00b7mer", "schon", "o\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "ADV", "ADV", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.12": {"text": "Da Gott Vater erst", "tokens": ["Da", "Gott", "Va\u00b7ter", "erst"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "NN", "ADV"], "meter": "--+-+", "measure": "anapaest.init"}, "line.13": {"text": "Und erste Welt;", "tokens": ["Und", "ers\u00b7te", "Welt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "War das nicht so wie himmlische", "tokens": ["War", "das", "nicht", "so", "wie", "himm\u00b7li\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "PTKNEG", "ADV", "KOKOM", "ADJA"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "Weltanfassende Dummheit.", "tokens": ["Welt\u00b7an\u00b7fas\u00b7sen\u00b7de", "Dumm\u00b7heit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}