{"textgrid.poem.46342": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "An Canidien", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Canidia! es tr\u00fcgt der Fr\u00fcling deiner Wangen;", "tokens": ["Ca\u00b7ni\u00b7dia", "!", "es", "tr\u00fcgt", "der", "Fr\u00fc\u00b7ling", "dei\u00b7ner", "Wan\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Du nicht, dein Kr\u00e4mer macht uns Lust;", "tokens": ["Du", "nicht", ",", "dein", "Kr\u00e4\u00b7mer", "macht", "uns", "Lust", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch sah ich neulich deine Brust,", "tokens": ["Auch", "sah", "ich", "neu\u00b7lich", "dei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Haar, Z\u00e4hn' und linkes Aug' in seiner Bude hangen.", "tokens": ["Haar", ",", "Z\u00e4hn'", "und", "lin\u00b7kes", "Aug'", "in", "sei\u00b7ner", "Bu\u00b7de", "han\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}