{"textgrid.poem.64154": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig Manfreds Grab", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den toten Manfred pl\u00fcnderten Burgunden,", "tokens": ["Den", "to\u00b7ten", "Man\u00b7fred", "pl\u00fcn\u00b7der\u00b7ten", "Bur\u00b7gun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zerfleischend ihn mit zwanzig Lanzenwunden,", "tokens": ["Zer\u00b7flei\u00b7schend", "ihn", "mit", "zwan\u00b7zig", "Lan\u00b7zen\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gern gab dem Ketzer jeder einen Stich:", "tokens": ["Gern", "gab", "dem", "Ket\u00b7zer", "je\u00b7der", "ei\u00b7nen", "Stich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Karl von Anjou trat, der bleifarbbleiche,", "tokens": ["Und", "Karl", "von", "An\u00b7jou", "trat", ",", "der", "blei\u00b7farb\u00b7blei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "NE", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit ehrnem Fu\u00df fest auf die Brust der Leiche", "tokens": ["Mit", "ehr\u00b7nem", "Fu\u00df", "fest", "auf", "die", "Brust", "der", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und sprach: \u00bbAas bist du \u2013 Herr bin ich.\u00ab", "tokens": ["Und", "sprach", ":", "\u00bb", "Aas", "bist", "du", "\u2013", "Herr", "bin", "ich", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "NE", "VAFIN", "PPER", "$(", "NN", "VAFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf \u00f6dem Heidemoor verscharrten Knechte", "tokens": ["Auf", "\u00f6\u00b7dem", "Hei\u00b7de\u00b7moor", "ver\u00b7scharr\u00b7ten", "Knech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Abseit vom Weg ihn unter Dorngeflechte. \u2013", "tokens": ["Ab\u00b7seit", "vom", "Weg", "ihn", "un\u00b7ter", "Dorn\u00b7ge\u00b7flech\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Kr\u00fcppel, dem er wohlgetan einmal,", "tokens": ["Ein", "Kr\u00fcp\u00b7pel", ",", "dem", "er", "wohl\u00b7ge\u00b7tan", "ein\u00b7mal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wollt' ihm ein Holzkreuz auf die Grube setzen:", "tokens": ["Wollt'", "ihm", "ein", "Holz\u00b7kreuz", "auf", "die", "Gru\u00b7be", "set\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Jedoch mit Hunden lie\u00df hinweg ihn hetzen", "tokens": ["Je\u00b7doch", "mit", "Hun\u00b7den", "lie\u00df", "hin\u00b7weg", "ihn", "het\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVFIN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Johann, Cosenzas Kardinal.", "tokens": ["Jo\u00b7hann", ",", "Co\u00b7sen\u00b7zas", "Kar\u00b7di\u00b7nal", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Ein Dornbusch nur war Merkmal jener St\u00e4tte. \u2013", "tokens": ["Ein", "Dorn\u00b7busch", "nur", "war", "Merk\u00b7mal", "je\u00b7ner", "St\u00e4t\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "NN", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch nach sechs Jahren tr\u00e4umt' im Purpurbette", "tokens": ["Doch", "nach", "sechs", "Jah\u00b7ren", "tr\u00e4umt'", "im", "Pur\u00b7pur\u00b7bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "CARD", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Anjou, \u2013 um sich schlug er mit der Hand! \u2013", "tokens": ["Dem", "An\u00b7jou", ",", "\u2013", "um", "sich", "schlug", "er", "mit", "der", "Hand", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "$(", "KOUI", "PRF", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den toten Manfred h\u00f6r' er drohend sprechen:", "tokens": ["Den", "to\u00b7ten", "Man\u00b7fred", "h\u00f6r'", "er", "dro\u00b7hend", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "\u00bbdein Reich wird spurlos in Italien brechen:", "tokens": ["\u00bb", "dein", "Reich", "wird", "spur\u00b7los", "in", "I\u00b7ta\u00b7li\u00b7en", "bre\u00b7chen", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "NE", "VVINF", "$."], "meter": "-+-+--++--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ich ruhe bald in freiem Land.\u00ab", "tokens": ["Ich", "ru\u00b7he", "bald", "in", "frei\u00b7em", "Land", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Empor fuhr der Tyrann: \u00bbDies Omen wend' ich!", "tokens": ["Em\u00b7por", "fuhr", "der", "Ty\u00b7rann", ":", "\u00bb", "Dies", "O\u00b7men", "wend'", "ich", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$.", "$(", "PDS", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Ketzers ausgegrab'ne Knochen send' ich", "tokens": ["Des", "Ket\u00b7zers", "aus\u00b7ge\u00b7grab'\u00b7ne", "Kno\u00b7chen", "send'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach Frankreich, dort zu senken sie ins Meer!\u00ab \u2013", "tokens": ["Nach", "Fran\u00b7kreich", ",", "dort", "zu", "sen\u00b7ken", "sie", "ins", "Meer", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NE", "$,", "ADV", "PTKZU", "VVINF", "PPER", "APPRART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und auf das Schlachtfeld sandt' er seine Boten,", "tokens": ["Und", "auf", "das", "Schlacht\u00b7feld", "sandt'", "er", "sei\u00b7ne", "Bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Viel hundert H\u00e4scher nach dem ", "tokens": ["Viel", "hun\u00b7dert", "H\u00e4\u00b7scher", "nach", "dem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "NN", "APPR", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie kamen heim, die H\u00e4nde leer.", "tokens": ["Sie", "ka\u00b7men", "heim", ",", "die", "H\u00e4n\u00b7de", "leer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbherr\u00ab \u2013 sprachen sie \u2013 \u00bbmag uns dein Zorn verschlingen \u2013", "tokens": ["\u00bb", "herr", "\u00ab", "\u2013", "spra\u00b7chen", "sie", "\u2013", "\u00bb", "mag", "uns", "dein", "Zorn", "ver\u00b7schlin\u00b7gen", "\u2013"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$(", "$(", "VVFIN", "PPER", "$(", "$(", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wir k\u00f6nnen diesen K\u00f6nig nicht dir bringen:", "tokens": ["Wir", "k\u00f6n\u00b7nen", "die\u00b7sen", "K\u00f6\u00b7nig", "nicht", "dir", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDAT", "NN", "PTKNEG", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Dornbusch \u2013 wie du wei\u00dft \u2013 stand an dem Ort:", "tokens": ["Ein", "Dorn\u00b7busch", "\u2013", "wie", "du", "wei\u00dft", "\u2013", "stand", "an", "dem", "Ort", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "PPER", "VVFIN", "$(", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der mu\u00df gewesen sein von wilden Rosen:", "tokens": ["Der", "mu\u00df", "ge\u00b7we\u00b7sen", "sein", "von", "wil\u00b7den", "Ro\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "VAPP", "VAINF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn unabsehbar jetzt im Lenzwind kosen", "tokens": ["Denn", "un\u00b7ab\u00b7seh\u00b7bar", "jetzt", "im", "Lenz\u00b7wind", "ko\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Viel tausend, tausend Rosen dort.", "tokens": ["Viel", "tau\u00b7send", ",", "tau\u00b7send", "Ro\u00b7sen", "dort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "CARD", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u203aden Wald der Rosen\u2039 nennt den Ort die Menge;", "tokens": ["\u203a", "den", "Wald", "der", "Ro\u00b7sen", "\u2039", "nennt", "den", "Ort", "die", "Men\u00b7ge", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ART", "NN", "$(", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Unscheidbar wogt das duft'ge Strauchgedr\u00e4nge:", "tokens": ["Un\u00b7scheid\u00b7bar", "wogt", "das", "duft'\u00b7ge", "Strauch\u00b7ge\u00b7dr\u00e4n\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Unm\u00f6glich ward, da\u00df man das Grab erkennt!\u00ab \u2013 \u2013", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "ward", ",", "da\u00df", "man", "das", "Grab", "er\u00b7kennt", "!", "\u00ab", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ADJD", "VAFIN", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$.", "$(", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Lang' ist des Anjous blutig Reich zerfallen:", "tokens": ["Lang'", "ist", "des", "An\u00b7jous", "blu\u00b7tig", "Reich", "zer\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Um Manfred singt ein Heer von Nachtigallen", "tokens": ["Um", "Man\u00b7fred", "singt", "ein", "Heer", "von", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "NE", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im Rosenwald von Benevent.", "tokens": ["Im", "Ro\u00b7sen\u00b7wald", "von", "Be\u00b7ne\u00b7vent", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}