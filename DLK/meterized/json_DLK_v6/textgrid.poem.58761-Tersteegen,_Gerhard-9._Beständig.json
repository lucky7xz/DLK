{"textgrid.poem.58761": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "9. Best\u00e4ndig", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unwandelbar bist du allein,", "tokens": ["Un\u00b7wan\u00b7del\u00b7bar", "bist", "du", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du gibst Bestehn und standhaft sein,", "tokens": ["Du", "gibst", "Be\u00b7stehn", "und", "stand\u00b7haft", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum wird dein Werk auch nicht vergehen.", "tokens": ["Drum", "wird", "dein", "Werk", "auch", "nicht", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein fest Gesetz die Welt regiert,", "tokens": ["Dein", "fest", "Ge\u00b7setz", "die", "Welt", "re\u00b7giert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich ohne Zwang zum Wohlsein f\u00fchrt;", "tokens": ["Mich", "oh\u00b7ne", "Zwang", "zum", "Wohl\u00b7sein", "f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich folg', drum mu\u00df mein Gl\u00fcck bestehen.", "tokens": ["Ich", "fol\u00b7g'", ",", "drum", "mu\u00df", "mein", "Gl\u00fcck", "be\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PAV", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Ob Licht und Trost und Kraft mich l\u00e4\u00dft,", "tokens": ["Ob", "Licht", "und", "Trost", "und", "Kraft", "mich", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Gnaden-Salzbund bleibt doch fest.", "tokens": ["Dein", "Gna\u00b7den\u00b7Sa\u00b7lz\u00b7bund", "bleibt", "doch", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Halleluja! Halleluja!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ITJ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}