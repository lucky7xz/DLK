{"textgrid.poem.57066": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die Oste", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er ersann zur Weste", "tokens": ["Er", "er\u00b7sann", "zur", "Wes\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "eines Nachts die Oste!", "tokens": ["ei\u00b7nes", "Nachts", "die", "Os\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "sprach: \u00bbWas es auch koste!\u00ab \u2013", "tokens": ["sprach", ":", "\u00bb", "Was", "es", "auch", "kos\u00b7te", "!", "\u00ab", "\u2013"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "$.", "$(", "PWS", "PPER", "ADV", "VVFIN", "$.", "$(", "$("], "meter": "+---+-", "measure": "dactylic.init"}, "line.4": {"text": "sprach (mit gro\u00dfer Geste):", "tokens": ["sprach", "(", "mit", "gro\u00b7\u00dfer", "Ges\u00b7te", ")", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$(", "APPR", "ADJA", "NN", "$(", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbla\u00dft uns auch von hinten", "tokens": ["\u00bb", "la\u00dft", "uns", "auch", "von", "hin\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "PPER", "ADV", "APPR", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "seidne Hyazinthen", "tokens": ["seid\u00b7ne", "Hy\u00b7a\u00b7zin\u00b7then"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "samt Karfunkelkn\u00f6pfen", "tokens": ["samt", "Kar\u00b7fun\u00b7kel\u00b7kn\u00f6p\u00b7fen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "unsern Rumpf umkr\u00f6pfen!", "tokens": ["un\u00b7sern", "Rumpf", "um\u00b7kr\u00f6p\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Nicht nur auf dem Magen", "tokens": ["Nicht", "nur", "auf", "dem", "Ma\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "la\u00dft uns Uhren tragen,", "tokens": ["la\u00dft", "uns", "Uh\u00b7ren", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "nicht nur \u00fcberm Herzen", "tokens": ["nicht", "nur", "\u00fc\u00b7berm", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "unsre Sparsesterzen!", "tokens": ["uns\u00b7re", "Spar\u00b7ses\u00b7ter\u00b7zen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Fort mit dem betre\u00dften", "tokens": ["Fort", "mit", "dem", "be\u00b7tre\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Privileg der Westen!", "tokens": ["Pri\u00b7vi\u00b7leg", "der", "Wes\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Gleichheit allerst\u00fccken!", "tokens": ["Gleich\u00b7heit", "al\u00b7ler\u00b7st\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Osten f\u00fcr den R\u00fccken!\u00ab", "tokens": ["Os\u00b7ten", "f\u00fcr", "den", "R\u00fc\u00b7cken", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und sieh da, kein Schneider", "tokens": ["Und", "sieh", "da", ",", "kein", "Schnei\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVIMP", "ADV", "$,", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "sagte hierzu: Leider \u2013!", "tokens": ["sag\u00b7te", "hier\u00b7zu", ":", "Lei\u00b7der", "\u2013", "!"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "PAV", "$.", "ADV", "$(", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hunderttausend Scheren", "tokens": ["Hun\u00b7dert\u00b7tau\u00b7send", "Sche\u00b7ren"], "token_info": ["word", "word"], "pos": ["CARD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "sah man Stoffe queren ...", "tokens": ["sah", "man", "Stof\u00b7fe", "que\u00b7ren", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ungez\u00e4hlte Posten", "tokens": ["Un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Pos\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "wurden sch\u00f6nster Osten", "tokens": ["wur\u00b7den", "sch\u00f6ns\u00b7ter", "Os\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "noch vor seinem Tode", "tokens": ["noch", "vor", "sei\u00b7nem", "To\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "\u00bbletzter Schrei\u00ab der Mode.", "tokens": ["\u00bb", "letz\u00b7ter", "Schrei", "\u00ab", "der", "Mo\u00b7de", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$(", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}