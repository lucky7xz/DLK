{"textgrid.poem.35806": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich hatte viel gefehlet,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hatte viel gefehlet,", "tokens": ["Ich", "hat\u00b7te", "viel", "ge\u00b7feh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er hatte viel verziehn,", "tokens": ["Er", "hat\u00b7te", "viel", "ver\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Viel Gnaden mir verliehn,", "tokens": ["Viel", "Gna\u00b7den", "mir", "ver\u00b7liehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zur Braut mich auserw\u00e4hlet,", "tokens": ["Zur", "Braut", "mich", "au\u00b7ser\u00b7w\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zum Kampf den Muth gest\u00e4hlet", "tokens": ["Zum", "Kampf", "den", "Muth", "ge\u00b7st\u00e4h\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch ew'ger Palme Gr\u00fcn.", "tokens": ["Durch", "ew'\u00b7ger", "Pal\u00b7me", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Heerde zugez\u00e4hlet", "tokens": ["Der", "Heer\u00b7de", "zu\u00b7ge\u00b7z\u00e4h\u00b7let"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Soll ich, dem Lamm verm\u00e4hlet,", "tokens": ["Soll", "ich", ",", "dem", "Lamm", "ver\u00b7m\u00e4h\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Nun keinem Andern bl\u00fch'n \u2013", "tokens": ["Nun", "kei\u00b7nem", "An\u00b7dern", "bl\u00fch'n", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Herr! Der Du mich erw\u00e4hlet:", "tokens": ["Herr", "!", "Der", "Du", "mich", "er\u00b7w\u00e4h\u00b7let", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr Dich nur will ich gl\u00fch'n.", "tokens": ["F\u00fcr", "Dich", "nur", "will", "ich", "gl\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PPER", "ADV", "VMFIN", "PPER", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}