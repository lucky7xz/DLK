{"textgrid.poem.45828": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[die Liebe fing mich ein mit ihren Netzen]", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe fing mich ein mit ihren Netzen,", "tokens": ["Die", "Lie\u00b7be", "fing", "mich", "ein", "mit", "ih\u00b7ren", "Net\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Hoffnung bietet mir die Freiheit an;", "tokens": ["Und", "Hoff\u00b7nung", "bie\u00b7tet", "mir", "die", "Frei\u00b7heit", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich binde mich den heiligen Gesetzen,", "tokens": ["Ich", "bin\u00b7de", "mich", "den", "hei\u00b7li\u00b7gen", "Ge\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und alle Pflicht erscheint ein leerer Wahn.", "tokens": ["Und", "al\u00b7le", "Pflicht", "er\u00b7scheint", "ein", "lee\u00b7rer", "Wahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es st\u00fcrzen bald des alten Glaubens G\u00f6tzen,", "tokens": ["Es", "st\u00fcr\u00b7zen", "bald", "des", "al\u00b7ten", "Glau\u00b7bens", "G\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zieht die Natur mich so mit Liebe an.", "tokens": ["Zieht", "die", "Na\u00b7tur", "mich", "so", "mit", "Lie\u00b7be", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O s\u00fc\u00dfer Tod, in Liebe neu geboren,", "tokens": ["O", "s\u00fc\u00b7\u00dfer", "Tod", ",", "in", "Lie\u00b7be", "neu", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bin ich der Welt, doch sie mir nicht verloren.", "tokens": ["Bin", "ich", "der", "Welt", ",", "doch", "sie", "mir", "nicht", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "KON", "PPER", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}