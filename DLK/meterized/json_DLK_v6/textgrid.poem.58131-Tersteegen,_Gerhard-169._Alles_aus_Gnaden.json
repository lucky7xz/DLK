{"textgrid.poem.58131": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "169. Alles aus Gnaden", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott hat so viel mit mir und meiner S\u00fcnd' zu tun,", "tokens": ["Gott", "hat", "so", "viel", "mit", "mir", "und", "mei\u00b7ner", "S\u00fcnd'", "zu", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "APPR", "PPER", "KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "W\u00e4r' er nicht selber Gott, er k\u00f6nnt' wohl nimmer ruhn;", "tokens": ["W\u00e4r'", "er", "nicht", "sel\u00b7ber", "Gott", ",", "er", "k\u00f6nnt'", "wohl", "nim\u00b7mer", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "NN", "$,", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verdienste meiner Werk', die hei\u00dfen H\u00f6llenpein,", "tokens": ["Ver\u00b7diens\u00b7te", "mei\u00b7ner", "Werk'", ",", "die", "hei\u00b7\u00dfen", "H\u00f6l\u00b7len\u00b7pein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn ich noch selig werd', so wird's aus Gnaden sein.", "tokens": ["Wenn", "ich", "noch", "se\u00b7lig", "werd'", ",", "so", "wird's", "aus", "Gna\u00b7den", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,", "ADV", "VAFIN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}