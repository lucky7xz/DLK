{"dta.poem.9444": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Vi .  \n Die Sechste Abtheilung.  \n Die Jungfrauen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Liebster ist jtzund spazieren gegangen/", "tokens": ["Mein", "Liebs\u00b7ter", "ist", "jt\u00b7zund", "spa\u00b7zie\u00b7ren", "ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVINF", "VVPP", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In semen W\u00fcrtzgarten und frischet den Sinn/", "tokens": ["In", "se\u00b7men", "W\u00fcrtz\u00b7gar\u00b7ten", "und", "fri\u00b7schet", "den", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Er binder die Narden und strencher an stangen/", "tokens": ["Er", "bin\u00b7der", "die", "Nar\u00b7den", "und", "stren\u00b7cher", "an", "stan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "KON", "ADJD", "APPR", "ADJA", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Bricht Rosen und setzet zun Liljen sich hin:", "tokens": ["Bricht", "Ro\u00b7sen", "und", "set\u00b7zet", "zun", "Lil\u00b7jen", "sich", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "VVFIN", "APPRART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Mein Liebster ist mein/", "tokens": ["Mein", "Liebs\u00b7ter", "ist", "mein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Das wei\u00df ich allein;", "tokens": ["Das", "wei\u00df", "ich", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Der unter den Rosen sich weidet/", "tokens": ["Der", "un\u00b7ter", "den", "Ro\u00b7sen", "sich", "wei\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "VVFIN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Kein trauren uns scheidet.", "tokens": ["Kein", "trau\u00b7ren", "uns", "schei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}