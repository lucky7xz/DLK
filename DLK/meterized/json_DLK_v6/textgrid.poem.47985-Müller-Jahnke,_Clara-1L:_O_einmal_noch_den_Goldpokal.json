{"textgrid.poem.47985": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: O einmal noch den Goldpokal", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O einmal noch den Goldpokal", "tokens": ["O", "ein\u00b7mal", "noch", "den", "Gold\u00b7po\u00b7kal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "an meine Lippen setzen,", "tokens": ["an", "mei\u00b7ne", "Lip\u00b7pen", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "in hast'gem Zug zum letztenmal", "tokens": ["in", "hast'\u00b7gem", "Zug", "zum", "letz\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mit spr\u00fch'ndem Schaum sie netzen!", "tokens": ["mit", "spr\u00fch'n\u00b7dem", "Schaum", "sie", "net\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O einmal nur in j\u00e4her Lust", "tokens": ["O", "ein\u00b7mal", "nur", "in", "j\u00e4\u00b7her", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "auflodern und \u2013 verderben, \u2013", "tokens": ["auf\u00b7lo\u00b7dern", "und", "\u2013", "ver\u00b7der\u00b7ben", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["VVINF", "KON", "$(", "VVFIN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "dann mag verwehn der Rosenblust,", "tokens": ["dann", "mag", "ver\u00b7wehn", "der", "Ro\u00b7sen\u00b7blust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "dann schmettert hin, ihr Scherben!! \u2013", "tokens": ["dann", "schmet\u00b7tert", "hin", ",", "ihr", "Scher\u00b7ben", "!!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gewandert bin ich ohne Stern,", "tokens": ["Ge\u00b7wan\u00b7dert", "bin", "ich", "oh\u00b7ne", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "fand nirgends Ruh und Segen,", "tokens": ["fand", "nir\u00b7gends", "Ruh", "und", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "das holde Gl\u00fcck zog meilenfern", "tokens": ["das", "hol\u00b7de", "Gl\u00fcck", "zog", "mei\u00b7len\u00b7fern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "vor\u00fcber meinen Wegen;", "tokens": ["vor\u00b7\u00fc\u00b7ber", "mei\u00b7nen", "We\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zuweilen klang in stiller Nacht", "tokens": ["Zu\u00b7wei\u00b7len", "klang", "in", "stil\u00b7ler", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "sein Ruf mir leis und linde,", "tokens": ["sein", "Ruf", "mir", "leis", "und", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "er klang so s\u00fc\u00df, er lockt mit Macht", "tokens": ["er", "klang", "so", "s\u00fc\u00df", ",", "er", "lockt", "mit", "Macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und ist verweht im Winde \u2013 \u2013 \u2013", "tokens": ["und", "ist", "ver\u00b7weht", "im", "Win\u00b7de", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VAFIN", "VVFIN", "APPRART", "NN", "$(", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du lichte Welt, du gr\u00fcner Hag,", "tokens": ["Du", "lich\u00b7te", "Welt", ",", "du", "gr\u00fc\u00b7ner", "Hag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "geschm\u00fcckt mit Blumenkr\u00e4nzen,", "tokens": ["ge\u00b7schm\u00fcckt", "mit", "Blu\u00b7men\u00b7kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "du sonnengoldner Sommertag,", "tokens": ["du", "son\u00b7nen\u00b7gold\u00b7ner", "Som\u00b7mer\u00b7tag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "nicht mir gilt euer Gl\u00e4nzen!", "tokens": ["nicht", "mir", "gilt", "eu\u00b7er", "Gl\u00e4n\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Verrauscht, verrauscht ist Spiel und Tanz,", "tokens": ["Ver\u00b7rauscht", ",", "ver\u00b7rauscht", "ist", "Spiel", "und", "Tanz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "es welkt das Gr\u00fcn der Linde:", "tokens": ["es", "welkt", "das", "Gr\u00fcn", "der", "Lin\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auf meinem Grab der Totenkranz,", "tokens": ["Auf", "mei\u00b7nem", "Grab", "der", "To\u00b7ten\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "bald flattert er im Winde!", "tokens": ["bald", "flat\u00b7tert", "er", "im", "Win\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O einmal noch den Goldpokal", "tokens": ["O", "ein\u00b7mal", "noch", "den", "Gold\u00b7po\u00b7kal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "an meine Lippen setzen,", "tokens": ["an", "mei\u00b7ne", "Lip