{"dta.poem.19422": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "155.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Menschengeist, du bist zu Gottes Thron gerufen;", "tokens": ["O", "Men\u00b7schen\u00b7geist", ",", "du", "bist", "zu", "Got\u00b7tes", "Thron", "ge\u00b7ru\u00b7fen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch welches Wegs du kommst, das \u00e4ndert dort die Stufen.", "tokens": ["Doch", "wel\u00b7ches", "Wegs", "du", "kommst", ",", "das", "\u00e4n\u00b7dert", "dort", "die", "Stu\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kommst du von deinem Grab, so bist du aufgenommen;", "tokens": ["Kommst", "du", "von", "dei\u00b7nem", "Grab", ",", "so", "bist", "du", "auf\u00b7ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch kommst du aus der Welt, so bist du erst willkommen.", "tokens": ["Doch", "kommst", "du", "aus", "der", "Welt", ",", "so", "bist", "du", "erst", "will\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum warte nicht durchs Grab den Weg zum Herren ab,", "tokens": ["Drum", "war\u00b7te", "nicht", "durchs", "Grab", "den", "Weg", "zum", "Her\u00b7ren", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "APPRART", "NN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und aus dem Leben nimm zu ihm den Pilgerstab.", "tokens": ["Und", "aus", "dem", "Le\u00b7ben", "nimm", "zu", "ihm", "den", "Pil\u00b7ger\u00b7stab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}