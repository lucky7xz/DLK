{"textgrid.poem.34708": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die sch\u00f6nen Augen der Fr\u00fchlingsnacht,", "tokens": ["Die", "sch\u00f6\u00b7nen", "Au\u00b7gen", "der", "Fr\u00fch\u00b7lings\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie schauen so tr\u00f6stend nieder:", "tokens": ["Sie", "schau\u00b7en", "so", "tr\u00f6s\u00b7tend", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hat dich die Liebe so kleinlich gemacht,", "tokens": ["Hat", "dich", "die", "Lie\u00b7be", "so", "klein\u00b7lich", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Liebe, sie hebt dich wieder.", "tokens": ["Die", "Lie\u00b7be", ",", "sie", "hebt", "dich", "wie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Auf gr\u00fcner Linde sitzt und singt", "tokens": ["Auf", "gr\u00fc\u00b7ner", "Lin\u00b7de", "sitzt", "und", "singt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NE", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die s\u00fc\u00dfe Philomele;", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Phi\u00b7lo\u00b7me\u00b7le", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie mir das Lied zur Seele dringt,", "tokens": ["Wie", "mir", "das", "Lied", "zur", "See\u00b7le", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So dehnt sich wieder die Seele.", "tokens": ["So", "dehnt", "sich", "wie\u00b7der", "die", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}