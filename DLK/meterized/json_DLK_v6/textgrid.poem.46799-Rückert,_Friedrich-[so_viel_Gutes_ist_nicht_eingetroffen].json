{"textgrid.poem.46799": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[so viel Gutes ist nicht eingetroffen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So viel Gutes ist nicht eingetroffen,", "tokens": ["So", "viel", "Gu\u00b7tes", "ist", "nicht", "ein\u00b7ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das ich hofft', und meint', ich d\u00fcrft' es hoffen.", "tokens": ["Das", "ich", "hofft'", ",", "und", "meint'", ",", "ich", "d\u00fcrft'", "es", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$,", "KON", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "M\u00f6cht' ich doch einmal nun heut auch sehen", "tokens": ["M\u00f6cht'", "ich", "doch", "ein\u00b7mal", "nun", "heut", "auch", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "B\u00f6ses, das ich f\u00fcrchte, nicht geschehen.", "tokens": ["B\u00f6\u00b7ses", ",", "das", "ich", "f\u00fcrch\u00b7te", ",", "nicht", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "VVFIN", "$,", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}