{"dta.poem.10150": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Qwitzen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie? sieht man hier auf hohen Stangen", "tokens": ["Wie", "?", "sieht", "man", "hier", "auf", "ho\u00b7hen", "Stan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "VVFIN", "PIS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rubinen in den L\u00fcfften hangen?", "tokens": ["Ru\u00b7bi\u00b7nen", "in", "den", "L\u00fcff\u00b7ten", "han\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So rieff ich, fast halb ausser mir,", "tokens": ["So", "rieff", "ich", ",", "fast", "halb", "aus\u00b7ser", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ich gantz unverhofft die rothe Zier", "tokens": ["Als", "ich", "gantz", "un\u00b7ver\u00b7hofft", "die", "ro\u00b7the", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der gleichsam brennenden Blut-rothen Qwitzen-Beeren,", "tokens": ["Der", "gleich\u00b7sam", "bren\u00b7nen\u00b7den", "Blut\u00b7ro\u00b7then", "Qwit\u00b7zen\u00b7Bee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "NN", "$,"], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Auf ihren Gipfeln, funckeln sah.", "tokens": ["Auf", "ih\u00b7ren", "Gip\u00b7feln", ",", "fun\u00b7ckeln", "sah", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich wuste kaum, wie mir geschah:", "tokens": ["Ich", "wus\u00b7te", "kaum", ",", "wie", "mir", "ge\u00b7schah", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Denn, ihre Glut noch zu vermehren,", "tokens": ["Denn", ",", "ih\u00b7re", "Glut", "noch", "zu", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Fiel eben dazumahl", "tokens": ["Fiel", "e\u00b7ben", "da\u00b7zu\u00b7mahl"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Der hellen Sonnen Mittags-Strahl", "tokens": ["Der", "hel\u00b7len", "Son\u00b7nen", "Mit\u00b7tags\u00b7Strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Auf ihr fast blendend Roth, das durch der Bl\u00e4tter Gr\u00fcn,", "tokens": ["Auf", "ihr", "fast", "blen\u00b7dend", "Roth", ",", "das", "durch", "der", "Bl\u00e4t\u00b7ter", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "NN", "$,", "PRELS", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zumahl, wann selbiges beschattet,", "tokens": ["Zu\u00b7mahl", ",", "wann", "sel\u00b7bi\u00b7ges", "be\u00b7schat\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Als durch vertiefften Grund, noch mehr erhaben schien.", "tokens": ["Als", "durch", "ver\u00b7tieff\u00b7ten", "Grund", ",", "noch", "mehr", "er\u00b7ha\u00b7ben", "schien", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "$,", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich danckte, durch der Augen Lust", "tokens": ["Ich", "danck\u00b7te", ",", "durch", "der", "Au\u00b7gen", "Lust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Sch\u00f6pfer aller Ding\u2019 empor gef\u00fchret,", "tokens": ["Zum", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Ding'", "em\u00b7por", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und durch das sch\u00f6ne Roth der sch\u00f6nen Frucht ger\u00fchret,", "tokens": ["Und", "durch", "das", "sch\u00f6\u00b7ne", "Roth", "der", "sch\u00f6\u00b7nen", "Frucht", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit einer angeflammten Brust,", "tokens": ["Mit", "ei\u00b7ner", "an\u00b7ge\u00b7flamm\u00b7ten", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem, Der nicht nur der Beeren holde Pracht,", "tokens": ["Dem", ",", "Der", "nicht", "nur", "der", "Bee\u00b7ren", "hol\u00b7de", "Pracht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ART", "PTKNEG", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und alle Ding\u2019 aus nichts gemacht;", "tokens": ["Und", "al\u00b7le", "Ding'", "aus", "nichts", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der auch zugleich durch mein geschenckt Gesicht,", "tokens": ["Der", "auch", "zu\u00b7gleich", "durch", "mein", "ge\u00b7schenckt", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "PPOSAT", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und durch das wunderbare Licht,", "tokens": ["Und", "durch", "das", "wun\u00b7der\u00b7ba\u00b7re", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mich daf\u00fcr sinnlich macht; ja gar mir Seelen-Augen,", "tokens": ["Mich", "da\u00b7f\u00fcr", "sinn\u00b7lich", "macht", ";", "ja", "gar", "mir", "See\u00b7len\u00b7Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "ADJD", "VVFIN", "$.", "ADV", "ADV", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die, da\u00df er alles schafft, zu sehen taugen,", "tokens": ["Die", ",", "da\u00df", "er", "al\u00b7les", "schafft", ",", "zu", "se\u00b7hen", "tau\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$,", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Aus Gnad\u2019 und Huld verliehn.", "tokens": ["Aus", "Gnad'", "und", "Huld", "ver\u00b7liehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Ich brach", "tokens": ["Ich", "brach"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Demnach", "tokens": ["Dem\u00b7nach"], "token_info": ["word"], "pos": ["PAV"], "meter": "++", "measure": "spondeus"}, "line.14": {"text": "Um mich noch ferner zu bem\u00fchn,", "tokens": ["Um", "mich", "noch", "fer\u00b7ner", "zu", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der Qwitzen Sch\u00f6nheit zu betrachten,", "tokens": ["Der", "Qwit\u00b7zen", "Sch\u00f6n\u00b7heit", "zu", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und in derselben Pracht auf Sein Gesch\u00f6pf zu achten,", "tokens": ["Und", "in", "der\u00b7sel\u00b7ben", "Pracht", "auf", "Sein", "Ge\u00b7sch\u00f6pf", "zu", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein Zweiglein voller Frucht und Bl\u00e4tter ab;", "tokens": ["Ein", "Zwei\u00b7glein", "vol\u00b7ler", "Frucht", "und", "Bl\u00e4t\u00b7ter", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Das mir, zu folgender Betrachtung, Anla\u00df gab:", "tokens": ["Das", "mir", ",", "zu", "fol\u00b7gen\u00b7der", "Be\u00b7trach\u00b7tung", ",", "An\u00b7la\u00df", "gab", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie reich erzeiget sich des Sch\u00f6pfers Herrlichkeit", "tokens": ["Wie", "reich", "er\u00b7zei\u00b7get", "sich", "des", "Sch\u00f6p\u00b7fers", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein in der Gesch\u00f6pfe Unterscheid!", "tokens": ["Al\u00b7lein", "in", "der", "Ge\u00b7sch\u00f6p\u00b7fe", "Un\u00b7ter\u00b7scheid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie seh ich hier in rother Glut", "tokens": ["Wie", "seh", "ich", "hier", "in", "ro\u00b7ther", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Solch eine Menge Beeren blitzen", "tokens": ["Solch", "ei\u00b7ne", "Men\u00b7ge", "Bee\u00b7ren", "blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und all\u2019 an einem Stengel sitzen,", "tokens": ["Und", "all'", "an", "ei\u00b7nem", "Sten\u00b7gel", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In andrer Ordnung noch, als wie das Trauben-Blut!", "tokens": ["In", "an\u00b7drer", "Ord\u00b7nung", "noch", ",", "als", "wie", "das", "Trau\u00b7ben\u00b7Blut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,", "KOUS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Beeren an sich selbst sind aus dermassen zierlich", "tokens": ["Die", "Bee\u00b7ren", "an", "sich", "selbst", "sind", "aus", "der\u00b7mas\u00b7sen", "zier\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PRF", "ADV", "VAFIN", "APPR", "PDS", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ger\u00fcndet und geformt. Die Bildung ist nat\u00fcrlich", "tokens": ["Ge\u00b7r\u00fcn\u00b7det", "und", "ge\u00b7formt", ".", "Die", "Bil\u00b7dung", "ist", "na\u00b7t\u00fcr\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVPP", "$.", "ART", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den nettsten Aepfeln gleich, die roth durchaus gef\u00e4rbt.", "tokens": ["Den", "netts\u00b7ten", "A\u00b7e\u00b7pfeln", "gleich", ",", "die", "roth", "durc\u00b7haus", "ge\u00b7f\u00e4rbt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "PRELS", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Besiehet man sie recht, wird auf der glatten Haut", "tokens": ["Be\u00b7sie\u00b7het", "man", "sie", "recht", ",", "wird", "auf", "der", "glat\u00b7ten", "Haut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPER", "ADJD", "$,", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein kleines Sonnen-Bild geschaut,", "tokens": ["Ein", "klei\u00b7nes", "Son\u00b7nen\u00b7Bild", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das lieblich wiederstrahlt, zumahl bey heiterm Wetter.", "tokens": ["Das", "lieb\u00b7lich", "wie\u00b7der\u00b7strahlt", ",", "zu\u00b7mahl", "bey", "hei\u00b7term", "Wet\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "$,", "KOUS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie l\u00e4nglich rund, und zierlich eingekerbt", "tokens": ["Wie", "l\u00e4ng\u00b7lich", "rund", ",", "und", "zier\u00b7lich", "ein\u00b7ge\u00b7kerbt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADJD", "$,", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sind dieses Baumes nette Bl\u00e4tter!", "tokens": ["Sind", "die\u00b7ses", "Bau\u00b7mes", "net\u00b7te", "Bl\u00e4t\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man kann derselben lieblich Gr\u00fcn,", "tokens": ["Man", "kann", "der\u00b7sel\u00b7ben", "lieb\u00b7lich", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PDAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bey ihrer Frucht ergetzenden Rubin,", "tokens": ["Bey", "ih\u00b7rer", "Frucht", "er\u00b7get\u00b7zen\u00b7den", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nicht sonder Anmuth sehn.", "tokens": ["Nicht", "son\u00b7der", "An\u00b7muth", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ob wir nun gleich bey uns der Qwitzen Frucht nicht", "tokens": ["Ob", "wir", "nun", "gleich", "bey", "uns", "der", "Qwit\u00b7zen", "Frucht", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "PPER", "ART", "NN", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So mu\u00df man doch des Nutzens nicht vergessen,", "tokens": ["So", "mu\u00df", "man", "doch", "des", "Nut\u00b7zens", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da sie uns wunderbar,", "tokens": ["Da", "sie", "uns", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn, mit fast ungezehlter Schaar,", "tokens": ["Wenn", ",", "mit", "fast", "un\u00b7ge\u00b7zehl\u00b7ter", "Schaar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Krammets-V\u00f6gel Heer durch unsre L\u00e4uder streicht,", "tokens": ["Der", "Kram\u00b7mets\u00b7V\u00f6\u00b7gel", "Heer", "durch", "uns\u00b7re", "L\u00e4u\u00b7der", "streicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dieselben uns zu Lecker-Bissen reicht:", "tokens": ["Die\u00b7sel\u00b7ben", "uns", "zu", "Le\u00b7cke\u00b7rBis\u00b7sen", "reicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die, wenn sie nicht die Qwitzen-Beeren", "tokens": ["Die", ",", "wenn", "sie", "nicht", "die", "Qwit\u00b7zen\u00b7Bee\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "PPER", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So gierig nascheten, gar nicht zu fangen w\u00e4ren.", "tokens": ["So", "gie\u00b7rig", "na\u00b7sche\u00b7ten", ",", "gar", "nicht", "zu", "fan\u00b7gen", "w\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "wor