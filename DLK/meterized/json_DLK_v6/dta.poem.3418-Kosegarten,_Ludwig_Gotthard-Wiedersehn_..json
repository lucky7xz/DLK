{"dta.poem.3418": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Wiedersehn .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Tag der Sehnsucht, Tag der Freuden,", "tokens": ["Tag", "der", "Sehn\u00b7sucht", ",", "Tag", "der", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tag des Zieles langer Leiden,", "tokens": ["Tag", "des", "Zie\u00b7les", "lan\u00b7ger", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sey gegr\u00fcsst, gew\u00fcnschter Tag!", "tokens": ["Sey", "ge\u00b7gr\u00fcs\u00b7st", ",", "ge\u00b7w\u00fcnschter", "Tag", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Aufgang deiner Sonne", "tokens": ["Mit", "dem", "Auf\u00b7gang", "dei\u00b7ner", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird die l\u00e4ngsterstorbne Wonne", "tokens": ["Wird", "die", "l\u00e4ngs\u00b7ter\u00b7storb\u00b7ne", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meines Herzens wieder wach.", "tokens": ["Mei\u00b7nes", "Her\u00b7zens", "wie\u00b7der", "wach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie, die Holde, sie, die Eine,", "tokens": ["Sie", ",", "die", "Hol\u00b7de", ",", "sie", ",", "die", "Ei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "PPER", "$,", "PRELS", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine S\u00fcsse, meine Reine,", "tokens": ["Mei\u00b7ne", "S\u00fcs\u00b7se", ",", "mei\u00b7ne", "Rei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mein Herz uns\u00e4glich liebt;", "tokens": ["Die", "mein", "Herz", "un\u00b7s\u00e4g\u00b7lich", "liebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie ist's, sie, die Langentbehrte,", "tokens": ["Sie", "ist's", ",", "sie", ",", "die", "Lan\u00b7gent\u00b7behr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Vielgew\u00fcnschte, Heissbegehrte,", "tokens": ["Viel\u00b7ge\u00b7w\u00fcnschte", ",", "Heiss\u00b7be\u00b7gehr\u00b7te", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Die dein Strahl mir wiedergiebt.", "tokens": ["Die", "dein", "Strahl", "mir", "wie\u00b7der\u00b7giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hundert Tage sind entwichen,", "tokens": ["Hun\u00b7dert", "Ta\u00b7ge", "sind", "ent\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hundert N\u00e4chte hingeschlichen,", "tokens": ["Hun\u00b7dert", "N\u00e4ch\u00b7te", "hin\u00b7ge\u00b7schli\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seit ich, Edle, dich nicht sah;", "tokens": ["Seit", "ich", ",", "Ed\u00b7le", ",", "dich", "nicht", "sah", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJA", "$,", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich, endlich sind sie schwunden,", "tokens": ["End\u00b7lich", ",", "end\u00b7lich", "sind", "sie", "schwun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der wonnereichsten Stunden", "tokens": ["Und", "der", "won\u00b7ne\u00b7reichs\u00b7ten", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wonnigste ist endlich da.", "tokens": ["Won\u00b7nigs\u00b7te", "ist", "end\u00b7lich", "da", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Endlich, endlich, o du Meine,", "tokens": ["End\u00b7lich", ",", "end\u00b7lich", ",", "o", "du", "Mei\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "FM", "PPER", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine S\u00fcsse, meine Reine,", "tokens": ["Mei\u00b7ne", "S\u00fcs\u00b7se", ",", "mei\u00b7ne", "Rei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Werd' ich, ach! dich wiedersehn.", "tokens": ["Werd'", "ich", ",", "ach", "!", "dich", "wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ITJ", "$.", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebend wird dein Wink mich gr\u00fcssen,", "tokens": ["Lie\u00b7bend", "wird", "dein", "Wink", "mich", "gr\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Z\u00e4rtlich mich dein Arm umschliessen,", "tokens": ["Z\u00e4rt\u00b7lich", "mich", "dein", "Arm", "um\u00b7schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00fcrzig mich dein Hauch umwehn.", "tokens": ["W\u00fcr\u00b7zig", "mich", "dein", "Hauch", "um\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie das schmachtende Verlangen,", "tokens": ["Wie", "das", "schmach\u00b7ten\u00b7de", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Edle Jungfrau, deine Wangen", "tokens": ["Ed\u00b7le", "Jung\u00b7frau", ",", "dei\u00b7ne", "Wan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit des Fr\u00fchroths Tinten mahlt!", "tokens": ["Mit", "des", "Fr\u00fch\u00b7roths", "Tin\u00b7ten", "mahlt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Augen reine Bl\u00e4ue,", "tokens": ["Wie", "der", "Au\u00b7gen", "rei\u00b7ne", "Bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Spiegel deiner G\u00fct' und Treue,", "tokens": ["Spie\u00b7gel", "dei\u00b7ner", "G\u00fct'", "und", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch den Thau der R\u00fchrung strahlt!", "tokens": ["Durch", "den", "Thau", "der", "R\u00fch\u00b7rung", "strahlt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Reiche mir den Wonnebecher", "tokens": ["Rei\u00b7che", "mir", "den", "Won\u00b7ne\u00b7be\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Lippen, lass den Lecher", "tokens": ["Dei\u00b7ner", "Lip\u00b7pen", ",", "lass", "den", "Le\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schl\u00fcrfen s\u00fcssen Labetrank!", "tokens": ["Schl\u00fcr\u00b7fen", "s\u00fcs\u00b7sen", "La\u00b7be\u00b7trank", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "An des Busens reiner F\u00fclle", "tokens": ["An", "des", "Bu\u00b7sens", "rei\u00b7ner", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lass den Schw\u00e4rmer sanft und stille", "tokens": ["Lass", "den", "Schw\u00e4r\u00b7mer", "sanft", "und", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "ADJD", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schlummern nach dem langen Drang!", "tokens": ["Schlum\u00b7mern", "nach", "dem", "lan\u00b7gen", "Drang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn der wache Tag sich neiget,", "tokens": ["Wenn", "der", "wa\u00b7che", "Tag", "sich", "nei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Sch\u00f6pfung Jubel schweiget,", "tokens": ["Wenn", "der", "Sch\u00f6p\u00b7fung", "Ju\u00b7bel", "schwei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wink' ihn traulich zu dir hin;", "tokens": ["Wink'", "ihn", "trau\u00b7lich", "zu", "dir", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lass ihn tr\u00e4umen Eine Stunde,", "tokens": ["Lass", "ihn", "tr\u00e4u\u00b7men", "Ei\u00b7ne", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihre Dauer ist Sekunde,", "tokens": ["Ih\u00b7re", "Dau\u00b7er", "ist", "Se\u00b7kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In dem Arm der Lieblingin.", "tokens": ["In", "dem", "Arm", "der", "Lieb\u00b7lin\u00b7gin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Im Panier der Mittern\u00e4chte", "tokens": ["Im", "Pa\u00b7nier", "der", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00d6ffne leise deine Rechte", "tokens": ["\u00d6ff\u00b7ne", "lei\u00b7se", "dei\u00b7ne", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihm der Liebe K\u00e4mmerlein.", "tokens": ["Ihm", "der", "Lie\u00b7be", "K\u00e4m\u00b7mer\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Welch' ein Herzen! welch' ein Drucken!", "tokens": ["Welch'", "ein", "Her\u00b7zen", "!", "welch'", "ein", "Dru\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$.", "PWAT", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Welch' ein Zittern! welch' ein Zucken!", "tokens": ["Welch'", "ein", "Zit\u00b7tern", "!", "welch'", "ein", "Zu\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$.", "PWAT", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schone, Beste, schone mein! \u2014", "tokens": ["Scho\u00b7ne", ",", "Bes\u00b7te", ",", "scho\u00b7ne", "mein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PPOSAT", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "O der Wonne, dem Getreuen", "tokens": ["O", "der", "Won\u00b7ne", ",", "dem", "Ge\u00b7treu\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ganz sich opfern, ganz sich weihen", "tokens": ["Ganz", "sich", "op\u00b7fern", ",", "ganz", "sich", "wei\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PRF", "VVINF", "$,", "ADV", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonder Str\u00e4uben, Furcht und Flucht!", "tokens": ["Son\u00b7der", "Str\u00e4u\u00b7ben", ",", "Furcht", "und", "Flucht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stark ist Liebe, k\u00fchn ihr Dr\u00e4ngen;", "tokens": ["Stark", "ist", "Lie\u00b7be", ",", "k\u00fchn", "ihr", "Dr\u00e4n\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "$,", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber ihre St\u00fcrme sprengen", "tokens": ["A\u00b7ber", "ih\u00b7re", "St\u00fcr\u00b7me", "spren\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nie die Riegel heil'ger Zucht!", "tokens": ["Nie", "die", "Rie\u00b7gel", "heil'\u00b7ger", "Zucht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ida, Ida, meine Eine,", "tokens": ["I\u00b7da", ",", "I\u00b7da", ",", "mei\u00b7ne", "Ei\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine S\u00fcsse, meine Reine,", "tokens": ["Mei\u00b7ne", "S\u00fcs\u00b7se", ",", "mei\u00b7ne", "Rei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00d6ffne, \u00f6ffne mir den Arm.", "tokens": ["\u00d6ff\u00b7ne", ",", "\u00f6ff\u00b7ne", "mir", "den", "Arm", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fest umsch\u00fcrzt von deiner Rechten", "tokens": ["Fest", "um\u00b7sch\u00fcrzt", "von", "dei\u00b7ner", "Rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lass mich fest dich, fest umflechten", "tokens": ["Lass", "mich", "fest", "dich", ",", "fest", "um\u00b7flech\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "PPER", "$,", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nach der Trennung langem Harm.", "tokens": ["Nach", "der", "Tren\u00b7nung", "lan\u00b7gem", "Harm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "\u00d6ffne, \u00f6ffne mir die Arme", "tokens": ["\u00d6ff\u00b7ne", ",", "\u00f6ff\u00b7ne", "mir", "die", "Ar\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00fccke fest mich an die warme", "tokens": ["Dr\u00fc\u00b7cke", "fest", "mich", "an", "die", "war\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Volle, treue, keusche Brust.", "tokens": ["Vol\u00b7le", ",", "treu\u00b7e", ",", "keu\u00b7sche", "Brust", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus den Wangen, aus den Augen", "tokens": ["Aus", "den", "Wan\u00b7gen", ",", "aus", "den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus den Lippen lass mich saugen,", "tokens": ["Aus", "den", "Lip\u00b7pen", "lass", "mich", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lebenskraft und Lebenslust.", "tokens": ["Le\u00b7bens\u00b7kraft", "und", "Le\u00b7bens\u00b7lust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "\u00d6ffne, \u00f6ffne mir die Sch\u00e4tze", "tokens": ["\u00d6ff\u00b7ne", ",", "\u00f6ff\u00b7ne", "mir", "die", "Sch\u00e4t\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deines Herzens \u2014 Seelenletze!", "tokens": ["Dei\u00b7nes", "Her\u00b7zens", "See\u00b7len\u00b7let\u00b7ze", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Geisterquickenden Genuss!", "tokens": ["Geis\u00b7ter\u00b7qui\u00b7cken\u00b7den", "Ge\u00b7nuss", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lass uns, S\u00fcsse, lass uns kosen,", "tokens": ["Lass", "uns", ",", "S\u00fcs\u00b7se", ",", "lass", "uns", "ko\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis erbl\u00fchn Aurorens Rosen,", "tokens": ["Bis", "er\u00b7bl\u00fchn", "Au\u00b7ro\u00b7rens", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und herauf strahlt Phosphorus.", "tokens": ["Und", "her\u00b7auf", "strahlt", "Phos\u00b7pho\u00b7rus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}