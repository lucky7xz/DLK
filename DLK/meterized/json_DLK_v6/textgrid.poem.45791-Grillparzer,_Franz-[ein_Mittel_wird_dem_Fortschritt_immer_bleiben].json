{"textgrid.poem.45791": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[ein Mittel wird dem Fortschritt immer bleiben]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Mittel wird dem Fortschritt immer bleiben,", "tokens": ["Ein", "Mit\u00b7tel", "wird", "dem", "Fort\u00b7schritt", "im\u00b7mer", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn nicht zu \u00fcbertreffen, zu \u00fcbertreiben,", "tokens": ["Wenn", "nicht", "zu", "\u00fc\u00b7bert\u00b7ref\u00b7fen", ",", "zu", "\u00fc\u00b7bert\u00b7rei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und bei der Einzelnen schm\u00e4hlicher Ermattung,", "tokens": ["Und", "bei", "der", "Ein\u00b7zel\u00b7nen", "schm\u00e4h\u00b7li\u00b7cher", "Er\u00b7mat\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Kultus der Nationen und der Gattung.", "tokens": ["Der", "Kul\u00b7tus", "der", "Na\u00b7ti\u00b7o\u00b7nen", "und", "der", "Gat\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}