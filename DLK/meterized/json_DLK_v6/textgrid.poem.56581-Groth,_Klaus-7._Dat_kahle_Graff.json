{"textgrid.poem.56581": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "7. Dat kahle Graff", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oppen Karkhof \u00fcnnern Lindenbom", "tokens": ["Op\u00b7pen", "Kark\u00b7hof", "\u00fcn\u00b7nern", "Lin\u00b7den\u00b7bom"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Dar slapt twee Leefsten un dr\u00f6mt en Drom.", "tokens": ["Dar", "slapt", "twee", "Leefs\u00b7ten", "un", "dr\u00f6mt", "en", "Drom", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Se slapt un dr\u00f6mt de lange lange Nacht,", "tokens": ["Se", "slapt", "un", "dr\u00f6mt", "de", "lan\u00b7ge", "lan\u00b7ge", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "ADV", "ADV", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Un lurt un lurt op den j\u00fcngsten Dag. \u2013", "tokens": ["Un", "lurt", "un", "lurt", "op", "den", "j\u00fcng\u00b7sten", "Dag", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "De Vader s\u00e4: min Dochter is rik,", "tokens": ["De", "Va\u00b7der", "s\u00e4", ":", "min", "Doch\u00b7ter", "is", "rik", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$.", "PPOSAT", "NN", "FM", "FM", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "En K\u0153thnerss\u0153n de is \u0119r ni lik.", "tokens": ["En", "K\u0153th\u00b7ner\u00b7ss\u0153n", "de", "is", "\u0119r", "ni", "lik", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "De Moder s\u00e4: min Dochter is fin,", "tokens": ["De", "Mo\u00b7der", "s\u00e4", ":", "min", "Doch\u00b7ter", "is", "fin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "PPOSAT", "NN", "FM", "FM", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "De Vullmacht sin S\u0153n de mag \u0119r li'n.", "tokens": ["De", "Vull\u00b7macht", "sin", "S\u0153n", "de", "mag", "\u0119r", "li'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Se s\u00e4: ade min Leev, min L\u0119bn,", "tokens": ["Se", "s\u00e4", ":", "a\u00b7de", "min", "Leev", ",", "min", "L\u0119bn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Min Vader hett mi en Annern g\u0119bn!", "tokens": ["Min", "Va\u00b7der", "hett", "mi", "en", "An\u00b7nern", "g\u0119bn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbdat lid ik nu un nimmermehr,", "tokens": ["\u00bb", "dat", "lid", "ik", "nu", "un", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "FM", "FM", "FM", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ehr liggt wi beid inne kole Eer!\u00ab", "tokens": ["Ehr", "liggt", "wi", "beid", "in\u00b7ne", "ko\u00b7le", "E\u00b7er", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Dar weer keen H\u00f6lp, dar weer keen Rath,", "tokens": ["Dar", "weer", "ke\u00b7en", "H\u00f6lp", ",", "dar", "weer", "ke\u00b7en", "Rath", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "VVFIN", "NN", "$,", "PAV", "ADJD", "VVFIN", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Do d\u0119n se beid de schrecklige Dad.", "tokens": ["Do", "d\u0119n", "se", "beid", "de", "schreck\u00b7li\u00b7ge", "Dad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Un weer keen Rath oder H\u00f6lp ni mehr,", "tokens": ["Un", "weer", "ke\u00b7en", "Rath", "o\u00b7der", "H\u00f6lp", "ni", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVFIN", "NN", "KON", "NN", "ADV", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "So lat se slapen inne kole Eer.", "tokens": ["So", "lat", "se", "sla\u00b7pen", "in\u00b7ne", "ko\u00b7le", "E\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "FM", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "So lat se t\u00f6ben ton j\u00fcngsten Dag,", "tokens": ["So", "lat", "se", "t\u00f6\u00b7ben", "ton", "j\u00fcng\u00b7sten", "Dag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ob Gott se tosam opwaken mag. \u2013", "tokens": ["Ob", "Gott", "se", "to\u00b7sam", "op\u00b7wa\u00b7ken", "mag.", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "abbreviation", "punct"], "pos": ["KOUS", "NN", "FM", "FM", "FM", "FM", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Dar weiht inne Nacht en Wind ut Nord,", "tokens": ["Dar", "weiht", "in\u00b7ne", "Nacht", "en", "Wind", "ut", "Nord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dar is keen rode Blom, se s\u00fcnd all versort.", "tokens": ["Dar", "is", "ke\u00b7en", "ro\u00b7de", "Blom", ",", "se", "s\u00fcnd", "all", "ver\u00b7sort", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}