{"textgrid.poem.57088": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Evas Haar", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als wie ein Feld, das erstes Licht ereilt,", "tokens": ["Als", "wie", "ein", "Feld", ",", "das", "ers\u00b7tes", "Licht", "er\u00b7eilt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "sind deines Hauptes wunderblonde Wellen:", "tokens": ["sind", "dei\u00b7nes", "Haup\u00b7tes", "wun\u00b7der\u00b7blon\u00b7de", "Wel\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein sanfter Morgen scheint ihr Gold zu schwellen,", "tokens": ["Ein", "sanf\u00b7ter", "Mor\u00b7gen", "scheint", "ihr", "Gold", "zu", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "darauf der Sonne Auge z\u00f6gernd weilt.", "tokens": ["da\u00b7rauf", "der", "Son\u00b7ne", "Au\u00b7ge", "z\u00f6\u00b7gernd", "weilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun flammt es auf, als k\u00e4m' es Purpur malen, \u2013", "tokens": ["Nun", "flammt", "es", "auf", ",", "als", "k\u00e4m'", "es", "Pur\u00b7pur", "ma\u00b7len", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "VVFIN", "PPER", "NN", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ist es der Mohn, der heimlich in ihm wohnt?", "tokens": ["ist", "es", "der", "Mohn", ",", "der", "heim\u00b7lich", "in", "ihm", "wohnt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Doch wann der Abend naht mit k\u00fchlern Strahlen,", "tokens": ["Doch", "wann", "der", "A\u00b7bend", "naht", "mit", "k\u00fch\u00b7lern", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "so ruht es wieder bla\u00df, wie keuscher Mond.", "tokens": ["so", "ruht", "es", "wie\u00b7der", "bla\u00df", ",", "wie", "keu\u00b7scher", "Mond", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}