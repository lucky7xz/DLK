{"dta.poem.923": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "MeNSCHLICHES ELEND", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Die Uhr, die vor der Sonne f\u00fcnfe schl\u00e4gt \u2013", "tokens": ["Die", "Uhr", ",", "die", "vor", "der", "Son\u00b7ne", "f\u00fcn\u00b7fe", "schl\u00e4gt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Einsame Menschen packt ein dunkles Grausen,", "tokens": ["Ein\u00b7sa\u00b7me", "Men\u00b7schen", "packt", "ein", "dunk\u00b7les", "Grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Im Abendgarten kahle B\u00e4ume sausen.", "tokens": ["Im", "A\u00b7bend\u00b7gar\u00b7ten", "kah\u00b7le", "B\u00e4u\u00b7me", "sau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Toten Antlitz sich am Fenster regt.", "tokens": ["Des", "To\u00b7ten", "Ant\u00b7litz", "sich", "am", "Fens\u00b7ter", "reg\u00b7t."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "NN", "PRF", "APPRART", "NN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vielleicht, da\u00df diese Stunde stille steht.", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "die\u00b7se", "Stun\u00b7de", "stil\u00b7le", "steht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PDAT", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor tr\u00fcben Augen blaue Bilder gaukeln", "tokens": ["Vor", "tr\u00fc\u00b7ben", "Au\u00b7gen", "blau\u00b7e", "Bil\u00b7der", "gau\u00b7keln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Takt der Schiffe, die am Flusse schaukeln.", "tokens": ["Im", "Takt", "der", "Schif\u00b7fe", ",", "die", "am", "Flus\u00b7se", "schau\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Kai ein Schwesternzug vor\u00fcberweht.", "tokens": ["Am", "Kai", "ein", "Schwes\u00b7tern\u00b7zug", "vor\u00b7\u00fc\u00b7ber\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Im Hasel spielen M\u00e4dchen bla\u00df und blind,", "tokens": ["Im", "Ha\u00b7sel", "spie\u00b7len", "M\u00e4d\u00b7chen", "bla\u00df", "und", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie Liebende, die sich im Schlaf umschlingen.", "tokens": ["Wie", "Lie\u00b7ben\u00b7de", ",", "die", "sich", "im", "Schlaf", "um\u00b7schlin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Vielleicht, da\u00df um ein Aas dort Fliegen singen,", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "um", "ein", "Aas", "dort", "Flie\u00b7gen", "sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "APPR", "ART", "NN", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vielleicht auch weint im Mutterscho\u00df ein Kind.", "tokens": ["Viel\u00b7leicht", "auch", "weint", "im", "Mut\u00b7ter\u00b7scho\u00df", "ein", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Aus H\u00e4nden sinken Astern blau und rot,", "tokens": ["Aus", "H\u00e4n\u00b7den", "sin\u00b7ken", "As\u00b7tern", "blau", "und", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des J\u00fcnglings Mund entgleitet fremd und weise;", "tokens": ["Des", "J\u00fcng\u00b7lings", "Mund", "ent\u00b7glei\u00b7tet", "fremd", "und", "wei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Lider flattern angstverwirrt und leise;", "tokens": ["Und", "Li\u00b7der", "flat\u00b7tern", "angst\u00b7ver\u00b7wirrt", "und", "lei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch Fieberschw\u00e4rze weht ein Duft von Brot.", "tokens": ["Durch", "Fie\u00b7ber\u00b7schw\u00e4r\u00b7ze", "weht", "ein", "Duft", "von", "Brot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es scheint, man h\u00f6rt auch gr\u00e4\u00dfliches Geschrei;", "tokens": ["Es", "scheint", ",", "man", "h\u00f6rt", "auch", "gr\u00e4\u00df\u00b7li\u00b7ches", "Ge\u00b7schrei", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PIS", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gebeine durch verfallne Mauern schimmern.", "tokens": ["Ge\u00b7bei\u00b7ne", "durch", "ver\u00b7fall\u00b7ne", "Mau\u00b7ern", "schim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein b\u00f6ses Herz lacht laut in sch\u00f6nen Zimmern;", "tokens": ["Ein", "b\u00f6\u00b7ses", "Herz", "lacht", "laut", "in", "sch\u00f6\u00b7nen", "Zim\u00b7mern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An einem Tr\u00e4umer l\u00e4uft ein Hund vorbei.", "tokens": ["An", "ei\u00b7nem", "Tr\u00e4u\u00b7mer", "l\u00e4uft", "ein", "Hund", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ein leerer Sarg im Dunkel sich verliert.", "tokens": ["Ein", "lee\u00b7rer", "Sarg", "im", "Dun\u00b7kel", "sich", "ver\u00b7liert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem M\u00f6rder will ein Raum sich bleich erhellen,", "tokens": ["Dem", "M\u00f6r\u00b7der", "will", "ein", "Raum", "sich", "bleich", "er\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Indes Laternen nachts im Sturm zerschellen.", "tokens": ["In\u00b7des", "La\u00b7ter\u00b7nen", "nachts", "im", "Sturm", "zer\u00b7schel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Edlen wei\u00dfe Schl\u00e4fe Lorbeer ziert.", "tokens": ["Des", "Ed\u00b7len", "wei\u00b7\u00dfe", "Schl\u00e4\u00b7fe", "Lor\u00b7beer", "ziert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}