{"dta.poem.10079": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nothwendigkeit der Betrachtung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann dich selbst in das Paradie\u00df", "tokens": ["Wann", "dich", "selbst", "in", "das", "Pa\u00b7ra\u00b7die\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sch\u00f6pfer w\u00fcrdigte zu setzen,", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "w\u00fcr\u00b7dig\u00b7te", "zu", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Eden selbst bewohnen lie\u00df;", "tokens": ["Und", "E\u00b7den", "selbst", "be\u00b7woh\u00b7nen", "lie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So w\u00fcrdest du, wenn du mit Edens Sch\u00e4tzen", "tokens": ["So", "w\u00fcr\u00b7dest", "du", ",", "wenn", "du", "mit", "E\u00b7dens", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Richt anders handeltest, als mit dem Schmuck der Erden,", "tokens": ["Richt", "an\u00b7ders", "han\u00b7del\u00b7test", ",", "als", "mit", "dem", "Schmuck", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Dadurch nicht gl\u00fccklicher, und nicht vergn\u00fcgter werden:", "tokens": ["Da\u00b7durch", "nicht", "gl\u00fcck\u00b7li\u00b7cher", ",", "und", "nicht", "ver\u00b7gn\u00fcg\u00b7ter", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "ADJD", "$,", "KON", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Indem Lust, Anmuth und Vergn\u00fcgen", "tokens": ["In\u00b7dem", "Lust", ",", "An\u00b7muth", "und", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Allein in der Betrachtung liegen.", "tokens": ["Al\u00b7lein", "in", "der", "Be\u00b7trach\u00b7tung", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}