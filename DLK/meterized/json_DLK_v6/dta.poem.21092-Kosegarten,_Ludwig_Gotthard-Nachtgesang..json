{"dta.poem.21092": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Nachtgesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Tiefe Feyer               ", "tokens": ["Tie\u00b7fe", "Fe\u00b7yer"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Schauert um die Welt.", "tokens": ["Schau\u00b7ert", "um", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Braune Schleyer", "tokens": ["Brau\u00b7ne", "Schle\u00b7yer"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "H\u00fcllen Wald und Feld.", "tokens": ["H\u00fcl\u00b7len", "Wald", "und", "Feld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Tr\u00fcb und matt und m\u00fcde", "tokens": ["Tr\u00fcb", "und", "matt", "und", "m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "KON", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Nickt jedes Leben ein,", "tokens": ["Nickt", "je\u00b7des", "Le\u00b7ben", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und namenloser Friede", "tokens": ["Und", "na\u00b7men\u00b7lo\u00b7ser", "Frie\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ums\u00e4uselt alles Seyn!", "tokens": ["Um\u00b7s\u00e4u\u00b7selt", "al\u00b7les", "Seyn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wacher Kummer,", "tokens": ["Wa\u00b7cher", "Kum\u00b7mer", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Verlass ein Weilchen mich!", "tokens": ["Ver\u00b7lass", "ein", "Weil\u00b7chen", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Goldner Schlummer,", "tokens": ["Gold\u00b7ner", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Komm und umfl\u00fcgle mich!", "tokens": ["Komm", "und", "um\u00b7fl\u00fcg\u00b7le", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Trockne meine Thr\u00e4nen", "tokens": ["Trock\u00b7ne", "mei\u00b7ne", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Mit deines Schleyers Saum,", "tokens": ["Mit", "dei\u00b7nes", "Schley\u00b7ers", "Saum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und t\u00e4usche, Freund, mein Sehnen", "tokens": ["Und", "t\u00e4u\u00b7sche", ",", "Freund", ",", "mein", "Seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit deinem sch\u00f6nsten Traum.", "tokens": ["Mit", "dei\u00b7nem", "sch\u00f6ns\u00b7ten", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Blaue Ferne,", "tokens": ["Blau\u00b7e", "Fer\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Hoch \u00fcber mich erh\u00f6ht!", "tokens": ["Hoch", "\u00fc\u00b7ber", "mich", "er\u00b7h\u00f6ht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heilge Sterne", "tokens": ["Heil\u00b7ge", "Ster\u00b7ne"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "In hehrer Majest\u00e4t!", "tokens": ["In", "heh\u00b7rer", "Ma\u00b7jes\u00b7t\u00e4t", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sagt mir, ist es stiller,", "tokens": ["Sagt", "mir", ",", "ist", "es", "stil\u00b7ler", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ihr Funkelnden, bey euch,", "tokens": ["Ihr", "Fun\u00b7keln\u00b7den", ",", "bey", "euch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als in der Eitelkeiten", "tokens": ["Als", "in", "der", "Ei\u00b7tel\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aufruhrvollem Reich?", "tokens": ["Auf\u00b7ruhr\u00b7vol\u00b7lem", "Reich", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}