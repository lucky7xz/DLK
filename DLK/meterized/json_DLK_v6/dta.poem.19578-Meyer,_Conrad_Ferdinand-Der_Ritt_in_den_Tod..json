{"dta.poem.19578": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Ritt in den Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "\u201egreif' aus, du mein junges, mein feuriges Thier!", "tokens": ["\u201e", "greif'", "aus", ",", "du", "mein", "jun\u00b7ges", ",", "mein", "feu\u00b7ri\u00b7ges", "Thier", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PTKVZ", "$,", "PPER", "PPOSAT", "ADJA", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Noch einmal verwachs' ich centaurisch mit dir!", "tokens": ["Noch", "ein\u00b7mal", "ver\u00b7wachs'", "ich", "cen\u00b7tau\u00b7risch", "mit", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Umschmettert mich, Tuben! Erhebet den Ton!", "tokens": ["Um\u00b7schmet\u00b7tert", "mich", ",", "Tu\u00b7ben", "!", "Er\u00b7he\u00b7bet", "den", "Ton", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Latiner besiegte des Manlius Sohn!", "tokens": ["Den", "La\u00b7ti\u00b7ner", "be\u00b7sieg\u00b7te", "des", "Man\u00b7lius", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NE", "NN", "$."], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Voran die Troph\u00e4'n! Der latinische Speer!", "tokens": ["Vo\u00b7ran", "die", "Tro\u00b7ph\u00e4'n", "!", "Der", "la\u00b7ti\u00b7ni\u00b7sche", "Speer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der eroberte Helm! Die erbeutete Wehr!", "tokens": ["Der", "er\u00b7o\u00b7ber\u00b7te", "Helm", "!", "Die", "er\u00b7beu\u00b7te\u00b7te", "Wehr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.4": {"line.1": {"text": "Duell ist bei Strafe des Beiles verp\u00f6nt ...", "tokens": ["Du\u00b7ell", "ist", "bei", "Stra\u00b7fe", "des", "Bei\u00b7les", "ver\u00b7p\u00f6nt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPR", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Doch er liegt der die r\u00f6mische W\u00f6lfin geh\u00f6hnt!", "tokens": ["Doch", "er", "liegt", "der", "die", "r\u00f6\u00b7mi\u00b7sche", "W\u00f6l\u00b7fin", "ge\u00b7h\u00f6hnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.5": {"line.1": {"text": "Lictoren, erf\u00fcllet des Vaters Gebot!", "tokens": ["Lic\u00b7to\u00b7ren", ",", "er\u00b7f\u00fcl\u00b7let", "des", "Va\u00b7ters", "Ge\u00b7bot", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich besitze den Kranz und verdiene den Tod \u2014", "tokens": ["Ich", "be\u00b7sit\u00b7ze", "den", "Kranz", "und", "ver\u00b7die\u00b7ne", "den", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$("], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.6": {"line.1": {"text": "Bevor es sich rollend im Sande bestaubt,", "tokens": ["Be\u00b7vor", "es", "sich", "rol\u00b7lend", "im", "San\u00b7de", "be\u00b7staubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Erheb' ich in ewigem Jubel das Haupt!\u201c", "tokens": ["Er\u00b7heb'", "ich", "in", "e\u00b7wi\u00b7gem", "Ju\u00b7bel", "das", "Haupt", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}