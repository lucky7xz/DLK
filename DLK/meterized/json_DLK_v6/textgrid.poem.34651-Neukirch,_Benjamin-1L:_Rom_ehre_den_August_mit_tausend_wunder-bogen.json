{"textgrid.poem.34651": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Rom ehre den August mit tausend wunder-bogen/", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rom ehre den August mit tausend wunder-bogen/", "tokens": ["Rom", "eh\u00b7re", "den", "Au\u00b7gust", "mit", "tau\u00b7send", "wun\u00b7der\u00b7bo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "CARD", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Pari\u00df erhebe sich durch seinen Ludewig;", "tokens": ["Pa\u00b7ri\u00df", "er\u00b7he\u00b7be", "sich", "durch", "sei\u00b7nen", "Lu\u00b7de\u00b7wig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "PPOSAT", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir streiten/ grosser Stosch/ mit beyden um den sieg/", "tokens": ["Wir", "strei\u00b7ten", "/", "gros\u00b7ser", "Stosch", "/", "mit", "bey\u00b7den", "um", "den", "sieg", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADJA", "NN", "$(", "APPR", "PIAT", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nachdem du wiederum in Tschirnau eingezogen.", "tokens": ["Nach\u00b7dem", "du", "wie\u00b7de\u00b7rum", "in", "Tschir\u00b7nau", "ein\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn ihre pracht bestund in marmel und porphir/", "tokens": ["Denn", "ih\u00b7re", "pracht", "be\u00b7stund", "in", "mar\u00b7mel", "und", "por\u00b7phir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "KON", "NE", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Hier aber opffern sich die hertzen selber dir.", "tokens": ["Hier", "a\u00b7ber", "opf\u00b7fern", "sich", "die", "hert\u00b7zen", "sel\u00b7ber", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ART", "NN", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach strahle/ wie du pflegst/ auch mit geneigten augen", "tokens": ["Ach", "strah\u00b7le", "/", "wie", "du", "pflegst", "/", "auch", "mit", "ge\u00b7neig\u00b7ten", "au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "$(", "PWAV", "PPER", "VVFIN", "$(", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Di\u00df liebes-volle pfand vor deinen f\u00fcssen an!", "tokens": ["Di\u00df", "lie\u00b7bes\u00b7vol\u00b7le", "pfand", "vor", "dei\u00b7nen", "f\u00fcs\u00b7sen", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn endlich bricht der stein wie m\u00fcrber porcellan/", "tokens": ["Denn", "end\u00b7lich", "bricht", "der", "stein", "wie", "m\u00fcr\u00b7ber", "por\u00b7cel\u00b7lan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "KOKOM", "APPR", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Aus b\u00fcchern kan die zeit die dinte wieder saugen/", "tokens": ["Aus", "b\u00fc\u00b7chern", "kan", "die", "zeit", "die", "din\u00b7te", "wie\u00b7der", "sau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VMFIN", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und wo man vor den ruhm Pompejus lie\u00df erh\u00f6h'n/", "tokens": ["Und", "wo", "man", "vor", "den", "ruhm", "Pom\u00b7pe\u00b7jus", "lie\u00df", "er\u00b7h\u00f6h'n", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "APPR", "ART", "NN", "NE", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da sieht man heute nichts als tr\u00fcbes wasser stehn;", "tokens": ["Da", "sieht", "man", "heu\u00b7te", "nichts", "als", "tr\u00fc\u00b7bes", "was\u00b7ser", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PIS", "KOKOM", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was aber pflicht und treu an ihre br\u00fcste dr\u00fccken/", "tokens": ["Was", "a\u00b7ber", "pflicht", "und", "treu", "an", "ih\u00b7re", "br\u00fcs\u00b7te", "dr\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "KON", "ADJD", "APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und selbst die redlichkeit in harte taffeln pr\u00e4gt/", "tokens": ["Und", "selbst", "die", "red\u00b7lich\u00b7keit", "in", "har\u00b7te", "taf\u00b7feln", "pr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Steht feste/ wenn der sturm Colossen niederlegt/", "tokens": ["Steht", "fes\u00b7te", "/", "wenn", "der", "sturm", "Co\u00b7los\u00b7sen", "nie\u00b7der\u00b7legt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "$(", "KOUS", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und Pyramiden sich wie kleine pappeln b\u00fccken;", "tokens": ["Und", "Py\u00b7ra\u00b7mi\u00b7den", "sich", "wie", "klei\u00b7ne", "pap\u00b7peln", "b\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PRF", "KOKOM", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Kein wasser/ keine zeit rei\u00dft ihre schrifften ein/", "tokens": ["Kein", "was\u00b7ser", "/", "kei\u00b7ne", "zeit", "rei\u00dft", "ih\u00b7re", "schriff\u00b7ten", "ein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "PIAT", "NN", "VVFIN", "PPOSAT", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Weil ihre farben blut/ die dinte liebe seyn.", "tokens": ["Weil", "ih\u00b7re", "far\u00b7ben", "blut", "/", "die", "din\u00b7te", "lie\u00b7be", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$(", "ART", "NN", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Schau/ Herr/ hier \u00f6ffnen sich die pforten unsrer seelen/", "tokens": ["Schau", "/", "Herr", "/", "hier", "\u00f6ff\u00b7nen", "sich", "die", "pfor\u00b7ten", "uns\u00b7rer", "see\u00b7len", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "ADV", "VVFIN", "PRF", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Hier liegt der b\u00fcrger hertz/ auff dem dein nahme steht/", "tokens": ["Hier", "liegt", "der", "b\u00fcr\u00b7ger", "hertz", "/", "auff", "dem", "dein", "nah\u00b7me", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Li\u00df selber/ wie es itzt vor freuden schwanger geht/", "tokens": ["Li\u00df", "sel\u00b7ber", "/", "wie", "es", "itzt", "vor", "freu\u00b7den", "schwan\u00b7ger", "geht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$(", "PWAV", "PPER", "ADV", "APPR", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Weil sich dein sonnen-licht nicht l\u00e4nger will verh\u00f6len.", "tokens": ["Weil", "sich", "dein", "son\u00b7nen\u00b7licht", "nicht", "l\u00e4n\u00b7ger", "will", "ver\u00b7h\u00f6\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "PTKNEG", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Li\u00df/ wie die kinder sich so wie die eltern freun/", "tokens": ["Li\u00df", "/", "wie", "die", "kin\u00b7der", "sich", "so", "wie", "die", "el\u00b7tern", "freun", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOKOM", "ART", "NN", "PRF", "ADV", "KOKOM", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Weil deine gegenwart nun soll best\u00e4ndig seyn.", "tokens": ["Weil", "dei\u00b7ne", "ge\u00b7gen\u00b7wart", "nun", "soll", "be\u00b7st\u00e4n\u00b7dig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Li\u00df endlich auch zuletzt/ was unsre seuffzer bitten/", "tokens": ["Li\u00df", "end\u00b7lich", "auch", "zu\u00b7letzt", "/", "was", "uns\u00b7re", "seuff\u00b7zer", "bit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "$(", "PWS", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da\u00df nehmlich alles gl\u00fcck dir zu der seiten stehn/", "tokens": ["Da\u00df", "nehm\u00b7lich", "al\u00b7les", "gl\u00fcck", "dir", "zu", "der", "sei\u00b7ten", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der himmel deinen stamm/ wie cedern/ m\u00f6g erh\u00f6hn/", "tokens": ["Der", "him\u00b7mel", "dei\u00b7nen", "stamm", "/", "wie", "ce\u00b7dern", "/", "m\u00f6g", "er\u00b7h\u00f6hn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "PWAV", "ADJD", "$(", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und also deine stadt mit honig \u00fcbersch\u00fctten;", "tokens": ["Und", "al\u00b7so", "dei\u00b7ne", "stadt", "mit", "ho\u00b7nig", "\u00fc\u00b7ber\u00b7sch\u00fct\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Denn was ein ieder ihm hier selber w\u00fcnschen soll/", "tokens": ["Denn", "was", "ein", "ie\u00b7der", "ihm", "hier", "sel\u00b7ber", "w\u00fcn\u00b7schen", "soll", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "PIAT", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ist dieses kurtze lied: Es gehe Stoschen wohl!", "tokens": ["Ist", "die\u00b7ses", "kurt\u00b7ze", "lied", ":", "Es", "ge\u00b7he", "Sto\u00b7schen", "wohl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "$.", "PPER", "VVFIN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}