{"textgrid.poem.37912": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Drei Reiter am Thor", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ritten drei Reiter zum Thor hinaus,", "tokens": ["Es", "rit\u00b7ten", "drei", "Rei\u00b7ter", "zum", "Thor", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPRART", "NN", "APZR", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ade!", "tokens": ["A\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Feins Liebchen schaute zum Fenster hinaus,", "tokens": ["Feins", "Lieb\u00b7chen", "schau\u00b7te", "zum", "Fens\u00b7ter", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ade!", "tokens": ["A\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Und wenn es denn soll geschieden seyn,", "tokens": ["Und", "wenn", "es", "denn", "soll", "ge\u00b7schie\u00b7den", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "So reich mir dein goldenes Ringelein,", "tokens": ["So", "reich", "mir", "dein", "gol\u00b7de\u00b7nes", "Rin\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Ade! Ade! Ade!", "tokens": ["A\u00b7de", "!", "A\u00b7de", "!", "A\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Ja, scheiden und lassen thut weh.", "tokens": ["Ja", ",", "schei\u00b7den", "und", "las\u00b7sen", "thut", "weh", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "KON", "VVINF", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Und der uns scheidet, das ist der Tod,", "tokens": ["Und", "der", "uns", "schei\u00b7det", ",", "das", "ist", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VVFIN", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ade!", "tokens": ["A\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Er scheidet so manches Jungfr\u00e4ulein roth,", "tokens": ["Er", "schei\u00b7det", "so", "man\u00b7ches", "Jung\u00b7fr\u00e4u\u00b7lein", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ade!", "tokens": ["A\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Und w\u00e4r doch geworden der liebe Leib,", "tokens": ["Und", "w\u00e4r", "doch", "ge\u00b7wor\u00b7den", "der", "lie\u00b7be", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VAPP", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Der Liebe ein s\u00fc\u00dfer Zeitvertreib,", "tokens": ["Der", "Lie\u00b7be", "ein", "s\u00fc\u00b7\u00dfer", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ade! Ade! Ade!", "tokens": ["A\u00b7de", "!", "A\u00b7de", "!", "A\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Ja, scheiden und lassen thut weh.", "tokens": ["Ja", ",", "schei\u00b7den", "und", "las\u00b7sen", "thut", "weh", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "KON", "VVINF", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Er scheidet das Kind wohl in der Wieg,", "tokens": ["Er", "schei\u00b7det", "das", "Kind", "wohl", "in", "der", "Wieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ade!", "tokens": ["A\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wenn werd ich mein Sch\u00e4tzel doch kriegen?", "tokens": ["Wenn", "werd", "ich", "mein", "Sch\u00e4t\u00b7zel", "doch", "krie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ade!", "tokens": ["A\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Und ist es nicht Morgen? Ach w\u00e4r es doch heut,", "tokens": ["Und", "ist", "es", "nicht", "Mor\u00b7gen", "?", "Ach", "w\u00e4r", "es", "doch", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "NN", "$.", "NN", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Es macht uns allbeiden gar gro\u00dfe Freud,", "tokens": ["Es", "macht", "uns", "all\u00b7bei\u00b7den", "gar", "gro\u00b7\u00dfe", "Freud", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.7": {"text": "Ade! Ade! Ade!", "tokens": ["A\u00b7de", "!", "A\u00b7de", "!", "A\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Ja, scheiden und lassen thut weh.", "tokens": ["Ja", ",", "schei\u00b7den", "und", "las\u00b7sen", "thut", "weh", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "KON", "VVINF", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}