{"textgrid.poem.58425": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "463. Gott hilft dem guten Willen", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer nicht kann werden v\u00f6llig los", "tokens": ["Wer", "nicht", "kann", "wer\u00b7den", "v\u00f6l\u00b7lig", "los"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "VMFIN", "VAFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und will es doch von Herzen,", "tokens": ["Und", "will", "es", "doch", "von", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem macht der Herr die Lust zur Last", "tokens": ["Dem", "macht", "der", "Herr", "die", "Lust", "zur", "Last"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und was er liebt, zu Schmerzen,", "tokens": ["Und", "was", "er", "liebt", ",", "zu", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem nimmt der H\u00f6chste dies und das", "tokens": ["Dem", "nimmt", "der", "H\u00f6chs\u00b7te", "dies", "und", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "PDS", "KON", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und endlich alles gar;", "tokens": ["Und", "end\u00b7lich", "al\u00b7les", "gar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wohl dem, der Gott nur machen l\u00e4\u00dft", "tokens": ["Wohl", "dem", ",", "der", "Gott", "nur", "ma\u00b7chen", "l\u00e4\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "ART", "NN", "ADV", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und gibt ihm alles dar!", "tokens": ["Und", "gibt", "ihm", "al\u00b7les", "dar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}