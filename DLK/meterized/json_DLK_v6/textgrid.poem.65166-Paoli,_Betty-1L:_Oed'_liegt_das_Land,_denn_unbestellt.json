{"textgrid.poem.65166": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oed' liegt das Land, denn unbestellt", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oed' liegt das Land, denn unbestellt", "tokens": ["O\u00b7ed'", "liegt", "das", "Land", ",", "denn", "un\u00b7be\u00b7stellt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "KON", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Blieb Wiese, Flur und Ackerfeld.", "tokens": ["Blieb", "Wie\u00b7se", ",", "Flur", "und", "A\u00b7cker\u00b7feld", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der fr\u00fchlingsmilde Morgenwind", "tokens": ["Der", "fr\u00fch\u00b7lings\u00b7mil\u00b7de", "Mor\u00b7gen\u00b7wind"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Macht keine gr\u00fcnen Halme wogen;", "tokens": ["Macht", "kei\u00b7ne", "gr\u00fc\u00b7nen", "Hal\u00b7me", "wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die D\u00f6rfer stehen leer, es sind", "tokens": ["Die", "D\u00f6r\u00b7fer", "ste\u00b7hen", "leer", ",", "es", "sind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die M\u00e4nner in den Kampf gezogen. \u2013", "tokens": ["Die", "M\u00e4n\u00b7ner", "in", "den", "Kampf", "ge\u00b7zo\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als von Paris die Kunde kam", "tokens": ["Als", "von", "Pa\u00b7ris", "die", "Kun\u00b7de", "kam"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Endes, das der K\u00f6nig nahm,", "tokens": ["Des", "En\u00b7des", ",", "das", "der", "K\u00f6\u00b7nig", "nahm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Als man's versucht, dem Volk den Glauben,", "tokens": ["Als", "man's", "ver\u00b7sucht", ",", "dem", "Volk", "den", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVPP", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Den Seelen ihren Hort zu rauben,", "tokens": ["Den", "See\u00b7len", "ih\u00b7ren", "Hort", "zu", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da hat, im Teuersten verletzt,", "tokens": ["Da", "hat", ",", "im", "Teu\u00b7ers\u00b7ten", "ver\u00b7letzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.12": {"text": "Sich die Vend\u00e9e zur Wehr gesetzt.", "tokens": ["Sich", "die", "Ven\u00b7d\u00e9e", "zur", "Wehr", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Cathelineau, der schlichte Mann,", "tokens": ["Cat\u00b7he\u00b7li\u00b7ne\u00b7au", ",", "der", "schlich\u00b7te", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "Aus seinen stillen Einsamkeiten", "tokens": ["Aus", "sei\u00b7nen", "stil\u00b7len", "Ein\u00b7sam\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Gerissen von dem Sturm der Zeiten", "tokens": ["Ge\u00b7ris\u00b7sen", "von", "dem", "Sturm", "der", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Zerbrach zuerst den dumpfen Bann.", "tokens": ["Zer\u00b7brach", "zu\u00b7erst", "den", "dum\u00b7pfen", "Bann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Durchgl\u00fcht von einem tiefen Schmerz,", "tokens": ["Durch\u00b7gl\u00fcht", "von", "ei\u00b7nem", "tie\u00b7fen", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Verschm\u00e4hte Trauer er und Klagen;", "tokens": ["Ver\u00b7schm\u00e4h\u00b7te", "Trau\u00b7er", "er", "und", "Kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Nach frischer That, nach k\u00fchnem Wagen", "tokens": ["Nach", "fri\u00b7scher", "That", ",", "nach", "k\u00fch\u00b7nem", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Verlangte sein gewaltig Herz.", "tokens": ["Ver\u00b7lang\u00b7te", "sein", "ge\u00b7wal\u00b7tig", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Zum Aufstand rief er, und in Haufen", "tokens": ["Zum", "Auf\u00b7stand", "rief", "er", ",", "und", "in", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Kam jung und alt ihm zugelaufen,", "tokens": ["Kam", "jung", "und", "alt", "ihm", "zu\u00b7ge\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Denn durch sein Wort ward jede Brust", "tokens": ["Denn", "durch", "sein", "Wort", "ward", "je\u00b7de", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Des eig'nen Dranges sich bewu\u00dft.", "tokens": ["Des", "eig'\u00b7nen", "Dran\u00b7ges", "sich", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Nach ihren Jagdgewehren griffen", "tokens": ["Nach", "ih\u00b7ren", "Jagd\u00b7ge\u00b7weh\u00b7ren", "grif\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Sie raschen Muts, nach Spie\u00df und Speer,", "tokens": ["Sie", "ra\u00b7schen", "Muts", ",", "nach", "Spie\u00df", "und", "Speer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Die Beile wurden zugeschliffen,", "tokens": ["Die", "Bei\u00b7le", "wur\u00b7den", "zu\u00b7ge\u00b7schlif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Der Knittel selber ward zur Wehr,", "tokens": ["Der", "Knit\u00b7tel", "sel\u00b7ber", "ward", "zur", "Wehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und, unges\u00e4umt, in Feindesmitten,", "tokens": ["Und", ",", "un\u00b7ge\u00b7s\u00e4umt", ",", "in", "Fein\u00b7des\u00b7mit\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Ward zu des Plans Vollzug geschritten.", "tokens": ["Ward", "zu", "des", "Plans", "Voll\u00b7zug", "ge\u00b7schrit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.31": {"text": "Die Schergen der verha\u00dften Macht,", "tokens": ["Die", "Scher\u00b7gen", "der", "ver\u00b7ha\u00df\u00b7ten", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Die, vom Convent hieher gesandt,", "tokens": ["Die", ",", "vom", "Con\u00b7vent", "hie\u00b7her", "ge\u00b7sandt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPRART", "NN", "PAV", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.33": {"text": "Dem Lande ma\u00dflos Weh gebracht,", "tokens": ["Dem", "Lan\u00b7de", "ma\u00df\u00b7los", "Weh", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Schlug und vertrieb der Bauern Hand.", "tokens": ["Schlug", "und", "ver\u00b7trieb", "der", "Bau\u00b7ern", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Jetzt stand ein blut'ger Weg nur offen,", "tokens": ["Jetzt", "stand", "ein", "blut'\u00b7ger", "Weg", "nur", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Und Heil war nur vom Kampf zu hoffen!", "tokens": ["Und", "Heil", "war", "nur", "vom", "Kampf", "zu", "hof\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Drum that es not, f\u00fcr ihr Beginnen", "tokens": ["Drum", "that", "es", "not", ",", "f\u00fcr", "ihr", "Be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Erprobte F\u00fchrer zu gewinnen,", "tokens": ["Er\u00b7prob\u00b7te", "F\u00fch\u00b7rer", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Die, schlachtenkundig und erfahren,", "tokens": ["Die", ",", "schlach\u00b7ten\u00b7kun\u00b7dig", "und", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Befehligten die wirren Scharen.", "tokens": ["Be\u00b7feh\u00b7lig\u00b7ten", "die", "wir\u00b7ren", "Scha\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.41": {"text": "Die aufzufinden war nicht schwer!", "tokens": ["Die", "auf\u00b7zu\u00b7fin\u00b7den", "war", "nicht", "schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Auf allen Schl\u00f6ssern ringsumher,", "tokens": ["Auf", "al\u00b7len", "Schl\u00f6s\u00b7sern", "rings\u00b7um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "So weit des Himmels W\u00f6lbung blaut,", "tokens": ["So", "weit", "des", "Him\u00b7mels", "W\u00f6l\u00b7bung", "blaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Ringsum auf allen Adelssitzen", "tokens": ["Ring\u00b7sum", "auf", "al\u00b7len", "A\u00b7dels\u00b7sit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.45": {"text": "Hat die Erhebung sich're St\u00fctzen,", "tokens": ["Hat", "die", "Er\u00b7he\u00b7bung", "sich'\u00b7re", "St\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Mit Krieg und Waffenwerk vertraut.", "tokens": ["Mit", "Krieg", "und", "Waf\u00b7fen\u00b7werk", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Es traten zum ersehnten Streite", "tokens": ["Es", "tra\u00b7ten", "zum", "er\u00b7sehn\u00b7ten", "Strei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Die Herren auf der Bauern Seite,", "tokens": ["Die", "Her\u00b7ren", "auf", "der", "Bau\u00b7ern", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Wie, von der L\u00fcfte Zug bewegt,", "tokens": ["Wie", ",", "von", "der", "L\u00fcf\u00b7te", "Zug", "be\u00b7wegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "In's Flammenmeer die Flamme schl\u00e4gt!", "tokens": ["In's", "Flam\u00b7men\u00b7meer", "die", "Flam\u00b7me", "schl\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Ein gro\u00dfer Br\u00fcderbund erstand", "tokens": ["Ein", "gro\u00b7\u00dfer", "Br\u00fc\u00b7der\u00b7bund", "er\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Zum Dienste der gemeinen Sache;", "tokens": ["Zum", "Diens\u00b7te", "der", "ge\u00b7mei\u00b7nen", "Sa\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Derselben Liebe starkes Band,", "tokens": ["Der\u00b7sel\u00b7ben", "Lie\u00b7be", "star\u00b7kes", "Band", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Derselbe hei\u00dfe Durst nach Rache", "tokens": ["Der\u00b7sel\u00b7be", "hei\u00b7\u00dfe", "Durst", "nach", "Ra\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.55": {"text": "Vereinigt Edelmann und Knecht.", "tokens": ["Ver\u00b7ei\u00b7nigt", "E\u00b7del\u00b7mann", "und", "Knecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Einstehend f\u00fcr das alte Recht", "tokens": ["Ein\u00b7ste\u00b7hend", "f\u00fcr", "das", "al\u00b7te", "Recht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Sind alle gleich bereit, ihr Leben", "tokens": ["Sind", "al\u00b7le", "gleich", "be\u00b7reit", ",", "ihr", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PIS", "ADV", "ADJD", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.58": {"text": "Als Kaufpreis f\u00fcr den Sieg zu geben. \u2013", "tokens": ["Als", "Kauf\u00b7preis", "f\u00fcr", "den", "Sieg", "zu", "ge\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vertrauend auf ihr Waffengl\u00fcck", "tokens": ["Ver\u00b7trau\u00b7end", "auf", "ihr", "Waf\u00b7fen\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entbeut ein Heer die Republik.", "tokens": ["Ent\u00b7beut", "ein", "Heer", "die", "Re\u00b7pub\u00b7lik", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Heer von solcher Uebermacht,", "tokens": ["Ein", "Heer", "von", "sol\u00b7cher", "Ue\u00b7ber\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df es in einer off'nen Schlacht", "tokens": ["Da\u00df", "es", "in", "ei\u00b7ner", "off'\u00b7nen", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5