{"textgrid.poem.56791": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vom Tisch des Abendmahls erhob", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Tisch des Abendmahls erhob", "tokens": ["Vom", "Tisch", "des", "A\u00b7bend\u00b7mahls", "er\u00b7hob"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der Nazarener sich zum Gehn", "tokens": ["der", "Na\u00b7za\u00b7re\u00b7ner", "sich", "zum", "Gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "APPRART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "und wandte sich mit seiner Schar", "tokens": ["und", "wand\u00b7te", "sich", "mit", "sei\u00b7ner", "Schar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "des \u00d6lbergs stillen W\u00e4ldern zu.", "tokens": ["des", "\u00d6l\u00b7bergs", "stil\u00b7len", "W\u00e4l\u00b7dern", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Erloschen war der Wolken Glut;", "tokens": ["Er\u00b7lo\u00b7schen", "war", "der", "Wol\u00b7ken", "Glut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "in H\u00fctt' und H\u00f6fen ward es licht;", "tokens": ["in", "H\u00fctt'", "und", "H\u00f6\u00b7fen", "ward", "es", "licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "hell gl\u00e4nzten nah und n\u00e4her schon", "tokens": ["hell", "gl\u00e4nz\u00b7ten", "nah", "und", "n\u00e4\u00b7her", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADJD", "KON", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die Fenster von Gethsemane.", "tokens": ["die", "Fens\u00b7ter", "von", "Geth\u00b7se\u00b7ma\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Aus einer Scheune klang vertraut", "tokens": ["Aus", "ei\u00b7ner", "Scheu\u00b7ne", "klang", "ver\u00b7traut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das Tanzlied eines Dudelsacks,", "tokens": ["das", "Tanz\u00b7lied", "ei\u00b7nes", "Du\u00b7del\u00b7sacks", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und M\u00e4gd und Bursche drehten sich", "tokens": ["und", "M\u00e4gd", "und", "Bur\u00b7sche", "dreh\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "zum Feierabend drin im Tanz.", "tokens": ["zum", "Fei\u00b7er\u00b7a\u00b7bend", "drin", "im", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und Jesus trat ans Tor und sah", "tokens": ["Und", "Je\u00b7sus", "trat", "ans", "Tor", "und", "sah"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "APPRART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mit tiefem Aug dem Treiben zu ...", "tokens": ["mit", "tie\u00b7fem", "Aug", "dem", "Trei\u00b7ben", "zu", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "PTKZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und pl\u00f6tzlich \u00fcbermannte ihn", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "\u00fc\u00b7berm\u00b7ann\u00b7te", "ihn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein dunkles, schluchzendes Gef\u00fchl.", "tokens": ["ein", "dunk\u00b7les", ",", "schluch\u00b7zen\u00b7des", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und, Tr\u00e4nen in den Augen, trat", "tokens": ["Und", ",", "Tr\u00e4\u00b7nen", "in", "den", "Au\u00b7gen", ",", "trat"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "NN", "APPR", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "er zu auf eine junge Magd", "tokens": ["er", "zu", "auf", "ei\u00b7ne", "jun\u00b7ge", "Magd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und fa\u00dfte l\u00e4chelnd ihre Hand", "tokens": ["und", "fa\u00df\u00b7te", "l\u00e4\u00b7chelnd", "ih\u00b7re", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und schritt und drehte sich mit ihr.", "tokens": ["und", "schritt", "und", "dreh\u00b7te", "sich", "mit", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PRF", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ehrf\u00fcrchtig wich der rohe Schwarm;", "tokens": ["Ehr\u00b7f\u00fcrch\u00b7tig", "wich", "der", "ro\u00b7he", "Schwarm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die J\u00fcnger standen starr und bleich; \u2013", "tokens": ["die", "J\u00fcn\u00b7ger", "stan\u00b7den", "starr", "und", "bleich", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er aber schritt und drehte sich", "tokens": ["Er", "a\u00b7ber", "schritt", "und", "dreh\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "KON", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "als wie ein Tr\u00e4umer, weltentr\u00fcckt.", "tokens": ["als", "wie", "ein", "Tr\u00e4u\u00b7mer", ",", "wel\u00b7ten\u00b7tr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da brach auf eines J\u00fcngers Wink", "tokens": ["Da", "brach", "auf", "ei\u00b7nes", "J\u00fcn\u00b7gers", "Wink"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "des Spielers Weise j\u00e4hlings ab \u2013", "tokens": ["des", "Spie\u00b7lers", "Wei\u00b7se", "j\u00e4h\u00b7lings", "ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ein krampfhaft Zucken \u00fcberschrak", "tokens": ["ein", "krampf\u00b7haft", "Zu\u00b7cken", "\u00fc\u00b7bersc\u00b7hrak"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "des Meisters hagre Hochgestalt \u2013:", "tokens": ["des", "Meis\u00b7ters", "hag\u00b7re", "Hoch\u00b7ge\u00b7stalt", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und tief verh\u00fcllten Hauptes ging", "tokens": ["Und", "tief", "ver\u00b7h\u00fcll\u00b7ten", "Haup\u00b7tes", "ging"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "er durch das Tor dem Garten zu ...", "tokens": ["er", "durch", "das", "Tor", "dem", "Gar\u00b7ten", "zu", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie dumpf Gest\u00f6hn verlor es sich", "tokens": ["Wie", "dumpf", "Ge\u00b7st\u00f6hn", "ver\u00b7lor", "es", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "NN", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in der Oliven grauer Nacht.", "tokens": ["in", "der", "O\u00b7li\u00b7ven", "grau\u00b7er", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}