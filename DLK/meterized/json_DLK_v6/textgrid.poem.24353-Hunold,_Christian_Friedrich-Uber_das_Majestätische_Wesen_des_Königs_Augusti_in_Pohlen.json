{"textgrid.poem.24353": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber das Majest\u00e4tische Wesen des K\u00f6nigs Augusti in Pohlen", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sagt die Majest\u00e4t/ die Tugend und dein Wesen/", "tokens": ["Es", "sagt", "die", "Ma\u00b7jes\u00b7t\u00e4t", "/", "die", "Tu\u00b7gend", "und", "dein", "We\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du bist zum K\u00f6nige vom Himmel auserlesen.", "tokens": ["Du", "bist", "zum", "K\u00f6\u00b7ni\u00b7ge", "vom", "Him\u00b7mel", "au\u00b7ser\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Rom r\u00fchme den August auch Majest\u00e4tisch sch\u00f6n;", "tokens": ["Rom", "r\u00fch\u00b7me", "den", "Au\u00b7gust", "auch", "Ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADV", "NN", "ADJD", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Dich g\u00f6ttlichen August/ hat es niemahls gesehn.", "tokens": ["Dich", "g\u00f6tt\u00b7li\u00b7chen", "Au\u00b7gust", "/", "hat", "es", "nie\u00b7mahls", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Held/ du verlohrst dein Reich/ August nur Legionen/", "tokens": ["Held", "/", "du", "ver\u00b7lohrst", "dein", "Reich", "/", "Au\u00b7gust", "nur", "Le\u00b7gi\u00b7o\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$(", "NN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seine Kleinmuth gab sich ungew\u00f6hnlich blo\u00df/", "tokens": ["Und", "sei\u00b7ne", "Klein\u00b7muth", "gab", "sich", "un\u00b7ge\u00b7w\u00f6hn\u00b7lich", "blo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "ADJD", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du aber wurdest noch bey dem Verluste gro\u00df.", "tokens": ["Du", "a\u00b7ber", "wur\u00b7dest", "noch", "bey", "dem", "Ver\u00b7lus\u00b7te", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sprich Gro\u00dfmuth nun/ in wem mehr Majest\u00e4t mu\u00df wohnen.", "tokens": ["Sprich", "Gro\u00df\u00b7muth", "nun", "/", "in", "wem", "mehr", "Ma\u00b7jes\u00b7t\u00e4t", "mu\u00df", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "ADV", "$(", "APPR", "PWS", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}