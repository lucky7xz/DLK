{"textgrid.poem.61455": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du jemals in ein leuchtend Auge", "tokens": ["Wenn", "du", "je\u00b7mals", "in", "ein", "leuch\u00b7tend", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "ADJD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schautest und in seiner feuchten Tiefe", "tokens": ["Schau\u00b7test", "und", "in", "sei\u00b7ner", "feuch\u00b7ten", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Eine liebe Menschenseele ruhn sahst,", "tokens": ["Ei\u00b7ne", "lie\u00b7be", "Men\u00b7schen\u00b7see\u00b7le", "ruhn", "sahst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "O so blick' empor zum Himmel heute!", "tokens": ["O", "so", "blick'", "em\u00b7por", "zum", "Him\u00b7mel", "heu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PTKVZ", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Denn ein gl\u00e4nzend aufgeschlagnes Auge", "tokens": ["Denn", "ein", "gl\u00e4n\u00b7zend", "auf\u00b7ge\u00b7schlag\u00b7nes", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ist auch er, und durch den blauen Schimmer", "tokens": ["Ist", "auch", "er", ",", "und", "durch", "den", "blau\u00b7en", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPER", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Magst du in den Abgrund aller Liebe,", "tokens": ["Magst", "du", "in", "den", "Ab\u00b7grund", "al\u00b7ler", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Magst du tief in Gottes Herz hinabsehn.", "tokens": ["Magst", "du", "tief", "in", "Got\u00b7tes", "Herz", "hin\u00b7ab\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "APPR", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}