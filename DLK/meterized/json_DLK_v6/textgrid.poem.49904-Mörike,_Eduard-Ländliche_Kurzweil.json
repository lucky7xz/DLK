{"textgrid.poem.49904": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "L\u00e4ndliche Kurzweil", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um die Herbstzeit, wenn man abends", "tokens": ["Um", "die", "Herbst\u00b7zeit", ",", "wenn", "man", "a\u00b7bends"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "$,", "KOUS", "PIS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Feld und Garten gerne wieder", "tokens": ["Feld", "und", "Gar\u00b7ten", "ger\u00b7ne", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tauschet mit dem w\u00e4rmern Zimmer,", "tokens": ["Tau\u00b7schet", "mit", "dem", "w\u00e4r\u00b7mern", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald auch schon den lang verschm\u00e4hten", "tokens": ["Bald", "auch", "schon", "den", "lang", "ver\u00b7schm\u00e4h\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ART", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ofen sieht mit andern Augen,", "tokens": ["O\u00b7fen", "sieht", "mit", "an\u00b7dern", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jetzo noch zweideutigen:", "tokens": ["Jet\u00b7zo", "noch", "zwei\u00b7deu\u00b7ti\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Haben wir hier auf dem Lande", "tokens": ["Ha\u00b7ben", "wir", "hier", "auf", "dem", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Noch die allersch\u00f6nsten Stunden", "tokens": ["Noch", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "M\u00fc\u00dfig halb und halb gesch\u00e4ftig", "tokens": ["M\u00fc\u00b7\u00dfig", "halb", "und", "halb", "ge\u00b7sch\u00e4f\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "KON", "ADJD", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Plaudernder Geselligkeit.", "tokens": ["Plau\u00b7dern\u00b7der", "Ge\u00b7sel\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "J\u00fcngst so waren wir am runden", "tokens": ["J\u00fcngst", "so", "wa\u00b7ren", "wir", "am", "run\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tisch versammelt um die Lampe.", "tokens": ["Tisch", "ver\u00b7sam\u00b7melt", "um", "die", "Lam\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Freundin, aus der Ferne", "tokens": ["Ei\u00b7ne", "Freun\u00b7din", ",", "aus", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neulich bei uns angekommen,", "tokens": ["Neu\u00b7lich", "bei", "uns", "an\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sa\u00df, ein holder Gast, im Kreise.", "tokens": ["Sa\u00df", ",", "ein", "hol\u00b7der", "Gast", ",", "im", "Krei\u00b7se", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Abgetragen war das Essen,", "tokens": ["Ab\u00b7ge\u00b7tra\u00b7gen", "war", "das", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur das Tischtuch mu\u00dfte bleiben.", "tokens": ["Nur", "das", "Tischtuch", "mu\u00df\u00b7te", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Reinliche Gef\u00e4\u00dfe vor sich", "tokens": ["Rein\u00b7li\u00b7che", "Ge\u00b7f\u00e4\u00b7\u00dfe", "vor", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "PRF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Eiferten die guten Frauen,", "tokens": ["Ei\u00b7fer\u00b7ten", "die", "gu\u00b7ten", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Wer des vielk\u00f6rnigen Mohnes", "tokens": ["Wer", "des", "viel\u00b7k\u00f6r\u00b7ni\u00b7gen", "Moh\u00b7nes"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.11": {"text": "Gr\u00f6\u00dfern Haufen vor sich bringe;", "tokens": ["Gr\u00f6\u00b7\u00dfern", "Hau\u00b7fen", "vor", "sich", "brin\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "\u2013 Wei\u00dfen hatten wir und blauen \u2013", "tokens": ["\u2013", "Wei\u00b7\u00dfen", "hat\u00b7ten", "wir", "und", "blau\u00b7en", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "VAFIN", "PPER", "KON", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Emsig klopften, unbeschadet", "tokens": ["Em\u00b7sig", "klopf\u00b7ten", ",", "un\u00b7be\u00b7scha\u00b7det"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Des Gespr\u00e4ches, ihre Messer,", "tokens": ["Des", "Ge\u00b7spr\u00e4\u00b7ches", ",", "ih\u00b7re", "Mes\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "W\u00e4hrend ich, zun\u00e4chst dem Lichte,", "tokens": ["W\u00e4h\u00b7rend", "ich", ",", "zu\u00b7n\u00e4chst", "dem", "Lich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "In den Haller Jahresheften", "tokens": ["In", "den", "Hal\u00b7ler", "Jah\u00b7res\u00b7hef\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Bl\u00e4tterte und hin und wieder", "tokens": ["Bl\u00e4t\u00b7ter\u00b7te", "und", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "PTKVZ", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Einen Brocken gab zum besten.", "tokens": ["Ei\u00b7nen", "Bro\u00b7cken", "gab", "zum", "bes\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch nach einer kleinen Stille,", "tokens": ["Doch", "nach", "ei\u00b7ner", "klei\u00b7nen", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Pl\u00f6tzlich wie vom Zaun gebrochen,", "tokens": ["Pl\u00f6tz\u00b7lich", "wie", "vom", "Zaun", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sagte meine Schwester Cl\u00e4rchen,", "tokens": ["Sag\u00b7te", "mei\u00b7ne", "Schwes\u00b7ter", "Cl\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schadenfrohen Blicks nach mir:", "tokens": ["Scha\u00b7den\u00b7fro\u00b7hen", "Blicks", "nach", "mir", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbgeld auf Zinsen auszulehnen", "tokens": ["\u00bb", "geld", "auf", "Zin\u00b7sen", "aus\u00b7zu\u00b7leh\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist wohl keine \u00fcble Sache,", "tokens": ["Ist", "wohl", "kei\u00b7ne", "\u00fcb\u00b7le", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn man es nur christlich treibt;", "tokens": ["Wenn", "man", "es", "nur", "christ\u00b7lich", "treibt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "Denn vom Hundert zieht man immer,", "tokens": ["Denn", "vom", "Hun\u00b7dert", "zieht", "man", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PIS", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wo nicht f\u00fcnfe, doch f\u00fcnfthalbe,", "tokens": ["Wo", "nicht", "f\u00fcn\u00b7fe", ",", "doch", "f\u00fcnf\u00b7thal\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "VVFIN", "$,", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Das ist einem wie geschenkt;", "tokens": ["Das", "ist", "ei\u00b7nem", "wie", "ge\u00b7schenkt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "KOKOM", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Aber wer in m\u00fc\u00dfger Weile", "tokens": ["A\u00b7ber", "wer", "in", "m\u00fc\u00df\u00b7ger", "Wei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "An dem Mohnfeld einst vor\u00fcber", "tokens": ["An", "dem", "Mohn\u00b7feld", "einst", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Schlenderte, der gr\u00fcnen H\u00e4upter", "tokens": ["Schlen\u00b7der\u00b7te", ",", "der", "gr\u00fc\u00b7nen", "H\u00e4up\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Eines an der Seite spaltend,", "tokens": ["Ei\u00b7nes", "an", "der", "Sei\u00b7te", "spal\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Kleine M\u00fcnze drin verbarg,", "tokens": ["Klei\u00b7ne", "M\u00fcn\u00b7ze", "drin", "ver\u00b7barg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Hoffend, da\u00df es gro\u00df und gr\u00f6\u00dfer,", "tokens": ["Hof\u00b7fend", ",", "da\u00df", "es", "gro\u00df", "und", "gr\u00f6\u00b7\u00dfer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Eine Wunderfrucht, erwachse,", "tokens": ["Ei\u00b7ne", "Wun\u00b7der\u00b7frucht", ",", "er\u00b7wach\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Und so viel es K\u00f6rner tr\u00fcge", "tokens": ["Und", "so", "viel", "es", "K\u00f6r\u00b7ner", "tr\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "PPER", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "So viel nagelneue Kreuzer", "tokens": ["So", "viel", "na\u00b7gel\u00b7neu\u00b7e", "Kreu\u00b7zer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "K\u00fcnftig in der d\u00fcrren H\u00fclse", "tokens": ["K\u00fcnf\u00b7tig", "in", "der", "d\u00fcr\u00b7ren", "H\u00fcl\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "(eine feine Kinderklapper,", "tokens": ["(", "ei\u00b7ne", "fei\u00b7ne", "Kin\u00b7der\u00b7klap\u00b7per", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Eine seltne Vogelscheuche!)", "tokens": ["Ei\u00b7ne", "selt\u00b7ne", "Vo\u00b7gel\u00b7scheu\u00b7che", "!", ")"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Klingend in dem Winde sch\u00fcttle,", "tokens": ["Klin\u00b7gend", "in", "dem", "Win\u00b7de", "sch\u00fctt\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Der ist \u00fcbel angef\u00fchrt.", "tokens": ["Der", "ist", "\u00fc\u00b7bel", "an\u00b7ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Nicht nur, da\u00df die Interessen", "tokens": ["Nicht", "nur", ",", "da\u00df", "die", "In\u00b7ter\u00b7es\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Fehlen, auch die sch\u00f6nen Samen", "tokens": ["Feh\u00b7len", ",", "auch", "die", "sch\u00f6\u00b7nen", "Sa\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Sind vergiftet, schwarz gemodert,", "tokens": ["Sind", "ver\u00b7gif\u00b7tet", ",", "schwarz", "ge\u00b7mo\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Und der unfruchtbare Mammon", "tokens": ["Und", "der", "un\u00b7frucht\u00b7ba\u00b7re", "Mam\u00b7mon"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--++--+-", "measure": "iambic.tri.relaxed"}, "line.29": {"text": "Lauter Gr\u00fcnspan, ganz unkenntlich,", "tokens": ["Lau\u00b7ter", "Gr\u00fcn\u00b7span", ",", "ganz", "un\u00b7kennt\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Garstig, wie dies Beispiel zeigt!\u00ab", "tokens": ["Gars\u00b7tig", ",", "wie", "dies", "Bei\u00b7spiel", "zeigt", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "$,", "PWAV", "PDS", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Und hiermit warf sie den Kreuzer", "tokens": ["Und", "hier\u00b7mit", "warf", "sie", "den", "Kreu\u00b7zer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.32": {"text": "Auf den Tisch, da lachte alles.", "tokens": ["Auf", "den", "Tisch", ",", "da", "lach\u00b7te", "al\u00b7les", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bblassen Sie sich das erkl\u00e4ren!\u00ab", "tokens": ["\u00bb", "las\u00b7sen", "Sie", "sich", "das", "er\u00b7kl\u00e4\u00b7ren", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "PRF", "PDS", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sagt ich, zu dem Gast gewendet:", "tokens": ["Sagt", "ich", ",", "zu", "dem", "Gast", "ge\u00b7wen\u00b7det", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbwer in Schwaben einen neuen", "tokens": ["\u00bb", "wer", "in", "Schwa\u00b7ben", "ei\u00b7nen", "neu\u00b7en"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "APPR", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rock anhat zum ersten Male,", "tokens": ["Rock", "an\u00b7hat", "zum", "ers\u00b7ten", "Ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df von Freunden und Bekannten", "tokens": ["Mu\u00df", "von", "Freun\u00b7den", "und", "Be\u00b7kann\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In das neue Taschenfutter", "tokens": ["In", "das", "neu\u00b7e", "Ta\u00b7schen\u00b7fut\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Einen blanken Kreuzer haben;", "tokens": ["Ei\u00b7nen", "blan\u00b7ken", "Kreu\u00b7zer", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und so ward mir, l\u00e4ndlich sittlich,", "tokens": ["Und", "so", "ward", "mir", ",", "l\u00e4nd\u00b7lich", "sitt\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "$,", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Auch der meine vorgen Sommer", "tokens": ["Auch", "der", "mei\u00b7ne", "vor\u00b7gen", "Som\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "F\u00fcr den h\u00fcbschen Schlafrock, eben", "tokens": ["F\u00fcr", "den", "h\u00fcb\u00b7schen", "Schla\u00b7frock", ",", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Den man gegenw\u00e4rtig sieht.", "tokens": ["Den", "man", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "sieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Jenen Morgen nun erging ich", "tokens": ["Je\u00b7nen", "Mor\u00b7gen", "nun", "er\u00b7ging", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Guten Mutes mich im Garten,", "tokens": ["Gu\u00b7ten", "Mu\u00b7tes", "mich", "im", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Tat auch wirklich wie sie sagt,", "tokens": ["Tat", "auch", "wirk\u00b7lich", "wie", "sie", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Doch was ich dabei mir dachte,", "tokens": ["Doch", "was", "ich", "da\u00b7bei", "mir", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Mu\u00df ich wohl am besten wissen.", "tokens": ["Mu\u00df", "ich", "wohl", "am", "bes\u00b7ten", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKA", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Ein Orakel sollt es sein,", "tokens": ["Ein", "O\u00b7ra\u00b7kel", "sollt", "es", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Das der Herbst erproben w\u00fcrde:", "tokens": ["Das", "der", "Herbst", "er\u00b7pro\u00b7ben", "w\u00fcr\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Bringt die Kolbe blauen Samen,", "tokens": ["Bringt", "die", "Kol\u00b7be", "blau\u00b7en", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Ist der liebe Gast nicht kommen;", "tokens": ["Ist", "der", "lie\u00b7be", "Gast", "nicht", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Bringt sie wei\u00dfen, wird er dasein", "tokens": ["Bringt", "sie", "wei\u00b7\u00dfen", ",", "wird", "er", "da\u00b7sein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ART"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.22": {"text": "Eben wenn man sie er\u00f6ffnet;", "tokens": ["E\u00b7ben", "wenn", "man", "sie", "er\u00b7\u00f6ff\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Und um sie genau zu zeichnen", "tokens": ["Und", "um", "sie", "ge\u00b7nau", "zu", "zeich\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADJD", "PTKZU", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "Legt ich jene M\u00fcnze ein.", "tokens": ["Legt", "ich", "je\u00b7ne", "M\u00fcn\u00b7ze", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Aber bald war dieses alles", "tokens": ["A\u00b7ber", "bald", "war", "die\u00b7ses", "al\u00b7les"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PDAT", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Bis den Augenblick vergessen.", "tokens": ["Bis", "den", "Au\u00b7gen\u00b7blick", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Und nun seht\u00ab \u2013", "tokens": ["Und", "nun", "seht", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "$(", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.28": {"text": "\u00bbnichts!\u00ab rief die Schwester:", "tokens": ["\u00bb", "nichts", "!", "\u00ab", "rief", "die", "Schwes\u00b7ter", ":"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "$.", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "\u00bbnein, ich lasse mir's nicht nehmen,", "tokens": ["\u00bb", "nein", ",", "ich", "las\u00b7se", "mir's", "nicht", "neh\u00b7men", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "PPER", "VVFIN", "NE", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Spekulieren wolltest du!", "tokens": ["Spe\u00b7ku\u00b7lie\u00b7ren", "woll\u00b7test", "du", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Und der Fall beweist nur wieder,", "tokens": ["Und", "der", "Fall", "be\u00b7weist", "nur", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Was oft, dich in Schutz zu nehmen,", "tokens": ["Was", "oft", ",", "dich", "in", "Schutz", "zu", "neh\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "PRF", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Andere mit mir bezeugten:", "tokens": ["An\u00b7de\u00b7re", "mit", "mir", "be\u00b7zeug\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.34": {"text": "Da\u00df mein teuerster Herr Bruder", "tokens": ["Da\u00df", "mein", "teu\u00b7ers\u00b7ter", "Herr", "Bru\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.35": {"text": "Bei dem allerbesten Willen", "tokens": ["Bei", "dem", "al\u00b7ler\u00b7bes\u00b7ten", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Zum Kapitalisten eben", "tokens": ["Zum", "Ka\u00b7pi\u00b7ta\u00b7lis\u00b7ten", "e\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.37": {"text": "Einmal nicht geboren ist.\u00ab", "tokens": ["Ein\u00b7mal", "nicht", "ge\u00b7bo\u00b7ren", "ist", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}