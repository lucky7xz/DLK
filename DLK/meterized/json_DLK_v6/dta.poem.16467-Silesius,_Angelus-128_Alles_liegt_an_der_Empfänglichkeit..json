{"dta.poem.16467": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "128  Alles liegt an der Empf\u00e4nglichkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Verm\u00f6cht\u2019 ich GOtts so viel als Christus zu empfange/", "tokens": ["Ver\u00b7m\u00f6cht'", "ich", "Gotts", "so", "viel", "als", "Chris\u00b7tus", "zu", "emp\u00b7fan\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "PIAT", "KOKOM", "NE", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er liesse mich darzu im Augenblik gelangen.", "tokens": ["Er", "lies\u00b7se", "mich", "dar\u00b7zu", "im", "Au\u00b7gen\u00b7blik", "ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}