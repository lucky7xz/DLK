{"textgrid.poem.60205": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freund! z\u00fcrne mit einem stillzankendem Blick", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund! z\u00fcrne mit einem stillzankendem Blick", "tokens": ["Freund", "!", "z\u00fcr\u00b7ne", "mit", "ei\u00b7nem", "still\u00b7zan\u00b7ken\u00b7dem", "Blick"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Am Tage deiner Geburt", "tokens": ["Am", "Ta\u00b7ge", "dei\u00b7ner", "Ge\u00b7burt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Leyer voll dumpfichter Th\u00f6ne zur\u00fcck", "tokens": ["Die", "Le\u00b7yer", "voll", "dump\u00b7fich\u00b7ter", "Th\u00f6\u00b7ne", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "PTKVZ"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und horch die Stimme der Braut.", "tokens": ["Und", "horch", "die", "Stim\u00b7me", "der", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Sie mahlte, die Feder in Flammen getaucht", "tokens": ["Sie", "mahl\u00b7te", ",", "die", "Fe\u00b7der", "in", "Flam\u00b7men", "ge\u00b7taucht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "APPR", "NN", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ihr sonst verschwiegen Gef\u00fchl", "tokens": ["Ihr", "sonst", "ver\u00b7schwie\u00b7gen", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gluth wird vom redenden Blatte gehaucht", "tokens": ["Gluth", "wird", "vom", "re\u00b7den\u00b7den", "Blat\u00b7te", "ge\u00b7haucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPRART", "ADJA", "NN", "VVPP"], "meter": "+-----+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "In dein er\u00f6fnetes Herz.", "tokens": ["In", "dein", "er\u00b7\u00f6f\u00b7ne\u00b7tes", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Du h\u00f6rest Ger\u00e4usche! \u2013 So fl\u00fcstert umher", "tokens": ["Du", "h\u00f6\u00b7rest", "Ge\u00b7r\u00e4u\u00b7sche", "!", "\u2013", "So", "fl\u00fcs\u00b7tert", "um\u00b7her"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$.", "$(", "ADV", "VVFIN", "PTKVZ"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Durch Palmen-B\u00e4ume der West;", "tokens": ["Durch", "Pal\u00b7men\u00b7B\u00e4u\u00b7me", "der", "West", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So murmelt der Bienen fortschw\u00e4rmendes Heer,", "tokens": ["So", "mur\u00b7melt", "der", "Bie\u00b7nen", "fort\u00b7schw\u00e4r\u00b7men\u00b7des", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "So rauscht durch Blumen ein Bach.", "tokens": ["So", "rauscht", "durch", "Blu\u00b7men", "ein", "Bach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Sie sandte Gedanken zu tausenden dir", "tokens": ["Sie", "sand\u00b7te", "Ge\u00b7dan\u00b7ken", "zu", "tau\u00b7sen\u00b7den", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "PTKZU", "VVFIN", "PPER"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und jeder wurde zu Mund.", "tokens": ["Und", "je\u00b7der", "wur\u00b7de", "zu", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Vor einem Wunsche geseufzet von ihr", "tokens": ["Vor", "ei\u00b7nem", "Wun\u00b7sche", "ge\u00b7seuf\u00b7zet", "von", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "APPR", "PPOSAT"], "meter": "-+-+--+-++", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Staunt meine Muse zur\u00fcck.", "tokens": ["Staunt", "mei\u00b7ne", "Mu\u00b7se", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}