{"textgrid.poem.32636": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich tat wahrhaftig alles,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich tat wahrhaftig alles,", "tokens": ["Ich", "tat", "wahr\u00b7haf\u00b7tig", "al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was ich ersinnen konnt',", "tokens": ["Was", "ich", "er\u00b7sin\u00b7nen", "konnt'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich hoffte, da\u00df doch endlich", "tokens": ["Ich", "hoff\u00b7te", ",", "da\u00df", "doch", "end\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir's ihre Liebe lohnt!", "tokens": ["Mir's", "ih\u00b7re", "Lie\u00b7be", "lohnt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich war ein z\u00fcgelloser", "tokens": ["Ich", "war", "ein", "z\u00fc\u00b7gel\u00b7lo\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wilder Feuerbrand,", "tokens": ["Und", "wil\u00b7der", "Feu\u00b7er\u00b7brand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich flog von Dach zu Dache,", "tokens": ["Ich", "flog", "von", "Dach", "zu", "Da\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verheerend was ich fand:", "tokens": ["Ver\u00b7hee\u00b7rend", "was", "ich", "fand", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was ward aus mir? ein Fl\u00e4mmchen,", "tokens": ["Was", "ward", "aus", "mir", "?", "ein", "Fl\u00e4mm\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bescheiden, zahm und zag,", "tokens": ["Be\u00b7schei\u00b7den", ",", "zahm", "und", "zag", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An dem der Hirte h\u00f6chstens", "tokens": ["An", "dem", "der", "Hir\u00b7te", "h\u00f6chs\u00b7tens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Herbst sich w\u00e4rmen mag.", "tokens": ["Im", "Herbst", "sich", "w\u00e4r\u00b7men", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["APPRART", "NN", "PRF", "VVFIN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ich st\u00fcrzte im Gebirge", "tokens": ["Ich", "st\u00fcrz\u00b7te", "im", "Ge\u00b7bir\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu Tal als Wasserfall,", "tokens": ["Zu", "Tal", "als", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KOUS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und brach mit lautem Tosen", "tokens": ["Und", "brach", "mit", "lau\u00b7tem", "To\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der eignen Wogen Schwall:", "tokens": ["Der", "eig\u00b7nen", "Wo\u00b7gen", "Schwall", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und jetzt? Ein tr\u00fcbes B\u00e4chlein,", "tokens": ["Und", "jetzt", "?", "Ein", "tr\u00fc\u00b7bes", "B\u00e4ch\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schleich' ich so stumm dahin,", "tokens": ["Schleich'", "ich", "so", "stumm", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zufrieden, seh' am Ufer", "tokens": ["Zu\u00b7frie\u00b7den", ",", "seh'", "am", "U\u00b7fer"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich ein paar Bl\u00fcmchen bl\u00fchn!", "tokens": ["Ich", "ein", "paar", "Bl\u00fcm\u00b7chen", "bl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich ragt' als rauher Felsblock", "tokens": ["Ich", "ragt'", "als", "rau\u00b7her", "Fels\u00b7block"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bis in die Wolken gar,", "tokens": ["Bis", "in", "die", "Wol\u00b7ken", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo neben Blitz und Donner", "tokens": ["Wo", "ne\u00b7ben", "Blitz", "und", "Don\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Nur haust der stolze Aar;", "tokens": ["Nur", "haust", "der", "stol\u00b7ze", "Aar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich ward zum, lausch'gen Haine", "tokens": ["Ich", "ward", "zum", ",", "lausch'\u00b7gen", "Hai\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im stillen gr\u00fcnen Tal,", "tokens": ["Im", "stil\u00b7len", "gr\u00fc\u00b7nen", "Tal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So einsam und alleine", "tokens": ["So", "ein\u00b7sam", "und", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Klagt hier die Nachtigall!", "tokens": ["Klagt", "hier", "die", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich war ... wie k\u00f6nnt' ich sagen,", "tokens": ["Ich", "war", "...", "wie", "k\u00f6nnt'", "ich", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PWAV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was ich noch alles war?", "tokens": ["Was", "ich", "noch", "al\u00b7les", "war", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PIS", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Herz jedoch, wie fr\u00fcher,", "tokens": ["Ihr", "Herz", "je\u00b7doch", ",", "wie", "fr\u00fc\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "PWAV", "ADJD", "$,"], "meter": "-++-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Blieb jeder Regung bar!", "tokens": ["Blieb", "je\u00b7der", "Re\u00b7gung", "bar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Nein, das ertrag' ich nimmer!", "tokens": ["Nein", ",", "das", "er\u00b7trag'", "ich", "nim\u00b7mer", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu sehr bin ich emp\u00f6rt!", "tokens": ["Zu", "sehr", "bin", "ich", "em\u00b7p\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist denn der Preis so lockend,", "tokens": ["Ist", "denn", "der", "Preis", "so", "lo\u00b7ckend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist er die Opfer wert?", "tokens": ["Ist", "er", "die", "Op\u00b7fer", "wert", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Die Ketten will ich sprengen,", "tokens": ["Die", "Ket\u00b7ten", "will", "ich", "spren\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entwinden mich dem Joch,", "tokens": ["Ent\u00b7win\u00b7den", "mich", "dem", "Joch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie s\u00fc\u00df auch Liebesketten,", "tokens": ["Wie", "s\u00fc\u00df", "auch", "Lie\u00b7bes\u00b7ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie bleiben Ketten doch!", "tokens": ["Sie", "blei\u00b7ben", "Ket\u00b7ten", "doch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Auf meinen alten Schwingen", "tokens": ["Auf", "mei\u00b7nen", "al\u00b7ten", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Flieg' ich empor, befreit:", "tokens": ["Flieg'", "ich", "em\u00b7por", ",", "be\u00b7freit", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ins goldne Reich der Freiheit,", "tokens": ["Ins", "gold\u00b7ne", "Reich", "der", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In die Unendlichkeit!", "tokens": ["In", "die", "Un\u00b7end\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}