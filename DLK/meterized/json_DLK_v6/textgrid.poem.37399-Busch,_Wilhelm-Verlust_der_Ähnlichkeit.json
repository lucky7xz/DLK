{"textgrid.poem.37399": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Verlust der \u00c4hnlichkeit", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man sagt, ein Schn\u00e4pschen, insofern", "tokens": ["Man", "sagt", ",", "ein", "Schn\u00e4p\u00b7schen", ",", "in\u00b7so\u00b7fern"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PIS", "VVFIN", "$,", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es kr\u00e4ftig ist, hat jeder gern.", "tokens": ["Es", "kr\u00e4f\u00b7tig", "ist", ",", "hat", "je\u00b7der", "gern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "$,", "VAFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ganz anders denkt das Volk der Bienen.", "tokens": ["Ganz", "an\u00b7ders", "denkt", "das", "Volk", "der", "Bie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der S\u00fcffel ist verha\u00dft bei ihnen,", "tokens": ["Der", "S\u00fcf\u00b7fel", "ist", "ver\u00b7ha\u00dft", "bei", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Wohlgeruch tut ihnen weh.", "tokens": ["Sein", "Wohl\u00b7ge\u00b7ruch", "tut", "ih\u00b7nen", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie trinken nichts wie Bl\u00fctentee,", "tokens": ["Sie", "trin\u00b7ken", "nichts", "wie", "Bl\u00fc\u00b7ten\u00b7tee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wenn wer kommt, der Schn\u00e4pse trank,", "tokens": ["Und", "wenn", "wer", "kommt", ",", "der", "Schn\u00e4p\u00b7se", "trank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PWS", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gleich ziehen sie den Stachel blank.", "tokens": ["Gleich", "zie\u00b7hen", "sie", "den", "Sta\u00b7chel", "blank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Letzthin hat einem Bienenst\u00f6ckel", "tokens": ["Letz\u00b7thin", "hat", "ei\u00b7nem", "Bie\u00b7nen\u00b7st\u00f6\u00b7ckel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der brave alte Schneider B\u00f6ckel,", "tokens": ["Der", "bra\u00b7ve", "al\u00b7te", "Schnei\u00b7der", "B\u00f6\u00b7ckel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NE", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der nicht mehr n\u00fcchtern in der Tat,", "tokens": ["Der", "nicht", "mehr", "n\u00fcch\u00b7tern", "in", "der", "Tat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Aus Neubegierde sich genaht.", "tokens": ["Aus", "Neu\u00b7be\u00b7gier\u00b7de", "sich", "ge\u00b7naht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Sofort von einem regen Leben", "tokens": ["So\u00b7fort", "von", "ei\u00b7nem", "re\u00b7gen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sieht Meister B\u00f6ckel sich umgeben.", "tokens": ["Sieht", "Meis\u00b7ter", "B\u00f6\u00b7ckel", "sich", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Es dringen giftgetr\u00e4nkte Pfeile", "tokens": ["Es", "drin\u00b7gen", "gift\u00b7ge\u00b7tr\u00e4nk\u00b7te", "Pfei\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "In seine nackten K\u00f6rperteile,", "tokens": ["In", "sei\u00b7ne", "nack\u00b7ten", "K\u00f6r\u00b7per\u00b7tei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ja, manche selbst durch die nur lose", "tokens": ["Ja", ",", "man\u00b7che", "selbst", "durch", "die", "nur", "lo\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PIS", "ADV", "APPR", "PRELS", "ADV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und leichtgewirkte Sommerhose,", "tokens": ["Und", "leicht\u00b7ge\u00b7wirk\u00b7te", "Som\u00b7mer\u00b7ho\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Besonders, weil sie stramm gespannt.", "tokens": ["Be\u00b7son\u00b7ders", ",", "weil", "sie", "stramm", "ge\u00b7spannt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Zum Gl\u00fcck ist B\u00f6ckel kriegsgewandt.", "tokens": ["Zum", "Gl\u00fcck", "ist", "B\u00f6\u00b7ckel", "kriegs\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Er zieht sich k\u00e4mpfend wie ein Held", "tokens": ["Er", "zieht", "sich", "k\u00e4mp\u00b7fend", "wie", "ein", "Held"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Zur\u00fcck ins hohe Erbsenfeld.", "tokens": ["Zu\u00b7r\u00fcck", "ins", "ho\u00b7he", "Erb\u00b7sen\u00b7feld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Hier hat er Zeit, an vielen Stellen", "tokens": ["Hier", "hat", "er", "Zeit", ",", "an", "vie\u00b7len", "Stel\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Des Leibes merklich anzuschwellen,", "tokens": ["Des", "Lei\u00b7bes", "merk\u00b7lich", "an\u00b7zu\u00b7schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Und als er wiederum erscheint,", "tokens": ["Und", "als", "er", "wie\u00b7de\u00b7rum", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Erkennt ihn kaum sein bester Freund.", "tokens": ["Er\u00b7kennt", "ihn", "kaum", "sein", "bes\u00b7ter", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Nat\u00fcrlich, denn bei solchem Streit", "tokens": ["Na\u00b7t\u00fcr\u00b7lich", ",", "denn", "bei", "sol\u00b7chem", "Streit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Verliert man seine \u00c4hnlichkeit.", "tokens": ["Ver\u00b7liert", "man", "sei\u00b7ne", "\u00c4hn\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}