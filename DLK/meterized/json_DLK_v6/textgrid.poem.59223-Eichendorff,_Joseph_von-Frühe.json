{"textgrid.poem.59223": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fche", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Osten graut's, der Nebel f\u00e4llt,", "tokens": ["Im", "Os\u00b7ten", "graut's", ",", "der", "Ne\u00b7bel", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer wei\u00df, wie bald sich's r\u00fchret!", "tokens": ["Wer", "wei\u00df", ",", "wie", "bald", "sich's", "r\u00fch\u00b7ret", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch schwer im Schlaf noch ruht die Welt,", "tokens": ["Doch", "schwer", "im", "Schlaf", "noch", "ruht", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von allem nichts versp\u00fcret.", "tokens": ["Von", "al\u00b7lem", "nichts", "ver\u00b7sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nur eine fr\u00fche Lerche steigt,", "tokens": ["Nur", "ei\u00b7ne", "fr\u00fc\u00b7he", "Ler\u00b7che", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es hat ihr was getr\u00e4umet", "tokens": ["Es", "hat", "ihr", "was", "ge\u00b7tr\u00e4u\u00b7met"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vom Lichte, wenn noch alles schweigt,", "tokens": ["Vom", "Lich\u00b7te", ",", "wenn", "noch", "al\u00b7les", "schweigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das kaum die H\u00f6hen s\u00e4umet.", "tokens": ["Das", "kaum", "die", "H\u00f6\u00b7hen", "s\u00e4u\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}