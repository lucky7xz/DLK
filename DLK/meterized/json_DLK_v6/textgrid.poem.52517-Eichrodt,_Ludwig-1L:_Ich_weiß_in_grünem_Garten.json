{"textgrid.poem.52517": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wei\u00df in gr\u00fcnem Garten", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df in gr\u00fcnem Garten", "tokens": ["Ich", "wei\u00df", "in", "gr\u00fc\u00b7nem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den allersch\u00f6nsten Ort,", "tokens": ["Den", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Ort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die stillen Sterne warten", "tokens": ["Die", "stil\u00b7len", "Ster\u00b7ne", "war\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf liebende Herzen dort.", "tokens": ["Auf", "lie\u00b7ben\u00b7de", "Her\u00b7zen", "dort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Es spielen durch die Lauben", "tokens": ["Es", "spie\u00b7len", "durch", "die", "Lau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Lichter des Mondenscheins,", "tokens": ["Die", "Lich\u00b7ter", "des", "Mon\u00b7den\u00b7scheins", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es fl\u00fcstern durch die Trauben", "tokens": ["Es", "fl\u00fcs\u00b7tern", "durch", "die", "Trau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Geister s\u00fc\u00dfen Weins.", "tokens": ["Die", "Geis\u00b7ter", "s\u00fc\u00b7\u00dfen", "Weins", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es wispern leis und linde", "tokens": ["Es", "wis\u00b7pern", "leis", "und", "lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Abendwinde, die laun,", "tokens": ["Die", "Ab\u00b7end\u00b7win\u00b7de", ",", "die", "laun", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und durch die schlanken Gewinde", "tokens": ["Und", "durch", "die", "schlan\u00b7ken", "Ge\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verliebte Blumen schaun.", "tokens": ["Ver\u00b7lieb\u00b7te", "Blu\u00b7men", "schaun", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vom H\u00fcgel rauschet nieder", "tokens": ["Vom", "H\u00fc\u00b7gel", "rau\u00b7schet", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der dunkle Kastanienwald,", "tokens": ["Der", "dunk\u00b7le", "Kas\u00b7ta\u00b7ni\u00b7en\u00b7wald", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du h\u00f6rest Schlummerlieder", "tokens": ["Du", "h\u00f6\u00b7rest", "Schlum\u00b7mer\u00b7lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Voll zaubrischer Gewalt.", "tokens": ["Voll", "zaub\u00b7ri\u00b7scher", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Sterne des Himmels erwarten", "tokens": ["Die", "Ster\u00b7ne", "des", "Him\u00b7mels", "er\u00b7war\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zwei liebende Herzen dort,", "tokens": ["Zwei", "lie\u00b7ben\u00b7de", "Her\u00b7zen", "dort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich wei\u00df in gr\u00fcnem Garten", "tokens": ["Ich", "wei\u00df", "in", "gr\u00fc\u00b7nem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den allersch\u00f6nsten Ort.", "tokens": ["Den", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Ort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}