{"dta.poem.3941": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Flieht, ihr treu-ergebnen Bl\u00e4tter,", "tokens": ["Flieht", ",", "ihr", "treu\u00b7er\u00b7geb\u00b7nen", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fcsset meinen lieben Vetter,", "tokens": ["Gr\u00fcs\u00b7set", "mei\u00b7nen", "lie\u00b7ben", "Vet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da er und sein sch\u00f6nes Kind", "tokens": ["Da", "er", "und", "sein", "sch\u00f6\u00b7nes", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KON", "PPOSAT", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Heute copuliret sind;", "tokens": ["Heu\u00b7te", "co\u00b7pu\u00b7li\u00b7ret", "sind", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VAFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Sagt, ihr seyd von dem geschrieben,", "tokens": ["Sagt", ",", "ihr", "seyd", "von", "dem", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VAFIN", "APPR", "ART", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der sie beyderseits zu lieben,", "tokens": ["Der", "sie", "bey\u00b7der\u00b7seits", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich bey Tage so als Nacht,", "tokens": ["Sich", "bey", "Ta\u00b7ge", "so", "als", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "ADV", "KOUS", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Jederzeit verbindlich macht.", "tokens": ["Je\u00b7der\u00b7zeit", "ver\u00b7bind\u00b7lich", "macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}