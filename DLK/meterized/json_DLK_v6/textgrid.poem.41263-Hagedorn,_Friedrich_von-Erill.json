{"textgrid.poem.41263": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erill", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir wissen, da\u00df Erill nie g\u00fcnstig denkt noch spricht:", "tokens": ["Wir", "wis\u00b7sen", ",", "da\u00df", "E\u00b7rill", "nie", "g\u00fcns\u00b7tig", "denkt", "noch", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NE", "ADV", "ADJD", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Beifall bringen ihn Geist oder Sitten nicht.", "tokens": ["Zum", "Bei\u00b7fall", "brin\u00b7gen", "ihn", "Geist", "o\u00b7der", "Sit\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "PTKNEG", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es gleiche noch ein Herz, mein Wilckens, deinem Herzen,", "tokens": ["Es", "glei\u00b7che", "noch", "ein", "Herz", ",", "mein", "Wil\u00b7ckens", ",", "dei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Witz selbst Rab'ners Witz in seiner Kunst zu scherzen;", "tokens": ["Ein", "Witz", "selbst", "Rab'\u00b7ners", "Witz", "in", "sei\u00b7ner", "Kunst", "zu", "scher\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Besitzet, k\u00f6nnt' es sein, zum sch\u00f6nsten Eigenthum,", "tokens": ["Be\u00b7sit\u00b7zet", ",", "k\u00f6nnt'", "es", "sein", ",", "zum", "sch\u00f6ns\u00b7ten", "Ei\u00b7gen\u00b7thum", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VMFIN", "PPER", "VAINF", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Leibnitz Wissenschaft und unbegrenzten Ruhm;", "tokens": ["Des", "Leib\u00b7nitz", "Wis\u00b7sen\u00b7schaft", "und", "un\u00b7be\u00b7grenz\u00b7ten", "Ruhm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Euch m\u00f6gen Tugenden, Verdienst' und Gl\u00fcck erheben:", "tokens": ["Euch", "m\u00f6\u00b7gen", "Tu\u00b7gen\u00b7den", ",", "Ver\u00b7dienst'", "und", "Gl\u00fcck", "er\u00b7he\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "An jedem Vorzug wird sein Bi\u00df, sein Geifer kleben.", "tokens": ["An", "je\u00b7dem", "Vor\u00b7zug", "wird", "sein", "Bi\u00df", ",", "sein", "Gei\u00b7fer", "kle\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man nenn' ihn, wie man will, stolz, neidisch und verg\u00e4llt:", "tokens": ["Man", "nenn'", "ihn", ",", "wie", "man", "will", ",", "stolz", ",", "nei\u00b7disch", "und", "ver\u00b7g\u00e4llt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "PWAV", "PIS", "VMFIN", "$,", "ADJD", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich nenne den gestraft, dem keiner wohlgef\u00e4llt.", "tokens": ["Ich", "nen\u00b7ne", "den", "ge\u00b7straft", ",", "dem", "kei\u00b7ner", "wohl\u00b7ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}