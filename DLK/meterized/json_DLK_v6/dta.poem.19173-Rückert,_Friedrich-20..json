{"dta.poem.19173": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um Mittag, wenn mit Duft der Himmel sich ums\u00e4umt,", "tokens": ["Um", "Mit\u00b7tag", ",", "wenn", "mit", "Duft", "der", "Him\u00b7mel", "sich", "um\u00b7s\u00e4umt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "KOUS", "APPR", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hinter wei\u00dfem Flor die stille Sonne tr\u00e4umt,", "tokens": ["Und", "hin\u00b7ter", "wei\u00b7\u00dfem", "Flor", "die", "stil\u00b7le", "Son\u00b7ne", "tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kein Hauch das welke Blatt im Waldgebirg erfrischt,", "tokens": ["Kein", "Hauch", "das", "wel\u00b7ke", "Blatt", "im", "Wald\u00b7ge\u00b7birg", "er\u00b7frischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo nur die Grille schrillt und nur die Schlange zischt;", "tokens": ["Wo", "nur", "die", "Gril\u00b7le", "schrillt", "und", "nur", "die", "Schlan\u00b7ge", "zischt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dann halten wei\u00dfverh\u00fcllt die Geister ihre Runde,", "tokens": ["Dann", "hal\u00b7ten", "wei\u00df\u00b7ver\u00b7h\u00fcllt", "die", "Geis\u00b7ter", "ih\u00b7re", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alle Sch\u00e4tze thun sich auf im Erdengrunde.", "tokens": ["Und", "al\u00b7le", "Sch\u00e4t\u00b7ze", "thun", "sich", "auf", "im", "Er\u00b7den\u00b7grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PRF", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das sind die Geister und die Sch\u00e4tze, die der Macht", "tokens": ["Das", "sind", "die", "Geis\u00b7ter", "und", "die", "Sch\u00e4t\u00b7ze", ",", "die", "der", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sonne folgen, nicht dem Mond der Mitternacht.", "tokens": ["Der", "Son\u00b7ne", "fol\u00b7gen", ",", "nicht", "dem", "Mond", "der", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und wer ein Sonnenkind ist rein von allem B\u00f6sen,", "tokens": ["Und", "wer", "ein", "Son\u00b7nen\u00b7kind", "ist", "rein", "von", "al\u00b7lem", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VAFIN", "ADJD", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der kann der Sch\u00e4tze Bann, das Band der Geister l\u00f6sen.", "tokens": ["Der", "kann", "der", "Sch\u00e4t\u00b7ze", "Bann", ",", "das", "Band", "der", "Geis\u00b7ter", "l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "NE", "$,", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}