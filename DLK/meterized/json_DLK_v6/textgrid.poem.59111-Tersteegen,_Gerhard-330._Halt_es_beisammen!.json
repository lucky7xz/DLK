{"textgrid.poem.59111": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "330. Halt es beisammen!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verwickle dich in Vielheit nicht,", "tokens": ["Ver\u00b7wick\u00b7le", "dich", "in", "Viel\u00b7heit", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie bei Zerstreuten leicht geschicht!", "tokens": ["Wie", "bei", "Zer\u00b7streu\u00b7ten", "leicht", "ge\u00b7schicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es h\u00e4lt vom Einen dich zur\u00fccke,", "tokens": ["Es", "h\u00e4lt", "vom", "Ei\u00b7nen", "dich", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zerrei\u00dft den Geist in so viel St\u00fccke.", "tokens": ["Zer\u00b7rei\u00dft", "den", "Geist", "in", "so", "viel", "St\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}