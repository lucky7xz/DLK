{"textgrid.poem.55648": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "[was wird mir jede Stunde so bang?]", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was wird mir jede Stunde so bang? \u2013", "tokens": ["Was", "wird", "mir", "je\u00b7de", "Stun\u00b7de", "so", "bang", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PIAT", "NN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das Leben ist kurz, der Tag ist lang.", "tokens": ["Das", "Le\u00b7ben", "ist", "kurz", ",", "der", "Tag", "ist", "lang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und immer sehnt sich fort das Herz,", "tokens": ["Und", "im\u00b7mer", "sehnt", "sich", "fort", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "PTKVZ", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wei\u00df nicht recht, ob himmelw\u00e4rts;", "tokens": ["Ich", "wei\u00df", "nicht", "recht", ",", "ob", "him\u00b7mel\u00b7w\u00e4rts", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "$,", "KOUS", "ADV", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Fort aber will es hin und hin", "tokens": ["Fort", "a\u00b7ber", "will", "es", "hin", "und", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VMFIN", "PPER", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und m\u00f6chte vor sich selber fliehn.", "tokens": ["Und", "m\u00f6ch\u00b7te", "vor", "sich", "sel\u00b7ber", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und fliegt es an der Liebsten Brust,", "tokens": ["Und", "fliegt", "es", "an", "der", "Liebs\u00b7ten", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da ruht's im Himmel unbewu\u00dft;", "tokens": ["Da", "ruht's", "im", "Him\u00b7mel", "un\u00b7be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Lebestrudel rei\u00dft es fort,", "tokens": ["Der", "Le\u00b7be\u00b7stru\u00b7del", "rei\u00dft", "es", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und immer h\u00e4ngt's an ", "tokens": ["Und", "im\u00b7mer", "h\u00e4ngt's", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PTKVZ"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Was es gewollt, was es verlor,", "tokens": ["Was", "es", "ge\u00b7wollt", ",", "was", "es", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMPP", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Es bleibt zuletzt sein eigner Tor.", "tokens": ["Es", "bleibt", "zu\u00b7letzt", "sein", "eig\u00b7ner", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}