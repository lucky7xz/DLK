{"dta.poem.20589": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Hans und Verene .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.99"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "Es gfallt mer nummen eini,               ", "tokens": ["Es", "gfallt", "mer", "num\u00b7men", "ei\u00b7ni", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und selli gfallt mer gwis!", "tokens": ["und", "sel\u00b7li", "gfallt", "mer", "gwis", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O wenni doch das Meidli h\u00e4tt,", "tokens": ["O", "wen\u00b7ni", "doch", "das", "Meid\u00b7li", "h\u00e4tt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "es isch so flink und dundersnett,", "tokens": ["es", "isch", "so", "flink", "und", "dun\u00b7ders\u00b7nett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "so dundersnett,", "tokens": ["so", "dun\u00b7ders\u00b7nett", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "i w\u00e4r im Paradies!", "tokens": ["i", "w\u00e4r", "im", "Pa\u00b7ra\u00b7dies", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u2019s isch wohr, das Meidli gfallt mer,", "tokens": ["'s", "isch", "wohr", ",", "das", "Meid\u00b7li", "gfallt", "mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "$,", "PRELS", "PIS", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und \u2019s Meidli h\u00e4tti gern!", "tokens": ["und", "'s", "Meid\u00b7li", "h\u00e4t\u00b7ti", "gern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NE", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u2019s het alliwil e frohe Mueth,", "tokens": ["'s", "het", "al\u00b7li\u00b7wil", "e", "fro\u00b7he", "Mu\u00b7eth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "e Gsichtli hets, wie Milch und Bluet,", "tokens": ["e", "Gsicht\u00b7li", "hets", ",", "wie", "Milch", "und", "Blu\u00b7et", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "PWAV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "wie Milch und Bluet,", "tokens": ["wie", "Milch", "und", "Blu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "und Auge wie ne Stern.", "tokens": ["und", "Au\u00b7ge", "wie", "ne", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wenni \u2019s sieh vo witem,", "tokens": ["Und", "wen\u00b7ni", "'s", "sieh", "vo", "wi\u00b7tem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "FM", "FM", "FM", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "se schie\u00dft mer \u2019s Bluet ins Gsicht;", "tokens": ["se", "schie\u00dft", "mer", "'s", "Blu\u00b7et", "ins", "Gsicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "es wird mer \u00fcbers Herz so chnapp,", "tokens": ["es", "wird", "mer", "\u00fc\u00b7bers", "Herz", "so", "chnapp", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und \u2019s Wasser lauft mer d\u2019 Backen ab,", "tokens": ["und", "'s", "Was\u00b7ser", "lauft", "mer", "d'", "Ba\u00b7cken", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "VVFIN", "ADV", "NE", "NN", "PTKVZ", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "wohl d\u2019 Backen ab;", "tokens": ["wohl", "d'", "Ba\u00b7cken", "ab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "i wei\u00df nit, wie mer gschicht.", "tokens": ["i", "wei\u00df", "nit", ",", "wie", "mer", "gschicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Am Zistig fr\u00fceih bym Brunne,", "tokens": ["Am", "Zis\u00b7tig", "fr\u00fc\u00b7eih", "bym", "Brun\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "se redt \u2019s mi frey no a:", "tokens": ["se", "redt", "'s", "mi", "frey", "no", "a", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "FM", "FM", "FM", "FM", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "\u201echumm, l\u00fcpf mer Hans! Was fehlt der echt?", "tokens": ["\u201e", "chumm", ",", "l\u00fcpf", "mer", "Hans", "!", "Was", "fehlt", "der", "echt", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "VVFIN", "ADV", "NE", "$.", "PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ees isch der n\u00e4ume gar nit recht,", "tokens": ["\u201e", "es", "isch", "der", "n\u00e4u\u00b7me", "gar", "nit", "recht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJD", "ART", "NN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "nei gar nit recht!\u201c", "tokens": ["nei", "gar", "nit", "recht", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ADJD", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "J denk mi Lebtig dra.", "tokens": ["J", "denk", "mi", "Leb\u00b7tig", "dra", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "J ha \u2019s em solle sage,", "tokens": ["J", "ha", "'s", "em", "sol\u00b7le", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und h\u00e4tti \u2019s numme gseit!", "tokens": ["und", "h\u00e4t\u00b7ti", "'s", "num\u00b7me", "gseit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wenni numme richer w\u00e4r,", "tokens": ["Und", "wen\u00b7ni", "num\u00b7me", "ri\u00b7cher", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und w\u00e4r mer nit mi Herz so schwer,", "tokens": ["und", "w\u00e4r", "mer", "nit", "mi", "Herz", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PTKNEG", "NE", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "mi Herz so schwer,", "tokens": ["mi", "Herz", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "\u2019s g\u00e4b wieder Glegeheit.", "tokens": ["'s", "g\u00e4b", "wie\u00b7der", "Gle\u00b7ge\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Und uf und furt, iez gangi,", "tokens": ["Und", "uf", "und", "furt", ",", "iez", "gan\u00b7gi", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "VVFIN", "$,", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u2019s w\u00fcrd i\u00e4ten im Salat,", "tokens": ["'s", "w\u00fcrd", "i\u00b7\u00e4\u00b7ten", "im", "Sa\u00b7lat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und sag em\u2019s, wenni n\u00e4ume cha,", "tokens": ["und", "sag", "em's", ",", "wen\u00b7ni", "n\u00e4u\u00b7me", "cha", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und luegt es mi nit fr\u00fcndli a,", "tokens": ["und", "luegt", "es", "mi", "nit", "fr\u00fcnd\u00b7li", "a", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NE", "PTKNEG", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "nit fr\u00fcndli a,", "tokens": ["nit", "fr\u00fcnd\u00b7li", "a", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "FM", "FM", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "se bini morn Saldat.", "tokens": ["se", "bi\u00b7ni", "morn", "Sal\u00b7dat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.7": {"line.1": {"text": "En arme Kerli bini,", "tokens": ["En", "ar\u00b7me", "Ker\u00b7li", "bi\u00b7ni", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "arm bini sel isch wohr!", "tokens": ["arm", "bi\u00b7ni", "sel", "isch", "wohr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Doch hani no n\u00fct Unrechts tho,", "tokens": ["Doch", "ha\u00b7ni", "no", "n\u00fct", "Un\u00b7rechts", "tho", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und sufer gwachse w\u00e4ri io,", "tokens": ["und", "su\u00b7fer", "gwach\u00b7se", "w\u00e4\u00b7ri", "i\u00b7o", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "das w\u00e4ri io,", "tokens": ["das", "w\u00e4\u00b7ri", "i\u00b7o", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "mit sellem h\u00e4tts kei Gfohr.", "tokens": ["mit", "sel\u00b7lem", "h\u00e4tts", "kei", "Gfohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Was wisplet in de H\u00fcrste,", "tokens": ["Was", "wisp\u00b7let", "in", "de", "H\u00fcrs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "was r\u00fcehrt si echterst d\u00f6rt?", "tokens": ["was", "r\u00fc\u00b7ehrt", "si", "ech\u00b7terst", "d\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es visperlet, es ruuscht im Laub.", "tokens": ["Es", "vis\u00b7per\u00b7let", ",", "es", "ruuscht", "im", "Laub", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", 