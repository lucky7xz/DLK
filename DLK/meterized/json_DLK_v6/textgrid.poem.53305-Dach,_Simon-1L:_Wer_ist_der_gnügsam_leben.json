{"textgrid.poem.53305": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer ist der gn\u00fcgsam leben", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer ist der gn\u00fcgsam leben", "tokens": ["Wer", "ist", "der", "gn\u00fcg\u00b7sam", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vnd selig sterben wil?", "tokens": ["Vnd", "se\u00b7lig", "ster\u00b7ben", "wil", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich weis jhm Raht zu geben:", "tokens": ["Ich", "weis", "jhm", "Raht", "zu", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er halte Gott fein still", "tokens": ["Er", "hal\u00b7te", "Gott", "fein", "still"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "ADJD", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd sch\u00e4tze wegen seiner Schuld", "tokens": ["Vnd", "sch\u00e4t\u00b7ze", "we\u00b7gen", "sei\u00b7ner", "Schuld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich aller Straff wehrt in Gedult,", "tokens": ["Sich", "al\u00b7ler", "Straff", "wehrt", "in", "Ge\u00b7dult", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gott bleibet doch die Ehr' allein,", "tokens": ["Gott", "blei\u00b7bet", "doch", "die", "Ehr'", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er mu\u00df nur Meister seyn.", "tokens": ["Er", "mu\u00df", "nur", "Meis\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Mensch, die arme Made,", "tokens": ["Ein", "Mensch", ",", "die", "ar\u00b7me", "Ma\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie m\u00e4chtig er sich h\u00e4lt,", "tokens": ["Wie", "m\u00e4ch\u00b7tig", "er", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat er nicht Gottes Gnade,", "tokens": ["Hat", "er", "nicht", "Got\u00b7tes", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was n\u00fctzt ihm alle Welt?", "tokens": ["Was", "n\u00fctzt", "ihm", "al\u00b7le", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hie hilfft kein Hertz, kein Lewen-Muth", "tokens": ["Hie", "hilfft", "kein", "Hertz", ",", "kein", "Le\u00b7wen\u00b7Muth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Adel, kein Geschlecht, kein Gut,", "tokens": ["Kein", "A\u00b7del", ",", "kein", "Ge\u00b7schlecht", ",", "kein", "Gut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer Demuth f\u00fcr dem Herren hat", "tokens": ["Wer", "De\u00b7muth", "f\u00fcr", "dem", "Her\u00b7ren", "hat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "APPR", "ART", "NN", "VAFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Weis seinen Sachen Raht.", "tokens": ["Weis", "sei\u00b7nen", "Sa\u00b7chen", "Raht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie hoch wer ist auff Erden,", "tokens": ["Wie", "hoch", "wer", "ist", "auff", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PWS", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie sehr er mus geehrt,", "tokens": ["Wie", "sehr", "er", "mus", "ge\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie sehr gef\u00fcrchtet werden,", "tokens": ["Wie", "sehr", "ge\u00b7f\u00fcrch\u00b7tet", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ob sich sein Ansehn mehrt,", "tokens": ["Ob", "sich", "sein", "An\u00b7sehn", "mehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ob alles jhm nach Wunsch ergeht,", "tokens": ["Ob", "al\u00b7les", "jhm", "nach", "Wunsch", "er\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd dienstlich zu Gebote steht,", "tokens": ["Vnd", "dienst\u00b7lich", "zu", "Ge\u00b7bo\u00b7te", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "K\u00f6mpt jhm ein Fieberchen nur bey,", "tokens": ["K\u00f6mpt", "jhm", "ein", "Fie\u00b7ber\u00b7chen", "nur", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So merckt er, wer er sey.", "tokens": ["So", "merckt", "er", ",", "wer", "er", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "K\u00f6mpt aber gar sein Ende,", "tokens": ["K\u00f6mpt", "a\u00b7ber", "gar", "sein", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Todt streckt nach Ihm au\u00df", "tokens": ["Der", "Todt", "streckt", "nach", "Ihm", "au\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Die abgefleischten H\u00e4nde,", "tokens": ["Die", "ab\u00b7ge\u00b7fleischten", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gemahlin, Kinder, Hau\u00df,", "tokens": ["Ge\u00b7mah\u00b7lin", ",", "Kin\u00b7der", ",", "Hau\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gut, Freundschafft, alle Herrlicheit", "tokens": ["Gut", ",", "Freund\u00b7schafft", ",", "al\u00b7le", "Herr\u00b7li\u00b7cheit"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sind vnd verbleiben dieser Zeit,", "tokens": ["Sind", "vnd", "ver\u00b7blei\u00b7ben", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er stirbt verlassen vnd allein,", "tokens": ["Er", "stirbt", "ver\u00b7las\u00b7sen", "vnd", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVINF", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was regt sich da f\u00fcr Pein!", "tokens": ["Was", "regt", "sich", "da", "f\u00fcr", "Pein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wird dann nicht bey jhm funden", "tokens": ["Wird", "dann", "nicht", "bey", "jhm", "fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PTKNEG", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Rew vnd Demuth Prei\u00df,", "tokens": ["Der", "Rew", "vnd", "De\u00b7muth", "Prei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo Er zu Christi Wunden", "tokens": ["Wo", "Er", "zu", "Chris\u00b7ti", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht schnelle Zuflucht weis,", "tokens": ["Nicht", "schnel\u00b7le", "Zu\u00b7flucht", "weis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So mu\u00df er nur verzweiffelt stehn,", "tokens": ["So", "mu\u00df", "er", "nur", "ver\u00b7zweif\u00b7felt", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd ewig, ewig vntergehn,", "tokens": ["Vnd", "e\u00b7wig", ",", "e\u00b7wig", "vn\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihm hilfft es nichts, vnd hat er gleich", "tokens": ["Ihm", "hilfft", "es", "nichts", ",", "vnd", "hat", "er", "gleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$,", "KON", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Erden gantzes Reich.", "tokens": ["Der", "Er\u00b7den", "gant\u00b7zes", "Reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nun heisst Gott alle scheiden", "tokens": ["Nun", "heisst", "Gott", "al\u00b7le", "schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PIAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So bald es jhm gef\u00e4llt,", "tokens": ["So", "bald", "es", "jhm", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann ist der Spruch zu leiden", "tokens": ["Dann", "ist", "der", "Spruch", "zu", "lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "De\u00df Richters aller Welt:", "tokens": ["De\u00df", "Rich\u00b7ters", "al\u00b7ler", "Welt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O Mensch, nimm st\u00fcndlich deiner wahr,", "tokens": ["O", "Mensch", ",", "nimm", "st\u00fcnd\u00b7lich", "dei\u00b7ner", "wahr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "ADJD", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Entkomm durch Busse der Gefahr,", "tokens": ["Ent\u00b7komm", "durch", "Bus\u00b7se", "der", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie giebet Ruh in dieser Zeit,", "tokens": ["Sie", "gie\u00b7bet", "Ruh", "in", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vnd dort die Seligkeit.", "tokens": ["Vnd", "dort", "die", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}