{"dta.poem.3116": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "161.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich w\u00fcnsche, da\u00df dein Gl\u00fcck sich jeden Tag erneue,", "tokens": ["Ich", "w\u00fcn\u00b7sche", ",", "da\u00df", "dein", "Gl\u00fcck", "sich", "je\u00b7den", "Tag", "er\u00b7neu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "PRF", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df eine gute That dich jede Stund' erfreue!", "tokens": ["Da\u00df", "ei\u00b7ne", "gu\u00b7te", "That", "dich", "je\u00b7de", "Stund'", "er\u00b7freu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wenn nicht eine That, sodoch ein gutes Wort,", "tokens": ["Und", "wenn", "nicht", "ei\u00b7ne", "That", ",", "so\u00b7doch", "ein", "gu\u00b7tes", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PTKNEG", "ART", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das selbst unsterblich wirkt zu guten Thaten fort.", "tokens": ["Das", "selbst", "uns\u00b7terb\u00b7lich", "wirkt", "zu", "gu\u00b7ten", "Tha\u00b7ten", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wenn kein Wort, doch ein Gedanke sch\u00f6n und wahr,", "tokens": ["Und", "wenn", "kein", "Wort", ",", "doch", "ein", "Ge\u00b7dan\u00b7ke", "sch\u00f6n", "und", "wahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "NN", "$,", "ADV", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der dir die Seele mach' und rings die Sch\u00f6pfung klar.", "tokens": ["Der", "dir", "die", "See\u00b7le", "mach'", "und", "rings", "die", "Sch\u00f6p\u00b7fung", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "KON", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nichts anders kann erfreun den Menschen und erheben,", "tokens": ["Nichts", "an\u00b7ders", "kann", "er\u00b7freun", "den", "Men\u00b7schen", "und", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VMFIN", "VVINF", "ART", "NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie diese Zeugnisse von eignem h\u00f6herm Leben.", "tokens": ["Wie", "die\u00b7se", "Zeug\u00b7nis\u00b7se", "von", "eig\u00b7nem", "h\u00f6\u00b7herm", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und was das Gl\u00fcck von Lohn ihm zu von au\u00dfen sp\u00fclt,", "tokens": ["Und", "was", "das", "Gl\u00fcck", "von", "Lohn", "ihm", "zu", "von", "au\u00b7\u00dfen", "sp\u00fclt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "NN", "PPER", "PTKZU", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erfreut ihn nur, wenn er sich dessen w\u00fcrdig f\u00fchlt.", "tokens": ["Er\u00b7freut", "ihn", "nur", ",", "wenn", "er", "sich", "des\u00b7sen", "w\u00fcr\u00b7dig", "f\u00fchlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PRF", "PDS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}