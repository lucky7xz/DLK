{"textgrid.poem.25152": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es fl\u00fcstert rings zum Strand in Ewigkeit,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es fl\u00fcstert rings zum Strand in Ewigkeit,", "tokens": ["Es", "fl\u00fcs\u00b7tert", "rings", "zum", "Strand", "in", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcllt flutend zwanzigtausend Grotten an,", "tokens": ["F\u00fcllt", "flu\u00b7tend", "zwan\u00b7zig\u00b7tau\u00b7send", "Grot\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis ihnen Hekate mit Zauberbann", "tokens": ["Bis", "ih\u00b7nen", "He\u00b7ka\u00b7te", "mit", "Zau\u00b7ber\u00b7bann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "NE", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wieder den alten dunklen Klang verleiht.", "tokens": ["Wie\u00b7der", "den", "al\u00b7ten", "dunk\u00b7len", "Klang", "ver\u00b7leiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Oft ist es von so sanfter Heiterkeit,", "tokens": ["Oft", "ist", "es", "von", "so", "sanf\u00b7ter", "Hei\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df allerkleinste Muschel ruhen kann,", "tokens": ["Da\u00df", "al\u00b7ler\u00b7kleins\u00b7te", "Mu\u00b7schel", "ru\u00b7hen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo sie dem lauten Wogenbraus entrann", "tokens": ["Wo", "sie", "dem", "lau\u00b7ten", "Wo\u00b7gen\u00b7braus", "ent\u00b7rann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nach letztem wildentbranntem Wetterstreit.", "tokens": ["Nach", "letz\u00b7tem", "wil\u00b7dent\u00b7brann\u00b7tem", "Wet\u00b7ter\u00b7streit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr, deren Augen brennend oder matt,", "tokens": ["Ihr", ",", "de\u00b7ren", "Au\u00b7gen", "bren\u00b7nend", "o\u00b7der", "matt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erg\u00f6tzt sie wieder auf der weiten Flut!", "tokens": ["Er\u00b7g\u00f6tzt", "sie", "wie\u00b7der", "auf", "der", "wei\u00b7ten", "Flut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr, deren Ohren taub vom rohen Spotte", "tokens": ["Ihr", ",", "de\u00b7ren", "Oh\u00b7ren", "taub", "vom", "ro\u00b7hen", "Spot\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELAT", "NN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.4": {"line.1": {"text": "Oder von Melodieen \u00fcbersatt,", "tokens": ["O\u00b7der", "von", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", "\u00fc\u00b7ber\u00b7satt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Sitzt nah dem Meer und h\u00f6rt in Traumesglut", "tokens": ["Sitzt", "nah", "dem", "Meer", "und", "h\u00f6rt", "in", "Trau\u00b7mes\u00b7glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Sang des Nymphenchors aus aller Grotte!", "tokens": ["Den", "Sang", "des", "Nym\u00b7phen\u00b7chors", "aus", "al\u00b7ler", "Grot\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}