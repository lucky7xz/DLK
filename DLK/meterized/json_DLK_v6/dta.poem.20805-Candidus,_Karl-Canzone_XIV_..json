{"dta.poem.20805": {"metadata": {"author": {"name": "Candidus, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Canzone   XIV .", "genre": "Epos", "period": "N.A.", "pub_year": "1854", "urn": "urn:nbn:de:kobv:b4-20090519783", "language": ["de:0.99"], "booktitle": "Candidus, Karl: Der deutsche Christus. F\u00fcnfzehn Canzonen. Leipzig, 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Wie heult es herzzerschneidend durch die L\u00fcfte!", "tokens": ["Wie", "heult", "es", "herz\u00b7zer\u00b7schnei\u00b7dend", "durch", "die", "L\u00fcf\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie tost es durch den Wald! Von jenen B\u00e4umen,", "tokens": ["Wie", "tost", "es", "durch", "den", "Wald", "!", "Von", "je\u00b7nen", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den stattlich hohen, st\u00f6hnt gewalt'ges St\u00f6hnen.", "tokens": ["Den", "statt\u00b7lich", "ho\u00b7hen", ",", "st\u00f6hnt", "ge\u00b7walt'\u00b7ges", "St\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "$,", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es wirft der Sturm des Wassersturzes Sch\u00e4umen", "tokens": ["Es", "wirft", "der", "Sturm", "des", "Was\u00b7ser\u00b7stur\u00b7zes", "Sch\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und zerrt und l\u00e4\u00dft ihn rasen in die Kl\u00fcfte.", "tokens": ["Und", "zerrt", "und", "l\u00e4\u00dft", "ihn", "ra\u00b7sen", "in", "die", "Kl\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie leuchtet neustets vor dem Donnerdr\u00f6hnen", "tokens": ["Wie", "leuch\u00b7tet", "neus\u00b7tets", "vor", "dem", "Don\u00b7ner\u00b7dr\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Fels im pr\u00e4chtig sch\u00f6nen", "tokens": ["Der", "Fels", "im", "pr\u00e4ch\u00b7tig", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Blitzglanz! Verkehrt in Nacht ist Tag. Es fallen", "tokens": ["Blitz\u00b7glanz", "!", "Ver\u00b7kehrt", "in", "Nacht", "ist", "Tag", ".", "Es", "fal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "VVFIN", "APPR", "NN", "VAFIN", "NN", "$.", "PPER", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Die Tropfen st\u00e4rker, doch mich schirmt, o Buche!", "tokens": ["Die", "Trop\u00b7fen", "st\u00e4r\u00b7ker", ",", "doch", "mich", "schirmt", ",", "o", "Bu\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "KON", "PPER", "VVFIN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Dein Laubdach. Keinem Fluche", "tokens": ["Dein", "Laub\u00b7dach", ".", "Kei\u00b7nem", "Flu\u00b7che"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Mehr bebt dies Herz wenn wilde Stimmen schallen.", "tokens": ["Mehr", "bebt", "dies", "Herz", "wenn", "wil\u00b7de", "Stim\u00b7men", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PDS", "NN", "KOUS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Du hast ganz andre St\u00fcrme mir geleget,", "tokens": ["Du", "hast", "ganz", "and\u00b7re", "St\u00fcr\u00b7me", "mir", "ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "O Herr! und sie auch hatte Huld erreget.", "tokens": ["O", "Herr", "!", "und", "sie", "auch", "hat\u00b7te", "Huld", "er\u00b7re\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "KON", "PPER", "ADV", "VAFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bestimmungsloses F\u00fchlen ist kein F\u00fchlen.", "tokens": ["Be\u00b7stim\u00b7mungs\u00b7lo\u00b7ses", "F\u00fch\u00b7len", "ist", "kein", "F\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Schmerz und Lust nur ist Gef\u00fchl entfaltet.", "tokens": ["In", "Schmerz", "und", "Lust", "nur", "ist", "Ge\u00b7f\u00fchl", "ent\u00b7fal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VAFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drum spannt das ", "tokens": ["Drum", "spannt", "das"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Als Widerspruch und Unortnung gestaltet.", "tokens": ["Als", "Wi\u00b7der\u00b7spruch", "und", "Un\u00b7ort\u00b7nung", "ge\u00b7stal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Und Wahn und L\u00fcge, Ha\u00df und Tod durchw\u00fchlen", "tokens": ["Und", "Wahn", "und", "L\u00fc\u00b7ge", ",", "Ha\u00df", "und", "Tod", "durch\u00b7w\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Welt. Doch sieh! mit Finsterni\u00df versticht sich", "tokens": ["Die", "Welt", ".", "Doch", "sieh", "!", "mit", "Fins\u00b7ter\u00b7ni\u00df", "ver\u00b7sticht", "sich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "VVIMP", "$.", "APPR", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und zeigt der Liebe Licht sich", "tokens": ["Und", "zeigt", "der", "Lie\u00b7be", "Licht", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PRF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Urstets, der Liebe Leben, denn begr\u00fcndet", "tokens": ["Urs\u00b7tets", ",", "der", "Lie\u00b7be", "Le\u00b7ben", ",", "denn", "be\u00b7gr\u00fcn\u00b7det"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "NN", "$,", "KON", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "In ew'ger Einheit nur ist ew'ges Brechen.", "tokens": ["In", "ew'\u00b7ger", "Ein\u00b7heit", "nur", "ist", "ew'\u00b7ges", "Bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Da mu\u00df der Bruch sich r\u00e4chen", "tokens": ["Da", "mu\u00df", "der", "Bruch", "sich", "r\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Durch Sehnsuchtsweh, und sel'ges Heil verb\u00fcndet", "tokens": ["Durch", "Sehn\u00b7suchts\u00b7weh", ",", "und", "sel'\u00b7ges", "Heil", "ver\u00b7b\u00fcn\u00b7det"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Treu mit Vers\u00f6hnung sich und stiller Friede.", "tokens": ["Treu", "mit", "Ver\u00b7s\u00f6h\u00b7nung", "sich", "und", "stil\u00b7ler", "Frie\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PRF", "KON", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.13": {"text": "Drob preist, o Herr! auch ", "tokens": ["Drob", "preist", ",", "o", "Herr", "!", "auch"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "FM", "NN", "$.", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Und wird nun wer gefangen war wol sagen:", "tokens": ["Und", "wird", "nun", "wer", "ge\u00b7fan\u00b7gen", "war", "wol", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PWS", "ADJD", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gebt nochmals Ketten da\u00df ich freier werde?", "tokens": ["Gebt", "noch\u00b7mals", "Ket\u00b7ten", "da\u00df", "ich", "frei\u00b7er", "wer\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "NN", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wer wird sprechen: la\u00dft uns mehr erkranken", "tokens": ["Und", "wer", "wird", "spre\u00b7chen", ":", "la\u00dft", "uns", "mehr", "er\u00b7kran\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VAFIN", "VVINF", "$.", "VVIMP", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um besser zu gesunden? Mensch von Erde!", "tokens": ["Um", "bes\u00b7ser", "zu", "ge\u00b7sun\u00b7den", "?", "Mensch", "von", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PTKZU", "VVINF", "$.", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie sollten mit der S\u00fcnde wir uns tragen", "tokens": ["Wie", "soll\u00b7ten", "mit", "der", "S\u00fcn\u00b7de", "wir", "uns", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "APPR", "ART", "NN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der wir gestorben sind, da in Gedanken", "tokens": ["Der", "wir", "ge\u00b7stor\u00b7ben", "sind", ",", "da", "in", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mit sterbensvollem Wanken", "tokens": ["Mit", "ster\u00b7bens\u00b7vol\u00b7lem", "Wan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der alte Mensch in uns zum Tod begleitet", "tokens": ["Der", "al\u00b7te", "Mensch", "in", "uns", "zum", "Tod", "be\u00b7glei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Den Herrn vom Himmel, der in uns nun lebet?", "tokens": ["Den", "Herrn", "vom", "Him\u00b7mel", ",", "der", "in", "uns", "nun", "le\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "PRELS", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wenn's von den Lippen bebet:", "tokens": ["Wenn's", "von", "den", "Lip\u00b7pen", "be\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.11": {"text": "\u201e", "tokens": ["\u201e"], "token_info": ["punct"], "pos": ["$("]}, "line.12": {"text": "Vers\u00f6hnt ist Alles. Von der S\u00fcnde Schranken,", "tokens": ["Ver\u00b7s\u00f6hnt", "ist", "Al\u00b7les", ".", "Von", "der", "S\u00fcn\u00b7de", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "$.", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Nun sie zertr\u00fcmmert, aufweint br\u00fcnstig Danken.", "tokens": ["Nun", "sie", "zer\u00b7tr\u00fcm\u00b7mert", ",", "auf\u00b7weint", "br\u00fcns\u00b7tig", "Dan\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "$,", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hier haben keine Stimme, die am Satze", "tokens": ["Hier", "ha\u00b7ben", "kei\u00b7ne", "Stim\u00b7me", ",", "die", "am", "Sat\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Widerspruches abzugweise hangen", "tokens": ["Des", "Wi\u00b7der\u00b7spru\u00b7ches", "ab\u00b7zu\u00b7gwei\u00b7se", "han\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und deren Denkthat wirklich noch im Grauen", "tokens": ["Und", "de\u00b7ren", "Denkt\u00b7hat", "wirk\u00b7lich", "noch", "im", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRELAT", "NN", "ADJD", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Bruches und des Widerspruchs befangen.", "tokens": ["Des", "Bru\u00b7ches", "und", "des", "Wi\u00b7der\u00b7spruchs", "be\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit Recht bezeugen sie die Teufelsfratze", "tokens": ["Mit", "Recht", "be\u00b7zeu\u00b7gen", "sie", "die", "Teu\u00b7fels\u00b7frat\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die sie als Hinterhaubt des Guten schauen.", "tokens": ["Die", "sie", "als", "Hin\u00b7ter\u00b7haubt", "des", "Gu\u00b7ten", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich m\u00f6cht' im bl\u00e4ulich grauen", "tokens": ["Ich", "m\u00f6cht'", "im", "bl\u00e4u\u00b7lich", "grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "ADJD", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Helldunkel eines Tempels lieber sehen", "tokens": ["Hell\u00b7dun\u00b7kel", "ei\u00b7nes", "Tem\u00b7pels", "lie\u00b7ber", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ein J\u00fcnglingsantlitz qualverst\u00f6rt und trotzig,", "tokens": ["Ein", "J\u00fcng\u00b7lings\u00b7ant\u00b7litz", "qual\u00b7ver\u00b7st\u00f6rt", "und", "trot\u00b7zig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Selbst etwas l\u00e4ppisch-protzig,", "tokens": ["Selbst", "et\u00b7was", "l\u00e4p\u00b7pischprot\u00b7zig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Doch immer gotthaft, w\u00e4hrend Heilsergehen", "tokens": ["Doch", "im\u00b7mer", "got\u00b7thaft", ",", "w\u00e4h\u00b7rend", "Heil\u00b7ser\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ADJD", "$,", "KOUS",