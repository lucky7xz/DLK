{"textgrid.poem.39123": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn die W\u00e4lder tief verstummen,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die W\u00e4lder tief verstummen,", "tokens": ["Wenn", "die", "W\u00e4l\u00b7der", "tief", "ver\u00b7stum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich der Himmel weitet, breitet,", "tokens": ["Sich", "der", "Him\u00b7mel", "wei\u00b7tet", ",", "brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch das Blau mit leisem Summen", "tokens": ["Durch", "das", "Blau", "mit", "lei\u00b7sem", "Sum\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht ein einzig L\u00fcftchen gleitet;", "tokens": ["Nicht", "ein", "ein\u00b7zig", "L\u00fcft\u00b7chen", "glei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn die Str\u00f6me schweigend rollen", "tokens": ["Wenn", "die", "Str\u00f6\u00b7me", "schwei\u00b7gend", "rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Sturm die Renner z\u00fcgelt,", "tokens": ["Und", "der", "Sturm", "die", "Ren\u00b7ner", "z\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Auf dem See, dem ruhevollen,", "tokens": ["Auf", "dem", "See", ",", "dem", "ru\u00b7he\u00b7vol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht ein Hauch die Welle h\u00fcgelt:", "tokens": ["Nicht", "ein", "Hauch", "die", "Wel\u00b7le", "h\u00fc\u00b7gelt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00f6rt man wohl beim Abendschillern", "tokens": ["H\u00f6rt", "man", "wohl", "beim", "A\u00b7bend\u00b7schil\u00b7lern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Turteltauben Seufzer tauschen,", "tokens": ["Tur\u00b7tel\u00b7tau\u00b7ben", "Seuf\u00b7zer", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rt man wohl die Lerche trillern", "tokens": ["H\u00f6rt", "man", "wohl", "die", "Ler\u00b7che", "tril\u00b7lern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das B\u00e4chlein schwatzend rauschen.", "tokens": ["Und", "das", "B\u00e4ch\u00b7lein", "schwat\u00b7zend", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn in zornigem Erzittern", "tokens": ["Wenn", "in", "zor\u00b7ni\u00b7gem", "Er\u00b7zit\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich im Kampf die \u00c4ste schlagen,", "tokens": ["Sich", "im", "Kampf", "die", "\u00c4s\u00b7te", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch das Blau in Schlachtgewittern", "tokens": ["Durch", "das", "Blau", "in", "Schlacht\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Donnerwolkenheere jagen;", "tokens": ["Don\u00b7ner\u00b7wol\u00b7ken\u00b7hee\u00b7re", "ja\u00b7gen", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn der Stromschu\u00df jach hereinbraust", "tokens": ["Wenn", "der", "Strom\u00b7schu\u00df", "jach", "her\u00b7ein\u00b7braust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN"], "meter": "--+-++-+", "measure": "anapaest.init"}, "line.2": {"text": "Und das Sturmro\u00df schnaubt im Zorne,", "tokens": ["Und", "das", "Sturm\u00b7ro\u00df", "schnaubt", "im", "Zor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hoch die Welle ans Gestein braust", "tokens": ["Hoch", "die", "Wel\u00b7le", "ans", "Ge\u00b7stein", "braust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus des Seees Strudelborne:", "tokens": ["Aus", "des", "See\u00b7es", "Stru\u00b7del\u00b7bor\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schweige dann, du Westess\u00e4useln,", "tokens": ["Schwei\u00b7ge", "dann", ",", "du", "Wes\u00b7tes\u00b7s\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lerchenschwirren, Taubengirren;", "tokens": ["Ler\u00b7chen\u00b7schwir\u00b7ren", ",", "Tau\u00b7ben\u00b7gir\u00b7ren", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6re, B\u00e4chlein, auf zu kr\u00e4useln", "tokens": ["H\u00f6\u00b7re", ",", "B\u00e4ch\u00b7lein", ",", "auf", "zu", "kr\u00e4u\u00b7seln"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "APPR", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und durch Rosen hinzuirren.", "tokens": ["Und", "durch", "Ro\u00b7sen", "hin\u00b7zu\u00b7ir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn mit Macht die Adern kochen", "tokens": ["Wenn", "mit", "Macht", "die", "A\u00b7dern", "ko\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und im Sturm die Triebe streiten:", "tokens": ["Und", "im", "Sturm", "die", "Trie\u00b7be", "strei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schweige dann, du leises Pochen", "tokens": ["Schwei\u00b7ge", "dann", ",", "du", "lei\u00b7ses", "Po\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebeszarter Seligkeiten.", "tokens": ["Lie\u00b7bes\u00b7zar\u00b7ter", "Se\u00b7lig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Tr\u00e4nen ihr, ihr s\u00fc\u00dfen, bittern,", "tokens": ["Tr\u00e4\u00b7nen", "ihr", ",", "ihr", "s\u00fc\u00b7\u00dfen", ",", "bit\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VVFIN", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00dft euch stillen, fl\u00fcssig laue,", "tokens": ["La\u00dft", "euch", "stil\u00b7len", ",", "fl\u00fcs\u00b7sig", "lau\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Blitze Felsen splittern,", "tokens": ["Wenn", "die", "Blit\u00b7ze", "Fel\u00b7sen", "split\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist's nicht Zeit zum Morgentaue.", "tokens": ["Ist's", "nicht", "Zeit", "zum", "Mor\u00b7gen\u00b7tau\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}