{"textgrid.poem.31349": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die gef\u00e4hrliche Aufmunterung", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Witzig nennst du mein Epigramm? Nun, weil du so sch\u00f6n doch", "tokens": ["Wit\u00b7zig", "nennst", "du", "mein", "E\u00b7pig\u00b7ramm", "?", "Nun", ",", "weil", "du", "so", "sch\u00f6n", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "ADV", "$,", "KOUS", "PPER", "ADV", "ADJD", "ADV"], "meter": "+-+--+-+-+-++-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Auf mich munterst, vernimm denn eine Probe auf dich.", "tokens": ["Auf", "mich", "mun\u00b7terst", ",", "ver\u00b7nimm", "denn", "ei\u00b7ne", "Pro\u00b7be", "auf", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "VVIMP", "ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Schauet ihn an! Da steht er und ficht und st\u00f6\u00dfet den L\u00fcften", "tokens": ["Schau\u00b7et", "ihn", "an", "!", "Da", "steht", "er", "und", "ficht", "und", "st\u00f6\u00b7\u00dfet", "den", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "KON", "ADJD", "KON", "VVFIN", "ART", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Quarten und Terzen durchs Herz jubelt und meint, er trifft ", "tokens": ["Quar\u00b7ten", "und", "Ter\u00b7zen", "durchs", "Herz", "ju\u00b7belt", "und", "meint", ",", "er", "trifft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wie er hei\u00dfet? Ihr fragt mich zuviel. Einen Namen zwar, glaub ich,", "tokens": ["Wie", "er", "hei\u00b7\u00dfet", "?", "Ihr", "fragt", "mich", "zu\u00b7viel", ".", "Ei\u00b7nen", "Na\u00b7men", "zwar", ",", "glaub", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "VVFIN", "$.", "ART", "NN", "ADV", "$,", "VVFIN", "PPER", "$,"], "meter": "--+--+--++-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Gab ihm der Vater: der Ruhm? Davon verlautete nichts.", "tokens": ["Gab", "ihm", "der", "Va\u00b7ter", ":", "der", "Ruhm", "?", "Da\u00b7von", "ver\u00b7lau\u00b7te\u00b7te", "nichts", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "ART", "NN", "$.", "PAV", "VVFIN", "PIS", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}}}}