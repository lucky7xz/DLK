{"textgrid.poem.64223": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Ordensmeister Hermann Balk baut die erste deutsche Warte auf der Heideneiche", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hieher, Genossen, in Sumpf und Wald!", "tokens": ["Hie\u00b7her", ",", "Ge\u00b7nos\u00b7sen", ",", "in", "Sumpf", "und", "Wald", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Noch W\u00fcste \u2013: deutsches Markland bald!", "tokens": ["Noch", "W\u00fcs\u00b7te", "\u2013", ":", "deut\u00b7sches", "Mar\u00b7kland", "bald", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "$.", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus Ried und R\u00f6hricht ragt empor", "tokens": ["Aus", "Ried", "und", "R\u00f6h\u00b7richt", "ragt", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Heideneiche: kurz zuvor", "tokens": ["Die", "Hei\u00b7de\u00b7nei\u00b7che", ":", "kurz", "zu\u00b7vor"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trank Ro\u00dfblut hier noch Gott Perkun:", "tokens": ["Trank", "Ro\u00df\u00b7blut", "hier", "noch", "Gott", "Per\u00b7kun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "ADV", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch deutsche Baumburg ward sie nun.", "tokens": ["Doch", "deut\u00b7sche", "Baum\u00b7burg", "ward", "sie", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Pflanzt unser Banner auf den Wipfel:", "tokens": ["Pflanzt", "un\u00b7ser", "Ban\u00b7ner", "auf", "den", "Wip\u00b7fel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Stolz wall' es \u00fcber alle Gipfel", "tokens": ["Stolz", "wall'", "es", "\u00fc\u00b7ber", "al\u00b7le", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und schaue k\u00fchn von hoher Wart,", "tokens": ["Und", "schau\u00b7e", "k\u00fchn", "von", "ho\u00b7her", "Wart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Von Gedanum bis Memelgard:", "tokens": ["Von", "Ge\u00b7da\u00b7num", "bis", "Me\u00b7mel\u00b7gard", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.11": {"text": "Hier tr\u00e4gt mit Rauschen unser Zeichen", "tokens": ["Hier", "tr\u00e4gt", "mit", "Rau\u00b7schen", "un\u00b7ser", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ein Fahnentr\u00e4ger sondergleichen:", "tokens": ["Ein", "Fah\u00b7nen\u00b7tr\u00e4\u00b7ger", "son\u00b7derg\u00b7lei\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "\u2013 Nie kann er Fu\u00dfbreit r\u00fcckw\u00e4rts weichen \u2013!", "tokens": ["\u2013", "Nie", "kann", "er", "Fu\u00df\u00b7breit", "r\u00fcck\u00b7w\u00e4rts", "wei\u00b7chen", "\u2013", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "NN", "ADV", "VVINF", "$(", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und ob der Pole sp\u00f6ttisch h\u00f6hnt,", "tokens": ["Und", "ob", "der", "Po\u00b7le", "sp\u00f6t\u00b7tisch", "h\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da\u00df wir wie V\u00f6gel sind gew\u00f6hnt,", "tokens": ["Da\u00df", "wir", "wie", "V\u00f6\u00b7gel", "sind", "ge\u00b7w\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die auf den B\u00e4umen bau'n ihr Nest: \u2013", "tokens": ["Die", "auf", "den", "B\u00e4u\u00b7men", "bau'n", "ihr", "Nest", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Baut ihr nur weiter, still und fest!", "tokens": ["Baut", "ihr", "nur", "wei\u00b7ter", ",", "still", "und", "fest", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "ADJD", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Bald wird's den Feinden schrecklich klar,", "tokens": ["Bald", "wird's", "den", "Fein\u00b7den", "schreck\u00b7lich", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Von welcher Art der Vogel war:", "tokens": ["Von", "wel\u00b7cher", "Art", "der", "Vo\u00b7gel", "war", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ART", "NE", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Der Vogel auf der Preu\u00dfeneiche \u2013", "tokens": ["Der", "Vo\u00b7gel", "auf", "der", "Preu\u00b7\u00dfen\u00b7ei\u00b7che", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Er baut den Adlerhorst dem Reiche!", "tokens": ["Er", "baut", "den", "Ad\u00b7ler\u00b7horst", "dem", "Rei\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}