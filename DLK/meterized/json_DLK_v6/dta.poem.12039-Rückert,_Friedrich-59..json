{"dta.poem.12039": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "59.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Aergste drohet nicht der Welt von Geld und Gut,", "tokens": ["Das", "A\u00b7ergs\u00b7te", "dro\u00b7het", "nicht", "der", "Welt", "von", "Geld", "und", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ART", "NN", "APPR", "NN", "KON", "ADJD", "$,"], "meter": "-++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Wo nur der Einzelne daf\u00fcr Unw\u00fcrd'ges thut.", "tokens": ["Wo", "nur", "der", "Ein\u00b7zel\u00b7ne", "da\u00b7f\u00fcr", "Un\u00b7w\u00fcrd'\u00b7ges", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "PAV", "NN", "VVFIN", "$."], "meter": "-+-++-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Das Aergste drohet da, wo es soweit gekommen,", "tokens": ["Das", "A\u00b7ergs\u00b7te", "dro\u00b7het", "da", ",", "wo", "es", "so\u00b7weit", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df es zum Ma\u00dfstab wird f\u00fcr jeden Werth genommen.", "tokens": ["Da\u00df", "es", "zum", "Ma\u00df\u00b7stab", "wird", "f\u00fcr", "je\u00b7den", "Werth", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O danke Gott, da\u00df du in einem Winkel stehst,", "tokens": ["O", "dan\u00b7ke", "Gott", ",", "da\u00df", "du", "in", "ei\u00b7nem", "Win\u00b7kel", "stehst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo dieser schrecklichsten Versuchung du entgehst,", "tokens": ["Wo", "die\u00b7ser", "schreck\u00b7lichs\u00b7ten", "Ver\u00b7su\u00b7chung", "du", "ent\u00b7gehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wo jeder zwar f\u00fcr sich nach eitlen G\u00fctern trachtet,", "tokens": ["Wo", "je\u00b7der", "zwar", "f\u00fcr", "sich", "nach", "eit\u00b7len", "G\u00fc\u00b7tern", "trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPR", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch der verachtet noch nicht ist, der sie verachtet.", "tokens": ["Doch", "der", "ver\u00b7ach\u00b7tet", "noch", "nicht", "ist", ",", "der", "sie", "ver\u00b7ach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "ADV", "PTKNEG", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}