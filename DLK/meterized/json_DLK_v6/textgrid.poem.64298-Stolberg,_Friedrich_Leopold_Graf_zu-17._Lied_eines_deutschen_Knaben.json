{"textgrid.poem.64298": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "17. Lied eines deutschen Knaben", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Arm wird stark, und gro\u00df mein Mut;", "tokens": ["Mein", "Arm", "wird", "stark", ",", "und", "gro\u00df", "mein", "Mut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gieb, Vater, mir ein Schwert!", "tokens": ["Gieb", ",", "Va\u00b7ter", ",", "mir", "ein", "Schwert", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verachte nicht mein junges Blut!", "tokens": ["Ver\u00b7ach\u00b7te", "nicht", "mein", "jun\u00b7ges", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bin der V\u00e4ter wert!", "tokens": ["Ich", "bin", "der", "V\u00e4\u00b7ter", "wert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich finde f\u00fcrder keine Ruh", "tokens": ["Ich", "fin\u00b7de", "f\u00fcr\u00b7der", "kei\u00b7ne", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im weichen Knabenstand!", "tokens": ["Im", "wei\u00b7chen", "Kna\u00b7ben\u00b7stand", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich st\u00fcrb', o Vater, stolz wie du,", "tokens": ["Ich", "st\u00fcrb'", ",", "o", "Va\u00b7ter", ",", "stolz", "wie", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$,", "ADJD", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Tod f\u00fcrs Vaterland!", "tokens": ["Den", "Tod", "f\u00fcrs", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Schon fr\u00fch in meiner Jugend war", "tokens": ["Schon", "fr\u00fch", "in", "mei\u00b7ner", "Ju\u00b7gend", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein t\u00e4glich Spiel der Krieg;", "tokens": ["Mein", "t\u00e4g\u00b7lich", "Spiel", "der", "Krieg", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Bette tr\u00e4umt' ich nur Gefahr,", "tokens": ["Im", "Bet\u00b7te", "tr\u00e4umt'", "ich", "nur", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Wunden nur und Sieg.", "tokens": ["Und", "Wun\u00b7den", "nur", "und", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mein Feldgeschrei erweckte mich", "tokens": ["Mein", "Feld\u00b7ge\u00b7schrei", "er\u00b7weck\u00b7te", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus mancher T\u00fcrkenschlacht;", "tokens": ["Aus", "man\u00b7cher", "T\u00fcr\u00b7ken\u00b7schlacht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch j\u00fcngst ein Faustschlag, welchen ich", "tokens": ["Noch", "j\u00fcngst", "ein", "Faust\u00b7schlag", ",", "wel\u00b7chen", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "PWAT", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Bassa zugedacht.", "tokens": ["Dem", "Bas\u00b7sa", "zu\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da neulich unsrer Krieger Schar", "tokens": ["Da", "neu\u00b7lich", "uns\u00b7rer", "Krie\u00b7ger", "Schar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf dieser Stra\u00dfe zog,", "tokens": ["Auf", "die\u00b7ser", "Stra\u00b7\u00dfe", "zog", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, wie ein Vogel, der Husar", "tokens": ["Und", ",", "wie", "ein", "Vo\u00b7gel", ",", "der", "Hu\u00b7sar"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "PWAV", "ART", "NE", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Haus vor\u00fcberflog:", "tokens": ["Das", "Haus", "vor\u00b7\u00fc\u00b7berf\u00b7log", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da gaffte starr, und freute sich", "tokens": ["Da", "gaff\u00b7te", "starr", ",", "und", "freu\u00b7te", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "KON", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Knaben froher Schwarm;", "tokens": ["Der", "Kna\u00b7ben", "fro\u00b7her", "Schwarm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich aber, Vater, h\u00e4rmte mich,", "tokens": ["Ich", "a\u00b7ber", ",", "Va\u00b7ter", ",", "h\u00e4rm\u00b7te", "mich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und pr\u00fcfte meinen Arm.", "tokens": ["Und", "pr\u00fcf\u00b7te", "mei\u00b7nen", "Arm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mein Arm wird stark, und gro\u00df mein Mut;", "tokens": ["Mein", "Arm", "wird", "stark", ",", "und", "gro\u00df", "mein", "Mut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gieb, Vater, mir ein Schwert!", "tokens": ["Gieb", ",", "Va\u00b7ter", ",", "mir", "ein", "Schwert", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verachte nicht mein junges Blut!", "tokens": ["Ver\u00b7ach\u00b7te", "nicht", "mein", "jun\u00b7ges", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bin der V\u00e4ter wert!", "tokens": ["Ich", "bin", "der", "V\u00e4\u00b7ter", "wert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}