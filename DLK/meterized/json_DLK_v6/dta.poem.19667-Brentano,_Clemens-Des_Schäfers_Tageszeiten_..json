{"dta.poem.19667": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Des Sch\u00e4fers Tageszeiten .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! wie sanft ruh ich hie               ", "tokens": ["Ach", "!", "wie", "sanft", "ruh", "ich", "hie"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "ADJD", "VVFIN", "PPER", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Bei meinem Vieh!", "tokens": ["Bei", "mei\u00b7nem", "Vieh", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Da schlaf ich s\u00fc\u00df im Moos,", "tokens": ["Da", "schlaf", "ich", "s\u00fc\u00df", "im", "Moos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dem Gl\u00fccke in dem Schoos,", "tokens": ["Dem", "Gl\u00fc\u00b7cke", "in", "dem", "Schoos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ganz sorgenlos.", "tokens": ["Ganz", "sor\u00b7gen\u00b7los", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Wenn ich die pr\u00e4chtigen Schl\u00f6sser beschau", "tokens": ["Wenn", "ich", "die", "pr\u00e4ch\u00b7ti\u00b7gen", "Schl\u00f6s\u00b7ser", "be\u00b7schau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sind sie doch nur mir,", "tokens": ["Sind", "sie", "doch", "nur", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "So zu sagen schier", "tokens": ["So", "zu", "sa\u00b7gen", "schier"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Ein k\u00fchler Thau.", "tokens": ["Ein", "k\u00fch\u00b7ler", "Thau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Kommt denn das Morgenroth,", "tokens": ["Kommt", "denn", "das", "Mor\u00b7gen\u00b7roth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So lob ich Gott.", "tokens": ["So", "lob", "ich", "Gott", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dann mit der Feldschallmey", "tokens": ["Dann", "mit", "der", "Feld\u00b7schall\u00b7mey"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Ruf ich das L\u00e4mmerg'schrey", "tokens": ["Ruf", "ich", "das", "L\u00e4m\u00b7mer\u00b7g'\u00b7schrey"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ganz nah herbey;", "tokens": ["Ganz", "nah", "her\u00b7bey", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Da ist kein Seufzen, kein trauriger Ton;", "tokens": ["Da", "ist", "kein", "Seuf\u00b7zen", ",", "kein", "trau\u00b7ri\u00b7ger", "Ton", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Denn die Morgenstund", "tokens": ["Denn", "die", "Mor\u00b7gen\u00b7stund"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "F\u00fchret Gold im Mund,", "tokens": ["F\u00fch\u00b7ret", "Gold", "im", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Baut mir ein'n Thron.", "tokens": ["Baut", "mir", "ein'n", "Thron", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Kommt dann die Mittagszeit,", "tokens": ["Kommt", "dann", "die", "Mit\u00b7tags\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bin ich voll Freud;", "tokens": ["Bin", "ich", "voll", "Freud", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Da grast das liebe Vieh,", "tokens": ["Da", "grast", "das", "lie\u00b7be", "Vieh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gei\u00df, L\u00e4mmer, Schaaf und K\u00fch,", "tokens": ["Gei\u00df", ",", "L\u00e4m\u00b7mer", ",", "Schaaf", "und", "K\u00fch", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auf gr\u00fcner Haid.", "tokens": ["Auf", "gr\u00fc\u00b7ner", "Haid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Setz' mich in Schatten hin, esse mein Brod.", "tokens": ["Setz'", "mich", "in", "Schat\u00b7ten", "hin", ",", "es\u00b7se", "mein", "Brod", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bey meinem Hirtenstab", "tokens": ["Bey", "mei\u00b7nem", "Hir\u00b7ten\u00b7stab"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Schw\u00f6r ich, da\u00df ich hab", "tokens": ["Schw\u00f6r", "ich", ",", "da\u00df", "ich", "hab"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPER", "$,", "KOUS", "PPER", "VAFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Niemals ein Noth.", "tokens": ["Nie\u00b7mals", "ein", "Noth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Endlich seh ich von fern", "tokens": ["End\u00b7lich", "seh", "ich", "von", "fern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJD"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Den Abendstern;", "tokens": ["Den", "A\u00b7bends\u00b7tern", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Dort draus am Wasserfall", "tokens": ["Dort", "draus", "am", "Was\u00b7ser\u00b7fall"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PAV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Schlaget die Nachtigall,", "tokens": ["Schla\u00b7get", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Giebt Wiederhall.", "tokens": ["Giebt", "Wie\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Freyheit in Armuth giebt Reichthum und Sieg,", "tokens": ["Frey\u00b7heit", "in", "Ar\u00b7muth", "giebt", "Reicht\u00b7hum", "und", "Sieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Allem Pomp und Pracht", "tokens": ["Al\u00b7lem", "Pomp", "und", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Sag ich gute Nacht", "tokens": ["Sag", "ich", "gu\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Und bleib ein Hirt.", "tokens": ["Und", "bleib", "ein", "Hirt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}