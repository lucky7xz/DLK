{"textgrid.poem.36886": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Vom Tode", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Lebenszeit verstreicht,", "tokens": ["Mei\u00b7ne", "Le\u00b7bens\u00b7zeit", "ver\u00b7streicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcndlich eil ich zu dem Grabe;", "tokens": ["St\u00fcnd\u00b7lich", "eil", "ich", "zu", "dem", "Gra\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und was ist's, das ich vielleicht,", "tokens": ["Und", "was", "ist's", ",", "das", "ich", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "$,", "PRELS", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das ich noch zu leben habe?", "tokens": ["Das", "ich", "noch", "zu", "le\u00b7ben", "ha\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denk, o Mensch! an deinen Tod.", "tokens": ["Denk", ",", "o", "Mensch", "!", "an", "dei\u00b7nen", "Tod", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$.", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "S\u00e4ume nicht; denn ", "tokens": ["S\u00e4u\u00b7me", "nicht", ";", "denn"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "PTKNEG", "$.", "KON"], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Lebe, wie du, wenn du stirbst,", "tokens": ["Le\u00b7be", ",", "wie", "du", ",", "wenn", "du", "stirbst", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAV", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcnschen wirst, gelebt zu haben.", "tokens": ["W\u00fcn\u00b7schen", "wirst", ",", "ge\u00b7lebt", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "VVPP", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00fcter, die du hier erwirbst,", "tokens": ["G\u00fc\u00b7ter", ",", "die", "du", "hier", "er\u00b7wirbst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcrden, die dir Menschen gaben;", "tokens": ["W\u00fcr\u00b7den", ",", "die", "dir", "Men\u00b7schen", "ga\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nichts wird dich im Tod erfreun;", "tokens": ["Nichts", "wird", "dich", "im", "Tod", "er\u00b7freun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Diese G\u00fcter sind nicht dein.", "tokens": ["Die\u00b7se", "G\u00fc\u00b7ter", "sind", "nicht", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "PTKNEG", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur ein Herz, das Gutes liebt,", "tokens": ["Nur", "ein", "Herz", ",", "das", "Gu\u00b7tes", "liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur ein ruhiges Gewissen,", "tokens": ["Nur", "ein", "ru\u00b7hi\u00b7ges", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das vor Gott dir Zeugnis gibt,", "tokens": ["Das", "vor", "Gott", "dir", "Zeug\u00b7nis", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird dir deinen Tod vers\u00fc\u00dfen;", "tokens": ["Wird", "dir", "dei\u00b7nen", "Tod", "ver\u00b7s\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses Herz, von Gott erneut,", "tokens": ["Die\u00b7ses", "Herz", ",", "von", "Gott", "er\u00b7neut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist des Todes Freudigkeit.", "tokens": ["Ist", "des", "To\u00b7des", "Freu\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn in deiner letzten Not", "tokens": ["Wenn", "in", "dei\u00b7ner", "letz\u00b7ten", "Not"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Freunde h\u00fclflos um dich beben:", "tokens": ["Freun\u00b7de", "h\u00fcl\u00b7flos", "um", "dich", "be\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Dann wird \u00fcber Welt und Tod", "tokens": ["Dann", "wird", "\u00fc\u00b7ber", "Welt", "und", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dich dies reine Herz erheben;", "tokens": ["Dich", "dies", "rei\u00b7ne", "Herz", "er\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dann erschreckt dich kein Gericht;", "tokens": ["Dann", "er\u00b7schreckt", "dich", "kein", "Ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gott ist deine Zuversicht.", "tokens": ["Gott", "ist", "dei\u00b7ne", "Zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Da\u00df du dieses Herz erwirbst,", "tokens": ["Da\u00df", "du", "die\u00b7ses", "Herz", "er\u00b7wirbst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcrchte Gott, und bet und wache.", "tokens": ["F\u00fcrch\u00b7te", "Gott", ",", "und", "bet", "und", "wa\u00b7che", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "KON", "VVFIN", "KON", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sorge nicht, wie fr\u00fch du stirbst;", "tokens": ["Sor\u00b7ge", "nicht", ",", "wie", "fr\u00fch", "du", "stirbst", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Zeit ist Gottes Sache.", "tokens": ["Dei\u00b7ne", "Zeit", "ist", "Got\u00b7tes", "Sa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lern nicht nur den Tod nicht scheun,", "tokens": ["Lern", "nicht", "nur", "den", "Tod", "nicht", "scheun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lern auch seiner dich erfreun.", "tokens": ["Lern", "auch", "sei\u00b7ner", "dich", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00dcberwind ihn durch Vertraun,", "tokens": ["\u00dc\u00b7berw\u00b7ind", "ihn", "durch", "Ver\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprich: Ich wei\u00df, an wen ich gl\u00e4ube,", "tokens": ["Sprich", ":", "Ich", "wei\u00df", ",", "an", "wen", "ich", "gl\u00e4u\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PPER", "VVFIN", "$,", "APPR", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich wei\u00df, ich werd ihn schaun", "tokens": ["Und", "ich", "wei\u00df", ",", "ich", "werd", "ihn", "schaun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst in diesem meinem Leibe.", "tokens": ["Einst", "in", "die\u00b7sem", "mei\u00b7nem", "Lei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Er, der rief: Es ist vollbracht!", "tokens": ["Er", ",", "der", "rief", ":", "Es", "ist", "voll\u00b7bracht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "VVFIN", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nahm dem Tode seine Macht.", "tokens": ["Nahm", "dem", "To\u00b7de", "sei\u00b7ne", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Tritt im Geist zum Grab oft hin,", "tokens": ["Tritt", "im", "Geist", "zum", "Grab", "oft", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "APPRART", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Siehe dein Gebein versenken;", "tokens": ["Sie\u00b7he", "dein", "Ge\u00b7bein", "ver\u00b7sen\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprich: Herr, da\u00df ich Erde bin,", "tokens": ["Sprich", ":", "Herr", ",", "da\u00df", "ich", "Er\u00b7de", "bin", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "NN", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Lehre du mich selbst bedenken;", "tokens": ["Leh\u00b7re", "du", "mich", "selbst", "be\u00b7den\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lehre du mich's jeden Tag,", "tokens": ["Leh\u00b7re", "du", "mich's", "je\u00b7den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ich weiser werden mag!", "tokens": ["Da\u00df", "ich", "wei\u00b7ser", "wer\u00b7den", "mag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}