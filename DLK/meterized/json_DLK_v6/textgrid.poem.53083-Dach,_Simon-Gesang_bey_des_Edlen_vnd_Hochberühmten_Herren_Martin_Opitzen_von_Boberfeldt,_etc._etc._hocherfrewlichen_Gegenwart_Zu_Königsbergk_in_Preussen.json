{"textgrid.poem.53083": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Gesang bey des Edlen vnd Hochber\u00fchmten Herren Martin Opitzen von Boberfeldt, etc. etc. hocherfrewlichen Gegenwart Zu K\u00f6nigsbergk in Preussen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist es vnsrer Seiten Werck'", "tokens": ["Ist", "es", "vns\u00b7rer", "Sei\u00b7ten", "Werck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Je einmahl so wol gelungen,", "tokens": ["Je", "ein\u00b7mahl", "so", "wol", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das wir dir, O K\u00f6nigsbergk,", "tokens": ["Das", "wir", "dir", ",", "O", "K\u00f6\u00b7nigs\u00b7bergk", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "$,", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Etwas gutes vorgesungen,", "tokens": ["Et\u00b7was", "gu\u00b7tes", "vor\u00b7ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "So vernimm auch die\u00df dabey,", "tokens": ["So", "ver\u00b7nimm", "auch", "die\u00df", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PDS", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer desselben Stiffter sey:", "tokens": ["Wer", "des\u00b7sel\u00b7ben", "Stiff\u00b7ter", "sey", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dieser Mann, durch welchen dir", "tokens": ["Die\u00b7ser", "Mann", ",", "durch", "wel\u00b7chen", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "APPR", "PWAT", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzt die Ehre wiederf\u00e4hret,", "tokens": ["Jetzt", "die", "Eh\u00b7re", "wie\u00b7der\u00b7f\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das der Deutschen Prei\u00df vnd Zier", "tokens": ["Das", "der", "Deut\u00b7schen", "Prei\u00df", "vnd", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00e4mptlich bey dir eingekehret,", "tokens": ["S\u00e4mpt\u00b7lich", "bey", "dir", "ein\u00b7ge\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Opitz, den die gantze Welt", "tokens": ["O\u00b7pitz", ",", "den", "die", "gant\u00b7ze", "Welt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "F\u00fcr der Deutschen Wunder helt.", "tokens": ["F\u00fcr", "der", "Deut\u00b7schen", "Wun\u00b7der", "helt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach der Au\u00dfbund vnd Begriff", "tokens": ["Ach", "der", "Au\u00df\u00b7bund", "vnd", "Be\u00b7griff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller hohen Kunst vnd Gaben,", "tokens": ["Al\u00b7ler", "ho\u00b7hen", "Kunst", "vnd", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die der Alten Wei\u00dfheit tieff", "tokens": ["Die", "der", "Al\u00b7ten", "Wei\u00df\u00b7heit", "tieff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihrem Ertz' hat eingegraben,", "tokens": ["Ih\u00b7rem", "Ertz'", "hat", "ein\u00b7ge\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd der lieben Vorfahrt Handt", "tokens": ["Vnd", "der", "lie\u00b7ben", "Vor\u00b7fahrt", "Handt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vns so trewlich zugesandt!", "tokens": ["Vns", "so", "trew\u00b7lich", "zu\u00b7ge\u00b7sandt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Man erschricket, wenn er nun", "tokens": ["Man", "er\u00b7schri\u00b7cket", ",", "wenn", "er", "nun"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner tieff erforschten Sachen", "tokens": ["Sei\u00b7ner", "tieff", "er\u00b7forschten", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Abgrundt anhebt auff-zuthun,", "tokens": ["Ab\u00b7grundt", "an\u00b7hebt", "auf\u00b7fzut\u00b7hun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVIZU", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnd sein Geist beginnt zu wachen,", "tokens": ["Vnd", "sein", "Geist", "be\u00b7ginnt", "zu", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer alsdan Ihn lo\u00df sieht gehn,", "tokens": ["Wer", "als\u00b7dan", "Ihn", "lo\u00df", "sieht", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "ADV", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der sieht Welschlandt vnd Athen.", "tokens": ["Der", "sieht", "Wel\u00b7schlandt", "vnd", "A\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "KON", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Orpheus giebt schon besser Kauff,", "tokens": ["Or\u00b7pheus", "giebt", "schon", "bes\u00b7ser", "Kauff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt er dieses Mannes Seiten,", "tokens": ["H\u00f6rt", "er", "die\u00b7ses", "Man\u00b7nes", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnser Maro horchet auff,", "tokens": ["Vn\u00b7ser", "Ma\u00b7ro", "hor\u00b7chet", "auff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagt: was sol mir das bedeuten?", "tokens": ["Sagt", ":", "was", "sol", "mir", "das", "be\u00b7deu\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PWS", "VMFIN", "PPER", "PDS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird der Weisen Lieder Ruhm", "tokens": ["Wird", "der", "Wei\u00b7sen", "Lie\u00b7der", "Ruhm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nun der Deutschen Eigenthum?", "tokens": ["Nun", "der", "Deut\u00b7schen", "Ei\u00b7gen\u00b7thum", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja, Herr Opitz, ewrer Kunst", "tokens": ["Ja", ",", "Herr", "O\u00b7pitz", ",", "ew\u00b7rer", "Kunst"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "NN", "NE", "$,", "PPOSAT", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Mag es Deutschland einig dancken,", "tokens": ["Mag", "es", "Deutschland", "ei\u00b7nig", "dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NE", "ADJD", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Das der frembden Sprachen Gunst", "tokens": ["Das", "der", "fremb\u00b7den", "Spra\u00b7chen", "Gunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mercklich schon beginnt zu wancken,", "tokens": ["Merck\u00b7lich", "schon", "be\u00b7ginnt", "zu", "wan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd man numehr ins gemein", "tokens": ["Vnd", "man", "nu\u00b7mehr", "ins", "ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieber Deutsch begehrt zu sein.", "tokens": ["Lie\u00b7ber", "Deutsch", "be\u00b7gehrt", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVPP", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wer hat ewrer s\u00fcssen Handt", "tokens": ["Wer", "hat", "ew\u00b7rer", "s\u00fcs\u00b7sen", "Handt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesen Nachdruck mit gegeben,", "tokens": ["Die\u00b7sen", "Nach\u00b7druck", "mit", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df das gantze Norden-Landt,", "tokens": ["Da\u00df", "das", "gant\u00b7ze", "Nor\u00b7den\u00b7Landt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn Ihr schlagt, sich mu\u00df erheben,", "tokens": ["Wenn", "Ihr", "schlagt", ",", "sich", "mu\u00df", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PRF", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd so mancher Edler Geist,", "tokens": ["Vnd", "so", "man\u00b7cher", "Ed\u00b7ler", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Euch zu folgen sich befleist?", "tokens": ["Euch", "zu", "fol\u00b7gen", "sich", "be\u00b7fleist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Last den stoltzen Thracer-Flu\u00df", "tokens": ["Last", "den", "stolt\u00b7zen", "Thra\u00b7cer\u00b7Flu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht so trotzig sich ergiessen,", "tokens": ["Nicht", "so", "trot\u00b7zig", "sich", "er\u00b7gies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd den edlen Mincius", "tokens": ["Vnd", "den", "ed\u00b7len", "Min\u00b7ci\u00b7us"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was bescheidentlicher fliessen,", "tokens": ["Was", "be\u00b7schei\u00b7dent\u00b7li\u00b7cher", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ewres Bobers kleine Fluth", "tokens": ["Ew\u00b7res", "Bo\u00b7bers", "klei\u00b7ne", "Fluth"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimpt doch allen nun den Muth.", "tokens": ["Nimpt", "doch", "al\u00b7len", "nun", "den", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wol euch, Herr! was f\u00fcr ein Lohn", "tokens": ["Wol", "euch", ",", "Herr", "!", "was", "f\u00fcr", "ein", "Lohn"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "NN", "$.", "PWS", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat sich hie mit eingedinget?", "tokens": ["Hat", "sich", "hie", "mit", "ein\u00b7ge\u00b7din\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "APPR", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df von hie-ab ewer Thon", "tokens": ["Da\u00df", "von", "hie\u00b7ab", "e\u00b7wer", "Thon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADV", "PPOSAT", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Bis in jenes Leben dringet,", "tokens": ["Bis", "in", "je\u00b7nes", "Le\u00b7ben", "drin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dessen Nachklangk aller Zeit", "tokens": ["Des\u00b7sen", "Nach\u00b7klangk", "al\u00b7ler", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vnd verg\u00e4ngn\u00fc\u00df sich befreyt.", "tokens": ["Vnd", "ver\u00b7g\u00e4n\u00b7gn\u00fc\u00df", "sich", "be\u00b7freyt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Hie kunt' ewre Jugend zwar", "tokens": ["Hie", "kunt'", "ew\u00b7re", "Ju\u00b7gend", "zwar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADV"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Schon den Lorber-Krantz erjagen,", "tokens": ["Schon", "den", "Lor\u00b7ber\u00b7Krantz", "er\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber dort wird ewer Haar", "tokens": ["A\u00b7ber", "dort", "wird", "e\u00b7wer", "Haar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Erst der Ehren Krohne tragen,", "tokens": ["Erst", "der", "Eh\u00b7ren", "Kroh\u00b7ne", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die euch David gern gesteht,", "tokens": ["Die", "euch", "Da\u00b7vid", "gern", "ge\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NE", "ADV", "VVFIN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Weil Ihr seinen Fu\u00dfpfad geht.", "tokens": ["Weil", "Ihr", "sei\u00b7nen", "Fu\u00df\u00b7pfad", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"li