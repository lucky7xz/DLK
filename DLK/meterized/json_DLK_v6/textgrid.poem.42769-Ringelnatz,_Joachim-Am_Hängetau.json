{"textgrid.poem.42769": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Am H\u00e4ngetau", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das H\u00e4ngetau ist lang und steil.", "tokens": ["Das", "H\u00e4n\u00b7ge\u00b7tau", "ist", "lang", "und", "steil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jedoch die \u00dcbung an dem Seil", "tokens": ["Je\u00b7doch", "die", "\u00dc\u00b7bung", "an", "dem", "Seil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist heilsam und veredelt.", "tokens": ["Ist", "heil\u00b7sam", "und", "ver\u00b7e\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dieweil du kletterst, w\u00e4chst das Tau", "tokens": ["Die\u00b7weil", "du", "klet\u00b7terst", ",", "w\u00e4chst", "das", "Tau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dir hintenraus und wedelt", "tokens": ["Dir", "hin\u00b7ten\u00b7raus", "und", "we\u00b7delt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "A la Wauwau.", "tokens": ["A", "la", "Wau\u00b7wau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM", "FM", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Marie, die unten nach dir blickt,", "tokens": ["Ma\u00b7rie", ",", "die", "un\u00b7ten", "nach", "dir", "blickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kommt mit der Quaste in Konflikt.", "tokens": ["Kommt", "mit", "der", "Quas\u00b7te", "in", "Kon\u00b7flikt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich wette um ein Fa\u00df Gelee:", "tokens": ["Ich", "wet\u00b7te", "um", "ein", "Fa\u00df", "Ge\u00b7lee", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drei Meter \u00fcber der Erden", "tokens": ["Drei", "Me\u00b7ter", "\u00fc\u00b7ber", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Erfa\u00dft dich pl\u00f6tzlich die Idee,", "tokens": ["Er\u00b7fa\u00dft", "dich", "pl\u00f6tz\u00b7lich", "die", "I\u00b7dee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du m\u00f6chtest Seemann werden.", "tokens": ["Du", "m\u00f6ch\u00b7test", "See\u00b7mann", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Kletterschlu\u00df mi\u00dflingt dir freilich.", "tokens": ["Der", "Klet\u00b7ter\u00b7schlu\u00df", "mi\u00df\u00b7lingt", "dir", "frei\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er klingt auch h\u00e4\u00dflich papageilich.", "tokens": ["Er", "klingt", "auch", "h\u00e4\u00df\u00b7lich", "pa\u00b7pa\u00b7gei\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schon dieserhalb und um so mehr", "tokens": ["Schon", "die\u00b7ser\u00b7halb", "und", "um", "so", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "KON", "APPR", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schwankst du verzweifelt hin und her,", "tokens": ["Schwankst", "du", "ver\u00b7zwei\u00b7felt", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als atemloser Pendel.", "tokens": ["Als", "a\u00b7tem\u00b7lo\u00b7ser", "Pen\u00b7del", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und j\u00e4h umgibt dich in der Luft", "tokens": ["Und", "j\u00e4h", "um\u00b7gibt", "dich", "in", "der", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein unartikulierter Duft", "tokens": ["Ein", "un\u00b7ar\u00b7ti\u00b7ku\u00b7lier\u00b7ter", "Duft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Sehr abseits von Lavendel.", "tokens": ["Sehr", "ab\u00b7seits", "von", "La\u00b7ven\u00b7del", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NE", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Und dann erreichst du ganz verzagt", "tokens": ["Und", "dann", "er\u00b7reichst", "du", "ganz", "ver\u00b7zagt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Balken unter Pusten,", "tokens": ["Den", "Bal\u00b7ken", "un\u00b7ter", "Pus\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und weil Marie von unten fragt,", "tokens": ["Und", "weil", "Ma\u00b7rie", "von", "un\u00b7ten", "fragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und weil die Stimme dir versagt,", "tokens": ["Und", "weil", "die", "Stim\u00b7me", "dir", "ver\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So f\u00e4ngst du an zu husten.", "tokens": ["So", "f\u00e4ngst", "du", "an", "zu", "hus\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Dame fr\u00e4gt ob schwindelfrei", "tokens": ["Die", "Da\u00b7me", "fr\u00e4gt", "ob", "schwin\u00b7del\u00b7frei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOUS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sch\u00fcttelt die Manilla.", "tokens": ["Und", "sch\u00fct\u00b7telt", "die", "Ma\u00b7nil\u00b7la", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du mimst voll Angst und Heuchelei", "tokens": ["Du", "mimst", "voll", "Angst", "und", "Heu\u00b7che\u00b7lei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den schw\u00e4rmenden Gorilla.", "tokens": ["Den", "schw\u00e4r\u00b7men\u00b7den", "Go\u00b7ril\u00b7la", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Doch weil allm\u00e4hlich Zeit vergeht", "tokens": ["Doch", "weil", "all\u00b7m\u00e4h\u00b7lich", "Zeit", "ver\u00b7geht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nirgends eine Leiter steht,", "tokens": ["Und", "nir\u00b7gends", "ei\u00b7ne", "Lei\u00b7ter", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Entschlie\u00dft du dich voll Grausen", "tokens": ["Ent\u00b7schlie\u00dft", "du", "dich", "voll", "Grau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und pr\u00e4sentierst dein Hinterteil", "tokens": ["Und", "pr\u00e4\u00b7sen\u00b7tierst", "dein", "Hin\u00b7ter\u00b7teil"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und angelst lange nach dem Seil", "tokens": ["Und", "an\u00b7gelst", "lan\u00b7ge", "nach", "dem", "Seil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und l\u00e4\u00dft dich pl\u00f6tzlich sausen.", "tokens": ["Und", "l\u00e4\u00dft", "dich", "pl\u00f6tz\u00b7lich", "sau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du plumpst der Dame auf die Brust", "tokens": ["Du", "plumpst", "der", "Da\u00b7me", "auf", "die", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und tust, als t\u00e4tst du das bewu\u00dft,", "tokens": ["Und", "tust", ",", "als", "t\u00e4tst", "du", "das", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "ART", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bl\u00e4hst dich wie ein Segel.", "tokens": ["Und", "bl\u00e4hst", "dich", "wie", "ein", "Se\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nickst ein heiteres Allheil!", "tokens": ["Und", "nickst", "ein", "hei\u00b7te\u00b7res", "All\u00b7heil", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und lachst und f\u00fchlst dich doch derweil", "tokens": ["Und", "lachst", "und", "f\u00fchlst", "dich", "doch", "der\u00b7weil"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Teils Burschenschaft, teils Flegel.", "tokens": ["Teils", "Bur\u00b7schen\u00b7schaft", ",", "teils", "Fle\u00b7gel", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Kein M\u00e4dchen, nicht einmal die Braut,", "tokens": ["Kein", "M\u00e4d\u00b7chen", ",", "nicht", "ein\u00b7mal", "die", "Braut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht gerne H\u00e4nde ohne Haut.", "tokens": ["Sieht", "ger\u00b7ne", "H\u00e4n\u00b7de", "oh\u00b7ne", "Haut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}