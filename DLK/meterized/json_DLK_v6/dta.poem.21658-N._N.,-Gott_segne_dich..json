{"dta.poem.21658": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Gott segne dich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Der einst mein erstes Lied erklungen", "tokens": ["Der", "einst", "mein", "ers\u00b7tes", "Lied", "er\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In sehnsuchtsscheuer Knabenzeit,", "tokens": ["In", "sehn\u00b7suchts\u00b7scheu\u00b7er", "Kna\u00b7ben\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hast dir den Brautkranz nun errungen \u2014", "tokens": ["Hast", "dir", "den", "Braut\u00b7kranz", "nun", "er\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott segne dich, du junge Maid.", "tokens": ["Gott", "seg\u00b7ne", "dich", ",", "du", "jun\u00b7ge", "Maid", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein goldner Stern in Maienn\u00e4chten,", "tokens": ["Ein", "gold\u00b7ner", "Stern", "in", "Mai\u00b7en\u00b7n\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stiegst du vor meinem Blick empor,", "tokens": ["Stiegst", "du", "vor", "mei\u00b7nem", "Blick", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du glich\u2019st dem Edelstein, dem \u00e4chten,", "tokens": ["Du", "glich'st", "dem", "E\u00b7del\u00b7stein", ",", "dem", "\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An den ich ganz mein Herz verlor.", "tokens": ["An", "den", "ich", "ganz", "mein", "Herz", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich sah dein Bild von fern nur leuchten", "tokens": ["Ich", "sah", "dein", "Bild", "von", "fern", "nur", "leuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJD", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und betete in stummer Nacht", "tokens": ["Und", "be\u00b7te\u00b7te", "in", "stum\u00b7mer", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit fleh\u2019nden Augen, thr\u00e4nenfeuchten,", "tokens": ["Mit", "fleh'n\u00b7den", "Au\u00b7gen", ",", "thr\u00e4\u00b7nen\u00b7feuch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu dir \u2014 die nimmer mein gedacht.", "tokens": ["Zu", "dir", "die", "nim\u00b7mer", "mein", "ge\u00b7dacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "ART", "ADV", "PPOSAT", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Wolken kamen schwarz gezogen,", "tokens": ["Die", "Wol\u00b7ken", "ka\u00b7men", "schwarz", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wetter peitschten wild mein Haupt,", "tokens": ["Die", "Wet\u00b7ter", "peitschten", "wild", "mein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mein Schiff versank im Grab der Wogen,", "tokens": ["Mein", "Schiff", "ver\u00b7sank", "im", "Grab", "der", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "All\u2019 meines Gl\u00fccks ward ich beraubt.", "tokens": ["All'", "mei\u00b7nes", "Gl\u00fccks", "ward", "ich", "be\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Durch Himmel und durch H\u00f6lle st\u00fcrmte", "tokens": ["Durch", "Him\u00b7mel", "und", "durch", "H\u00f6l\u00b7le", "st\u00fcrm\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein furiengehetzter Sinn,", "tokens": ["Mein", "fu\u00b7ri\u00b7en\u00b7ge\u00b7hetz\u00b7ter", "Sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Ideal, das treu mich schirmte,", "tokens": ["Das", "I\u00b7deal", ",", "das", "treu", "mich", "schirm\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Warf mir den Rettungsanker hin.", "tokens": ["Warf", "mir", "den", "Ret\u00b7tungs\u00b7an\u00b7ker", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Sieh\u2019! Da zerflo\u00df das trunk\u2019ne Sehnen,", "tokens": ["Sieh'", "!", "Da", "zer\u00b7flo\u00df", "das", "trunk'\u00b7ne", "Seh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das wonnebang mich einst umschwoll,", "tokens": ["Das", "won\u00b7ne\u00b7bang", "mich", "einst", "um\u00b7schwoll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Bild verschwand, des Geistes W\u00e4hnen", "tokens": ["Dein", "Bild", "ver\u00b7schwand", ",", "des", "Geis\u00b7tes", "W\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ward ew\u2019ger Gottversenkung voll.", "tokens": ["Ward", "ew'\u00b7ger", "Gott\u00b7ver\u00b7sen\u00b7kung", "voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Menschheit Heiligthum zu retten,", "tokens": ["Der", "Menschheit", "Hei\u00b7lig\u00b7thum", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wall\u2019 predigend ich meinen Pfad,", "tokens": ["Wall'", "pre\u00b7di\u00b7gend", "ich", "mei\u00b7nen", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erl\u00f6send aus der Selbstsucht Ketten", "tokens": ["Er\u00b7l\u00f6\u00b7send", "aus", "der", "Selbst\u00b7sucht", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit meines Liedes freier That.", "tokens": ["Mit", "mei\u00b7nes", "Lie\u00b7des", "frei\u00b7er", "That", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch heute bei des T\u00e4ubers Girren,", "tokens": ["Doch", "heu\u00b7te", "bei", "des", "T\u00e4u\u00b7bers", "Gir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umhaucht vom Dust des Lindenbaums,", "tokens": ["Um\u00b7haucht", "vom", "Dust", "des", "Lin\u00b7den\u00b7baums", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Darum die Bienen l\u00fcstern schwirren,", "tokens": ["Da\u00b7rum", "die", "Bie\u00b7nen", "l\u00fcs\u00b7tern", "schwir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gedacht\u2019 ich jenes s\u00fc\u00dfen Traums:", "tokens": ["Ge\u00b7dacht'", "ich", "je\u00b7nes", "s\u00fc\u00b7\u00dfen", "Traums", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Da ich mein erstes Lied gesungen", "tokens": ["Da", "ich", "mein", "ers\u00b7tes", "Lied", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In sehnsuchtsscheuer Knabenzeit", "tokens": ["In", "sehn\u00b7suchts\u00b7scheu\u00b7er", "Kna\u00b7ben\u00b7zeit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir, die den Brautkranz nun errungen \u2014", "tokens": ["Dir", ",", "die", "den", "Braut\u00b7kranz", "nun", "er\u00b7run\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott segne dich, du junge Maid!", "tokens": ["Gott", "seg\u00b7ne", "dich", ",", "du", "jun\u00b7ge", "Maid", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}