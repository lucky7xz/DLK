{"textgrid.poem.49678": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das uralte M\u00e4nnchen", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kennt ihr das uralte M\u00e4nnchen", "tokens": ["Kennt", "ihr", "das", "ur\u00b7al\u00b7te", "M\u00e4nn\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit runzligem Angesicht", "tokens": ["Mit", "runz\u00b7li\u00b7gem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und mit dem wackligen Kopfe?", "tokens": ["Und", "mit", "dem", "wack\u00b7li\u00b7gen", "Kop\u00b7fe", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Kennt ihr das M\u00e4nnchen denn nicht?", "tokens": ["Kennt", "ihr", "das", "M\u00e4nn\u00b7chen", "denn", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "PTKNEG", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Man siehts nur einmal im Jahre;", "tokens": ["Man", "siehts", "nur", "ein\u00b7mal", "im", "Jah\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dann bleiben die Leute stehn", "tokens": ["Dann", "blei\u00b7ben", "die", "Leu\u00b7te", "stehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und sagen: \u00bbDas M\u00fcmmelgreischen,", "tokens": ["Und", "sa\u00b7gen", ":", "\u00bb", "Das", "M\u00fcm\u00b7mel\u00b7grei\u00b7schen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "$(", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ei, kann es wirklich noch gehn?", "tokens": ["Ei", ",", "kann", "es", "wirk\u00b7lich", "noch", "gehn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "ADJD", "ADV", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Wir glaubten, es sei gestorben,", "tokens": ["Wir", "glaub\u00b7ten", ",", "es", "sei", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Erde deckte es zu;", "tokens": ["Die", "Er\u00b7de", "deck\u00b7te", "es", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wallt es noch immer auf Erden?", "tokens": ["Wallt", "es", "noch", "im\u00b7mer", "auf", "Er\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wann kriegt's die ewige Ruh?\u00ab", "tokens": ["Wann", "kriegt's", "die", "e\u00b7wi\u00b7ge", "Ruh", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Das M\u00e4nnchen schreitet vor\u00fcber,", "tokens": ["Das", "M\u00e4nn\u00b7chen", "schrei\u00b7tet", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es hustet, r\u00e4uspert und spuckt", "tokens": ["Es", "hus\u00b7tet", ",", "r\u00e4us\u00b7pert", "und", "spuckt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und hat aus erloschnen Augen", "tokens": ["Und", "hat", "aus", "er\u00b7lo\u00b7schnen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gar seltsam uns angeguckt.", "tokens": ["Gar", "selt\u00b7sam", "uns", "an\u00b7ge\u00b7guckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Es klettert auf einen Brunnen,", "tokens": ["Es", "klet\u00b7tert", "auf", "ei\u00b7nen", "Brun\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der vor dem Rathause stund.", "tokens": ["Der", "vor", "dem", "Rat\u00b7hau\u00b7se", "stund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "\u00bbes lebe der Landesvater", "tokens": ["\u00bb", "es", "le\u00b7be", "der", "Lan\u00b7des\u00b7va\u00b7ter"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und bleibe lange gesund!\u00ab", "tokens": ["Und", "blei\u00b7be", "lan\u00b7ge", "ge\u00b7sund", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "So ruft es mit meckernder Stimme,", "tokens": ["So", "ruft", "es", "mit", "me\u00b7ckern\u00b7der", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dann steigt es wieder herab.", "tokens": ["Dann", "steigt", "es", "wie\u00b7der", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ein Jahr lang sieht man's nicht wieder,", "tokens": ["Ein", "Jahr", "lang", "sieht", "man's", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "PIS", "PTKNEG", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein Jahr lang liegt es im Grab.", "tokens": ["Ein", "Jahr", "lang", "liegt", "es", "im", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Ihr fragt, wer das alte M\u00e4nnchen", "tokens": ["Ihr", "fragt", ",", "wer", "das", "al\u00b7te", "M\u00e4nn\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWS", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit wackligem Kopfe sei?", "tokens": ["Mit", "wack\u00b7li\u00b7gem", "Kop\u00b7fe", "sei", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbdie nationalliberale,", "tokens": ["\u00bb", "die", "na\u00b7ti\u00b7o\u00b7nal\u00b7li\u00b7be\u00b7ra\u00b7le", ","], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die M\u00fcmmelgreisenpartei.\u00ab", "tokens": ["Die", "M\u00fcm\u00b7mel\u00b7grei\u00b7sen\u00b7par\u00b7tei", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}