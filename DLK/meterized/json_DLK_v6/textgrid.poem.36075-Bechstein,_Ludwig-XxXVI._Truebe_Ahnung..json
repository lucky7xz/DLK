{"textgrid.poem.36075": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "XxXVI. Truebe Ahnung.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbkein Himmel ist so helle, dass ihn kein W\u00f6lkchen tr\u00fcbt;", "tokens": ["\u00bb", "kein", "Him\u00b7mel", "ist", "so", "hel\u00b7le", ",", "dass", "ihn", "kein", "W\u00f6lk\u00b7chen", "tr\u00fcbt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VAFIN", "ADV", "ADJA", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Kein Fl\u00fcgel ist so schnelle, der nicht auch Ruhe liebt.", "tokens": ["Kein", "Fl\u00fc\u00b7gel", "ist", "so", "schnel\u00b7le", ",", "der", "nicht", "auch", "Ru\u00b7he", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "VVFIN", "$,", "PRELS", "PTKNEG", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Kein Herz ist so zufrieden, dass es nicht W\u00fcnsche hegt,", "tokens": ["Kein", "Herz", "ist", "so", "zu\u00b7frie\u00b7den", ",", "dass", "es", "nicht", "W\u00fcn\u00b7sche", "hegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PPER", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Es hofft ein Herz hienieden bis man's zu Grabe tr\u00e4gt.\u00ab", "tokens": ["Es", "hofft", "ein", "Herz", "hien\u00b7ie\u00b7den", "bis", "man's", "zu", "Gra\u00b7be", "tr\u00e4gt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "PIS", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbund mir, mir sollte Ruhe durch stilles Gl\u00fcck erbl\u00fchn?", "tokens": ["\u00bb", "und", "mir", ",", "mir", "soll\u00b7te", "Ru\u00b7he", "durch", "stil\u00b7les", "Gl\u00fcck", "er\u00b7bl\u00fchn", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "$,", "PPER", "VMFIN", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dazu sind meine W\u00fcnsche zu hoch und \u00fcberk\u00fchn.", "tokens": ["Da\u00b7zu", "sind", "mei\u00b7ne", "W\u00fcn\u00b7sche", "zu", "hoch", "und", "\u00fc\u00b7berk\u00b7\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN", "PTKA", "ADJD", "KON", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das Gl\u00fcck wird nie gebunden an Wesen, Zeit und Raum;", "tokens": ["Das", "Gl\u00fcck", "wird", "nie", "ge\u00b7bun\u00b7den", "an", "We\u00b7sen", ",", "Zeit", "und", "Raum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das Gl\u00fcck wird nie gefunden, als nur im Hoffnungstraum.\u00ab", "tokens": ["Das", "Gl\u00fcck", "wird", "nie", "ge\u00b7fun\u00b7den", ",", "als", "nur", "im", "Hoff\u00b7nungs\u00b7traum", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,", "KOUS", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbeine graue Riesenmutter sitzt \u00fcberm Ei der Welt,", "tokens": ["\u00bb", "ei\u00b7ne", "grau\u00b7e", "Rie\u00b7sen\u00b7mut\u00b7ter", "sitzt", "\u00fc\u00b7berm", "Ei", "der", "Welt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Das sie mit eh'rnen Ketten an sich gefesselt h\u00e4lt.", "tokens": ["Das", "sie", "mit", "eh'r\u00b7nen", "Ket\u00b7ten", "an", "sich", "ge\u00b7fes\u00b7selt", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ADJA", "NN", "APPR", "PRF", "VVPP", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und unaufh\u00f6rlich regt sich ihr dunkles Fl\u00fcgelpaar,", "tokens": ["Und", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "regt", "sich", "ihr", "dunk\u00b7les", "Fl\u00fc\u00b7gel\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wild ineinander wirrend umweht sie Schlangenhaar.\u00ab", "tokens": ["Wild", "in\u00b7ein\u00b7an\u00b7der", "wir\u00b7rend", "um\u00b7weht", "sie", "Schlan\u00b7gen\u00b7haar", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "ADJD", "VVFIN", "PPER", "NN", "$.", "$("], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "\u00bbsie h\u00e4lt in ihren H\u00e4nden ein schneidend Schwert empor;", "tokens": ["\u00bb", "sie", "h\u00e4lt", "in", "ih\u00b7ren", "H\u00e4n\u00b7den", "ein", "schnei\u00b7dend", "Schwert", "em\u00b7por", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Umflammt von rothem Mantel wie Nordlands Meteor.", "tokens": ["Um\u00b7flammt", "von", "ro\u00b7them", "Man\u00b7tel", "wie", "Nord\u00b7lands", "Me\u00b7te\u00b7or", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "KOKOM", "NE", "NE", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Aus ihrem Schooss gebiert sie drei Drachen: einer ", "tokens": ["Aus", "ih\u00b7rem", "Scho\u00b7oss", "ge\u00b7biert", "sie", "drei", "Dra\u00b7chen", ":", "ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "CARD", "NN", "$.", "ART"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der andre ", "tokens": ["Der", "and\u00b7re"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "So Faustus spricht zum Wagner in stillvertrauter Stund,", "tokens": ["So", "Faus\u00b7tus", "spricht", "zum", "Wag\u00b7ner", "in", "still\u00b7ver\u00b7trau\u00b7ter", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und dieser: \u00bbTiefe R\u00e4thsel verk\u00fcndet Euer Mund.", "tokens": ["Und", "die\u00b7ser", ":", "\u00bb", "Tie\u00b7fe", "R\u00e4th\u00b7sel", "ver\u00b7k\u00fcn\u00b7det", "Eu\u00b7er", "Mund", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "$.", "$(", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "D\u00fcrft' ich die Deutung wagen, wie solches Ihr gemeint,", "tokens": ["D\u00fcrft'", "ich", "die", "Deu\u00b7tung", "wa\u00b7gen", ",", "wie", "sol\u00b7ches", "Ihr", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,", "PWAV", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So m\u00f6cht' ich frei Euch sagen, wie mir der Sinn erscheint.\u00ab", "tokens": ["So", "m\u00f6cht'", "ich", "frei", "Euch", "sa\u00b7gen", ",", "wie", "mir", "der", "Sinn", "er\u00b7scheint", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbdie graue Riesenmutter ist ", "tokens": ["\u00bb", "die", "grau\u00b7e", "Rie\u00b7sen\u00b7mut\u00b7ter", "ist"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die br\u00fctend \u00fcberm Weltei, gleich einem Drachen, wacht.", "tokens": ["Die", "br\u00fc\u00b7tend", "\u00fc\u00b7berm", "Wel\u00b7tei", ",", "gleich", "ei\u00b7nem", "Dra\u00b7chen", ",", "wacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "$,", "ADV", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}}, "stanza.7": {"line.1": {"text": "\u00bbund ihres Hauptes Wirrhaar ist ", "tokens": ["\u00bb", "und", "ih\u00b7res", "Haup\u00b7tes", "Wirr\u00b7haar", "ist"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "PPOSAT", "NN", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem wirds zur g\u00fcldnen Kette, dem andern wirds zum Strick.", "tokens": ["Dem", "wirds", "zur", "g\u00fcld\u00b7nen", "Ket\u00b7te", ",", "dem", "an\u00b7dern", "wirds", "zum", "Strick", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "ADJA", "NN", "$,", "PRELS", "PIS", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein Ariadnenfaden wirds dem zu Gl\u00fcck und Heil,", "tokens": ["Ein", "A\u00b7riad\u00b7nen\u00b7fa\u00b7den", "wirds", "dem", "zu", "Gl\u00fcck", "und", "Heil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Ein Spinneweb der Hoffnung f\u00fchrt's den am Narrenseil.\u00ab", "tokens": ["Ein", "Spin\u00b7ne\u00b7web", "der", "Hoff\u00b7nung", "f\u00fchrt's", "den", "am", "Nar\u00b7ren\u00b7seil", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbihr schneidend Schwert ist ", "tokens": ["\u00bb", "ihr", "schnei\u00b7dend", "Schwert", "ist"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "ADJD", "NN", "VAFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Den breitet sie gar weit aus, so \u00fcber S\u00fcd als Nord.", "tokens": ["Den", "brei\u00b7tet", "sie", "gar", "weit", "aus", ",", "so", "\u00fc\u00b7ber", "S\u00fcd", "als", "Nord", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,", "ADV", "APPR", "NN", "KOKOM", "NE", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und ihre Drachenkinder? Die tr\u00e4ge ", "tokens": ["Und", "ih\u00b7re", "Dra\u00b7chen\u00b7kin\u00b7der", "?", "Die", "tr\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$.", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}}, "stanza.9": {"line.1": {"text": "Mit tr\u00fcbem Sinnen Faustus zum treuen Freunde spricht:", "tokens": ["Mit", "tr\u00fc\u00b7bem", "Sin\u00b7nen", "Faus\u00b7tus", "zum", "treu\u00b7en", "Freun\u00b7de", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbo nein, Du deutest, Wagner, mir meine R\u00e4thsel nicht!", "tokens": ["\u00bb", "o", "nein", ",", "Du", "deu\u00b7test", ",", "Wag\u00b7ner", ",", "mir", "mei\u00b7ne", "R\u00e4th\u00b7sel", "nicht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PTKANT", "$,", "PPER", "VVFIN", "$,", "NN", "$,", "PPER", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und wenn auch nicht verachtbar mir Deine Deutung scheint,", "tokens": ["Und", "wenn", "auch", "nicht", "ver\u00b7acht\u00b7bar", "mir", "Dei\u00b7ne", "Deu\u00b7tung", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PTKNEG", "ADJD", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Mir brennt's im Herzen anders, und anders ist's gemeint.\u00ab", "tokens": ["Mir", "brennt's", "im", "Her\u00b7zen", "an\u00b7ders", ",", "und", "an\u00b7ders", "ist's", "ge\u00b7meint", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "ADV", "$,", "KON", "ADV", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.2": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Ueberm Haupte Gottverhasster regt sie das immerdar!\u00ab", "tokens": ["Ue\u00b7berm", "Haup\u00b7te", "Gott\u00b7ver\u00b7hass\u00b7ter", "regt", "sie", "das", "im\u00b7mer\u00b7dar", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "NN", "VVFIN", "PPER", "PDS", "ADV", "$.", "$("], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.11": {"line.1": {"text": "\u00bbihr Haar ist das ", "tokens": ["\u00bb", "ihr", "Haar", "ist", "das"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und schwebend ob dem S\u00fcnder h\u00e4ngt stets ein Schwert daran.", "tokens": ["Und", "schwe\u00b7bend", "ob", "dem", "S\u00fcn\u00b7der", "h\u00e4ngt", "stets", "ein", "Schwert", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOUS", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das Schwert, das heisst ", "tokens": ["Das", "Schwert", ",", "das", "heisst"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Der wie ein Flammenmantel um ihre Schultern weht!\u00ab", "tokens": ["Der", "wie", "ein", "Flam\u00b7men\u00b7man\u00b7tel", "um", "ih\u00b7re", "Schul\u00b7tern", "weht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "\u00bbund nun? Das Drachenkleeblatt, das sie geboren hat?", "tokens": ["\u00bb", "und", "nun", "?", "Das", "Dra\u00b7chen\u00b7klee\u00b7blatt", ",", "das", "sie", "ge\u00b7bo\u00b7ren", "hat", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "$.", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Den er