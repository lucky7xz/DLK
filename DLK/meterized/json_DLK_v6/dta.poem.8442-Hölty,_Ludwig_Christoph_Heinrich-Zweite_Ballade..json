{"dta.poem.8442": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zweite Ballade.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Bis dass der Himmel graute,", "tokens": ["Bis", "dass", "der", "Him\u00b7mel", "grau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und man beim ersten Sonnenblick", "tokens": ["Und", "man", "beim", "ers\u00b7ten", "Son\u00b7nen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein gr\u00fcnes Eiland schaute.", "tokens": ["Ein", "gr\u00fc\u00b7nes", "Ei\u00b7land", "schau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es lag im S\u00fcderozean", "tokens": ["Es", "lag", "im", "S\u00fc\u00b7de\u00b7ro\u00b7ze\u00b7an"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Seit lieben langen Jahren,", "tokens": ["Seit", "lie\u00b7ben", "lan\u00b7gen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo weder Cook noch Magellan", "tokens": ["Wo", "we\u00b7der", "Cook", "noch", "Ma\u00b7gel\u00b7lan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "KON", "NE", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Noch Dampier gefahren.", "tokens": ["Noch", "Dam\u00b7pier", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie traten in ein Paradies,", "tokens": ["Sie", "tra\u00b7ten", "in", "ein", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Freud' und Wollust lauschte,", "tokens": ["Wo", "Freud'", "und", "Wol\u00b7lust", "lauschte", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In jedem Fr\u00fchlingsl\u00fcftchen blies,", "tokens": ["In", "je\u00b7dem", "Fr\u00fch\u00b7lings\u00b7l\u00fcft\u00b7chen", "blies", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In jeder Quelle rauschte.", "tokens": ["In", "je\u00b7der", "Quel\u00b7le", "rauschte", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das war euch traun ein Luftgefild!", "tokens": ["Das", "war", "euch", "traun", "ein", "Luft\u00b7ge\u00b7fild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Rings lachten bunte Fl\u00e4chen,", "tokens": ["Rings", "lach\u00b7ten", "bun\u00b7te", "Fl\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Rings zitterte das goldne Bild", "tokens": ["Rings", "zit\u00b7ter\u00b7te", "das", "gold\u00b7ne", "Bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Sonn' in hundert B\u00e4chen.", "tokens": ["Der", "Sonn'", "in", "hun\u00b7dert", "B\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Weste fl\u00fcsterten vertraut", "tokens": ["Die", "Wes\u00b7te", "fl\u00fcs\u00b7ter\u00b7ten", "ver\u00b7traut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und raubten jungen Veilchen,", "tokens": ["Und", "raub\u00b7ten", "jun\u00b7gen", "Veil\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie der Geliebte seiner Braut,", "tokens": ["Wie", "der", "Ge\u00b7lieb\u00b7te", "sei\u00b7ner", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf jeder Wiese M\u00e4ulchen.", "tokens": ["Auf", "je\u00b7der", "Wie\u00b7se", "M\u00e4ul\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es bl\u00fchte rings im Zauberglanz", "tokens": ["Es", "bl\u00fch\u00b7te", "rings", "im", "Zau\u00b7ber\u00b7glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Hiazint' und Rose;", "tokens": ["Die", "Hia\u00b7zint'", "und", "Ro\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Es trug und bl\u00fchte Pomeranz'", "tokens": ["Es", "trug", "und", "bl\u00fch\u00b7te", "Po\u00b7me\u00b7ranz'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Pfirsch' und Aprikose.", "tokens": ["Und", "Pfir\u00b7sch'", "und", "Ap\u00b7ri\u00b7ko\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Musik entstr\u00f6mte sonder Rast", "tokens": ["Mu\u00b7sik", "ent\u00b7str\u00f6m\u00b7te", "son\u00b7der", "Rast"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Den k\u00fchlen Rebenlauben;", "tokens": ["Den", "k\u00fch\u00b7len", "Re\u00b7ben\u00b7lau\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es herzten sich auf jedem Ast", "tokens": ["Es", "herz\u00b7ten", "sich", "auf", "je\u00b7dem", "Ast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verliebte Turteltauben.", "tokens": ["Ver\u00b7lieb\u00b7te", "Tur\u00b7tel\u00b7tau\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es sprang, poz Stern, da m\u00f6cht' ich sein!", "tokens": ["Es", "sprang", ",", "poz", "Stern", ",", "da", "m\u00f6cht'", "ich", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN", "$,", "KOUS", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Schatten gr\u00fcner Hecken,", "tokens": ["Im", "Schat\u00b7ten", "gr\u00fc\u00b7ner", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der feurigste Burgunderwein,", "tokens": ["Der", "feu\u00b7rigs\u00b7te", "Bur\u00b7gun\u00b7der\u00b7wein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "In weite goldne Becken.", "tokens": ["In", "wei\u00b7te", "gold\u00b7ne", "Be\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es ragt' ein pr\u00e4chtiger Pallast,", "tokens": ["Es", "ragt'", "ein", "pr\u00e4ch\u00b7ti\u00b7ger", "Pal\u00b7last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erbauet aus T\u00fcrkisen,", "tokens": ["Er\u00b7bau\u00b7et", "aus", "T\u00fcr\u00b7ki\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "-+-++--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Mit Gold' und Perlen eingefasst,", "tokens": ["Mit", "Gold'", "und", "Per\u00b7len", "ein\u00b7ge\u00b7fasst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf angenehmen Wiesen.", "tokens": ["Auf", "an\u00b7ge\u00b7neh\u00b7men", "Wie\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Treppen waren aus Achat;", "tokens": ["Die", "Trep\u00b7pen", "wa\u00b7ren", "aus", "A\u00b7chat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Die weiten Fl\u00fcgelth\u00fcren,", "tokens": ["Die", "wei\u00b7ten", "Fl\u00fc\u00b7gelt\u00b7h\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Durch die man in den Pallast trat,", "tokens": ["Durch", "die", "man", "in", "den", "Pal\u00b7last", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aus blizenden Saffiren.", "tokens": ["Aus", "bli\u00b7zen\u00b7den", "Saf\u00b7fi\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das Dach und auch der Wetterhahn,", "tokens": ["Das", "Dach", "und", "auch", "der", "Wet\u00b7ter\u00b7hahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie leichtlich zu erachten,", "tokens": ["Wie", "leicht\u00b7lich", "zu", "e\u00b7rach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von feinem Gold' aus Hindostan,", "tokens": ["Von", "fei\u00b7nem", "Gold'", "aus", "Hin\u00b7dos\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Besezet mit Smaragden.", "tokens": ["Be\u00b7se\u00b7zet", "mit", "Sma\u00b7rag\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "-+-+++-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Ein wunderbares Feienschloss,", "tokens": ["Ein", "wun\u00b7der\u00b7ba\u00b7res", "Fei\u00b7en\u00b7schloss", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bei welchem sonder Zweifel,", "tokens": ["Bei", "wel\u00b7chem", "son\u00b7der", "Zwei\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der es erbaut, viel Schweiss vergoss,", "tokens": ["Der", "es", "er\u00b7baut", ",", "viel", "Schweiss", "ver\u00b7goss", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gott sey bey uns, der Teufel!", "tokens": ["Gott", "sey", "bey", "uns", ",", "der", "Teu\u00b7fel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ein grosser tapezirter Saal", "tokens": ["Ein", "gros\u00b7ser", "ta\u00b7pe\u00b7zir\u00b7ter", "Saal"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ging mitten durchs Geb\u00e4ude,", "tokens": ["Ging", "mit\u00b7ten", "durchs", "Ge\u00b7b\u00e4u\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Schildereien ohne Zahl:", "tokens": ["Mit", "Schil\u00b7de\u00b7rei\u00b7en", "oh\u00b7ne", "Zahl", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die sch\u00f6nste Augenweide!", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Au\u00b7gen\u00b7wei\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von Rafael und Tizian,", "tokens": ["Von", "Ra\u00b7fael", "und", "Ti\u00b7zi\u00b7an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hier eine nackte Lede,", "tokens": ["Hier", "ei\u00b7ne", "nack\u00b7te", "Le\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dort Vater Zeus mit ihr als Schwan", "tokens": ["Dort", "Va\u00b7ter", "Zeus", "mit", "ihr", "als", "Schwan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "NE", "APPR", "PPOSAT", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In einer Liebesfehde;", "tokens": ["In", "ei\u00b7ner", "Lie\u00b7bes\u00b7feh\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Grossultan, der Perser Schach,", "tokens": ["Der", "Gros\u00b7sul\u00b7tan", ",", "der", "Per\u00b7ser", "Schach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Zirkel ihrer Frauen;", "tokens": ["Im", "Zir\u00b7kel", "ih\u00b7rer", "Frau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein lustig Karnevalgelag,", "tokens": ["Ein", "lus\u00b7tig", "Kar\u00b7ne\u00b7val\u00b7ge\u00b7lag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar lieblich anzuschauen;", "tokens": ["Gar", "lieb\u00b7lich", "an\u00b7zu\u00b7schau\u00b7en", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Muselm\u00e4nner Himmelreich", "tokens": ["Der", "Mu\u00b7sel\u00b7m\u00e4n\u00b7ner", "Him\u00b7mel\u00b7reich"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Voll niedlicher Figuren;", "tokens": ["Voll", "nied\u00b7li\u00b7cher", "Fi\u00b7gu\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein gr\u00fcner Wald, im Wald' ein Teich", "tokens": ["Ein", "gr\u00fc\u00b7ner", "Wald", ",", "im", "Wald'", "ein", "Teich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "NN"], "meter": "-