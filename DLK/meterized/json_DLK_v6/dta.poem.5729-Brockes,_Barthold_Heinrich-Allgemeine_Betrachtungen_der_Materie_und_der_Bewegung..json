{"dta.poem.5729": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Allgemeine Betrachtungen der Materie  \n und der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Stoff, so sprachen sie, ist alle Ding' allein,", "tokens": ["Der", "Stoff", ",", "so", "spra\u00b7chen", "sie", ",", "ist", "al\u00b7le", "Ding'", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "PPER", "$,", "VAFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der C\u00f6rper C\u00f6rper, ja ein Grund, der allgemein.", "tokens": ["Der", "C\u00f6r\u00b7per", "C\u00f6r\u00b7per", ",", "ja", "ein", "Grund", ",", "der", "all\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ADV", "ART", "NN", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhm k\u00f6nnen mitgetheilet seyn", "tokens": ["Jhm", "k\u00f6n\u00b7nen", "mit\u00b7ge\u00b7thei\u00b7let", "seyn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Unz\u00e4hlige Verwandelungen.", "tokens": ["Un\u00b7z\u00e4h\u00b7li\u00b7ge", "Ver\u00b7wan\u00b7de\u00b7lun\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Allein, was man ihn auch f\u00fcr Aenderungen", "tokens": ["Al\u00b7lein", ",", "was", "man", "ihn", "auch", "f\u00fcr", "A\u00b7en\u00b7de\u00b7run\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PRELS", "PIS", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Empfinden l\u00e4st, durch tausend Sachen;", "tokens": ["Emp\u00b7fin\u00b7den", "l\u00e4st", ",", "durch", "tau\u00b7send", "Sa\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Tangt ihn doch Nichts, zu Nichts zu machen.", "tokens": ["Tangt", "ihn", "doch", "Nichts", ",", "zu", "Nichts", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIS", "$,", "APPR", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Man theile nur den Stoff, man dr\u00fccke,", "tokens": ["Man", "thei\u00b7le", "nur", "den", "Stoff", ",", "man", "dr\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Man geb\u2019 auf tausend Art ihm ein Geschicke;", "tokens": ["Man", "geb'", "auf", "tau\u00b7send", "Art", "ihm", "ein", "Ge\u00b7schi\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "CARD", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Er bleibt in seiner Unbest\u00e4ndigkeit,", "tokens": ["Er", "bleibt", "in", "sei\u00b7ner", "Un\u00b7be\u00b7st\u00e4n\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ohn etwas von sich zu verlieren,", "tokens": ["Ohn", "et\u00b7was", "von", "sich", "zu", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "In stetiger Vollkommenheit:", "tokens": ["In", "ste\u00b7ti\u00b7ger", "Voll\u00b7kom\u00b7men\u00b7heit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Indem ein Wesen stets entsteht,", "tokens": ["In\u00b7dem", "ein", "We\u00b7sen", "stets", "ent\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Aus einem Wesen, das vergeht.", "tokens": ["Aus", "ei\u00b7nem", "We\u00b7sen", ",", "das", "ver\u00b7geht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Dies ist ", "tokens": ["Dies", "ist"], "token_info": ["word", "word"], "pos": ["PDS", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "So bald sie nur das Licht versp\u00fcret,", "tokens": ["So", "bald", "sie", "nur", "das", "Licht", "ver\u00b7sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und der doch immer fruchtbar ist,", "tokens": ["Und", "der", "doch", "im\u00b7mer", "frucht\u00b7bar", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und sie stets wiederum gebieret,", "tokens": ["Und", "sie", "stets", "wie\u00b7de\u00b7rum", "ge\u00b7bie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Um sie aufs neue zu verzehren.", "tokens": ["Um", "sie", "aufs", "neu\u00b7e", "zu", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "APPRART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Er wird nicht m\u00fcd\u2019 im Zeugen, im Zerst\u00f6hren;", "tokens": ["Er", "wird", "nicht", "m\u00fcd'", "im", "Zeu\u00b7gen", ",", "im", "Zer\u00b7st\u00f6h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Er h\u00f6rt nicht auf zu w\u00fcrgen, zu geb\u00e4hren.", "tokens": ["Er", "h\u00f6rt", "nicht", "auf", "zu", "w\u00fcr\u00b7gen", ",", "zu", "ge\u00b7b\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}