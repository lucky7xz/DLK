{"textgrid.poem.59827": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Woran erkennest du die sch\u00f6nsten Blumen? An ihrer Bl\u00fcte!", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Woran erkennest du die sch\u00f6nsten Blumen? An ihrer Bl\u00fcte!", "tokens": ["Wo\u00b7ran", "er\u00b7ken\u00b7nest", "du", "die", "sch\u00f6ns\u00b7ten", "Blu\u00b7men", "?", "An", "ih\u00b7rer", "Bl\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Woran erkennest du die besten Weine? An ihrer G\u00fcte!", "tokens": ["Wo\u00b7ran", "er\u00b7ken\u00b7nest", "du", "die", "bes\u00b7ten", "Wei\u00b7ne", "?", "An", "ih\u00b7rer", "G\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Woran erkennest du die besten Menschen? An dem Gem\u00fcte!", "tokens": ["Wo\u00b7ran", "er\u00b7ken\u00b7nest", "du", "die", "bes\u00b7ten", "Men\u00b7schen", "?", "An", "dem", "Ge\u00b7m\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Woran erkennest du den Scheich und Mufti? An der Kapuze!", "tokens": ["Wo\u00b7ran", "er\u00b7ken\u00b7nest", "du", "den", "Scheich", "und", "Muf\u00b7ti", "?", "An", "der", "Ka\u00b7pu\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "KON", "NN", "$.", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Die Antwort, Freund, ist richtig, \u2013 geh und mache sie dir zunutze!", "tokens": ["Die", "Ant\u00b7wort", ",", "Freund", ",", "ist", "rich\u00b7tig", ",", "\u2013", "geh", "und", "ma\u00b7che", "sie", "dir", "zu\u00b7nut\u00b7ze", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "VAFIN", "ADJD", "$,", "$(", "VVFIN", "KON", "VVFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}}}}}