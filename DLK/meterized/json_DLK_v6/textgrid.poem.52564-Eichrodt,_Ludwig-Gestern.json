{"textgrid.poem.52564": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Gestern", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gestern war ich k\u00fchn und fr\u00f6hlich,", "tokens": ["Ge\u00b7stern", "war", "ich", "k\u00fchn", "und", "fr\u00f6h\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Heute bin ich ungl\u00fcckselig,", "tokens": ["Heu\u00b7te", "bin", "ich", "un\u00b7gl\u00fcck\u00b7se\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Stunden nach Erl\u00f6sung", "tokens": ["Und", "die", "Stun\u00b7den", "nach", "Er\u00b7l\u00f6\u00b7sung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weinend z\u00e4hl ich.", "tokens": ["Wei\u00b7nend", "z\u00e4hl", "ich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Reiz des Fr\u00fchlings, Schmuck des Lebens,", "tokens": ["Reiz", "des", "Fr\u00fch\u00b7lings", ",", "Schmuck", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, f\u00fcr mich bl\u00fcht ihr vergebens!", "tokens": ["Ach", ",", "f\u00fcr", "mich", "bl\u00fcht", "ihr", "ver\u00b7ge\u00b7bens", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Schafft mir einen Augenblick nur", "tokens": ["Schafft", "mir", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Des Erhebens!", "tokens": ["Des", "Er\u00b7he\u00b7bens", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Liebesqualen mu\u00df ich klagen,", "tokens": ["Lie\u00b7bes\u00b7qua\u00b7len", "mu\u00df", "ich", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seht, mein Hochmuth liegt erschlagen \u2013", "tokens": ["Seht", ",", "mein", "Hoch\u00b7muth", "liegt", "er\u00b7schla\u00b7gen", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses Elend, unertr\u00e4glich,", "tokens": ["Die\u00b7ses", "E\u00b7lend", ",", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werd ichs tragen?", "tokens": ["Werd", "ichs", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}