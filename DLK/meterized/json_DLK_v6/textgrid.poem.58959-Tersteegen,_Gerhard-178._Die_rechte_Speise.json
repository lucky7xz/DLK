{"textgrid.poem.58959": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "178. Die rechte Speise", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist eine d\u00fcrre W\u00fcste,", "tokens": ["Die", "Welt", "ist", "ei\u00b7ne", "d\u00fcr\u00b7re", "W\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es schmecket sau'r das Allers\u00fc\u00dfte;", "tokens": ["Es", "schme\u00b7cket", "sau'r", "das", "Al\u00b7ler\u00b7s\u00fc\u00df\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kehr deinen Hunger ganz hinein,", "tokens": ["Kehr", "dei\u00b7nen", "Hun\u00b7ger", "ganz", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Gott mu\u00df deine Speise sein!", "tokens": ["Nur", "Gott", "mu\u00df", "dei\u00b7ne", "Spei\u00b7se", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}