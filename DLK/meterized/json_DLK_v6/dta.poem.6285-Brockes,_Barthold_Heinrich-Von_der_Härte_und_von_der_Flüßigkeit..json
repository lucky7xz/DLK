{"dta.poem.6285": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie da\u00df, da jegliches f\u00fcr sich", "tokens": ["Wie", "da\u00df", ",", "da", "jeg\u00b7li\u00b7ches", "f\u00fcr", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "$,", "KOUS", "PIS", "APPR", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich r\u00fchret und bewegt, sie durch einander eilen,", "tokens": ["Sich", "r\u00fch\u00b7ret", "und", "be\u00b7wegt", ",", "sie", "durch", "ein\u00b7an\u00b7der", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "KON", "VVFIN", "$,", "PPER", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und fast im Augenblick an allen Seiten", "tokens": ["Und", "fast", "im", "Au\u00b7gen\u00b7blick", "an", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sehr fertig sind, sich auszubreiten.", "tokens": ["Sehr", "fer\u00b7tig", "sind", ",", "sich", "aus\u00b7zu\u00b7brei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "PRF", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}