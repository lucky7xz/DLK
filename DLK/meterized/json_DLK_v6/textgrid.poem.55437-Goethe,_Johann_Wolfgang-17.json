{"textgrid.poem.55437": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "17", "genre": "verse", "period": "N.A.", "pub_year": 1823, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein freundlich Wort kommt eines nach dem andern", "tokens": ["Ein", "freund\u00b7lich", "Wort", "kommt", "ei\u00b7nes", "nach", "dem", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ART", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von S\u00fcden her und bringt uns frohe Stunden;", "tokens": ["Von", "S\u00fc\u00b7den", "her", "und", "bringt", "uns", "fro\u00b7he", "Stun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es ruft uns auf, zum Edelsten zu wandern,", "tokens": ["Es", "ruft", "uns", "auf", ",", "zum", "E\u00b7dels\u00b7ten", "zu", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "APPRART", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Nicht ist der Geist, doch ist der Fu\u00df gebunden.", "tokens": ["Nicht", "ist", "der", "Geist", ",", "doch", "ist", "der", "Fu\u00df", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie soll ich dem, den ich so lang begleitet,", "tokens": ["Wie", "soll", "ich", "dem", ",", "den", "ich", "so", "lang", "be\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nun etwas Traulichs in die Ferne sagen?", "tokens": ["Nun", "et\u00b7was", "Trau\u00b7lichs", "in", "die", "Fer\u00b7ne", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihm, der sich selbst im Innersten bestreitet,", "tokens": ["Ihm", ",", "der", "sich", "selbst", "im", "In\u00b7ners\u00b7ten", "be\u00b7strei\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PRF", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Stark angewohnt, das tiefste Weh zu tragen.", "tokens": ["Stark", "an\u00b7ge\u00b7wohnt", ",", "das", "tiefs\u00b7te", "Weh", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wohl sei ihm doch, wenn er sich selbst empfindet!", "tokens": ["Wohl", "sei", "ihm", "doch", ",", "wenn", "er", "sich", "selbst", "emp\u00b7fin\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er wage selbst, sich hochbegl\u00fcckt zu nennen,", "tokens": ["Er", "wa\u00b7ge", "selbst", ",", "sich", "hoch\u00b7be\u00b7gl\u00fcckt", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Musenkraft die Schmerzen \u00fcberwindet;", "tokens": ["Wenn", "Mu\u00b7sen\u00b7kraft", "die", "Schmer\u00b7zen", "\u00fc\u00b7berw\u00b7in\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wie ich ihn erkannt, m\u00f6g er sich kennen.", "tokens": ["Und", "wie", "ich", "ihn", "er\u00b7kannt", ",", "m\u00f6g", "er", "sich", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "VVPP", "$,", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}