{"textgrid.poem.55776": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer gab denn zu warten der Heerde das Recht,", "tokens": ["Wer", "gab", "denn", "zu", "war\u00b7ten", "der", "Heer\u00b7de", "das", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PTKZU", "VVINF", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wer nennt, was ich \u00fcbte, verworfen und schlecht,", "tokens": ["Wer", "nennt", ",", "was", "ich", "\u00fcb\u00b7te", ",", "ver\u00b7wor\u00b7fen", "und", "schlecht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Wer hetzt mich entgegen der Nacht und der Noth,", "tokens": ["Wer", "hetzt", "mich", "ent\u00b7ge\u00b7gen", "der", "Nacht", "und", "der", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wer gibt meiner Seele Befehl und Gebot,", "tokens": ["Wer", "gibt", "mei\u00b7ner", "See\u00b7le", "Be\u00b7fehl", "und", "Ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Wer hebt gegen mich seine strafende Hand,", "tokens": ["Wer", "hebt", "ge\u00b7gen", "mich", "sei\u00b7ne", "stra\u00b7fen\u00b7de", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wer hat mir den Sclaven als Richter gesandt?", "tokens": ["Wer", "hat", "mir", "den", "Scla\u00b7ven", "als", "Rich\u00b7ter", "ge\u00b7sandt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ART", "NN", "KOUS", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Und jauchzend erdr\u00f6hnte mein f\u00fcrstlicher Schrei:", "tokens": ["Und", "jauch\u00b7zend", "er\u00b7dr\u00f6hn\u00b7te", "mein", "f\u00fcrst\u00b7li\u00b7cher", "Schrei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wer wagt es zu sagen, dass S\u00fcnder ich sei?", "tokens": ["Wer", "wagt", "es", "zu", "sa\u00b7gen", ",", "dass", "S\u00fcn\u00b7der", "ich", "sei", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKZU", "VVINF", "$,", "KOUS", "NN", "PPER", "VAFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}