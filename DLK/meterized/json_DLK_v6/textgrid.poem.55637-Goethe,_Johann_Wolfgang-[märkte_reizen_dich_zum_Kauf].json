{"textgrid.poem.55637": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "[m\u00e4rkte reizen dich zum Kauf]", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4rkte reizen dich zum Kauf;", "tokens": ["M\u00e4rk\u00b7te", "rei\u00b7zen", "dich", "zum", "Kauf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch das Wissen bl\u00e4het auf.", "tokens": ["Doch", "das", "Wis\u00b7sen", "bl\u00e4\u00b7het", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer im stillen um sich schaut,", "tokens": ["Wer", "im", "stil\u00b7len", "um", "sich", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "ADJA", "APPR", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lernet, wie die Lieb erbaut.", "tokens": ["Ler\u00b7net", ",", "wie", "die", "Lieb", "er\u00b7baut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bist du Tag und Nacht beflissen,", "tokens": ["Bist", "du", "Tag", "und", "Nacht", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Viel zu h\u00f6ren, viel zu wissen.", "tokens": ["Viel", "zu", "h\u00f6\u00b7ren", ",", "viel", "zu", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Horch an einer andern T\u00fcre,", "tokens": ["Horch", "an", "ei\u00b7ner", "an\u00b7dern", "T\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie zu wissen sich geb\u00fchre.", "tokens": ["Wie", "zu", "wis\u00b7sen", "sich", "ge\u00b7b\u00fch\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKZU", "VVINF", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Soll das Rechte zu dir ein,", "tokens": ["Soll", "das", "Rech\u00b7te", "zu", "dir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "F\u00fchl, in Gott was Rechts zu sein:", "tokens": ["F\u00fchl", ",", "in", "Gott", "was", "Rechts", "zu", "sein", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "PWS", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wer von reiner Lieb entbrannt,", "tokens": ["Wer", "von", "rei\u00b7ner", "Lieb", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Wird vom lieben Gott erkannt.", "tokens": ["Wird", "vom", "lie\u00b7ben", "Gott", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}