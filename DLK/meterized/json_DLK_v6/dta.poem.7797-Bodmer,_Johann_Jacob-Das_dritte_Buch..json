, "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ART", "ADJA", "NE", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.56": {"text": "Die schr\u00f6cklich seinem Feind und lieblich seinem Land,", "tokens": ["Die", "schr\u00f6ck\u00b7lich", "sei\u00b7nem", "Feind", "und", "lieb\u00b7lich", "sei\u00b7nem", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "KON", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Und gienge unges\u00e4umt mit ihm und andern Helden", "tokens": ["Und", "gien\u00b7ge", "un\u00b7ge\u00b7s\u00e4umt", "mit", "ihm", "und", "an\u00b7dern", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPER", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Nach Saul, des Angesicht konnt seine Freude melden,", "tokens": ["Nach", "Saul", ",", "des", "An\u00b7ge\u00b7sicht", "konnt", "sei\u00b7ne", "Freu\u00b7de", "mel\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Die seine Seel geno\u00df: Er gr\u00fcst sie insgesamt", "tokens": ["Die", "sei\u00b7ne", "Seel", "ge\u00b7no\u00df", ":", "Er", "gr\u00fcst", "sie", "ins\u00b7ge\u00b7samt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Gantz liebreich, und wie nun die Aufbruchsstund berahmt,", "tokens": ["Gantz", "lieb\u