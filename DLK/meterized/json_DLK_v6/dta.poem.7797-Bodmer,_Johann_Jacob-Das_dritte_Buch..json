{"dta.poem.7797": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Das dritte Buch.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als nun die letzte Nacht nach aller ihr Verlangen", "tokens": ["Als", "nun", "die", "letz\u00b7te", "Nacht", "nach", "al\u00b7ler", "ihr", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Tag gewichen war, darinnen sollt empfangen", "tokens": ["Dem", "Tag", "ge\u00b7wi\u00b7chen", "war", ",", "da\u00b7rin\u00b7nen", "sollt", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das frohe Gibea den K\u00f6nig und das Heer,", "tokens": ["Das", "fro\u00b7he", "Gi\u00b7bea", "den", "K\u00f6\u00b7nig", "und", "das", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Kam kaum die Morgenr\u00f6th in ihrem Purpurmeer", "tokens": ["Kam", "kaum", "die", "Mor\u00b7gen\u00b7r\u00f6th", "in", "ih\u00b7rem", "Pur\u00b7pur\u00b7meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit munterm Angesicht daher, die Welt zu gr\u00fcssen,", "tokens": ["Mit", "mun\u00b7term", "An\u00b7ge\u00b7sicht", "da\u00b7her", ",", "die", "Welt", "zu", "gr\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PAV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da ist schon jedermann auf seinen Schmuck beflissen.", "tokens": ["Da", "ist", "schon", "je\u00b7der\u00b7mann", "auf", "sei\u00b7nen", "Schmuck", "be\u00b7flis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das k\u00f6stliche Gewand, da vor Gath mit geprangt,", "tokens": ["Das", "k\u00f6st\u00b7li\u00b7che", "Ge\u00b7wand", ",", "da", "vor", "Gath", "mit", "ge\u00b7prangt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "APPR", "NN", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das wird Jsrael heut zum Preise umgehangt.", "tokens": ["Das", "wird", "Js\u00b7rael", "heut", "zum", "Prei\u00b7se", "um\u00b7ge\u00b7hangt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Was Sidon sch\u00f6n gew\u00fcrckt, was Tiro zugerichtet,", "tokens": ["Was", "Si\u00b7don", "sch\u00f6n", "ge\u00b7w\u00fcrckt", ",", "was", "Ti\u00b7ro", "zu\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "ADJD", "VVPP", "$,", "PRELS", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da Ecron mit geprahlt, hat so das Gl\u00fcck geschlichtet,", "tokens": ["Da", "E\u00b7cron", "mit", "ge\u00b7prahlt", ",", "hat", "so", "das", "Gl\u00fcck", "ge\u00b7schlich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "VVPP", "$,", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df der Ebr\u00e4er Sieg damit wird hoch beziert,", "tokens": ["Da\u00df", "der", "Eb\u00b7r\u00e4\u00b7er", "Sieg", "da\u00b7mit", "wird", "hoch", "be\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PAV", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Und alles im Triumph nach Gibea gef\u00fchrt.", "tokens": ["Und", "al\u00b7les", "im", "Tri\u00b7umph", "nach", "Gi\u00b7bea", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPRART", "NN", "APPR", "NE", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Saul der erfreute Saul war gleich als neu gebohren,", "tokens": ["Saul", "der", "er\u00b7freu\u00b7te", "Saul", "war", "gleich", "als", "neu", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VAFIN", "ADV", "KOKOM", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sein Muth brach wieder an, den er vorhin verlohren,", "tokens": ["Sein", "Muth", "brach", "wie\u00b7der", "an", ",", "den", "er", "vor\u00b7hin", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PTKVZ", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.15": {"text": "Die Ehre war so s\u00fc\u00df, da\u00df als ein Siegesmann", "tokens": ["Die", "Eh\u00b7re", "war", "so", "s\u00fc\u00df", ",", "da\u00df", "als", "ein", "Sie\u00b7ges\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Jhn heut sein K\u00f6nigreich soll freudig schauen an.", "tokens": ["Jhn", "heut", "sein", "K\u00f6\u00b7nig\u00b7reich", "soll", "freu\u00b7dig", "schau\u00b7en", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "VMFIN", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Woraus ihm Ruhm entstand, war seiner Ruhe Zunder,", "tokens": ["Wo\u00b7raus", "ihm", "Ruhm", "ent\u00b7stand", ",", "war", "sei\u00b7ner", "Ru\u00b7he", "Zun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Durch diesen grossen Sieg ward sein Hertz wieder munter,", "tokens": ["Durch", "die\u00b7sen", "gros\u00b7sen", "Sieg", "ward", "sein", "Hertz", "wie\u00b7der", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Da\u00df er auch nichtes lie\u00df ermangeln an dem Pracht,", "tokens": ["Da\u00df", "er", "auch", "nich\u00b7tes", "lie\u00df", "er\u00b7man\u00b7geln", "an", "dem", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VVFIN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der ihm bey allem Volck ein grosses Ansehn macht.", "tokens": ["Der", "ihm", "bey", "al\u00b7lem", "Volck", "ein", "gros\u00b7ses", "An\u00b7sehn", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIS", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der Abner kriegt Befehl, wie sollt der Einzug gehen,", "tokens": ["Der", "Ab\u00b7ner", "kriegt", "Be\u00b7fehl", ",", "wie", "sollt", "der", "Ein\u00b7zug", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,", "PWAV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und da\u00df er aller Welt m\u00f6cht geben zu verstehen,", "tokens": ["Und", "da\u00df", "er", "al\u00b7ler", "Welt", "m\u00f6cht", "ge\u00b7ben", "zu", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIAT", "NN", "VMFIN", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wie lieb ihm David sey, so soll in Ritters Zier", "tokens": ["Wie", "lieb", "ihm", "Da\u00b7vid", "sey", ",", "so", "soll", "in", "Rit\u00b7ters", "Zier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "NE", "VAFIN", "$,", "ADV", "VMFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der junge k\u00fchne Held dem K\u00f6nig reiten f\u00fcr.", "tokens": ["Der", "jun\u00b7ge", "k\u00fch\u00b7ne", "Held", "dem", "K\u00f6\u00b7nig", "rei\u00b7ten", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ART", "NN", "VVFIN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Er sendet ihm ein Ro\u00df, das Nilus eh erzogen,", "tokens": ["Er", "sen\u00b7det", "ihm", "ein", "Ro\u00df", ",", "das", "Ni\u00b7lus", "eh", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "NE", "KOUS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und seinen Kleiderschmuck, den Helm, Gewehr und Bogen,", "tokens": ["Und", "sei\u00b7nen", "Klei\u00b7der\u00b7schmuck", ",", "den", "Helm", ",", "Ge\u00b7wehr", "und", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Durch Doeg; der verricht was ihm befohlen war,", "tokens": ["Durch", "Doeg", ";", "der", "ver\u00b7richt", "was", "ihm", "be\u00b7foh\u00b7len", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "ART", "VVFIN", "PWS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Die K\u00f6nigliche Gab die reicht er David dar.", "tokens": ["Die", "K\u00f6\u00b7nig\u00b7li\u00b7che", "Gab", "die", "reicht", "er", "Da\u00b7vid", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "VVFIN", "PPER", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Mit Ehrerbietung nimmt derselbe die\u00df Geschencke", "tokens": ["Mit", "Ehr\u00b7er\u00b7bie\u00b7tung", "nimmt", "der\u00b7sel\u00b7be", "die\u00df", "Ge\u00b7schen\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PDAT", "PDS", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Von Doegs H\u00e4nden an, und sagt wie er gedencke.", "tokens": ["Von", "Doegs", "H\u00e4n\u00b7den", "an", ",", "und", "sagt", "wie", "er", "ge\u00b7den\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PTKVZ", "$,", "KON", "VVFIN", "KOKOM", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.31": {"text": "Dem K\u00f6nig darzuthun mit unverdro\u00dfnem Flei\u00df,", "tokens": ["Dem", "K\u00f6\u00b7nig", "dar\u00b7zu\u00b7thun", "mit", "un\u00b7ver\u00b7dro\u00df\u00b7nem", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df er so hoher Gnad nicht unerk\u00e4nntlich hei\u00df.", "tokens": ["Da\u00df", "er", "so", "ho\u00b7her", "Gnad", "nicht", "un\u00b7er\u00b7k\u00e4nnt\u00b7lich", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "PTKNEG", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Es ware Jonathan zugegen, dem beliebte,", "tokens": ["Es", "wa\u00b7re", "Jo\u00b7na\u00b7than", "zu\u00b7ge\u00b7gen", ",", "dem", "be\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "ADJD", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Da\u00df solch ein Werck der Gunst Saul an dem David \u00fcbte,", "tokens": ["Da\u00df", "solch", "ein", "Werck", "der", "Gunst", "Saul", "an", "dem", "Da\u00b7vid", "\u00fcb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ART", "NN", "ART", "NN", "NE", "APPR", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Den er liebt als sich selbst, und ist er nun bem\u00fcht", "tokens": ["Den", "er", "liebt", "als", "sich", "selbst", ",", "und", "ist", "er", "nun", "be\u00b7m\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "KOUS", "PRF", "ADV", "$,", "KON", "VAFIN", "PPER", "ADV", "VVFIN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.36": {"text": "Da\u00df David diesen Schmuck in seinem Zelt anzieht.", "tokens": ["Da\u00df", "Da\u00b7vid", "die\u00b7sen", "Schmuck", "in", "sei\u00b7nem", "Zelt", "an\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wie dieses nun verricht, hebt an der Printz zu sagen,", "tokens": ["Wie", "die\u00b7ses", "nun", "ver\u00b7richt", ",", "hebt", "an", "der", "Printz", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADV", "VVPP", "$,", "VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wie er niemahlen nicht in seinen Lebenstagen", "tokens": ["Wie", "er", "nie\u00b7mah\u00b7len", "nicht", "in", "sei\u00b7nen", "Le\u00b7bens\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Was lieblichers gesehn, es w\u00fcrd der Nymphenschaar", "tokens": ["Was", "lieb\u00b7li\u00b7chers", "ge\u00b7sehn", ",", "es", "w\u00fcrd", "der", "Nym\u00b7phen\u00b7schaar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVPP", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "An Davids Wunderglantz sich heut verblenden gar,", "tokens": ["An", "Da\u00b7vids", "Wun\u00b7der\u00b7glantz", "sich", "heut", "ver\u00b7blen\u00b7den", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PRF", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ja Merob w\u00fcrd sich gleich als Goliath ergeben,", "tokens": ["Ja", "Me\u00b7rob", "w\u00fcrd", "sich", "gleich", "als", "Go\u00b7li\u00b7ath", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NE", "VAFIN", "PRF", "ADV", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Mit nichten seiner Kraft und Liebe wiederstreben.", "t