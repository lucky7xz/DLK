{"dta.poem.8591": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf einem Berg aus Zuckerkant,", "tokens": ["Auf", "ei\u00b7nem", "Berg", "aus", "Zu\u00b7cker\u00b7kant", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "unter einem bl\u00fchenden Machandelbaum,", "tokens": ["un\u00b7ter", "ei\u00b7nem", "bl\u00fc\u00b7hen\u00b7den", "Ma\u00b7chan\u00b7del\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "blinkt mein Pfefferkuchenh\u00e4uschen.", "tokens": ["blinkt", "mein", "Pfef\u00b7fer\u00b7ku\u00b7chen\u00b7h\u00e4usc\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seine Fensterchen sind aus Goldpapier,", "tokens": ["Sei\u00b7ne", "Fens\u00b7ter\u00b7chen", "sind", "aus", "Gold\u00b7pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "aus seinem Schornstein raucht Watte.", "tokens": ["aus", "sei\u00b7nem", "Schorn\u00b7stein", "raucht", "Wat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Im gr\u00fcnen Himmel, \u00fcber mir, rauscht die Weihnachtstanne.", "tokens": ["Im", "gr\u00fc\u00b7nen", "Him\u00b7mel", ",", "\u00fc\u00b7ber", "mir", ",", "rauscht", "die", "Weih\u00b7nachts\u00b7tan\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "PPER", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "In meinem See aus Staniol", "tokens": ["In", "mei\u00b7nem", "See", "aus", "Sta\u00b7ni\u00b7ol"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "spiegeln sich alle ihre Engel, alle ihre Lichter!", "tokens": ["spie\u00b7geln", "sich", "al\u00b7le", "ih\u00b7re", "En\u00b7gel", ",", "al\u00b7le", "ih\u00b7re", "Lich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "PPOSAT", "NN", "$,", "PIAT", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}}, "stanza.5": {"line.1": {"text": "Die kleinen Kinder stehn rum", "tokens": ["Die", "klei\u00b7nen", "Kin\u00b7der", "stehn", "rum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "und staunen mich an.", "tokens": ["und", "stau\u00b7nen", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Ich bin der Zwerg Turlitipu.", "tokens": ["Ich", "bin", "der", "Zwerg", "Tur\u00b7li\u00b7ti\u00b7pu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Mein dicker Bauch ist aus Traganth,", "tokens": ["Mein", "di\u00b7cker", "Bauch", "ist", "aus", "Tra\u00b7gan\u00b7th", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "meine Beinchen Streichh\u00f6lzer,", "tokens": ["mei\u00b7ne", "Be\u00b7in\u00b7chen", "Streich\u00b7h\u00f6l\u00b7zer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.3": {"text": "meine listigen Aeugelchen", "tokens": ["mei\u00b7ne", "lis\u00b7ti\u00b7gen", "A\u00b7e\u00b7u\u00b7gel\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Korinthen.", "tokens": ["Ko\u00b7rin\u00b7then", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}