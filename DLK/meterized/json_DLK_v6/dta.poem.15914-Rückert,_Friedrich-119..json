{"dta.poem.15914": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "119.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du h\u00f6rst ein Buch als gut von guten Freunden preisen,", "tokens": ["Du", "h\u00f6rst", "ein", "Buch", "als", "gut", "von", "gu\u00b7ten", "Freun\u00b7den", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und preisest nun als gut es auch in Freundeskreisen.", "tokens": ["Und", "prei\u00b7sest", "nun", "als", "gut", "es", "auch", "in", "Freun\u00b7des\u00b7krei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "KOUS", "ADJD", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hast du's gelesen? Nein! Und h\u00e4ttest du's gelesen,", "tokens": ["Hast", "du's", "ge\u00b7le\u00b7sen", "?", "Nein", "!", "Und", "h\u00e4t\u00b7test", "du's", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "$.", "PTKANT", "$.", "KON", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du w\u00fc\u00dftest auch nicht mehr, als da\u00df es gut gewesen.", "tokens": ["Du", "w\u00fc\u00df\u00b7test", "auch", "nicht", "mehr", ",", "als", "da\u00df", "es", "gut", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADV", "$,", "KOKOM", "KOUS", "PPER", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum es zu lesen kannst du Zeit und M\u00fche sparen;", "tokens": ["Drum", "es", "zu", "le\u00b7sen", "kannst", "du", "Zeit", "und", "M\u00fc\u00b7he", "spa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PTKZU", "VVINF", "VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du hast genug davon zu deinem Zweck erfahren.", "tokens": ["Du", "hast", "ge\u00b7nug", "da\u00b7von", "zu", "dei\u00b7nem", "Zweck", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PAV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}