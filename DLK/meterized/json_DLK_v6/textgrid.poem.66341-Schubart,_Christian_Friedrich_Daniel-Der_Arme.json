{"textgrid.poem.66341": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der Arme", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott, wie lange mu\u00df ich darben!", "tokens": ["Gott", ",", "wie", "lan\u00b7ge", "mu\u00df", "ich", "dar\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewig gl\u00fccklich sind die nun,", "tokens": ["E\u00b7wig", "gl\u00fcck\u00b7lich", "sind", "die", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die vor mir in Frieden starben,", "tokens": ["Die", "vor", "mir", "in", "Frie\u00b7den", "star\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um vom Elend auszuruhn.", "tokens": ["Um", "vom", "E\u00b7lend", "aus\u00b7zu\u00b7ruhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00fclfe, willst du lange s\u00e4umen?", "tokens": ["H\u00fcl\u00b7fe", ",", "willst", "du", "lan\u00b7ge", "s\u00e4u\u00b7men", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halb verschmachtet steh' ich hier;", "tokens": ["Halb", "ver\u00b7schmach\u00b7tet", "steh'", "ich", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Goldne Fr\u00fcchte an den B\u00e4umen,", "tokens": ["Gold\u00b7ne", "Fr\u00fcch\u00b7te", "an", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reicher Herbst, was helft ihr mir?", "tokens": ["Rei\u00b7cher", "Herbst", ",", "was", "helft", "ihr", "mir", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWS", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bauern sammlen in die Scheune", "tokens": ["Bau\u00b7ern", "samm\u00b7len", "in", "die", "Scheu\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Korn und Waizen auf, wie Sand:", "tokens": ["Korn", "und", "Wai\u00b7zen", "auf", ",", "wie", "Sand", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKVZ", "$,", "PWAV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber wenn ich Armer weine,", "tokens": ["A\u00b7ber", "wenn", "ich", "Ar\u00b7mer", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So verschlie\u00dfen sie die Hand.", "tokens": ["So", "ver\u00b7schlie\u00b7\u00dfen", "sie", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Reiche rasseln mit dem Wagen,", "tokens": ["Rei\u00b7che", "ras\u00b7seln", "mit", "dem", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fett vom Haber ist ihr Pferd;", "tokens": ["Fett", "vom", "Ha\u00b7ber", "ist", "ihr", "Pferd", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rasselt nur, da\u00df ihr die Klagen", "tokens": ["Ras\u00b7selt", "nur", ",", "da\u00df", "ihr", "die", "Kla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eines armen Manns nicht h\u00f6rt.", "tokens": ["Ei\u00b7nes", "ar\u00b7men", "Manns", "nicht", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Knabe, den mir Gott gegeben,", "tokens": ["Kna\u00b7be", ",", "den", "mir", "Gott", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sein Elend noch nicht f\u00fchlt,", "tokens": ["Der", "sein", "E\u00b7lend", "noch", "nicht", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seh' ich dich im Herbstwind beben,", "tokens": ["Seh'", "ich", "dich", "im", "Herbst\u00b7wind", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der mit deinen Lumpen spielt:", "tokens": ["Der", "mit", "dei\u00b7nen", "Lum\u00b7pen", "spielt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "O! dann gr\u00e4m' ich mich am Stabe,", "tokens": ["O", "!", "dann", "gr\u00e4m'", "ich", "mich", "am", "Sta\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6re dein Geschrei nach Brod,", "tokens": ["H\u00f6\u00b7re", "dein", "Ge\u00b7schrei", "nach", "Brod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seufz' im Stillen: armer Knabe,", "tokens": ["Seuf\u00b7z'", "im", "Stil\u00b7len", ":", "ar\u00b7mer", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$.", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "W\u00e4rst du todt; ach w\u00e4rst du todt!", "tokens": ["W\u00e4rst", "du", "todt", ";", "ach", "w\u00e4rst", "du", "todt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$.", "XY", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Menschen, ist denn kein Erbarmen,", "tokens": ["Men\u00b7schen", ",", "ist", "denn", "kein", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kein Erbarmen unter euch?", "tokens": ["Kein", "Er\u00b7bar\u00b7men", "un\u00b7ter", "euch", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind die D\u00fcrftigen, die Armen,", "tokens": ["Sind", "die", "D\u00fcrf\u00b7ti\u00b7gen", ",", "die", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch an Fleisch und Blut nicht gleich?", "tokens": ["Euch", "an", "Fleisch", "und", "Blut", "nicht", "gleich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "O so werft, wie euren Hunden,", "tokens": ["O", "so", "werft", ",", "wie", "eu\u00b7ren", "Hun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir nur einen Bissen zu!", "tokens": ["Mir", "nur", "ei\u00b7nen", "Bis\u00b7sen", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch wer Armuth nie empfunden,", "tokens": ["Doch", "wer", "Ar\u00b7muth", "nie", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00df es nicht, wie weh' sie thu'.", "tokens": ["Wei\u00df", "es", "nicht", ",", "wie", "weh'", "sie", "thu'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Gott, so mu\u00df ich ewig darben,", "tokens": ["Gott", ",", "so", "mu\u00df", "ich", "e\u00b7wig", "dar\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VMFIN", "PPER", "ADJD", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O wie gl\u00fccklich sind die nun,", "tokens": ["O", "wie", "gl\u00fcck\u00b7lich", "sind", "die", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "ART", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die vor mir im Frieden starben,", "tokens": ["Die", "vor", "mir", "im", "Frie\u00b7den", "star\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Um vom Elend auszuruhn!", "tokens": ["Um", "vom", "E\u00b7lend", "aus\u00b7zu\u00b7ruhn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}