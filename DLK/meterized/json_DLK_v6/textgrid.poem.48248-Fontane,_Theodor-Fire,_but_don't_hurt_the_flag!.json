{"textgrid.poem.48248": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Fire, but don't hurt the flag!", "genre": "verse", "period": "N.A.", "pub_year": 1895, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Konsul Cunningham, an die drei\u00dfig Jahr", "tokens": ["Kon\u00b7sul", "Cun\u00b7ning\u00b7ham", ",", "an", "die", "drei\u00b7\u00dfig", "Jahr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "APPR", "ART", "CARD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist er im Amt schon in Tulcahuar.", "tokens": ["Ist", "er", "im", "Amt", "schon", "in", "Tul\u00b7ca\u00b7hu\u00b7ar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein chilenischer Tag heut; stahlblau die Luft,", "tokens": ["Ein", "chi\u00b7le\u00b7ni\u00b7scher", "Tag", "heut", ";", "stahl\u00b7blau", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$.", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von Westen her weht es wie Meeresduft,", "tokens": ["Von", "Wes\u00b7ten", "her", "weht", "es", "wie", "Mee\u00b7res\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APZR", "VVFIN", "PPER", "KOKOM", "NN", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und auf Cunninghams Hause, leis und lind,", "tokens": ["Und", "auf", "Cun\u00b7ning\u00b7hams", "Hau\u00b7se", ",", "leis", "und", "lind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Englands Flagge spielt im Wind.", "tokens": ["En\u00b7glands", "Flag\u00b7ge", "spielt", "im", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jetzt aber, ein Windsto\u00df setzt eben ein,", "tokens": ["Jetzt", "a\u00b7ber", ",", "ein", "Wind\u00b7sto\u00df", "setzt", "e\u00b7ben", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Klingt's die Stra\u00dfe herauf wie von L\u00e4rmen und Schrei'n,", "tokens": ["Klingt's", "die", "Stra\u00b7\u00dfe", "her\u00b7auf", "wie", "von", "L\u00e4r\u00b7men", "und", "Schrei'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "KOKOM", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Soldaten und Volk (\u00bbist der Teufel los?\u00ab)", "tokens": ["Sol\u00b7da\u00b7ten", "und", "Volk", "(", "\u00bb", "ist", "der", "Teu\u00b7fel", "los", "?", "\u00ab", ")"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "KON", "NN", "$(", "$(", "VAFIN", "ART", "NN", "PTKVZ", "$.", "$(", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und inmitten des Haufens ein brit'scher Matros.", "tokens": ["Und", "in\u00b7mit\u00b7ten", "des", "Hau\u00b7fens", "ein", "brit'\u00b7scher", "Mat\u00b7ros", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "An schwillt das Gel\u00e4rm, und als n\u00e4her es kam,", "tokens": ["An", "schwillt", "das", "Ge\u00b7l\u00e4rm", ",", "und", "als", "n\u00e4\u00b7her", "es", "kam", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$,", "KON", "KOUS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf die Stra\u00dfe hinaus tritt Cunningham,", "tokens": ["Auf", "die", "Stra\u00b7\u00dfe", "hin\u00b7aus", "tritt", "Cun\u00b7ning\u00b7ham", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APZR", "VVFIN", "NE", "$,"], "meter": "+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Engl\u00e4nder der Alte, von Kopf zu Zeh,", "tokens": ["En\u00b7gl\u00e4n\u00b7der", "der", "Al\u00b7te", ",", "von", "Kopf", "zu", "Zeh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "APPR", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Glatt, rosig, sein sp\u00e4rliches Haar wie Schnee,", "tokens": ["Glatt", ",", "ro\u00b7sig", ",", "sein", "sp\u00e4r\u00b7li\u00b7ches", "Haar", "wie", "Schnee", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "KOKOM", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Dazu, nach britischem Brauch und Geschmack,", "tokens": ["Da\u00b7zu", ",", "nach", "bri\u00b7ti\u00b7schem", "Brauch", "und", "Ge\u00b7schmack", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "In wei\u00dfem Gilet und schwarzem Frack.", "tokens": ["In", "wei\u00b7\u00dfem", "Gi\u00b7let", "und", "schwar\u00b7zem", "Frack", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Trommeln wirbeln, die Pfeife gellt,", "tokens": ["Trom\u00b7meln", "wir\u00b7beln", ",", "die", "Pfei\u00b7fe", "gellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und als der Zug vor dem Hause jetzt h\u00e4lt,", "tokens": ["Und", "als", "der", "Zug", "vor", "dem", "Hau\u00b7se", "jetzt", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Matrose tritt vor: \u00bbHerr, bin in Not,", "tokens": ["Der", "Mat\u00b7ro\u00b7se", "tritt", "vor", ":", "\u00bb", "Herr", ",", "bin", "in", "Not", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "$(", "NN", "$,", "VAFIN", "APPR", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erbarmt Euch, sie schleppen mich in den Tod,", "tokens": ["Er\u00b7barmt", "Euch", ",", "sie", "schlep\u00b7pen", "mich", "in", "den", "Tod", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Chilenisch Volk, es klagt mich an,", "tokens": ["Chi\u00b7le\u00b7nisch", "Volk", ",", "es", "klagt", "mich", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Ein andrer f\u00fchrte Sto\u00df und Stich,", "tokens": ["Ein", "an\u00b7drer", "f\u00fchr\u00b7te", "Sto\u00df", "und", "Stich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Unschuldig bin ich, rettet mich!\u00ab", "tokens": ["Un\u00b7schul\u00b7dig", "bin", "ich", ",", "ret\u00b7tet", "mich", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein Murmeln, ein Murren. Noch h\u00e4lt der Hauf,", "tokens": ["Ein", "Mur\u00b7meln", ",", "ein", "Mur\u00b7ren", ".", "Noch", "h\u00e4lt", "der", "Hauf", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Konsul Cunnigham steigt auf das Flachdach hinauf,", "tokens": ["Kon\u00b7sul", "Cun\u00b7nig\u00b7ham", "steigt", "auf", "das", "Flach\u00b7dach", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Auf dem Flachdach oben, leis und lind,", "tokens": ["Auf", "dem", "Flach\u00b7dach", "o\u00b7ben", ",", "leis", "und", "lind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Englands Flagge spielt im Wind;", "tokens": ["En\u00b7glands", "Flag\u00b7ge", "spielt", "im", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schlingt er ruhig-bed\u00e4chtig den Union-Jack,", "tokens": ["Schlingt", "er", "ru\u00b7hig\u00b7be\u00b7d\u00e4ch\u00b7tig", "den", "U\u00b7ni\u00b7o\u00b7nJack", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Dann wieder treppabw\u00e4rts: \u00bbNun la\u00dft uns gehn.", "tokens": ["Dann", "wie\u00b7der", "tre\u00b7ppab\u00b7w\u00e4rts", ":", "\u00bb", "Nun", "la\u00dft", "uns", "gehn", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$.", "$(", "ADV", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich will dich begleiten. Wir wollen sehn.\u00ab", "tokens": ["Ich", "will", "dich", "be\u00b7glei\u00b7ten", ".", "Wir", "wol\u00b7len", "sehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$.", "PPER", "VMFIN", "VVINF", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Und drau\u00dfen, auf dem H\u00fcgel von Sand,", "tokens": ["Und", "drau\u00b7\u00dfen", ",", "auf", "dem", "H\u00fc\u00b7gel", "von", "Sand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In des Todes Aug' der Matrose stand,", "tokens": ["In", "des", "To\u00b7des", "Aug'", "der", "Mat\u00b7ro\u00b7se", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Peloton tritt vor, schon schlagen sie an,", "tokens": ["Pe\u00b7lo\u00b7ton", "tritt", "vor", ",", "schon", "schla\u00b7gen", "sie", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Da, \u00fcber den verlorenen Mann", "tokens": ["Da", ",", "\u00fc\u00b7ber", "den", "ver\u00b7lo\u00b7re\u00b7nen", "Mann"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Wirft der Konsul das Flaggtuch: \u00bbNun schie\u00dfe, wer mag;", "tokens": ["Wirft", "der", "Kon\u00b7sul", "das", "Flag\u00b7gtuch", ":", "\u00bb", "Nun", "schie\u00b7\u00dfe", ",", "wer", "mag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$.", "$(", "ADV", "VVFIN", "$,", "PWS", "VMFIN", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Fire, but don't hurt the flag!\u00ab", "tokens": ["Fi\u00b7re", ",", "but", "don't", "hurt", "the", "flag", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "FM", "FM", "FM", "FM", "FM", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Da senken die Gewehre sich still,", "tokens": ["Da", "sen\u00b7ken", "die", "Ge\u00b7weh\u00b7re", "sich", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "+----+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Keiner, der es wagen will.", "tokens": ["Kei\u00b7ner", ",", "der", "es", "wa\u00b7gen", "will", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wann kommt auch f\u00fcr uns der goldne Tag:", "tokens": ["Wann", "kommt", "auch", "f\u00fcr", "uns", "der", "gold\u00b7ne", "Tag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Fire, but don't hurt the flag!", "tokens": ["Fi\u00b7re", ",", "but", "don't", "hurt", "the", "flag", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}