{"dta.poem.4267": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Le Cercle.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Eine Zirkel- runde Stelle, weit vom Umfang, und", "tokens": ["Ei\u00b7ne", "Zir\u00b7kel", "run\u00b7de", "Stel\u00b7le", ",", "weit", "vom", "Um\u00b7fang", ",", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "TRUNC", "ADJA", "NN", "$,", "ADJD", "APPRART", "NN", "$,", "KON"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "geraum,", "tokens": ["ge\u00b7raum", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Deren sch\u00f6ner Mittelpunct ein bejahrter Eichen-Baum,", "tokens": ["De\u00b7ren", "sch\u00f6\u00b7ner", "Mit\u00b7tel\u00b7punct", "ein", "be\u00b7jahr\u00b7ter", "Ei\u00b7chen\u00b7Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Bietet eine runde Bank gr\u00fcner Rasen uns zum Sitze,", "tokens": ["Bie\u00b7tet", "ei\u00b7ne", "run\u00b7de", "Bank", "gr\u00fc\u00b7ner", "Ra\u00b7sen", "uns", "zum", "Sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Und ein sch\u00f6nes Bl\u00e4tter-Zelt uns zum Schirm-Dach f\u00fcr", "tokens": ["Und", "ein", "sch\u00f6\u00b7nes", "Bl\u00e4t\u00b7ter\u00b7Zelt", "uns", "zum", "Schir\u00b7mDach", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "PPER", "APPRART", "NN", "APPR"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "die Hitze,", "tokens": ["die", "Hit\u00b7ze", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Voller k\u00fchlen Schatten, dar. Hier in diesen holden", "tokens": ["Vol\u00b7ler", "k\u00fch\u00b7len", "Schat\u00b7ten", ",", "dar", ".", "Hier", "in", "die\u00b7sen", "hol\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "$,", "PTKVZ", "$.", "ADV", "APPR", "PDAT", "ADJA"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Schranken", "tokens": ["Schran\u00b7ken"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Dick-verwachsener Geb\u00fcsche k\u00f6nnen ruhige Gedanken,", "tokens": ["Dick\u00b7ver\u00b7wach\u00b7se\u00b7ner", "Ge\u00b7b\u00fc\u00b7sche", "k\u00f6n\u00b7nen", "ru\u00b7hi\u00b7ge", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "ADJA", "NN", "$,"], "meter": "+-+---+-+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Mit recht innigem Vergn\u00fcgen, ungest\u00f6ret, wie so sch\u00f6n,", "tokens": ["Mit", "recht", "in\u00b7ni\u00b7gem", "Ver\u00b7gn\u00fc\u00b7gen", ",", "un\u00b7ge\u00b7st\u00f6\u00b7ret", ",", "wie", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "ADJD", "$,", "PWAV", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Des allm\u00e4chtig- weisen Sch\u00f6pfers liebliches Gesch\u00f6pfe,", "tokens": ["Des", "all\u00b7m\u00e4ch\u00b7tig", "wei\u00b7sen", "Sch\u00f6p\u00b7fers", "lieb\u00b7li\u00b7ches", "Ge\u00b7sch\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "seh'n,", "tokens": ["seh'n", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Seine Liebe gleichsam schmecken, und, im angenehmen", "tokens": ["Sei\u00b7ne", "Lie\u00b7be", "gleich\u00b7sam", "schme\u00b7cken", ",", "und", ",", "im", "an\u00b7ge\u00b7neh\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$,", "KON", "$,", "APPRART", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.14": {"text": "K\u00fchlen,", "tokens": ["K\u00fch\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Seiner Creatur geniessen, Seine G\u00fcte gleichsam f\u00fchlen.", "tokens": ["Sei\u00b7ner", "Crea\u00b7tur", "ge\u00b7nies\u00b7sen", ",", "Sei\u00b7ne", "G\u00fc\u00b7te", "gleich\u00b7sam", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.16": {"text": "M\u00f6chte man Dem, welcher Gras, Laub und Kr\u00e4uter", "tokens": ["M\u00f6ch\u00b7te", "man", "Dem", ",", "wel\u00b7cher", "Gras", ",", "Laub", "und", "Kr\u00e4u\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PDS", "$,", "PWAT", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.17": {"text": "schuf, zum Preise,", "tokens": ["schuf", ",", "zum", "Prei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "Mit beruhigten Gedanken, hier in diesem holden Kreise,", "tokens": ["Mit", "be\u00b7ru\u00b7hig\u00b7ten", "Ge\u00b7dan\u00b7ken", ",", "hier", "in", "die\u00b7sem", "hol\u00b7den", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-++--+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.19": {"text": "Von Betrachtung und von Lust nur begleitet, oft sich setzen,", "tokens": ["Von", "Be\u00b7trach\u00b7tung", "und", "von", "Lust", "nur", "be\u00b7glei\u00b7tet", ",", "oft", "sich", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ADV", "VVPP", "$,", "ADV", "PRF", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Und an den bewundernswehrten, k\u00fchlen, gr\u00fcn- und bunten", "tokens": ["Und", "an", "den", "be\u00b7wun\u00b7derns\u00b7wehr\u00b7ten", ",", "k\u00fch\u00b7len", ",", "gr\u00fcn", "und", "bun\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "VVINF", "$,", "TRUNC", "KON", "ADJA"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Sch\u00e4tzen,", "tokens": ["Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Voller innerlichen Lust, voll Vergn\u00fcgen sich ergetzen;", "tokens": ["Vol\u00b7ler", "in\u00b7ner\u00b7li\u00b7chen", "Lust", ",", "voll", "Ver\u00b7gn\u00fc\u00b7gen", "sich", "er\u00b7get\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "ADJD", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.23": {"text": "W\u00fcrde, durch den kleinen Kreis, uns vielleicht der Kreis", "tokens": ["W\u00fcr\u00b7de", ",", "durch", "den", "klei\u00b7nen", "Kreis", ",", "uns", "viel\u00b7leicht", "der", "Kreis"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "der Erden,", "tokens": ["der", "Er\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Als zu GOtt die andre Staffel, ein beliebter Vorwurf", "tokens": ["Als", "zu", "Gott", "die", "and\u00b7re", "Staf\u00b7fel", ",", "ein", "be\u00b7lieb\u00b7ter", "Vor\u00b7wurf"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.26": {"text": "werden.", "tokens": ["wer\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}