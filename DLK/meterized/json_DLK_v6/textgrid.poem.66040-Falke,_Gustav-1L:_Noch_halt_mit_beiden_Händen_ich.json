{"textgrid.poem.66040": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch halt mit beiden H\u00e4nden ich", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch halt mit beiden H\u00e4nden ich", "tokens": ["Noch", "halt", "mit", "bei\u00b7den", "H\u00e4n\u00b7den", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lebens sch\u00f6ne Schale fest,", "tokens": ["Des", "Le\u00b7bens", "sch\u00f6\u00b7ne", "Scha\u00b7le", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch trink und kann nicht enden ich", "tokens": ["Noch", "trink", "und", "kann", "nicht", "en\u00b7den", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VMFIN", "PTKNEG", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und denk nicht an den letzten Rest.", "tokens": ["Und", "denk", "nicht", "an", "den", "letz\u00b7ten", "Rest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbdoch einmal wird die Schale leer,", "tokens": ["\u00bb", "doch", "ein\u00b7mal", "wird", "die", "Scha\u00b7le", "leer", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die letzte Neige schl\u00fcrftest du.\u00ab", "tokens": ["Die", "letz\u00b7te", "Nei\u00b7ge", "schl\u00fcrf\u00b7test", "du", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So trank ich doch, was will ich mehr,", "tokens": ["So", "trank", "ich", "doch", ",", "was", "will", "ich", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWS", "VMFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Tod ein volles Leben zu.", "tokens": ["Dem", "Tod", "ein", "vol\u00b7les", "Le\u00b7ben", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}