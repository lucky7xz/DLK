{"textgrid.poem.41561": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am zw\u00f6lften Sonntage nach Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, wenn ich schaue deine Opferflamme", "tokens": ["Ja", ",", "wenn", "ich", "schau\u00b7e", "dei\u00b7ne", "Op\u00b7fer\u00b7flam\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In eines frommen Auges reiner Glut,", "tokens": ["In", "ei\u00b7nes", "from\u00b7men", "Au\u00b7ges", "rei\u00b7ner", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann schimmert es, als ob es mich verdamme;", "tokens": ["Dann", "schim\u00b7mert", "es", ",", "als", "ob", "es", "mich", "ver\u00b7dam\u00b7me", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOKOM", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der scharfe Strahl f\u00e4hrt in mein schuldig Blut.", "tokens": ["Der", "schar\u00b7fe", "Strahl", "f\u00e4hrt", "in", "mein", "schul\u00b7dig", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie blendet mich das Licht!", "tokens": ["Wie", "blen\u00b7det", "mich", "das", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Augen darf ich nicht erheben;", "tokens": ["Die", "Au\u00b7gen", "darf", "ich", "nicht", "er\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich darf es nicht,", "tokens": ["Ich", "darf", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Und meine Wimper beben.", "tokens": ["Und", "mei\u00b7ne", "Wim\u00b7per", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und unter den gcschlo\u00dfnen Lidern fahren", "tokens": ["Und", "un\u00b7ter", "den", "gcschlo\u00df\u00b7nen", "Li\u00b7dern", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Schatten alter S\u00fcnden hin und her.", "tokens": ["Die", "Schat\u00b7ten", "al\u00b7ter", "S\u00fcn\u00b7den", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was dann sich mu\u00df dem Hirne offenbaren,", "tokens": ["Was", "dann", "sich", "mu\u00df", "dem", "Hir\u00b7ne", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PRF", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O meinem Feinde werd' es nicht so schwer!", "tokens": ["O", "mei\u00b7nem", "Fein\u00b7de", "werd'", "es", "nicht", "so", "schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Aus Grund und W\u00e4nden auch", "tokens": ["Aus", "Grund", "und", "W\u00e4n\u00b7den", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sie dampfen, schweben durch die Zimmer,", "tokens": ["Sie", "damp\u00b7fen", ",", "schwe\u00b7ben", "durch", "die", "Zim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gebild' aus Rauch;", "tokens": ["Ge\u00b7bild'", "aus", "Rauch", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "So war und bleibt es immer.", "tokens": ["So", "war", "und", "bleibt", "es", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn eine milde Tat ich seh vollbringen,", "tokens": ["Wenn", "ei\u00b7ne", "mil\u00b7de", "Tat", "ich", "seh", "voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So recht aus \u00fcbervollen Herzens Grund,", "tokens": ["So", "recht", "aus", "\u00fc\u00b7ber\u00b7vol\u00b7len", "Her\u00b7zens", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So klar die hei\u00dfen Liebesquellen springen,", "tokens": ["So", "klar", "die", "hei\u00b7\u00dfen", "Lie\u00b7bes\u00b7quel\u00b7len", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur achtend was dem Bruder sei gesund,", "tokens": ["Nur", "ach\u00b7tend", "was", "dem", "Bru\u00b7der", "sei", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "PWS", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn ganz ein Gotteskind,", "tokens": ["Wenn", "ganz", "ein", "Got\u00b7tes\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sich unbewu\u00dft, am Gnadenkleide scheinet", "tokens": ["Sich", "un\u00b7be\u00b7wu\u00dft", ",", "am", "Gna\u00b7den\u00b7klei\u00b7de", "schei\u00b7net"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "ADJD", "$,", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Tr\u00e4ne lind,", "tokens": ["Die", "Tr\u00e4\u00b7ne", "lind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Nicht fragt, warum sie weinet,", "tokens": ["Nicht", "fragt", ",", "wa\u00b7rum", "sie", "wei\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann w\u00fchlt in meinem Busen das Gewissen,", "tokens": ["Dann", "w\u00fchlt", "in", "mei\u00b7nem", "Bu\u00b7sen", "das", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schutt und Ger\u00f6ll stellt sich mein Wirken dar;", "tokens": ["Schutt", "und", "Ge\u00b7r\u00f6ll", "stellt", "sich", "mein", "Wir\u00b7ken", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Das Geben und das Streben mir zerrissen", "tokens": ["Das", "Ge\u00b7ben", "und", "das", "Stre\u00b7ben", "mir", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Gr\u00fcbelns Dornen, wie der Einfalt bar,", "tokens": ["Von", "Gr\u00fc\u00b7belns", "Dor\u00b7nen", ",", "wie", "der", "Ein\u00b7falt", "bar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "PWAV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ja \u00fcberall mein Fu\u00df", "tokens": ["Ja", "\u00fc\u00b7be\u00b7rall", "mein", "Fu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "An Gitter st\u00f6\u00dft, an Kerkerschragen,", "tokens": ["An", "Git\u00b7ter", "st\u00f6\u00dft", ",", "an", "Ker\u00b7ker\u00b7schra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und zitternd mu\u00df", "tokens": ["Und", "zit\u00b7ternd", "mu\u00df"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "An meine Brust ich schlagen.", "tokens": ["An", "mei\u00b7ne", "Brust", "ich", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Vor allem, ach! wenn eine fromme Stimme", "tokens": ["Vor", "al\u00b7lem", ",", "ach", "!", "wenn", "ei\u00b7ne", "from\u00b7me", "Stim\u00b7me"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "$,", "ITJ", "$.", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir fl\u00fcstert zu ein einfach heilig Wort,", "tokens": ["Mir", "fl\u00fcs\u00b7tert", "zu", "ein", "ein\u00b7fach", "hei\u00b7lig", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So sicher da\u00df mein Herz in Glauben schwimme,", "tokens": ["So", "si\u00b7cher", "da\u00df", "mein", "Herz", "in", "Glau\u00b7ben", "schwim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So unbesorgt um meines Lebens Port,", "tokens": ["So", "un\u00b7be\u00b7sorgt", "um", "mei\u00b7nes", "Le\u00b7bens", "Port", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mir deiner Gnade Laut", "tokens": ["Mir", "dei\u00b7ner", "Gna\u00b7de", "Laut"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Unschuldig beut als Losungszeichen", "tokens": ["Un\u00b7schul\u00b7dig", "beut", "als", "Lo\u00b7sungs\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und ganz vertraut", "tokens": ["Und", "ganz", "ver\u00b7traut"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "An meine Brust will schleichen:", "tokens": ["An", "mei\u00b7ne", "Brust", "will", "schlei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dann m\u00fcssen alle Worte sich emp\u00f6ren,", "tokens": ["Dann", "m\u00fcs\u00b7sen", "al\u00b7le", "Wor\u00b7te", "sich", "em\u00b7p\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die frevelnd ich gesprochen einst und je", "tokens": ["Die", "fre\u00b7velnd", "ich", "ge\u00b7spro\u00b7chen", "einst", "und", "je"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PPER", "VVPP", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und alles was noch jetzt mich kann verst\u00f6ren,", "tokens": ["Und", "al\u00b7les", "was", "noch", "jetzt", "mich", "kann", "ver\u00b7st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "ADV", "ADV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das steigt und wirbelt um mich wie ein See,", "tokens": ["Das", "steigt", "und", "wir\u00b7belt", "um", "mich", "wie", "ein", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dann f\u00fchl' ich in dem Schaum", "tokens": ["Dann", "f\u00fchl'", "ich", "in", "dem", "Schaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Noch heut mich keiner Bande ledig,", "tokens": ["Noch", "heut", "mich", "kei\u00b7ner", "Ban\u00b7de", "le\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann st\u00f6hn' ich kaum:", "tokens": ["Dann", "st\u00f6hn'", "ich", "kaum", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Gott sei mir S\u00fcnder gn\u00e4dig!", "tokens": ["Gott", "sei", "mir", "S\u00fcn\u00b7der", "gn\u00e4\u00b7dig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}