{"textgrid.poem.60398": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An eine Freundin", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dichterherzen k\u00f6nnen segnen,", "tokens": ["Dich\u00b7ter\u00b7her\u00b7zen", "k\u00f6n\u00b7nen", "seg\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wen sie lieben; fremd und rauh", "tokens": ["Wen", "sie", "lie\u00b7ben", ";", "fremd", "und", "rauh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "VVINF", "$.", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinem Herzen zu begegnen,", "tokens": ["Mei\u00b7nem", "Her\u00b7zen", "zu", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fcte dich, du sch\u00f6ne Frau.", "tokens": ["H\u00fc\u00b7te", "dich", ",", "du", "sch\u00f6\u00b7ne", "Frau", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eine Sage l\u00e4\u00dft dich gr\u00fc\u00dfen,", "tokens": ["Ei\u00b7ne", "Sa\u00b7ge", "l\u00e4\u00dft", "dich", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So ich im Gebirg vernahm,", "tokens": ["So", "ich", "im", "Ge\u00b7birg", "ver\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als ich einst vor Wetterg\u00fcssen", "tokens": ["Als", "ich", "einst", "vor", "Wet\u00b7ter\u00b7g\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fl\u00fcchtend in ein H\u00fcttlein kam:", "tokens": ["Fl\u00fcch\u00b7tend", "in", "ein", "H\u00fctt\u00b7lein", "kam", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In den tiefsten Einsamkeiten,", "tokens": ["In", "den", "tiefs\u00b7ten", "Ein\u00b7sam\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwischen Felsen, ruht ein See;", "tokens": ["Zwi\u00b7schen", "Fel\u00b7sen", ",", "ruht", "ein", "See", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem entstieg ein Geist vor Zeiten,", "tokens": ["Dem", "ent\u00b7stieg", "ein", "Geist", "vor", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kam den Menschen in die N\u00e4h.", "tokens": ["Kam", "den", "Men\u00b7schen", "in", "die", "N\u00e4h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kam ins Dorf, erschien beim Feste,", "tokens": ["Kam", "ins", "Dorf", ",", "er\u00b7schien", "beim", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brachte Segen in das Haus,", "tokens": ["Brach\u00b7te", "Se\u00b7gen", "in", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es blickten Wirt und G\u00e4ste", "tokens": ["Und", "es", "blick\u00b7ten", "Wirt", "und", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oft gar sehnlich nach ihm aus.", "tokens": ["Oft", "gar", "sehn\u00b7lich", "nach", "ihm", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Pl\u00f6tzlich stand er unter ihnen,", "tokens": ["Pl\u00f6tz\u00b7lich", "stand", "er", "un\u00b7ter", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trug ein dunkles M\u00f6nchs gewand,", "tokens": ["Trug", "ein", "dunk\u00b7les", "M\u00f6nchs", "ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VAPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Mann mit ernsten Mienen", "tokens": ["Und", "der", "Mann", "mit", "erns\u00b7ten", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Freud an ihrer Freude fand.", "tokens": ["Freud", "an", "ih\u00b7rer", "Freu\u00b7de", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Gerne weilt' er eine Stunde,", "tokens": ["Ger\u00b7ne", "weilt'", "er", "ei\u00b7ne", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nickte und verlor sich sacht", "tokens": ["Nick\u00b7te", "und", "ver\u00b7lor", "sich", "sacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "PRF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den See, zum stillen Grunde", "tokens": ["In", "den", "See", ",", "zum", "stil\u00b7len", "Grun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Taucht' er heim um Mitternacht.", "tokens": ["Taucht'", "er", "heim", "um", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Gl\u00fccklich ward die Braut gepriesen,", "tokens": ["Gl\u00fcck\u00b7lich", "ward", "die", "Braut", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn er kam und ihr zum Tanz", "tokens": ["Wenn", "er", "kam", "und", "ihr", "zum", "Tanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Brachte von verborgnen Wiesen", "tokens": ["Brach\u00b7te", "von", "ver\u00b7borg\u00b7nen", "Wie\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fremder Blumen einen Kranz.", "tokens": ["Frem\u00b7der", "Blu\u00b7men", "ei\u00b7nen", "Kranz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wohlgeruch durchquoll das Zimmer,", "tokens": ["Wohl\u00b7ge\u00b7ruch", "durch\u00b7quoll", "das", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ner bl\u00fchte dann die Braut,", "tokens": ["Sch\u00f6\u00b7ner", "bl\u00fch\u00b7te", "dann", "die", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward im gleichen Jugendschimmer", "tokens": ["Ward", "im", "glei\u00b7chen", "Ju\u00b7gend\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Viele Jahre noch geschaut.", "tokens": ["Vie\u00b7le", "Jah\u00b7re", "noch", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Mutter ward sie guter Kinder,", "tokens": ["Mut\u00b7ter", "ward", "sie", "gu\u00b7ter", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haus und Feld gedieh; bis sp\u00e4t", "tokens": ["Haus", "und", "Feld", "ge\u00b7dieh", ";", "bis", "sp\u00e4t"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "$.", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie der Tod, ein leiser, linder,", "tokens": ["Sie", "der", "Tod", ",", "ein", "lei\u00b7ser", ",", "lin\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcberraschte beim Gebet.", "tokens": ["\u00dc\u00b7berr\u00b7aschte", "beim", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Einst mit rauher Ungeb\u00fchre", "tokens": ["Einst", "mit", "rau\u00b7her", "Un\u00b7ge\u00b7b\u00fch\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprach ihm eines was zuleid;", "tokens": ["Sprach", "ihm", "ei\u00b7nes", "was", "zu\u00b7leid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "PWS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Traurig schwieg er, und zur T\u00fcre", "tokens": ["Trau\u00b7rig", "schwieg", "er", ",", "und", "zur", "T\u00fc\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "KON", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwand der Saum von seinem Kleid.", "tokens": ["Schwand", "der", "Saum", "von", "sei\u00b7nem", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Und sie sahn vom Ufer nieder,", "tokens": ["Und", "sie", "sahn", "vom", "U\u00b7fer", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Riefen, klagten je und je;", "tokens": ["Rie\u00b7fen", ",", "klag\u00b7ten", "je", "und", "je", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch es kam der Geist nie wieder,", "tokens": ["Doch", "es", "kam", "der", "Geist", "nie", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blieb in seinem tiefen See.", "tokens": ["Blieb", "in", "sei\u00b7nem", "tie\u00b7fen", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}