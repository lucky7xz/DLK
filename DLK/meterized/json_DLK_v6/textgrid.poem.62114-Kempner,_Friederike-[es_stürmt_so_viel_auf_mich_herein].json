{"textgrid.poem.62114": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[es st\u00fcrmt so viel auf mich herein]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es st\u00fcrmt so viel auf mich herein,", "tokens": ["Es", "st\u00fcrmt", "so", "viel", "auf", "mich", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mag sein, mag sein,", "tokens": ["Mag", "sein", ",", "mag", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "VAINF", "$,", "VMFIN", "VAINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das Gute findet doch Gedeih'n,", "tokens": ["Das", "Gu\u00b7te", "fin\u00b7det", "doch", "Ge\u00b7deih'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf einmal seh'n es alle ein.", "tokens": ["Auf", "ein\u00b7mal", "seh'n", "es", "al\u00b7le", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}