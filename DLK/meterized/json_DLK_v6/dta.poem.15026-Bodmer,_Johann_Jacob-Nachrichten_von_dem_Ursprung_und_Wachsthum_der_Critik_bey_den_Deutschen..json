{"dta.poem.15026": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Nachrichten von dem Ursprung und  \n Wachsthum der Critik bey den  \n Deutschen.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1741", "urn": "urn:nbn:de:kobv:b4-200905198526", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u2012 \u2012 \u2012 Denn Opitz ist zwar werth,", "tokens": ["\u2012", "\u2012", "\u2012", "Denn", "O\u00b7pitz", "ist", "zwar", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "$(", "KON", "NE", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Den ersten Lorbeerkrantz in Deutschland zu erlangen,", "tokens": ["Den", "ers\u00b7ten", "Lor\u00b7beer\u00b7krantz", "in", "Deutschland", "zu", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er hat mit solchem Ruhm das Hauptwerck angefangen,", "tokens": ["Er", "hat", "mit", "sol\u00b7chem", "Ruhm", "das", "Haupt\u00b7werck", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df keiner nach der Zeit ihm ist geflogen f\u00fcr,", "tokens": ["Da\u00df", "kei\u00b7ner", "nach", "der", "Zeit", "ihm", "ist", "ge\u00b7flo\u00b7gen", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "PPER", "VAFIN", "VVPP", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als Hofmannswaldaus Geist, der Oder h\u00f6chste Zier;", "tokens": ["Als", "Hof\u00b7manns\u00b7wald\u00b7aus", "Geist", ",", "der", "O\u00b7der", "h\u00f6chs\u00b7te", "Zier", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "$,", "ART", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der deutsche Pindarus, dem keiner nach wird kommen.", "tokens": ["Der", "deut\u00b7sche", "Pin\u00b7da\u00b7rus", ",", "dem", "kei\u00b7ner", "nach", "wird", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIS", "APPR", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}