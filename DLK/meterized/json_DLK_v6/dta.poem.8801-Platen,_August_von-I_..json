{"dta.poem.8801": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "I .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Wer hat nicht, was er mu\u00df, geha\u00dft, und was er mu\u00df,", "tokens": ["Wer", "hat", "nicht", ",", "was", "er", "mu\u00df", ",", "ge\u00b7ha\u00dft", ",", "und", "was", "er", "mu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "$,", "PWS", "PPER", "VMFIN", "$,", "VVPP", "$,", "KON", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir haben stets das volle Glas, das auf und nieder", "tokens": ["Wir", "ha\u00b7ben", "stets", "das", "vol\u00b7le", "Glas", ",", "das", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "KON", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dabey der Rede Wechselkampf, des Lieds Ergu\u00df geliebt;", "tokens": ["Da\u00b7bey", "der", "Re\u00b7de", "Wech\u00b7sel\u00b7kampf", ",", "des", "Lieds", "Er\u00b7gu\u00df", "ge\u00b7liebt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Wir haben stets den Wohlgeruch im Rosenhain und stets", "tokens": ["Wir", "ha\u00b7ben", "stets", "den", "Wohl\u00b7ge\u00b7ruch", "im", "Ro\u00b7sen\u00b7hain", "und", "stets"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "APPRART", "NN", "KON", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Das feuerfarbne Tulpenbeet am k\u00fchlen Flu\u00df geliebt;", "tokens": ["Das", "feu\u00b7er\u00b7farb\u00b7ne", "Tul\u00b7pen\u00b7beet", "am", "k\u00fch\u00b7len", "Flu\u00df", "ge\u00b7liebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Wo M\u00e4dchenwange ladet ein, wo M\u00e4dchenauge sp\u00e4ht,", "tokens": ["Wo", "M\u00e4d\u00b7chen\u00b7wan\u00b7ge", "la\u00b7det", "ein", ",", "wo", "M\u00e4d\u00b7chen\u00b7au\u00b7ge", "sp\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PTKVZ", "$,", "PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Wer h\u00e4tte nicht verstohlnen Wink, verstohlnen Ku\u00df ge-", "tokens": ["Wer", "h\u00e4t\u00b7te", "nicht", "ver\u00b7stohl\u00b7nen", "Wink", ",", "ver\u00b7stohl\u00b7nen", "Ku\u00df", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PTKNEG", "ADJA", "NN", "$,", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es bleibe fern der feige Knecht, der sch\u00f6ne Form er-", "tokens": ["Es", "blei\u00b7be", "fern", "der", "fei\u00b7ge", "Knecht", ",", "der", "sch\u00f6\u00b7ne", "Form", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und nicht sie mit unendlichem Gem\u00fcthsentschlu\u00df geliebt!", "tokens": ["Und", "nicht", "sie", "mit", "un\u00b7end\u00b7li\u00b7chem", "Ge\u00b7m\u00fcth\u00b7sent\u00b7schlu\u00df", "ge\u00b7liebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Vor Allem lebe ", "tokens": ["Vor", "Al\u00b7lem", "le\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIS", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Wer unsres Liedes Anbeginn, und wer den Schlu\u00df ge-", "tokens": ["Wer", "uns\u00b7res", "Lie\u00b7des", "An\u00b7be\u00b7ginn", ",", "und", "wer", "den", "Schlu\u00df", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "NN", "$,", "KON", "PWS", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}