{"textgrid.poem.60507": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hast du noch immer nicht gefunden den teuren, teuren Fingerhut,", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast du noch immer nicht gefunden den teuren, teuren Fingerhut,", "tokens": ["Hast", "du", "noch", "im\u00b7mer", "nicht", "ge\u00b7fun\u00b7den", "den", "teu\u00b7ren", ",", "teu\u00b7ren", "Fin\u00b7ger\u00b7hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVPP", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Um den du pl\u00f6tzlich aufgesprungen", "tokens": ["Um", "den", "du", "pl\u00f6tz\u00b7lich", "auf\u00b7ge\u00b7sprun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und meinen Armen dich entrungen?", "tokens": ["Und", "mei\u00b7nen", "Ar\u00b7men", "dich", "ent\u00b7run\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich lie\u00df dich fahren mit verbi\u00dfner, doch wahrlich nicht geringer Wut.", "tokens": ["Ich", "lie\u00df", "dich", "fah\u00b7ren", "mit", "ver\u00b7bi\u00df\u00b7ner", ",", "doch", "wahr\u00b7lich", "nicht", "ge\u00b7rin\u00b7ger", "Wut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "$,", "ADV", "ADV", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "War ich ein Forscher, sprach ich trocken:", "tokens": ["War", "ich", "ein", "For\u00b7scher", ",", "sprach", "ich", "tro\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Indes du 's H\u00fctlein suchst erschrocken,", "tokens": ["In\u00b7des", "du", "'s", "H\u00fct\u00b7lein", "suchst", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Such ich, worauf das Herz des Weibes, das wandelbare Ding, beruht?", "tokens": ["Such", "ich", ",", "wo\u00b7rauf", "das", "Herz", "des", "Wei\u00b7bes", ",", "das", "wan\u00b7del\u00b7ba\u00b7re", "Ding", ",", "be\u00b7ruht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPER", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.8": {"text": "War ich ein Schw\u00e4rmer, rief ich fluchend:", "tokens": ["War", "ich", "ein", "Schw\u00e4r\u00b7mer", ",", "rief", "ich", "flu\u00b7chend", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "O w\u00e4r ich doch, den Rhein besuchend,", "tokens": ["O", "w\u00e4r", "ich", "doch", ",", "den", "Rhein", "be\u00b7su\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "$,", "ART", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ertrunken in den tiefsten Wirbeln der weitverrufnen Bingerflut!", "tokens": ["Er\u00b7trun\u00b7ken", "in", "den", "tiefs\u00b7ten", "Wir\u00b7beln", "der", "weit\u00b7ver\u00b7ruf\u00b7nen", "Bin\u00b7ger\u00b7flut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.11": {"text": "Als Egoiste w\u00fcrd ich sprechen:", "tokens": ["Als", "E\u00b7gois\u00b7te", "w\u00fcrd", "ich", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Das H\u00fctlein sch\u00fctzt sie vor dem Stechen,", "tokens": ["Das", "H\u00fct\u00b7lein", "sch\u00fctzt", "sie", "vor", "dem", "Ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ich wills mit meinem Herzen halten, wie sie mit ihrem Finger tut,", "tokens": ["Ich", "wills", "mit", "mei\u00b7nem", "Her\u00b7zen", "hal\u00b7ten", ",", "wie", "sie", "mit", "ih\u00b7rem", "Fin\u00b7ger", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,", "PWAV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.14": {"text": "Ich leg ans Herz, da\u00df sie's nicht raube,", "tokens": ["Ich", "leg", "ans", "Herz", ",", "da\u00df", "sie's", "nicht", "rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "APPRART", "NN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Mir eine Sturm- und Pickelhaube,", "tokens": ["Mir", "ei\u00b7ne", "Stur\u00b7m", "und", "Pi\u00b7ckel\u00b7hau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "TRUNC", "KON", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.16": {"text": "Das ist f\u00fcr ihre Liebesblicke, die scharfen Herzdurchdringer, gut.", "tokens": ["Das", "ist", "f\u00fcr", "ih\u00b7re", "Lie\u00b7bes\u00b7bli\u00b7cke", ",", "die", "schar\u00b7fen", "Herz\u00b7durch\u00b7drin\u00b7ger", ",", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.17": {"text": "Doch bin ich nichts davon und sage:", "tokens": ["Doch", "bin", "ich", "nichts", "da\u00b7von", "und", "sa\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIS", "PAV", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Such \u00fcberall herum und frage;", "tokens": ["Such", "\u00fc\u00b7be\u00b7rall", "he\u00b7rum", "und", "fra\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Kannst doch das Meer nicht meiner Liebe aussch\u00f6pfen mit dem Fingerhut,", "tokens": ["Kannst", "doch", "das", "Meer", "nicht", "mei\u00b7ner", "Lie\u00b7be", "aus\u00b7sch\u00f6p\u00b7fen", "mit", "dem", "Fin\u00b7ger\u00b7hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PTKNEG", "PPOSAT", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.20": {"text": "Hat die Romantik deiner Liebe auch Platz in einem Fingerhut.", "tokens": ["Hat", "die", "Ro\u00b7man\u00b7tik", "dei\u00b7ner", "Lie\u00b7be", "auch", "Platz", "in", "ei\u00b7nem", "Fin\u00b7ger\u00b7hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN", "ADV", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--+-+--+-+-+-+", "measure": "anapaest.di.plus"}}}}}