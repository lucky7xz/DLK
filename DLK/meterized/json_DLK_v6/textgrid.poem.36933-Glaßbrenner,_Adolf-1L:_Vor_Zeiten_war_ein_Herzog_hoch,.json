{"textgrid.poem.36933": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vor Zeiten war ein Herzog hoch,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor Zeiten war ein Herzog hoch,", "tokens": ["Vor", "Zei\u00b7ten", "war", "ein", "Her\u00b7zog", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der wohl sein armes Land betrog;", "tokens": ["Der", "wohl", "sein", "ar\u00b7mes", "Land", "be\u00b7trog", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der war bei seinem Volk verha\u00dft,", "tokens": ["Der", "war", "bei", "sei\u00b7nem", "Volk", "ver\u00b7ha\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wie der Gottseibeiuns fast.", "tokens": ["Als", "wie", "der", "Got\u00b7tsei\u00b7bei\u00b7uns", "fast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da pl\u00f6tzlich schon fr\u00fch Morgens stund", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "schon", "fr\u00fch", "Mor\u00b7gens", "stund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "ADJD", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Tambour in des Schlosses Rund,", "tokens": ["Ein", "Tam\u00b7bour", "in", "des", "Schlos\u00b7ses", "Rund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der hatte gar ein bleich Gesicht,", "tokens": ["Der", "hat\u00b7te", "gar", "ein", "bleich", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und blickte stier, wie Menschen nicht.", "tokens": ["Und", "blick\u00b7te", "stier", ",", "wie", "Men\u00b7schen", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "PWAV", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der trommelte in wildem Drang,", "tokens": ["Der", "trom\u00b7mel\u00b7te", "in", "wil\u00b7dem", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df wohl dem Besten wurde bang:", "tokens": ["Da\u00df", "wohl", "dem", "Bes\u00b7ten", "wur\u00b7de", "bang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Rundherum, rundherum am Thron", "tokens": ["Rund\u00b7he\u00b7rum", ",", "rund\u00b7he\u00b7rum", "am", "Thron"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "APPRART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Rebellion, Rebellion, Rebellion!", "tokens": ["Re\u00b7bel\u00b7lion", ",", "Re\u00b7bel\u00b7lion", ",", "Re\u00b7bel\u00b7lion", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Dem Herzog f\u00e4hrt es durch den Leib;", "tokens": ["Dem", "Her\u00b7zog", "f\u00e4hrt", "es", "durch", "den", "Leib", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er zittert wie ein schwaches Weib;", "tokens": ["Er", "zit\u00b7tert", "wie", "ein", "schwa\u00b7ches", "Weib", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kaum hat er Kraft zu diesem Wort:", "tokens": ["Kaum", "hat", "er", "Kraft", "zu", "die\u00b7sem", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schafft mir den bleichen Tambour fort.", "tokens": ["Schafft", "mir", "den", "blei\u00b7chen", "Tam\u00b7bour", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Der Tambour trommelt immerzu,", "tokens": ["Der", "Tam\u00b7bour", "trom\u00b7melt", "im\u00b7mer\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sonder Rast und sonder Ruh':", "tokens": ["Und", "son\u00b7der", "Rast", "und", "son\u00b7der", "Ruh'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Rundherum, rundherum am Thron", "tokens": ["Rund\u00b7he\u00b7rum", ",", "rund\u00b7he\u00b7rum", "am", "Thron"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "APPRART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Rebellion, Rebellion, Rebellion!", "tokens": ["Re\u00b7bel\u00b7lion", ",", "Re\u00b7bel\u00b7lion", ",", "Re\u00b7bel\u00b7lion", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Es lauft hinab die H\u00f6flingsschaar;", "tokens": ["Es", "lauft", "hin\u00b7ab", "die", "H\u00f6f\u00b7lings\u00b7schaar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie fand nicht, wo der Schrecken war;", "tokens": ["Sie", "fand", "nicht", ",", "wo", "der", "Schre\u00b7cken", "war", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat Nichts gesehn, hat Nichts geh\u00f6rt,", "tokens": ["Hat", "Nichts", "ge\u00b7sehn", ",", "hat", "Nichts", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "$,", "VAFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und glaubt, der Herzog sei beth\u00f6rt.", "tokens": ["Und", "glaubt", ",", "der", "Her\u00b7zog", "sei", "be\u00b7th\u00f6rt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Tambour trommelt immer zu,", "tokens": ["Der", "Tam\u00b7bour", "trom\u00b7melt", "im\u00b7mer", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sonder Rast und sonder Ruh:", "tokens": ["Und", "son\u00b7der", "Rast", "und", "son\u00b7der", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Rundherum, rundherum am Thron", "tokens": ["Rund\u00b7he\u00b7rum", ",", "rund\u00b7he\u00b7rum", "am", "Thron"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "APPRART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Rebellion, Rebellion, Rebellion!", "tokens": ["Re\u00b7bel\u00b7lion", ",", "Re\u00b7bel\u00b7lion", ",", "Re\u00b7bel\u00b7lion", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Der Herzog wurde matt und krank,", "tokens": ["Der", "Her\u00b7zog", "wur\u00b7de", "matt", "und", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es klang ihm schon wie Grabgesang;", "tokens": ["Es", "klang", "ihm", "schon", "wie", "Grab\u00b7ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er schrie vor Schmerz, er schrie vor Wuth,", "tokens": ["Er", "schrie", "vor", "Schmerz", ",", "er", "schrie", "vor", "Wuth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verzweifelt war sein b\u00f6ser Muth:", "tokens": ["Ver\u00b7zwei\u00b7felt", "war", "sein", "b\u00f6\u00b7ser", "Muth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der Tambour trommelt immerzu,", "tokens": ["Der", "Tam\u00b7bour", "trom\u00b7melt", "im\u00b7mer\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sonder Rast und sonder Ruh:", "tokens": ["Und", "son\u00b7der", "Rast", "und", "son\u00b7der", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Rundherum, rundherum am Thron", "tokens": ["Rund\u00b7he\u00b7rum", ",", "rund\u00b7he\u00b7rum", "am", "Thron"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "APPRART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Rebellion, Rebellion, Rebellion!", "tokens": ["Re\u00b7bel\u00b7lion", ",", "Re\u00b7bel\u00b7lion", ",", "Re\u00b7bel\u00b7lion", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}