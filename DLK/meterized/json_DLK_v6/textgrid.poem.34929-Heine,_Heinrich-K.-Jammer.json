{"textgrid.poem.34929": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "K.-Jammer", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese graue Wolkenschar", "tokens": ["Die\u00b7se", "grau\u00b7e", "Wol\u00b7ken\u00b7schar"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stieg aus einem Meer von Freuden;", "tokens": ["Stieg", "aus", "ei\u00b7nem", "Meer", "von", "Freu\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heute mu\u00df ich daf\u00fcr leiden,", "tokens": ["Heu\u00b7te", "mu\u00df", "ich", "da\u00b7f\u00fcr", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich gestern gl\u00fccklich war.", "tokens": ["Da\u00df", "ich", "ge\u00b7stern", "gl\u00fcck\u00b7lich", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Ach, in Wermut hat verkehrt", "tokens": ["Ach", ",", "in", "Wer\u00b7mut", "hat", "ver\u00b7kehrt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich der Nektar! Ach, wie qu\u00e4lend,", "tokens": ["Sich", "der", "Nek\u00b7tar", "!", "Ach", ",", "wie", "qu\u00e4\u00b7lend", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$.", "ITJ", "$,", "PWAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Katzenjammer, Hundeelend", "tokens": ["Kat\u00b7zen\u00b7jam\u00b7mer", ",", "Hun\u00b7dee\u00b7lend"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Herz und Magen mir beschwert!", "tokens": ["Herz", "und", "Ma\u00b7gen", "mir", "be\u00b7schwert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}