{"textgrid.poem.46791": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[der Freund hat Recht: Kein dunkler Schleier]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Freund hat Recht: Kein dunkler Schleier", "tokens": ["Der", "Freund", "hat", "Recht", ":", "Kein", "dunk\u00b7ler", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NN", "$.", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Soll mehr dein klares Bild verhangen;", "tokens": ["Soll", "mehr", "dein", "kla\u00b7res", "Bild", "ver\u00b7han\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vor meinen Augen sollst du freier", "tokens": ["Vor", "mei\u00b7nen", "Au\u00b7gen", "sollst", "du", "frei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bekr\u00e4nzt mit lichten Rosen prangen,", "tokens": ["Be\u00b7kr\u00e4nzt", "mit", "lich\u00b7ten", "Ro\u00b7sen", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df jeder Sonnenstrahl als Freier", "tokens": ["Da\u00df", "je\u00b7der", "Son\u00b7nen\u00b7strahl", "als", "Frei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durchs Fenster zu dir mag gelangen.", "tokens": ["Durchs", "Fens\u00b7ter", "zu", "dir", "mag", "ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er werb' um dich, und von der Leier", "tokens": ["Er", "werb'", "um", "dich", ",", "und", "von", "der", "Lei\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Vaters sei das Fest begangen:", "tokens": ["Des", "Va\u00b7ters", "sei", "das", "Fest", "be\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mein Kind h\u00e4lt seine Hochzeitsfeier", "tokens": ["Mein", "Kind", "h\u00e4lt", "sei\u00b7ne", "Hoch\u00b7zeits\u00b7fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und soll nicht vorm Erbleichen bangen.", "tokens": ["Und", "soll", "nicht", "vorm", "Er\u00b7blei\u00b7chen", "ban\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}