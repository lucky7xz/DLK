{"textgrid.poem.31467": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "da\u00df / eh der Tag vergeht / die Tugend siegen soll", "tokens": ["da\u00df", "/", "eh", "der", "Tag", "ver\u00b7geht", "/", "die", "Tu\u00b7gend", "sie\u00b7gen", "soll"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$(", "KOUS", "ART", "NN", "VVFIN", "$(", "ART", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "mit samt der Dapferkeit / und werden Freuden voll.", "tokens": ["mit", "samt", "der", "Dap\u00b7fer\u00b7keit", "/", "und", "wer\u00b7den", "Freu\u00b7den", "voll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$(", "KON", "VAFIN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach! das mein Vngel\u00fcck auch w\u00e4r bey seinem Ziele /", "tokens": ["Ach", "!", "das", "mein", "Vn\u00b7ge\u00b7l\u00fcck", "auch", "w\u00e4r", "bey", "sei\u00b7nem", "Zie\u00b7le", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "PPOSAT", "NN", "ADV", "VAFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "vor Zornes-Donner / mir ein Gnaden Strahl her fiele!", "tokens": ["vor", "Zor\u00b7nes\u00b7Don\u00b7ner", "/", "mir", "ein", "Gna\u00b7den", "Strahl", "her", "fie\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "PPER", "ART", "NN", "NN", "APZR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "da\u00df ich nur einst erf\u00fchr' / wie \u00fcber Irdisch wol", "tokens": ["da\u00df", "ich", "nur", "einst", "er\u00b7f\u00fchr'", "/", "wie", "\u00fc\u00b7ber", "Ir\u00b7disch", "wol"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$(", "KOKOM", "APPR", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "das Gut' auf B\u00f6ses schmeck; das deinen Hasses groll /", "tokens": ["das", "Gut'", "auf", "B\u00f6\u00b7ses", "schmeck", ";", "das", "dei\u00b7nen", "Has\u00b7ses", "groll", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NE", "$.", "ART", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "O unbarmherzigs Gl\u00fcck / dein Muht an mir nicht k\u00fchle.", "tokens": ["O", "un\u00b7barm\u00b7her\u00b7zigs", "Gl\u00fcck", "/", "dein", "Muht", "an", "mir", "nicht", "k\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "PPOSAT", "NN", "APPR", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Soll denn die Warheit selbst bey mir unwarhafft seyn /", "tokens": ["Soll", "denn", "die", "War\u00b7heit", "selbst", "bey", "mir", "un\u00b7war\u00b7hafft", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "APPR", "PPER", "ADJD", "VAINF", "$("], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "un\u00fcberwindlich auch mein Vngl\u00fcck nur allein?", "tokens": ["un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "auch", "mein", "Vn\u00b7gl\u00fcck", "nur", "al\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ach! so befihl' ichs dem / der alle Sachen lenket /", "tokens": ["Ach", "!", "so", "be\u00b7fihl'", "ichs", "dem", "/", "der", "al\u00b7le", "Sa\u00b7chen", "len\u00b7ket", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PIS", "ART", "$(", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "das sie doch endlich gut / wie b\u00f6\u00df sie sehn / ausgehn.", "tokens": ["das", "sie", "doch", "end\u00b7lich", "gut", "/", "wie", "b\u00f6\u00df", "sie", "sehn", "/", "aus\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "ADJD", "$(", "PWAV", "ADJD", "PPER", "VVINF", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sein Raht (tobt H\u00f6ll und Welt / ) mu\u00df doch zu letzt geschehn.", "tokens": ["Sein", "Raht", "(", "tobt", "H\u00f6ll", "und", "Welt", "/", ")", "mu\u00df", "doch", "zu", "letzt", "ge\u00b7schehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "NN", "KON", "NN", "$(", "$(", "VMFIN", "ADV", "PTKA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Leicht hat Gott zu erh\u00f6hn im Sinn / weil er versenket!", "tokens": ["Leicht", "hat", "Gott", "zu", "er\u00b7h\u00f6hn", "im", "Sinn", "/", "weil", "er", "ver\u00b7sen\u00b7ket", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "PTKZU", "VVINF", "APPRART", "NN", "$(", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}