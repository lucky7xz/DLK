{"textgrid.poem.49058": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "Erstes Lied", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n war der J\u00fcngling von stolzem Sinn,", "tokens": ["Sch\u00f6n", "war", "der", "J\u00fcng\u00b7ling", "von", "stol\u00b7zem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Sein Ro\u00df spornte er an des K\u00f6nigs Seite,", "tokens": ["Sein", "Ro\u00df", "sporn\u00b7te", "er", "an", "des", "K\u00f6\u00b7nigs", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dessen Waffen trug er,", "tokens": ["Des\u00b7sen", "Waf\u00b7fen", "trug", "er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Beym Lanzenspiel und im blutigen Streite,", "tokens": ["Beym", "Lan\u00b7zen\u00b7spiel", "und", "im", "blu\u00b7ti\u00b7gen", "Strei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Vom S\u00f6ller herab, erblickte einst Luitberta so", "tokens": ["Vom", "S\u00f6l\u00b7ler", "her\u00b7ab", ",", "er\u00b7blick\u00b7te", "einst", "Luit\u00b7ber\u00b7ta", "so"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "$,", "VVFIN", "ADV", "NE", "ADV"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Den Holden, mit purpurrothen Wangen,", "tokens": ["Den", "Hol\u00b7den", ",", "mit", "pur\u00b7pur\u00b7ro\u00b7then", "Wan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Umblizt vom Stahle, kam aus dem Schlachtfelde er froh,", "tokens": ["Um\u00b7blizt", "vom", "Stah\u00b7le", ",", "kam", "aus", "dem", "Schlacht\u00b7fel\u00b7de", "er", "froh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Da ward der Jungfrau Busen in Liebesbanden gefangen.", "tokens": ["Da", "ward", "der", "Jung\u00b7frau", "Bu\u00b7sen", "in", "Lie\u00b7bes\u00b7ban\u00b7den", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Das Rehelein, das gestern noch sich in Unschuld gelezt", "tokens": ["Das", "Re\u00b7hel\u00b7ein", ",", "das", "ge\u00b7stern", "noch", "sich", "in", "Un\u00b7schuld", "ge\u00b7lezt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "PRF", "APPR", "NN", "VVPP"], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "An der Blumenquelle Rand, st\u00f6hnt getroffen jetzt", "tokens": ["An", "der", "Blu\u00b7men\u00b7quel\u00b7le", "Rand", ",", "st\u00f6hnt", "ge\u00b7trof\u00b7fen", "jetzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "VVFIN", "VVPP", "ADV"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Vom scharfen Pfeil, der des J\u00e4gers Bogen entgangen,", "tokens": ["Vom", "schar\u00b7fen", "Pfeil", ",", "der", "des", "J\u00e4\u00b7gers", "Bo\u00b7gen", "ent\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Des Fr\u00e4uleins zarte Brust", "tokens": ["Des", "Fr\u00e4u\u00b7leins", "zar\u00b7te", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "N\u00e4hret seitdem peinlicher Liebe Verlangen.", "tokens": ["N\u00e4h\u00b7ret", "seit\u00b7dem", "pein\u00b7li\u00b7cher", "Lie\u00b7be", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Gedankenvoll gehet sie in ihrem Lustg\u00e4rtlein umher,", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7voll", "ge\u00b7het", "sie", "in", "ih\u00b7rem", "Lust\u00b7g\u00e4rt\u00b7lein", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Die Augen \u00f6fters schwanger von s\u00fc\u00dfen Z\u00e4hren,", "tokens": ["Die", "Au\u00b7gen", "\u00f6f\u00b7ters", "schwan\u00b7ger", "von", "s\u00fc\u00b7\u00dfen", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nichts erg\u00f6tzet ihren Sinn, sie seufzet schwer,", "tokens": ["Nichts", "er\u00b7g\u00f6t\u00b7zet", "ih\u00b7ren", "Sinn", ",", "sie", "seuf\u00b7zet", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Die Blumen wollen ihr", "tokens": ["Die", "Blu\u00b7men", "wol\u00b7len", "ihr"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht Trost und Labung mehr gew\u00e4hren.", "tokens": ["Nicht", "Trost", "und", "La\u00b7bung", "mehr", "ge\u00b7w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was hilft's, da\u00df ich Erbin bin des Thrones am Rhein,", "tokens": ["Was", "hilft's", ",", "da\u00df", "ich", "Er\u00b7bin", "bin", "des", "Thro\u00b7nes", "am", "Rhein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "NN", "VAFIN", "ART", "NN", "APPRART", "NE", "$,"], "meter": "-+--++--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Klagt, von ihren Zofen entfernt, sie in einsamer Kammer;", "tokens": ["Klagt", ",", "von", "ih\u00b7ren", "Zo\u00b7fen", "ent\u00b7fernt", ",", "sie", "in", "ein\u00b7sa\u00b7mer", "Kam\u00b7mer", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Soll er, der vor Allen mir gef\u00e4llt, mich nicht freyn \u2013", "tokens": ["Soll", "er", ",", "der", "vor", "Al\u00b7len", "mir", "ge\u00b7f\u00e4llt", ",", "mich", "nicht", "freyn", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PRELS", "APPR", "NE", "PPER", "VVPP", "$,", "PPER", "PTKNEG", "ADJD", "$("], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.9": {"text": "Die\u00df zermalmet ihr Innres, wie ein schwerer Hammer.", "tokens": ["Die\u00df", "zer\u00b7mal\u00b7met", "ihr", "Inn\u00b7res", ",", "wie", "ein", "schwe\u00b7rer", "Ham\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "So verzehret sie sich am Tage in Sehnsucht bang und hei\u00df,", "tokens": ["So", "ver\u00b7zeh\u00b7ret", "sie", "sich", "am", "Ta\u00b7ge", "in", "Sehn\u00b7sucht", "bang", "und", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "--+--+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Dem Monde erz\u00e4hlt bey Nacht sie ihr herbes Leiden;", "tokens": ["Dem", "Mon\u00b7de", "er\u00b7z\u00e4hlt", "bey", "Nacht", "sie", "ihr", "her\u00b7bes", "Lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "NN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Mag die Tochter des Herrschers nicht werden dem Tapfern zum Preis,", "tokens": ["Mag", "die", "Toch\u00b7ter", "des", "Herr\u00b7schers", "nicht", "wer\u00b7den", "dem", "Tap\u00b7fern", "zum", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NN", "PTKNEG", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Das Schicksal einer Magd", "tokens": ["Das", "Schick\u00b7sal", "ei\u00b7ner", "Magd"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Mu\u00df ich, Aermste! dann wahrlich beneiden.", "tokens": ["Mu\u00df", "ich", ",", "A\u00b7erms\u00b7te", "!", "dann", "wahr\u00b7lich", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADJA", "$.", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Die Turteltaube sucht im gr\u00fcnen Holze frey,", "tokens": ["Die", "Tur\u00b7tel\u00b7tau\u00b7be", "sucht", "im", "gr\u00fc\u00b7nen", "Hol\u00b7ze", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nach Gefallen sich aus den lieben Gatten,", "tokens": ["Nach", "Ge\u00b7fal\u00b7len", "sich", "aus", "den", "lie\u00b7ben", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Die Hindinn lockt den Liebling ihrer Wahl herbey", "tokens": ["Die", "Hin\u00b7dinn", "lockt", "den", "Lieb\u00b7ling", "ih\u00b7rer", "Wahl", "her\u00b7bey"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und scherzt mit ihm auf thauigen Matten,", "tokens": ["Und", "scherzt", "mit", "ihm", "auf", "thau\u00b7i\u00b7gen", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Nur K\u00f6nigst\u00f6chter sollen, o herbes Loos! die Hand", "tokens": ["Nur", "K\u00f6\u00b7nig\u00b7st\u00f6ch\u00b7ter", "sol\u00b7len", ",", "o", "her\u00b7bes", "Loos", "!", "die", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "NN", "VMFIN", "$,", "FM", "ADJA", "NN", "$.", "ART", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Hingeben nach Anderer Verlangen zum Pfand,", "tokens": ["Hin\u00b7ge\u00b7ben", "nach", "An\u00b7de\u00b7rer", "Ver\u00b7lan\u00b7gen", "zum", "Pfand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Sollen zu bitterster Bu\u00dfe, zu Quaalen,", "tokens": ["Sol\u00b7len", "zu", "bit\u00b7ters\u00b7ter", "Bu\u00b7\u00dfe", ",", "zu", "Qua\u00b7a\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "$,", "APPR", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.22": {"text": "Mit dem Herzen ihren Stand,", "tokens": ["Mit", "dem", "Her\u00b7zen", "ih\u00b7ren", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Ach, tausendf\u00e4ltig schwer und bitter bezahlen.", "tokens": ["Ach", ",", "tau\u00b7send\u00b7f\u00e4l\u00b7tig", "schwer", "und", "bit\u00b7ter", "be\u00b7zah\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJD", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "So jammert sie bis der Morgen verschlossen ihr Weh;", "tokens": ["So", "jam\u00b7mert", "sie", "bis", "der", "Mor\u00b7gen", "ver\u00b7schlos\u00b7sen", "ihr", "Weh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Schaam h\u00e4lt am Tage ihre Zunge gebunden;", "tokens": ["Die", "Schaam", "h\u00e4lt", "am", "Ta\u00b7ge", "ih\u00b7re", "Zun\u00b7ge", "ge\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Leben schmelzt weg ihr, wie im Fr\u00fchling der Schnee,", "tokens": ["Das", "Le\u00b7ben", "schmelzt", "weg", "ihr", ",", "wie", "im", "Fr\u00fch\u00b7ling", "der", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPER", "$,", "PWAV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nichts stillte bey ihr", "tokens": ["Nichts", "still\u00b7te", "bey", "ihr"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Des Herzens verborgene Wunden.", "tokens": ["Des", "Her\u00b7zens", "ver\u00b7bor\u00b7ge\u00b7ne", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Schon seit langem gefiel des Fr\u00e4uleins Zustand nicht", "tokens": ["Schon", "seit", "lan\u00b7gem", "ge\u00b7fiel", "des", "Fr\u00e4u\u00b7leins", "Zu\u00b7stand", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "ART", "NN", "NN", "PTKNEG"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Der Amme: sie rief jezt voll herzlichem Kummer:", "tokens": ["Der", "Am\u00b7me", ":", "sie", "rief", "jezt", "voll", "herz\u00b7li\u00b7chem", "Kum\u00b7mer", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Was mangelt Dir, Liebchen? in Deinem lieben Angesicht", "tokens": ["Was", "man\u00b7gelt", "Dir", ",", "Lieb\u00b7chen", "?", "in", "Dei\u00b7nem", "lie\u00b7ben", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$.", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Welken die R\u00f6slein, der Augen heiteres Licht", "tokens": ["Wel\u00b7ken", "die", "R\u00f6s\u00b7lein", ",", "der", "Au\u00b7gen", "hei\u00b7te\u00b7res", "Licht"],