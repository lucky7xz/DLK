{"dta.poem.15869": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "74.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von einem Freunde kanst du Freundesdienst' annehmen,", "tokens": ["Von", "ei\u00b7nem", "Freun\u00b7de", "kanst", "du", "Freun\u00b7des\u00b7dienst'", "an\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die, w\u00e4r' er nicht dein Freund, dich w\u00fcrden sehr besch\u00e4men.", "tokens": ["Die", ",", "w\u00e4r'", "er", "nicht", "dein", "Freund", ",", "dich", "w\u00fcr\u00b7den", "sehr", "be\u00b7sch\u00e4\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich sch\u00e4me mich, da\u00df ich mir Freundschaft bieten lie\u00df", "tokens": ["Ich", "sch\u00e4\u00b7me", "mich", ",", "da\u00df", "ich", "mir", "Freund\u00b7schaft", "bie\u00b7ten", "lie\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "NN", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von einem, der sich nun nicht als mein Freund erwies.", "tokens": ["Von", "ei\u00b7nem", ",", "der", "sich", "nun", "nicht", "als", "mein", "Freund", "er\u00b7wies", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word",