{"textgrid.poem.41354": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Das Delphische Orakel und der Gottlose", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Sch\u00fcler des Diagoras,", "tokens": ["Ein", "Sch\u00fc\u00b7ler", "des", "Di\u00b7a\u00b7go\u00b7ras", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein B\u00f6sewicht, der wenig glaubte,", "tokens": ["Ein", "B\u00f6\u00b7se\u00b7wicht", ",", "der", "we\u00b7nig", "glaub\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und seinem frechen G\u00f6tterha\u00df", "tokens": ["Und", "sei\u00b7nem", "fre\u00b7chen", "G\u00f6t\u00b7ter\u00b7ha\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die gr\u00f6\u00dfte Frevelthat erlaubte,", "tokens": ["Die", "gr\u00f6\u00df\u00b7te", "Fre\u00b7vel\u00b7that", "er\u00b7laub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ging einstens, aus verruchtem Sinn,", "tokens": ["Ging", "eins\u00b7tens", ",", "aus", "ver\u00b7ruch\u00b7tem", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach Delphos zum Orakel hin,", "tokens": ["Nach", "Del\u00b7phos", "zum", "O\u00b7ra\u00b7kel", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Mit atheistischem Vergn\u00fcgen", "tokens": ["Mit", "at\u00b7heis\u00b7ti\u00b7schem", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Den Gott der Dichtkunst zu betr\u00fcgen.", "tokens": ["Den", "Gott", "der", "Dicht\u00b7kunst", "zu", "be\u00b7tr\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Ph\u00f6bus, (sprach er) dein Verstand", "tokens": ["O", "Ph\u00f6\u00b7bus", ",", "(", "sprach", "er", ")", "dein", "Ver\u00b7stand"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "$(", "VVFIN", "PPER", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erforschet die geheimsten Dinge.", "tokens": ["Er\u00b7for\u00b7schet", "die", "ge\u00b7heims\u00b7ten", "Din\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hier halt' ich etwas in der Hand,", "tokens": ["Hier", "halt'", "ich", "et\u00b7was", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ich f\u00fcr dich zum Opfer bringe.", "tokens": ["Das", "ich", "f\u00fcr", "dich", "zum", "Op\u00b7fer", "brin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du Sohn Latonens, gib Bericht:", "tokens": ["Du", "Sohn", "La\u00b7to\u00b7nens", ",", "gib", "Be\u00b7richt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "NE", "$,", "VVIMP", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist es am Leben? oder nicht?", "tokens": ["Ist", "es", "am", "Le\u00b7ben", "?", "o\u00b7der", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "$.", "KON", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du wei\u00dft, es dient zu deiner Ehre,", "tokens": ["Du", "wei\u00dft", ",", "es", "dient", "zu", "dei\u00b7ner", "Eh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df ich von dir die Wahrheit h\u00f6re.", "tokens": ["Da\u00df", "ich", "von", "dir", "die", "Wahr\u00b7heit", "h\u00f6\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er dachte: gibt man zum Bescheid,", "tokens": ["Er", "dach\u00b7te", ":", "gibt", "man", "zum", "Be\u00b7scheid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Vogel ist nicht mehr am Leben,", "tokens": ["Dein", "Vo\u00b7gel", "ist", "nicht", "mehr", "am", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So will ich schon zur rechten Zeit", "tokens": ["So", "will", "ich", "schon", "zur", "rech\u00b7ten", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm Flug und Freiheit wiedergeben.", "tokens": ["Ihm", "Flug", "und", "Frei\u00b7heit", "wie\u00b7der\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn der sch\u00f6ne Leirer glaubt,", "tokens": ["Und", "wenn", "der", "sch\u00f6\u00b7ne", "Lei\u00b7rer", "glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Athem sei ihm nicht geraubt,", "tokens": ["Der", "A\u00b7them", "sei", "ihm", "nicht", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So soll, auch dann ihn zu ber\u00fccken,", "tokens": ["So", "soll", ",", "auch", "dann", "ihn", "zu", "be\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "ADV", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Druck den Vogel gleich ersticken.", "tokens": ["Ein", "Druck", "den", "Vo\u00b7gel", "gleich", "er\u00b7sti\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Apollo \u00fcbte nur Geduld,", "tokens": ["A\u00b7pol\u00b7lo", "\u00fcb\u00b7te", "nur", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Aus Mitleid mit der k\u00fchnen Schw\u00e4che,", "tokens": ["Aus", "Mit\u00b7leid", "mit", "der", "k\u00fch\u00b7nen", "Schw\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sprach: Versuchst du meine Huld?", "tokens": ["Und", "sprach", ":", "Ver\u00b7suchst", "du", "mei\u00b7ne", "Huld", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist kaum werth, da\u00df ich mich r\u00e4che.", "tokens": ["Du", "bist", "kaum", "werth", ",", "da\u00df", "ich", "mich", "r\u00e4\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PPER", "PRF", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zeuch deinen Sperling, o du Thor,", "tokens": ["Zeuch", "dei\u00b7nen", "Sper\u00b7ling", ",", "o", "du", "Thor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "FM", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lebendig oder todt hervor.", "tokens": ["Le\u00b7ben\u00b7dig", "o\u00b7der", "todt", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Die G\u00f6tter lassen sich nicht \u00e4ffen:", "tokens": ["Die", "G\u00f6t\u00b7ter", "las\u00b7sen", "sich", "nicht", "\u00e4f\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich kann von ferne sehn, und treffen.", "tokens": ["Ich", "kann", "von", "fer\u00b7ne", "sehn", ",", "und", "tref\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADV", "VVINF", "$,", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}