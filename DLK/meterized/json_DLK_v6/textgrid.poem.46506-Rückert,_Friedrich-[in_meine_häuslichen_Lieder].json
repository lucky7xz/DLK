{"textgrid.poem.46506": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[in meine h\u00e4uslichen Lieder]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In meine h\u00e4uslichen Lieder,", "tokens": ["In", "mei\u00b7ne", "h\u00e4us\u00b7li\u00b7chen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Tagebuch meiner Lust,", "tokens": ["Das", "Ta\u00b7ge\u00b7buch", "mei\u00b7ner", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schrieb ich mit Freuden bewu\u00dft", "tokens": ["Schrieb", "ich", "mit", "Freu\u00b7den", "be\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN", "ADJD"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Nur Freudengewinnste nieder,", "tokens": ["Nur", "Freu\u00b7den\u00b7ge\u00b7winns\u00b7te", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nie schrieb ich einen Verlust", "tokens": ["Nie", "schrieb", "ich", "ei\u00b7nen", "Ver\u00b7lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "In meine h\u00e4uslichen Lieder.", "tokens": ["In", "mei\u00b7ne", "h\u00e4us\u00b7li\u00b7chen", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "In meine h\u00e4uslichen Lieder", "tokens": ["In", "mei\u00b7ne", "h\u00e4us\u00b7li\u00b7chen", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schreib' ich nun euern Verlust", "tokens": ["Schreib'", "ich", "nun", "eu\u00b7ern", "Ver\u00b7lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "So hat sich schlie\u00dfen gemu\u00dft", "tokens": ["So", "hat", "sich", "schlie\u00b7\u00dfen", "ge\u00b7mu\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "VVFIN", "ADJD"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Die Rechnung! und wohl nicht wieder", "tokens": ["Die", "Rech\u00b7nung", "!", "und", "wohl", "nicht", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "ADV", "PTKNEG", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Schreib ich sobald eine Lust", "tokens": ["Schreib", "ich", "so\u00b7bald", "ei\u00b7ne", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ART", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "In meine h\u00e4uslichen Lieder.", "tokens": ["In", "mei\u00b7ne", "h\u00e4us\u00b7li\u00b7chen", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}