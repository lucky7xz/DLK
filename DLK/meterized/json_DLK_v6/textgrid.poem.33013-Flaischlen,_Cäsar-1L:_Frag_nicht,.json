{"textgrid.poem.33013": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Frag nicht,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frag nicht,", "tokens": ["Frag", "nicht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "mach's fertig,", "tokens": ["mach's", "fer\u00b7tig", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,"], "meter": "---", "measure": "unknown.measure.zero"}, "line.3": {"text": "und es ist gut!", "tokens": ["und", "es", "ist", "gut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Fr\u00e4gst du,", "tokens": ["Fr\u00e4gst", "du", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "wei\u00df jeder was einzuwenden,", "tokens": ["wei\u00df", "je\u00b7der", "was", "ein\u00b7zu\u00b7wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIS", "VVIZU", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "der im Ernst und der im Spa\u00df,", "tokens": ["der", "im", "Ernst", "und", "der", "im", "Spa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "KON", "ART", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "du aber stehst mit verdrossenen H\u00e4nden,", "tokens": ["du", "a\u00b7ber", "stehst", "mit", "ver\u00b7dros\u00b7se\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "zweifelnd, mi\u00dfgestimmt und la\u00df ...", "tokens": ["zwei\u00b7felnd", ",", "mi\u00df\u00b7ge\u00b7stimmt", "und", "la\u00df", "..."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "KON", "VVIMP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "und beginnst zu \u00e4ndern ...", "tokens": ["und", "be\u00b7ginnst", "zu", "\u00e4n\u00b7dern", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "aber die erste", "tokens": ["a\u00b7ber", "die", "ers\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Freude ist weg", "tokens": ["Freu\u00b7de", "ist", "weg"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "und ihr heiliger Mut ...", "tokens": ["und", "ihr", "hei\u00b7li\u00b7ger", "Mut", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Frag nicht,", "tokens": ["Frag", "nicht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "mach's fertig,", "tokens": ["mach's", "fer\u00b7tig", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,"], "meter": "---", "measure": "unknown.measure.zero"}, "line.3": {"text": "und es ist gut!", "tokens": ["und", "es", "ist", "gut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measu