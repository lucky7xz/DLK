{"textgrid.poem.55801": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieh', Du musst es mir vergeben,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh', Du musst es mir vergeben,", "tokens": ["Sieh'", ",", "Du", "musst", "es", "mir", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VMFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ich manchmal schroff und hart:", "tokens": ["Wenn", "ich", "manch\u00b7mal", "schroff", "und", "hart", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Toll und traurig war mein Leben,", "tokens": ["Toll", "und", "trau\u00b7rig", "war", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine w\u00fcste Pilgerfahrt.", "tokens": ["Ei\u00b7ne", "w\u00fcs\u00b7te", "Pil\u00b7ger\u00b7fahrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schwer hab ich nach Haus' gefunden,", "tokens": ["Schwer", "hab", "ich", "nach", "Haus'", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bitter musst' ich irre geh'n,", "tokens": ["Bit\u00b7ter", "musst'", "ich", "ir\u00b7re", "geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich kenne Stunden ... Stunden,", "tokens": ["Und", "ich", "ken\u00b7ne", "Stun\u00b7den", "...", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "$(", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Wo Gespenster auferstehen.", "tokens": ["Wo", "Ge\u00b7spens\u00b7ter", "auf\u00b7er\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}