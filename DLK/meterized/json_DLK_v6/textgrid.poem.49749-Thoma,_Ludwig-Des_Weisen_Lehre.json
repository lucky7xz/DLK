{"textgrid.poem.49749": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Des Weisen Lehre", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00fcnstler, wollt ihr Geld verdienen \u2013", "tokens": ["K\u00fcnst\u00b7ler", ",", "wollt", "ihr", "Geld", "ver\u00b7die\u00b7nen", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wer wollte dieses nicht? \u2013", "tokens": ["Und", "wer", "woll\u00b7te", "die\u00b7ses", "nicht", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWS", "VMFIN", "PDS", "PTKNEG", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6ret, was mir recht geschienen,", "tokens": ["H\u00f6\u00b7ret", ",", "was", "mir", "recht", "ge\u00b7schie\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6ret, was der Weise spricht:", "tokens": ["H\u00f6\u00b7ret", ",", "was", "der", "Wei\u00b7se", "spricht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wessen Gunst sollst du erringen", "tokens": ["Wes\u00b7sen", "Gunst", "sollst", "du", "er\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem Bild, was du gemalt?", "tokens": ["Mit", "dem", "Bild", ",", "was", "du", "ge\u00b7malt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Erstens doch vor allen Dingen", "tokens": ["Ers\u00b7tens", "doch", "vor", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dessen, der den Kitsch bezahlt.", "tokens": ["Des\u00b7sen", ",", "der", "den", "Kitsch", "be\u00b7zahlt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zweitens f\u00e4llt der Kritisierer", "tokens": ["Zwei\u00b7tens", "f\u00e4llt", "der", "Kri\u00b7ti\u00b7sie\u00b7rer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei der Sache ins Gewicht,", "tokens": ["Bei", "der", "Sa\u00b7che", "ins", "Ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn als Mensch und Zeitungsschmierer", "tokens": ["Denn", "als", "Mensch", "und", "Zei\u00b7tungs\u00b7schmie\u00b7rer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist er ohne Einflu\u00df nicht.", "tokens": ["Ist", "er", "oh\u00b7ne", "Ein\u00b7flu\u00df", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Drittens oder allererschtens,", "tokens": ["Drit\u00b7tens", "o\u00b7der", "al\u00b7le\u00b7rerschtens", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Maler, was du auch gemacht,", "tokens": ["Ma\u00b7ler", ",", "was", "du", "auch", "ge\u00b7macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hast du dabei deines Ferschtens,", "tokens": ["Hast", "du", "da\u00b7bei", "dei\u00b7nes", "Ferschtens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Landesherrn gedacht?", "tokens": ["Dei\u00b7nes", "Lan\u00b7des\u00b7herrn", "ge\u00b7dacht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Seine Huld belebt die Musen,", "tokens": ["Sei\u00b7ne", "Huld", "be\u00b7lebt", "die", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und auch die der Malerei.", "tokens": ["Und", "auch", "die", "der", "Ma\u00b7le\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sorge, da\u00df in deinem Busen", "tokens": ["Sor\u00b7ge", ",", "da\u00df", "in", "dei\u00b7nem", "Bu\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Glaube innig sei.", "tokens": ["Die\u00b7ser", "Glau\u00b7be", "in\u00b7nig", "sei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Male, wie du, wenn du laben", "tokens": ["Ma\u00b7le", ",", "wie", "du", ",", "wenn", "du", "la\u00b7ben"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER", "$,", "KOUS", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Willst dich an des Herrschers Gunst,", "tokens": ["Willst", "dich", "an", "des", "Herr\u00b7schers", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcnschen wirst, gemalt zu haben.", "tokens": ["W\u00fcn\u00b7schen", "wirst", ",", "ge\u00b7malt", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "VVPP", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses hei\u00dft man auch 'ne Kunst.", "tokens": ["Die\u00b7ses", "hei\u00dft", "man", "auch", "'ne", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}