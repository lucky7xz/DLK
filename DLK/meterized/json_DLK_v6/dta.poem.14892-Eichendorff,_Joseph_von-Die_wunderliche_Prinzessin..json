{"dta.poem.14892": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die wunderliche Prinzessin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Nun verliebt die Augen gehen", "tokens": ["Nun", "ver\u00b7liebt", "die", "Au\u00b7gen", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber ihres Gartens Mauer,", "tokens": ["Ue\u00b7ber", "ih\u00b7res", "Gar\u00b7tens", "Mau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "War so einsam dort zu sehen", "tokens": ["War", "so", "ein\u00b7sam", "dort", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schimmernd Land und Str\u00f6m' und Auen.", "tokens": ["Schim\u00b7mernd", "Land", "und", "Str\u00f6m'", "und", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wo ihre Augen gingen:", "tokens": ["Und", "wo", "ih\u00b7re", "Au\u00b7gen", "gin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Quellen aus der Gr\u00fcne sprangen,", "tokens": ["Quel\u00b7len", "aus", "der", "Gr\u00fc\u00b7ne", "spran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Berg und Wald verzaubert standen", "tokens": ["Berg", "und", "Wald", "ver\u00b7zau\u00b7bert", "stan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Tausend V\u00f6gel schwirrend sangen.", "tokens": ["Tau\u00b7send", "V\u00f6\u00b7gel", "schwir\u00b7rend", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Golden blitzt es \u00fcber'm Grunde,", "tokens": ["Gol\u00b7den", "blitzt", "es", "\u00fc\u00b7ber'm", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Selt'ne Farben irrend schweifen,", "tokens": ["Selt'\u00b7ne", "Far\u00b7ben", "ir\u00b7rend", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wie zu lang entbehrtem Feste", "tokens": ["Wie", "zu", "lang", "ent\u00b7behr\u00b7tem", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PTKA", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Will die Erde sich bereiten.", "tokens": ["Will", "die", "Er\u00b7de", "sich", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und nun kamen angezogen", "tokens": ["Und", "nun", "ka\u00b7men", "an\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Freier bald von allen Seiten,", "tokens": ["Frei\u00b7er", "bald", "von", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Federn bunt im Winde flogen,", "tokens": ["Fe\u00b7dern", "bunt", "im", "Win\u00b7de", "flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "J\u00e4ger schmuck im Walde reiten.", "tokens": ["J\u00e4\u00b7ger", "schmuck", "im", "Wal\u00b7de", "rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "H\u00f6rner munter drein erschallen", "tokens": ["H\u00f6r\u00b7ner", "mun\u00b7ter", "drein", "er\u00b7schal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJD", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Auf und unter durch das Gr\u00fcne,", "tokens": ["Auf", "und", "un\u00b7ter", "durch", "das", "Gr\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "APPR", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Pilger fromm dazwischen wallen,", "tokens": ["Pil\u00b7ger", "fromm", "da\u00b7zwi\u00b7schen", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Die das Heimathsfieber sp\u00fcren.", "tokens": ["Die", "das", "Hei\u00b7maths\u00b7fie\u00b7ber", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Auf vielsonn'gen Wiesen fl\u00f6ten", "tokens": ["Auf", "viel\u00b7sonn'\u00b7gen", "Wie\u00b7sen", "fl\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Sch\u00e4fer bei schneeflock'gen Schafen,", "tokens": ["Sch\u00e4\u00b7fer", "bei", "schnee\u00b7flock'\u00b7gen", "Scha\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.23": {"text": "Ritter in der Abendr\u00f6the", "tokens": ["Rit\u00b7ter", "in", "der", "A\u00b7bend\u00b7r\u00f6\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Knien auf des Berges Hange,", "tokens": ["Kni\u00b7en", "auf", "des", "Ber\u00b7ges", "Han\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}