{"dta.poem.13331": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von der verbl\u00fcmten Schreibart. Ist die Fertsetzung der Abhandl. von dem Sinnr.  \n und Scharssinn, aus der Ankl. des verd. Geschmacks.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198519", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Haucht ihre Menge nicht den st\u00e4rcksten Visam aus,", "tokens": ["Haucht", "ih\u00b7re", "Men\u00b7ge", "nicht", "den", "st\u00e4rcks\u00b7ten", "Vi\u00b7sam", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKNEG", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie w\u00fcrtzen durch so angenehme D\u00fcfte,", "tokens": ["Sie", "w\u00fcrt\u00b7zen", "durch", "so", "an\u00b7ge\u00b7neh\u00b7me", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die voller Amber und Ziebeth,", "tokens": ["Die", "vol\u00b7ler", "Am\u00b7ber", "und", "Zie\u00b7beth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die ausgespannten lauen L\u00fcfte,", "tokens": ["Die", "aus\u00b7ge\u00b7spann\u00b7ten", "lau\u00b7en", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ihre Balsam-Kraft uns recht ans Hertze geht.", "tokens": ["Da\u00df", "ih\u00b7re", "Bal\u00b7sam\u00b7Kraft", "uns", "recht", "ans", "Hert\u00b7ze", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "APPRART", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}