{"textgrid.poem.63898": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "An Grillparzer", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schien das goldne Buch geschlossen,", "tokens": ["Es", "schien", "das", "gold\u00b7ne", "Buch", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drin die erlauchten Namen stehn,", "tokens": ["Drin", "die", "er\u00b7lauch\u00b7ten", "Na\u00b7men", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die als Unsterblichkeitsgenossen", "tokens": ["Die", "als", "U\u00b7nsterb\u00b7lich\u00b7keits\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "KOUS", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Hell durch der Zeiten Wandel gehn.", "tokens": ["Hell", "durch", "der", "Zei\u00b7ten", "Wan\u00b7del", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "***Der letzte, der vom Gotte trunken", "tokens": ["*", "*", "*", "Der", "letz\u00b7te", ",", "der", "vom", "Got\u00b7te", "trun\u00b7ken"], "token_info": ["punct", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["XY", "XY", "XY", "ART", "ADJA", "$,", "PRELS", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im wachen Tag ein Tr\u00e4umer stand,", "tokens": ["Im", "wa\u00b7chen", "Tag", "ein", "Tr\u00e4u\u00b7mer", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "War in die Schattennacht versunken,", "tokens": ["War", "in", "die", "Schat\u00b7ten\u00b7nacht", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Penthesileen wahlverwandt.", "tokens": ["Pen\u00b7the\u00b7si\u00b7leen", "wahl\u00b7ver\u00b7wandt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Nun loschen aus die sch\u00f6nen Flammen,", "tokens": ["Nun", "lo\u00b7schen", "aus", "die", "sch\u00f6\u00b7nen", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die leuchteten der goldnen Zeit.", "tokens": ["Die", "leuch\u00b7te\u00b7ten", "der", "gold\u00b7nen", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Dichtung Hochwald schrumpft zusammen,", "tokens": ["Der", "Dich\u00b7tung", "Hoch\u00b7wald", "schrumpft", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur flacher Nachwuchs weit und breit.", "tokens": ["Nur", "fla\u00b7cher", "Nach\u00b7wuchs", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Zerrbild schwand das Gro\u00dfe, K\u00fchne,", "tokens": ["Zum", "Zerr\u00b7bild", "schwand", "das", "Gro\u00b7\u00dfe", ",", "K\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Sinnentaumel ward gefr\u00f6nt,", "tokens": ["Dem", "Sin\u00b7nen\u00b7tau\u00b7mel", "ward", "ge\u00b7fr\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Friedrich Schillers stolze B\u00fchne", "tokens": ["Und", "Fried\u00b7rich", "Schil\u00b7lers", "stol\u00b7ze", "B\u00fch\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Schien wieder des Kothurns entw\u00f6hnt.", "tokens": ["Schien", "wie\u00b7der", "des", "Ko\u00b7thurns", "ent\u00b7w\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da stand in weiheloser \u00d6de", "tokens": ["Da", "stand", "in", "wei\u00b7he\u00b7lo\u00b7ser", "\u00d6\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Einsam ein Nachgeborner auf,", "tokens": ["Ein\u00b7sam", "ein", "Nach\u00b7ge\u00b7bor\u00b7ner", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ein gottbegnadeter Trag\u00f6de", "tokens": ["Ein", "gott\u00b7be\u00b7gna\u00b7de\u00b7ter", "Tra\u00b7g\u00f6\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Begann den raschen Siegeslauf.", "tokens": ["Be\u00b7gann", "den", "ra\u00b7schen", "Sie\u00b7ges\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus der Romantik Jugendwildnis,", "tokens": ["Aus", "der", "Ro\u00b7man\u00b7tik", "Ju\u00b7gend\u00b7wild\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Wo er den ersten Kranz sich brach,", "tokens": ["Wo", "er", "den", "ers\u00b7ten", "Kranz", "sich", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zog ihn der ernsten Muse Bildnis", "tokens": ["Zog", "ihn", "der", "erns\u00b7ten", "Mu\u00b7se", "Bild\u00b7nis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auf vielverschlungnem Pfad sich nach.", "tokens": ["Auf", "viel\u00b7ver\u00b7schlung\u00b7nem", "Pfad", "sich", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie f\u00fchrt' ihn, der ihr fromm vertraute,", "tokens": ["Sie", "f\u00fchrt'", "ihn", ",", "der", "ihr", "fromm", "ver\u00b7trau\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In alter Sagen D\u00e4mmernis,", "tokens": ["In", "al\u00b7ter", "Sa\u00b7gen", "D\u00e4m\u00b7mer\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein k\u00fchner Dichterargonaute", "tokens": ["Ein", "k\u00fch\u00b7ner", "Dich\u00b7ter\u00b7ar\u00b7go\u00b7nau\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu retten dort ihr goldnes Vlie\u00df.", "tokens": ["Zu", "ret\u00b7ten", "dort", "ihr", "gold\u00b7nes", "Vlie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und als nach Haus die Segel schwellen,", "tokens": ["Und", "als", "nach", "Haus", "die", "Se\u00b7gel", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Umrauschen ihn auf sichrer Bahn", "tokens": ["Um\u00b7rau\u00b7schen", "ihn", "auf", "sich\u00b7rer", "Bahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Meeres und der Liebe Wellen,", "tokens": ["Des", "Mee\u00b7res", "und", "der", "Lie\u00b7be", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Sapphos Schatten schwebt heran.", "tokens": ["Und", "Sap\u00b7phos", "Schat\u00b7ten", "schwebt", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O frohe Fahrt, rings mit Troph\u00e4en", "tokens": ["O", "fro\u00b7he", "Fahrt", ",", "rings", "mit", "Trop\u00b7h\u00e4\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "ADV", "APPR", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Geschm\u00fcckt des Schiffes hoher Bord!", "tokens": ["Ge\u00b7schm\u00fcckt", "des", "Schif\u00b7fes", "ho\u00b7her", "Bord", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl flog die Kunde von Medeen", "tokens": ["Wohl", "flog", "die", "Kun\u00b7de", "von", "Me\u00b7de\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Durch alle Lande siegend fort;", "tokens": ["Durch", "al\u00b7le", "Lan\u00b7de", "sie\u00b7gend", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch ihm, der Heimat treustem Sohne,", "tokens": ["Doch", "ihm", ",", "der", "Hei\u00b7mat", "treus\u00b7tem", "Soh\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schien kein Gewinn ", "tokens": ["Schien", "kein", "Ge\u00b7winn"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Sein ", "tokens": ["Sein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Sein ", "tokens": ["Sein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}}, "stanza.6": {"line.1": {"text": "Da, auf des Lebens Sonnenwende,", "tokens": ["Da", ",", "auf", "des", "Le\u00b7bens", "Son\u00b7nen\u00b7wen\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stellt' er die m\u00e4cht'gen Bilder hin", "tokens": ["Stellt'", "er", "die", "m\u00e4cht'\u00b7gen", "Bil\u00b7der", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von jenes B\u00f6hmen Gl\u00fcck und Ende", "tokens": ["Von", "je\u00b7nes", "B\u00f6h\u00b7men", "Gl\u00fcck", "und", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Habsburgs leuchtendem Beginn.", "tokens": ["Und", "Habs\u00b7burgs", "leuch\u00b7ten\u00b7dem", "Be\u00b7ginn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nie herzgewinnender und schlichter", "tokens": ["Nie", "herz\u00b7ge\u00b7win\u00b7nen\u00b7der", "und", "schlich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ging auf ein f\u00fcrstlich hoher Stern,", "tokens": ["Ging", "auf", "ein", "f\u00fcrst\u00b7lich", "ho\u00b7her", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und freie Liebe macht den Dichter", "tokens": ["Und", "frei\u00b7e", "Lie\u00b7be", "macht", "den", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zum treusten Diener seines Herrn.", "tokens": ["Zum", "treus\u00b7ten", "Die\u00b7ner", "sei\u00b7nes", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Das Werk des K\u00fcnstlers ist sein eigen,", "tokens": ["Das", "Werk", "des", "K\u00fcnst\u00b7lers", "ist", "sein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch da\u00df es wirke, braucht's der Zeit.", "tokens": ["Doch", "da\u00df", "es", "wir\u00b7ke", ",", "braucht's", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am lauten Markt h\u00fcllt sich in Schweigen", "tokens": ["Am", "lau\u00b7ten", "Markt", "h\u00fcllt", "sich", "in", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Genius, den ein Gott geweiht.", "tokens": ["Der", "Ge\u00b7nius", ",", "den", "ein", "Gott", "ge\u00b7weiht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Anbrach mit st\u00fcrmischen Gewalten", "tokens": ["An\u00b7brach", "mit", "st\u00fcr\u00b7mi\u00b7schen", "Ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein V\u00f6lkerfr\u00fchling wild und schw\u00fcl;", "tokens": ["Ein", "V\u00f6l\u00b7ker\u00b7fr\u00fch\u00b7ling", "wild", "und", "schw\u00fcl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Dichters sinnende Gestalten", "tokens": ["Des", "Dich\u00b7ters", "sin\u00b7nen\u00b7de", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sahn fremd herab auf das Gew\u00fchl.", "tokens": ["Sahn", "fremd", "her\u00b7ab", "auf", "das", "Ge\u00b7w\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da lie\u00dfest du, erhabner Meister,", "tokens": ["Da", "lie\u00b7\u00dfest", "du", ",", "er\u00b7hab\u00b7ner", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weltabgewandt den Griffel ruhn.", "tokens": ["Welt\u00