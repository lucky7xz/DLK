{"dta.poem.20845": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seele so bey Lebens Zeit", "tokens": ["Die", "See\u00b7le", "so", "bey", "Le\u00b7bens", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein kleines Feuer hat entbrennet/", "tokens": ["Ein", "klei\u00b7nes", "Feu\u00b7er", "hat", "ent\u00b7bren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wird in dem Tod\u2019 als Sand zerstreut/", "tokens": ["Wird", "in", "dem", "Tod'", "als", "Sand", "zer\u00b7streut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "KOUS", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie ein leichter Rauch zertrennet/", "tokens": ["Und", "wie", "ein", "leich\u00b7ter", "Rauch", "zer\u00b7tren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So nun das allgemeine Gifft", "tokens": ["So", "nun", "das", "all\u00b7ge\u00b7mei\u00b7ne", "Gifft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht diesen gantzen Menschen trifft", "tokens": ["Nicht", "die\u00b7sen", "gant\u00b7zen", "Men\u00b7schen", "trifft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PDAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So halt ich/ da\u00df nach diesem Leben", "tokens": ["So", "halt", "ich", "/", "da\u00df", "nach", "die\u00b7sem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Seele diesen Grad erreicht/", "tokens": ["Die", "See\u00b7le", "die\u00b7sen", "Grad", "er\u00b7reicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Von welchem keine Klarheit weicht/", "tokens": ["Von", "wel\u00b7chem", "kei\u00b7ne", "Klar\u00b7heit", "weicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und da de\u00df Himmels Sch\u00e4tze schweben.", "tokens": ["Und", "da", "de\u00df", "Him\u00b7mels", "Sch\u00e4t\u00b7ze", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Di\u00df was dein weiser Mund verspricht/", "tokens": ["Di\u00df", "was", "dein", "wei\u00b7ser", "Mund", "ver\u00b7spricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Von G\u00fctern/ die kein Todt zubricht/", "tokens": ["Von", "G\u00fc\u00b7tern", "/", "die", "kein", "Todt", "zu\u00b7bricht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und ewig in den Himmel bl\u00fchen/", "tokens": ["Und", "e\u00b7wig", "in", "den", "Him\u00b7mel", "bl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Kan warlich nicht viel anders seyn/", "tokens": ["Kan", "war\u00b7lich", "nicht", "viel", "an\u00b7ders", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "ADV", "ADV", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Wo unsre Seelen sich bem\u00fchen", "tokens": ["Wo", "uns\u00b7re", "See\u00b7len", "sich", "be\u00b7m\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Zukommen zu der Sternen Schein/", "tokens": ["Zu\u00b7kom\u00b7men", "zu", "der", "Ster\u00b7nen", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wann F\u00e4ulni\u00df/ Todt und Nacht umziehen", "tokens": ["Wann", "F\u00e4ul\u00b7ni\u00df", "/", "Todt", "und", "Nacht", "um\u00b7zie\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "$(", "NN", "KON", "NN", "VVIZU"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Fleisch/ Sehnen/ Adern/ Haut und Bein.", "tokens": ["Fleisch", "/", "Seh\u00b7nen", "/", "A\u00b7dern", "/", "Haut", "und", "Bein", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}