{"textgrid.poem.55594": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Gewi\u00df, ich w\u00e4re schon so ferne, ferne ...", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gewi\u00df, ich w\u00e4re schon so ferne, ferne,", "tokens": ["Ge\u00b7wi\u00df", ",", "ich", "w\u00e4\u00b7re", "schon", "so", "fer\u00b7ne", ",", "fer\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADV", "ADV", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So weit die Welt nur offen liegt, gegangen,", "tokens": ["So", "weit", "die", "Welt", "nur", "of\u00b7fen", "liegt", ",", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bezw\u00e4ngen mich nicht \u00fcberm\u00e4cht'ge Sterne,", "tokens": ["Be\u00b7zw\u00e4n\u00b7gen", "mich", "nicht", "\u00fc\u00b7ber\u00b7m\u00e4cht'\u00b7ge", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die mein Geschick an deines angehangen,", "tokens": ["Die", "mein", "Ge\u00b7schick", "an", "dei\u00b7nes", "an\u00b7ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df ich in dir nun erst mich kennenlerne.", "tokens": ["Da\u00df", "ich", "in", "dir", "nun", "erst", "mich", "ken\u00b7nen\u00b7ler\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mein Dichten, Trachten, Hoffen und Verlangen", "tokens": ["Mein", "Dich\u00b7ten", ",", "Trach\u00b7ten", ",", "Hof\u00b7fen", "und", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Allein nach dir und deinem Wesen dr\u00e4ngt,", "tokens": ["Al\u00b7lein", "nach", "dir", "und", "dei\u00b7nem", "We\u00b7sen", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mein Leben nur an deinem Leben h\u00e4ngt.", "tokens": ["Mein", "Le\u00b7ben", "nur", "an", "dei\u00b7nem", "Le\u00b7ben", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}