{"textgrid.poem.61376": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Welche tiefe Mittagsschw\u00fcle", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welche tiefe Mittagsschw\u00fcle", "tokens": ["Wel\u00b7che", "tie\u00b7fe", "Mit\u00b7tags\u00b7schw\u00fc\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lagert \u00fcberm Tal und zieht mich", "tokens": ["La\u00b7gert", "\u00fc\u00b7berm", "Tal", "und", "zieht", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "KON", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf das weiche Moos hernieder,", "tokens": ["Auf", "das", "wei\u00b7che", "Moos", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das, ein gr\u00fcn und goldner Teppich,", "tokens": ["Das", ",", "ein", "gr\u00fcn", "und", "gold\u00b7ner", "Tep\u00b7pich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ART", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "--+-+-++", "measure": "anapaest.init"}, "line.5": {"text": "Sich um Eichenwurzeln breitet!", "tokens": ["Sich", "um", "Ei\u00b7chen\u00b7wur\u00b7zeln", "brei\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles still! Kein L\u00fcftchen atmet.", "tokens": ["Al\u00b7les", "still", "!", "Kein", "L\u00fcft\u00b7chen", "at\u00b7met", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "PTKVZ", "$.", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In den m\u00e4cht'gen Wipfeln r\u00fchret", "tokens": ["In", "den", "m\u00e4cht'\u00b7gen", "Wip\u00b7feln", "r\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich kein Blatt, am See kein Schilfhalm", "tokens": ["Sich", "kein", "Blatt", ",", "am", "See", "kein", "Schilf\u00b7halm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "PIAT", "NN", "$,", "APPRART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Neigt sich fl\u00fcsternd hin und wieder.", "tokens": ["Neigt", "sich", "fl\u00fcs\u00b7ternd", "hin", "und", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "PTKVZ", "KON", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Tief im k\u00fchlsten Dickicht schlummern", "tokens": ["Tief", "im", "k\u00fchls\u00b7ten", "Di\u00b7ckicht", "schlum\u00b7mern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Fink und Amsel, selbst die Sonne", "tokens": ["Fink", "und", "Am\u00b7sel", ",", "selbst", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wandelt, m\u00fcd und l\u00e4ssig blickend,", "tokens": ["Wan\u00b7delt", ",", "m\u00fcd", "und", "l\u00e4s\u00b7sig", "bli\u00b7ckend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Langsam ihre Bahn im Traume;", "tokens": ["Lang\u00b7sam", "ih\u00b7re", "Bahn", "im", "Trau\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und wie alles nun im Kreise", "tokens": ["Und", "wie", "al\u00b7les", "nun", "im", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Schweigt und ausruht, wie mir selber", "tokens": ["Schweigt", "und", "aus\u00b7ruht", ",", "wie", "mir", "sel\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "PWAV", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Schwer es lastet auf den Wimpern,", "tokens": ["Schwer", "es", "las\u00b7tet", "auf", "den", "Wim\u00b7pern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Ist es mir, der Weltgeist schlafe.", "tokens": ["Ist", "es", "mir", ",", "der", "Welt\u00b7geist", "schla\u00b7fe", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Nur die Wolken dort, die luft'gen,", "tokens": ["Nur", "die", "Wol\u00b7ken", "dort", ",", "die", "luft'\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Ewig wechselnden Gestalten,", "tokens": ["E\u00b7wig", "wech\u00b7seln\u00b7den", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Ziehn im Blau, wie durch die Seele", "tokens": ["Ziehn", "im", "Blau", ",", "wie", "durch", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Wandelbare Tr\u00e4ume ziehen", "tokens": ["Wan\u00b7del\u00b7ba\u00b7re", "Tr\u00e4u\u00b7me", "zie\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Schnell geboren, schnell verschwindend.", "tokens": ["Schnell", "ge\u00b7bo\u00b7ren", ",", "schnell", "ver\u00b7schwin\u00b7dend", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Jetzt sind's wei\u00dfe Friedensschw\u00e4ne,", "tokens": ["Jetzt", "sin\u00b7d's", "wei\u00b7\u00dfe", "Frie\u00b7dens\u00b7schw\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Schiffe jetzt mit stolzen Wimpeln,", "tokens": ["Schif\u00b7fe", "jetzt", "mit", "stol\u00b7zen", "Wim\u00b7peln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Jetzt ein Schlo\u00df, auf dessen Zinnen", "tokens": ["Jetzt", "ein", "Schlo\u00df", ",", "auf", "des\u00b7sen", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Bl\u00fchend pr\u00e4cht'ge G\u00e4rten hangen.", "tokens": ["Bl\u00fc\u00b7hend", "pr\u00e4cht'\u00b7ge", "G\u00e4r\u00b7ten", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Aus dem Schlosse steigt ein K\u00f6nig", "tokens": ["Aus", "dem", "Schlos\u00b7se", "steigt", "ein", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Silberb\u00e4rtig, mit erhobner", "tokens": ["Sil\u00b7ber\u00b7b\u00e4r\u00b7tig", ",", "mit", "er\u00b7hob\u00b7ner"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJD", "$,", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Rechten segnet er die V\u00f6lker;", "tokens": ["Rech\u00b7ten", "seg\u00b7net", "er", "die", "V\u00f6l\u00b7ker", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Nun auf goldnem Wagen thronend", "tokens": ["Nun", "auf", "gold\u00b7nem", "Wa\u00b7gen", "thro\u00b7nend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Naht ein hohes Weib, es schimmert", "tokens": ["Naht", "ein", "ho\u00b7hes", "Weib", ",", "es", "schim\u00b7mert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Schneerein ihr Gewand \u2013 so dacht' ich", "tokens": ["Schnee\u00b7rein", "ihr", "Ge\u00b7wand", "\u2013", "so", "dacht'", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$(", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Mir die Freiheit, wenn sie siegreich", "tokens": ["Mir", "die", "Frei\u00b7heit", ",", "wenn", "sie", "sieg\u00b7reich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "$,", "KOUS", "PPER", "PRF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "L\u00e4chelnd hinf\u00e4hrt durch die St\u00e4dte", "tokens": ["L\u00e4\u00b7chelnd", "hin\u00b7f\u00e4hrt", "durch", "die", "St\u00e4d\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Mit der Wage, mit dem Palmzweig.", "tokens": ["Mit", "der", "Wa\u00b7ge", ",", "mit", "dem", "Palm\u00b7zweig", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Weil', o G\u00f6ttliche! \u2013 Vergebens!", "tokens": ["Weil'", ",", "o", "G\u00f6tt\u00b7li\u00b7che", "!", "\u2013", "Ver\u00b7ge\u00b7bens", "!"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$.", "$(", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Schon zerrinnt die Glanzerscheinung", "tokens": ["Schon", "zer\u00b7rinnt", "die", "Glan\u00b7zer\u00b7schei\u00b7nung"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "In die Luft, und neue Bilder", "tokens": ["In", "die", "Luft", ",", "und", "neu\u00b7e", "Bil\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Dr\u00e4ngen sich empor am Himmel.", "tokens": ["Dr\u00e4n\u00b7gen", "sich", "em\u00b7por", "am", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKVZ", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sind vielleicht die Wolken droben", "tokens": ["Sind", "viel\u00b7leicht", "die", "Wol\u00b7ken", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lichte Tr\u00e4ume nur des Weltgeists,", "tokens": ["Lich\u00b7te", "Tr\u00e4u\u00b7me", "nur", "des", "Welt\u00b7geists", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn er schlummert, Gottgedanken,", "tokens": ["Wenn", "er", "schlum\u00b7mert", ",", "Gott\u00b7ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die in luft'gen Stoff gebildet", "tokens": ["Die", "in", "luft'\u00b7gen", "Stoff", "ge\u00b7bil\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch den klaren Himmel fluten,", "tokens": ["Durch", "den", "kla\u00b7ren", "Him\u00b7mel", "flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Allzu sch\u00f6n f\u00fcr unsre Erde?", "tokens": ["All\u00b7zu", "sch\u00f6n", "f\u00fcr", "uns\u00b7re", "Er\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}