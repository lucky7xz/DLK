{"textgrid.poem.64004": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Heines Geburtshaus", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein leichtsinnkrankes H\u00f6fchen,", "tokens": ["Ein", "leicht\u00b7sinn\u00b7kran\u00b7kes", "H\u00f6f\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein B\u00e4umlein und ein Hahn,", "tokens": ["Ein", "B\u00e4um\u00b7lein", "und", "ein", "Hahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das H\u00e4uslein da ein Z\u00f6fchen,", "tokens": ["Das", "H\u00e4us\u00b7lein", "da", "ein", "Z\u00f6f\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hecktisch Champagnersch\u00e4fchen \u2013", "tokens": ["Heck\u00b7tisch", "Cham\u00b7pag\u00b7ner\u00b7sch\u00e4f\u00b7chen", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Das B\u00e4umlein will nicht wachsen,", "tokens": ["Das", "B\u00e4um\u00b7lein", "will", "nicht", "wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dir Hahn kein Morgen nahn.", "tokens": ["Dir", "Hahn", "kein", "Mor\u00b7gen", "nahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "PIAT", "NN", "ADJA", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}