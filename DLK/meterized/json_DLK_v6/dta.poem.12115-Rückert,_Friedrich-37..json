{"dta.poem.12115": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es steht ein Fels am Weg, gehst du an ihm vorbei,", "tokens": ["Es", "steht", "ein", "Fels", "am", "Weg", ",", "gehst", "du", "an", "ihm", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So f\u00e4llt dir gar nicht ein, da\u00df er was andres sei;", "tokens": ["So", "f\u00e4llt", "dir", "gar", "nicht", "ein", ",", "da\u00df", "er", "was", "and\u00b7res", "sei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$,", "KOUS", "PPER", "PIS", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch bist du nun vorbei, und wendest dich zur\u00fcck,", "tokens": ["Doch", "bist", "du", "nun", "vor\u00b7bei", ",", "und", "wen\u00b7dest", "dich", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So zeigt ein menschliches Gesicht das Felsenst\u00fcck.", "tokens": ["So", "zeigt", "ein", "menschli\u00b7ches", "Ge\u00b7sicht", "das", "Fel\u00b7sen\u00b7st\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Ist es mit manchen Herrn wie mit dem Felsen nicht?", "tokens": ["Ist", "es", "mit", "man\u00b7chen", "Herrn", "wie", "mit", "dem", "Fel\u00b7sen", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PIAT", "NN", "KOKOM", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie haben nur von fern ein Menschenangesicht.", "tokens": ["Sie", "ha\u00b7ben", "nur", "von", "fern", "ein", "Men\u00b7schen\u00b7an\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}