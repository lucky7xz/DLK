{"dta.poem.21691": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Persia war besiegt. Den hohen Triumph zu feyern", "tokens": ["Per\u00b7sia", "war", "be\u00b7siegt", ".", "Den", "ho\u00b7hen", "Tri\u00b7umph", "zu", "fey\u00b7ern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "VVPP", "$.", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Gab Philipp's Sohn ein k\u00f6nigliches Mahl.", "tokens": ["Gab", "Phil\u00b7ipp's", "Sohn", "ein", "k\u00f6\u00b7nig\u00b7li\u00b7ches", "Mahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hoch prangt' auf g\u00fcldnem Stuhl", "tokens": ["Hoch", "prangt'", "auf", "g\u00fcld\u00b7nem", "Stuhl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In hehrer Majest\u00e4t", "tokens": ["In", "heh\u00b7rer", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der g\u00f6ttergleiche Held;", "tokens": ["Der", "g\u00f6t\u00b7ter\u00b7glei\u00b7che", "Held", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Rings um ihn seines Reiches Grosse,", "tokens": ["Rings", "um", "ihn", "sei\u00b7nes", "Rei\u00b7ches", "Gros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Myrthen die Stirne bekr\u00e4nzt, die Scheitel um-", "tokens": ["Mit", "Myr\u00b7then", "die", "Stir\u00b7ne", "be\u00b7kr\u00e4nzt", ",", "die", "Schei\u00b7tel", "um"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$,", "ART", "NN", "TRUNC"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "(also geziemt es den Siegern nach blutiger M\u00fch.)", "tokens": ["(", "al\u00b7so", "ge\u00b7ziemt", "es", "den", "Sie\u00b7gern", "nach", "blu\u00b7ti\u00b7ger", "M\u00fch", ".", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+--+---+-+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Neben ihm lehnt' in der Blume der ", "tokens": ["Ne\u00b7ben", "ihm", "lehnt'", "in", "der", "Blu\u00b7me", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "APPR", "ART", "NN", "ART"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.10": {"text": "Im Glanze der Sch\u00f6nheit die freundliche", "tokens": ["Im", "Glan\u00b7ze", "der", "Sch\u00f6n\u00b7heit", "die", "freund\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "ADJA"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Mit des Aufgangs k\u00f6stlichsten Steinen ge-", "tokens": ["Mit", "des", "Auf\u00b7gangs", "k\u00f6st\u00b7lichs\u00b7ten", "Stei\u00b7nen", "ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "TRUNC"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Preiset die Herrlichen, Heldens\u00f6hne.", "tokens": ["Prei\u00b7set", "die", "Herr\u00b7li\u00b7chen", ",", "Hel\u00b7den\u00b7s\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.13": {"text": "Ares und K\u00fcpris umarmen sich hier.", "tokens": ["A\u00b7res", "und", "K\u00fcp\u00b7ris", "um\u00b7ar\u00b7men", "sich", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "VVFIN", "PRF", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.14": {"text": "Tapfrer, nur dir", "tokens": ["Tapf\u00b7rer", ",", "nur", "dir"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "PPER"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Tapfrer, nur dir", "tokens": ["Tapf\u00b7rer", ",", "nur", "dir"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "PPER"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Tapferer, dir nur geb\u00fchret das Sch\u00f6ne", "tokens": ["Tap\u00b7fe\u00b7rer", ",", "dir", "nur", "ge\u00b7b\u00fch\u00b7ret", "das", "Sch\u00f6\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "ADV", "VVFIN", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Preiset die Herrlichen, Heldens\u00f6hne.", "tokens": ["Prei\u00b7set", "die", "Herr\u00b7li\u00b7chen", ",", "Hel\u00b7den\u00b7s\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "K\u00fcpris und Ares umarmen sich hier.", "tokens": ["K\u00fcp\u00b7ris", "und", "A\u00b7res", "um\u00b7ar\u00b7men", "sich", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "PRF", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Tapfrer, nur dir,", "tokens": ["Tapf\u00b7rer", ",", "nur", "dir", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PPER", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Tapfrer, nur dir,", "tokens": ["Tapf\u00b7rer", ",", "nur", "dir", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PPER", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Tapferer, dir nur geb\u00fchret das Sch\u00f6ne.", "tokens": ["Tap\u00b7fe\u00b7rer", ",", "dir", "nur", "ge\u00b7b\u00fch\u00b7ret", "das", "Sch\u00f6\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}