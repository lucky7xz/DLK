{"textgrid.poem.47673": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[mir unter die F\u00fc\u00dfe kommt ein Schwein]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir unter die F\u00fc\u00dfe kommt ein Schwein;", "tokens": ["Mir", "un\u00b7ter", "die", "F\u00fc\u00b7\u00dfe", "kommt", "ein", "Schwein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich sehe mich so ganz allein;", "tokens": ["Ich", "se\u00b7he", "mich", "so", "ganz", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da hebt es an, so laut zu schrei'n:", "tokens": ["Da", "hebt", "es", "an", ",", "so", "laut", "zu", "schrei'n", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbzigeunerlein, Zigeunerlein!", "tokens": ["\u00bb", "zi\u00b7geu\u00b7ner\u00b7lein", ",", "Zi\u00b7geu\u00b7ner\u00b7lein", "!"], "token_info": ["punct", "word", "punct", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin ja dein, ich bin ja dein;", "tokens": ["Ich", "bin", "ja", "dein", ",", "ich", "bin", "ja", "dein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "$,", "PPER", "VAFIN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Rasch zugelangt, die M\u00fch' ist klein,", "tokens": ["Rasch", "zu\u00b7ge\u00b7langt", ",", "die", "M\u00fch'", "ist", "klein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und hurtig in's Geb\u00fcsch hinein!\u00ab", "tokens": ["Und", "hur\u00b7tig", "in's", "Ge\u00b7b\u00fcsch", "hin\u00b7ein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}