{"dta.poem.7281": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Knabe im Moor.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "O schaurig ist's \u00fcber's Moor zu gehn,", "tokens": ["O", "schau\u00b7rig", "ist's", "\u00fc\u00b7ber's", "Moor", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn es wimmelt vom Haiderauche,", "tokens": ["Wenn", "es", "wim\u00b7melt", "vom", "Hai\u00b7de\u00b7rau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sich wie Phantome die D\u00fcnste drehn", "tokens": ["Sich", "wie", "Phan\u00b7to\u00b7me", "die", "D\u00fcns\u00b7te", "drehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "KOKOM", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Ranke h\u00e4kelt am Strauche,", "tokens": ["Und", "die", "Ran\u00b7ke", "h\u00e4\u00b7kelt", "am", "Strau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Unter jedem Tritte ein Quellchen springt,", "tokens": ["Un\u00b7ter", "je\u00b7dem", "Trit\u00b7te", "ein", "Quell\u00b7chen", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Wenn aus der Spalte es zischt und singt,", "tokens": ["Wenn", "aus", "der", "Spal\u00b7te", "es", "zischt", "und", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "O schaurig ist's \u00fcber's Moor zu gehn,", "tokens": ["O", "schau\u00b7rig", "ist's", "\u00fc\u00b7ber's", "Moor", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wenn das R\u00f6hricht knistert im Hauche!", "tokens": ["Wenn", "das", "R\u00f6h\u00b7richt", "knis\u00b7tert", "im", "Hau\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Fest h\u00e4lt die Fibel das zitternde Kind", "tokens": ["Fest", "h\u00e4lt", "die", "Fi\u00b7bel", "das", "zit\u00b7tern\u00b7de", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Und rennt als ob man es jage;", "tokens": ["Und", "rennt", "als", "ob", "man", "es", "ja\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Hohl \u00fcber die Fl\u00e4che sauset der Wind \u2014", "tokens": ["Hohl", "\u00fc\u00b7ber", "die", "Fl\u00e4\u00b7che", "sau\u00b7set", "der", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Was raschelt dr\u00fcben am Haage?", "tokens": ["Was", "ra\u00b7schelt", "dr\u00fc\u00b7ben", "am", "Haa\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das ist der gespenstige Gr\u00e4berknecht,", "tokens": ["Das", "ist", "der", "ge\u00b7spens\u00b7ti\u00b7ge", "Gr\u00e4\u00b7berk\u00b7necht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Der dem Meister die besten Torfe verzecht;", "tokens": ["Der", "dem", "Meis\u00b7ter", "die", "bes\u00b7ten", "Tor\u00b7fe", "ver\u00b7zecht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Hu, hu, es bricht wie ein irres Rind!", "tokens": ["Hu", ",", "hu", ",", "es", "bricht", "wie", "ein", "ir\u00b7res", "Rind", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "XY", "$,", "PPER", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Hinducket das Kn\u00e4blein zage.", "tokens": ["Hin\u00b7du\u00b7cket", "das", "Kn\u00e4\u00b7blein", "za\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Vom Ufer starret Gestumpf hervor,", "tokens": ["Vom", "U\u00b7fer", "star\u00b7ret", "Ge\u00b7stumpf", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Unheimlich nicket die F\u00f6hre,", "tokens": ["Un\u00b7heim\u00b7lich", "ni\u00b7cket", "die", "F\u00f6h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Der Knabe rennt, gespannt das Ohr,", "tokens": ["Der", "Kna\u00b7be", "rennt", ",", "ge\u00b7spannt", "das", "Ohr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Riesenhalme wie Speere;", "tokens": ["Durch", "Rie\u00b7sen\u00b7hal\u00b7me", "wie", "Spee\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOKOM", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und wie es rieselt und knittert darin!", "tokens": ["Und", "wie", "es", "rie\u00b7selt", "und", "knit\u00b7tert", "da\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "PAV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das ist die unselige Spinnerin,", "tokens": ["Das", "ist", "die", "un\u00b7se\u00b7li\u00b7ge", "Spin\u00b7ne\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das ist die gebannte Spinnlenor',", "tokens": ["Das", "ist", "die", "ge\u00b7bann\u00b7te", "Spinn\u00b7le\u00b7nor'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die den Haspel dreht im Ger\u00f6hre!", "tokens": ["Die", "den", "Has\u00b7pel", "dreht", "im", "Ge\u00b7r\u00f6h\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Voran, voran, nur immer im Lauf,", "tokens": ["Vo\u00b7ran", ",", "vo\u00b7ran", ",", "nur", "im\u00b7mer", "im", "Lauf", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$,", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Voran als woll' es ihn hohlen;", "tokens": ["Vo\u00b7ran", "als", "woll'", "es", "ihn", "hoh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "VMFIN", "PPER", "PPER", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vor seinem Fu\u00dfe brodelt es auf,", "tokens": ["Vor", "sei\u00b7nem", "Fu\u00b7\u00dfe", "bro\u00b7delt", "es", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Es pfeift ihm unter den Sohlen", "tokens": ["Es", "pfeift", "ihm", "un\u00b7ter", "den", "Soh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wie eine gespenstige Melodey;", "tokens": ["Wie", "ei\u00b7ne", "ge\u00b7spens\u00b7ti\u00b7ge", "Me\u00b7lo\u00b7dey", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Das ist der Geigemann ungetreu,", "tokens": ["Das", "ist", "der", "Gei\u00b7ge\u00b7mann", "un\u00b7ge\u00b7treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Das ist der diebische Fiedler Knauf,", "tokens": ["Das", "ist", "der", "die\u00b7bi\u00b7sche", "Fied\u00b7ler", "Knauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der den Hochzeitheller gestohlen!", "tokens": ["Der", "den", "Hoch\u00b7zeit\u00b7hel\u00b7ler", "ge\u00b7stoh\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Da birst das Moor, ein Seufzer geht", "tokens": ["Da", "birst", "das", "Moor", ",", "ein", "Seuf\u00b7zer", "geht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hervor aus der klaffenden H\u00f6hle;", "tokens": ["Her\u00b7vor", "aus", "der", "klaf\u00b7fen\u00b7den", "H\u00f6h\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Weh, weh, da ruft die verdammte Margreth:", "tokens": ["Weh", ",", "weh", ",", "da", "ruft", "die", "ver\u00b7damm\u00b7te", "Mar\u00b7greth", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "\u201eho, ho, meine arme Seele!\u201c", "tokens": ["\u201e", "ho", ",", "ho", ",", "mei\u00b7ne", "ar\u00b7me", "See\u00b7le", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "ITJ", "$,", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der Knabe springt wie ein wundes Reh,", "tokens": ["Der", "Kna\u00b7be", "springt", "wie", "ein", "wun\u00b7des", "Reh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "W\u00e4r' nicht Schutzengel in seiner N\u00e4h',", "tokens": ["W\u00e4r'", "nicht", "Schut\u00b7zen\u00b7gel", "in", "sei\u00b7ner", "N\u00e4h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Seine bleichenden Kn\u00f6chelchen f\u00e4nde sp\u00e4t", "tokens": ["Sei\u00b7ne", "blei\u00b7chen\u00b7den", "Kn\u00f6\u00b7chel\u00b7chen", "f\u00e4n\u00b7de", "sp\u00e4t"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Ein Gr\u00e4ber im Moorgeschwehle.", "tokens": ["Ein", "Gr\u00e4\u00b7ber", "im", "Mo\u00b7or\u00b7ge\u00b7schweh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da m\u00e4hlig gr\u00fcndet der Boden sich,", "tokens": ["Da", "m\u00e4h\u00b7lig", "gr\u00fcn\u00b7det", "der", "Bo\u00b7den", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "PRF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und dr\u00fcben, neben der Weide,", "tokens": ["Und", "dr\u00fc\u00b7ben", ",", "ne\u00b7ben", "der", "Wei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Lampe flimmert so heimathlich,", "tokens": ["Die", "Lam\u00b7pe", "flim\u00b7mert", "so", "hei\u00b7math\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Knabe steht an der Scheide.", "tokens": ["Der", "Kna\u00b7be", "steht", "an", "der", "Schei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Tief athmet er auf, zum Moor zur\u00fcck", "tokens": ["Tief", "ath\u00b7met", "er", "auf", ",", "zum", "Moor", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "APPRART", "NN", "PTKVZ"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Noch immer wirft er den scheuen Blick:", "tokens": ["Noch", "im\u00b7mer", "wirft", "er", "den", "scheu\u00b7en", "Blick", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ja, im Ger\u00f6hre war's f\u00fcrchterlich,", "tokens": ["Ja", ",", "im", "Ge\u00b7r\u00f6h\u00b7re", "wa\u00b7r's", "f\u00fcrch\u00b7ter\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPRART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "O schaurig wars in der Haide!", "tokens": ["O", "schau\u00b7rig", "wars", "in", "der", "Hai\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "APPR", "ART", "NE", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}