{"textgrid.poem.39605": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Friedrich der Erste nahm sich Herrn von Besser", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Friedrich der Erste nahm sich Herrn von Besser", "tokens": ["Fried\u00b7rich", "der", "Ers\u00b7te", "nahm", "sich", "Herrn", "von", "Bes\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "VVFIN", "PRF", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Hofpoeten; und der war auch be\u00dfer,", "tokens": ["Zum", "Hof\u00b7po\u00b7e\u00b7ten", ";", "und", "der", "war", "auch", "be\u00b7\u00dfer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "KON", "ART", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als meistens die Poeten seiner Zeit.", "tokens": ["Als", "meis\u00b7tens", "die", "Po\u00b7et\u00b7en", "sei\u00b7ner", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nun wurde Tieck ein be\u00dfrer Herr von Besser;", "tokens": ["Nun", "wur\u00b7de", "Tieck", "ein", "be\u00df\u00b7rer", "Herr", "von", "Bes\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er leitet in die Havel die Gew\u00e4\u00dfer,", "tokens": ["Er", "lei\u00b7tet", "in", "die", "Ha\u00b7vel", "die", "Ge\u00b7w\u00e4\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die ihm die Elb' und Weiseritz verleiht.", "tokens": ["Die", "ihm", "die", "Elb'", "und", "Wei\u00b7se\u00b7ritz", "ver\u00b7leiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch ich, Verge\u00dfner, werde bla\u00df und bl\u00e4\u00dfer;", "tokens": ["Doch", "ich", ",", "Ver\u00b7ge\u00df\u00b7ner", ",", "wer\u00b7de", "bla\u00df", "und", "bl\u00e4\u00b7\u00dfer", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "$,", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verzweiflung setzt mir an den Hals das Me\u00dfer.", "tokens": ["Ver\u00b7zwei\u00b7flung", "setzt", "mir", "an", "den", "Hals", "das", "Me\u00b7\u00dfer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Es thut nicht noth: ich berste schon vor Neid.", "tokens": ["Es", "thut", "nicht", "noth", ":", "ich", "bers\u00b7te", "schon", "vor", "Neid", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "$.", "PPER", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}