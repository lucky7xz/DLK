{"textgrid.poem.58884": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "103. Arm und doch getrost", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer immer an sein Elend denket,", "tokens": ["Wer", "im\u00b7mer", "an", "sein", "E\u00b7lend", "den\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verlieret alle Lust und Mut;", "tokens": ["Ver\u00b7lie\u00b7ret", "al\u00b7le", "Lust", "und", "Mut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer innig sich in Jesus senket,", "tokens": ["Wer", "in\u00b7nig", "sich", "in", "Je\u00b7sus", "sen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PRF", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bei seinem Elend Wunder tut.", "tokens": ["Bei", "sei\u00b7nem", "E\u00b7lend", "Wun\u00b7der", "tut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}