{"dta.poem.19636": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Zueignung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lasset uns Mayen und Kr\u00e4nze bereiten,               ", "tokens": ["Las\u00b7set", "uns", "Ma\u00b7yen", "und", "Kr\u00e4n\u00b7ze", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+---+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sehet, ach sehet die fr\u00f6hligen Zeiten!", "tokens": ["Se\u00b7het", ",", "ach", "se\u00b7het", "die", "fr\u00f6h\u00b7li\u00b7gen", "Zei\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ITJ", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Sehet ihr Br\u00fcder und merket hierbey,", "tokens": ["Se\u00b7het", "ihr", "Br\u00fc\u00b7der", "und", "mer\u00b7ket", "hier\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "ADV", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Welche Ver\u00e4nderung solches nur sey.", "tokens": ["Wel\u00b7che", "Ver\u00b7\u00e4n\u00b7de\u00b7rung", "sol\u00b7ches", "nur", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PIAT", "ADV", "VAFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Lasset uns Weinen und Trauren vertreiben,", "tokens": ["Las\u00b7set", "uns", "Wei\u00b7nen", "und", "Trau\u00b7ren", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Klagen und Zagen soll heute verbleiben,", "tokens": ["Kla\u00b7gen", "und", "Za\u00b7gen", "soll", "heu\u00b7te", "ver\u00b7blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Klagen und Zagen verjaget jetzund,", "tokens": ["Kla\u00b7gen", "und", "Za\u00b7gen", "ver\u00b7ja\u00b7get", "je\u00b7tzund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Heute seyd lustig und machet es kund.", "tokens": ["Heu\u00b7te", "seyd", "lus\u00b7tig", "und", "ma\u00b7chet", "es", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Lasset uns Zucker und Honig bestellen,", "tokens": ["Las\u00b7set", "uns", "Zu\u00b7cker", "und", "Ho\u00b7nig", "be\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Lasset uns holen die guten Gesellen,", "tokens": ["Las\u00b7set", "uns", "ho\u00b7len", "die", "gu\u00b7ten", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Lasset herbringen den Spanischen Wein,", "tokens": ["Las\u00b7set", "her\u00b7brin\u00b7gen", "den", "Spa\u00b7ni\u00b7schen", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Weil wir anjetzo beysammen hier seyn.", "tokens": ["Weil", "wir", "an\u00b7je\u00b7tzo", "bey\u00b7sam\u00b7men", "hier", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Lasset uns Birkene-Mayer bestellen", "tokens": ["Las\u00b7set", "uns", "Bir\u00b7ke\u00b7ne\u00b7Mayer", "be\u00b7stel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "VVINF"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Da\u00df wir euch schenken ihr guten Gesellen,", "tokens": ["Da\u00df", "wir", "euch", "schen\u00b7ken", "ihr", "gu\u00b7ten", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Lasset den Birkenen-Mayer umgehn,", "tokens": ["Las\u00b7set", "den", "Bir\u00b7ke\u00b7nen\u00b7Mayer", "um\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Lasset die Gl\u00e4ser nicht stille so stehn.", "tokens": ["Las\u00b7set", "die", "Gl\u00e4\u00b7ser", "nicht", "stil\u00b7le", "so", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "VVFIN", "ADV", "VVINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Lasset die Lauten und Geigen erklingen,", "tokens": ["Las\u00b7set", "die", "Lau\u00b7ten", "und", "Gei\u00b7gen", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Lasset uns eilen zum Tanze zu springen,", "tokens": ["Las\u00b7set", "uns", "ei\u00b7len", "zum", "Tan\u00b7ze", "zu", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Nehmet die Kegel und Bossel in acht,", "tokens": ["Neh\u00b7met", "die", "Ke\u00b7gel", "und", "Bos\u00b7sel", "in", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "NN", "APPR", "CARD", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Lasset uns spielen, bis kommet die Nacht.", "tokens": ["Las\u00b7set", "uns", "spie\u00b7len", ",", "bis", "kom\u00b7met", "die", "Nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Lasset uns geistliche, weltliche Lieder", "tokens": ["Las\u00b7set", "uns", "geist\u00b7li\u00b7che", ",", "welt\u00b7li\u00b7che", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "$,", "PWAT", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Klingen und singen ihr herrlichen Br\u00fcder,", "tokens": ["Klin\u00b7gen", "und", "sin\u00b7gen", "ihr", "herr\u00b7li\u00b7chen", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Lasset uns letzen: Die Jugend vergeht,", "tokens": ["Las\u00b7set", "uns", "let\u00b7zen", ":", "Die", "Ju\u00b7gend", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Wehmuth und Trauren im Alter entsteht.", "tokens": ["Weh\u00b7muth", "und", "Trau\u00b7ren", "im", "Al\u00b7ter", "ent\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}