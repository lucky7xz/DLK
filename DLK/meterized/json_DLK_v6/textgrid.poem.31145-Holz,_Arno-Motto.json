{"textgrid.poem.31145": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Motto", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr kriegt mich nicht nieder,", "tokens": ["Ihr", "kriegt", "mich", "nicht", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ohnm\u00e4chtige Tr\u00f6pfe,", "tokens": ["Ohn\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Tr\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ich kehre wieder und wieder,", "tokens": ["Ich", "keh\u00b7re", "wie\u00b7der", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und meine steigenden Lieder", "tokens": ["Und", "mei\u00b7ne", "stei\u00b7gen\u00b7den", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wachsen begrabend Euch \u00fcber die K\u00f6pfe!", "tokens": ["Wach\u00b7sen", "be\u00b7gra\u00b7bend", "Euch", "\u00fc\u00b7ber", "die", "K\u00f6p\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}