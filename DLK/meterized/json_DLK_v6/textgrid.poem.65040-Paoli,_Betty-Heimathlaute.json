{"textgrid.poem.65040": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "Heimathlaute", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00fc\u00df' ihn, der unter fremdem Dache", "tokens": ["Gr\u00fc\u00df'", "ihn", ",", "der", "un\u00b7ter", "frem\u00b7dem", "Da\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Fl\u00fcchtling lebt im fremden Land,", "tokens": ["Ein", "Fl\u00fccht\u00b7ling", "lebt", "im", "frem\u00b7den", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gr\u00fc\u00df' ihn in der geliebten Sprache", "tokens": ["Gr\u00fc\u00df'", "ihn", "in", "der", "ge\u00b7lieb\u00b7ten", "Spra\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Des Land's, wo seine Wiege stand,", "tokens": ["Des", "Lan\u00b7d's", ",", "wo", "sei\u00b7ne", "Wie\u00b7ge", "stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mit Thr\u00e4nen wird er sich erinnern", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "wird", "er", "sich", "e\u00b7rin\u00b7nern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des stillen Gl\u00fccks, das er verlor,", "tokens": ["Des", "stil\u00b7len", "Gl\u00fccks", ",", "das", "er", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wilder schl\u00e4gt in seinem Innern", "tokens": ["Und", "wil\u00b7der", "schl\u00e4gt", "in", "sei\u00b7nem", "In\u00b7nern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Heimweh's hei\u00dfe Qual empor.", "tokens": ["Des", "Heim\u00b7weh's", "hei\u00b7\u00dfe", "Qual", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So zaubern deine sanften Reden,", "tokens": ["So", "zau\u00b7bern", "dei\u00b7ne", "sanf\u00b7ten", "Re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So zaubert mir dein reiner Blick,", "tokens": ["So", "zau\u00b7bert", "mir", "dein", "rei\u00b7ner", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Unschuld bl\u00fcthenreiches Eden,", "tokens": ["Der", "Un\u00b7schuld", "bl\u00fc\u00b7then\u00b7rei\u00b7ches", "E\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Seele Heimathland zur\u00fcck,", "tokens": ["Der", "See\u00b7le", "Hei\u00b7mat\u00b7hland", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und neu ergl\u00fcht die Sehnsuchtswunde", "tokens": ["Und", "neu", "er\u00b7gl\u00fcht", "die", "Sehn\u00b7suchts\u00b7wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nach dem verlor'nen Paradies,", "tokens": ["Nach", "dem", "ver\u00b7lor'\u00b7nen", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Aus dem in einer Unheilsstunde", "tokens": ["Aus", "dem", "in", "ei\u00b7ner", "Un\u00b7heils\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein finstrer D\u00e4mon mich verstie\u00df. \u2013", "tokens": ["Ein", "finst\u00b7rer", "D\u00e4\u00b7mon", "mich", "ver\u00b7stie\u00df", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}