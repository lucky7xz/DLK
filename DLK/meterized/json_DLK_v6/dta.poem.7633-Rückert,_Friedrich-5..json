{"dta.poem.7633": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Wirken wirst du nach verschiednen Stund- und Tagen", "tokens": ["Dein", "Wir\u00b7ken", "wirst", "du", "nach", "ver\u00b7schied\u00b7nen", "Stun\u00b7d", "und", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "APPR", "ADJA", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Bald alzu niedrig, bald auch alzu hoch anschlagen.", "tokens": ["Bald", "al\u00b7zu", "nied\u00b7rig", ",", "bald", "auch", "al\u00b7zu", "hoch", "an\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "$,", "ADV", "ADV", "PTKA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das sind des Hochmuths und des Kleinmuths b\u00f6se Geister,", "tokens": ["Das", "sind", "des", "Hoch\u00b7muths", "und", "des", "Klein\u00b7muths", "b\u00f6\u00b7se", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die la\u00df nie seyn in dir der rechten Demuth Meister.", "tokens": ["Die", "la\u00df", "nie", "seyn", "in", "dir", "der", "rech\u00b7ten", "De\u00b7muth", "Meis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVIMP", "ADV", "VAINF", "APPR", "PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mit h\u00f6chstem Selbstgef\u00fchl vertr\u00e4gt die Demuth sich:", "tokens": ["Mit", "h\u00f6chs\u00b7tem", "Selbst\u00b7ge\u00b7f\u00fchl", "ver\u00b7tr\u00e4gt", "die", "De\u00b7muth", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "PRF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O Werkzeug Gottes, du nicht wirkst, er wirkt durch dich.", "tokens": ["O", "Werk\u00b7zeug", "Got\u00b7tes", ",", "du", "nicht", "wirkst", ",", "er", "wirkt", "durch", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "$,", "PPER", "PTKNEG", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}