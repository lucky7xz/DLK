{"textgrid.poem.62710": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Unterth\u00e4nigstes Sendschreiben an Ihro Hochf\u00fcrstl. Durchlaucht. zu Sachsen-Weymar", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So oft ich nur gedenke", "tokens": ["So", "oft", "ich", "nur", "ge\u00b7den\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An das vortreffliche und F\u00fcrstliche Geschenke,", "tokens": ["An", "das", "vor\u00b7treff\u00b7li\u00b7che", "und", "F\u00fcrst\u00b7li\u00b7che", "Ge\u00b7schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "So bin ich ausser mir, und bleibe ganz entz\u00fcckt.", "tokens": ["So", "bin", "ich", "aus\u00b7ser", "mir", ",", "und", "blei\u00b7be", "ganz", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "$,", "KON", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich kenne dessen Werth und will aus Ehrfurcht schweigen.", "tokens": ["Ich", "ken\u00b7ne", "des\u00b7sen", "Werth", "und", "will", "aus", "Ehr\u00b7furcht", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "NN", "KON", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Bewundert stets mein Geist, und ist in sich erfreut,", "tokens": ["Be\u00b7wun\u00b7dert", "stets", "mein", "Geist", ",", "und", "ist", "in", "sich", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,", "KON", "VAFIN", "APPR", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn dieses \u00fcbersteigt den Werth der Kostbarkeit.", "tokens": ["Denn", "die\u00b7ses", "\u00fc\u00b7bers\u00b7teigt", "den", "Werth", "der", "Kost\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So hoch nun ", "tokens": ["So", "hoch", "nun"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "So stark die Ehrfurcht mich, nach Schuldigkeit besieget;", "tokens": ["So", "stark", "die", "Ehr\u00b7furcht", "mich", ",", "nach", "Schul\u00b7dig\u00b7keit", "be\u00b7sie\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "PPER", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So scharf sucht auch mein Herz, mein Geist, Gem\u00fcth und Sinn,", "tokens": ["So", "scharf", "sucht", "auch", "mein", "Herz", ",", "mein", "Geist", ",", "Ge\u00b7m\u00fcth", "und", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dem Dank davor zu thun/ den ich ", "tokens": ["Dem", "Dank", "da\u00b7vor", "zu", "thun", "/", "den", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PAV", "PTKZU", "VVINF", "$(", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Da\u00df es nicht m\u00f6glich ist so stark, so gro\u00df, so rein,", "tokens": ["Da\u00df", "es", "nicht", "m\u00f6g\u00b7lich", "ist", "so", "stark", ",", "so", "gro\u00df", ",", "so", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So lebhaft und geschickt, als es wohl solte seyn,", "tokens": ["So", "leb\u00b7haft", "und", "ge\u00b7schickt", ",", "als", "es", "wohl", "sol\u00b7te", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVPP", "$,", "KOUS", "PPER", "ADV", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.16": {"text": "Den unterth\u00e4ngen Dank ", "tokens": ["Den", "un\u00b7ter\u00b7th\u00e4n\u00b7gen", "Dank"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "So weis ich, wirst ", "tokens": ["So", "weis", "ich", ",", "wirst"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "PTKVZ", "PPER", "$,", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Das Lallen nicht verschm\u00e4hn. In Unterth\u00e4nigkeit,", "tokens": ["Das", "Lal\u00b7len", "nicht", "ver\u00b7schm\u00e4hn", ".", "In", "Un\u00b7ter\u00b7th\u00e4\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$.", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dank ich vor das Geschenk, womit ", "tokens": ["Dank", "ich", "vor", "das", "Ge\u00b7schenk", ",", "wo\u00b7mit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "$,", "PWAV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.20": {"text": "Und hoch begnadget hast. So viel es Sch\u00f6nheit tr\u00e4get;", "tokens": ["Und", "hoch", "be\u00b7gnad\u00b7get", "hast", ".", "So", "viel", "es", "Sch\u00f6n\u00b7heit", "tr\u00e4\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VAFIN", "$.", "ADV", "ADV", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So viel es Augen sehn, und Herzen auch beweget;", "tokens": ["So", "viel", "es", "Au\u00b7gen", "sehn", ",", "und", "Her\u00b7zen", "auch", "be\u00b7we\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "VVINF", "$,", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So vieles F\u00fcrsten Gl\u00fcck; so viele Lust und Zier,", "tokens": ["So", "vie\u00b7les", "F\u00fcrs\u00b7ten", "Gl\u00fcck", ";", "so", "vie\u00b7le", "Lust", "und", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "NN", "NN", "$.", "ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und Jahre nahen sich davor, ", "tokens": ["Und", "Jah\u00b7re", "na\u00b7hen", "sich", "da\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "PRF", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "So wird ", "tokens": ["So", "wird"], "token_info": ["word", "word"], "pos": ["ADV", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.25": {"text": "Du aber wirst mir auch Zeitlebenslang verg\u00f6nnen,", "tokens": ["Du", "a\u00b7ber", "wirst", "mir", "auch", "Zeit\u00b7le\u00b7bens\u00b7lang", "ver\u00b7g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+--+----+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Da\u00df ich, so oft ichs nur bemerke, trag und seh,", "tokens": ["Da\u00df", "ich", ",", "so", "oft", "ichs", "nur", "be\u00b7mer\u00b7ke", ",", "trag", "und", "seh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "ADV", "PIS", "ADV", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Allzeit auf ", "tokens": ["All\u00b7zeit", "auf"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.28": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.29": {"text": "Du wollest fernerhin ", "tokens": ["Du", "wol\u00b7lest", "fer\u00b7ner\u00b7hin"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "So wird mein ganzer Sinn in tausend Lust gesetzt,", "tokens": ["So", "wird", "mein", "gan\u00b7zer", "Sinn", "in", "tau\u00b7send", "Lust", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So acht ich mich begl\u00fcckt, und werde hoch erg\u00f6tzt.", "tokens": ["So", "acht", "ich", "mich", "be\u00b7gl\u00fcckt", ",", "und", "wer\u00b7de", "hoch", "er\u00b7g\u00f6tzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "PPER", "PRF", "VVPP", "$,", "KON", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}