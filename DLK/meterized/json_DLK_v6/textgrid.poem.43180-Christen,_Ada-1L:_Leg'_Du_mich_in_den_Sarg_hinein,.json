{"textgrid.poem.43180": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Leg' Du mich in den Sarg hinein,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leg' Du mich in den Sarg hinein,", "tokens": ["Leg'", "Du", "mich", "in", "den", "Sarg", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schlie\u00df Du den Deckel zu,", "tokens": ["Schlie\u00df", "Du", "den", "De\u00b7ckel", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hinter meinem Sarg allein,", "tokens": ["Und", "hin\u00b7ter", "mei\u00b7nem", "Sarg", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geh' Du \u2013 Niemand als Du.", "tokens": ["Geh'", "Du", "\u2013", "Nie\u00b7mand", "als", "Du", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PIS", "KOKOM", "PPER", "$."], "meter": "+-+-++", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Den ich geliebt, und Leid's gethan", "tokens": ["Den", "ich", "ge\u00b7liebt", ",", "und", "Lei\u00b7d's", "ge\u00b7than"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPER", "VVPP", "$,", "KON", "NN", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Warst Du \u2013 nur Du allein....", "tokens": ["Warst", "Du", "\u2013", "nur", "Du", "al\u00b7lein", "...."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "ADV", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Komm' nie zu meinem Grabe Mann,", "tokens": ["Komm'", "nie", "zu", "mei\u00b7nem", "Gra\u00b7be", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich will vergessen sein.", "tokens": ["Ich", "will", "ver\u00b7ges\u00b7sen", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}