{"dta.poem.17202": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "52.  Die Liebe hats erfunden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df GOtt gecreutzigt wird! da\u00df man jhn kan verwun-", "tokens": ["Da\u00df", "Gott", "ge\u00b7cr\u00b7eut\u00b7zigt", "wird", "!", "da\u00df", "man", "jhn", "kan", "ver\u00b7wun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVPP", "VAFIN", "$.", "KOUS", "PIS", "PPER", "VMFIN", "TRUNC"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "den!", "tokens": ["den", "!"], "token_info": ["word", "punct"], "pos": ["ART", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Da\u00df Er die Schmach vertr\u00e4gt/ die man jhm ange-", "tokens": ["Da\u00df", "Er", "die", "Schmach", "ver\u00b7tr\u00e4gt", "/", "die", "man", "jhm", "an\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$(", "PRELS", "PIS", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "than!", "tokens": ["than", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Da\u00df Er solch\u2019 Angst aussteht! und da\u00df Er sterben kan!", "tokens": ["Da\u00df", "Er", "solch'", "Angst", "aus\u00b7steht", "!", "und", "da\u00df", "Er", "ster\u00b7ben", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$.", "KON", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verwundere dich nicht/ die Liebe hats erfunden.", "tokens": ["Ver\u00b7wun\u00b7de\u00b7re", "dich", "nicht", "/", "die", "Lie\u00b7be", "hats", "er\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$(", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}