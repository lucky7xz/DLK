{"textgrid.poem.25851": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[und jede Pore, die einst f\u00fcr dich brannte]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und jede Pore, die einst f\u00fcr dich brannte,", "tokens": ["Und", "je\u00b7de", "Po\u00b7re", ",", "die", "einst", "f\u00fcr", "dich", "brann\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jeder Gedanke, der dich kosend nannte,", "tokens": ["Je\u00b7der", "Ge\u00b7dan\u00b7ke", ",", "der", "dich", "ko\u00b7send", "nann\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Mu\u00df sich in meinem Blute hassend wenden", "tokens": ["Mu\u00df", "sich", "in", "mei\u00b7nem", "Blu\u00b7te", "has\u00b7send", "wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "APPR", "PPOSAT", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und statt der S\u00fc\u00dfe \u2013 Galle nach dir senden.", "tokens": ["Und", "statt", "der", "S\u00fc\u00b7\u00dfe", "\u2013", "Gal\u00b7le", "nach", "dir", "sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$(", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch das ist nicht das Ende.", "tokens": ["Doch", "das", "ist", "nicht", "das", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Ende ist, wenn meinen H\u00e4nden,", "tokens": ["Das", "En\u00b7de", "ist", ",", "wenn", "mei\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Meinen Lippen, meinen Augen", "tokens": ["Mei\u00b7nen", "Lip\u00b7pen", ",", "mei\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das schwere, lange Bluten endet,", "tokens": ["Das", "schwe\u00b7re", ",", "lan\u00b7ge", "Blu\u00b7ten", "en\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und sie nach langem fremden Schweigen", "tokens": ["Und", "sie", "nach", "lan\u00b7gem", "frem\u00b7den", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sich endlich wieder zu dir neigen", "tokens": ["Sich", "end\u00b7lich", "wie\u00b7der", "zu", "dir", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "ADV", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und sagen k\u00f6nnen: \u00bbFreund\u00ab.", "tokens": ["Und", "sa\u00b7gen", "k\u00f6n\u00b7nen", ":", "\u00bb", "Freund", "\u00ab", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["KON", "VVINF", "VMINF", "$.", "$(", "NN", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Dann ist das Ende meiner gro\u00dfen Liebe.", "tokens": ["Dann", "ist", "das", "En\u00b7de", "mei\u00b7ner", "gro\u00b7\u00dfen", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}