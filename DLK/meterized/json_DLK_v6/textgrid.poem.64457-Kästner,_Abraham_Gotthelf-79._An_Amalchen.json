{"textgrid.poem.64457": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "79. An Amalchen", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Freunde nur von sieben Tagen", "tokens": ["Dem", "Freun\u00b7de", "nur", "von", "sie\u00b7ben", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hast du bethr\u00e4nt die Blicke nachgeschickt;", "tokens": ["Hast", "du", "be\u00b7thr\u00e4nt", "die", "Bli\u00b7cke", "nach\u00b7ge\u00b7schickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es lasse Der, den einst dein Herz begl\u00fcckt,", "tokens": ["Es", "las\u00b7se", "Der", ",", "den", "einst", "dein", "Herz", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dich nie um seinen Abschied klagen!", "tokens": ["Dich", "nie", "um", "sei\u00b7nen", "Ab\u00b7schied", "kla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}