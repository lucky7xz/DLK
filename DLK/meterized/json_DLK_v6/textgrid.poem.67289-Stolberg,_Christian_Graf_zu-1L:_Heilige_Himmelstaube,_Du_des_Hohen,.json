{"textgrid.poem.67289": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heilige Himmelstaube, Du des Hohen,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heilige Himmelstaube, Du des Hohen,", "tokens": ["Hei\u00b7li\u00b7ge", "Him\u00b7mel\u00b7stau\u00b7be", ",", "Du", "des", "Ho\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Unerforschlichen Sinnbild, schwebe nieder,", "tokens": ["Un\u00b7er\u00b7for\u00b7schli\u00b7chen", "Sinn\u00b7bild", ",", "schwe\u00b7be", "nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Athm' in's Herz mir schmachtende Sehnsucht, frommer", "tokens": ["Athm'", "in's", "Herz", "mir", "schmach\u00b7ten\u00b7de", "Sehn\u00b7sucht", ",", "from\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "APPRART", "NN", "PPER", "ADJA", "NN", "$,", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Liebe Vertraute;", "tokens": ["Lie\u00b7be", "Ver\u00b7trau\u00b7te", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Oeffnet es ihr sich, o, dann weih's zu Seinem", "tokens": ["Oeff\u00b7net", "es", "ihr", "sich", ",", "o", ",", "dann", "weih's", "zu", "Sei\u00b7nem"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "PRF", "$,", "FM", "$,", "ADV", "PIS", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Tempel! Segnendes S\u00e4useln hauch' hinweg der", "tokens": ["Tem\u00b7pel", "!", "Seg\u00b7nen\u00b7des", "S\u00e4u\u00b7seln", "hauch'", "hin\u00b7weg", "der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "NE", "NN", "VVFIN", "ADV", "ART"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Erde Staub, es komme der Gast und nehme", "tokens": ["Er\u00b7de", "Staub", ",", "es", "kom\u00b7me", "der", "Gast", "und", "neh\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Bleibende Wohnung!", "tokens": ["Blei\u00b7ben\u00b7de", "Woh\u00b7nung", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}