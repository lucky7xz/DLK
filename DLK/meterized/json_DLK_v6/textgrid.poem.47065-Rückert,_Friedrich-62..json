{"textgrid.poem.47065": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "62.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du mein gar zu flei\u00df'ges Spinnerm\u00e4dchen,", "tokens": ["O", "du", "mein", "gar", "zu", "flei\u00df'\u00b7ges", "Spin\u00b7ner\u00b7m\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im sch\u00f6nen selbst gesponnenen Gew\u00e4ndchen,", "tokens": ["Im", "sch\u00f6\u00b7nen", "selbst", "ge\u00b7spon\u00b7ne\u00b7nen", "Ge\u00b7w\u00e4nd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die r\u00fchrig mit dem F\u00fc\u00dfchen und dem H\u00e4ndchen", "tokens": ["Die", "r\u00fch\u00b7rig", "mit", "dem", "F\u00fc\u00df\u00b7chen", "und", "dem", "H\u00e4nd\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du sitzest Tag und Nacht am Spinner\u00e4dchen.", "tokens": ["Du", "sit\u00b7zest", "Tag", "und", "Nacht", "am", "Spin\u00b7ne\u00b7r\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wieviel gesponnen hast du feine F\u00e4dchen", "tokens": ["Wie\u00b7viel", "ge\u00b7spon\u00b7nen", "hast", "du", "fei\u00b7ne", "F\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "VAFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ausgesponnen sie zu festen B\u00e4ndchen;", "tokens": ["Und", "aus\u00b7ge\u00b7spon\u00b7nen", "sie", "zu", "fes\u00b7ten", "B\u00e4nd\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O wieviel hast du angesponnen St\u00e4ndchen", "tokens": ["O", "wie\u00b7viel", "hast", "du", "an\u00b7ge\u00b7spon\u00b7nen", "St\u00e4nd\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "VAFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Th\u00fcrchen oft und oft am Fensterl\u00e4dchen.", "tokens": ["Am", "Th\u00fcr\u00b7chen", "oft", "und", "oft", "am", "Fens\u00b7ter\u00b7l\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "KON", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O wieviel haben Vetterchen und B\u00e4schen", "tokens": ["O", "wie\u00b7viel", "ha\u00b7ben", "Vet\u00b7ter\u00b7chen", "und", "B\u00e4\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "VAFIN", "NN", "KON", "NN"], "meter": "+--+-+---+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Verworrene Gespinste dir ins H\u00e4uschen", "tokens": ["Ver\u00b7wor\u00b7re\u00b7ne", "Ge\u00b7spins\u00b7te", "dir", "ins", "H\u00e4usc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Getragen, mit umsponnen dich beim T\u00e4nzchen.", "tokens": ["Ge\u00b7tra\u00b7gen", ",", "mit", "um\u00b7spon\u00b7nen", "dich", "beim", "T\u00e4nz\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann hat sich oft aus H\u00e4lmchen und aus Gr\u00e4schen", "tokens": ["Dann", "hat", "sich", "oft", "aus", "H\u00e4lm\u00b7chen", "und", "aus", "Gr\u00e4sc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "ADV", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entsponnen zwischen uns ein Haderstr\u00e4u\u00dfchen,", "tokens": ["Ent\u00b7spon\u00b7nen", "zwi\u00b7schen", "uns", "ein", "Ha\u00b7der\u00b7str\u00e4u\u00df\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch oftmals auch gewebt ein Liebeskr\u00e4nzchen.", "tokens": ["Doch", "oft\u00b7mals", "auch", "ge\u00b7webt", "ein", "Lie\u00b7bes\u00b7kr\u00e4nz\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}