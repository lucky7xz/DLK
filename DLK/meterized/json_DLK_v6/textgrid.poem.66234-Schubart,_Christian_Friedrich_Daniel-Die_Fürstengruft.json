{"textgrid.poem.66234": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Die F\u00fcrstengruft", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da liegen sie, die stolzen F\u00fcrstentr\u00fcmmer,", "tokens": ["Da", "lie\u00b7gen", "sie", ",", "die", "stol\u00b7zen", "F\u00fcrs\u00b7ten\u00b7tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ehmals die G\u00f6tzen ihrer Welt!", "tokens": ["Eh\u00b7mals", "die", "G\u00f6t\u00b7zen", "ih\u00b7rer", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Da liegen sie, vom f\u00fcrchterlichen Schimmer", "tokens": ["Da", "lie\u00b7gen", "sie", ",", "vom", "f\u00fcrch\u00b7ter\u00b7li\u00b7chen", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des blassen Tags erhellt!", "tokens": ["Des", "blas\u00b7sen", "Tags", "er\u00b7hellt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die alten S\u00e4rge leuchten in der dunkeln", "tokens": ["Die", "al\u00b7ten", "S\u00e4r\u00b7ge", "leuch\u00b7ten", "in", "der", "dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verwesungsgruft, wie faules Holz;", "tokens": ["Ver\u00b7we\u00b7sungs\u00b7gruft", ",", "wie", "fau\u00b7les", "Holz", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie matt die gro\u00dfen Silberschilde funkeln,", "tokens": ["Wie", "matt", "die", "gro\u00b7\u00dfen", "Sil\u00b7ber\u00b7schil\u00b7de", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der F\u00fcrsten letzter Stolz!", "tokens": ["Der", "F\u00fcrs\u00b7ten", "letz\u00b7ter", "Stolz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Entsetzen packt den Wandrer hier am Haare,", "tokens": ["Ent\u00b7set\u00b7zen", "packt", "den", "Wand\u00b7rer", "hier", "am", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geu\u00dft Schauer \u00fcber seine Haut,", "tokens": ["Geu\u00dft", "Schau\u00b7er", "\u00fc\u00b7ber", "sei\u00b7ne", "Haut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Eitelkeit, gelehnt an eine Bahre,", "tokens": ["Wo", "Ei\u00b7tel\u00b7keit", ",", "ge\u00b7lehnt", "an", "ei\u00b7ne", "Bah\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus hohlen Augen schaut.", "tokens": ["Aus", "hoh\u00b7len", "Au\u00b7gen", "schaut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie f\u00fcrchterlich ist hier des Nachhalls Stimme,", "tokens": ["Wie", "f\u00fcrch\u00b7ter\u00b7lich", "ist", "hier", "des", "Nach\u00b7halls", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Zehentritt st\u00f6rt seine Ruh'!", "tokens": ["Ein", "Ze\u00b7hen\u00b7tritt", "st\u00f6rt", "sei\u00b7ne", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Wetter Gottes spricht mit lauterm Grimme:", "tokens": ["Kein", "Wet\u00b7ter", "Got\u00b7tes", "spricht", "mit", "lau\u00b7term", "Grim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O Mensch, wie klein bist du!", "tokens": ["O", "Mensch", ",", "wie", "klein", "bist", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Denn ach! hier liegt der edle F\u00fcrst, der gute,", "tokens": ["Denn", "ach", "!", "hier", "liegt", "der", "ed\u00b7le", "F\u00fcrst", ",", "der", "gu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum V\u00f6lkersegen einst gesandt,", "tokens": ["Zum", "V\u00f6l\u00b7ker\u00b7se\u00b7gen", "einst", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie der, den Gott zur Nationenruthe", "tokens": ["Wie", "der", ",", "den", "Gott", "zur", "Na\u00b7ti\u00b7o\u00b7nen\u00b7ru\u00b7the"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "$,", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Zorn zusammenband.", "tokens": ["Im", "Zorn", "zu\u00b7sam\u00b7men\u00b7band", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "An ihren Urnen weinen Marmorgeister,", "tokens": ["An", "ih\u00b7ren", "Ur\u00b7nen", "wei\u00b7nen", "Mar\u00b7mor\u00b7geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch kalte Thr\u00e4nen nur, von Stein,", "tokens": ["Doch", "kal\u00b7te", "Thr\u00e4\u00b7nen", "nur", ",", "von", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lachend grub vielleicht ein welscher Meister", "tokens": ["Und", "la\u00b7chend", "grub", "viel\u00b7leicht", "ein", "wel\u00b7scher", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie einst dem Marmor ein.", "tokens": ["Sie", "einst", "dem", "Mar\u00b7mor", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da liegen Sch\u00e4del mit verloschnen Blicken,", "tokens": ["Da", "lie\u00b7gen", "Sch\u00e4\u00b7del", "mit", "ver\u00b7lo\u00b7schnen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ehmals hoch herabgedroht,", "tokens": ["Die", "eh\u00b7mals", "hoch", "her\u00b7ab\u00b7ge\u00b7droht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Menschheit Schrecken! denn an ihrem Nicken", "tokens": ["Der", "Menschheit", "Schre\u00b7cken", "!", "denn", "an", "ih\u00b7rem", "Ni\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$.", "KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hing Leben oder Tod.", "tokens": ["Hing", "Le\u00b7ben", "o\u00b7der", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nun ist die Hand herabgefault zum Knochen,", "tokens": ["Nun", "ist", "die", "Hand", "her\u00b7ab\u00b7ge\u00b7fault", "zum", "Kno\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die oft mit kaltem Federzug", "tokens": ["Die", "oft", "mit", "kal\u00b7tem", "Fe\u00b7der\u00b7zug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Weisen, der am Thron zu laut gesprochen,", "tokens": ["Den", "Wei\u00b7sen", ",", "der", "am", "Thron", "zu", "laut", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In harte Fesseln schlug.", "tokens": ["In", "har\u00b7te", "Fes\u00b7seln", "schlug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Zum Todtenbein ist nun die Brust geworden,", "tokens": ["Zum", "Tod\u00b7ten\u00b7bein", "ist", "nun", "die", "Brust", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einst eingeh\u00fcllt in Goldgewand,", "tokens": ["Einst", "ein\u00b7ge\u00b7h\u00fcllt", "in", "Gold\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Daran ein Stern und ein entweihter Orden", "tokens": ["Da\u00b7ran", "ein", "Stern", "und", "ein", "ent\u00b7weih\u00b7ter", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie zween Kometen stand.", "tokens": ["Wie", "zween", "Ko\u00b7me\u00b7ten", "stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Vertrocknet und verschrumpft sind die Kan\u00e4le,", "tokens": ["Ver\u00b7trock\u00b7net", "und", "ver\u00b7schrumpft", "sind", "die", "Ka\u00b7n\u00e4\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drin geiles Blut wie Feuer flo\u00df,", "tokens": ["Drin", "gei\u00b7les", "Blut", "wie", "Feu\u00b7er", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das sch\u00e4umend Gift der Unschuld in die Seele,", "tokens": ["Das", "sch\u00e4u\u00b7mend", "Gift", "der", "Un\u00b7schuld", "in", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie in den K\u00f6rper go\u00df.", "tokens": ["Wie", "in", "den", "K\u00f6r\u00b7per", "go\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sprecht H\u00f6flinge, mit Ehrfurcht auf der Lippe,", "tokens": ["Sprecht", "H\u00f6f\u00b7lin\u00b7ge", ",", "mit", "Ehr\u00b7furcht", "auf", "der", "Lip\u00b7pe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Nun Schmeichelei'n ins taube Ohr!", "tokens": ["Nun", "Schmei\u00b7che\u00b7lei'n", "ins", "tau\u00b7be", "Ohr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ber\u00e4uchert das durchlauchtige Gerippe", "tokens": ["Be\u00b7r\u00e4u\u00b7chert", "das", "durch\u00b7lauch\u00b7ti\u00b7ge", "Ge\u00b7rip\u00b7pe"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Weihrauch, wie zuvor!", "tokens": ["Mit", "Weih\u00b7rauch", ",", "wie", "zu\u00b7vor", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Er steht nicht auf, euch Beifall zuzul\u00e4cheln,", "tokens": ["Er", "steht", "nicht", "auf", ",", "euch", "Bei\u00b7fall", "zu\u00b7zu\u00b7l\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,", "PPER", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wiehert keine Zoten mehr,", "tokens": ["Und", "wie\u00b7hert", "kei\u00b7ne", "Zo\u00b7ten", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "ADV", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Damit geschminkte Zofen ihn bef\u00e4cheln,", "tokens": ["Da\u00b7mit", "ge\u00b7schmink\u00b7te", "Zo\u00b7fen", "ihn", "be\u00b7f\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schamlos und geil, wie er.", "tokens": ["Scham\u00b7los", "und", "geil", ",", "wie", "er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Sie liegen nun, den eiser'n Schlaf zu schlafen,", "tokens": ["Sie", "lie\u00b7gen", "nun", ",", "den", "ei\u00b7ser'n", "Schlaf", "zu", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Menschengeiseln, unbetraurt,", "tokens": ["Die", "Men\u00b7schen\u00b7gei\u00b7seln", ",", "un\u00b7be\u00b7traurt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Felsengrab, ver\u00e4chtlicher als Sklaven,", "tokens": ["Im", "Fel\u00b7sen\u00b7grab", ",", "ver\u00b7\u00e4cht\u00b7li\u00b7cher", "als", "Skla\u00b7ven", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Kerker eingemaurt.", "tokens": ["In", "Ker\u00b7ker", "ein\u00b7ge\u00b7maurt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Sie, die im eh'rnen Busen niemals f\u00fchlten", "tokens": ["Sie", ",", "die", "im", "eh'r\u00b7nen", "Bu\u00b7sen", "nie\u00b7mals", "f\u00fchl\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPRART", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Schrecken der Religion,", "tokens": ["Die", "Schre\u00b7cken", "der", "Re\u00b7li\u00b7gi\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und gottgeschaffne, bessre Menschen hielten", "tokens": ["Und", "gott\u00b7ge\u00b7schaff\u00b7ne", ",", "bess\u00b7re", "Men\u00b7schen", "hiel\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr Vieh, bestimmt zur Frohn;", "tokens": ["F\u00fcr", "Vieh", ",", "be\u00b7stimmt", "zur", "Frohn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Die das Gewissen, jenen m\u00e4cht'gen Kl\u00e4ger,", "tokens": ["Die", "das", "Ge\u00b7wis\u00b7sen", ",", "je\u00b7nen", "m\u00e4cht'\u00b7gen", "Kl\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Durch Trommelschlag, durch welsche Trillerschl\u00e4ger", "tokens": ["Durch", "Trom\u00b7mel\u00b7schlag", ",", "durch", "wel\u00b7sche", "Tril\u00b7ler\u00b7schl\u00e4\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Jagdl\u00e4rm \u00fcbert\u0