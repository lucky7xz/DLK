, "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein W\u00fcthrich hier erwacht!", "tokens": ["Kein", "W\u00fcth\u00b7rich", "hier", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Hier klatsche nicht des armen Landmanns Peitsche,", "tokens": ["Hier", "klat\u00b7sche", "nicht", "des", "ar\u00b7men", "Land\u00b7manns", "Peit\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Nachts das Wild vom Acker scheucht,", "tokens": ["Die", "Nachts", "das", "Wild", "vom", "A\u00b7cker", "scheucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An diesem Gitter weile nicht der Deutsche,", "tokens": ["An", "die\u00b7sem", "Git\u00b7ter", "wei\u00b7le", "nicht", "der", "Deut\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der siech vor\u00fcberkeucht!", "tokens": ["Der", "siech", "vor\u00b7\u00fc\u00b7berk\u00b7eucht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Hier heule nicht der bleiche Waisenknabe,", "tokens": ["Hier", "heu\u00b7le", "nicht", "der", "blei\u00b7che", "Wai\u00b7sen\u00b7kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem ein Tyrann den Vater nahm;", "tokens": ["Dem", "ein", "Ty\u00b7rann", "den", "Va\u00b7ter", "nahm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nie fluche hier der Kr\u00fcppel an dem Stabe,", "tokens": ["Nie", "flu\u00b7che", "hier", "der", "Kr\u00fcp\u00b7pel", "an", "dem", "Sta\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von fremdem Solde lahm!", "tokens": ["Von", "frem\u00b7dem", "Sol\u00b7de", "lahm", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Damit die Qu\u00e4ler nicht zu fr\u00fch erwachen,", "tokens": ["Da\u00b7mit", "die", "Qu\u00e4\u00b7ler", "nicht", "zu", "fr\u00fch", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PTKNEG", "PTKA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seid menschlicher, erweckt sie nicht.", "tokens": ["Seid", "menschli\u00b7cher", ",", "er\u00b7weckt", "sie", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ha! fr\u00fch genug wird \u00fcber ihnen krachen", "tokens": ["Ha", "!", "fr\u00fch", "ge\u00b7nug", "wird", "\u00fc\u00b7ber", "ih\u00b7nen", "kra\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "w