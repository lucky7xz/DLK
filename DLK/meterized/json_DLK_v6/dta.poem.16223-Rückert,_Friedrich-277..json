{"dta.poem.16223": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "277.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Je l\u00e4nger du's gehabt, je l\u00e4nger willst du's haben,", "tokens": ["Je", "l\u00e4n\u00b7ger", "du's", "ge\u00b7habt", ",", "je", "l\u00e4n\u00b7ger", "willst", "du's", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "VAPP", "$,", "ADV", "ADJD", "VMFIN", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ein Geliebtes wird dir stets zu fr\u00fch begraben.", "tokens": ["Und", "ein", "Ge\u00b7lieb\u00b7tes", "wird", "dir", "stets", "zu", "fr\u00fch", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du bildetest dir ein, es sei auf ewig dein,", "tokens": ["Du", "bil\u00b7de\u00b7test", "dir", "ein", ",", "es", "sei", "auf", "e\u00b7wig", "dein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VAFIN", "APPR", "ADJD", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und solltest Gott, der dir's solang lie\u00df, dankbar seyn.", "tokens": ["Und", "soll\u00b7test", "Gott", ",", "der", "dir's", "so\u00b7lang", "lie\u00df", ",", "dank\u00b7bar", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "p