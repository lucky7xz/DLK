{"textgrid.poem.37312": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: In der ersten Nacht des Maien", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der ersten Nacht des Maien", "tokens": ["In", "der", "ers\u00b7ten", "Nacht", "des", "Mai\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4\u00dft's den Hexen keine Ruh.", "tokens": ["L\u00e4\u00dft's", "den", "He\u00b7xen", "kei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich gesellig zu erfreuen,", "tokens": ["Sich", "ge\u00b7sel\u00b7lig", "zu", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eilen sie dem Brocken zu.", "tokens": ["Ei\u00b7len", "sie", "dem", "Bro\u00b7cken", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dorten haben sie ihr Kr\u00e4nzchen.", "tokens": ["Dor\u00b7ten", "ha\u00b7ben", "sie", "ihr", "Kr\u00e4nz\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Man verleumdet, man verf\u00fchrt,", "tokens": ["Man", "ver\u00b7leum\u00b7det", ",", "man", "ver\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Macht ein lasterhaftes T\u00e4nzchen,", "tokens": ["Macht", "ein", "las\u00b7ter\u00b7haf\u00b7tes", "T\u00e4nz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Teufel pr\u00e4sidiert.", "tokens": ["Und", "der", "Teu\u00b7fel", "pr\u00e4\u00b7si\u00b7diert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}