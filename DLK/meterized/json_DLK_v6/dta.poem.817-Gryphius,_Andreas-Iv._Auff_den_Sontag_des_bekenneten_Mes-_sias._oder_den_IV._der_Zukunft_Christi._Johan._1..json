{"dta.poem.817": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Iv.  Auff den Sontag des bekenneten Mes-  \n sias. oder den  IV.  der Zukunft Christi.  \n Johan. 1.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Was f\u00fcrcht mein bl\u00f6des Fleisch dich IESu zu bekennen? ", "tokens": ["Was", "f\u00fcrcht", "mein", "bl\u00f6\u00b7des", "Fleisch", "dich", "Ie\u00b7Su", "zu", "be\u00b7ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich bin\u2019s ja der recht Geist-vnd Mutt-v\u00f1 Eyvers voll", "tokens": ["Ich", "bin's", "ja", "der", "recht", "Geist\u00b7\u00b7vnd", "Mut\u00b7tv\u00f1", "Ey\u00b7vers", "voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJD", "NN", "NN", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dich in dem w\u00fc\u00dften thal der welt au\u00dfruffen soll", "tokens": ["Dich", "in", "dem", "w\u00fc\u00df\u00b7ten", "thal", "der", "welt", "au\u00df\u00b7ruf\u00b7fen", "soll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd dich mit hoher stim in aller Ohren nennen!", "tokens": ["Vnd", "dich", "mit", "ho\u00b7her", "stim", "in", "al\u00b7ler", "Oh\u00b7ren", "nen\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auch wei\u00df vnd glaub ich fest da\u00df mich von dir abtrennen", "tokens": ["Auch", "wei\u00df", "vnd", "glaub", "ich", "fest", "da\u00df", "mich", "von", "dir", "ab\u00b7tren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "VVFIN", "KOUS", "PPER", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "(weil du im mittel stehst/) kan weder weh\u2019 noch wol", "tokens": ["(", "weil", "du", "im", "mit\u00b7tel", "stehst", "/", ")", "kan", "we\u00b7der", "weh'", "noch", "wol"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$(", "$(", "VMFIN", "KON", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verley nur da\u00df ich mich an dir mein Schutz erhol", "tokens": ["Ver\u00b7ley", "nur", "da\u00df", "ich", "mich", "an", "dir", "mein", "Schutz", "er\u00b7hol"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "KOUS", "PPER", "PRF", "APPR", "PPER", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd la\u00df mich von der Flamm/ mit der du tauff\u2019st entbre\u00f1en.", "tokens": ["Vnd", "la\u00df", "mich", "von", "der", "Flamm", "/", "mit", "der", "du", "tauff'st", "ent\u00b7bre\u00f1en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "ART", "NN", "$(", "APPR", "PRELS", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zeuch selbst in dis mein Hertz/ dir ist der weg bereit;", "tokens": ["Zeuch", "selbst", "in", "dis", "mein", "Hertz", "/", "dir", "ist", "der", "weg", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PDS", "PPOSAT", "NN", "$(", "PPER", "VAFIN", "ART", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mach eben was erh\u00f6h\u2019t/ vertreib das stette leid.", "tokens": ["Mach", "e\u00b7ben", "was", "er\u00b7h\u00f6h't", "/", "ver\u00b7treib", "das", "stet\u00b7te", "leid", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PWS", "VVFIN", "$(", "VVIMP", "PDS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O Schlangentretter tritt was dir wil wiederstehen.", "tokens": ["O", "Schlan\u00b7gent\u00b7ret\u00b7ter", "tritt", "was", "dir", "wil", "wie\u00b7ders\u00b7te\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PWS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-++-+--", "measure": "unknown.measure.hexa"}}, "stanza.4": {"line.1": {"text": "Erheb was niedrig ist/ vergleiche was nicht recht/", "tokens": ["Er\u00b7heb", "was", "nied\u00b7rig", "ist", "/", "ver\u00b7glei\u00b7che", "was", "nicht", "recht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "ADJD", "VAFIN", "$(", "VVFIN", "PWS", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Raum ab was hindern kan. Vnd la\u00df mir/ deinem Knecht", "tokens": ["Raum", "ab", "was", "hin\u00b7dern", "kan", ".", "Vnd", "la\u00df", "mir", "/", "dei\u00b7nem", "Knecht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "PIS", "VVINF", "VMFIN", "$.", "KON", "VVIMP", "PPER", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den glantz der Herrligkeit! O Lebens Sonn\u2019 aufgehen/", "tokens": ["Den", "glantz", "der", "Herr\u00b7lig\u00b7keit", "!", "O", "Le\u00b7bens", "Sonn'", "auf\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "NE", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}