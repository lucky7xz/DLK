{"textgrid.poem.63182": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1L: Strophis, K\u00f6nig von Phokis,", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Strophis, K\u00f6nig von Phokis,", "tokens": ["Stro\u00b7phis", ",", "K\u00f6\u00b7nig", "von", "Pho\u00b7kis", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Erzog Orest und Pylades.", "tokens": ["Er\u00b7zog", "O\u00b7rest", "und", "Py\u00b7la\u00b7des", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "NE", "$."], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Hand in Hand gingen die Knaben,", "tokens": ["Hand", "in", "Hand", "gin\u00b7gen", "die", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Brust an Brust schliefen die Knaben,", "tokens": ["Brust", "an", "Brust", "schlie\u00b7fen", "die", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Mund an Mund sangen die Knaben.", "tokens": ["Mund", "an", "Mund", "san\u00b7gen", "die", "Kna\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Sie warfen ihre Sehnsucht und den Diskos", "tokens": ["Sie", "war\u00b7fen", "ih\u00b7re", "Sehn\u00b7sucht", "und", "den", "Dis\u00b7kos"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Gleich weit. Und stoben", "tokens": ["Gleich", "weit", ".", "Und", "sto\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$.", "KON", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Im Viergespann als Sieger durch das Ziel.", "tokens": ["Im", "Vier\u00b7ge\u00b7spann", "als", "Sie\u00b7ger", "durch", "das", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da wollte es Ananke, dass die Eumeniden", "tokens": ["Da", "woll\u00b7te", "es", "A\u00b7nan\u00b7ke", ",", "dass", "die", "Eu\u00b7me\u00b7ni\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Orest befielen und sein Hirn", "tokens": ["O\u00b7rest", "be\u00b7fie\u00b7len", "und", "sein", "Hirn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVINF", "KON", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wie Hunde fleischten.", "tokens": ["Wie", "Hun\u00b7de", "fleischten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Im Heiligtum zu Delphi", "tokens": ["Im", "Hei\u00b7lig\u00b7tum", "zu", "Del\u00b7phi"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Orestes lag ermattet.", "tokens": ["O\u00b7res\u00b7tes", "lag", "er\u00b7mat\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Um seine Stirne st\u00fcrmten", "tokens": ["Um", "sei\u00b7ne", "Stir\u00b7ne", "st\u00fcrm\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die G\u00f6ttinnen der Nacht.", "tokens": ["Die", "G\u00f6t\u00b7tin\u00b7nen", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+---+", "measure": "dactylic.init"}, "line.5": {"text": "Die Flederm\u00e4use kreischten", "tokens": ["Die", "Fle\u00b7der\u00b7m\u00e4u\u00b7se", "kreischten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und die Erinnyen sangen:", "tokens": ["Und", "die", "E\u00b7rin\u00b7ny\u00b7en", "san\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die Mutter ist erschlagen,", "tokens": ["Die", "Mut\u00b7ter", "ist", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die M\u00f6rderin des Vaters;", "tokens": ["Die", "M\u00f6r\u00b7de\u00b7rin", "des", "Va\u00b7ters", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Der Mord hat Mord geboren:", "tokens": ["Der", "Mord", "hat", "Mord", "ge\u00b7bo\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Der M\u00f6rder sei gef\u00e4llt!", "tokens": ["Der", "M\u00f6r\u00b7der", "sei", "ge\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Menschen flohn entsetzt. Nur Pylades", "tokens": ["Die", "Men\u00b7schen", "flohn", "ent\u00b7setzt", ".", "Nur", "Py\u00b7la\u00b7des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$.", "ADV", "NE"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Blieb bei dem Freund und liebte", "tokens": ["Blieb", "bei", "dem", "Freund", "und", "lieb\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den M\u00f6rder wie den Sch\u00f6pfer er geliebt.", "tokens": ["Den", "M\u00f6r\u00b7der", "wie", "den", "Sch\u00f6p\u00b7fer", "er", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und liebte seinen Wahnsinn,", "tokens": ["Und", "lieb\u00b7te", "sei\u00b7nen", "Wahn\u00b7sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die irre Tat, den staubbedeckten Leib,", "tokens": ["Die", "ir\u00b7re", "Tat", ",", "den", "staub\u00b7be\u00b7deck\u00b7ten", "Leib", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie er den J\u00fcngling nicht geliebt,", "tokens": ["Wie", "er", "den", "J\u00fcng\u00b7ling", "nicht", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den klug gestaltenden,", "tokens": ["Den", "klug", "ge\u00b7stal\u00b7ten\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den sch\u00f6n gestalteten.", "tokens": ["Den", "sch\u00f6n", "ge\u00b7stal\u00b7te\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Er schlief mit ihm wie je. Orest, der Irre,", "tokens": ["Er", "schlief", "mit", "ihm", "wie", "je", ".", "O\u00b7rest", ",", "der", "Ir\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KOKOM", "ADV", "$.", "NE", "$,", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Erf\u00fcllte Bett und Raum", "tokens": ["Er\u00b7f\u00fcll\u00b7te", "Bett", "und", "Raum"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und Traum", "tokens": ["Und", "Traum"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Mit Stank und Kot.", "tokens": ["Mit", "Stank", "und", "Kot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}