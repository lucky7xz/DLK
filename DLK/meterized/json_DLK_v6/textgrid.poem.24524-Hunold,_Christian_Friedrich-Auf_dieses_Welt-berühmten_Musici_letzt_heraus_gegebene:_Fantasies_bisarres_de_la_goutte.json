{"textgrid.poem.24524": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf dieses Welt-ber\u00fchmten Musici letzt heraus gegebene: Fantasies bisarres de la goutte", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein ", "tokens": ["Dein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Das so viel Anmuth l\u00e4\u00dft auf ein ", "tokens": ["Das", "so", "viel", "An\u00b7muth", "l\u00e4\u00dft", "auf", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "PIAT", "NN", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein deswegen wird es nicht von mir begehrt:", "tokens": ["Al\u00b7lein", "des\u00b7we\u00b7gen", "wird", "es", "nicht", "von", "mir", "be\u00b7gehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VAFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur Schenckens-", "tokens": ["Nur", "Schen\u00b7ckens"], "token_info": ["word", "word"], "pos": ["ADV", "TRUNC"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Kriegst du es wiederum/ so sind wir zwar betr\u00fcbt/", "tokens": ["Kriegst", "du", "es", "wie\u00b7de\u00b7rum", "/", "so", "sind", "wir", "zwar", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$(", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn dich der schlimme Gast in etwas mitgenommen:", "tokens": ["Wenn", "dich", "der", "schlim\u00b7me", "Gast", "in", "et\u00b7was", "mit\u00b7ge\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch wenn es dir darauf so viel erfreulichs giebt:", "tokens": ["Doch", "wenn", "es", "dir", "da\u00b7rauf", "so", "viel", "er\u00b7freu\u00b7lichs", "giebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "PAV", "ADV", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So kanst du es vor uns nicht oft genug bekommen.", "tokens": ["So", "kanst", "du", "es", "vor", "uns", "nicht", "oft", "ge\u00b7nug", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "APPR", "PPER", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}