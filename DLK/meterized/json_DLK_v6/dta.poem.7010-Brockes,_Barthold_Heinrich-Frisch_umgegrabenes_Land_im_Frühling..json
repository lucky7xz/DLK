{"dta.poem.7010": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Frisch umgegrabenes Land  \n im Fr\u00fchling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kein dunckel-brauner Samm't, in welchen sich das Licht", "tokens": ["Kein", "dun\u00b7ckel\u00b7brau\u00b7ner", "Sam\u00b7m't", ",", "in", "wel\u00b7chen", "sich", "das", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "APPR", "PWAT", "PRF", "ART", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auf eine sanffte Weise sencket,", "tokens": ["Auf", "ei\u00b7ne", "sanff\u00b7te", "Wei\u00b7se", "sen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erf\u00fcllt mit solcher Lust das Menschliche Gesicht;", "tokens": ["Er\u00b7f\u00fcllt", "mit", "sol\u00b7cher", "Lust", "das", "Menschli\u00b7che", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Als, wenn man Aug\u2019 und Sinnen lencket,", "tokens": ["Als", ",", "wenn", "man", "Aug'", "und", "Sin\u00b7nen", "len\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PIS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auf ein frisch umgegrabnes Land,", "tokens": ["Auf", "ein", "frisch", "um\u00b7ge\u00b7grab\u00b7nes", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und dessen Dunckelheit den Blick und den Verstand", "tokens": ["Und", "des\u00b7sen", "Dun\u00b7ckel\u00b7heit", "den", "Blick", "und", "den", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRELAT", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit Lust erf\u00fcllt, wenn man es recht bedencket.", "tokens": ["Mit", "Lust", "er\u00b7f\u00fcllt", ",", "wenn", "man", "es", "recht", "be\u00b7den\u00b7cket", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "KOUS", "PIS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}