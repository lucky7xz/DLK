{"textgrid.poem.64056": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Der Sommerregen", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie milde s\u00e4uselst Du, o k\u00fchler Regen,", "tokens": ["Wie", "mil\u00b7de", "s\u00e4u\u00b7selst", "Du", ",", "o", "k\u00fch\u00b7ler", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf die verschmachtende, verbleichte Flur.", "tokens": ["Auf", "die", "ver\u00b7schmach\u00b7ten\u00b7de", ",", "ver\u00b7bleich\u00b7te", "Flur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein l\u00e4ngst so heiss, so bang erflehter Seegen,", "tokens": ["Dein", "l\u00e4ngst", "so", "heiss", ",", "so", "bang", "er\u00b7fleh\u00b7ter", "See\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erfrischt die ganze seufzende Natur,", "tokens": ["Er\u00b7frischt", "die", "gan\u00b7ze", "seuf\u00b7zen\u00b7de", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und neu gest\u00e4rkt erheben Gras und B\u00e4ume", "tokens": ["Und", "neu", "ge\u00b7st\u00e4rkt", "er\u00b7he\u00b7ben", "Gras", "und", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die matten H\u00e4upter in der L\u00fcfte R\u00e4ume.", "tokens": ["Die", "mat\u00b7ten", "H\u00e4up\u00b7ter", "in", "der", "L\u00fcf\u00b7te", "R\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Sonne Gluth schien alles zu verzehren;", "tokens": ["Der", "Son\u00b7ne", "Gluth", "schien", "al\u00b7les", "zu", "ver\u00b7zeh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es welkte still dahin der Blumen Glanz.", "tokens": ["Es", "welk\u00b7te", "still", "da\u00b7hin", "der", "Blu\u00b7men", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PAV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Pflanzen neigten sich \u2013 ein allgemein Verheeren", "tokens": ["Die", "Pflan\u00b7zen", "neig\u00b7ten", "sich", "\u2013", "ein", "all\u00b7ge\u00b7mein", "Ver\u00b7hee\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "$(", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bedrohte selbst der W\u00e4lder dunklen Kranz,", "tokens": ["Be\u00b7droh\u00b7te", "selbst", "der", "W\u00e4l\u00b7der", "dunk\u00b7len", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und brennend schien in ihrer dumpfen Schw\u00fcle", "tokens": ["Und", "bren\u00b7nend", "schien", "in", "ih\u00b7rer", "dum\u00b7pfen", "Schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die schwere Luft dem lechzenden Gef\u00fchle.", "tokens": ["Die", "schwe\u00b7re", "Luft", "dem", "lech\u00b7zen\u00b7den", "Ge\u00b7f\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da str\u00f6mtest Du, aus h\u00f6hern Regionen", "tokens": ["Da", "str\u00f6m\u00b7test", "Du", ",", "aus", "h\u00f6\u00b7hern", "Re\u00b7gi\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur Labung freundlich uns herabgesandt,", "tokens": ["Zur", "La\u00b7bung", "freund\u00b7lich", "uns", "her\u00b7ab\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die k\u00fchlen Perlen, die in Millionen", "tokens": ["Die", "k\u00fch\u00b7len", "Per\u00b7len", ",", "die", "in", "Mil\u00b7lion\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Voll heissen Durstes trank das d\u00fcrre Land.", "tokens": ["Voll", "heis\u00b7sen", "Durs\u00b7tes", "trank", "das", "d\u00fcr\u00b7re", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie gute Geister wehen durch die Fluren", "tokens": ["Wie", "gu\u00b7te", "Geis\u00b7ter", "we\u00b7hen", "durch", "die", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der neuen Lust und der Erquickung Spuren.", "tokens": ["Der", "neu\u00b7en", "Lust", "und", "der", "Er\u00b7quic\u00b7kung", "Spu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So mildert gern den heissen Brand der Schmerzen,", "tokens": ["So", "mil\u00b7dert", "gern", "den", "heis\u00b7sen", "Brand", "der", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der uns im Lauf des Lebens oft versengt,", "tokens": ["Der", "uns", "im", "Lauf", "des", "Le\u00b7bens", "oft", "ver\u00b7sengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Thr\u00e4nen Thau, der sanft aus unsern Herzen", "tokens": ["Der", "Thr\u00e4\u00b7nen", "Thau", ",", "der", "sanft", "aus", "un\u00b7sern", "Her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das bittre Gift verschlossnen Grames dr\u00e4ngt,", "tokens": ["Das", "bitt\u00b7re", "Gift", "ver\u00b7schloss\u00b7nen", "Gra\u00b7mes", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Lindrung bringen uns der Wehmuth Gaben,", "tokens": ["Und", "Lind\u00b7rung", "brin\u00b7gen", "uns", "der", "Weh\u00b7muth", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Indem sie still den bangen Busen laben.", "tokens": ["In\u00b7dem", "sie", "still", "den", "ban\u00b7gen", "Bu\u00b7sen", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O netzt auch mir das Auge, das so dunkel", "tokens": ["O", "netzt", "auch", "mir", "das", "Au\u00b7ge", ",", "das", "so", "dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "PPER", "ART", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur \u00f6de W\u00fcsten steinigt vor sich sieht,", "tokens": ["Nur", "\u00f6\u00b7de", "W\u00fcs\u00b7ten", "stei\u00b7nigt", "vor", "sich", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und dem der Hoffnung goldnes Sterngefunkel", "tokens": ["Und", "dem", "der", "Hoff\u00b7nung", "gold\u00b7nes", "Stern\u00b7ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In unerreichbar weite Ferne flieht.", "tokens": ["In", "un\u00b7er\u00b7reich\u00b7bar", "wei\u00b7te", "Fer\u00b7ne", "flieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach, wie der matten Flur ein frischer Regen,", "tokens": ["Ach", ",", "wie", "der", "mat\u00b7ten", "Flur", "ein", "fri\u00b7scher", "Re\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sind Thr\u00e4nen meinem kranken Herzen Seegen.", "tokens": ["Sind", "Thr\u00e4\u00b7nen", "mei\u00b7nem", "kran\u00b7ken", "Her\u00b7zen", "See\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}