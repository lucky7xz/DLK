{"textgrid.poem.55625": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Dornburg", "genre": "verse", "period": "N.A.", "pub_year": 1828, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fch, wenn Tal, Gebirg und Garten", "tokens": ["Fr\u00fch", ",", "wenn", "Tal", ",", "Ge\u00b7birg", "und", "Gar\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nebelschleiern sich enth\u00fcllen,", "tokens": ["Ne\u00b7bel\u00b7schlei\u00b7ern", "sich", "ent\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dem sehnlichsten Erwarten", "tokens": ["Und", "dem", "sehn\u00b7lichs\u00b7ten", "Er\u00b7war\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--++--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Blumenkelche bunt sich f\u00fcllen;", "tokens": ["Blu\u00b7men\u00b7kel\u00b7che", "bunt", "sich", "f\u00fcl\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn der \u00c4ther, Wolken tragend,", "tokens": ["Wenn", "der", "\u00c4\u00b7ther", ",", "Wol\u00b7ken", "tra\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem klaren Tage streitet,", "tokens": ["Mit", "dem", "kla\u00b7ren", "Ta\u00b7ge", "strei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Ostwind, sie verjagend,", "tokens": ["Und", "ein", "Ost\u00b7wind", ",", "sie", "ver\u00b7ja\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blaue Sonnenbahn bereitet;", "tokens": ["Blau\u00b7e", "Son\u00b7nen\u00b7bahn", "be\u00b7rei\u00b7tet", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dankst du dann, am Blick dich weidend,", "tokens": ["Dankst", "du", "dann", ",", "am", "Blick", "dich", "wei\u00b7dend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reiner Brust der Gro\u00dfen, Holden,", "tokens": ["Rei\u00b7ner", "Brust", "der", "Gro\u00b7\u00dfen", ",", "Hol\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird die Sonne, r\u00f6tlich scheidend,", "tokens": ["Wird", "die", "Son\u00b7ne", ",", "r\u00f6t\u00b7lich", "schei\u00b7dend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rings den Horizont vergolden.", "tokens": ["Rings", "den", "Ho\u00b7ri\u00b7zont", "ver\u00b7gol\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}