{"dta.poem.13374": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Nachrichten  \n von gelehrten Schriften.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198519", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die\u00df hat mich, grosser Graf, bisher noch abgeschreckt:", "tokens": ["Die\u00df", "hat", "mich", ",", "gros\u00b7ser", "Graf", ",", "bis\u00b7her", "noch", "ab\u00b7ge\u00b7schreckt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Staatsmann hatte mir den Wahrheits-Freund verdekt.", "tokens": ["Der", "Staats\u00b7mann", "hat\u00b7te", "mir", "den", "Wahr\u00b7heits\u00b7Freund", "ver\u00b7dekt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch hab ich dich geliebt. \u2012 \u2012 \u2012 \u2012", "tokens": ["Doch", "hab", "ich", "dich", "ge\u00b7liebt", ".", "\u2012", "\u2012", "\u2012", "\u2012"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PRF", "VVPP", "$.", "$(", "$(", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}