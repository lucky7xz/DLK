{"dta.poem.7307": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Brief aus der Heimath.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Sie sa\u00df am Fensterrand im Morgenlicht,", "tokens": ["Sie", "sa\u00df", "am", "Fens\u00b7ter\u00b7rand", "im", "Mor\u00b7gen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und starrte in das aufgeschlagne Buch,", "tokens": ["Und", "starr\u00b7te", "in", "das", "auf\u00b7ge\u00b7schlag\u00b7ne", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Zeilen z\u00e4hlte sie und wu\u00dft es nicht,", "tokens": ["Die", "Zei\u00b7len", "z\u00e4hl\u00b7te", "sie", "und", "wu\u00dft", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ach weithin, weithin der Gedanken Flug!", "tokens": ["Ach", "weit\u00b7hin", ",", "weit\u00b7hin", "der", "Ge\u00b7dan\u00b7ken", "Flug", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was sind so \u00e4ngstlich ihre n\u00e4cht'gen Tr\u00e4ume?", "tokens": ["Was", "sind", "so", "\u00e4ngst\u00b7lich", "ih\u00b7re", "n\u00e4cht'\u00b7gen", "Tr\u00e4u\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was scheint die Sonne durch so \u00f6de R\u00e4ume?", "tokens": ["Was", "scheint", "die", "Son\u00b7ne", "durch", "so", "\u00f6\u00b7de", "R\u00e4u\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "\u2014 Auch heute kam kein Brief, auch heute nicht.", "tokens": ["Auch", "heu\u00b7te", "kam", "kein", "Brief", ",", "auch", "heu\u00b7te", "nicht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "VVFIN", "PIAT", "NN", "$,", "ADV", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Seit Wochen weckte sie der Lampe Schein,", "tokens": ["Seit", "Wo\u00b7chen", "weck\u00b7te", "sie", "der", "Lam\u00b7pe", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hat bebend an der Stiege sie gelauscht;", "tokens": ["Hat", "be\u00b7bend", "an", "der", "Stie\u00b7ge", "sie", "ge\u00b7lauscht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn pl\u00f6tzlich am Gem\u00e4uer knackt der Schrein,", "tokens": ["Wenn", "pl\u00f6tz\u00b7lich", "am", "Ge\u00b7m\u00e4u\u00b7er", "knackt", "der", "Schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Fensterladen auf im Winde rauscht, \u2014", "tokens": ["Ein", "Fens\u00b7ter\u00b7la\u00b7den", "auf", "im", "Win\u00b7de", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "APPRART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es k\u00f6mmt, es naht, die Sorgen sind geendet:", "tokens": ["Es", "k\u00f6mmt", ",", "es", "naht", ",", "die", "Sor\u00b7gen", "sind", "ge\u00b7en\u00b7det", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie hat gefragt, sie hat sich abgewendet,", "tokens": ["Sie", "hat", "ge\u00b7fragt", ",", "sie", "hat", "sich", "ab\u00b7ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und schlo\u00df sich dann in ihre Kammer ein.", "tokens": ["Und", "schlo\u00df", "sich", "dann", "in", "ih\u00b7re", "Kam\u00b7mer", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kein Lebenszeichen von der liebsten Hand,", "tokens": ["Kein", "Le\u00b7bens\u00b7zei\u00b7chen", "von", "der", "liebs\u00b7ten", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von jener, die sie sorglich hat gelenkt,", "tokens": ["Von", "je\u00b7ner", ",", "die", "sie", "sorg\u00b7lich", "hat", "ge\u00b7lenkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als sie zum ersten Mal zu festem Stand", "tokens": ["Als", "sie", "zum", "ers\u00b7ten", "Mal", "zu", "fes\u00b7tem", "Stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die zarten Kinderf\u00fc\u00dfchen hat gesenkt;", "tokens": ["Die", "zar\u00b7ten", "Kin\u00b7der\u00b7f\u00fc\u00df\u00b7chen", "hat", "ge\u00b7senkt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Versprengter Tropfen von der Quelle Rande,", "tokens": ["Ver\u00b7spreng\u00b7ter", "Trop\u00b7fen", "von", "der", "Quel\u00b7le", "Ran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Harrt sie vergebens in dem fremden Lande;", "tokens": ["Harrt", "sie", "ver\u00b7ge\u00b7bens", "in", "dem", "frem\u00b7den", "Lan\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Tage schleichen hin, die Woche schwand.", "tokens": ["Die", "Ta\u00b7ge", "schlei\u00b7chen", "hin", ",", "die", "Wo\u00b7che", "schwand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was ihre rege Phantasie geweckt?", "tokens": ["Was", "ih\u00b7re", "re\u00b7ge", "Phan\u00b7ta\u00b7sie", "ge\u00b7weckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ach, Eine Leiche sah die Heimath schon,", "tokens": ["Ach", ",", "Ei\u00b7ne", "Lei\u00b7che", "sah", "die", "Hei\u00b7math", "schon", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Seit sie den unbedachten Fu\u00df gestreckt", "tokens": ["Seit", "sie", "den", "un\u00b7be\u00b7dach\u00b7ten", "Fu\u00df", "ge\u00b7streckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf fremden Grund und h\u00f6rte fremden Ton;", "tokens": ["Auf", "frem\u00b7den", "Grund", "und", "h\u00f6r\u00b7te", "frem\u00b7den", "Ton", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie k\u00fc\u00dfte scheidend jung' und frische Wangen,", "tokens": ["Sie", "k\u00fc\u00df\u00b7te", "schei\u00b7dend", "jung'", "und", "fri\u00b7sche", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die jetzt von tiefer Grabesnacht umfangen;", "tokens": ["Die", "jetzt", "von", "tie\u00b7fer", "Gra\u00b7bes\u00b7nacht", "um\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ist's Wunder, da\u00df sie t\u00f6dtlich aufgeschreckt?", "tokens": ["Ist's", "Wun\u00b7der", ",", "da\u00df", "sie", "t\u00f6dt\u00b7lich", "auf\u00b7ge\u00b7schreckt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "In Tr\u00e4umen steigt das Krankenbett empor,", "tokens": ["In", "Tr\u00e4u\u00b7men", "steigt", "das", "Kran\u00b7ken\u00b7bett", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Z\u00fcge d\u00e4mmern, wie in halber Nacht;", "tokens": ["Und", "Z\u00fc\u00b7ge", "d\u00e4m\u00b7mern", ",", "wie", "in", "hal\u00b7ber", "Nacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,", "PWAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer ist's? \u2014 sie wei\u00df es nicht und spannt das Ohr,", "tokens": ["Wer", "ist's", "?", "sie", "wei\u00df", "es", "nicht", "und", "spannt", "das", "Ohr", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$.", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sie horcht mit ihrer ganzen Seele Macht;", "tokens": ["Sie", "horcht", "mit", "ih\u00b7rer", "gan\u00b7zen", "See\u00b7le", "Macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dann f\u00e4hrt sie pl\u00f6tzlich auf beim Windesrauschen,", "tokens": ["Dann", "f\u00e4hrt", "sie", "pl\u00f6tz\u00b7lich", "auf", "beim", "Win\u00b7des\u00b7rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und glaubt dem matten St\u00f6hnen noch zu lauschen,", "tokens": ["Und", "glaubt", "dem", "mat\u00b7ten", "St\u00f6h\u00b7nen", "noch", "zu", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und kann erst sp\u00e4t begreifen da\u00df sie wacht.", "tokens": ["Und", "kann", "erst", "sp\u00e4t", "be\u00b7grei\u00b7fen", "da\u00df", "sie", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADJD", "VVINF", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch sieh, dort fliegt sie \u00fcber'n glatten Flur,", "tokens": ["Doch", "sieh", ",", "dort", "fliegt", "sie", "\u00fc\u00b7ber'n", "glat\u00b7ten", "Flur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr aufgel\u00f6stes Haar umflie\u00dft sie rund,", "tokens": ["Ihr", "auf\u00b7ge\u00b7l\u00f6s\u00b7tes", "Haar", "um\u00b7flie\u00dft", "sie", "rund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und zitternd ruft sie, mit des Weinens Spur:", "tokens": ["Und", "zit\u00b7ternd", "ruft", "sie", ",", "mit", "des", "Wei\u00b7nens", "Spur", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u201eein Brief, ein Brief, die Mutter ist gesund!\u201c", "tokens": ["\u201e", "ein", "Brief", ",", "ein", "Brief", ",", "die", "Mut\u00b7ter", "ist", "ge\u00b7sund", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und ihre Thr\u00e4nen st\u00fcrzen wie zwei Quellen,", "tokens": ["Und", "ih\u00b7re", "Thr\u00e4\u00b7nen", "st\u00fcr\u00b7zen", "wie", "zwei", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "KOKOM", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die \u00fcbervoll aus ihren Ufern schwellen;", "tokens": ["Die", "\u00fc\u00b7ber\u00b7voll", "aus", "ih\u00b7ren", "U\u00b7fern", "schwel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ach, eine Mutter hat man einmal nur!", "tokens": ["Ach", ",", "ei\u00b7ne", "Mut\u00b7ter", "hat", "man", "ein\u00b7mal", "nur", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VAFIN", "PIS", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}