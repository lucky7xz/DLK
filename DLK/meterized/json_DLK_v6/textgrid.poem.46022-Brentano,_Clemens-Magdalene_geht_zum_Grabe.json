{"textgrid.poem.46022": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Magdalene geht zum Grabe", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die bange Nacht ist hingegangen,", "tokens": ["Die", "ban\u00b7ge", "Nacht", "ist", "hin\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch kennt noch nicht ein Mensch den andern,", "tokens": ["Doch", "kennt", "noch", "nicht", "ein", "Mensch", "den", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Noch kann ich mit den Tr\u00e4nen prangen,", "tokens": ["Noch", "kann", "ich", "mit", "den", "Tr\u00e4\u00b7nen", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und zu des Meisters Grab still wandern;", "tokens": ["Und", "zu", "des", "Meis\u00b7ters", "Grab", "still", "wan\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Tempelw\u00e4chter ruft die Stunden,", "tokens": ["Der", "Tem\u00b7pel\u00b7w\u00e4ch\u00b7ter", "ruft", "die", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Maria ringt in stummen Schmerzen,", "tokens": ["Ma\u00b7ria", "ringt", "in", "stum\u00b7men", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sie z\u00e4hlte ihres Kindes Wunden,", "tokens": ["Sie", "z\u00e4hl\u00b7te", "ih\u00b7res", "Kin\u00b7des", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Schwert drang schneidend ihr zum Herzen.", "tokens": ["Ein", "Schwert", "drang", "schnei\u00b7dend", "ihr", "zum", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df los von der Welt,", "tokens": ["La\u00df", "los", "von", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.2": {"text": "Von dem bunten Zelt,", "tokens": ["Von", "dem", "bun\u00b7ten", "Zelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Von der Truggestalt,", "tokens": ["Von", "der", "Trug\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Jesus kommt bald!", "tokens": ["Je\u00b7sus", "kommt", "bald", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Freundlich wird er gr\u00fc\u00dfen,", "tokens": ["Freund\u00b7lich", "wird", "er", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Sink zu seinen F\u00fc\u00dfen,", "tokens": ["Sink", "zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Suche, fange, halt ihn dir,", "tokens": ["Su\u00b7che", ",", "fan\u00b7ge", ",", "halt", "ihn", "dir", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Seufze, sage, klage hier;", "tokens": ["Seuf\u00b7ze", ",", "sa\u00b7ge", ",", "kla\u00b7ge", "hier", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Bleib, o Jesu, bleib bei mir!", "tokens": ["Bleib", ",", "o", "Je\u00b7su", ",", "bleib", "bei", "mir", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NE", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Spezerei, die wir bereitet", "tokens": ["Die", "Spe\u00b7ze\u00b7rei", ",", "die", "wir", "be\u00b7rei\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Myrrhen, Aloe und Narden,", "tokens": ["Von", "Myr\u00b7rhen", ",", "A\u00b7loe", "und", "Nar\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Tr\u00e4gt Salome, die mich begleitet,", "tokens": ["Tr\u00e4gt", "Sa\u00b7lo\u00b7me", ",", "die", "mich", "be\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Jacobe zum Grabesgarten.", "tokens": ["Und", "Ja\u00b7co\u00b7be", "zum", "Gra\u00b7bes\u00b7gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPRART", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Schon f\u00e4ngt's im Osten an zu tagen,", "tokens": ["Schon", "f\u00e4ngt's", "im", "Os\u00b7ten", "an", "zu", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich streck' die H\u00e4nde aus zum Felsen,", "tokens": ["Ich", "streck'", "die", "H\u00e4n\u00b7de", "aus", "zum", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Johanna fragt mit bangem Zagen:", "tokens": ["Jo\u00b7han\u00b7na", "fragt", "mit", "ban\u00b7gem", "Za\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wer wird vom Grab den Stein uns w\u00e4lzen?", "tokens": ["Wer", "wird", "vom", "Grab", "den", "Stein", "uns", "w\u00e4l\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPRART", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So wandle ich im Morgengrauen", "tokens": ["So", "wand\u00b7le", "ich", "im", "Mor\u00b7gen\u00b7grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Grab, wo meine Liebe ruht;", "tokens": ["Zum", "Grab", ",", "wo", "mei\u00b7ne", "Lie\u00b7be", "ruht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will mit Tr\u00e4nen ihn betauen,", "tokens": ["Ich", "will", "mit", "Tr\u00e4\u00b7nen", "ihn", "be\u00b7tau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er gab mir all sein Herzensblut.", "tokens": ["Er", "gab", "mir", "all", "sein", "Her\u00b7zens\u00b7blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Um mir den Stein vom Herz zu heben,", "tokens": ["Um", "mir", "den", "Stein", "vom", "Herz", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ging in den Tod all meine Wonne;", "tokens": ["Ging", "in", "den", "Tod", "all", "mei\u00b7ne", "Won\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da zuckt ein Blitz, die Felsen beben,", "tokens": ["Da", "zuckt", "ein", "Blitz", ",", "die", "Fel\u00b7sen", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und aus dem Grab schwingt sich die Sonne.", "tokens": ["Und", "aus", "dem", "Grab", "schwingt", "sich", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Wachen rings erstarret liegen,", "tokens": ["Die", "Wa\u00b7chen", "rings", "er\u00b7star\u00b7ret", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und von dem abgew\u00e4lzten Steine", "tokens": ["Und", "von", "dem", "ab\u00b7ge\u00b7w\u00e4lz\u00b7ten", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gr\u00fc\u00dft uns, die sich zum Grabe biegen,", "tokens": ["Gr\u00fc\u00dft", "uns", ",", "die", "sich", "zum", "Gra\u00b7be", "bie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Engel, wei\u00df im Himmelsscheine:", "tokens": ["Ein", "En\u00b7gel", ",", "wei\u00df", "im", "Him\u00b7mels\u00b7schei\u00b7ne", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erschrecket nicht! Er ist erstanden,", "tokens": ["Er\u00b7schre\u00b7cket", "nicht", "!", "Er", "ist", "er\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er ist nicht hier. Kommt her und schauet", "tokens": ["Er", "ist", "nicht", "hier", ".", "Kommt", "her", "und", "schau\u00b7et"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "$.", "VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "An leerem Ort die Todesbanden,", "tokens": ["An", "lee\u00b7rem", "Ort", "die", "To\u00b7des\u00b7ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es ist sein Tempel neu erbauet.", "tokens": ["Es", "ist", "sein", "Tem\u00b7pel", "neu", "er\u00b7bau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Weil ich den Leib entf\u00fchret glaubte,", "tokens": ["Weil", "ich", "den", "Leib", "ent\u00b7f\u00fch\u00b7ret", "glaub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Eilt' nochmals ich das Grab zu gr\u00fc\u00dfen;", "tokens": ["Eilt'", "noch\u00b7mals", "ich", "das", "Grab", "zu", "gr\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Engel sa\u00df dem Grab zu Haupte,", "tokens": ["Ein", "En\u00b7gel", "sa\u00df", "dem", "Grab", "zu", "Haup\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Engel sa\u00df dem Grab zu F\u00fc\u00dfen.", "tokens": ["Ein", "En\u00b7gel", "sa\u00df", "dem", "Grab", "zu", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was weinst du Weib? zu mir sie sagen.", "tokens": ["Was", "weinst", "du", "Weib", "?", "zu", "mir", "sie", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "$.", "APPR", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich sprach: Weil sie ihn weggenommen,", "tokens": ["Ich", "sprach", ":", "Weil", "sie", "ihn", "weg\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und ich nicht wei\u00df, wohin getragen.", "tokens": ["Und", "ich", "nicht", "wei\u00df", ",", "wo\u00b7hin", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "VVFIN", "$,", "PWAV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O sagt: wo ist er hingekommen?", "tokens": ["O", "sagt", ":", "wo", "ist", "er", "hin\u00b7ge\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dann blickt' ich um, und sah im Schimmer", "tokens": ["Dann", "blickt'", "ich", "um", ",", "und", "sah", "im", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Den G\u00e4rtner, glaubt' ich, h\u00f6rt' ihn fragen:", "tokens": ["Den", "G\u00e4rt\u00b7ner", ",", "glaubt'", "ich", ",", "h\u00f6rt'", "ihn", "fra\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Was weinst du, Weib, wen suchst du immer?", "tokens": ["Was", "weinst", "du", ",", "Weib", ",", "wen", "suchst", "du", "im\u00b7mer", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ich sprach: Hast du ihn weggetragen?", "tokens": ["Ich", "sprach", ":", "Hast", "du", "ihn", "weg\u00b7ge\u00b7tra\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VAFIN", "PPER", "PPER", "VVINF", "$."], "m