{"dta.poem.19948": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsmorgen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann die L\u00e4mmer wieder springen,", "tokens": ["Wann", "die", "L\u00e4m\u00b7mer", "wie\u00b7der", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lerchen jubeln, Rosen gl\u00fchn,", "tokens": ["Ler\u00b7chen", "ju\u00b7beln", ",", "Ro\u00b7sen", "gl\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df das kr\u00e4nkste Herze singen", "tokens": ["Mu\u00df", "das", "kr\u00e4nks\u00b7te", "Her\u00b7ze", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und im Welken noch erbl\u00fchn.", "tokens": ["Und", "im", "Wel\u00b7ken", "noch", "er\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer in bangen Lebensschmerzen", "tokens": ["Wer", "in", "ban\u00b7gen", "Le\u00b7bens\u00b7schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam jezt die Stra\u00dfe geht,", "tokens": ["Ein\u00b7sam", "jezt", "die", "Stra\u00b7\u00dfe", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Singet selbst aus d\u00fcstrem Herzen", "tokens": ["Sin\u00b7get", "selbst", "aus", "d\u00fcst\u00b7rem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Lied aus Wolken weht.", "tokens": ["Wie", "ein", "Lied", "aus", "Wol\u00b7ken", "weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer verbannt, das Aug' in Thr\u00e4nen,", "tokens": ["Wer", "ver\u00b7bannt", ",", "das", "Aug'", "in", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jezt im fremden Lande zieht,", "tokens": ["Jezt", "im", "frem\u00b7den", "Lan\u00b7de", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch bethaute Blumen t\u00f6nen,", "tokens": ["Durch", "be\u00b7thau\u00b7te", "Blu\u00b7men", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4\u00dft der seiner Heimath Lied.", "tokens": ["L\u00e4\u00dft", "der", "sei\u00b7ner", "Hei\u00b7math", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Fl\u00fcsse, Saaten, t\u00f6nend wallen; \u2014", "tokens": ["Fl\u00fcs\u00b7se", ",", "Saa\u00b7ten", ",", "t\u00f6\u00b7nend", "wal\u00b7len", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem fernsten Himmel blau", "tokens": ["Aus", "dem", "ferns\u00b7ten", "Him\u00b7mel", "blau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weht ein Singen, lieblich Schallen,", "tokens": ["Weht", "ein", "Sin\u00b7gen", ",", "lieb\u00b7lich", "Schal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber Wald und helle Au.", "tokens": ["Ue\u00b7ber", "Wald", "und", "hel\u00b7le", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Alter Gram! jezt zeuch von hinnen,", "tokens": ["Al\u00b7ter", "Gram", "!", "jezt", "zeuch", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "ADV", "VVFIN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fclle nicht die\u00df Herze bang,", "tokens": ["F\u00fcl\u00b7le", "nicht", "die\u00df", "Her\u00b7ze", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PDS", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Str\u00f6me ein von Himmelszinnen", "tokens": ["Str\u00f6\u00b7me", "ein", "von", "Him\u00b7mels\u00b7zin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgenroth und Lustgesang!", "tokens": ["Mor\u00b7gen\u00b7roth", "und", "Lust\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}