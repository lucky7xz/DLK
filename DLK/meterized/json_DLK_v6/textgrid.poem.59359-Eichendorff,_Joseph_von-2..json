{"textgrid.poem.59359": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich nun zum ersten Male", "tokens": ["Als", "ich", "nun", "zum", "ers\u00b7ten", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder durch den Garten ging,", "tokens": ["Wie\u00b7der", "durch", "den", "Gar\u00b7ten", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Busch und B\u00e4chlein in dem Tale", "tokens": ["Busch", "und", "B\u00e4ch\u00b7lein", "in", "dem", "Ta\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lustig an zu plaudern fing.", "tokens": ["Lus\u00b7tig", "an", "zu", "plau\u00b7dern", "fing", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Blumen halbverstohlen blickten", "tokens": ["Blu\u00b7men", "halb\u00b7ver\u00b7stoh\u00b7len", "blick\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVINF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Neckend aus dem Gras heraus,", "tokens": ["Ne\u00b7ckend", "aus", "dem", "Gras", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bunte Schmetterlinge schickten", "tokens": ["Bun\u00b7te", "Schmet\u00b7ter\u00b7lin\u00b7ge", "schick\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie sogleich auf Kundschaft aus.", "tokens": ["Sie", "sog\u00b7leich", "auf", "Kund\u00b7schaft", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Auch der Kuckuck in den Zweigen", "tokens": ["Auch", "der", "Ku\u00b7ckuck", "in", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fand sich bald zum Spielen ein,", "tokens": ["Fand", "sich", "bald", "zum", "Spie\u00b7len", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Endlich brach der Baum das Schweigen:", "tokens": ["End\u00b7lich", "brach", "der", "Baum", "das", "Schwei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbwarum kommst du heut allein?\u00ab", "tokens": ["\u00bb", "wa\u00b7rum", "kommst", "du", "heut", "al\u00b7lein", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "ADV", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da ich aber schwieg, da r\u00fchrt' er", "tokens": ["Da", "ich", "a\u00b7ber", "schwieg", ",", "da", "r\u00fchrt'", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wunderbar sein dunkles Haupt,", "tokens": ["Wun\u00b7der\u00b7bar", "sein", "dunk\u00b7les", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Fl\u00fcstern konnt ich sp\u00fcren", "tokens": ["Und", "ein", "Fl\u00fcs\u00b7tern", "konnt", "ich", "sp\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwischen V\u00f6glein, Bl\u00fct und Laub.", "tokens": ["Zwi\u00b7schen", "V\u00f6\u00b7glein", ",", "Bl\u00fct", "und", "Laub", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Tr\u00e4nen in dem Grase hingen,", "tokens": ["Tr\u00e4\u00b7nen", "in", "dem", "Gra\u00b7se", "hin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die abendstille Rund", "tokens": ["Durch", "die", "a\u00b7bend\u00b7stil\u00b7le", "Rund"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Klagend nun die Quellen gingen,", "tokens": ["Kla\u00b7gend", "nun", "die", "Quel\u00b7len", "gin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich weint aus Herzensgrund.", "tokens": ["Und", "ich", "weint", "aus", "Her\u00b7zens\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}