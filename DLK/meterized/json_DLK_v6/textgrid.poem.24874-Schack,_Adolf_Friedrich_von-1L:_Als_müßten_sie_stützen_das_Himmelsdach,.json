{"textgrid.poem.24874": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als m\u00fc\u00dften sie st\u00fctzen das Himmelsdach,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als m\u00fc\u00dften sie st\u00fctzen das Himmelsdach,", "tokens": ["Als", "m\u00fc\u00df\u00b7ten", "sie", "st\u00fct\u00b7zen", "das", "Him\u00b7mels\u00b7dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Auf steigen die Wogens\u00e4ulen;", "tokens": ["Auf", "stei\u00b7gen", "die", "Wo\u00b7gen\u00b7s\u00e4u\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Empor zu den Wolken, und wieder dann j\u00e4h", "tokens": ["Em\u00b7por", "zu", "den", "Wol\u00b7ken", ",", "und", "wie\u00b7der", "dann", "j\u00e4h"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "$,", "KON", "ADV", "ADV", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Hinab in die schwindligen Tiefen der See,", "tokens": ["Hin\u00b7ab", "in", "die", "schwind\u00b7li\u00b7gen", "Tie\u00b7fen", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Rei\u00dft uns die Flut bei des Donners Gekrach", "tokens": ["Rei\u00dft", "uns", "die", "Flut", "bei", "des", "Don\u00b7ners", "Ge\u00b7krach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Und der St\u00fcrme Tosen und Heulen.", "tokens": ["Und", "der", "St\u00fcr\u00b7me", "To\u00b7sen", "und", "Heu\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch ferne zur\u00fcck an die K\u00fcste fliegt", "tokens": ["Doch", "fer\u00b7ne", "zu\u00b7r\u00fcck", "an", "die", "K\u00fcs\u00b7te", "fliegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKVZ", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Herz in d\u00e4mmernde Weiten.", "tokens": ["Mein", "Herz", "in", "d\u00e4m\u00b7mern\u00b7de", "Wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hell schimmert das Dach aus Pinien hervor,", "tokens": ["Hell", "schim\u00b7mert", "das", "Dach", "aus", "Pi\u00b7ni\u00b7en", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das H\u00fcndchen bellt, wie ich schreite durchs Thor,", "tokens": ["Das", "H\u00fcnd\u00b7chen", "bellt", ",", "wie", "ich", "schrei\u00b7te", "durchs", "Thor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und wei\u00dfe Arme, die oft mich gewiegt,", "tokens": ["Und", "wei\u00b7\u00dfe", "Ar\u00b7me", ",", "die", "oft", "mich", "ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Entgegen mir seh' ich sich breiten.", "tokens": ["Ent\u00b7ge\u00b7gen", "mir", "seh'", "ich", "sich", "brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "O N\u00e4chte, wie sie f\u00fcr G\u00f6tter sind!", "tokens": ["O", "N\u00e4ch\u00b7te", ",", "wie", "sie", "f\u00fcr", "G\u00f6t\u00b7ter", "sind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ergl\u00fchen und wieder erblassen,", "tokens": ["Er\u00b7gl\u00fc\u00b7hen", "und", "wie\u00b7der", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Vor Wonne verstummen, Lippe fest", "tokens": ["Vor", "Won\u00b7ne", "ver\u00b7stum\u00b7men", ",", "Lip\u00b7pe", "fest"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "NN", "PTKVZ"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An Lippe und Herz an Herz gepre\u00dft \u2013", "tokens": ["An", "Lip\u00b7pe", "und", "Herz", "an", "Herz", "ge\u00b7pre\u00dft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Was brausest du, Flut? Was tobst du, Wind?", "tokens": ["Was", "brau\u00b7sest", "du", ",", "Flut", "?", "Was", "tobst", "du", ",", "Wind", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$.", "PWS", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mein Gl\u00fcck doch m\u00fc\u00dft ihr mir lassen!", "tokens": ["Mein", "Gl\u00fcck", "doch", "m\u00fc\u00dft", "ihr", "mir", "las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und mu\u00df es sein, und rei\u00dft das Geschick", "tokens": ["Und", "mu\u00df", "es", "sein", ",", "und", "rei\u00dft", "das", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VAINF", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mich hinab zu dem g\u00e4hnenden Schlunde,", "tokens": ["Mich", "hin\u00b7ab", "zu", "dem", "g\u00e4h\u00b7nen\u00b7den", "Schlun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Noch im Versinken, wenn \u00fcber mich her", "tokens": ["Noch", "im", "Ver\u00b7sin\u00b7ken", ",", "wenn", "\u00fc\u00b7ber", "mich", "her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "KOUS", "APPR", "PRF", "APZR"], "meter": "++-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Die Wogen w\u00e4lzt das sch\u00e4umende Meer,", "tokens": ["Die", "Wo\u00b7gen", "w\u00e4lzt", "das", "sch\u00e4u\u00b7men\u00b7de", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Gedenk' ich an zweier Augen Blick,", "tokens": ["Ge\u00b7denk'", "ich", "an", "zwei\u00b7er", "Au\u00b7gen", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die K\u00fcsse von einem Munde.", "tokens": ["Die", "K\u00fcs\u00b7se", "von", "ei\u00b7nem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}