{"textgrid.poem.36990": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "Die Trennung", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als die Jungfrau Lilialinda", "tokens": ["Als", "die", "Jung\u00b7frau", "Li\u00b7li\u00b7a\u00b7lin\u00b7da"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NE"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Meinen Wonnerausch bemerkte,", "tokens": ["Mei\u00b7nen", "Won\u00b7ne\u00b7rausch", "be\u00b7merk\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem ich, schwelgend im Genusse", "tokens": ["Dem", "ich", ",", "schwel\u00b7gend", "im", "Ge\u00b7nus\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "$,", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihrer Reize, war verfallen:", "tokens": ["Ih\u00b7rer", "Rei\u00b7ze", ",", "war", "ver\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Strahlte auch aus ihrem holden", "tokens": ["Strahl\u00b7te", "auch", "aus", "ih\u00b7rem", "hol\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Antlitz seliges Entz\u00fccken,", "tokens": ["Ant\u00b7litz", "se\u00b7li\u00b7ges", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und sie wollte eben, glaub' ich,", "tokens": ["Und", "sie", "woll\u00b7te", "e\u00b7ben", ",", "glaub'", "ich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mich an ihres Leibes Dolden,", "tokens": ["Mich", "an", "ih\u00b7res", "Lei\u00b7bes", "Dol\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "An den keuschen Busen dr\u00fccken.", "tokens": ["An", "den", "keu\u00b7schen", "Bu\u00b7sen", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch verwandelt pl\u00f6tzlich schien sie,", "tokens": ["Doch", "ver\u00b7wan\u00b7delt", "pl\u00f6tz\u00b7lich", "schien", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ich nun \u2013 ein Kind der Erde,", "tokens": ["Als", "ich", "nun", "\u2013", "ein", "Kind", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$(", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welches weibliche Avancen", "tokens": ["Wel\u00b7ches", "weib\u00b7li\u00b7che", "A\u00b7van\u00b7cen"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solcher Art nicht dulden darf \u2013", "tokens": ["Sol\u00b7cher", "Art", "nicht", "dul\u00b7den", "darf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "An der Schwelle dieser Kirche", "tokens": ["An", "der", "Schwel\u00b7le", "die\u00b7ser", "Kir\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Salomonis hin mich warf,", "tokens": ["Sa\u00b7lo\u00b7mo\u00b7nis", "hin", "mich", "warf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Flehend: \u00bbNoch in dieser Stunde", "tokens": ["Fle\u00b7hend", ":", "\u00bb", "Noch", "in", "die\u00b7ser", "Stun\u00b7de"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "$(", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mu\u00dft', o Engel ohne Gleichen,", "tokens": ["Mu\u00dft'", ",", "o", "En\u00b7gel", "oh\u00b7ne", "Glei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "FM", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Du zum heil'gen Ehebunde", "tokens": ["Du", "zum", "heil'\u00b7gen", "E\u00b7heb\u00b7un\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Am Altar die Hand mir reichen!\u00ab", "tokens": ["Am", "Al\u00b7tar", "die", "Hand", "mir", "rei\u00b7chen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "VVINF", "$.", "$("], "meter": "----+-+-", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Leichenbla\u00df und tief ersch\u00fcttert", "tokens": ["Lei\u00b7chen\u00b7bla\u00df", "und", "tief", "er\u00b7sch\u00fct\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Erst, dann stolz empor sich richtend", "tokens": ["Erst", ",", "dann", "stolz", "em\u00b7por", "sich", "rich\u00b7tend"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "ADJD", "PTKVZ", "PRF", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ver\u00e4chtlich auf die Gr\u00e4fin,", "tokens": ["Und", "ver\u00b7\u00e4cht\u00b7lich", "auf", "die", "Gr\u00e4\u00b7fin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welche h\u00f6hnisch lachte, blickend,", "tokens": ["Wel\u00b7che", "h\u00f6h\u00b7nisch", "lach\u00b7te", ",", "bli\u00b7ckend", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Winkte sie mir, ihr zu folgen,", "tokens": ["Wink\u00b7te", "sie", "mir", ",", "ihr", "zu", "fol\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fchrte mich hinab zur Gasse,", "tokens": ["F\u00fchr\u00b7te", "mich", "hin\u00b7ab", "zur", "Gas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fchrte mich nach ihrer Wohnung,", "tokens": ["F\u00fchr\u00b7te", "mich", "nach", "ih\u00b7rer", "Woh\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Tugend-Holzweg Nr. 80;", "tokens": ["Tu\u00b7gen\u00b7dHolz\u00b7weg", "Nr.", "80", ";"], "token_info": ["word", "abbreviation", "number", "punct"], "pos": ["NN", "NN", "CARD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "F\u00fchrte mich zu ihrer Mutter,", "tokens": ["F\u00fchr\u00b7te", "mich", "zu", "ih\u00b7rer", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Flog an deren Hals und schluchzte,", "tokens": ["Flog", "an", "de\u00b7ren", "Hals", "und", "schluchz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRELAT", "NN", "KON", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Weinte hei\u00dfe, bittre Thr\u00e4nen.", "tokens": ["Wein\u00b7te", "hei\u00b7\u00dfe", ",", "bitt\u00b7re", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bblilia! Lilchen! Armes Kindchen!\u00ab", "tokens": ["\u00bb", "li\u00b7lia", "!", "Lil\u00b7chen", "!", "Ar\u00b7mes", "Kind\u00b7chen", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "$.", "NN", "$.", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rief die Mutter, gleichfalls weinend,", "tokens": ["Rief", "die", "Mut\u00b7ter", ",", "gleich\u00b7falls", "wei\u00b7nend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbw\u00e4r' es m\u00f6glich? Ruhig, Lindchen!", "tokens": ["\u00bb", "w\u00e4r'", "es", "m\u00f6g\u00b7lich", "?", "Ru\u00b7hig", ",", "Lind\u00b7chen", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ADJD", "$.", "NE", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4tte jener Fremde wirklich", "tokens": ["H\u00e4t\u00b7te", "je\u00b7ner", "Frem\u00b7de", "wirk\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dich in Deiner Jugend Sch\u00f6ne", "tokens": ["Dich", "in", "Dei\u00b7ner", "Ju\u00b7gend", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich zur ", "tokens": ["Sich", "zur"], "token_info": ["word", "word"], "pos": ["PRF", "APPRART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "\u00bbja,\u00ab war der Geliebten Antwort", "tokens": ["\u00bb", "ja", ",", "\u00ab", "war", "der", "Ge\u00b7lieb\u00b7ten", "Ant\u00b7wort"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "$,", "$(", "VAFIN", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit von Schmerz gebrochner Stimme,", "tokens": ["Mit", "von", "Schmerz", "ge\u00b7broch\u00b7ner", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbja, es ist geschehen, was ich,", "tokens": ["\u00bb", "ja", ",", "es", "ist", "ge\u00b7sche\u00b7hen", ",", "was", "ich", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "PPER", "VAFIN", "VVPP", "$,", "PWS", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Liegt mir fern auch eitles Wesen,", "tokens": ["Liegt", "mir", "fern", "auch", "eit\u00b7les", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimmer, nimmer konnte glauben!", "tokens": ["Nim\u00b7mer", ",", "nim\u00b7mer", "konn\u00b7te", "glau\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich, die achtzehnj\u00e4hr'ge Jungfrau,", "tokens": ["Mich", ",", "die", "acht\u00b7zehn\u00b7j\u00e4hr'\u00b7ge", "Jung\u00b7frau", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Voller Lebens f\u00fcr die Liebe,", "tokens": ["Vol\u00b7ler", "Le\u00b7bens", "f\u00fcr", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und ihm dieses Leben weihend,", "tokens": ["Und", "ihm", "die\u00b7ses", "Le\u00b7ben", "wei\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Mich, o es ist mehr als grausam!", "tokens": ["Mich", ",", "o", "es", "ist", "mehr", "als", "grau\u00b7sam", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "PPER", "VAFIN", "PIS", "KOKOM", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "H\u00f6hnt er durch erk\u00fcnstelt Staunen", "tokens": ["H\u00f6hnt", "er", "durch", "er\u00b7k\u00fcns\u00b7telt", "Stau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Ueber meines Leibes Sch\u00f6nheit,", "tokens": ["Ue\u00b7ber", "mei\u00b7nes", "Lei\u00b7bes", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Durch erlog'ne Liebesworte,", "tokens": ["Durch", "er\u00b7lo\u00b7g'\u00b7ne", "Lie\u00b7bes\u00b7wor\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Und st\u00f6\u00dft dann zur\u00fcck mich, w\u00e4hlend,", "tokens": ["Und", "st\u00f6\u00dft", "dann", "zu\u00b7r\u00fcck", "mich", ",", "w\u00e4h\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "PPER", "$,", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "O ihr G\u00f6tter! mich zur Gattin!\u00ab", "tokens": ["O", "ihr", "G\u00f6t\u00b7ter", "!", "mich", "zur", "Gat\u00b7tin", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PPOSAT", "NN", "$.", "PPER", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Alle Fragen des Erstaunens", "tokens": ["Al\u00b7le", "Fra\u00b7gen", "des", "Er\u00b7stau\u00b7nens"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber ihr ver\u00e4ndert Wesen,", "tokens": ["Ue\u00b7ber", "ihr", "ver\u00b7\u00e4n\u00b7dert", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die an Lilialinda fr\u00fcher", "tokens": ["Die", "an", "Li\u00b7li\u00b7a\u00b7lin\u00b7da", "fr\u00fc\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "NE", "ADJD"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ich schon richten wollte, hatte", "tokens": ["Ich", "schon", "rich\u00b7ten", "woll\u00b7te", ",", "hat\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "VVINF", "VMFIN", "$,", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch abwehrende Geb\u00e4rden", "tokens": ["Durch", "ab\u00b7weh\u00b7ren\u00b7de", "Ge\u00b7b\u00e4r\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sie erstickt mir. Eben wollt' ich", "tokens": ["Sie", "er\u00b7stickt", "mir", ".", "E\u00b7ben", "wollt'", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ADV", "VMFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nochmals heilig ihr betheuern,", "tokens": ["Noch\u00b7mals", "hei\u00b7lig", "ihr", "be\u00b7theu\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df es ja nur hei\u00dfe Liebe,", "tokens": ["Da\u00df", "es", "ja", "nur", "hei\u00b7\u00dfe", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Die ich f\u00fchlte, die mich triebe,", "tokens": ["Die", "ich", "f\u00fchl\u00b7te", ",", "die", "mich", "trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sie, die G\u00f6ttliche, zu heuern:", "tokens": ["Sie", ",", "die", "G\u00f6tt\u00b7li\u00b7che", ",", "zu", "heu\u00b7ern", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Da begann vor'm off'nen Fenster", "tokens": ["Da", "be\u00b7gann", "vor'm", "off'\u00b7nen", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine Menschenschaar, in welcher", "tokens": ["Ei\u00b7ne", "Men\u00b7schen\u00b7schaar", ",", "in", "wel\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Gr\u00e4fin ich bemerkte,", "tokens": ["Mei\u00b7ne", "Gr\u00e4\u00b7fin", "ich", "be\u00b7merk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pl\u00f6tzlich, wie wenn Hunde heulen,", "tokens": ["Pl\u00f6tz\u00b7lich", ",", "wie", "wenn", "Hun\u00b7de", "heu\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOKOM", "KOUS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einen Trauersang zu singen!", "tokens": ["Ei\u00b7nen", "Trau\u00b7er\u00b7sang", "zu", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und in's Zimmer trat ein Mufti,", "tokens": ["Und", "in's", "Zim\u00b7mer", "trat", "ein", "Muf\u00b7ti", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Drehte bis das Lied zu Ende,", "tokens": ["Dreh\u00b7te", "bis", "das", "Lied", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich auf einem Bein im Kreise,", "tokens": ["Sich", "auf", "ei\u00b7nem", "Bein", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aehnlich wie die K\u00f6niglichen", "tokens": ["A\u00b7ehn\u00b7lich", "wie", "die", "K\u00f6\u00b7nig\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Solot\u00e4nzer unsrer Erde", "tokens": ["So\u00b7lo\u00b7t\u00e4n\u00b7zer", "uns\u00b7rer", "Er\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Alle ihre Werke schlie\u00dfen;", "tokens": ["Al\u00b7le", "ih\u00b7re", "Wer\u00b7ke", "schlie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Schnitt dann eine ganz abscheulich", "tokens": ["Schnitt", "dann", "ei\u00b7ne", "ganz", "ab\u00b7scheu\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Dumme Fratze, welche hierorts", "tokens": ["Dum\u00b7me", "Frat\u00b7ze", ",", "wel\u00b7che", "hier\u00b7orts"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Gilt als \u00fcberweltlich, heilig;", "tokens": ["Gilt", "als", "\u00fc\u00b7ber\u00b7welt\u00b7lich", ",", "hei\u00b7lig", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Gab der sch\u00f6nen Lilialinda", "tokens": ["Gab", "der", "sch\u00f6\u00b7nen", "Li\u00b7li\u00b7a\u00b7lin\u00b7da"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "Warm und herzlich sie umarmend,", "tokens": ["Warm", "und", "herz\u00b7lich", "sie", "um\u00b7ar\u00b7mend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Schmatzend drei Mal sieben K\u00fcsse;", "tokens": ["Schmat\u00b7zend", "drei", "Mal", "sie\u00b7ben", "K\u00fcs\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "CARD", "NN", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Machte dann an mir ein Zeichen,", "tokens": ["Mach\u00b7te", "dann", "an", "mir", "ein", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.19": {"text": "Welches f\u00fcr die sittlich-reine", "tokens": ["Wel\u00b7ches", "f\u00fcr", "die", "sitt\u00b7lich\u00b7rei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Erdenwelt inexpressibel;", "tokens": ["Er\u00b7den\u00b7welt", "i\u00b7nex\u00b7pres\u00b7si\u00b7bel", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.21": {"text": "Trank hierauf von Amtes wegen", "tokens": ["Trank", "hier\u00b7auf", "von", "Am\u00b7tes", "we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PAV", "APPR", "NN", "APPR"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Sieben Schn\u00e4pse Salamander;", "tokens": ["Sie\u00b7ben", "Schn\u00e4p\u00b7se", "Sa\u00b7la\u00b7man\u00b7der", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Stellte mich und meine Braut dann", "tokens": ["Stell\u00b7te", "mich", "und", "mei\u00b7ne", "Braut", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Mit dem R\u00fccken aneinander,", "tokens": ["Mit", "dem", "R\u00fc\u00b7cken", "an\u00b7ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Und rief salbungsvoll und laut dann", "tokens": ["Und", "rief", "sal\u00b7bungs\u00b7voll", "und", "laut", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "Also seinen Muftisegen:", "tokens": ["Al\u00b7so", "sei\u00b7nen", "Muf\u00b7ti\u00b7se\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbheil Dir, J\u00fcngling, da\u00df zur Gattin,", "tokens": ["\u00bb", "heil", "Dir", ",", "J\u00fcng\u00b7ling", ",", "da\u00df", "zur", "Gat\u00b7tin", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "PPER", "$,", "NN", "$,", "KOUS", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihren Reizen widerstrebend:", "tokens": ["Ih\u00b7ren", "Rei\u00b7zen", "wi\u00b7der\u00b7stre\u00b7bend", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Frommen Sinn's Du Dir erw\u00e4hlet", "tokens": ["From\u00b7men", "Sinn's", "Du", "Dir", "er\u00b7w\u00e4h\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese farbenpr\u00e4cht'ge, duft'ge,", "tokens": ["Die\u00b7se", "fa\u00b7rben\u00b7pr\u00e4cht'\u00b7ge", ",", "duft'\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PDAT", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aufgebl\u00fchte Himmelsblume,", "tokens": ["Auf\u00b7ge\u00b7bl\u00fch\u00b7te", "Him\u00b7mels\u00b7blu\u00b7me", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Namens Lilia Lilialinda,", "tokens": ["Na\u00b7mens", "Li\u00b7lia", "Li\u00b7li\u00b7a\u00b7lin\u00b7da", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und dadurch f\u00fcr jetzt und ewig", "tokens": ["Und", "da\u00b7durch", "f\u00fcr", "jetzt", "und", "e\u00b7wig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "APPR", "ADV", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jedes Anspruchs auf die Wonne", "tokens": ["Je\u00b7des", "An\u00b7spruchs", "auf", "die", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ihrer Liebe Dich begeben!", "tokens": ["Ih\u00b7rer", "Lie\u00b7be", "Dich", "be\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Heil Dir, Jungfrau, der das seltne,", "tokens": ["Heil", "Dir", ",", "Jung\u00b7frau", ",", "der", "das", "selt\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "PRELS", "ART", "ADJA", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.11": {"text": "Neidenswerthe Loos geworden,", "tokens": ["Nei\u00b7dens\u00b7wert\u00b7he", "Loos", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Da\u00df, trotz ihrer Leibessch\u00f6nheit,", "tokens": ["Da\u00df", ",", "trotz", "ih\u00b7rer", "Lei\u00b7bes\u00b7sch\u00f6n\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Und bevor die Bl\u00fcthe solcher", "tokens": ["Und", "be\u00b7vor", "die", "Bl\u00fc\u00b7the", "sol\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Abgeknicket und verdorret,", "tokens": ["Ab\u00b7ge\u00b7kni\u00b7cket", "und", "ver\u00b7dor\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Einen Gatten sie gefunden,", "tokens": ["Ei\u00b7nen", "Gat\u00b7ten", "sie", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Der, entsagend all den Reizen,", "tokens": ["Der", ",", "ent\u00b7sa\u00b7gend", "all", "den", "Rei\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVPP", "PIAT", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Ihr den frommen R\u00fccken zukehrt!", "tokens": ["Ihr", "den", "from\u00b7men", "R\u00fc\u00b7cken", "zu\u00b7kehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.18": {"text": "Heil Dir, Jungfrau! Denn von nun an", "tokens": ["Heil", "Dir", ",", "Jung\u00b7frau", "!", "Denn", "von", "nun", "an"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "NN", "$.", "KON", "APPR", "ADV", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Darf bei unsrer Tempelstraf' der", "tokens": ["Darf", "bei", "uns\u00b7rer", "Tem\u00b7pel\u00b7straf'", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "\u203amuftiheiligen Erleuchtung,\u2039", "tokens": ["\u203a", "muf\u00b7ti\u00b7hei\u00b7li\u00b7gen", "Er\u00b7leuch\u00b7tung", ",", "\u2039"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Des \u203aVerbrennens,\u2039 wie's profan hei\u00dft,", "tokens": ["Des", "\u203a", "Ver\u00b7bren\u00b7nens", ",", "\u2039", "wie's", "pro\u00b7fan", "hei\u00dft", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "$(", "NE", "NE", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.22": {"text": "Uns, die Priester, ausgenommen,", "tokens": ["Uns", ",", "die", "Pries\u00b7ter", ",", "aus\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Keiner der Verkehrten Welter,", "tokens": ["Kei\u00b7ner", "der", "Ver\u00b7kehr\u00b7ten", "Wel\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Weder Greis, noch Mann, noch J\u00fcngling,", "tokens": ["We\u00b7der", "Greis", ",", "noch", "Mann", ",", "noch", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADV", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Und am wenigsten Dein Gatte,", "tokens": ["Und", "am", "we\u00b7nigs\u00b7ten", "Dein", "Gat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "Sich Dir z\u00e4rtlich nah'n, geschweige", "tokens": ["Sich", "Dir", "z\u00e4rt\u00b7lich", "nah'n", ",", "ge\u00b7schwei\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PRF", "PPER", "ADJD", "VVFIN", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Jenes staatlich zwar erlaubten", "tokens": ["Je\u00b7nes", "staat\u00b7lich", "zwar", "er\u00b7laub\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "ADJD", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Aber immer frevelhaften,", "tokens": ["A\u00b7ber", "im\u00b7mer", "fre\u00b7vel\u00b7haf\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Ungepriesterten, unfrommen", "tokens": ["Un\u00b7ge\u00b7pries\u00b7ter\u00b7ten", ",", "un\u00b7from\u00b7men"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "ADJA"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.30": {"text": "Laien-Liebesgl\u00fccks genie\u00dfen!", "tokens": ["Lai\u00b7en\u00b7Lie\u00b7bes\u00b7gl\u00fccks", "ge\u00b7nie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Und zum dritte