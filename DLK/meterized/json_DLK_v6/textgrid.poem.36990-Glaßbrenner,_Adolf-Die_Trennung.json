-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Komm' nun, holde Mufti-Nonne,", "tokens": ["Komm'", "nun", ",", "hol\u00b7de", "Muf\u00b7ti\u00b7Non\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit mir in des Tempels Hallen,", "tokens": ["Mit", "mir", "in", "des", "Tem\u00b7pels", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo bei Sang und Liebeswonne,", "tokens": ["Wo", "bei", "Sang", "und", "Lie\u00b7bes\u00b7won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoch des Glaubens Korke knallen!", "tokens": ["Hoch", "des", "Glau\u00b7bens", "Kor\u00b7ke", "knal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und Du, Gatte, zieh' des Weges,", "tokens": ["Und", "Du", ",", "Gat\u00b7te", ",", "zieh'", "des", "We\u00b7ges", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kose mit den M\u00e4dchen allen,", "tokens": ["Ko\u00b7se", "mit", "den", "M\u00e4d\u00b7chen", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und geneu\u00df des Privileges", "tokens": ["Und", "ge\u0