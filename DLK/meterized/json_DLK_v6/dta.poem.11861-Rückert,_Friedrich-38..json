{"dta.poem.11861": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du siehst mit Augen nur und h\u00f6rest nur mit Ohren;", "tokens": ["Du", "siehst", "mit", "Au\u00b7gen", "nur", "und", "h\u00f6\u00b7rest", "nur", "mit", "Oh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geht Sehn und H\u00f6ren drum mit Aug' und Ohr verloren?", "tokens": ["Geht", "Sehn", "und", "H\u00f6\u00b7ren", "drum", "mit", "Aug'", "und", "Ohr", "ver\u00b7lo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PAV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nein, nur die Art zu sehn, zu h\u00f6ren, nicht die Kraft", "tokens": ["Nein", ",", "nur", "die", "Art", "zu", "sehn", ",", "zu", "h\u00f6\u00b7ren", ",", "nicht", "die", "Kraft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu sehn, zu h\u00f6ren, die der Seel' ist wesenhaft;", "tokens": ["Zu", "sehn", ",", "zu", "h\u00f6\u00b7ren", ",", "die", "der", "Seel'", "ist", "we\u00b7sen\u00b7haft", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,", "PRELS", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Kraft, in der sie schwebt, in der sie ruht und flie\u00dft,", "tokens": ["Die", "Kraft", ",", "in", "der", "sie", "schwebt", ",", "in", "der", "sie", "ruht", "und", "flie\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich ausgie\u00dft, in sich selbst sich schlie\u00dft und sich genie\u00dft;", "tokens": ["Sich", "aus\u00b7gie\u00dft", ",", "in", "sich", "selbst", "sich", "schlie\u00dft", "und", "sich", "ge\u00b7nie\u00dft", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "APPR", "PRF", "ADV", "PRF", "VVFIN", "KON", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Kraft, die denkt im Haupt und dir im Herzen f\u00fchlt,", "tokens": ["Die", "Kraft", ",", "die", "denkt", "im", "Haupt", "und", "dir", "im", "Her\u00b7zen", "f\u00fchlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPRART", "NN", "KON", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die auch mit Hirn und Blut nicht ist hinweggesp\u00fclt;", "tokens": ["Die", "auch", "mit", "Hirn", "und", "Blut", "nicht", "ist", "hin\u00b7weg\u00b7ge\u00b7sp\u00fclt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "KON", "NN", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die, ist ihr jeder Weg der Aeu\u00dfrung abgeschnitten,", "tokens": ["Die", ",", "ist", "ihr", "je\u00b7der", "Weg", "der", "A\u00b7e\u00b7u\u00df\u00b7rung", "ab\u00b7ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VAFIN", "PPER", "PIAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Ganz in sich selber ruht in ihrer eignen Mitten;", "tokens": ["Ganz", "in", "sich", "sel\u00b7ber", "ruht", "in", "ih\u00b7rer", "eig\u00b7nen", "Mit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und eben, wann sie sich nicht au\u00dfen th\u00e4tig zeigt,", "tokens": ["Und", "e\u00b7ben", ",", "wann", "sie", "sich", "nicht", "au\u00b7\u00dfen", "th\u00e4\u00b7tig", "zeigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "PRF", "PTKNEG", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In sich hinein, hinab, hinauf zur Gottheit steigt.", "tokens": ["In", "sich", "hin\u00b7ein", ",", "hin\u00b7ab", ",", "hin\u00b7auf", "zur", "Got\u00b7theit", "steigt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "PTKVZ", "$,", "PTKVZ", "$,", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wie wann die Blume Nachts sich schlie\u00dft, sie nun in sich", "tokens": ["Wie", "wann", "die", "Blu\u00b7me", "Nachts", "sich", "schlie\u00dft", ",", "sie", "nun", "in", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PWAV", "ART", "NN", "ADV", "PRF", "VVFIN", "$,", "PPER", "ADV", "APPR", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gesammelt hat den Duft, der Tags im Wind entwich.", "tokens": ["Ge\u00b7sam\u00b7melt", "hat", "den", "Duft", ",", "der", "Tags", "im", "Wind", "ent\u00b7wich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wie der entlaubte Baum im Winter seinen Saft", "tokens": ["Wie", "der", "ent\u00b7laub\u00b7te", "Baum", "im", "Win\u00b7ter", "sei\u00b7nen", "Saft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur\u00fcck aus Stamm und Zweig zog in den Wurzelschaft.", "tokens": ["Zu\u00b7r\u00fcck", "aus", "Stamm", "und", "Zweig", "zog", "in", "den", "Wur\u00b7zel\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "So seiner Sinne Zweig' entfaltet in den Raum,", "tokens": ["So", "sei\u00b7ner", "Sin\u00b7ne", "Zweig'", "ent\u00b7fal\u00b7tet", "in", "den", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seine Wurzel birgt in Gott des Lebens Baum.", "tokens": ["Und", "sei\u00b7ne", "Wur\u00b7zel", "birgt", "in", "Gott", "des", "Le\u00b7bens", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "O la\u00df die Sinne nicht sich in die Welt verirren,", "tokens": ["O", "la\u00df", "die", "Sin\u00b7ne", "nicht", "sich", "in", "die", "Welt", "ver\u00b7ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKNEG", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um ihre Mutter, die Besinnung, zu verwirren!", "tokens": ["Um", "ih\u00b7re", "Mut\u00b7ter", ",", "die", "Be\u00b7sin\u00b7nung", ",", "zu", "ver\u00b7wir\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Vor lauter Sehen siehst du sonst nur Nebelflor,", "tokens": ["Vor", "lau\u00b7ter", "Se\u00b7hen", "siehst", "du", "sonst", "nur", "Ne\u00b7bel\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor lauter H\u00f6ren h\u00f6rst du nur den Lerm vom Chor.", "tokens": ["Vor", "lau\u00b7ter", "H\u00f6\u00b7ren", "h\u00f6rst", "du", "nur", "den", "Lerm", "vom", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Doch wie der Astronom im Nebel nur den Stern,", "tokens": ["Doch", "wie", "der", "Ast\u00b7ro\u00b7nom", "im", "Ne\u00b7bel", "nur", "den", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So in den H\u00fcllen der Erscheinung sieh den Kern.", "tokens": ["So", "in", "den", "H\u00fcl\u00b7len", "der", "Er\u00b7schei\u00b7nung", "sieh", "den", "Kern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Wie ein Tonkundiger den Grundton aus dem Braus", "tokens": ["Wie", "ein", "Ton\u00b7kun\u00b7di\u00b7ger", "den", "Grund\u00b7ton", "aus", "dem", "Braus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Stimmen, h\u00f6re du ihn aus der Welt heraus.", "tokens": ["Der", "Stim\u00b7men", ",", "h\u00f6\u00b7re", "du", "ihn", "aus", "der", "Welt", "he\u00b7raus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Alswie ein Liebender erkl\u00e4rt f\u00fcr eine L\u00fcge", "tokens": ["Als\u00b7wie", "ein", "Lie\u00b7ben\u00b7der", "er\u00b7kl\u00e4rt", "f\u00fcr", "ei\u00b7ne", "L\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Bild, an dem er nicht erblickt geliebte Z\u00fcge;", "tokens": ["Ein", "Bild", ",", "an", "dem", "er", "nicht", "er\u00b7blickt", "ge\u00b7lieb\u00b7te", "Z\u00fc\u00b7ge", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "PTKNEG", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Denn sehenswerth ist nur am ganzen Weltgetriebe", "tokens": ["Denn", "se\u00b7hens\u00b7werth", "ist", "nur", "am", "gan\u00b7zen", "Welt\u00b7ge\u00b7trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein der Liebe Spur, gesehn vom Blick der Liebe.", "tokens": ["Al\u00b7lein", "der", "Lie\u00b7be", "Spur", ",", "ge\u00b7sehn", "vom", "Blick", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,", "VVPP", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Und wie der Freund dem Ruf des fernen Freundes lauscht,", "tokens": ["Und", "wie", "der", "Freund", "dem", "Ruf", "des", "fer\u00b7nen", "Freun\u00b7des", "lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob auch des lauten Markts Get\u00f6s dazwischen rauscht;", "tokens": ["Ob", "auch", "des", "lau\u00b7ten", "Markts", "Ge\u00b7t\u00f6s", "da\u00b7zwi\u00b7schen", "rauscht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Vom Meer, worin es schwimmt, wird er das Tr\u00f6pfchen trennen,", "tokens": ["Vom", "Meer", ",", "wo\u00b7rin", "es", "schwimmt", ",", "wird", "er", "das", "Tr\u00f6pf\u00b7chen", "tren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird seines Freundes Stimm' als Perl' im Ohr erkennen.", "tokens": ["Wird", "sei\u00b7nes", "Freun\u00b7des", "Stimm'", "als", "Perl'", "im", "Ohr", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "KOUS", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "Im Ton ist nah der Freund, von dem du bist geschieden;", "tokens": ["Im", "Ton", "ist", "nah", "der", "Freund", ",", "von", "dem", "du", "bist", "ge\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADJD", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn du treu ihm bist, so h\u00f6rst du ihn zufrieden.", "tokens": ["Und", "wenn", "du", "treu", "ihm", "bist", ",", "so", "h\u00f6rst", "du", "ihn", "zu\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PPER", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.19": {"line.1": {"text": "Im Herzen habe stets den Freund vor Angesicht,", "tokens": ["Im", "Her\u00b7zen", "ha\u00b7be", "stets", "den", "Freund", "vor", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df nie dich schrecke, was er in der Seele spricht.", "tokens": ["Da\u00df", "nie", "dich", "schre\u00b7cke", ",", "was", "er", "in", "der", "See\u00b7le", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "VVFIN", "$,", "PWS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}