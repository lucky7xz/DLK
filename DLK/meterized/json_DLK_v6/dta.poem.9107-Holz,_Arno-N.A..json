{"dta.poem.9107": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich weiss.", "tokens": ["Ich", "weiss", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Oft", "tokens": ["Oft"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "wars nur ein Lachen, ein Handdruck von dir,", "tokens": ["wars", "nur", "ein", "La\u00b7chen", ",", "ein", "Hand\u00b7druck", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "oder ein H\u00e4rchen, ein blosses H\u00e4rchen,", "tokens": ["o\u00b7der", "ein", "H\u00e4r\u00b7chen", ",", "ein", "blos\u00b7ses", "H\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "das dir der Wind ins Genick geweht,", "tokens": ["das", "dir", "der", "Wind", "ins", "Ge\u00b7nick", "ge\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "und all mein Blut", "tokens": ["und", "all", "mein", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "g\u00e4hrte gleich auf,", "tokens": ["g\u00e4hr\u00b7te", "gleich", "auf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "und all mein Herz", "tokens": ["und", "all", "mein", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "schlug nach dir.", "tokens": ["schlug", "nach", "dir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Dich haben, dich haben,", "tokens": ["Dich", "ha\u00b7ben", ",", "dich", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VAFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "dich endlich mal haben,", "tokens": ["dich", "end\u00b7lich", "mal", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "ganz und nackt, ganz und nackt!", "tokens": ["ganz", "und", "nackt", ",", "ganz", "und", "nackt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADJD", "$,", "ADV", "KON", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Und heut,", "tokens": ["Und", "heut", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "zum ersten Mal,", "tokens": ["zum", "ers\u00b7ten", "Mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "unten am See, glitzernd im Mittag,", "tokens": ["un\u00b7ten", "am", "See", ",", "glit\u00b7zernd", "im", "Mit\u00b7tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "sah ich dich so.", "tokens": ["sah", "ich", "dich", "so", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Ganz und nackt! Ganz und nackt!", "tokens": ["Ganz", "und", "nackt", "!", "Ganz", "und", "nackt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADJD", "$.", "ADV", "KON", "ADJD", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "Und mein Herz", "tokens": ["Und", "mein", "Herz"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "stand still.", "tokens": ["stand", "still", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.7": {"line.1": {"text": "Vor Gl\u00fcck, vor Gl\u00fcck.", "tokens": ["Vor", "Gl\u00fcck", ",", "vor", "Gl\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Und es war keine Welt mehr,", "tokens": ["Und", "es", "war", "kei\u00b7ne", "Welt", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "nichts, nichts, nichts,", "tokens": ["nichts", ",", "nichts", ",", "nichts", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "PIS", "$,", "PIS", "$,"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "es war nur noch Sonne, nur noch Sonne \u2014", "tokens": ["es", "war", "nur", "noch", "Son\u00b7ne", ",", "nur", "noch", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "NN", "$,", "ADV", "ADV", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "so sch\u00f6n warst du!", "tokens": ["so", "sch\u00f6n", "warst", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}