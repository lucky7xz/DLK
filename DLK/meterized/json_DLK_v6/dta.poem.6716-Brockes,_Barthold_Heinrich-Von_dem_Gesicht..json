{"dta.poem.6716": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn von des hohen Himmels Welt", "tokens": ["Wenn", "von", "des", "ho\u00b7hen", "Him\u00b7mels", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der C\u00f6rper Glantz uns in die Augen f\u00e4llt,", "tokens": ["Der", "C\u00f6r\u00b7per", "Glantz", "uns", "in", "die", "Au\u00b7gen", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Legt man denselbigen die bunten Strahlen bey.", "tokens": ["Legt", "man", "den\u00b7sel\u00b7bi\u00b7gen", "die", "bun\u00b7ten", "Strah\u00b7len", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Da mu\u00df das Licht nicht hin verfetzet seyn:", "tokens": ["Da", "mu\u00df", "das", "Licht", "nicht", "hin", "ver\u00b7fet\u00b7zet", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PTKNEG", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sie haben nichts, als blo\u00df ein eintzelnes Bewegen.", "tokens": ["Sie", "ha\u00b7ben", "nichts", ",", "als", "blo\u00df", "ein", "eint\u00b7zel\u00b7nes", "Be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "$,", "KOUS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Das Licht ist eine Art der Krafft der Seel\u2019 hingegen,", "tokens": ["Das", "Licht", "ist", "ei\u00b7ne", "Art", "der", "Krafft", "der", "Seel'", "hin\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die auf dieselbe Art, wie man versp\u00fcrt,", "tokens": ["Die", "auf", "die\u00b7sel\u00b7be", "Art", ",", "wie", "man", "ver\u00b7sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDAT", "NN", "$,", "PWAV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein Werckzeug, das beweget wird, gebiert.", "tokens": ["Ein", "Wer\u00b7ck\u00b7zeug", ",", "das", "be\u00b7we\u00b7get", "wird", ",", "ge\u00b7biert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "VAFIN", "$,", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}