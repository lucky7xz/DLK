{"textgrid.poem.60737": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Zobir", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Raublustig und schreckenverbreitend und arm", "tokens": ["Raub\u00b7lus\u00b7tig", "und", "schre\u00b7cken\u00b7ver\u00b7brei\u00b7tend", "und", "arm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Geleitet Abdalla den Araberschwarm", "tokens": ["Ge\u00b7lei\u00b7tet", "Ab\u00b7dal\u00b7la", "den", "A\u00b7ra\u00b7ber\u00b7schwarm"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "NE", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Gen Afrika zu,", "tokens": ["Gen", "Af\u00b7ri\u00b7ka", "zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "PTKVZ", "$,"], "meter": "-+---", "measure": "dactylic.init"}, "line.4": {"text": "Vor Tripoli stehn die Beherzten im Nu.", "tokens": ["Vor", "Tri\u00b7po\u00b7li", "stehn", "die", "Be\u00b7herz\u00b7ten", "im", "Nu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "APPRART", "ADV", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch ehe sie st\u00fcrmen um Mauer und Tor,", "tokens": ["Doch", "e\u00b7he", "sie", "st\u00fcr\u00b7men", "um", "Mau\u00b7er", "und", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Erscheint mit dem Heere der hohe Gregor,", "tokens": ["Er\u00b7scheint", "mit", "dem", "Hee\u00b7re", "der", "ho\u00b7he", "Gre\u00b7gor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NE", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Statthalter im Glanz", "tokens": ["Statt\u00b7hal\u00b7ter", "im", "Glanz"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Erfochtener Siege, geschickt von Byzanz.", "tokens": ["Er\u00b7foch\u00b7te\u00b7ner", "Sie\u00b7ge", ",", "ge\u00b7schickt", "von", "By\u00b7zanz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVPP", "APPR", "NE", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Und w\u00e4hrend er dr\u00e4ngt die fanatische Schar,", "tokens": ["Und", "w\u00e4h\u00b7rend", "er", "dr\u00e4ngt", "die", "fa\u00b7na\u00b7ti\u00b7sche", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ritt ihm an der Seite mit goldenem Haar,", "tokens": ["Ritt", "ihm", "an", "der", "Sei\u00b7te", "mit", "gol\u00b7de\u00b7nem", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Den Speer in der Hand,", "tokens": ["Den", "Speer", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die liebliche Tochter im Panzergewand.", "tokens": ["Die", "lieb\u00b7li\u00b7che", "Toch\u00b7ter", "im", "Pan\u00b7zer\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Sie hatte gew\u00e4hlt sich ein m\u00e4nnliches Teil,", "tokens": ["Sie", "hat\u00b7te", "ge\u00b7w\u00e4hlt", "sich", "ein", "m\u00e4nn\u00b7li\u00b7ches", "Teil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie schwenkte die Lanze, sie scho\u00df mit dem Pfeil,", "tokens": ["Sie", "schwenk\u00b7te", "die", "Lan\u00b7ze", ",", "sie", "scho\u00df", "mit", "dem", "Pfeil", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Im Schlachtenget\u00f6n", "tokens": ["Im", "Schlach\u00b7ten\u00b7ge\u00b7t\u00f6n"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wie Pallas und doch wie Cythere so sch\u00f6n.", "tokens": ["Wie", "Pal\u00b7las", "und", "doch", "wie", "Cy\u00b7the\u00b7re", "so", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "ADV", "KOKOM", "NE", "ADV", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Der Vater erhub sich, und blickend umher", "tokens": ["Der", "Va\u00b7ter", "er\u00b7hub", "sich", ",", "und", "bli\u00b7ckend", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "KON", "ADJD", "PTKVZ"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Befeuerte m\u00e4chtig die Seinigen er:", "tokens": ["Be\u00b7feu\u00b7er\u00b7te", "m\u00e4ch\u00b7tig", "die", "Sei\u00b7ni\u00b7gen", "er", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "PPOSS", "PPER", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nicht l\u00e4nger gespielt,", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "ge\u00b7spielt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ihr M\u00e4nner, und stets nach Abdalla gezielt!", "tokens": ["Ihr", "M\u00e4n\u00b7ner", ",", "und", "stets", "nach", "Ab\u00b7dal\u00b7la", "ge\u00b7zielt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "ADV", "APPR", "NE", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Und wer mir das Haupt des Erschlagenen beut,", "tokens": ["Und", "wer", "mir", "das", "Haupt", "des", "Er\u00b7schla\u00b7ge\u00b7nen", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem geb ich die sch\u00f6ne Maria noch heut,", "tokens": ["Dem", "geb", "ich", "die", "sch\u00f6\u00b7ne", "Ma\u00b7ria", "noch", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "ADJA", "NE", "ADV", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ein k\u00f6stlicher Sold,", "tokens": ["Ein", "k\u00f6st\u00b7li\u00b7cher", "Sold", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Mit ihr unerme\u00dfliche Sch\u00e4tze von Gold!", "tokens": ["Mit", "ihr", "un\u00b7er\u00b7me\u00df\u00b7li\u00b7che", "Sch\u00e4t\u00b7ze", "von", "Gold", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Da warfen die Christen verdoppelten Schaft,", "tokens": ["Da", "war\u00b7fen", "die", "Chris\u00b7ten", "ver\u00b7dop\u00b7pel\u00b7ten", "Schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Den Gl\u00e4ubigen Mekkas erlahmte die Kraft,", "tokens": ["Den", "Gl\u00e4u\u00b7bi\u00b7gen", "Mek\u00b7kas", "er\u00b7lahm\u00b7te", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Abdalla begab", "tokens": ["Ab\u00b7dal\u00b7la", "be\u00b7gab"], "token_info": ["word", "word"], "pos": ["NE", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ins Zelt sich und mied ein bereitetes Grab.", "tokens": ["Ins", "Zelt", "sich", "und", "mied", "ein", "be\u00b7rei\u00b7te\u00b7tes", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Doch stritt in dem Heere, von Eifer entfacht,", "tokens": ["Doch", "stritt", "in", "dem", "Hee\u00b7re", ",", "von", "Ei\u00b7fer", "ent\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Zobir, ein gewaltiger Blitz in der Schlacht;", "tokens": ["Zo\u00b7bir", ",", "ein", "ge\u00b7wal\u00b7ti\u00b7ger", "Blitz", "in", "der", "Schlacht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Fort jagt er im Zorn,", "tokens": ["Fort", "jagt", "er", "im", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ihm triefte der klirrende, blutige Sporn.", "tokens": ["Ihm", "trief\u00b7te", "der", "klir\u00b7ren\u00b7de", ",", "blu\u00b7ti\u00b7ge", "Sporn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "Er eilt zum Gebieter und spricht: Du vers\u00e4umst", "tokens": ["Er", "eilt", "zum", "Ge\u00b7bie\u00b7ter", "und", "spricht", ":", "Du", "ver\u00b7s\u00e4umst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Abdalla, die Schlacht, wie ein Knabe? Du tr\u00e4umst", "tokens": ["Ab\u00b7dal\u00b7la", ",", "die", "Schlacht", ",", "wie", "ein", "Kna\u00b7be", "?", "Du", "tr\u00e4umst"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "$,", "PWAV", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Im weichen Gezelt?", "tokens": ["Im", "wei\u00b7chen", "Ge\u00b7zelt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und sollst dem Kalifen erobern die Welt?", "tokens": ["Und", "sollst", "dem", "Ka\u00b7li\u00b7fen", "er\u00b7o\u00b7bern", "die", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.10": {"line.1": {"text": "Was, uns zu entnerven, ersonnen der Christ,", "tokens": ["Was", ",", "uns", "zu", "ent\u00b7ner\u00b7ven", ",", "er\u00b7son\u00b7nen", "der", "Christ", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ihn m\u00f6g es verderben mit \u00e4hnlicher List!", "tokens": ["Ihn", "m\u00f6g", "es", "ver\u00b7der\u00b7ben", "mit", "\u00e4hn\u00b7li\u00b7cher", "List", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Das Gleiche sogleich", "tokens": ["Das", "Glei\u00b7che", "sog\u00b7leich"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Versprich es und stelle dich eben so reich!", "tokens": ["Ver\u00b7sprich", "es", "und", "stel\u00b7le", "dich", "e\u00b7ben", "so", "reich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "KON", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.11": {"line.1": {"text": "Den Deinen verk\u00fcndige folgendes Wort:", "tokens": ["Den", "Dei\u00b7nen", "ver\u00b7k\u00fcn\u00b7di\u00b7ge", "fol\u00b7gen\u00b7des", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wer immer dem feindlichen F\u00fchrer sofort", "tokens": ["Wer", "im\u00b7mer", "dem", "feind\u00b7li\u00b7chen", "F\u00fch\u00b7rer", "so\u00b7fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Den Sch\u00e4del zerhaut,", "tokens": ["Den", "Sch\u00e4\u00b7del", "zer\u00b7haut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Der nehme die sch\u00f6ne Maria zur Braut!", "tokens": ["Der", "neh\u00b7me", "die", "sch\u00f6\u00b7ne", "Ma\u00b7ria", "zur", "Braut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NE", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.12": {"line.1": {"text": "Dies k\u00fcndet Abdalla mit frischerem Sinn,", "tokens": ["Dies", "k\u00fcn\u00b7det", "Ab\u00b7dal\u00b7la", "mit", "fri\u00b7sche\u00b7rem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Die Seinen ermutiget hoher Gewinn;", "tokens": ["Die", "Sei\u00b7nen", "er\u00b7mu\u00b7ti\u00b7get", "ho\u00b7her", "Ge\u00b7winn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSS", "VVFIN", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Zobir dringt vor,", "tokens": ["Zo\u00b7bir", "dringt", "vor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Sein kreisender S\u00e4bel erlegt den Gregor.", "tokens": ["Sein", "krei\u00b7sen\u00b7der", "S\u00e4\u00b7bel", "er\u00b7legt", "den", "Gre\u00b7gor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NE", "$."], "meter": "-+--+--+---", "measure": "amphibrach.tri.plus"}}, "stanza.13": {"line.1": {"text": "Schon birgt in die Stadt sich die christliche Schmach,", "tokens": ["Schon", "birgt", "in", "die", "Stadt", "sich", "die", "christ\u00b7li\u00b7che", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Schon folgen die Sieger und st\u00fcrzen sich nach,", "tokens": ["Schon", "fol\u00b7gen", "die", "Sie\u00b7ger", "und", "st\u00fcr\u00b7zen", "sich", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Schon weht von den vier", "tokens": ["Schon", "weht", "von", "den", "vier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "CARD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Kastellen herab des Propheten Panier.", "token