{"textgrid.poem.32561": {"metadata": {"author": {"name": "Wieland, Christoph Martin", "birth": "N.A.", "death": "N.A."}, "title": "Ode. An seine Freundin", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doris, f\u00fchle dies Lied, f\u00fchl in der Ferne selbst", "tokens": ["Do\u00b7ris", ",", "f\u00fch\u00b7le", "dies", "Lied", ",", "f\u00fchl", "in", "der", "Fer\u00b7ne", "selbst"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PDS", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wie dein Thyrsis itzt f\u00fchlt, hohe Empfindungen,", "tokens": ["Wie", "dein", "Thyr\u00b7sis", "itzt", "f\u00fchlt", ",", "ho\u00b7he", "Emp\u00b7fin\u00b7dun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Gleich dem Gef\u00fchl des D\u00e4mons", "tokens": ["Gleich", "dem", "Ge\u00b7f\u00fchl", "des", "D\u00e4\u00b7mons"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Wenn er die himmlische Nymphe k\u00fc\u00dft.", "tokens": ["Wenn", "er", "die", "himm\u00b7li\u00b7sche", "Nym\u00b7phe", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Sanft, mit stiller Gewalt, fasse die zarte Brust", "tokens": ["Sanft", ",", "mit", "stil\u00b7ler", "Ge\u00b7walt", ",", "fas\u00b7se", "die", "zar\u00b7te", "Brust"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die Bewegung die itzt, G\u00f6ttliche, mich ergreift,", "tokens": ["Die", "Be\u00b7we\u00b7gung", "die", "itzt", ",", "G\u00f6tt\u00b7li\u00b7che", ",", "mich", "er\u00b7greift", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "$,", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Von sympathetischen Freuden", "tokens": ["Von", "sym\u00b7pa\u00b7the\u00b7ti\u00b7schen", "Freu\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bebe dein Herz und empfind wie ich.", "tokens": ["Be\u00b7be", "dein", "Herz", "und", "emp\u00b7find", "wie", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "ADJD", "KOKOM", "PPER", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Welche Ruhe, die sich \u00fcber mein Herz ergie\u00dft?", "tokens": ["Wel\u00b7che", "Ru\u00b7he", ",", "die", "sich", "\u00fc\u00b7ber", "mein", "Herz", "er\u00b7gie\u00dft", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "PRELS", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Welche Himmel von Lust wo sich mein Blick verl\u00e4uft?", "tokens": ["Wel\u00b7che", "Him\u00b7mel", "von", "Lust", "wo", "sich", "mein", "Blick", "ver\u00b7l\u00e4uft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "APPR", "NN", "PWAV", "PRF", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Doris, dich denkt mein Geist nur!", "tokens": ["Do\u00b7ris", ",", "dich", "denkt", "mein", "Geist", "nur", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich und die himmlische Liebe nur.", "tokens": ["Dich", "und", "die", "himm\u00b7li\u00b7sche", "Lie\u00b7be", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Tod ist ihm itzt die Welt, kein Gesch\u00f6pf ist ihm mehr,", "tokens": ["Tod", "ist", "ihm", "itzt", "die", "Welt", ",", "kein", "Ge\u00b7sch\u00f6pf", "ist", "ihm", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ART", "NN", "$,", "PIAT", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du, du winkest ihm itzt, l\u00e4chelnder Himmel, nicht,", "tokens": ["Du", ",", "du", "win\u00b7kest", "ihm", "itzt", ",", "l\u00e4\u00b7cheln\u00b7der", "Him\u00b7mel", ",", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,", "PTKNEG", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Kein einladender Abend", "tokens": ["Kein", "ein\u00b7la\u00b7den\u00b7der", "A\u00b7bend"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Nimmt mich in tauende Schatten ein.", "tokens": ["Nimmt", "mich", "in", "tau\u00b7en\u00b7de", "Schat\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Dein Olympisches Lied t\u00f6nt nicht mehr in mein Ohr", "tokens": ["Dein", "O\u00b7lym\u00b7pi\u00b7sches", "Lied", "t\u00f6nt", "nicht", "mehr", "in", "mein", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du, bei dem ich so oft meinen Virgil verga\u00df,", "tokens": ["Du", ",", "bei", "dem", "ich", "so", "oft", "mei\u00b7nen", "Vir\u00b7gil", "ver\u00b7ga\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "NE", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der du in Harfen der Engel", "tokens": ["Der", "du", "in", "Har\u00b7fen", "der", "En\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den erhabnen Messias singst!", "tokens": ["Den", "er\u00b7hab\u00b7nen", "Mes\u00b7si\u00b7as", "singst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Doris bleibt mir allein aus der Unendlichkeit", "tokens": ["Do\u00b7ris", "bleibt", "mir", "al\u00b7lein", "aus", "der", "Un\u00b7end\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Deiner Bildungen, Gott, ist Sie allein mir noch,", "tokens": ["Dei\u00b7ner", "Bil\u00b7dun\u00b7gen", ",", "Gott", ",", "ist", "Sie", "al\u00b7lein", "mir", "noch", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VAFIN", "PPER", "ADV", "PPER", "ADV", "$,"], "meter": "+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "F\u00fcllt die Sch\u00f6nste der Seelen", "tokens": ["F\u00fcllt", "die", "Sch\u00f6ns\u00b7te", "der", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ganz dies ihr nur geschaffne Herz.", "tokens": ["Ganz", "dies", "ihr", "nur", "ge\u00b7schaff\u00b7ne", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O wie wallt es so sanft! o wie befriediget", "tokens": ["O", "wie", "wallt", "es", "so", "sanft", "!", "o", "wie", "be\u00b7frie\u00b7di\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "ADV", "ADJD", "$.", "FM", "KOKOM", "VVFIN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Schlummern tief in der Brust alle Begierden ein,", "tokens": ["Schlum\u00b7mern", "tief", "in", "der", "Brust", "al\u00b7le", "Be\u00b7gier\u00b7den", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und die schauende Seele", "tokens": ["Und", "die", "schau\u00b7en\u00b7de", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "G\u00f6ttliche Sch\u00f6ne, h\u00e4ngt ganz an dir!", "tokens": ["G\u00f6tt\u00b7li\u00b7che", "Sch\u00f6\u00b7ne", ",", "h\u00e4ngt", "ganz", "an", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Wie Dein himmlischer Geist jeglichen Blick belebt!", "tokens": ["Wie", "Dein", "himm\u00b7li\u00b7scher", "Geist", "jeg\u00b7li\u00b7chen", "Blick", "be\u00b7lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "PIAT", "NN", "VVPP", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Wie im redenden Aug, ach! im so sch\u00f6nen Aug!", "tokens": ["Wie", "im", "re\u00b7den\u00b7den", "Aug", ",", "ach", "!", "im", "so", "sch\u00f6\u00b7nen", "Aug", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "$,", "ITJ", "$.", "APPRART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sich sie Seele enth\u00fcllet", "tokens": ["Sich", "sie", "See\u00b7le", "ent\u00b7h\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "PPER", "NN", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Die So z\u00e4rtlich und edel denkt?", "tokens": ["Die", "So", "z\u00e4rt\u00b7lich", "und", "e\u00b7del", "denkt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.9": {"line.1": {"text": "Wie den bl\u00fchenden Leib Anmut und Huld umflie\u00dft?", "tokens": ["Wie", "den", "bl\u00fc\u00b7hen\u00b7den", "Leib", "An\u00b7mut", "und", "Huld", "um\u00b7flie\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "War nicht Eva so sch\u00f6n, da ihr entstehend Bild", "tokens": ["War", "nicht", "E\u00b7va", "so", "sch\u00f6n", ",", "da", "ihr", "ent\u00b7ste\u00b7hend", "Bild"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "NN", "ADV", "ADJD", "$,", "KOUS", "PPER", "VVPP", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zur begeisterten Seele", "tokens": ["Zur", "be\u00b7geis\u00b7ter\u00b7ten", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "G\u00f6ttlicher Milton! herunter stieg?", "tokens": ["G\u00f6tt\u00b7li\u00b7cher", "Mil\u00b7ton", "!", "her\u00b7un\u00b7ter", "stieg", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "APZR", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "O! wie liebt dich dein Freund? o wie begl\u00fcckst du ihn!", "tokens": ["O", "!", "wie", "liebt", "dich", "dein", "Freund", "?", "o", "wie", "be\u00b7gl\u00fcckst", "du", "ihn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "FM", "KOKOM", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn dein Hyblischer Mund sich seinen K\u00fcssen beut,", "tokens": ["Wenn", "dein", "Hyb\u00b7li\u00b7scher", "Mund", "sich", "sei\u00b7nen", "K\u00fcs\u00b7sen", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und die Sanftzitternde Lippe", "tokens": ["Und", "die", "Sanft\u00b7zit\u00b7tern\u00b7de", "Lip\u00b7pe"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Gleich der Rose in Knospen schwellt.", "tokens": ["Gleich", "der", "Ro\u00b7se", "in", "Knos\u00b7pen", "schwellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Wenn mein freudiger Blick an deinen Blicken h\u00e4ngt", "tokens": ["Wenn", "mein", "freu\u00b7di\u00b7ger", "Blick", "an", "dei\u00b7nen", "Bli\u00b7cken", "h\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und die Seligkeit sieht, die itzt dein Herz umfa\u00dft,", "tokens": ["Und", "die", "Se\u00b7lig\u00b7keit", "sieht", ",", "die", "itzt", "dein", "Herz", "um\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Freuden erhabnerer Sph\u00e4ren", "tokens": ["Freu\u00b7den", "er\u00b7hab\u00b7ne\u00b7rer", "Sph\u00e4\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Die kein Sklave der Erden kennt.", "tokens": ["Die", "kein", "Skla\u00b7ve", "der", "Er\u00b7den", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.12": {"line.1": {"text": "O! wie ist er entz\u00fcckt? o wie begeisternd gl\u00e4nzt", "tokens": ["O", "!", "wie", "ist", "er", "ent\u00b7z\u00fcckt", "?", "o", "wie", "be\u00b7geis\u00b7ternd", "gl\u00e4nzt"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "VAFIN", "PPER", "VVPP", "$.", "FM", "KOKOM", "VVPP", "VVFIN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ihm dein himmlisches Aug und das zufriedne Rot", "tokens": ["Ihm", "dein", "himm\u00b7li\u00b7sches", "Aug", "und", "das", "zu\u00b7fried\u00b7ne", "Rot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Das die Wangen umflie\u00dfet", "tokens": ["Das", "die", "Wan\u00b7gen", "um\u00b7flie\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "VVFIN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und im Munde noch frischer bl\u00fcht.", "tokens": ["Und", "im", "Mun\u00b7de", "noch", "fri\u00b7scher", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "