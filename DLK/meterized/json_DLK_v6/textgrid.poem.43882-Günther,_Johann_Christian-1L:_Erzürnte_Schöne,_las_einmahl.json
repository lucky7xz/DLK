{"textgrid.poem.43882": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erz\u00fcrnte Sch\u00f6ne, las einmahl", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erz\u00fcrnte Sch\u00f6ne, las einmahl", "tokens": ["Er\u00b7z\u00fcrn\u00b7te", "Sch\u00f6\u00b7ne", ",", "las", "ein\u00b7mahl"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den alten Unmuth aus dem Herzen", "tokens": ["Den", "al\u00b7ten", "Un\u00b7muth", "aus", "dem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und deiner holden Augen Strahl", "tokens": ["Und", "dei\u00b7ner", "hol\u00b7den", "Au\u00b7gen", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit angenehmen Blicken scherzen!", "tokens": ["Mit", "an\u00b7ge\u00b7neh\u00b7men", "Bli\u00b7cken", "scher\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich habe freylich viel versehn,", "tokens": ["Ich", "ha\u00b7be", "frey\u00b7lich", "viel", "ver\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch ist's aus Vorsaz nicht geschehn,", "tokens": ["Doch", "ist's", "aus", "Vor\u00b7saz", "nicht", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es sind vorwahr nur Schwachheitss\u00fcnden.", "tokens": ["Es", "sind", "vor\u00b7wahr", "nur", "Schwach\u00b7heits\u00b7s\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Mensch, der seine Schuld bekennt,", "tokens": ["Ein", "Mensch", ",", "der", "sei\u00b7ne", "Schuld", "be\u00b7kennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der mu\u00df, so sehr der Eifer brennt,", "tokens": ["Der", "mu\u00df", ",", "so", "sehr", "der", "Ei\u00b7fer", "brennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Auf Reu und Bu\u00dfe Gnade finden.", "tokens": ["Auf", "Reu", "und", "Bu\u00b7\u00dfe", "Gna\u00b7de", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Schaden, den ich dir gemacht,", "tokens": ["Der", "Scha\u00b7den", ",", "den", "ich", "dir", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist doch noch endlich zu ersezen.", "tokens": ["Ist", "doch", "noch", "end\u00b7lich", "zu", "er\u00b7se\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ach w\u00fcte doch nur mit Bedacht!", "tokens": ["Ach", "w\u00fc\u00b7te", "doch", "nur", "mit", "Be\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du solst mich einmahl redlich sch\u00e4zen.", "tokens": ["Du", "solst", "mich", "ein\u00b7mahl", "red\u00b7lich", "sch\u00e4\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich habe ja genung geb\u00fc\u00dft,", "tokens": ["Ich", "ha\u00b7be", "ja", "ge\u00b7nung", "ge\u00b7b\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nachdem ich eine Lust vermi\u00dft,", "tokens": ["Nach\u00b7dem", "ich", "ei\u00b7ne", "Lust", "ver\u00b7mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wozu du mich vorl\u00e4ngst erlesen.", "tokens": ["Wo\u00b7zu", "du", "mich", "vor\u00b7l\u00e4ngst", "er\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Strafe nehm ich willig an,", "tokens": ["Die", "Stra\u00b7fe", "nehm", "ich", "wil\u00b7lig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Weil sie, wie ich nicht leugnen kan,", "tokens": ["Weil", "sie", ",", "wie", "ich", "nicht", "leug\u00b7nen", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In etwas ist verdient gewesen.", "tokens": ["In", "et\u00b7was", "ist", "ver\u00b7dient", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "VVPP", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Von nun an aber bitt ich dich", "tokens": ["Von", "nun", "an", "a\u00b7ber", "bitt", "ich", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PTKVZ", "KON", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey deinen feuerreichen Augen:", "tokens": ["Bey", "dei\u00b7nen", "feu\u00b7er\u00b7rei\u00b7chen", "Au\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wirf Zorn und Eifer hinter sich", "tokens": ["Wirf", "Zorn", "und", "Ei\u00b7fer", "hin\u00b7ter", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "APPR", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und las dir meine Demuth taugen!", "tokens": ["Und", "las", "dir", "mei\u00b7ne", "De\u00b7muth", "tau\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dein klug- und aufgeweckter Geist,", "tokens": ["Dein", "klug", "und", "auf\u00b7ge\u00b7weck\u00b7ter", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der sich aus allen Worten weist,", "tokens": ["Der", "sich", "aus", "al\u00b7len", "Wor\u00b7ten", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Macht, da\u00df ich deinen Umgang liebe.", "tokens": ["Macht", ",", "da\u00df", "ich", "dei\u00b7nen", "Um\u00b7gang", "lie\u00b7be", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Fehlt mir Gelegenheit dazu,", "tokens": ["Fehlt", "mir", "Ge\u00b7le\u00b7gen\u00b7heit", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PAV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "So rathe, was ich jezund thu?", "tokens": ["So", "ra\u00b7the", ",", "was", "ich", "je\u00b7zund", "thu", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dich ehr ich mit verschwiegnem Triebe.", "tokens": ["Dich", "ehr", "ich", "mit", "ver\u00b7schwieg\u00b7nem", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}