{"dta.poem.8710": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "In der Nacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Alles schl\u00e4ft, und \u00fcber's Gefild der Ruhe", "tokens": ["Al\u00b7les", "schl\u00e4ft", ",", "und", "\u00fc\u00b7ber's", "Ge\u00b7fild", "der", "Ru\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KON", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Wandelt leisen Schrittes dahin des Lebens", "tokens": ["Wan\u00b7delt", "lei\u00b7sen", "Schrit\u00b7tes", "da\u00b7hin", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "PAV", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Genius; sanft schimmert vom Weltendom die", "tokens": ["Ge\u00b7nius", ";", "sanft", "schim\u00b7mert", "vom", "Wel\u00b7ten\u00b7dom", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADJD", "VVFIN", "APPRART", "NN", "ART"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Lampe des Mondes.", "tokens": ["Lam\u00b7pe", "des", "Mon\u00b7des", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Sieh! den ernsten Z\u00fcgen des Gott's entringet", "tokens": ["Sieh", "!", "den", "erns\u00b7ten", "Z\u00fc\u00b7gen", "des", "Gott's", "ent\u00b7rin\u00b7get"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ART", "ADJA", "NN", "ART", "NE", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Holdes L\u00e4cheln sich, denn er sieht die Lieben", "tokens": ["Hol\u00b7des", "L\u00e4\u00b7cheln", "sich", ",", "denn", "er", "sieht", "die", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PRF", "$,", "KON", "PPER", "VVFIN", "ART", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "In des Schlafes s\u00fc\u00dfer Umarmung ihrer", "tokens": ["In", "des", "Schla\u00b7fes", "s\u00fc\u00b7\u00dfer", "Um\u00b7ar\u00b7mung", "ih\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "PPOSAT"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Qualen vergessen.", "tokens": ["Qua\u00b7len", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "H\u00fcll' in deine Schatten mich tief, geliebte", "tokens": ["H\u00fcll'", "in", "dei\u00b7ne", "Schat\u00b7ten", "mich", "tief", ",", "ge\u00b7lieb\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PPER", "ADJD", "$,", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Linde, da\u00df die kummergebleichte Wange,", "tokens": ["Lin\u00b7de", ",", "da\u00df", "die", "kum\u00b7mer\u00b7ge\u00b7bleich\u00b7te", "Wan\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Und die bange Thr\u00e4ne sein holdes L\u00e4cheln", "tokens": ["Und", "die", "ban\u00b7ge", "Thr\u00e4\u00b7ne", "sein", "hol\u00b7des", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nimmer verscheuche!", "tokens": ["Nim\u00b7mer", "ver\u00b7scheu\u00b7che", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJA", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Ach, schon dreimal sank dir die Bl\u00fcth', o Linde,", "tokens": ["Ach", ",", "schon", "drei\u00b7mal", "sank", "dir", "die", "Bl\u00fcth'", ",", "o", "Lin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "FM", "NE", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Seit der Stunde, wo das Gespr\u00e4ch der Freunde", "tokens": ["Seit", "der", "Stun\u00b7de", ",", "wo", "das", "Ge\u00b7spr\u00e4ch", "der", "Freun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von Unsterblichkeit du behorchtest, und ein", "tokens": ["Von", "U\u00b7nsterb\u00b7lich\u00b7keit", "du", "be\u00b7horch\u00b7test", ",", "und", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$,", "KON", "ART"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sanftes Ges\u00e4usel", "tokens": ["Sanf\u00b7tes", "Ge\u00b7s\u00e4u\u00b7sel"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Durch dein mondversilbertes Laub uns Hoffnung", "tokens": ["Durch", "dein", "mond\u00b7ver\u00b7sil\u00b7ber\u00b7tes", "Laub", "uns", "Hoff\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPER", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "In die Seele go\u00df, da\u00df wir einst uns wieder", "tokens": ["In", "die", "See\u00b7le", "go\u00df", ",", "da\u00df", "wir", "einst", "uns", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PPER", "ADV"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Finden; \u2014 dreimal welkte der Halm am Grabe", "tokens": ["Fin\u00b7den", ";", "drei\u00b7mal", "welk\u00b7te", "der", "Halm", "am", "Gra\u00b7be"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "$(", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Meines Geliebten!", "tokens": ["Mei\u00b7nes", "Ge\u00b7lieb\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}