{"dta.poem.7007": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Herrn  B .  H .  B rockes  \n eigene,  \n so wohl  M oralische als  \n Physicalische,  \n  G edichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die dadurch erregten Triebe", "tokens": ["Die", "da\u00b7durch", "er\u00b7reg\u00b7ten", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PAV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stiller Freuden, br\u00fcnst\u2019ger Liebe,", "tokens": ["Stil\u00b7ler", "Freu\u00b7den", ",", "br\u00fcnst'\u00b7ger", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein durch Lust gewirckt Entz\u00fccken", "tokens": ["Ein", "durch", "Lust", "ge\u00b7wirckt", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "VVPP", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gottes Wunder anzublicken,", "tokens": ["Got\u00b7tes", "Wun\u00b7der", "an\u00b7zu\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mit Danck sie zu beschauen,", "tokens": ["Und", "mit", "Danck", "sie", "zu", "be\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ehrfurcht, Andacht, einen Willen,", "tokens": ["Ehr\u00b7furcht", ",", "An\u00b7dacht", ",", "ei\u00b7nen", "Wil\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "GoTTES Willen zu erf\u00fcllen,", "tokens": ["GoT\u00b7TES", "Wil\u00b7len", "zu", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und ein kindliches Vertrauen,", "tokens": ["Und", "ein", "kind\u00b7li\u00b7ches", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Stellen sie in holder Zier", "tokens": ["Stel\u00b7len", "sie", "in", "hol\u00b7der", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Jhrem GOTT gef\u00e4llig f\u00fcr.", "tokens": ["Ih\u00b7rem", "GoTT", "ge\u00b7f\u00e4l\u00b7lig", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}