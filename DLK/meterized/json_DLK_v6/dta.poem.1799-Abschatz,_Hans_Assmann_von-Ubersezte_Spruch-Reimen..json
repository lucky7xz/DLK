{"dta.poem.1799": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wilt du in der Welt was seyn/ m\u00fche dich was zu erwerben/", "tokens": ["Wilt", "du", "in", "der", "Welt", "was", "seyn", "/", "m\u00fc\u00b7he", "dich", "was", "zu", "er\u00b7wer\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "PWS", "VAINF", "$(", "VVFIN", "PPER", "PIS", "PTKZU", "VVINF", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Feige Fr\u00f6migkeit hat Lob/ aber mu\u00df wohl frostig sterben.", "tokens": ["Fei\u00b7ge", "Fr\u00f6\u00b7mig\u00b7keit", "hat", "Lob", "/", "a\u00b7ber", "mu\u00df", "wohl", "fros\u00b7tig", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "NN", "$(", "ADV", "VMFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "f\u00fcr Frost verderben.", "tokens": ["f\u00fcr", "Frost", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}