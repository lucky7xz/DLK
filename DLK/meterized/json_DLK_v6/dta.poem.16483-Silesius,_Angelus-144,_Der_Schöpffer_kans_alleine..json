{"dta.poem.16483": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "144,  Der Sch\u00f6pffer kans alleine.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Was bildestu dir ein zu zehln der Sternenschaar?", "tokens": ["Was", "bil\u00b7des\u00b7tu", "dir", "ein", "zu", "zehln", "der", "Ster\u00b7nen\u00b7schaar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "PTKZU", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der sch\u00f6pffer ists allein/ der sie kan zehlen gar.", "tokens": ["Der", "sch\u00f6pf\u00b7fer", "ists", "al\u00b7lein", "/", "der", "sie", "kan", "zeh\u00b7len", "gar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ADV", "$(", "PRELS", "PPER", "VMFIN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}