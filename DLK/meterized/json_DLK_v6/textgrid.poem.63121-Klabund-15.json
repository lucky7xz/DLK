{"textgrid.poem.63121": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "15", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll man denn den Dichtern trauen?", "tokens": ["Soll", "man", "denn", "den", "Dich\u00b7tern", "trau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr Gesch\u00e4ft hei\u00dft: Lob der Frauen.", "tokens": ["Ihr", "Ge\u00b7sch\u00e4ft", "hei\u00dft", ":", "Lob", "der", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst der blinde Dichtervater", "tokens": ["Selbst", "der", "blin\u00b7de", "Dich\u00b7ter\u00b7va\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schnurrt gleich einem Fr\u00fchlingskater,", "tokens": ["Schnurrt", "gleich", "ei\u00b7nem", "Fr\u00fch\u00b7lings\u00b7ka\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Harft er von der Helena,", "tokens": ["Harft", "er", "von", "der", "He\u00b7le\u00b7na", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die sein Auge niemals sah.", "tokens": ["Die", "sein", "Au\u00b7ge", "nie\u00b7mals", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Trumpf ist beides: blond und braun.", "tokens": ["Trumpf", "ist", "bei\u00b7des", ":", "blond", "und", "braun", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "$.", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Doch die Krone aller Fraun,", "tokens": ["Doch", "die", "Kro\u00b7ne", "al\u00b7ler", "Fraun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wild und mild und bitters\u00fc\u00df", "tokens": ["Wild", "und", "mild", "und", "bit\u00b7ter\u00b7s\u00fc\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "KON", "XY"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Sind die M\u00e4dchen von Paris.", "tokens": ["Sind", "die", "M\u00e4d\u00b7chen", "von", "Pa\u00b7ris", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dunkle Italienerinnen", "tokens": ["Dunk\u00b7le", "I\u00b7ta\u00b7li\u00b7e\u00b7ne\u00b7rin\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "M\u00f6gen Liebesf\u00e4den spinnen.", "tokens": ["M\u00f6\u00b7gen", "Lie\u00b7bes\u00b7f\u00e4\u00b7den", "spin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Deutsche, eine T\u00fcrkin", "tokens": ["Ei\u00b7ne", "Deut\u00b7sche", ",", "ei\u00b7ne", "T\u00fcr\u00b7kin"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mag auf manchen J\u00fcngling wirken.", "tokens": ["Mag", "auf", "man\u00b7chen", "J\u00fcng\u00b7ling", "wir\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit der schlanken Angelsachsin", "tokens": ["Mit", "der", "schlan\u00b7ken", "An\u00b7gel\u00b7sac\u00b7hsin"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "F\u00fchlt man seelisch sich verwachsen.", "tokens": ["F\u00fchlt", "man", "see\u00b7lisch", "sich", "ver\u00b7wach\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Trumpf ist beides: blond und braun.", "tokens": ["Trumpf", "ist", "bei\u00b7des", ":", "blond", "und", "braun", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "$.", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Doch die Krone aller Fraun,", "tokens": ["Doch", "die", "Kro\u00b7ne", "al\u00b7ler", "Fraun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wild und mild und bitters\u00fc\u00df", "tokens": ["Wild", "und", "mild", "und", "bit\u00b7ter\u00b7s\u00fc\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "KON", "XY"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Sind die M\u00e4dchen von Paris.", "tokens": ["Sind", "die", "M\u00e4d\u00b7chen", "von", "Pa\u00b7ris", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Welche Szene: an der Seine:", "tokens": ["Wel\u00b7che", "Sze\u00b7ne", ":", "an", "der", "Sei\u00b7ne", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$.", "APPR", "ART", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine Nymphe! Eine Sch\u00f6ne!", "tokens": ["Ei\u00b7ne", "Nym\u00b7phe", "!", "Ei\u00b7ne", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleicht ihr Leib nicht der Alhambra", "tokens": ["Gleicht", "ihr", "Leib", "nicht", "der", "Al\u00b7hamb\u00b7ra"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoch gebaut? Es atmen Ambra", "tokens": ["Hoch", "ge\u00b7baut", "?", "Es", "at\u00b7men", "A\u00b7mbra"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$.", "PPER", "VVFIN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihre tulpenroten Lippen,", "tokens": ["Ih\u00b7re", "tul\u00b7pen\u00b7ro\u00b7ten", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "(die am liebsten Portwein nippen...)", "tokens": ["(", "die", "am", "liebs\u00b7ten", "Port\u00b7wein", "nip\u00b7pen", "...", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "APPRART", "ADJA", "NN", "VVINF", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schopf und Scho\u00df: ein goldnes Braun", "tokens": ["Schopf", "und", "Scho\u00df", ":", "ein", "gold\u00b7nes", "Braun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Bei der Krone aller Fraun,", "tokens": ["Bei", "der", "Kro\u00b7ne", "al\u00b7ler", "Fraun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wild und mild und bitters\u00fc\u00df", "tokens": ["Wild", "und", "mild", "und", "bit\u00b7ter\u00b7s\u00fc\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "KON", "XY"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Sind die M\u00e4dchen von Paris...", "tokens": ["Sind", "die", "M\u00e4d\u00b7chen", "von", "Pa\u00b7ris", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}