{"dta.poem.13034": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Zweifel, der zuvor sich aus dem Saal verschlichen,", "tokens": ["Der", "Zwei\u00b7fel", ",", "der", "zu\u00b7vor", "sich", "aus", "dem", "Saal", "ver\u00b7schli\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jezo wieder kam; erz\u00e4hlte von den Spr\u00fcchen", "tokens": ["Und", "je\u00b7zo", "wie\u00b7der", "kam", ";", "er\u00b7z\u00e4hl\u00b7te", "von", "den", "Spr\u00fc\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN", "$.", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die er selbst angeh\u00f6rt, den k\u00fcrzesten Begriff;", "tokens": ["Die", "er", "selbst", "an\u00b7ge\u00b7h\u00f6rt", ",", "den", "k\u00fcr\u00b7zes\u00b7ten", "Be\u00b7griff", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Besonders da\u00df die Zeit fast ohne Frucht verlief:", "tokens": ["Be\u00b7son\u00b7ders", "da\u00df", "die", "Zeit", "fast", "oh\u00b7ne", "Frucht", "ver\u00b7lief", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "235So woll' er seine Stimm und Meinung offenbaren;", "tokens": ["woll'", "er", "sei\u00b7ne", "Stimm", "und", "Mei\u00b7nung", "of\u00b7fen\u00b7ba\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Um mehr Weitl\u00e4ufigkeit der Sache zu erspahren.", "tokens": ["Um", "mehr", "Weit\u00b7l\u00e4u\u00b7fig\u00b7keit", "der", "Sa\u00b7che", "zu", "er\u00b7spah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}