{"textgrid.poem.39277": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "Anastasius Gr\u00fcn", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel hab' ich oft im Herzen, in dem jungen,", "tokens": ["Viel", "hab'", "ich", "oft", "im", "Her\u00b7zen", ",", "in", "dem", "jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$,", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Getr\u00e4umt vom Liedermut, dem freien, wahren,", "tokens": ["Ge\u00b7tr\u00e4umt", "vom", "Lie\u00b7der\u00b7mut", ",", "dem", "frei\u00b7en", ",", "wah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,", "ART", "ADJA", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du solltest mir es herrlich offenbaren,", "tokens": ["Du", "soll\u00b7test", "mir", "es", "herr\u00b7lich", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was mir nur fern ein leiser Hauch geklungen.", "tokens": ["Was", "mir", "nur", "fern", "ein", "lei\u00b7ser", "Hauch", "ge\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die W\u00fcrde hast du mit der Kraft verschlungen,", "tokens": ["Die", "W\u00fcr\u00b7de", "hast", "du", "mit", "der", "Kraft", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die feurig hinzieht, gleich den Sonnenaaren,", "tokens": ["Die", "feu\u00b7rig", "hin\u00b7zieht", ",", "gleich", "den", "Son\u00b7ne\u00b7naa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mit dem Wort, dem ernsten, lichtvoll klaren,", "tokens": ["Und", "mit", "dem", "Wort", ",", "dem", "erns\u00b7ten", ",", "licht\u00b7voll", "kla\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hast dem Geschlecht ein Straflied du gesungen.", "tokens": ["Hast", "dem", "Ge\u00b7schlecht", "ein", "Straf\u00b7lied", "du", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dich seh' ich stehn, wie deinen letzten Ritter,", "tokens": ["Dich", "seh'", "ich", "stehn", ",", "wie", "dei\u00b7nen", "letz\u00b7ten", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Schwall der Schlaffheit, deiner selbst bewu\u00dft,", "tokens": ["Im", "Schwall", "der", "Schlaff\u00b7heit", ",", "dei\u00b7ner", "selbst", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PPOSAT", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein gr\u00fcner Stamm im grauen Nachtewitter.", "tokens": ["Ein", "gr\u00fc\u00b7ner", "Stamm", "im", "grau\u00b7en", "Nach\u00b7te\u00b7wit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ging auch das Land in tausend morsche Splitter,", "tokens": ["Ging", "auch", "das", "Land", "in", "tau\u00b7send", "mor\u00b7sche", "Split\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr das der Ton scholl deiner deutschen Brust,", "tokens": ["F\u00fcr", "das", "der", "Ton", "scholl", "dei\u00b7ner", "deut\u00b7schen", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist Gr\u00fcn die Farbe doch der Hoffnungslust.", "tokens": ["Ist", "Gr\u00fcn", "die", "Far\u00b7be", "doch", "der", "Hoff\u00b7nungs\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}