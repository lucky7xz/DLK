{"dta.poem.12794": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf denselben.  \n D. M. B.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Brich, winter!", "tokens": ["Brich", ",", "win\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Zerschmeltze schnee und ei\u00df,", "tokens": ["Zer\u00b7schmelt\u00b7ze", "schnee", "und", "ei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf da\u00df der erden-krei\u00df", "tokens": ["Auf", "da\u00df", "der", "er\u00b7den\u00b7krei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sich nicht bey rauhen tagen", "tokens": ["Sich", "nicht", "bey", "rau\u00b7hen", "ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Darff \u00fcber dich beklagen.", "tokens": ["Darff", "\u00fc\u00b7ber", "dich", "be\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Brich, winter!", "tokens": ["Brich", ",", "win\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Lacht, felder!", "tokens": ["Lacht", ",", "fel\u00b7der", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "In einer gr\u00fcnen pracht,", "tokens": ["In", "ei\u00b7ner", "gr\u00fc\u00b7nen", "pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die alles lustig macht,", "tokens": ["Die", "al\u00b7les", "lus\u00b7tig", "macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Tragt rosen und violen,", "tokens": ["Tragt", "ro\u00b7sen", "und", "vi\u00b7o\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir m\u00fcssen sie itzt holen.", "tokens": ["Wir", "m\u00fcs\u00b7sen", "sie", "itzt", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Lacht, felder!", "tokens": ["Lacht", ",", "fel\u00b7der", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Holt, Musen!", "tokens": ["Holt", ",", "Mu\u00b7sen", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Der sch\u00f6nsten blumen glantz,", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "blu\u00b7men", "glantz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und windet einen crantz,", "tokens": ["Und", "win\u00b7det", "ei\u00b7nen", "crantz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Denselben zu beehren,", "tokens": ["Den\u00b7sel\u00b7ben", "zu", "be\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der euch pflegt stets zu h\u00f6ren.", "tokens": ["Der", "euch", "pflegt", "stets", "zu", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Holt, Musen!", "tokens": ["Holt", ",", "Mu\u00b7sen", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Erfrischet", "tokens": ["Er\u00b7fri\u00b7schet"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Sein h\u00f6chst-best\u00fcrtztes haupt,", "tokens": ["Sein", "h\u00f6chst\u00b7be\u00b7st\u00fcrtz\u00b7tes", "haupt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem alle lust geraubt", "tokens": ["Dem", "al\u00b7le", "lust", "ge\u00b7raubt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bey \u00fcberh\u00e4ufften sorgen,", "tokens": ["Bey", "\u00fc\u00b7berh\u00b7\u00e4uff\u00b7ten", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durch kummer und durch borgen.", "tokens": ["Durch", "kum\u00b7mer", "und", "durch", "bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erfrischet!", "tokens": ["Er\u00b7fri\u00b7schet", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Gel\u00fccke!", "tokens": ["Ge\u00b7l\u00fc\u00b7cke", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Wirff deinen reichen klos", "tokens": ["Wirff", "dei\u00b7nen", "rei\u00b7chen", "klos"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In seine leere schoos,", "tokens": ["In", "sei\u00b7ne", "lee\u00b7re", "schoos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und la\u00df ihn einst erwarmen,", "tokens": ["Und", "la\u00df", "ihn", "einst", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In deinen goldnen armen.", "tokens": ["In", "dei\u00b7nen", "gold\u00b7nen", "ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gel\u00fccke!", "tokens": ["Ge\u00b7l\u00fc\u00b7cke", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.6": {"line.1": {"text": "Erblasse,", "tokens": ["Er\u00b7blas\u00b7se", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Du molchen-gelber neid!", "tokens": ["Du", "mol\u00b7chen\u00b7gel\u00b7ber", "neid", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der du ihn lange zeit", "tokens": ["Der", "du", "ihn", "lan\u00b7ge", "zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit giffte so bespr\u00fctzet,", "tokens": ["Mit", "giff\u00b7te", "so", "be\u00b7spr\u00fct\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df er dar\u00fcber schwitzet.", "tokens": ["Da\u00df", "er", "da\u00b7r\u00fc\u00b7ber", "schwit\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Erblasse!", "tokens": ["Er\u00b7blas\u00b7se", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "Besch\u00fctze,", "tokens": ["Be\u00b7sch\u00fct\u00b7ze", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "O tugend! deinen Sohn", "tokens": ["O", "tu\u00b7gend", "!", "dei\u00b7nen", "Sohn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor falschem spott und hohn!", "tokens": ["Vor", "fal\u00b7schem", "spott", "und", "hohn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dem b\u00f6se zungen dichten", "tokens": ["Dem", "b\u00f6\u00b7se", "zun\u00b7gen", "dich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die ehre zu vernichten,", "tokens": ["Die", "eh\u00b7re", "zu", "ver\u00b7nich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Besch\u00fctze!", "tokens": ["Be\u00b7sch\u00fct\u00b7ze", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.8": {"line.1": {"text": "O sonne!", "tokens": ["O", "son\u00b7ne", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Bescheine diese zeit", "tokens": ["Be\u00b7schei\u00b7ne", "die\u00b7se", "zeit"], "token_info": ["word", "word", "word"], "pos": ["NN", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit lust und fr\u00f6ligkeit,", "tokens": ["Mit", "lust", "und", "fr\u00f6\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zutreibe keil und blitzen,", "tokens": ["Zu\u00b7trei\u00b7be", "keil", "und", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die sich auf sie erhitzen.", "tokens": ["Die", "sich", "auf", "sie", "er\u00b7hit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "O sonne!", "tokens": ["O", "son\u00b7ne", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.9": {"line.1": {"text": "Bringt lauten!", "tokens": ["Bringt", "lau\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Und spielt dem matten ohr", "tokens": ["Und", "spielt", "dem", "mat\u00b7ten", "ohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die frohen w\u00f6rter vor:", "tokens": ["Die", "fro\u00b7hen", "w\u00f6r\u00b7ter", "vor", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dis licht soll sich erkl\u00e4ren,", "tokens": ["Dis", "licht", "soll", "sich", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und lange jahre w\u00e4hren.", "tokens": ["Und", "lan\u00b7ge", "jah\u00b7re", "w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bringt lauten!", "tokens": ["Bringt", "lau\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.10": {"line.1": {"text": "Ach himmel!", "tokens": ["Ach", "him\u00b7mel", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Erhalte diesen Freund,", "tokens": ["Er\u00b7hal\u00b7te", "die\u00b7sen", "Freund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der es rechtschaffen meynt,", "tokens": ["Der", "es", "recht\u00b7schaf\u00b7fen", "meynt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vermehre seine kr\u00e4ffte,", "tokens": ["Ver\u00b7meh\u00b7re", "sei\u00b7ne", "kr\u00e4ff\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durch neue lebens-s\u00e4ffte,", "tokens": ["Durch", "neu\u00b7e", "le\u00b7bens\u00b7s\u00e4ff\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ach himmel!", "tokens": ["Ach", "him\u00b7mel", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.11": {"line.1": {"text": "So lebe,", "tokens": ["So", "le\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Des schwachen hauses zier!", "tokens": ["Des", "schwa\u00b7chen", "hau\u00b7ses", "zier", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und gr\u00fcne f\u00fcr und f\u00fcr,", "tokens": ["Und", "gr\u00fc\u00b7ne", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df sich von deinen zweigen", "tokens": ["Da\u00df", "sich", "von", "dei\u00b7nen", "zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Noch gro\u00dfe st\u00e4mme zeigen.", "tokens": ["Noch", "gro\u00b7\u00dfe", "st\u00e4m\u00b7me", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So lebe.", "tokens": ["So", "le\u00b7be", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.12": {"line.1": {"text": "Drum gehe!", "tokens": ["Drum", "ge\u00b7he", "!"], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Aus deinem lazareth,", "tokens": ["Aus", "dei\u00b7nem", "la\u00b7za\u00b7reth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um das ein dornbusch steht,", "tokens": ["Um", "das", "ein", "dorn\u00b7busch", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du wirst auf unsern auen", "tokens": ["Du", "wirst", "auf", "un\u00b7sern", "au\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das rosenthal erst bauen.", "tokens": ["Das", "ro\u00b7sen\u00b7thal", "erst", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Drum gehe!", "tokens": ["Drum", "ge\u00b7he", "!"], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}