{"textgrid.poem.34414": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kein Tag des Leides noch der Freude flieht,", "genre": "verse", "period": "N.A.", "pub_year": 1885, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Tag des Leides noch der Freude flieht,", "tokens": ["Kein", "Tag", "des", "Lei\u00b7des", "noch", "der", "Freu\u00b7de", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "dass ich nicht deiner, der verlornen, d\u00e4chte.", "tokens": ["dass", "ich", "nicht", "dei\u00b7ner", ",", "der", "ver\u00b7lor\u00b7nen", ",", "d\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein Bild, das vor die frohen Sinne zieht", "tokens": ["Kein", "Bild", ",", "das", "vor", "die", "fro\u00b7hen", "Sin\u00b7ne", "zieht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und nicht dein zartes Bild zur\u00fcck mir br\u00e4chte.", "tokens": ["und", "nicht", "dein", "zar\u00b7tes", "Bild", "zu\u00b7r\u00fcck", "mir", "br\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PPOSAT", "ADJA", "NN", "PTKVZ", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit hellem Auge gr\u00fcsstest du den Tod", "tokens": ["Mit", "hel\u00b7lem", "Au\u00b7ge", "gr\u00fcs\u00b7stest", "du", "den", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und voll Verachtung bist du hingesunken,", "tokens": ["und", "voll", "Ver\u00b7ach\u00b7tung", "bist", "du", "hin\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verachtung f\u00fcr der Menschen Zwanggebot,", "tokens": ["Ver\u00b7ach\u00b7tung", "f\u00fcr", "der", "Men\u00b7schen", "Zwang\u00b7ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verachtung f\u00fcr den Kelch, den du getrunken.", "tokens": ["Ver\u00b7ach\u00b7tung", "f\u00fcr", "den", "Kelch", ",", "den", "du", "ge\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Im Kampf des Lebens standest du enterbt", "tokens": ["Im", "Kampf", "des", "Le\u00b7bens", "stan\u00b7dest", "du", "en\u00b7terbt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und rauh erfasst schon in den Kindesjahren,", "tokens": ["und", "rauh", "er\u00b7fasst", "schon", "in", "den", "Kin\u00b7des\u00b7jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "des Lenzes Knospen, fr\u00fch vom Frost verderbt \u2013", "tokens": ["des", "Len\u00b7zes", "Knos\u00b7pen", ",", "fr\u00fch", "vom", "Frost", "ver\u00b7derbt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADJD", "APPRART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und noch im Sterben hast du Neid erfahren.", "tokens": ["und", "noch", "im", "Ster\u00b7ben", "hast", "du", "Neid", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VAFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mit hellem Auge gr\u00fcsstest du den Tod,", "tokens": ["Mit", "hel\u00b7lem", "Au\u00b7ge", "gr\u00fcs\u00b7stest", "du", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und klar und fest ist deine Stirn geblieben.", "tokens": ["und", "klar", "und", "fest", "ist", "dei\u00b7ne", "Stirn", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wem je dein Blick den stillen Gruss entbot,", "tokens": ["Wem", "je", "dein", "Blick", "den", "stil\u00b7len", "Gruss", "ent\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "dem bleibt er immerdar ins Herz geschrieben.", "tokens": ["dem", "bleibt", "er", "im\u00b7mer\u00b7dar", "ins", "Herz", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}