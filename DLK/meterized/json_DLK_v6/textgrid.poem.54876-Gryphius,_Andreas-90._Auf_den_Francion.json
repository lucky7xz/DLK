{"textgrid.poem.54876": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "90. Auf den Francion", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man hat vor dem sehr offt dich Huren sehn verkauffen/", "tokens": ["Man", "hat", "vor", "dem", "sehr", "offt", "dich", "Hu\u00b7ren", "sehn", "ver\u00b7kauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ART", "ADV", "ADV", "PPER", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Itzt pflegst du als ein Artzt gro\u00df sprechend umbzulauffen;", "tokens": ["Itzt", "pflegst", "du", "als", "ein", "Artzt", "gro\u00df", "spre\u00b7chend", "umb\u00b7zu\u00b7lauf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "ART", "NN", "ADJD", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du hast dich wohl bedacht des Handels unternommen:", "tokens": ["Du", "hast", "dich", "wohl", "be\u00b7dacht", "des", "Han\u00b7dels", "un\u00b7ter\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Damit du k\u00f6ntest was zu heilen \u00fcberkommen.", "tokens": ["Da\u00b7mit", "du", "k\u00f6n\u00b7test", "was", "zu", "hei\u00b7len", "\u00fc\u00b7ber\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "PIS", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}