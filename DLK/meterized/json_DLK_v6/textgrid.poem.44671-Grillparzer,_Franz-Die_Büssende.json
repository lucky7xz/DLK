{"textgrid.poem.44671": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Die B\u00fcssende", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der frommen Bu\u00dfe Dauer zu vermehren \u2013", "tokens": ["Der", "from\u00b7men", "Bu\u00b7\u00dfe", "Dau\u00b7er", "zu", "ver\u00b7meh\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie einst Penelope im Freier-Hauf \u2013", "tokens": ["Wie", "einst", "Pe\u00b7ne\u00b7lo\u00b7pe", "im", "Frei\u00b7er\u00b7Hauf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NE", "APPRART", "NN", "$("], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Was du bei Tag erwirkt an Kirchen und Alt\u00e4ren,", "tokens": ["Was", "du", "bei", "Tag", "er\u00b7wirkt", "an", "Kir\u00b7chen", "und", "Al\u00b7t\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Trennst du bei Nacht geduldig wieder auf.", "tokens": ["Trennst", "du", "bei", "Nacht", "ge\u00b7dul\u00b7dig", "wie\u00b7der", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}