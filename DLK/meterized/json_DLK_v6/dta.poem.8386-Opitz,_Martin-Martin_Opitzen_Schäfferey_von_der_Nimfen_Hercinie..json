{"dta.poem.8386": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Martin Opitzen Sch\u00e4fferey  \n von der Nimfen Hercinie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1630", "urn": "urn:nbn:de:kobv:b4-200905197863", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Sch\u00e4fferey Von der Nimfen Hercinie. Breslau, 1630."}, "poem": {"stanza.1": {"line.1": {"text": "Vngrad ist den g\u00f6ttern lieb; dreymal ist er auch gebunden;", "tokens": ["Vn\u00b7grad", "ist", "den", "g\u00f6t\u00b7tern", "lieb", ";", "drey\u00b7mal", "ist", "er", "auch", "ge\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "ADJD", "$.", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Dreyer farben faden sindt vmb den harten hal\u00df gewunden.", "tokens": ["Drey\u00b7er", "far\u00b7ben", "fa\u00b7den", "sindt", "vmb", "den", "har\u00b7ten", "hal\u00df", "ge\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "VVINF", "VAFIN", "APPR", "ART", "ADJA", "NN", "VAPP", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}