{"textgrid.poem.31386": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Komm sch\u00f6nster Seraphin / ber\u00fchre meinen Mund!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm sch\u00f6nster Seraphin / ber\u00fchre meinen Mund!", "tokens": ["Komm", "sch\u00f6ns\u00b7ter", "Se\u00b7ra\u00b7phin", "/", "be\u00b7r\u00fch\u00b7re", "mei\u00b7nen", "Mund", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$(", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mich woll der Flammen-Flu\u00df / die Gottes wei\u00dfheit / tr\u00e4nken:", "tokens": ["mich", "woll", "der", "Flam\u00b7men\u00b7Flu\u00df", "/", "die", "Got\u00b7tes", "wei\u00df\u00b7heit", "/", "tr\u00e4n\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$(", "ART", "NN", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df ich was w\u00fcrdigs kan zu seinem Lob erdenken /", "tokens": ["da\u00df", "ich", "was", "w\u00fcr\u00b7digs", "kan", "zu", "sei\u00b7nem", "Lob", "er\u00b7den\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VAFIN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df alle Lieblichkeit mich netze diese Stund.", "tokens": ["da\u00df", "al\u00b7le", "Lieb\u00b7lich\u00b7keit", "mich", "net\u00b7ze", "die\u00b7se", "Stund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "auf da\u00df das h\u00f6chste Gut aufs h\u00f6chst' ich prei\u00dfen kund /", "tokens": ["auf", "da\u00df", "das", "h\u00f6chs\u00b7te", "Gut", "aufs", "h\u00f6chst'", "ich", "prei\u00b7\u00dfen", "kund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN", "APPRART", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wollst / H\u00f6chster / Safft und Krafft / Geist / Witz und Blitz mir schenken.", "tokens": ["wollst", "/", "H\u00f6chs\u00b7ter", "/", "Safft", "und", "Krafft", "/", "Geist", "/", "Witz", "und", "Blitz", "mir", "schen\u00b7ken", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$(", "NN", "$(", "NN", "KON", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich will mich in die See der Gnaden M\u00e4nge senken /", "tokens": ["Ich", "will", "mich", "in", "die", "See", "der", "Gna\u00b7den", "M\u00e4n\u00b7ge", "sen\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "weil in der Gottheits-Sonn' ich doch zerschmelzung fund'.", "tokens": ["weil", "in", "der", "Gott\u00b7heits\u00b7Sonn'", "ich", "doch", "zer\u00b7schmel\u00b7zung", "fund'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach aller Ehren Zweck! la\u00df mich mein Ziel erreichen /", "tokens": ["Ach", "al\u00b7ler", "Eh\u00b7ren", "Zweck", "!", "la\u00df", "mich", "mein", "Ziel", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PIAT", "NN", "NN", "$.", "VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "dein Lob! ich lebe nur / wann dieses in mir lebt.", "tokens": ["dein", "Lob", "!", "ich", "le\u00b7be", "nur", "/", "wann", "die\u00b7ses", "in", "mir", "lebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "$(", "PWAV", "PDAT", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "la\u00df mein dich prei\u00dfend Werk der Pharus Fackel gleichen /", "tokens": ["la\u00df", "mein", "dich", "prei\u00b7\u00dfend", "Werk", "der", "Pha\u00b7rus", "Fa\u00b7ckel", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "PRF", "ADJD", "NN", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "die schiffend auf der Flut man herrlichst siht erhebt:", "tokens": ["die", "schif\u00b7fend", "auf", "der", "Flut", "man", "herr\u00b7lichst", "siht", "er\u00b7hebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "PIS", "ADV", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "da\u00df in der Schn\u00f6dheit / ich mach deinen Ruhm erschallen", "tokens": ["da\u00df", "in", "der", "Schn\u00f6d\u00b7heit", "/", "ich", "mach", "dei\u00b7nen", "Ruhm", "er\u00b7schal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "mit Herzen / Mund / und Hand / ja kurz / in-und mit allen.", "tokens": ["mit", "Her\u00b7zen", "/", "Mund", "/", "und", "Hand", "/", "ja", "kurz", "/", "in\u00b7\u00b7und", "mit", "al\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "NN", "$(", "KON", "NN", "$(", "ADV", "ADJD", "$(", "NE", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}