{"dta.poem.19115": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XcVII.  Henochs Leben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Vor seinen Augen schweben", "tokens": ["Vor", "sei\u00b7nen", "Au\u00b7gen", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist wahre Seeligkeit;", "tokens": ["Ist", "wah\u00b7re", "See\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein unverrucktes Leben", "tokens": ["Ein", "un\u00b7ver\u00b7ruck\u00b7tes", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Eingesunckenheit:", "tokens": ["In", "Ein\u00b7ge\u00b7sun\u00b7cken\u00b7heit", ":"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nichts k\u00f6nnen und nichts wissen,", "tokens": ["Nichts", "k\u00f6n\u00b7nen", "und", "nichts", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "KON", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nichts wollen und nichts thun,", "tokens": ["Nichts", "wol\u00b7len", "und", "nichts", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "KON", "PIS", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als JEsu folgen m\u00fcssen,", "tokens": ["Als", "Je\u00b7su", "fol\u00b7gen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das heist im Friede ruhn.", "tokens": ["Das", "heist", "im", "Frie\u00b7de", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Man steht von seinem Schlafe", "tokens": ["Man", "steht", "von", "sei\u00b7nem", "Schla\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Christi Freundschafft auf;", "tokens": ["In", "Chris\u00b7ti", "Freund\u00b7schafft", "auf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man f\u00fcrchtet keine Strafe", "tokens": ["Man", "f\u00fcrch\u00b7tet", "kei\u00b7ne", "Stra\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jm gantzen Lebens-Lauff;", "tokens": ["Jm", "gant\u00b7zen", "Le\u00b7bens\u00b7Lauff", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Man i\u00dft und trinckt in Liebe,", "tokens": ["Man", "i\u00dft", "und", "trinckt", "in", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Man hungerte wohl auch:", "tokens": ["Man", "hun\u00b7ger\u00b7te", "wohl", "auch", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Man h\u00e4lt im Gnaden-Triebe", "tokens": ["Man", "h\u00e4lt", "im", "Gna\u00b7den\u00b7Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Best\u00e4ndig einen Brauch.", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "ei\u00b7nen", "Brauch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn man den Tag vollendet,", "tokens": ["Wenn", "man", "den", "Tag", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So legt man sich zu Ruh,", "tokens": ["So", "legt", "man", "sich", "zu", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Christo unverwendet", "tokens": ["Von", "Chris\u00b7to", "un\u00b7ver\u00b7wen\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Thut man die Sinnen zu;", "tokens": ["Thut", "man", "die", "Sin\u00b7nen", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wei\u00df auch denen Tr\u00e4umen,", "tokens": ["Und", "wei\u00df", "auch", "de\u00b7nen", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PDS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenns ja getr\u00e4umt soll seyn,", "tokens": ["Wenns", "ja", "ge\u00b7tr\u00e4umt", "soll", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVPP", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nichts anders einzur\u00e4umen,", "tokens": ["Nichts", "an\u00b7ders", "ein\u00b7zu\u00b7r\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als Christi Wiederschein.", "tokens": ["Als", "Chris\u00b7ti", "Wie\u00b7der\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Man geht in einer Fassung", "tokens": ["Man", "geht", "in", "ei\u00b7ner", "Fas\u00b7sung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dahin bey Tag und Nacht,", "tokens": ["Da\u00b7hin", "bey", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ist auf die Verlassung", "tokens": ["Und", "ist", "auf", "die", "Ver\u00b7las\u00b7sung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der gantzen Welt bedacht:", "tokens": ["Der", "gant\u00b7zen", "Welt", "be\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Man h\u00f6rt, und sieht, und f\u00fchlet,", "tokens": ["Man", "h\u00f6rt", ",", "und", "sieht", ",", "und", "f\u00fch\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "H\u00f6rt, sieht und f\u00fchlt doch nicht;", "tokens": ["H\u00f6rt", ",", "sieht", "und", "f\u00fchlt", "doch", "nicht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wenn uns Schmertz durchw\u00fchlet,", "tokens": ["Und", "wenn", "uns", "Schmertz", "durch\u00b7w\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wei\u00df man nicht, was geschicht.", "tokens": ["Wei\u00df", "man", "nicht", ",", "was", "ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "$,", "PRELS", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Gewi\u00df, wer erst die S\u00fcnde", "tokens": ["Ge\u00b7wi\u00df", ",", "wer", "erst", "die", "S\u00fcn\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PWS", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Christi Blut ertr\u00e4nckt,", "tokens": ["In", "Chris\u00b7ti", "Blut", "er\u00b7tr\u00e4nckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hurtig und geschwinde", "tokens": ["Und", "hur\u00b7tig", "und", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf JEsum zugelenckt;", "tokens": ["Auf", "Je\u00b7sum", "zu\u00b7ge\u00b7lenckt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Der kan sehr heilig handeln,", "tokens": ["Der", "kan", "sehr", "hei\u00b7lig", "han\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und kan bald anders nicht.", "tokens": ["Und", "kan", "bald", "an\u00b7ders", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Herr JEsu, lehr uns wandeln", "tokens": ["Herr", "Je\u00b7su", ",", "lehr", "uns", "wan\u00b7deln"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "VVIMP", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In deiner Augen Licht!", "tokens": ["In", "dei\u00b7ner", "Au\u00b7gen", "Licht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}