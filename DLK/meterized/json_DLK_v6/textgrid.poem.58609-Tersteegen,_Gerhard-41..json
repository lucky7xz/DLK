{"textgrid.poem.58609": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil ich so blind und dumm, will Gott mich selber lehren,", "tokens": ["Weil", "ich", "so", "blind", "und", "dumm", ",", "will", "Gott", "mich", "sel\u00b7ber", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,", "VMFIN", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ich so schwach und matt, will er mein F\u00fchrer sein;", "tokens": ["Weil", "ich", "so", "schwach", "und", "matt", ",", "will", "er", "mein", "F\u00fch\u00b7rer", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,", "VMFIN", "PPER", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein Lehren ist nicht leer, sein Wort den Geist kann n\u00e4hren,", "tokens": ["Sein", "Leh\u00b7ren", "ist", "nicht", "leer", ",", "sein", "Wort", "den", "Geist", "kann", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$,", "PPOSAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sein Leiten f\u00fchrt gerad ins wahre Leben ein.", "tokens": ["Sein", "Lei\u00b7ten", "f\u00fchrt", "ge\u00b7rad", "ins", "wah\u00b7re", "Le\u00b7ben", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Seel', merk, was Gott dich lehrt, bleib still und abgeschieden", "tokens": ["Seel'", ",", "merk", ",", "was", "Gott", "dich", "lehrt", ",", "bleib", "still", "und", "ab\u00b7ge\u00b7schie\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "PRELS", "NN", "PPER", "VVFIN", "$,", "VVFIN", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und folge wie ein Kind der treuen Leitung blo\u00df,", "tokens": ["Und", "fol\u00b7ge", "wie", "ein", "Kind", "der", "treu\u00b7en", "Lei\u00b7tung", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So wird ein sanfter Strom dir bringen tiefen Frieden", "tokens": ["So", "wird", "ein", "sanf\u00b7ter", "Strom", "dir", "brin\u00b7gen", "tie\u00b7fen", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und deine Fr\u00f6mmigkeit wird m\u00e4chtig sein und gro\u00df!", "tokens": ["Und", "dei\u00b7ne", "Fr\u00f6m\u00b7mig\u00b7keit", "wird", "m\u00e4ch\u00b7tig", "sein", "und", "gro\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "VAINF", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}