{"textgrid.poem.39296": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich rei\u00dfe mich aus der \u00fcppigen Ruh'", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich rei\u00dfe mich aus der \u00fcppigen Ruh'", "tokens": ["Ich", "rei\u00b7\u00dfe", "mich", "aus", "der", "\u00fcp\u00b7pi\u00b7gen", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und eile dem blutigen Streite zu,", "tokens": ["Und", "ei\u00b7le", "dem", "blu\u00b7ti\u00b7gen", "Strei\u00b7te", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "So handeln, wie schwelgen ich kann!", "tokens": ["So", "han\u00b7deln", ",", "wie", "schwel\u00b7gen", "ich", "kann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PWAV", "VVFIN", "PPER", "VMFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Wohl hab' ich den Becher der Lust hier geleert,", "tokens": ["Wohl", "hab'", "ich", "den", "Be\u00b7cher", "der", "Lust", "hier", "ge\u00b7leert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Und wenn dorten den Becher der Tod mir beschert,", "tokens": ["Und", "wenn", "dor\u00b7ten", "den", "Be\u00b7cher", "der", "Tod", "mir", "be\u00b7schert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Ich leere auch ihn wie ein Mann!", "tokens": ["Ich", "lee\u00b7re", "auch", "ihn", "wie", "ein", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sei klar, mein Auge, sei kalt, mein Blut,", "tokens": ["Sei", "klar", ",", "mein", "Au\u00b7ge", ",", "sei", "kalt", ",", "mein", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PPOSAT", "NN", "$,", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Soldaten, beweist den uralten Mut,", "tokens": ["Sol\u00b7da\u00b7ten", ",", "be\u00b7weist", "den", "ur\u00b7al\u00b7ten", "Mut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+----+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "La\u00dft es mannlich ergehen, frisch auf!", "tokens": ["La\u00dft", "es", "mann\u00b7lich", "er\u00b7ge\u00b7hen", ",", "frisch", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "VVINF", "$,", "ADJD", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Will schau'n in der Schlacht tiefinnersten Schlund", "tokens": ["Will", "schau'n", "in", "der", "Schlacht", "tie\u00b7fin\u00b7ners\u00b7ten", "Schlund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und jeglichem Feind will ich geben vom Grund", "tokens": ["Und", "jeg\u00b7li\u00b7chem", "Feind", "will", "ich", "ge\u00b7ben", "vom", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Drei Ellen, zu fallen darauf.", "tokens": ["Drei", "El\u00b7len", ",", "zu", "fal\u00b7len", "da\u00b7rauf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PTKZU", "VVINF", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "K\u00f6nig Gustav der Gro\u00dfe, er f\u00fchrt uns nicht mehr,", "tokens": ["K\u00f6\u00b7nig", "Gus\u00b7tav", "der", "Gro\u00b7\u00dfe", ",", "er", "f\u00fchrt", "uns", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "ADJA", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Doch wieder vom Himmel schaut wachend er", "tokens": ["Doch", "wie\u00b7der", "vom", "Him\u00b7mel", "schaut", "wa\u00b7chend", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "ADJD", "PPER"], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auf den Streit, wie wir halten Stand;", "tokens": ["Auf", "den", "Streit", ",", "wie", "wir", "hal\u00b7ten", "Stand", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Noch herrscht in der Welt seine m\u00e4chtige Seel'", "tokens": ["Noch", "herrscht", "in", "der", "Welt", "sei\u00b7ne", "m\u00e4ch\u00b7ti\u00b7ge", "Seel'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Und siegt wie vordem. Noch sieht er nicht scheel", "tokens": ["Und", "siegt", "wie", "vor\u00b7dem", ".", "Noch", "sieht", "er", "nicht", "scheel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ADV", "$.", "ADV", "VVFIN", "PPER", "PTKNEG", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Auf uns und das Vaterland.", "tokens": ["Auf", "uns", "und", "das", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sein Ged\u00e4chtnis, wir feiern's mit Kugelmusik,", "tokens": ["Sein", "Ge\u00b7d\u00e4cht\u00b7nis", ",", "wir", "fei\u00b7ern's", "mit", "Ku\u00b7gel\u00b7mu\u00b7sik", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Sein Grabmal, wir bauen's mit Leichen dick", "tokens": ["Sein", "Grab\u00b7mal", ",", "wir", "bau\u00b7en's", "mit", "Lei\u00b7chen", "dick"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "ADJD"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im brennenden Sturm und Streit.", "tokens": ["Im", "bren\u00b7nen\u00b7den", "Sturm", "und", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jeder Feind, der gef\u00e4llt von dem r\u00e4chenden Schwert,", "tokens": ["Je\u00b7der", "Feind", ",", "der", "ge\u00b7f\u00e4llt", "von", "dem", "r\u00e4\u00b7chen\u00b7den", "Schwert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Ist nicht mehr als ein Haar seines Hauptes wert,", "tokens": ["Ist", "nicht", "mehr", "als", "ein", "Haar", "sei\u00b7nes", "Haup\u00b7tes", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "KOKOM", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Um so Mehre erschlagen wir heut'!", "tokens": ["Um", "so", "Meh\u00b7re", "er\u00b7schla\u00b7gen", "wir", "heut'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "PIS", "VVFIN", "PPER", "ADV", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.5": {"line.1": {"text": "Flieg', Fahne, flieg' unter den Wolken k\u00fchn,", "tokens": ["Flieg'", ",", "Fah\u00b7ne", ",", "flieg'", "un\u00b7ter", "den", "Wol\u00b7ken", "k\u00fchn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Voran magst Du schweben, Du kannst nicht fliehn.", "tokens": ["Vo\u00b7ran", "magst", "Du", "schwe\u00b7ben", ",", "Du", "kannst", "nicht", "fliehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Flieg' hoch, da\u00df wir schauen nach Dir!", "tokens": ["Flieg'", "hoch", ",", "da\u00df", "wir", "schau\u00b7en", "nach", "Dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "In der Sonne la\u00df leuchten die Kronen Dein,", "tokens": ["In", "der", "Son\u00b7ne", "la\u00df", "leuch\u00b7ten", "die", "Kro\u00b7nen", "Dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVIMP", "VVFIN", "ART", "NN", "PPOSAT", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Unsre Burg, unser K\u00f6nigreich sollst Du sein,", "tokens": ["Uns\u00b7re", "Burg", ",", "un\u00b7ser", "K\u00f6\u00b7nig\u00b7reich", "sollst", "Du", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VMFIN", "PPER", "VAINF", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Hochstrahlendes Schwedenpanier!", "tokens": ["Hoch\u00b7strah\u00b7len\u00b7des", "Schwe\u00b7den\u00b7pa\u00b7nier", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}