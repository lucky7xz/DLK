{"textgrid.poem.41191": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht wolkennahe Th\u00fcrme mit jeder Kunst,", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht wolkennahe Th\u00fcrme mit jeder Kunst,", "tokens": ["Nicht", "wol\u00b7ken\u00b7na\u00b7he", "Th\u00fcr\u00b7me", "mit", "je\u00b7der", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Menschenh\u00e4nde tr\u00e4chtig, den Aufenthalt", "tokens": ["Der", "Men\u00b7schen\u00b7h\u00e4n\u00b7de", "tr\u00e4ch\u00b7tig", ",", "den", "Auf\u00b7ent\u00b7halt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Erdeg\u00f6tter, nicht den Erbstuhl", "tokens": ["Der", "Er\u00b7de\u00b7g\u00f6t\u00b7ter", ",", "nicht", "den", "Erb\u00b7stuhl"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "M\u00e4chtiger Herrscher in Gold gekleidet,", "tokens": ["M\u00e4ch\u00b7ti\u00b7ger", "Herr\u00b7scher", "in", "Gold", "ge\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Um ihn gebeugt der dienenden V\u00f6lker Welt;", "tokens": ["Um", "ihn", "ge\u00b7beugt", "der", "die\u00b7nen\u00b7den", "V\u00f6l\u00b7ker", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auch nicht den Herrscher selber von ihm erh\u00f6ht,", "tokens": ["Auch", "nicht", "den", "Herr\u00b7scher", "sel\u00b7ber", "von", "ihm", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Herrschaft Donner in der Rechten,", "tokens": ["Der", "Herr\u00b7schaft", "Don\u00b7ner", "in", "der", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sing' ich in eichenumlaubte Saiten;", "tokens": ["Sing'", "ich", "in", "ei\u00b7chen\u00b7um\u00b7laub\u00b7te", "Sai\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Auch nicht den Ruhm des Herrschers in Ost und West", "tokens": ["Auch", "nicht", "den", "Ruhm", "des", "Herr\u00b7schers", "in", "Ost", "und", "West"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ART", "NN", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und S\u00fcd und Norden; w\u00e4r' er erworben in", "tokens": ["Und", "S\u00fcd", "und", "Nor\u00b7den", ";", "w\u00e4r'", "er", "er\u00b7wor\u00b7ben", "in"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "$.", "VAFIN", "PPER", "VVPP", "APPR"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gefahrenvollen Eisenfeldern,", "tokens": ["Ge\u00b7fah\u00b7ren\u00b7vol\u00b7len", "Ei\u00b7sen\u00b7fel\u00b7dern", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Oder in friedlichen Segensfluren.", "tokens": ["O\u00b7der", "in", "fried\u00b7li\u00b7chen", "Se\u00b7gens\u00b7flu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Auch die\u00df ist Bardenarbeit. Allein wie schnell", "tokens": ["Auch", "die\u00df", "ist", "Bar\u00b7den\u00b7ar\u00b7beit", ".", "Al\u00b7lein", "wie", "schnell"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PDS", "VAFIN", "NN", "$.", "ADV", "KOKOM", "ADJD"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verbl\u00fcht ein Erderzeugter, sein Ruhm mit ihm!", "tokens": ["Ver\u00b7bl\u00fcht", "ein", "E\u00b7rder\u00b7zeug\u00b7ter", ",", "sein", "Ruhm", "mit", "ihm", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und sind nicht Herrscher Erderzeugte?", "tokens": ["Und", "sind", "nicht", "Herr\u00b7scher", "E\u00b7rder\u00b7zeug\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Str\u00f6men die Jahre sie nicht von hinnen?", "tokens": ["Str\u00f6\u00b7men", "die", "Jah\u00b7re", "sie", "nicht", "von", "hin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "PTKNEG", "APPR", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Ich singe G\u00fcter sicher der Ewigkeit,", "tokens": ["Ich", "sin\u00b7ge", "G\u00fc\u00b7ter", "si\u00b7cher", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf sich gegr\u00fcndet, \u00fcber den Unbestand", "tokens": ["Auf", "sich", "ge\u00b7gr\u00fcn\u00b7det", ",", "\u00fc\u00b7ber", "den", "Un\u00b7be\u00b7stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRF", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Der Lebensgr\u00f6\u00dfen weit erhoben,", "tokens": ["Der", "Le\u00b7bens\u00b7gr\u00f6\u00b7\u00dfen", "weit", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weit, wie die sichtbare Welt hin\u00fcber", "tokens": ["Weit", ",", "wie", "die", "sicht\u00b7ba\u00b7re", "Welt", "hin\u00b7\u00fc\u00b7ber"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "ADV"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Der Sitz Allvaters. Seele Theresien's", "tokens": ["Der", "Sitz", "All\u00b7va\u00b7ters", ".", "See\u00b7le", "The\u00b7re\u00b7si\u00b7en's"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NE", "$.", "NN", "NE"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Erf\u00fcllt mit diesen G\u00fctern! und w\u00e4rest du", "tokens": ["Er\u00b7f\u00fcllt", "mit", "die\u00b7sen", "G\u00fc\u00b7tern", "!", "und", "w\u00e4\u00b7rest", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "$.", "KON", "VAFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auch keiner F\u00fcrstinn Seele, dennoch", "tokens": ["Auch", "kei\u00b7ner", "F\u00fcrs\u00b7tinn", "See\u00b7le", ",", "den\u00b7noch"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PIAT", "NN", "NN", "$,", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wunderbar, herrlich und liederw\u00fcrdig!", "tokens": ["Wun\u00b7der\u00b7bar", ",", "herr\u00b7lich", "und", "lie\u00b7der\u00b7w\u00fcr\u00b7dig", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Dich, dich besing' ich! H\u00f6ret mein Feierlied,", "tokens": ["Dich", ",", "dich", "be\u00b7sing'", "ich", "!", "H\u00f6\u00b7ret", "mein", "Fei\u00b7er\u00b7lied", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VVFIN", "PPER", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Allvaters Boten!", "tokens": ["All\u00b7va\u00b7ters", "Bo\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Umschwebet, wenn des Reiches Hauptschmuck,", "tokens": ["Um\u00b7schwe\u00b7bet", ",", "wenn", "des", "Rei\u00b7ches", "Haupt\u00b7schmuck", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Ihr der goldene Stab der Herrschaft", "tokens": ["Wenn", "Ihr", "der", "gol\u00b7de\u00b7ne", "Stab", "der", "Herr\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Vor ihm entsinket, wenn Sie vor ihm, die Brust", "tokens": ["Vor", "ihm", "ent\u00b7sin\u00b7ket", ",", "wenn", "Sie", "vor", "ihm", ",", "die", "Brust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PPER", "$,", "ART", "NN"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Voll Unterwerfung, treuer Erkenntni\u00df voll,", "tokens": ["Voll", "Un\u00b7ter\u00b7wer\u00b7fung", ",", "treu\u00b7er", "Er\u00b7kennt\u00b7ni\u00df", "voll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Gluth des Eifers auf den Wangen,", "tokens": ["Die", "Gluth", "des", "Ei\u00b7fers", "auf", "den", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Himmel im Auge, die Kniee beuget.", "tokens": ["Him\u00b7mel", "im", "Au\u00b7ge", ",", "die", "Kni\u00b7ee", "beu\u00b7get", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Dann steht der Herrschersorgen entfernter Schwarm", "tokens": ["Dann", "steht", "der", "Herr\u00b7scher\u00b7sor\u00b7gen", "ent\u00b7fern\u00b7ter", "Schwarm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Finger auf dem Munde, beflattert nicht", "tokens": ["Den", "Fin\u00b7ger", "auf", "dem", "Mun\u00b7de", ",", "be\u00b7flat\u00b7tert", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "VVFIN", "PTKNEG"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der feierlichen Stille Lichtkreis,", "tokens": ["Der", "fei\u00b7er\u00b7li\u00b7chen", "Stil\u00b7le", "Licht\u00b7kreis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welcher die betende F\u00fcrstinn einschleu\u00dft.", "tokens": ["Wel\u00b7cher", "die", "be\u00b7ten\u00b7de", "F\u00fcrs\u00b7tinn", "ein\u00b7schleu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.10": {"line.1": {"text": "Dann geu\u00dft Allvater \u00fcber die reinste", "tokens": ["Dann", "geu\u00dft", "All\u00b7va\u00b7ter", "\u00fc\u00b7ber", "die", "reins\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Seelen ganz sein v\u00e4terlich Herz herab,", "tokens": ["Der", "See\u00b7len", "ganz", "sein", "v\u00e4\u00b7ter\u00b7lich", "Herz", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "ADJD", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und jedes Heil und jeder Segen", "tokens": ["Und", "je\u00b7des", "Heil", "und", "je\u00b7der", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird Ihr im reichesten Ueberflu\u00dfe.", "tokens": ["Wird", "Ihr", "im", "rei\u00b7ches\u00b7ten", "Ue\u00b7berf\u00b7lu\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Und jede Tugend sprie\u00dfet in Ihr empor", "tokens": ["Und", "je\u00b7de", "Tu\u00b7gend", "sprie\u00b7\u00dfet", "in", "Ihr", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vom Himmelthaue trunken, und breitet weit", "tokens": ["Vom", "Him\u00b7melt\u00b7hau\u00b7e", "trun\u00b7ken", ",", "und", "brei\u00b7tet", "weit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "$,", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von Fr\u00fcchten schwer behang'ne Schatten,", "tokens": ["Von", "Fr\u00fcch\u00b7ten", "schwer", "be\u00b7hang'\u00b7ne", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schatten, in welchen die L\u00e4nder wohnen.", "tokens": ["Schat\u00b7ten", ",", "in", "wel\u00b7chen", "die", "L\u00e4n\u00b7der", "woh\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PWAT", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Da lernet Sie der zeitlichen Gr\u00f6\u00dfe Werth,", "tokens": ["Da", "ler\u00b7net", "Sie", "der", "zeit\u00b7li\u00b7chen", "Gr\u00f6\u00b7\u00dfe", "Werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Zweck der F\u00fcrstinn, Mutter des Volks zu seyn,", "tokens": ["Den", "Zweck", "der", "F\u00fcrs\u00b7tinn", ",", "Mut\u00b7ter", "des", "Volks", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "NN", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihr ungemessenes Verm\u00f6gen", "tokens": ["Ihr", "un\u00b7ge\u00b7mes\u00b7se\u00b7nes", "Ver\u00b7m\u00f6\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Immer auf Menschenhuld einzuschr\u00e4nken:", "tokens": ["Im\u00b7mer", "auf", "Men\u00b7schen\u00b7huld", "ein\u00b7zu\u00b7schr\u00e4n\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "Umringt von Lebensfreuden, von Tausenden", "tokens": ["Um\u00b7ringt", "von", "Le\u00b7bens\u00b7freu\u00b7den", ",", "von", "Tau\u00b7sen\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gelobt, bewundert, niemal der Sterblichkeit,", "tokens": ["Ge\u00b7lobt", ",", "be\u00b7wun\u0