{"textgrid.poem.37657": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Reichsapfel", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbsagt, wo unser Apfel ist getrieben?", "tokens": ["\u00bb", "sagt", ",", "wo", "un\u00b7ser", "Ap\u00b7fel", "ist", "ge\u00b7trie\u00b7ben", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "War er Paradieses Zier?\u00ab", "tokens": ["War", "er", "Pa\u00b7ra\u00b7die\u00b7ses", "Zier", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbauf des Reiches Apfel steht geschrieben:", "tokens": ["\u00bb", "auf", "des", "Rei\u00b7ches", "Ap\u00b7fel", "steht", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "ADJA", "NN", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ich nicht in Versuchung f\u00fchr'.\u00ab", "tokens": ["Ich", "nicht", "in", "Ver\u00b7su\u00b7chung", "f\u00fchr'", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sprecht ihr so zum K\u00f6nig, Theologen,", "tokens": ["Sprecht", "ihr", "so", "zum", "K\u00f6\u00b7nig", ",", "Theo\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Alles wei\u00df ich, was geschieht,", "tokens": ["Al\u00b7les", "wei\u00df", "ich", ",", "was", "ge\u00b7schieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und dies Eine wird mir so entzogen,", "tokens": ["Und", "dies", "Ei\u00b7ne", "wird", "mir", "so", "ent\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PIS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wo der Apfel hat gebl\u00fcht?", "tokens": ["Wo", "der", "Ap\u00b7fel", "hat", "ge\u00b7bl\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Jedes Haus von Glas ist ungelogen,", "tokens": ["Je\u00b7des", "Haus", "von", "Glas", "ist", "un\u00b7ge\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Polizei hinein so sieht,", "tokens": ["Po\u00b7li\u00b7zei", "hin\u00b7ein", "so", "sieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Alles ist Maschine und Befehlen,", "tokens": ["Al\u00b7les", "ist", "Ma\u00b7schi\u00b7ne", "und", "Be\u00b7feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Nichts geschieht, denn jeder sieht,", "tokens": ["Nichts", "ge\u00b7schieht", ",", "denn", "je\u00b7der", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KON", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Einer immer mu\u00df den andern qu\u00e4len,", "tokens": ["Ei\u00b7ner", "im\u00b7mer", "mu\u00df", "den", "an\u00b7dern", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VMFIN", "ART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Da\u00df er in die Karten sieht;", "tokens": ["Da\u00df", "er", "in", "die", "Kar\u00b7ten", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Sollte eines dieser R\u00e4der brechen,", "tokens": ["Soll\u00b7te", "ei\u00b7nes", "die\u00b7ser", "R\u00e4\u00b7der", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "W\u00e4r' wohl alles schlecht bestellt,", "tokens": ["W\u00e4r'", "wohl", "al\u00b7les", "schlecht", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Alles schreibt und keiner will mehr sprechen,", "tokens": ["Al\u00b7les", "schreibt", "und", "kei\u00b7ner", "will", "mehr", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "PIS", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "In den Akten sinkt die Welt.", "tokens": ["In", "den", "Ak\u00b7ten", "sinkt", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "\u00bbsagt, wie mag der heil'ge Apfel schmecken,", "tokens": ["\u00bb", "sagt", ",", "wie", "mag", "der", "heil'\u00b7ge", "Ap\u00b7fel", "schme\u00b7cken", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PWAV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Denn ich hab' danach Gelust?\u00ab", "tokens": ["Denn", "ich", "hab'", "da\u00b7nach", "Ge\u00b7lust", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "PAV", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "\u00bbk\u00f6nig, nein, wir k\u00f6nnen dich nicht schrecken,", "tokens": ["\u00bb", "k\u00f6\u00b7nig", ",", "nein", ",", "wir", "k\u00f6n\u00b7nen", "dich", "nicht", "schre\u00b7cken", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "PTKANT", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Aber Schauder f\u00fcllt die Brust!\u00ab", "tokens": ["A\u00b7ber", "Schau\u00b7der", "f\u00fcllt", "die", "Brust", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Doch der K\u00f6nig geht den Apfel holen", "tokens": ["Doch", "der", "K\u00f6\u00b7nig", "geht", "den", "Ap\u00b7fel", "ho\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "In des Domes heil'ge Still',", "tokens": ["In", "des", "Do\u00b7mes", "heil'\u00b7ge", "Still'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Wo die Chorherrn singen unverhohlen", "tokens": ["Wo", "die", "Chor\u00b7herrn", "sin\u00b7gen", "un\u00b7ver\u00b7hoh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "\u00bbdie Versuchung in mir still.", "tokens": ["\u00bb", "die", "Ver\u00b7su\u00b7chung", "in", "mir", "still", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Und die Chorherrn singen aus der Stille:", "tokens": ["Und", "die", "Chor\u00b7herrn", "sin\u00b7gen", "aus", "der", "Stil\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Mich nicht in Versuchung f\u00fchr'.\u00ab", "tokens": ["Mich", "nicht", "in", "Ver\u00b7su\u00b7chung", "f\u00fchr'", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Doch die Uhr, ein fester, ernster Wille,", "tokens": ["Doch", "die", "Uhr", ",", "ein", "fes\u00b7ter", ",", "erns\u00b7ter", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Schl\u00e4gt, da\u00df sich der Ton verlier.", "tokens": ["Schl\u00e4gt", ",", "da\u00df", "sich", "der", "Ton", "ver\u00b7lier", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Auf den Thurm der K\u00f6nig steigt mit Springen,", "tokens": ["Auf", "den", "Thurm", "der", "K\u00f6\u00b7nig", "steigt", "mit", "Sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "Der sich schwindlig \u00fcberbeugt,", "tokens": ["Der", "sich", "schwind\u00b7lig", "\u00fc\u00b7ber\u00b7beugt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Wo die hell und dunklen Stunden klingen.", "tokens": ["Wo", "die", "hell", "und", "dunk\u00b7len", "Stun\u00b7den", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.34": {"text": "Die der Mann im Harnisch zeigt.", "tokens": ["Die", "der", "Mann", "im", "Har\u00b7nisch", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Mit dem Schwerte schl\u00e4gt er an die Stunden,", "tokens": ["Mit", "dem", "Schwer\u00b7te", "schl\u00e4gt", "er", "an", "die", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.36": {"text": "Tr\u00e4gt den Apfel in der Hand,", "tokens": ["Tr\u00e4gt", "den", "Ap\u00b7fel", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "K\u00f6nig nun hast du den Apfel funden,", "tokens": ["K\u00f6\u00b7nig", "nun", "hast", "du", "den", "Ap\u00b7fel", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.38": {"text": "Hast den Apfel k\u00fchn entwandt.", "tokens": ["Hast", "den", "Ap\u00b7fel", "k\u00fchn", "ent\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Auf dem Apfel liest du nun geschrieben:", "tokens": ["Auf", "dem", "Ap\u00b7fel", "liest", "du", "nun", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.40": {"text": "\u00bbich nicht in Versuchung f\u00fchr'!\u00ab", "tokens": ["\u00bb", "ich", "nicht", "in", "Ver\u00b7su\u00b7chung", "f\u00fchr'", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Doch er beist hinein von Lust getrieben,", "tokens": ["Doch", "er", "beist", "hin\u00b7ein", "von", "Lust", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.42": {"text": "Seine Z\u00e4hne brechen schier,", "tokens": ["Sei\u00b7ne", "Z\u00e4h\u00b7ne", "bre\u00b7chen", "schier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Mitten durch hat er ihn aufgebissen", "tokens": ["Mit\u00b7ten", "durch", "hat", "er", "ihn", "auf\u00b7ge\u00b7bis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "VAFIN", "PPER", "PPER", "VVINF"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.44": {"text": "Und er liest im Apfelstern:", "tokens": ["Und", "er", "liest", "im", "Ap\u00b7fels\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.45": {"text": "\u00bbreifen mu\u00df der Menschen sch\u00f6nes Wissen,", "tokens": ["\u00bb", "rei\u00b7fen", "mu\u00df", "der", "Men\u00b7schen", "sch\u00f6\u00b7nes", "Wis\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "VMFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.46": {"text": "Reif und schwarz wird dann der Kern,", "tokens": ["Reif", "und", "schwarz", "wird", "dann", "der", "Kern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Ist der Apfel fr\u00fcher aufgerissen,", "tokens": ["Ist", "der", "Ap\u00b7fel", "fr\u00fc\u00b7her", "auf\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.48": {"text": "Sinkt verwelkt dein bleicher Stern!\u00ab", "tokens": ["Sinkt", "ver\u00b7welkt", "dein", "blei\u00b7cher", "Stern", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Wie im Traume schrecklich festgehalten,", "tokens": ["Wie", "im", "Trau\u00b7me", "schreck\u00b7lich", "fest\u00b7ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.50": {"text": "Starrt der K\u00f6nig, sieht ihn wei\u00df,", "tokens": ["Starrt", "der", "K\u00f6\u00b7nig", ",", "sieht", "ihn", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Und die Sterne in des Himmels Falten", "tokens": ["Und", "die", "Ster\u00b7ne", "in", "des", "Him\u00b7mels", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.52": {"text": "Bergen sich schon alle leis,", "tokens": ["Ber\u00b7gen", "sich", "schon", "al\u00b7le", "leis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "PIS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Mann im Harnisch, du kannst ruhig walten,", "tokens": ["Mann", "im", "Har\u00b7nisch", ",", "du", "kannst", "ru\u00b7hig", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.54": {"text": "Diese Stund' verging so leis.", "tokens": ["Die\u00b7se", "Stund'", "ver\u00b7ging", "so", "leis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Mit dem Schwerte schl\u00e4gt er an die Stunde.", "tokens": ["Mit", "dem", "Schwer\u00b7te", "schl\u00e4gt", "er", "an", "die", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.56": {"text": "Und der K\u00f6nig steht davor,", "tokens": ["Und", "der", "K\u00f6\u00b7nig", "steht", "da\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.57": {"text": "Schl\u00e4gt in's Hirn ihm eine tiefe Wunde,", "tokens": ["Schl\u00e4gt", "in's", "Hirn", "ihm", "ei\u00b7ne", "tie\u00b7fe", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.58": {"text": "Seinem Geist ein offnes Thor.", "tokens": ["Sei\u00b7nem", "Geist", "ein", "off\u00b7nes", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure"