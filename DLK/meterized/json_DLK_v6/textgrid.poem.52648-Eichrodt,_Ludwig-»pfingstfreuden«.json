{"textgrid.poem.52648": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "\u00bbpfingstfreuden\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ueberall Gr\u00fcn.", "tokens": ["Ue\u00b7be\u00b7rall", "Gr\u00fcn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Gr\u00fcn Gras, Kraut, gr\u00fcne B\u00e4ume,", "tokens": ["Gr\u00fcn", "Gras", ",", "Kraut", ",", "gr\u00fc\u00b7ne", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Gr\u00fcn Saatfeld, gr\u00fcne Tr\u00e4ume,", "tokens": ["Gr\u00fcn", "Saat\u00b7feld", ",", "gr\u00fc\u00b7ne", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herz voll Hoffnung", "tokens": ["Herz", "voll", "Hoff\u00b7nung"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Wo schw\u00e4rmst du hin?", "tokens": ["Wo", "schw\u00e4rmst", "du", "hin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Achte des Fr\u00f6schleins, Schw\u00e4rmer,", "tokens": ["Ach\u00b7te", "des", "Fr\u00f6schleins", ",", "Schw\u00e4r\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Mache die Sch\u00f6pfung", "tokens": ["Ma\u00b7che", "die", "Sch\u00f6p\u00b7fung"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Um ein Gesch\u00f6pf nicht \u00e4rmer!", "tokens": ["Um", "ein", "Ge\u00b7sch\u00f6pf", "nicht", "\u00e4r\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Der arme Gr\u00fcnrock, ohne Spa\u00df,", "tokens": ["Der", "ar\u00b7me", "Gr\u00fcn\u00b7rock", ",", "oh\u00b7ne", "Spa\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mu\u00dfte durch dich bei\u00dfen ins gr\u00fcne Gras.", "tokens": ["Mu\u00df\u00b7te", "durch", "dich", "bei\u00b7\u00dfen", "ins", "gr\u00fc\u00b7ne", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Was kleidet er sich in Hoffnungstracht?", "tokens": ["Was", "klei\u00b7det", "er", "sich", "in", "Hoff\u00b7nungs\u00b7tracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Ha ha! wie habe ich gr\u00fcn gelacht!", "tokens": ["Ha", "ha", "!", "wie", "ha\u00b7be", "ich", "gr\u00fcn", "ge\u00b7lacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "PWAV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}