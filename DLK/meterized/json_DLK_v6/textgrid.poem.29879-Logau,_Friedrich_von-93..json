{"textgrid.poem.29879": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "93.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Casca ist wie Finstern\u00fc\u00df, und ihr Gold ist wie die Sonne;", "tokens": ["Cas\u00b7ca", "ist", "wie", "Fins\u00b7ter\u00b7n\u00fc\u00df", ",", "und", "ihr", "Gold", "ist", "wie", "die", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "KOKOM", "NN", "$,", "KON", "PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ihr Gesichte bringet Grau, und ihr Beutel bringet Wonne.", "tokens": ["Ihr", "Ge\u00b7sich\u00b7te", "brin\u00b7get", "Grau", ",", "und", "ihr", "Beu\u00b7tel", "brin\u00b7get", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Wer nun Sonn und Wonne liebet, mu\u00df sich machen auch bereit,", "tokens": ["Wer", "nun", "Sonn", "und", "Won\u00b7ne", "lie\u00b7bet", ",", "mu\u00df", "sich", "ma\u00b7chen", "auch", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "KON", "NN", "VVFIN", "$,", "VMFIN", "PRF", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Da\u00df er mit der Finstern\u00fcsse bringe zu bestimmte Zeit.", "tokens": ["Da\u00df", "er", "mit", "der", "Fins\u00b7ter\u00b7n\u00fcs\u00b7se", "brin\u00b7ge", "zu", "be\u00b7stimm\u00b7te", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}