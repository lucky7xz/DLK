{"textgrid.poem.51351": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An Ihro Gn. Fra\u00fclein R\u00f6\u00dfle gebohrne Fra\u00fclein von Sedlintzky", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Titul, Fra\u00fclein, ihr, das Buch hab ich gemacht,", "tokens": ["Den", "Ti\u00b7tul", ",", "Fra\u00fc\u00b7lein", ",", "ihr", ",", "das", "Buch", "hab", "ich", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "PPER", "$,", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Buch nehmt ihr, und mir wolt ihr den Titul geben,", "tokens": ["Das", "Buch", "nehmt", "ihr", ",", "und", "mir", "wolt", "ihr", "den", "Ti\u00b7tul", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KON", "PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich la\u00df es ja geschehn, doch, ist es recht erdacht:", "tokens": ["Ich", "la\u00df", "es", "ja", "ge\u00b7schehn", ",", "doch", ",", "ist", "es", "recht", "er\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVPP", "$,", "ADV", "$,", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kan, was von Euch gebohrn, bey Euch auch eintzig leben.", "tokens": ["Kan", ",", "was", "von", "Euch", "ge\u00b7bohrn", ",", "bey", "Euch", "auch", "eint\u00b7zig", "le\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "PRELS", "APPR", "PPER", "VVPP", "$,", "APPR", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}