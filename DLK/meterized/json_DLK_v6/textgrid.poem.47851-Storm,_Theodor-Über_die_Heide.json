{"textgrid.poem.47851": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "\u00dcber die Heide", "genre": "verse", "period": "N.A.", "pub_year": 1875, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber die Heide hallet mein Schritt;", "tokens": ["\u00dc\u00b7ber", "die", "Hei\u00b7de", "hal\u00b7let", "mein", "Schritt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dumpf aus der Erde wandert es mit.", "tokens": ["Dumpf", "aus", "der", "Er\u00b7de", "wan\u00b7dert", "es", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Herbst ist gekommen, Fr\u00fchling ist weit \u2013", "tokens": ["Herbst", "ist", "ge\u00b7kom\u00b7men", ",", "Fr\u00fch\u00b7ling", "ist", "weit", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "NN", "VAFIN", "ADJD", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Gab es denn einmal selige Zeit?", "tokens": ["Gab", "es", "denn", "ein\u00b7mal", "se\u00b7li\u00b7ge", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Brauende Nebel geisten umher;", "tokens": ["Brau\u00b7en\u00b7de", "Ne\u00b7bel", "geis\u00b7ten", "um\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Schwarz ist das Kraut und der Himmel so leer.", "tokens": ["Schwarz", "ist", "das", "Kraut", "und", "der", "Him\u00b7mel", "so", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "KON", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "W\u00e4r ich hier nur nicht gegangen im Mai!", "tokens": ["W\u00e4r", "ich", "hier", "nur", "nicht", "ge\u00b7gan\u00b7gen", "im", "Mai", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Leben und Liebe \u2013 wie flog es vorbei!", "tokens": ["Le\u00b7ben", "und", "Lie\u00b7be", "\u2013", "wie", "flog", "es", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$(", "PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}