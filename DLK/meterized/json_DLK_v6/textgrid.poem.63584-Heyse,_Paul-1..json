{"textgrid.poem.63584": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1883, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie hast du nur hinweg dich stehlen k\u00f6nnen", "tokens": ["Wie", "hast", "du", "nur", "hin\u00b7weg", "dich", "steh\u00b7len", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "PPER", "VVINF", "VMINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus dieser Lichtwelt, ohne \u2013 b\u00f6ses Kind! \u2013", "tokens": ["Aus", "die\u00b7ser", "Licht\u00b7welt", ",", "oh\u00b7ne", "\u2013", "b\u00f6\u00b7ses", "Kind", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "KOUI", "$(", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mir einen Scheideliebesblick zu g\u00f6nnen!", "tokens": ["Mir", "ei\u00b7nen", "Schei\u00b7de\u00b7lie\u00b7bes\u00b7blick", "zu", "g\u00f6n\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hast, da ich arglos ferne war, geschwind", "tokens": ["Hast", ",", "da", "ich", "arg\u00b7los", "fer\u00b7ne", "war", ",", "ge\u00b7schwind"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "ADJD", "ADV", "VAFIN", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dich fortgeschlichen, ohn' ade zu sagen,", "tokens": ["Dich", "fort\u00b7ge\u00b7schli\u00b7chen", ",", "ohn'", "a\u00b7de", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KOUI", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und ich, in Tr\u00e4nen, suche nun mich blind!", "tokens": ["Und", "ich", ",", "in", "Tr\u00e4\u00b7nen", ",", "su\u00b7che", "nun", "mich", "blind", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "NN", "$,", "VVFIN", "ADV", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sonst, wenn du fr\u00fche schon an Sommertagen", "tokens": ["Sonst", ",", "wenn", "du", "fr\u00fc\u00b7he", "schon", "an", "Som\u00b7mer\u00b7ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJA", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Spazieren gingst und lie\u00dfest dich hinab", "tokens": ["Spa\u00b7zie\u00b7ren", "gingst", "und", "lie\u00b7\u00dfest", "dich", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die Treppe nur bis in den Garten tragen,", "tokens": ["Die", "Trep\u00b7pe", "nur", "bis", "in", "den", "Gar\u00b7ten", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Da klopftest du, bis ich dir Einla\u00df gab,", "tokens": ["Da", "klopf\u00b7test", "du", ",", "bis", "ich", "dir", "Ein\u00b7la\u00df", "gab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und botst das M\u00e4ulchen mir, bewegtest winkend", "tokens": ["Und", "botst", "das", "M\u00e4ul\u00b7chen", "mir", ",", "be\u00b7weg\u00b7test", "win\u00b7kend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Schalkhaft das kleine H\u00e4ndchen auf und ab.", "tokens": ["Schalk\u00b7haft", "das", "klei\u00b7ne", "H\u00e4nd\u00b7chen", "auf", "und", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und ich, von deinen Lippen Freude trinkend,", "tokens": ["Und", "ich", ",", "von", "dei\u00b7nen", "Lip\u00b7pen", "Freu\u00b7de", "trin\u00b7kend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Zog dich ans Herz und gab dich z\u00f6gernd frei,", "tokens": ["Zog", "dich", "ans", "Herz", "und", "gab", "dich", "z\u00f6\u00b7gernd", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "KON", "VVFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Mich aller V\u00e4ter gl\u00fccklichsten bed\u00fcnkend.", "tokens": ["Mich", "al\u00b7ler", "V\u00e4\u00b7ter", "gl\u00fcck\u00b7lichs\u00b7ten", "be\u00b7d\u00fcn\u00b7kend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.16": {"text": "Nun brachst du scheidend mir das Herz entzwei.", "tokens": ["Nun", "brachst", "du", "schei\u00b7dend", "mir", "das", "Herz", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Ich durfte nicht dir von den Lippen k\u00fcssen", "tokens": ["Ich", "durf\u00b7te", "nicht", "dir", "von", "den", "Lip\u00b7pen", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Den letzten Seufzer, ach, den letzten Schrei.", "tokens": ["Den", "letz\u00b7ten", "Seuf\u00b7zer", ",", "ach", ",", "den", "letz\u00b7ten", "Schrei", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ITJ", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Warst du so klug, mein Liebling, um zu wissen,", "tokens": ["Warst", "du", "so", "klug", ",", "mein", "Lieb\u00b7ling", ",", "um", "zu", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Da\u00df dieser Abschied, dieser jammervolle,", "tokens": ["Da\u00df", "die\u00b7ser", "Ab\u00b7schied", ",", "die\u00b7ser", "jam\u00b7mer\u00b7vol\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "PDAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Mein Leben h\u00e4tte mit hinweggerissen?", "tokens": ["Mein", "Le\u00b7ben", "h\u00e4t\u00b7te", "mit", "hin\u00b7weg\u00b7ge\u00b7ris\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Das Graun, da\u00df ich dahin dich geben solle", "tokens": ["Das", "Graun", ",", "da\u00df", "ich", "da\u00b7hin", "dich", "ge\u00b7ben", "sol\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PAV", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Dem Reich der Nacht, mich h\u00e4tte selbst verleitet,", "tokens": ["Dem", "Reich", "der", "Nacht", ",", "mich", "h\u00e4t\u00b7te", "selbst", "ver\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Dir nachzuschleichen unter deine Scholle?", "tokens": ["Dir", "nach\u00b7zu\u00b7schlei\u00b7chen", "un\u00b7ter", "dei\u00b7ne", "Schol\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "O Kind, du hast das Schlimmre mir bereitet,", "tokens": ["O", "Kind", ",", "du", "hast", "das", "Schlimm\u00b7re", "mir", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Da\u00df des vers\u00e4umten Abschieds mahnend Bild", "tokens": ["Da\u00df", "des", "ver\u00b7s\u00e4um\u00b7ten", "Ab\u00b7schieds", "mah\u00b7nend", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Auf Schritt und Tritt gespenstisch mich begleitet;", "tokens": ["Auf", "Schritt", "und", "Tritt", "ge\u00b7spens\u00b7tisch", "mich", "be\u00b7glei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Da\u00df mir nun ist, als k\u00f6nn' ich ins Gefild", "tokens": ["Da\u00df", "mir", "nun", "ist", ",", "als", "k\u00f6nn'", "ich", "ins", "Ge\u00b7fild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Des Lebens keinen festen Schritt mehr tun,", "tokens": ["Des", "Le\u00b7bens", "kei\u00b7nen", "fes\u00b7ten", "Schritt", "mehr", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Eh' ich den letzten bangen Wunsch gestillt.", "tokens": ["Eh'", "ich", "den", "letz\u00b7ten", "ban\u00b7gen", "Wunsch", "ge\u00b7stillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Und wenn ein wenig kaum die Schmerzen ruhn", "tokens": ["Und", "wenn", "ein", "we\u00b7nig", "kaum", "die", "Schmer\u00b7zen", "ruhn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "PIS", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Und Lebenshoffnung sich hervor will wagen,", "tokens": ["Und", "Le\u00b7bens\u00b7hoff\u00b7nung", "sich", "her\u00b7vor", "will", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PRF", "PTKVZ", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Bebt pl\u00f6tzlich mir das Herz, als sollte nun", "tokens": ["Bebt", "pl\u00f6tz\u00b7lich", "mir", "das", "Herz", ",", "als", "soll\u00b7te", "nun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "NN", "$,", "KOUS", "VMFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Mein Kind erst kommen, gute Nacht! zu sagen.", "tokens": ["Mein", "Kind", "erst", "kom\u00b7men", ",", "gu\u00b7te", "Nacht", "!", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$,", "ADJA", "NN", "$.", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}