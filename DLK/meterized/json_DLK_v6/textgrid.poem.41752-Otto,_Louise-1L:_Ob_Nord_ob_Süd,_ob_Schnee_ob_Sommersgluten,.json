{"textgrid.poem.41752": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ob Nord ob S\u00fcd, ob Schnee ob Sommersgluten,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob Nord ob S\u00fcd, ob Schnee ob Sommersgluten,", "tokens": ["Ob", "Nord", "ob", "S\u00fcd", ",", "ob", "Schnee", "ob", "Som\u00b7mers\u00b7glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KOUS", "NN", "$,", "KOUS", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das k\u00fcmmert nie ein echtes Bergmannskind,", "tokens": ["Das", "k\u00fcm\u00b7mert", "nie", "ein", "ech\u00b7tes", "Berg\u00b7manns\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus ihren Adern mu\u00df die Erde bluten", "tokens": ["Aus", "ih\u00b7ren", "A\u00b7dern", "mu\u00df", "die", "Er\u00b7de", "blu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo es am reichsten drinnen wallt und rinnt.", "tokens": ["Wo", "es", "am", "reichs\u00b7ten", "drin\u00b7nen", "wallt", "und", "rinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der F\u00e4ustel klingt \u2013 der Andern Ohr verborgen", "tokens": ["Der", "F\u00e4us\u00b7tel", "klingt", "\u2013", "der", "An\u00b7dern", "Ohr", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die droben wohnen in des Himmels Blau.", "tokens": ["Die", "dro\u00b7ben", "woh\u00b7nen", "in", "des", "Him\u00b7mels", "Blau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es f\u00e4llt ein Schu\u00df \u2013 der Tiefe Geister horchen", "tokens": ["Es", "f\u00e4llt", "ein", "Schu\u00df", "\u2013", "der", "Tie\u00b7fe", "Geis\u00b7ter", "hor\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und rings ert\u00f6nt der unterw\u00fchlte Bau.", "tokens": ["Und", "rings", "er\u00b7t\u00f6nt", "der", "un\u00b7ter\u00b7w\u00fchl\u00b7te", "Bau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So ist es hier, so ist es allerorten!", "tokens": ["So", "ist", "es", "hier", ",", "so", "ist", "es", "al\u00b7le\u00b7ror\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Erz ist schon im Mutterleib die Kraft", "tokens": ["Dem", "Erz", "ist", "schon", "im", "Mut\u00b7ter\u00b7leib", "die", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den, der ihm naht, Gefahr zu bringen worden,", "tokens": ["Den", ",", "der", "ihm", "naht", ",", "Ge\u00b7fahr", "zu", "brin\u00b7gen", "wor\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "VVFIN", "$,", "NN", "PTKZU", "VVINF", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur Flei\u00df und Kampf befreit es seiner Haft.", "tokens": ["Nur", "Flei\u00df", "und", "Kampf", "be\u00b7freit", "es", "sei\u00b7ner", "Haft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Flei\u00df und Kampf ist sein Geschick hienieden,", "tokens": ["Und", "Flei\u00df", "und", "Kampf", "ist", "sein", "Ge\u00b7schick", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So dient es Jedem, der sich's unterwarf,", "tokens": ["So", "dient", "es", "Je\u00b7dem", ",", "der", "sich's", "un\u00b7ter\u00b7warf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mag draus die Pflugschar, mag das Schwert man schmieden.", "tokens": ["Mag", "draus", "die", "Pflug\u00b7schar", ",", "mag", "das", "Schwert", "man", "schmie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PAV", "ART", "NN", "$,", "VMFIN", "ART", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Schienenweg der L\u00e4nder einen darf.", "tokens": ["Den", "Schie\u00b7nen\u00b7weg", "der", "L\u00e4n\u00b7der", "ei\u00b7nen", "darf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dir, Eisen, m\u00f6cht ein stolzes Lied ich singen", "tokens": ["Dir", ",", "Ei\u00b7sen", ",", "m\u00f6cht", "ein", "stol\u00b7zes", "Lied", "ich", "sin\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "VMFIN", "ART", "ADJA", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du kannst ein Engel f\u00fcr die Menschheit sein,", "tokens": ["Du", "kannst", "ein", "En\u00b7gel", "f\u00fcr", "die", "Menschheit", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf Deinen ", "tokens": ["Auf", "Dei\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Mit Deinen ", "tokens": ["Mit", "Dei\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Als Pflugschar w\u00fchlen in dem Scho\u00df der Erde,", "tokens": ["Als", "Pflug\u00b7schar", "w\u00fch\u00b7len", "in", "dem", "Scho\u00df", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bis er sich segenbringend rings erschlie\u00dft", "tokens": ["Bis", "er", "sich", "se\u00b7gen\u00b7brin\u00b7gend", "rings", "er\u00b7schlie\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "ADV", "VVFIN"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.7": {"text": "Und gr\u00fcn und wachsend durch ein neues Werde", "tokens": ["Und", "gr\u00fcn", "und", "wach\u00b7send", "durch", "ein", "neu\u00b7es", "Wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "N\u00e4hrendes Korn f\u00fcr Alle ihm entsprie\u00dft.", "tokens": ["N\u00e4h\u00b7ren\u00b7des", "Korn", "f\u00fcr", "Al\u00b7le", "ihm", "ent\u00b7sprie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIS", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Du aber, Silber mit dem bleichen Schimmer,", "tokens": ["Du", "a\u00b7ber", ",", "Sil\u00b7ber", "mit", "dem", "blei\u00b7chen", "Schim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Du, sein stolzer Bruder, lockend Gold,", "tokens": ["Und", "Du", ",", "sein", "stol\u00b7zer", "Bru\u00b7der", ",", "lo\u00b7ckend", "Gold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr beide brachtet solchen Segen nimmer,", "tokens": ["Ihr", "bei\u00b7de", "brach\u00b7tet", "sol\u00b7chen", "Se\u00b7gen", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Freiheit nicht, Knechtschaft habt Ihr gewollt.", "tokens": ["Die", "Frei\u00b7heit", "nicht", ",", "Knecht\u00b7schaft", "habt", "Ihr", "ge\u00b7wollt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$,", "NN", "VAFIN", "PPER", "VMPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und wenn als Engel Ihr der Welt erschienen,", "tokens": ["Und", "wenn", "als", "En\u00b7gel", "Ihr", "der", "Welt", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "KOKOM", "NE", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So hat die Welt zu Teufeln Euch gemacht,", "tokens": ["So", "hat", "die", "Welt", "zu", "Teu\u00b7feln", "Euch", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dem Satan nur und seinen Ruhm zu dienen", "tokens": ["Dem", "Sa\u00b7tan", "nur", "und", "sei\u00b7nen", "Ruhm", "zu", "die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "KON", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Steigt Ihr zum Licht empor aus Eurer Nacht.", "tokens": ["Steigt", "Ihr", "zum", "Licht", "em\u00b7por", "aus", "Eu\u00b7rer", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Steigt Ihr empor \u2013 und auf den bleichen Wangen", "tokens": ["Steigt", "Ihr", "em\u00b7por", "\u2013", "und", "auf", "den", "blei\u00b7chen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$(", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des armen Bergmanns gl\u00fcht ein pl\u00f6tzlich Rot,", "tokens": ["Des", "ar\u00b7men", "Berg\u00b7manns", "gl\u00fcht", "ein", "pl\u00f6tz\u00b7lich", "Rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An Eurem Glanze feine Blicke hangen,", "tokens": ["An", "Eu\u00b7rem", "Glan\u00b7ze", "fei\u00b7ne", "Bli\u00b7cke", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bei Eurem Glanze denkt er seiner Not!", "tokens": ["Bei", "Eu\u00b7rem", "Glan\u00b7ze", "denkt", "er", "sei\u00b7ner", "Not", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ist es doch all sein Sorgen und sein M\u00fchen", "tokens": ["Ist", "es", "doch", "all", "sein", "Sor\u00b7gen", "und", "sein", "M\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PIAT", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Euch aus dem finstern Kerker zu befrein,", "tokens": ["Euch", "aus", "dem", "fins\u00b7tern", "Ker\u00b7ker", "zu", "be\u00b7fr\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ihr aber wollt nicht dankbar f\u00fcr ihn gl\u00fchen", "tokens": ["Ihr", "a\u00b7ber", "wollt", "nicht", "dank\u00b7bar", "f\u00fcr", "ihn", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "PTKNEG", "ADJD", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und gl\u00e4nzt ihm nur zu h\u00f6hnen seine Pein.", "tokens": ["Und", "gl\u00e4nzt", "ihm", "nur", "zu", "h\u00f6h\u00b7nen", "sei\u00b7ne", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Er hat kein Silber und kein Gold im Hause,", "tokens": ["Er", "hat", "kein", "Sil\u00b7ber", "und", "kein", "Gold", "im", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "KON", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur Thr\u00e4nen, nur der Kinder Hungerschrei,", "tokens": ["Nur", "Thr\u00e4\u00b7nen", ",", "nur", "der", "Kin\u00b7der", "Hun\u00b7ger\u00b7schrei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die harren sein in seiner engen Klause,", "tokens": ["Die", "har\u00b7ren", "sein", "in", "sei\u00b7ner", "en\u00b7gen", "Klau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAINF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein bleiches Weib, in Not und Elend treu.", "tokens": ["Ein", "blei\u00b7ches", "Weib", ",", "in", "Not", "und", "E\u00b7lend", "treu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und m\u00fcde sinkt er auf sein Lager nieder", "tokens": ["Und", "m\u00fc\u00b7de", "sinkt", "er", "auf", "sein", "La\u00b7ger", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und st\u00f6hnt und hat ein b\u00f6ses Traumgesicht:", "tokens": ["Und", "st\u00f6hnt", "und", "hat", "ein", "b\u00f6\u00b7ses", "Traum\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Er springt empor, hebt stolz die starken Arme", "tokens": ["Er", "springt", "em\u00b7por", ",", "hebt", "stolz", "die", "star\u00b7ken", "Ar\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da f\u00e4hrt sein Weib empor wie niemals je,", "tokens": ["Da", "f\u00e4hrt", "sein", "Weib", "em\u00b7por", "wie", "nie\u00b7mals", "je", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "KOKOM", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und fl\u00fcstert tonlos matt vom gro\u00dfen Harme:", "tokens": ["Und", "fl\u00fcs\u00b7tert", "ton\u00b7los", "matt", "vom", "gro\u00b7\u00dfen", "Har\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.5": {"text": "Da ist ihm bei dem Wort der Traum entschwunden,", "tokens": ["Da", "ist", "ihm", "bei", "dem", "Wort", "der", "Traum", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er ist erwacht und spricht: \u00bbSchnell ist der Lauf", "tokens": ["Er", "ist", "er\u00b7wacht", "und", "spricht", ":", "\u00bb", "Schnell", "ist", "der", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVFIN", "$.", "$(", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der kurz uns zu geme\u00dfnen Ruhestunden \u2013", "tokens": ["Der", "kurz", "uns", "zu", "ge\u00b7me\u00df\u00b7nen", "Ru\u00b7hes\u00b7tun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "APPR", "ADJA", "NN", "$("], "meter"