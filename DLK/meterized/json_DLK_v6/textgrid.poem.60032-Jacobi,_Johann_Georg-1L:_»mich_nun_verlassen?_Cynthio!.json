{"textgrid.poem.60032": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbmich nun verlassen? Cynthio!", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmich nun verlassen? Cynthio!", "tokens": ["\u00bb", "mich", "nun", "ver\u00b7las\u00b7sen", "?", "Cyn\u00b7thio", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "ADV", "VVINF", "$.", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Mich nun auf ewig? Liebst du so", "tokens": ["Mich", "nun", "auf", "e\u00b7wig", "?", "Liebst", "du", "so"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ADJD", "$.", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die z\u00e4rtliche Rosette?", "tokens": ["Die", "z\u00e4rt\u00b7li\u00b7che", "Ro\u00b7set\u00b7te", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Belohnst sie mit Verr\u00e4therey,", "tokens": ["Be\u00b7lohnst", "sie", "mit", "Ver\u00b7r\u00e4\u00b7the\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und achtest nicht ihr Klaggeschrey", "tokens": ["Und", "ach\u00b7test", "nicht", "ihr", "Klag\u00b7ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am na\u00dfgeweinten Bette?", "tokens": ["Am", "na\u00df\u00b7ge\u00b7wein\u00b7ten", "Bet\u00b7te", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Verschm\u00e4hst getreuer Liebe Gunst,", "tokens": ["Ver\u00b7schm\u00e4hst", "ge\u00b7treu\u00b7er", "Lie\u00b7be", "Gunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sie, beh\u00fclflich deiner Kunst", "tokens": ["Da", "sie", ",", "be\u00b7h\u00fcl\u00b7flich", "dei\u00b7ner", "Kunst"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Marmor zu beleben,", "tokens": ["Den", "Mar\u00b7mor", "zu", "be\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu deinen Venusbildern dir,", "tokens": ["Zu", "dei\u00b7nen", "Ve\u00b7nus\u00b7bil\u00b7dern", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was sch\u00f6n und artig war an ihr,", "tokens": ["Was", "sch\u00f6n", "und", "ar\u00b7tig", "war", "an", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Unschuld Preis gegeben?", "tokens": ["In", "Un\u00b7schuld", "Preis", "ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wohlan, Verr\u00e4ther! so vergi\u00df,", "tokens": ["Wo\u00b7hlan", ",", "Ver\u00b7r\u00e4\u00b7ther", "!", "so", "ver\u00b7gi\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$.", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer diese H\u00fclle mir entri\u00df", "tokens": ["Wer", "die\u00b7se", "H\u00fcl\u00b7le", "mir", "ent\u00b7ri\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PDAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit seinen Schmeicheleyen.", "tokens": ["Mit", "sei\u00b7nen", "Schmei\u00b7che\u00b7le\u00b7yen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und ach! mit K\u00fcssen ohne Zahl,", "tokens": ["Und", "ach", "!", "mit", "K\u00fcs\u00b7sen", "oh\u00b7ne", "Zahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer durfte mir zum ersten Mahl", "tokens": ["Wer", "durf\u00b7te", "mir", "zum", "ers\u00b7ten", "Mahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die junge Brust entweihen?", "tokens": ["Die", "jun\u00b7ge", "Brust", "ent\u00b7wei\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du fliehst Rosettens Angesicht?", "tokens": ["Du", "fliehst", "Ro\u00b7set\u00b7tens", "An\u00b7ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NN", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "O Cynthio! so sprachst du nicht,", "tokens": ["O", "Cyn\u00b7thio", "!", "so", "sprachst", "du", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ich, von deinem Flehen", "tokens": ["Als", "ich", ",", "von", "dei\u00b7nem", "Fle\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erweicht, die H\u00fclle fa\u00dfte, gieng,", "tokens": ["Er\u00b7weicht", ",", "die", "H\u00fcl\u00b7le", "fa\u00df\u00b7te", ",", "gieng", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und meine Heiligen behieng,", "tokens": ["Und", "mei\u00b7ne", "Hei\u00b7li\u00b7gen", "be\u00b7hieng", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus Furcht, sie m\u00f6chten sehen;", "tokens": ["Aus", "Furcht", ",", "sie", "m\u00f6ch\u00b7ten", "se\u00b7hen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Als noch mein unverstellter Blick", "tokens": ["Als", "noch", "mein", "un\u00b7ver\u00b7stell\u00b7ter", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu manchem hohen Meisterst\u00fcck", "tokens": ["Zu", "man\u00b7chem", "ho\u00b7hen", "Meis\u00b7ter\u00b7st\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Morgen dich entz\u00fcckte;", "tokens": ["Am", "Mor\u00b7gen", "dich", "ent\u00b7z\u00fcck\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als ich, so bald der Abend kam,", "tokens": ["Als", "ich", ",", "so", "bald", "der", "A\u00b7bend", "kam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Werkzeug deinen H\u00e4nden nahm,", "tokens": ["Das", "Werk\u00b7zeug", "dei\u00b7nen", "H\u00e4n\u00b7den", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und dich mein Ku\u00df begl\u00fcckte.\u00ab", "tokens": ["Und", "dich", "mein", "Ku\u00df", "be\u00b7gl\u00fcck\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbverzeih, Geliebteste! verzeih;", "tokens": ["\u00bb", "ver\u00b7zeih", ",", "Ge\u00b7lieb\u00b7tes\u00b7te", "!", "ver\u00b7zeih", ";"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "NN", "$.", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Ruf ist eine W\u00fcsteney,", "tokens": ["Mein", "Ruf", "ist", "ei\u00b7ne", "W\u00fcs\u00b7te\u00b7ney", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verborgen deinen K\u00fcssen;", "tokens": ["Ver\u00b7bor\u00b7gen", "dei\u00b7nen", "K\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In W\u00e4ldern mu\u00df ich, fromm und wild,", "tokens": ["In", "W\u00e4l\u00b7dern", "mu\u00df", "ich", ",", "fromm", "und", "wild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr jedes allzusch\u00f6ne Bild,", "tokens": ["F\u00fcr", "je\u00b7des", "all\u00b7zu\u00b7sch\u00f6\u00b7ne", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach dir geformet, b\u00fc\u00dfen.", "tokens": ["Nach", "dir", "ge\u00b7for\u00b7met", ",", "b\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Im Himmel, o du gutes Kind!", "tokens": ["Im", "Him\u00b7mel", ",", "o", "du", "gu\u00b7tes", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "FM", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bekenn' es nur, im Himmel sind", "tokens": ["Be\u00b7kenn'", "es", "nur", ",", "im", "Him\u00b7mel", "sind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "$,", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht Heben und Dianen:", "tokens": ["Nicht", "He\u00b7ben", "und", "Di\u00b7a\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da treffen wir uns wieder an:", "tokens": ["Da", "tref\u00b7fen", "wir", "uns", "wie\u00b7der", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich will inde\u00df, so gut ich kann,", "tokens": ["Ich", "will", "in\u00b7de\u00df", ",", "so", "gut", "ich", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "ADV", "ADJD", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr uns die Wege bahnen.\u00ab", "tokens": ["F\u00fcr", "uns", "die", "We\u00b7ge", "bah\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Das treue M\u00e4dchen weinte Blut;", "tokens": ["Das", "treu\u00b7e", "M\u00e4d\u00b7chen", "wein\u00b7te", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dennoch wandelte, voll Muth,", "tokens": ["Und", "den\u00b7noch", "wan\u00b7del\u00b7te", ",", "voll", "Muth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Heilige von dannen,", "tokens": ["Der", "Hei\u00b7li\u00b7ge", "von", "dan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bereits im Haar den goldnen Schein,", "tokens": ["Be\u00b7reits", "im", "Haar", "den", "gold\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Kopfe nichts als Engelein,", "tokens": ["Im", "Kop\u00b7fe", "nichts", "als", "En\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Agnesen und Susannen.", "tokens": ["Ag\u00b7ne\u00b7sen", "und", "Su\u00b7san\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.9": {"line.1": {"text": "Nach einer kurzen Reise kroch", "tokens": ["Nach", "ei\u00b7ner", "kur\u00b7zen", "Rei\u00b7se", "kroch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er in ein dunkles Felsenloch,", "tokens": ["Er", "in", "ein", "dunk\u00b7les", "Fel\u00b7sen\u00b7loch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und baute seine Zelle.", "tokens": ["Und", "bau\u00b7te", "sei\u00b7ne", "Zel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zusammen trug er in den Wald", "tokens": ["Zu\u00b7sam\u00b7men", "trug", "er", "in", "den", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sich Steine dann, die wurden bald", "tokens": ["Sich", "Stei\u00b7ne", "dann", ",", "die", "wur\u00b7den", "bald"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "NN", "ADV", "$,", "PRELS", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zur artigen Kapelle.", "tokens": ["Zur", "ar\u00b7ti\u00b7gen", "Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "In tiefer Reue schnitzt' er nun,", "tokens": ["In", "tie\u00b7fer", "Reu\u00b7e", "schnitzt'", "er", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Beten dann und wann zu ruhn,", "tokens": ["Vom", "Be\u00b7ten", "dann", "und", "wann", "zu", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "KON", "PWAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich eine Magdalene,", "tokens": ["Sich", "ei\u00b7ne", "Mag\u00b7da\u00b7le\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit blonden Locken, d\u00fcnner Tracht,", "tokens": ["Mit", "blon\u00b7den", "Lo\u00b7cken", ",", "d\u00fcn\u00b7ner", "Tracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In allen Theilen wohl gemacht,", "tokens": ["In", "al\u00b7len", "Thei\u00b7len", "wohl", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bis auf die kleinste Thr\u00e4ne.", "tokens": ["Bis", "auf", "die", "kleins\u00b7te", "Thr\u00e4\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sie lag am Felsen j\u00e4mmerlich,", "tokens": ["Sie", "lag", "am", "Fel\u00b7sen", "j\u00e4m\u00b7mer\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sch\u00f6n, da\u00df auch ein T\u00fcrke sich", "tokens": ["So", "sch\u00f6n", ",", "da\u00df", "auch", "ein", "T\u00fcr\u00b7ke", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ADV", "ART", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit ihr betr\u00fcbet h\u00e4tte.", "tokens": ["Mit", "ihr", "be\u00b7tr\u00fc\u00b7bet", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wi\u00dft ihr, wem sie \u00e4hnlich war?", "tokens": ["Und", "wi\u00dft", "ihr", ",", "wem", "sie", "\u00e4hn\u00b7lich", "war", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An Auge, Busen, Mund und Haar,", "tokens": ["An", "Au\u00b7ge", ",", "Bu\u00b7sen", ",", "Mund", "und", "Haar", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der weinenden Rosette.", "tokens": ["Der", "wei\u00b7nen\u00b7den", "Ro\u00b7set\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "\u00bbwas seh' ich? Welche Pr\u00fcfung? O!", "tokens": ["\u00bb", "was", "seh'", "ich", "?", "Wel\u00b7che", "Pr\u00fc\u00b7fung", "?", "O", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$.", "PWAT", "NN", "$.", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Himmel will, des bin ich froh,", "tokens": ["Der", "Him\u00b7mel", "will", ",", "des", "bin", "ich", "froh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "PDS", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die stolze Brust zermalmen.", "tokens": ["Die", "stol\u00b7ze", "Brust", "zer\u00b7mal\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich folge williglich.\u00ab Er bringt", "tokens": ["Ich", "fol\u00b7ge", "wil\u00b7lig\u00b7lich", ".", "\u00ab", "Er", "bringt"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "$(", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Bild in sein Kappellchen, singt", "tokens": ["Das", "Bild", "in", "sein", "Kap\