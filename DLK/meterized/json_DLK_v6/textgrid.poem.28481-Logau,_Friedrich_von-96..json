{"textgrid.poem.28481": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "96.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df die Sueden heissen G\u00f6tter,", "tokens": ["Da\u00df", "die", "Sue\u00b7den", "heis\u00b7sen", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleibt wol wahr; sie machten Wetter,", "tokens": ["Bleibt", "wol", "wahr", ";", "sie", "mach\u00b7ten", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$.", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit ihren Donnerkeilen", "tokens": ["Und", "mit", "ih\u00b7ren", "Don\u00b7ner\u00b7kei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kunten Deutschland sie zertheilen.", "tokens": ["Kun\u00b7ten", "Deutschland", "sie", "zer\u00b7thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPER", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "G\u00f6tter sind sie, nicht zum sch\u00fctzen,", "tokens": ["G\u00f6t\u00b7ter", "sind", "sie", ",", "nicht", "zum", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PTKNEG", "APPRART", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber kr\u00e4fftig zum beschnitzen;", "tokens": ["A\u00b7ber", "kr\u00e4ff\u00b7tig", "zum", "be\u00b7schnit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "G\u00f6tter sind sie, die die Christen", "tokens": ["G\u00f6t\u00b7ter", "sind", "sie", ",", "die", "die", "Chris\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenig bauten, sehr verw\u00fcsten;", "tokens": ["We\u00b7nig", "bau\u00b7ten", ",", "sehr", "ver\u00b7w\u00fcs\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "G\u00f6tter sind sie; ihr berauben", "tokens": ["G\u00f6t\u00b7ter", "sind", "sie", ";", "ihr", "be\u00b7rau\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$.", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Soll man noch f\u00fcr Wolthat glauben;", "tokens": ["Soll", "man", "noch", "f\u00fcr", "Wolt\u00b7hat", "glau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "G\u00f6tter sind sie; ihre Plagen", "tokens": ["G\u00f6t\u00b7ter", "sind", "sie", ";", "ih\u00b7re", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Sollen seyn ein Liebe-schlagen;", "tokens": ["Sol\u00b7len", "seyn", "ein", "Lie\u00b7be\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "G\u00f6tter sind sie; wahrem Gotte", "tokens": ["G\u00f6t\u00b7ter", "sind", "sie", ";", "wah\u00b7rem", "Got\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$.", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Als zu Ehren, mehr zu Spotte.", "tokens": ["Als", "zu", "Eh\u00b7ren", ",", "mehr", "zu", "Spot\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}