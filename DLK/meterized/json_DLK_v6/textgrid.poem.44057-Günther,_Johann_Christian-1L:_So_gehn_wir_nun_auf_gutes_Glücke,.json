{"textgrid.poem.44057": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: So gehn wir nun auf gutes Gl\u00fccke,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So gehn wir nun auf gutes Gl\u00fccke,", "tokens": ["So", "gehn", "wir", "nun", "auf", "gu\u00b7tes", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und keiner weis, womit, wohin;", "tokens": ["Und", "kei\u00b7ner", "weis", ",", "wo\u00b7mit", ",", "wo\u00b7hin", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "PTKVZ", "$,", "PWAV", "$,", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jedoch erscheint aus unserm Blicke", "tokens": ["Je\u00b7doch", "er\u00b7scheint", "aus", "un\u00b7serm", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein unmuthsvoller Eigensinn,", "tokens": ["Kein", "un\u00b7muths\u00b7vol\u00b7ler", "Ei\u00b7gen\u00b7sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Welt ist gro\u00df, und Gottes G\u00fcte", "tokens": ["Die", "Welt", "ist", "gro\u00df", ",", "und", "Got\u00b7tes", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Reicht auch noch weit;", "tokens": ["Reicht", "auch", "noch", "weit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Drum lacht mein ruhiges Gem\u00fcthe", "tokens": ["Drum", "lacht", "mein", "ru\u00b7hi\u00b7ges", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Der vorgeworfnen D\u00fcrftigkeit.", "tokens": ["Der", "vor\u00b7ge\u00b7worf\u00b7nen", "D\u00fcrf\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich h\u00f6re wohl den Spott der Leute;", "tokens": ["Ich", "h\u00f6\u00b7re", "wohl", "den", "Spott", "der", "Leu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es \u00e4rgert sie mein Elendsstand,", "tokens": ["Es", "\u00e4r\u00b7gert", "sie", "mein", "E\u00b7lends\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ob ich auf der faulen Seite", "tokens": ["Als", "ob", "ich", "auf", "der", "fau\u00b7len", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Jugend \u00fcbel angewand.", "tokens": ["Die", "Ju\u00b7gend", "\u00fc\u00b7bel", "an\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Vorsicht mag das Urtheil richten;", "tokens": ["Die", "Vor\u00b7sicht", "mag", "das", "Ur\u00b7theil", "rich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich lebe froh", "tokens": ["Ich", "le\u00b7be", "froh"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und dencke, wenn sie was erdichten,", "tokens": ["Und", "den\u00b7cke", ",", "wenn", "sie", "was", "er\u00b7dich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der blinde P\u00f6bel macht es so.", "tokens": ["Der", "blin\u00b7de", "P\u00f6\u00b7bel", "macht", "es", "so", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Natur und Zeit zu meistern suchen,", "tokens": ["Na\u00b7tur", "und", "Zeit", "zu", "meis\u00b7tern", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zeigt allerdings den gr\u00f6bsten Thor.", "tokens": ["Zeigt", "al\u00b7ler\u00b7dings", "den", "gr\u00f6bs\u00b7ten", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer ist so blind und wirft mit Fluchen", "tokens": ["Wer", "ist", "so", "blind", "und", "wirft", "mit", "Flu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Sommer Durst und Hize vor?", "tokens": ["Dem", "Som\u00b7mer", "Durst", "und", "Hi\u00b7ze", "vor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus eben diesen Grundgesezen", "tokens": ["Aus", "e\u00b7ben", "die\u00b7sen", "Grund\u00b7ge\u00b7se\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kommt Wohl und Weh;", "tokens": ["Kommt", "Wohl", "und", "Weh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Was wir vor Gl\u00fcck und Ungl\u00fcck sch\u00e4zen,", "tokens": ["Was", "wir", "vor", "Gl\u00fcck", "und", "Un\u00b7gl\u00fcck", "sch\u00e4\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das hat die Ursach aus der H\u00f6h.", "tokens": ["Das", "hat", "die", "Ur\u00b7sach", "aus", "der", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein Leben f\u00e4llt in tolle Zeiten,", "tokens": ["Mein", "Le\u00b7ben", "f\u00e4llt", "in", "tol\u00b7le", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo niemand mehr als Geld regiert", "tokens": ["Wo", "nie\u00b7mand", "mehr", "als", "Geld", "re\u00b7giert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "KOUS", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wo nunmehr bey allen Leuten", "tokens": ["Und", "wo", "nun\u00b7mehr", "bey", "al\u00b7len", "Leu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Mode fast den Besten schiert;", "tokens": ["Die", "Mo\u00b7de", "fast", "den", "Bes\u00b7ten", "schiert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir aber wallt ein Trieb im Herzen,", "tokens": ["Mir", "a\u00b7ber", "wallt", "ein", "Trieb", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Freyheit liebt,", "tokens": ["Der", "Frey\u00b7heit", "liebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Als die mein Geist im Ernst und Scherzen", "tokens": ["Als", "die", "mein", "Geist", "im", "Ernst", "und", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "PPOSAT", "NN", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sich selber nimmt und andern giebt.", "tokens": ["Sich", "sel\u00b7ber", "nimmt", "und", "an\u00b7dern", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVFIN", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was schnizt ihr unbesonn'nen Richter", "tokens": ["Was", "schnizt", "ihr", "un\u00b7be\u00b7sonn'\u00b7nen", "Rich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Umstand meines Theils nach euch?", "tokens": ["Den", "Um\u00b7stand", "mei\u00b7nes", "Theils", "nach", "euch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So mancher Mensch, so viel Gesichter,", "tokens": ["So", "man\u00b7cher", "Mensch", ",", "so", "viel", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und doch ist keins dem andern gleich.", "tokens": ["Und", "doch", "ist", "keins", "dem", "an\u00b7dern", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIAT", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht anders ist zum Stehn und Fallen", "tokens": ["Nicht", "an\u00b7ders", "ist", "zum", "Stehn", "und", "Fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VAFIN", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Lust und Leid", "tokens": ["In", "Lust", "und", "Leid"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Bey jeglichem und auch bey allen", "tokens": ["Bey", "jeg\u00b7li\u00b7chem", "und", "auch", "bey", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "KON", "ADV", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein allgemeiner Unterscheid.", "tokens": ["Ein", "all\u00b7ge\u00b7mei\u00b7ner", "Un\u00b7ter\u00b7scheid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Immittelst las ich mich nichts irren", "tokens": ["Im\u00b7mit\u00b7telst", "las", "ich", "mich", "nichts", "ir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lebe, wo und wie ich kan.", "tokens": ["Und", "le\u00b7be", ",", "wo", "und", "wie", "ich", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "KON", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Hofnung darf mich nicht verwirren,", "tokens": ["Die", "Hof\u00b7nung", "darf", "mich", "nicht", "ver\u00b7wir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was Gott bescheert, das steht mir an.", "tokens": ["Was", "Gott", "be\u00b7scheert", ",", "das", "steht", "mir", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVPP", "$,", "PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin ein sehr verfolgt Gesch\u00f6pfe,", "tokens": ["Ich", "bin", "ein", "sehr", "ver\u00b7folgt", "Ge\u00b7sch\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADV", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch klag ich nicht,", "tokens": ["Doch", "klag", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wenn gleich der Wahn verkehrter K\u00f6pfe", "tokens": ["Wenn", "gleich", "der", "Wahn", "ver\u00b7kehr\u00b7ter", "K\u00f6p\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das \u00c4rgste von der Unschuld spricht.", "tokens": ["Das", "\u00c4rgs\u00b7te", "von", "der", "Un\u00b7schuld", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Auch bricht der Abend meiner Tage", "tokens": ["Auch", "bricht", "der", "A\u00b7bend", "mei\u00b7ner", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vielleicht noch nicht so bald herein,", "tokens": ["Viel\u00b7leicht", "noch", "nicht", "so", "bald", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es wird auch nach der l\u00e4ngsten Plage", "tokens": ["Es", "wird", "auch", "nach", "der", "l\u00e4ngs\u00b7ten", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor mich ein Ruhplaz \u00fcbrig seyn.", "tokens": ["Vor", "mich", "ein", "Ruh\u00b7plaz", "\u00fcb\u00b7rig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Pilgersdorfer Lustgefilde", "tokens": ["Das", "Pil\u00b7gers\u00b7dor\u00b7fer", "Lust\u00b7ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wirft Tr\u00f6stung zu;", "tokens": ["Wirft", "Tr\u00f6s\u00b7tung", "zu", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die Deutung zeigt uns hier im Bilde:", "tokens": ["Die", "Deu\u00b7tung", "zeigt", "uns", "hier", "im", "Bil\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wir wandeln nach der Armen Ruh.", "tokens": ["Wir", "wan\u00b7deln", "nach", "der", "Ar\u00b7men", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}