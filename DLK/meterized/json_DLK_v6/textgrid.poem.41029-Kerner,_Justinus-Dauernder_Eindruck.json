{"textgrid.poem.41029": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Dauernder Eindruck", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bald mir schwand, als du gegangen,", "tokens": ["Bald", "mir", "schwand", ",", "als", "du", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem Sinn dein Angesicht;", "tokens": ["Aus", "dem", "Sinn", "dein", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob du bleich, ob rot von Wangen,", "tokens": ["Ob", "du", "bleich", ",", "ob", "rot", "von", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$,", "KOUS", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie dein Wuchs? ich wei\u00df es nicht.", "tokens": ["Wie", "dein", "Wuchs", "?", "ich", "wei\u00df", "es", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber auf dem Grund, dem tr\u00fcben,", "tokens": ["A\u00b7ber", "auf", "dem", "Grund", ",", "dem", "tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist mir einzig wunderklar,", "tokens": ["Ist", "mir", "ein\u00b7zig", "wun\u00b7der\u00b7klar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gutes Kind! von dir geblieben", "tokens": ["Gu\u00b7tes", "Kind", "!", "von", "dir", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein gar liebes Augenpaar.", "tokens": ["Ein", "gar", "lie\u00b7bes", "Au\u00b7gen\u00b7paar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wandrer, der im Abendscheine", "tokens": ["Wand\u00b7rer", ",", "der", "im", "A\u00b7bend\u00b7schei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Still hinpilgert durch die Flur,", "tokens": ["Still", "hin\u00b7pil\u00b7gert", "durch", "die", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem erscheint in Au' und Haine", "tokens": ["Dem", "er\u00b7scheint", "in", "Au'", "und", "Hai\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hell das Bildnis der Natur:", "tokens": ["Hell", "das", "Bild\u00b7nis", "der", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber zieht die Sonne ferne,", "tokens": ["A\u00b7ber", "zieht", "die", "Son\u00b7ne", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird es um ihn Nacht zur Stund',", "tokens": ["Wird", "es", "um", "ihn", "Nacht", "zur", "Stund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaut er nichts mehr als die Sterne", "tokens": ["Schaut", "er", "nichts", "mehr", "als", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIS", "PIS", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leuchten auf dem schwarzen Grund.", "tokens": ["Leuch\u00b7ten", "auf", "dem", "schwar\u00b7zen", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}