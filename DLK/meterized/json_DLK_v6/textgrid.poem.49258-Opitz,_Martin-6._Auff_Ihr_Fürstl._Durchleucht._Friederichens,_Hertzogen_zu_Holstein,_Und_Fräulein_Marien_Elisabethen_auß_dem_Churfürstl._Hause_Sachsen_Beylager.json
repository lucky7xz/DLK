{"textgrid.poem.49258": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "6. Auff Ihr F\u00fcrstl. Durchleucht. Friederichens, Hertzogen zu Holstein, Und Fr\u00e4ulein Marien Elisabethen au\u00df dem Churf\u00fcrstl. Hause Sachsen Beylager", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonne, derer sch\u00f6nes Liecht", "tokens": ["Son\u00b7ne", ",", "de\u00b7rer", "sch\u00f6\u00b7nes", "Liecht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PDS", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nunmehr Ei\u00df und Schnee betauet", "tokens": ["Nun\u00b7mehr", "Ei\u00df", "und", "Schnee", "be\u00b7tau\u00b7et"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und de\u00df Winters H\u00e4rte bricht,", "tokens": ["Und", "de\u00df", "Win\u00b7ters", "H\u00e4r\u00b7te", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hastu jemals angeschauet,", "tokens": ["Has\u00b7tu", "je\u00b7mals", "an\u00b7ge\u00b7schau\u00b7et", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das was edlers vor der Zeit", "tokens": ["Das", "was", "ed\u00b7lers", "vor", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "ADJA", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Freyheit hat verfreyt?", "tokens": ["Sei\u00b7ne", "Frey\u00b7heit", "hat", "ver\u00b7freyt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Vatterland, bekenne mir,", "tokens": ["Vat\u00b7ter\u00b7land", ",", "be\u00b7ken\u00b7ne", "mir", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sage mir von gantzem Hertzen,", "tokens": ["Sa\u00b7ge", "mir", "von", "gant\u00b7zem", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hoffest du nicht auch von hier", "tokens": ["Hof\u00b7fest", "du", "nicht", "auch", "von", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Stillung derer Schmertzen,", "tokens": ["Ei\u00b7ne", "Stil\u00b7lung", "de\u00b7rer", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Welche dich bi\u00dfher gekrenckt", "tokens": ["Wel\u00b7che", "dich", "bi\u00df\u00b7her", "ge\u00b7krenckt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VVPP"], "meter": "+-+++-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Und dir deinen Muth gesenckt?", "tokens": ["Und", "dir", "dei\u00b7nen", "Muth", "ge\u00b7senckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun, der H\u00f6chste sey gelobt!", "tokens": ["Nun", ",", "der", "H\u00f6chs\u00b7te", "sey", "ge\u00b7lobt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber ihr, o wilde Waffen,", "tokens": ["A\u00b7ber", "ihr", ",", "o", "wil\u00b7de", "Waf\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ergrimmt ihr habt getobt,", "tokens": ["Wie", "er\u00b7grimmt", "ihr", "habt", "ge\u00b7tobt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dennoch sollt ihr jetzt entschlaffen.", "tokens": ["Den\u00b7noch", "sollt", "ihr", "jetzt", "ent\u00b7schlaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Solche Heyrath kan allein", "tokens": ["Sol\u00b7che", "Hey\u00b7rath", "kan", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht nur eine Heyrath seyn.", "tokens": ["Nicht", "nur", "ei\u00b7ne", "Hey\u00b7rath", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Starcke Raute, gr\u00fcne wol;", "tokens": ["Star\u00b7cke", "Rau\u00b7te", ",", "gr\u00fc\u00b7ne", "wol", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinen s\u00fcssen Bitterkeiten,", "tokens": ["Dei\u00b7nen", "s\u00fcs\u00b7sen", "Bit\u00b7ter\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche nichts bezwingen soll,", "tokens": ["Wel\u00b7che", "nichts", "be\u00b7zwin\u00b7gen", "soll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weiche dieses Gifft der Zeiten,", "tokens": ["Wei\u00b7che", "die\u00b7ses", "Gifft", "der", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses Gifft, das nur zu viel", "tokens": ["Die\u00b7ses", "Gifft", ",", "das", "nur", "zu", "viel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADV", "APPR", "PIAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hertz' und Haupt durchdringen wil.", "tokens": ["Hertz'", "und", "Haupt", "durch\u00b7drin\u00b7gen", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Gr\u00fcne du auch, werthes Par,", "tokens": ["Gr\u00fc\u00b7ne", "du", "auch", ",", "wert\u00b7hes", "Par", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das sich nun zusammen giebet;", "tokens": ["Das", "sich", "nun", "zu\u00b7sam\u00b7men", "gie\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimfe, was vor Hoffnung war,", "tokens": ["Nim\u00b7fe", ",", "was", "vor", "Hoff\u00b7nung", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "APPR", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird jetzt mit der That geliebet,", "tokens": ["Wird", "jetzt", "mit", "der", "That", "ge\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Held, de\u00df Landes Liecht und Schein,", "tokens": ["Held", ",", "de\u00df", "Lan\u00b7des", "Liecht", "und", "Schein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wil dein Liecht alleine seyn.", "tokens": ["Wil", "dein", "Liecht", "al\u00b7lei\u00b7ne", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Diese neue Galathee", "tokens": ["Die\u00b7se", "neu\u00b7e", "Ga\u00b7la\u00b7thee"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird dir Land und Leut' erquicken,", "tokens": ["Wird", "dir", "Land", "und", "Leut'", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird dir deine Cimbersee", "tokens": ["Wird", "dir", "dei\u00b7ne", "Cim\u00b7ber\u00b7see"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit den Stralen \u00fcberblicken,", "tokens": ["Mit", "den", "Stra\u00b7len", "\u00fc\u00b7berb\u00b7li\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit den Stralen, derer Ziehr", "tokens": ["Mit", "den", "Stra\u00b7len", ",", "de\u00b7rer", "Ziehr"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PDS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie Diana gl\u00e4ntzt herf\u00fcr.", "tokens": ["Wie", "Di\u00b7a\u00b7na", "gl\u00e4ntzt", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sey getrost, o Vatterland,", "tokens": ["Sey", "ge\u00b7trost", ",", "o", "Vat\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "O du Himmlisches Gew\u00f6lbe,", "tokens": ["O", "du", "Himm\u00b7li\u00b7sches", "Ge\u00b7w\u00f6l\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Segne dieses Friedenpfand;", "tokens": ["Seg\u00b7ne", "die\u00b7ses", "Frie\u00b7den\u00b7pfand", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauff und eyle dich, o Elbe,", "tokens": ["Lauff", "und", "ey\u00b7le", "dich", ",", "o", "El\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "$,", "FM", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zeig' es deinem Holstein an,", "tokens": ["Zeig'", "es", "dei\u00b7nem", "Hol\u00b7stein", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df es auch sich freuen kan.", "tokens": ["Da\u00df", "es", "auch", "sich", "freu\u00b7en", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Singe fr\u00f6lich, Wild und Wald,", "tokens": ["Sin\u00b7ge", "fr\u00f6\u00b7lich", ",", "Wild", "und", "Wald", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Singe, was sich regt auff Erden,", "tokens": ["Sin\u00b7ge", ",", "was", "sich", "regt", "auff", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kind und Eltern, Jung und Alt,", "tokens": ["Kind", "und", "El\u00b7tern", ",", "Jung", "und", "Alt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Singet: Es wird besser werden,", "tokens": ["Sin\u00b7get", ":", "Es", "wird", "bes\u00b7ser", "wer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VAFIN", "ADJD", "VAINF", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Singt: Der Lentz verj\u00fcngt das Feld", "tokens": ["Singt", ":", "Der", "Lentz", "ver\u00b7j\u00fcngt", "das", "Feld"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Rautenstrauch die Welt.", "tokens": ["Und", "der", "Rau\u00b7ten\u00b7strauch", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}