{"textgrid.poem.40442": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "W\u00fcrzburger Festlied", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Julius Echter von Mespelbrunn,", "tokens": ["Herr", "Ju\u00b7lius", "Ech\u00b7ter", "von", "Me\u00b7spel\u00b7brunn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fcrstbischof und Herzog in Franken,", "tokens": ["F\u00fcrst\u00b7bi\u00b7schof", "und", "Her\u00b7zog", "in", "Fran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "APPR", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Trank seinen Becher Leisten und sprach:", "tokens": ["Trank", "sei\u00b7nen", "Be\u00b7cher", "Leis\u00b7ten", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "\u00bbmir kommt ein guter Gedanken:", "tokens": ["\u00bb", "mir", "kommt", "ein", "gu\u00b7ter", "Ge\u00b7dan\u00b7ken", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Meine W\u00fcrzburger Gl\u00f6cklein", "tokens": ["Mei\u00b7ne", "W\u00fcrz\u00b7bur\u00b7ger", "Gl\u00f6c\u00b7klein"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Haben sch\u00f6nes Gel\u00e4ut,", "tokens": ["Ha\u00b7ben", "sch\u00f6\u00b7nes", "Ge\u00b7l\u00e4ut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und die W\u00fcrzburger M\u00e4gdlein", "tokens": ["Und", "die", "W\u00fcrz\u00b7bur\u00b7ger", "M\u00e4gd\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sind kreuzbrave Leut',", "tokens": ["Sind", "kreuz\u00b7bra\u00b7ve", "Leut'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Jetzt fehlt nur noch eines:", "tokens": ["Jetzt", "fehlt", "nur", "noch", "ei\u00b7nes", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PIS", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Stadt ist zu leer,", "tokens": ["Die", "Stadt", "ist", "zu", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich schaff' etwas Feines:", "tokens": ["Ich", "schaff'", "et\u00b7was", "Fei\u00b7nes", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Eine Hochschul' mu\u00df her!\u00ab", "tokens": ["Ei\u00b7ne", "Hoch\u00b7schul'", "mu\u00df", "her", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Herr Julius Echter von Mespelbrunn,", "tokens": ["Herr", "Ju\u00b7lius", "Ech\u00b7ter", "von", "Me\u00b7spel\u00b7brunn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fcrstbischof und Herzog in Franken,", "tokens": ["F\u00fcrst\u00b7bi\u00b7schof", "und", "Her\u00b7zog", "in", "Fran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "APPR", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Sprach weiter: \u00bbEin deutscher Mann und Christ", "tokens": ["Sprach", "wei\u00b7ter", ":", "\u00bb", "Ein", "deut\u00b7scher", "Mann", "und", "Christ"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "$(", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Denkt auch der Armen und Kranken.", "tokens": ["Denkt", "auch", "der", "Ar\u00b7men", "und", "Kran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Zwar heilt uns am besten", "tokens": ["Zwar", "heilt", "uns", "am", "bes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ein fr\u00f6hlicher Sinn,", "tokens": ["Ein", "fr\u00f6h\u00b7li\u00b7cher", "Sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und Mainwein und Steinwein", "tokens": ["Und", "Main\u00b7wein", "und", "Stein\u00b7wein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sind auch Medizin.", "tokens": ["Sind", "auch", "Me\u00b7di\u00b7zin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Doch wenn der Mensch krank ist,", "tokens": ["Doch", "wenn", "der", "Mensch", "krank", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wird die Welt ihm zur Qual,", "tokens": ["Wird", "die", "Welt", "ihm", "zur", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Drum stift' ich zur Hochschul'", "tokens": ["Drum", "stift'", "ich", "zur", "Hoch\u00b7schul'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das Julius-Spital.\u00ab", "tokens": ["Das", "Ju\u00b7li\u00b7us\u00b7Spi\u00b7tal", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Herr Julius Echter von Mespelbrunn,", "tokens": ["Herr", "Ju\u00b7lius", "Ech\u00b7ter", "von", "Me\u00b7spel\u00b7brunn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fcrstbischof und Herzog in Franken,", "tokens": ["F\u00fcrst\u00b7bi\u00b7schof", "und", "Her\u00b7zog", "in", "Fran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "APPR", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Als Domherrngeiz nichts steuern wollt',", "tokens": ["Als", "Dom\u00b7herrn\u00b7geiz", "nichts", "steu\u00b7ern", "wollt'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tat stramm er sie verzanken:", "tokens": ["Tat", "stramm", "er", "sie", "ver\u00b7zan\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbihr wollt mich vexieren?!", "tokens": ["\u00bb", "ihr", "wollt", "mich", "ve\u00b7xie\u00b7ren", "?!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Aus Nichts wird ja Nichts;", "tokens": ["Aus", "Nichts", "wird", "ja", "Nichts", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "ADV", "PIS", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Tut auf eure Truhen", "tokens": ["Tut", "auf", "eu\u00b7re", "Tru\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "F\u00fcr Werke des Lichts!", "tokens": ["F\u00fcr", "Wer\u00b7ke", "des", "Lichts", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Das Hochstift braucht Umgeld", "tokens": ["Das", "Hoch\u00b7stift", "braucht", "Um\u00b7geld"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und viel Kapital,", "tokens": ["Und", "viel", "Ka\u00b7pi\u00b7tal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df Schnabelweid werde", "tokens": ["Da\u00df", "Schna\u00b7bel\u00b7weid", "wer\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "VAFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "F\u00fcr Schul' und Spital!\u00ab", "tokens": ["F\u00fcr", "Schul'", "und", "Spi\u00b7tal", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-++", "measure": "unknown.measure.tri"}}, "stanza.10": {"line.1": {"text": "Herr Julius Echter von Mespelbrunn,", "tokens": ["Herr", "Ju\u00b7lius", "Ech\u00b7ter", "von", "Me\u00b7spel\u00b7brunn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fcrstbischof und Herzog in Franken,", "tokens": ["F\u00fcrst\u00b7bi\u00b7schof", "und", "Her\u00b7zog", "in", "Fran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "APPR", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Hing stolz den Rektormantel um", "tokens": ["Hing", "stolz", "den", "Rek\u00b7tor\u00b7man\u00b7tel", "um"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Purpur und goldenen Ranken;", "tokens": ["Mit", "Pur\u00b7pur", "und", "gol\u00b7de\u00b7nen", "Ran\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.11": {"line.1": {"text": "Denn er war ja selber", "tokens": ["Denn", "er", "war", "ja", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wohl an die zehn Jahr'", "tokens": ["Wohl", "an", "die", "zehn", "Jahr'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "CARD", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "In Paris und Pavia", "tokens": ["In", "Pa\u00b7ris", "und", "Pa\u00b7via"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NE"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ein tapfrer Scholar.", "tokens": ["Ein", "tapf\u00b7rer", "Scho\u00b7lar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.12": {"line.1": {"text": "Und was so ein frommes,", "tokens": ["Und", "was", "so", "ein", "from\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ART", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Bemoostes Haupt schafft,", "tokens": ["Be\u00b7moos\u00b7tes", "Haupt", "schafft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Das hat f\u00fcr die Nachwelt", "tokens": ["Das", "hat", "f\u00fcr", "die", "Nach\u00b7welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Noch Segen und Kraft.", "tokens": ["Noch", "Se\u00b7gen", "und", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "i