word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sind kreuzbrave Leut'.", "tokens": ["Sind", "kreuz\u00b7bra\u00b7ve", "Leut'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.15": {"line.1": {"text": "Und die Alma Frau Julia", "tokens": ["Und", "die", "Al\u00b7ma", "Frau", "Ju\u00b7lia"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NE", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt strahlend stolziert:", "tokens": ["Kommt", "strah\u00b7lend", "stol\u00b7ziert", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "\u00bbhoch lebe ein jeder,", "tokens": ["\u00bb", "hoch", "le\u00b7be", "ein", "je\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "ART", "PIS", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Der mitjubiliert!\u00ab", "tokens": ["Der", "mit\u00b7ju\u00b7bi\u00b7liert", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "VVPP", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}