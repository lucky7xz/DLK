{"dta.poem.10168": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Norder-Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ist es doch so hell? was ist es f\u00fcr ein Licht,", "tokens": ["Wie", "ist", "es", "doch", "so", "hell", "?", "was", "ist", "es", "f\u00fcr", "ein", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$.", "PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das, da der Mond nicht scheint, durch dunckle", "tokens": ["Das", ",", "da", "der", "Mond", "nicht", "scheint", ",", "durch", "dunck\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "$,", "KOUS", "ART", "NN", "PTKNEG", "VVFIN", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So dacht ich, als ich j\u00fcngst, fast mitten in der Nacht,", "tokens": ["So", "dacht", "ich", ",", "als", "ich", "j\u00fcngst", ",", "fast", "mit\u00b7ten", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "$,", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus meinem Fenster sah. Doch wie ward mir zu Muth,", "tokens": ["Aus", "mei\u00b7nem", "Fens\u00b7ter", "sah", ".", "Doch", "wie", "ward", "mir", "zu", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "KON", "PWAV", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als mein best\u00fcrtzter Blick sich in die H\u00f6he zog,", "tokens": ["Als", "mein", "be\u00b7st\u00fcrtz\u00b7ter", "Blick", "sich", "in", "die", "H\u00f6\u00b7he", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und, da\u00df ein\u2019 allgemeine Gluth", "tokens": ["Und", ",", "da\u00df", "ein'", "all\u00b7ge\u00b7mei\u00b7ne", "Gluth"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch alle Himmels-Theile flog,", "tokens": ["Durch", "al\u00b7le", "Him\u00b7mels\u00b7T\u00b7hei\u00b7le", "flog", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Recht mit Entsetzen fand!", "tokens": ["Recht", "mit", "Ent\u00b7set\u00b7zen", "fand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Offt fliegt ein schneller Rauch, offt l\u00e4ssts, als ob von", "tokens": ["Offt", "fliegt", "ein", "schnel\u00b7ler", "Rauch", ",", "offt", "l\u00e4ssts", ",", "als", "ob", "von"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "$,", "KOKOM", "KOUS", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich, Streiffen-weis\u2019 ein d\u00fcnn Gest\u00f6ber seh.", "tokens": ["Ich", ",", "Streif\u00b7fen\u00b7weis'", "ein", "d\u00fcnn", "Ge\u00b7st\u00f6\u00b7ber", "seh", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es blitzt\u2019, es strahlt\u2019, es scho\u00df", "tokens": ["Es", "blitzt'", ",", "es", "strahlt'", ",", "es", "scho\u00df"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein wildes Feur durchs gantze Firmament.", "tokens": ["Ein", "wil\u00b7des", "Feur", "durchs", "gant\u00b7ze", "Fir\u00b7ma\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein wallend Flammen-Meer ergo\u00df,", "tokens": ["Ein", "wal\u00b7lend", "Flam\u00b7men\u00b7Meer", "er\u00b7go\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit einem dicken Schwall,", "tokens": ["Mit", "ei\u00b7nem", "di\u00b7cken", "Schwall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sich wie ein Blitz offt \u00fcberall.", "tokens": ["Sich", "wie", "ein", "Blitz", "offt", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KOKOM", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Offt schien die schnelle Fluth zertrennt", "tokens": ["Offt", "schien", "die", "schnel\u00b7le", "Fluth", "zer\u00b7trennt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In grossen Str\u00f6men fortzueilen;", "tokens": ["In", "gros\u00b7sen", "Str\u00f6\u00b7men", "fort\u00b7zu\u00b7ei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Bald waren Gluth und Fluth verschwunden;", "tokens": ["Bald", "wa\u00b7ren", "Gluth", "und", "Fluth", "ver\u00b7schwun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die aber, wie der Blitz, geschwind aufs nen entstunden,", "tokens": ["Die", "a\u00b7ber", ",", "wie", "der", "Blitz", ",", "ge\u00b7schwind", "aufs", "nen", "ent\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PWAV", "ART", "NN", "$,", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.12": {"text": "Aufs neue w\u00fcteten, mit Strahl- und Feuer-Pfeilen", "tokens": ["Aufs", "neu\u00b7e", "w\u00fc\u00b7te\u00b7ten", ",", "mit", "Strahl", "und", "Feu\u00b7er\u00b7Pfei\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VVFIN", "$,", "APPR", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Begleitet und vermengt. Ein f\u00fcrchterliches Wittern,", "tokens": ["Be\u00b7glei\u00b7tet", "und", "ver\u00b7mengt", ".", "Ein", "f\u00fcrch\u00b7ter\u00b7li\u00b7ches", "Wit\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein unbeschreiblich streng, offt wiederhohltes, Zittern", "tokens": ["Ein", "un\u00b7be\u00b7schreib\u00b7lich", "streng", ",", "offt", "wie\u00b7der\u00b7hohl\u00b7tes", ",", "Zit\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJD", "VVFIN", "$,", "ADV", "ADJA", "$,", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ersch\u00fctterte, nebst allen Himmels-Theilen,", "tokens": ["Er\u00b7sch\u00fct\u00b7ter\u00b7te", ",", "nebst", "al\u00b7len", "Him\u00b7mels\u00b7T\u00b7hei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Auch mein beklemmtes Hertz. Denn ob mir gleich der", "tokens": ["Auch", "mein", "be\u00b7klemm\u00b7tes", "Hertz", ".", "Denn", "ob", "mir", "gleich", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$.", "KON", "KOUS", "PPER", "ADV", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Da\u00df es das Norder-Licht, nicht unbekannt;", "tokens": ["Da\u00df", "es", "das", "Nor\u00b7der\u00b7Licht", ",", "nicht", "un\u00b7be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "So war iedoch das strahlende bewegen", "tokens": ["So", "war", "ie\u00b7doch", "das", "strah\u00b7len\u00b7de", "be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Des gantzen Firmaments so hefftig; da\u00df ich mich", "tokens": ["Des", "gant\u00b7zen", "Fir\u00b7ma\u00b7ments", "so", "heff\u00b7tig", ";", "da\u00df", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "$.", "KOUS", "PPER", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Zu dencken, wie hier folgt, nicht kunnt\u2019 entlegen:", "tokens": ["Zu", "den\u00b7cken", ",", "wie", "hier", "folgt", ",", "nicht", "kunnt'", "ent\u00b7le\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "ADV", "VVFIN", "$,", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Wie ist mir? schwindelt mir? zertheilet sich, zerf\u00e4llt", "tokens": ["Wie", "ist", "mir", "?", "schwin\u00b7delt", "mir", "?", "zer\u00b7thei\u00b7let", "sich", ",", "zer\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "VVFIN", "PPER", "$.", "VVFIN", "PRF", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der gantze Bau der Ober-Welt?", "tokens": ["Der", "gant\u00b7ze", "Bau", "der", "O\u00b7ber\u00b7Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Lodernde Flammen mit wallenden Blitzen,", "tokens": ["Lo\u00b7dern\u00b7de", "Flam\u00b7men", "mit", "wal\u00b7len\u00b7den", "Blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Fliegende D\u00fcffte, voll strahlender Spitzen,", "tokens": ["Flie\u00b7gen\u00b7de", "D\u00fcff\u00b7te", ",", "voll", "strah\u00b7len\u00b7der", "Spit\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Circkeln sich, wirbeln sich, schiessen zusammen;", "tokens": ["Cir\u00b7ckeln", "sich", ",", "wir\u00b7beln", "sich", ",", "schies\u00b7sen", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PRF", "$,", "VVFIN", "PRF", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Leuchten und schrecken, verschwinden, entstehn,", "tokens": ["Leuch\u00b7ten", "und", "schre\u00b7cken", ",", "ver\u00b7schwin\u00b7den", ",", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Wallen und wittern, erscheinen, vergehn.", "tokens": ["Wal\u00b7len", "und", "wit\u00b7tern", ",", "er\u00b7schei\u00b7nen", ",", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Allein:", "tokens": ["Al\u00b7lein", ":"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Dort zeigt sich gar ein bunter Blitz und Schein.", "tokens": ["Dort", "zeigt", "sich", "gar", "ein", "bun\u00b7ter", "Blitz", "und", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gelb, feurig, gr\u00fcn und blau", "tokens": ["Gelb", ",", "feu\u00b7rig", ",", "gr\u00fcn", "und", "blau"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "F\u00e4rbt sich ein Flammen-Heer.", "tokens": ["F\u00e4rbt", "sich", "ein", "Flam\u00b7men\u00b7Heer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es schrecket und ergetzt zugleich, die bunte Gluth.", "tokens": ["Es", "schre\u00b7cket", "und", "er\u00b7getzt", "zu\u00b7gleich", ",", "die", "bun\u00b7te", "Gluth", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Recht wie die Wellen sich, in einer wilden Fluth,", "tokens": ["Recht", "wie", "die", "Wel\u00b7len", "sich", ",", "in", "ei\u00b7ner", "wil\u00b7den", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "PRF", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Best\u00fcrmen, fressen und verdringen;", "tokens": ["Be\u00b7st\u00fcr\u00b7men", ",", "fres\u00b7sen", "und", "ver\u00b7drin\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So sieht man hier, im bunten Feuer-Meer,", "tokens": ["So", "sieht", "man", "hier", ",", "im", "bun\u00b7ten", "Feu\u00b7er\u00b7Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die regen Flammen sich verschlingen.", "tokens": ["Die", "re\u00b7gen", "Flam\u00b7men", "sich", "ver\u00b7schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was aber mag doch wol der Schein", "tokens": ["Was", "a\u00b7ber", "mag", "doch", "wol", "der", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VMFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Recht eigentlich, und was die Ursach seyn?", "tokens": ["Recht", "ei\u00b7gent\u00b7lich", ",", "und", "was", "die", "Ur\u00b7sach", "seyn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "PWS", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf! auf! mein Geist, du must dich aufw\u00e4rts schwingen!", "tokens": ["Auf", "!", "auf", "!", "mein", "Geist", ",", "du", "must", "dich", "auf\u00b7w\u00e4rts", "schwin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "PTKVZ", "$.", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bestrebe dich, mit Ehr-Furcht, in die Tieffe", "tokens": ["Be\u00b7stre\u00b7be", "dich", ",", "mit", "Ehr\u00b7Furcht", ",", "in", "die", "Tief\u00b7fe"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der wirckenden Natur zu dringen,", "tokens": ["Der", "wir\u00b7cken\u00b7den", "Na\u00b7tur", "zu", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu unsers Sch\u00f6pfers Preis\u2019; \u00fcm auch in diesen Dingen", "tokens": ["Zu", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "Preis'", ";", "\u00fcm", "auch", "in", "die\u00b7sen", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$.", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sein\u2019 Allmacht, Seine Lieb\u2019 und Weisheit zu besingen.", "tokens": ["Sein'", "All\u00b7macht", ",", "Sei\u00b7ne", "Lieb'", "und", "Weis\u00b7heit", "zu", "be\u00b7sin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die\u00df wird, wenn auch ein Fehl mit unterlieffe,", "tokens": ["Die\u00df", "wird", ",", "wenn", "auch", "ein", "Fehl", "mit", "un\u00b7ter\u00b7lief\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "KOUS", "ADV", "ART", "NN", "APPR", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Jhm hoffentlich doch nicht zuwieder seyn.", "tokens": ["Jhm", "hof\u00b7fent\u00b7lich", "doch", "nicht", "zu\u00b7wie\u00b7der", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Es scheinet zwar von diesem Lufft-Gesichte,", "tokens": ["Es", "schei\u00b7net", "zwar", "von", "die\u00b7sem", "Lufft\u00b7Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Worauf ich nun mein dencken richte,", "tokens": ["Wo\u00b7rauf", "ich", "nun", "mein", "den\u00b7cken", "rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Ursach diese: Wenn die Nacht", "tokens": ["Die", "Ur\u00b7sach", "die\u00b7se", ":", "Wenn", "die", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "$.", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch noch so schwartz, so dunckel und so dicht;", "tokens": ["Auch", "noch", "so", "schwartz", ",", "so", "dun\u00b7ckel", "und", "so", "dicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So ist dennoch, vom Sonnen-Licht", "tokens": ["So", "ist", "den\u00b7noch", ",", "vom", "Son\u00b7nen\u00b7Licht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihrer immer hellen Pracht,", "tokens": ["Und", "ih\u00b7rer", "im\u00b7mer", "hel\u00b7len", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das gantze Firmament best\u00e4ndig angef\u00fcllt:", "tokens": ["Das", "gant\u00b7ze", "Fir\u00b7ma\u00b7ment", "be\u00b7st\u00e4n\u00b7dig", "an\u00b7ge\u00b7f\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ob gleich der Schatten unsrer Erden,", "tokens": ["Ob", "gleich", "der", "Schat\u00b7ten", "uns\u00b7rer", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der, durch die Dichtigkeit derselben, uns \u00fcmh\u00fcllt,", "tokens": ["Der", ",", "durch", "die", "Dich\u00b7tig\u00b7keit", "der\u00b7sel\u00b7ben", ",", "uns", "\u00fcm\u00b7h\u00fcllt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "PDAT", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das Licht nicht l\u00e4sset sichtbar werden,", "tokens": ["Das", "Licht", "nicht", "l\u00e4s\u00b7set", "sicht\u00b7bar", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Als welches, sonder Gegenschlag,", "tokens": ["Als", "wel\u00b7ches", ",", "son\u00b7der", "Ge\u00b7gen\u00b7schlag", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Auf unser Aug\u2019 zu wircken nicht vermag.", "tokens": ["Auf", "un\u00b7ser", "Aug'", "zu", "wir\u00b7cken", "nicht", "ver\u00b7mag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Daher nun kommt es mir", "tokens": ["Da\u00b7her", "nun", "kommt", "es", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Nicht unwahrscheinlich f\u00fcr,", "tokens": ["Nicht", "un\u00b7wahr\u00b7schein\u00b7lich", "f\u00fcr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Da\u00df etwa D\u00fcnste sich zu solcher H\u00f6h\u2019 geschwungen,", "tokens": ["Da\u00df", "et\u00b7wa", "D\u00fcns\u00b7te", "sich", "zu", "sol\u00b7cher", "H\u00f6h'", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "PRF", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df sie den Schatten durchgedrungen,", "tokens": ["Da\u00df", "sie", "den", "Schat\u00b7ten", "durch\u00b7ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Den unser Erd-Kreis macht: wodurch sie, von dem Schein", "tokens": ["Den", "un\u00b7ser", "Erd\u00b7Kreis", "macht", ":", "wo\u00b7durch", "sie", ",", "von", "dem", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$.", "PWAV", "PPER", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Des Sonnen-Lichts so dann getroffen, sichtbar seyn.", "tokens": ["Des", "Son\u00b7nen\u00b7Lichts", "so", "dann", "ge\u00b7trof\u00b7fen", ",", "sicht\u00b7bar", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVPP", "$,", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Weil dieses gar zu fern, f\u00e4llt mir ein\u2019 Ursach ein,", "tokens": ["Weil", "die\u00b7ses", "gar", "zu", "fern", ",", "f\u00e4llt", "mir", "ein'", "Ur\u00b7sach", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "PTKA", "ADJD", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die n\u00e4her ist. Vielleicht kann dieses Licht entstehen", "tokens": ["Die", "n\u00e4\u00b7her", "ist", ".", "Viel\u00b7leicht", "kann", "die\u00b7ses", "Licht", "ent\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VAFIN", "$.", "ADV", "VMFIN", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus D\u00fcnsten, die voll Saltz, und die den Theilchen gleich,", "tokens": ["Aus", "D\u00fcn