{"textgrid.poem.34895": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle Liebesg\u00f6tter jauchzen", "tokens": ["Al\u00b7le", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", "jauch\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir im Herzen, und Fanfare", "tokens": ["Mir", "im", "Her\u00b7zen", ",", "und", "Fan\u00b7fa\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "APPRART", "NN", "$,", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blasen sie und rufen: \u00bbHeil!", "tokens": ["Bla\u00b7sen", "sie", "und", "ru\u00b7fen", ":", "\u00bb", "Heil", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVINF", "$.", "$(", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heil, der K\u00f6nigin Pomare!\u00ab", "tokens": ["Heil", ",", "der", "K\u00f6\u00b7ni\u00b7gin", "Po\u00b7ma\u00b7re", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "ART", "NN", "NE", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jene nicht von Otahaiti \u2013", "tokens": ["Je\u00b7ne", "nicht", "von", "O\u00b7ta\u00b7hai\u00b7ti", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "APPR", "NE", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mission\u00e4risiert ist jene \u2013", "tokens": ["Mis\u00b7si\u00b7o\u00b7n\u00e4\u00b7ri\u00b7siert", "ist", "je\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PDAT", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die ich meine, die ist wild,", "tokens": ["Die", "ich", "mei\u00b7ne", ",", "die", "ist", "wild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PRELS", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine ungez\u00e4hmte Sch\u00f6ne.", "tokens": ["Ei\u00b7ne", "un\u00b7ge\u00b7z\u00e4hm\u00b7te", "Sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zweimal in der Woche zeigt sie", "tokens": ["Zwei\u00b7mal", "in", "der", "Wo\u00b7che", "zeigt", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00d6ffentlich sich ihrem Volke", "tokens": ["\u00d6f\u00b7fent\u00b7lich", "sich", "ih\u00b7rem", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PRF", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In dem Garten Mabill, tanzt", "tokens": ["In", "dem", "Gar\u00b7ten", "Ma\u00b7bill", ",", "tanzt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "NE", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort den Cancan, auch die Polke.", "tokens": ["Dort", "den", "Can\u00b7can", ",", "auch", "die", "Pol\u00b7ke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "$,", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Majest\u00e4t in jedem Schritte,", "tokens": ["Ma\u00b7jes\u00b7t\u00e4t", "in", "je\u00b7dem", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jede Beugung Huld und Gnade,", "tokens": ["Je\u00b7de", "Beu\u00b7gung", "Huld", "und", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine F\u00fcrstin jeder Zoll", "tokens": ["Ei\u00b7ne", "F\u00fcrs\u00b7tin", "je\u00b7der", "Zoll"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der H\u00fcfte bis zur Wade \u2013", "tokens": ["Von", "der", "H\u00fcf\u00b7te", "bis", "zur", "Wa\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Also tanzt sie \u2013 und es blasen", "tokens": ["Al\u00b7so", "tanzt", "sie", "\u2013", "und", "es", "bla\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KON", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebesg\u00f6tter die Fanfare", "tokens": ["Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", "die", "Fan\u00b7fa\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir im Herzen, rufen: \u00bbHeil!", "tokens": ["Mir", "im", "Her\u00b7zen", ",", "ru\u00b7fen", ":", "\u00bb", "Heil", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "VVFIN", "$.", "$(", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heil der K\u00f6nigin Pomare!\u00ab", "tokens": ["Heil", "der", "K\u00f6\u00b7ni\u00b7gin", "Po\u00b7ma\u00b7re", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "NE", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}