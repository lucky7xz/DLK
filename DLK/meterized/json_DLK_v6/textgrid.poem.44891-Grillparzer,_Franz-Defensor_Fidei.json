{"textgrid.poem.44891": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Defensor Fidei", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.71", "no:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Hermes ist er spinnefeind,", "tokens": ["Dem", "Her\u00b7mes", "ist", "er", "spin\u00b7ne\u00b7feind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der vertrakte Jarke.", "tokens": ["Der", "ver\u00b7trak\u00b7te", "Jar\u00b7ke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hetzen wir ihn denn zum Spa\u00df", "tokens": ["Het\u00b7zen", "wir", "ihn", "denn", "zum", "Spa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und lassen ihn dann im Quarke.", "tokens": ["Und", "las\u00b7sen", "ihn", "dann", "im", "Quar\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}