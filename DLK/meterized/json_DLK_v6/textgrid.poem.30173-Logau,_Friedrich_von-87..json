{"textgrid.poem.30173": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "87.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gedencken magst du alles, nicht alles darffstu sagen;", "tokens": ["Ge\u00b7den\u00b7cken", "magst", "du", "al\u00b7les", ",", "nicht", "al\u00b7les", "darffs\u00b7tu", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PIS", "$,", "PTKNEG", "PIS", "PAV", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das sagen pfleget Busse, das dencken nicht zutragen,", "tokens": ["Das", "sa\u00b7gen", "pfle\u00b7get", "Bus\u00b7se", ",", "das", "den\u00b7cken", "nicht", "zu\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VVFIN", "NN", "$,", "PDS", "VVFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wil nur nicht dein Gewissen dich f\u00fcr dir selbst beklagen.", "tokens": ["Wil", "nur", "nicht", "dein", "Ge\u00b7wis\u00b7sen", "dich", "f\u00fcr", "dir", "selbst", "be\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "PRF", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}