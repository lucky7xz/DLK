{"dta.poem.716": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Ii.  An Gott den heiligen Geist.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Bjsher hab ich die alte kalte Welt/ ", "tokens": ["Bjsher", "hab", "ich", "die", "al\u00b7te", "kal\u00b7te", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bisher hab ", "tokens": ["Bis\u00b7her", "hab"], "token_info": ["word", "word"], "pos": ["ADV", "VAFIN"], "meter": "--+", "measure": "anapaest.init"}, "line.3": {"text": "Bisher hat mich der harte sturm betr\u00fcbet.", "tokens": ["Bis\u00b7her", "hat", "mich", "der", "har\u00b7te", "sturm", "be\u00b7tr\u00fc\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich der ich falschem gutte nachgestelt. ", "tokens": ["Mich", "der", "ich", "fal\u00b7schem", "gut\u00b7te", "nach\u00b7ge\u00b7stelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PPER", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kom reiner Geist/ entz\u00fcnde meine k\u00e4lt.", "tokens": ["Kom", "rei\u00b7ner", "Geist", "/", "ent\u00b7z\u00fcn\u00b7de", "mei\u00b7ne", "k\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$(", "VVFIN", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zureis das band das meine Seel\u2019 vmbgiebet", "tokens": ["Zu\u00b7reis", "das", "band", "das", "mei\u00b7ne", "Seel'", "vmb\u00b7gie\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "VVFIN", "PDS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergib was ich f\u00fcr missethat ver\u00fcbet/", "tokens": ["Ver\u00b7gib", "was", "ich", "f\u00fcr", "mis\u00b7se\u00b7that", "ver\u00b7\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PWS", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vnd tr\u00f6ste wenn mein Hertz in schmertz verfelt.", "tokens": ["Vnd", "tr\u00f6s\u00b7te", "wenn", "mein", "Hertz", "in", "schmertz", "ver\u00b7felt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "PPOSAT", "NN", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O Helles licht/ erleuchte meine Nacht", "tokens": ["O", "Hel\u00b7les", "licht", "/", "er\u00b7leuch\u00b7te", "mei\u00b7ne", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "NN", "$(", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Nacht voll angst/ vol wehmut/ ach vnd zagen", "tokens": ["Die", "Nacht", "voll", "angst", "/", "vol", "weh\u00b7mut", "/", "ach", "vnd", "za\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVPP", "$(", "ADJD", "NN", "$(", "XY", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erquicke mich eh' als mein Geist verschmacht. ", "tokens": ["Er\u00b7qui\u00b7cke", "mich", "eh'", "als", "mein", "Geist", "ver\u00b7schmacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "KOKOM", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O wahre lust wie da\u00df ich trawrig bin?", "tokens": ["O", "wah\u00b7re", "lust", "wie", "da\u00df", "ich", "traw\u00b7rig", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KOKOM", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Weil du nicht hier/ mu\u00df ich so hefftig klagen. ", "tokens": ["Weil", "du", "nicht", "hier", "/", "mu\u00df", "ich", "so", "heff\u00b7tig", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "$(", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein beysein nimm't all' angst v&#241; trawren hin. ", "tokens": ["Dein", "bey\u00b7se\u00b7in", "nim\u00b7m't", "all'", "angst", "v", "&#241;", "traw\u00b7ren", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "XY", "$(", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}