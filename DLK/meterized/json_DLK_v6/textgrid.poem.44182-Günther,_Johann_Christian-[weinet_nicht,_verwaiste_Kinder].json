{"textgrid.poem.44182": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[weinet nicht, verwaiste Kinder]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weinet nicht, verwaiste Kinder,", "tokens": ["Wei\u00b7net", "nicht", ",", "ver\u00b7wais\u00b7te", "Kin\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da der Himmel wieder lacht;", "tokens": ["Da", "der", "Him\u00b7mel", "wie\u00b7der", "lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht, des Ungl\u00fccks tr\u00fcbe Nacht", "tokens": ["Seht", ",", "des", "Un\u00b7gl\u00fccks", "tr\u00fc\u00b7be", "Nacht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat den Tag zum \u00dcberwinder,", "tokens": ["Hat", "den", "Tag", "zum", "\u00dc\u00b7berw\u00b7in\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An welchem Gl\u00fcck und Sonnenschein", "tokens": ["An", "wel\u00b7chem", "Gl\u00fcck", "und", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Einander der Verschwendung zeihn.", "tokens": ["Ein\u00b7an\u00b7der", "der", "Ver\u00b7schwen\u00b7dung", "zeihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Schweigt, ihre bangen Klagelieder,", "tokens": ["Schweigt", ",", "ih\u00b7re", "ban\u00b7gen", "Kla\u00b7ge\u00b7lie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sucht, ihr Thr\u00e4nen, euer Grab!", "tokens": ["Sucht", ",", "ihr", "Thr\u00e4\u00b7nen", ",", "eu\u00b7er", "Grab", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zion legt die Trauer ab,", "tokens": ["Zi\u00b7on", "legt", "die", "Trau\u00b7er", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zion nimmt den Brautschmuck wieder", "tokens": ["Zi\u00b7on", "nimmt", "den", "Brautsc\u00b7hmuck", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und cr\u00f6nt den hei\u00dfen Danckaltar,", "tokens": ["Und", "cr\u00f6nt", "den", "hei\u00b7\u00dfen", "Dan\u00b7ckal\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der schon mit Moos bewachsen war.", "tokens": ["Der", "schon", "mit", "Moos", "be\u00b7wach\u00b7sen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Heute wird die stumme W\u00fcste", "tokens": ["Heu\u00b7te", "wird", "die", "stum\u00b7me", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Des betr\u00fcbten Helicons,", "tokens": ["Des", "be\u00b7tr\u00fcb\u00b7ten", "He\u00b7li\u00b7cons", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Des best\u00fcrzten Libanons", "tokens": ["Des", "be\u00b7st\u00fcrz\u00b7ten", "Li\u00b7ba\u00b7nons"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein beredtes Schauger\u00fcste,", "tokens": ["Ein", "be\u00b7red\u00b7tes", "Schau\u00b7ge\u00b7r\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von dem der Musen Jubelfest", "tokens": ["Von", "dem", "der", "Mu\u00b7sen", "Ju\u00b7bel\u00b7fest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Traurigkeit verweisen l\u00e4st.", "tokens": ["Die", "Trau\u00b7rig\u00b7keit", "ver\u00b7wei\u00b7sen", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Neulich, als der Gottheit Rache", "tokens": ["Neu\u00b7lich", ",", "als", "der", "Got\u00b7theit", "Ra\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsern Hirten pl\u00f6zlich traf,", "tokens": ["Un\u00b7sern", "Hir\u00b7ten", "pl\u00f6z\u00b7lich", "traf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lehrte fast ein jedes Schaaf,", "tokens": ["Lehr\u00b7te", "fast", "ein", "je\u00b7des", "Schaaf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df des H\u00f6chsten Zorn erwache,", "tokens": ["Da\u00df", "des", "H\u00f6chs\u00b7ten", "Zorn", "er\u00b7wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wir erfuhren, als er schlug,", "tokens": ["Und", "wir", "er\u00b7fuh\u00b7ren", ",", "als", "er", "schlug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Warum sein Arm den Donner trug.", "tokens": ["Wa\u00b7rum", "sein", "Arm", "den", "Don\u00b7ner", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch der V\u00e4ter kluges Wehlen", "tokens": ["Doch", "der", "V\u00e4\u00b7ter", "klu\u00b7ges", "Weh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00c4ndert unsern Waysenstand,", "tokens": ["\u00c4n\u00b7dert", "un\u00b7sern", "Way\u00b7sen\u00b7stand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und daher soll man den Sand", "tokens": ["Und", "da\u00b7her", "soll", "man", "den", "Sand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VMFIN", "PIS", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Eher als die W\u00fcntsche zehlen,", "tokens": ["E\u00b7her", "als", "die", "W\u00fcnt\u00b7sche", "zeh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die unsers Herzens Danckbarkeit", "tokens": ["Die", "un\u00b7sers", "Her\u00b7zens", "Dan\u00b7ck\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Dem Wachsthum ihrer H\u00e4user weiht.", "tokens": ["Dem", "Wach\u00b7sthum", "ih\u00b7rer", "H\u00e4u\u00b7ser", "weiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Komm mit deinen s\u00fc\u00dfen Lehren,", "tokens": ["Komm", "mit", "dei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Theurester Gamaliel!", "tokens": ["Theu\u00b7res\u00b7ter", "Ga\u00b7ma\u00b7liel", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wir, dein kleines Israel,", "tokens": ["Wir", ",", "dein", "klei\u00b7nes", "Is\u00b7rael", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "ADJA", "NE", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sind bereit, dein Wort zu h\u00f6ren,", "tokens": ["Sind", "be\u00b7reit", ",", "dein", "Wort", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil Gott, der deine Gaben sch\u00e4zt,", "tokens": ["Weil", "Gott", ",", "der", "dei\u00b7ne", "Ga\u00b7ben", "sch\u00e4zt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich \u00fcber uns zum Hirten sezt.", "tokens": ["Dich", "\u00fc\u00b7ber", "uns", "zum", "Hir\u00b7ten", "sezt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wir verpf\u00e4nden dir die Herzen,", "tokens": ["Wir", "ver\u00b7pf\u00e4n\u00b7den", "dir", "die", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein Befehl ist unsre Lust,", "tokens": ["Dein", "Be\u00b7fehl", "ist", "uns\u00b7re", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wir er\u00f6fnen dir die Brust", "tokens": ["Wir", "er\u00b7\u00f6f\u00b7nen", "dir", "die", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ein Feld voll Ehrenkerzen", "tokens": ["Als", "ein", "Feld", "voll", "Eh\u00b7ren\u00b7ker\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und leisten sonder Heucheley", "tokens": ["Und", "leis\u00b7ten", "son\u00b7der", "Heu\u00b7che\u00b7ley"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVINF", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den ungezwungnen Eid der Treu.", "tokens": ["Den", "un\u00b7ge\u00b7zwung\u00b7nen", "Eid", "der", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Verr\u00e4ther unsrer Freude,", "tokens": ["Die", "Ver\u00b7r\u00e4\u00b7ther", "uns\u00b7rer", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mund und Zunge, k\u00fc\u00dfen dich", "tokens": ["Mund", "und", "Zun\u00b7ge", ",", "k\u00fc\u00b7\u00dfen", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verlangen inniglich,", "tokens": ["Und", "ver\u00b7lan\u00b7gen", "in\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df uns deine Vorsicht weide;", "tokens": ["Da\u00df", "uns", "dei\u00b7ne", "Vor\u00b7sicht", "wei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier, Vater, stellt die Liebe dir", "tokens": ["Hier", ",", "Va\u00b7ter", ",", "stellt", "die", "Lie\u00b7be", "dir"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den kindlichen Gehorsam f\u00fcr.", "tokens": ["Den", "kind\u00b7li\u00b7chen", "Ge\u00b7hor\u00b7sam", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wir empfinden schon den Seegen", "tokens": ["Wir", "emp\u00b7fin\u00b7den", "schon", "den", "See\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als den Erstling deiner Huld;", "tokens": ["Als", "den", "Erst\u00b7ling", "dei\u00b7ner", "Huld", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsrer Sehnsucht Ungedult", "tokens": ["Uns\u00b7rer", "Sehn\u00b7sucht", "Un\u00b7ge\u00b7dult"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Geht demselben schon entgegen,", "tokens": ["Geht", "dem\u00b7sel\u00b7ben", "schon", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der, weil ihn Gott und Himmel k\u00fcst,", "tokens": ["Der", ",", "weil", "ihn", "Gott", "und", "Him\u00b7mel", "k\u00fcst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Nachbar Obededoms ist.", "tokens": ["Ein", "Nach\u00b7bar", "O\u00b7be\u00b7de\u00b7doms", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}