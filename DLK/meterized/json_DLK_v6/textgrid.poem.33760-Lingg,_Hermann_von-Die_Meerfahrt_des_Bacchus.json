{"textgrid.poem.33760": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Die Meerfahrt des Bacchus", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ehrt den Genius k\u00fchner Taten,", "tokens": ["Ehrt", "den", "Ge\u00b7nius", "k\u00fch\u00b7ner", "Ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "H\u00f6hnet seiner Milde nicht!", "tokens": ["H\u00f6h\u00b7net", "sei\u00b7ner", "Mil\u00b7de", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schiffer wollten einst verraten", "tokens": ["Schif\u00b7fer", "woll\u00b7ten", "einst", "ver\u00b7ra\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jenen Gott, der Fesseln bricht;", "tokens": ["Je\u00b7nen", "Gott", ",", "der", "Fes\u00b7seln", "bricht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber da\u00df er sie auch flicht,", "tokens": ["A\u00b7ber", "da\u00df", "er", "sie", "auch", "flicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00dften die Verruchten bald gewahren,", "tokens": ["Mu\u00df\u00b7ten", "die", "Ver\u00b7ruch\u00b7ten", "bald", "ge\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Als sie auf dem Meere waren.", "tokens": ["Als", "sie", "auf", "dem", "Mee\u00b7re", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihn nach Naxos hinzuf\u00fchren,", "tokens": ["Ihn", "nach", "Na\u00b7xos", "hin\u00b7zu\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Hatten sie mit Mund und Hand", "tokens": ["Hat\u00b7ten", "sie", "mit", "Mund", "und", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zugesagt in hohen Schw\u00fcren;", "tokens": ["Zu\u00b7ge\u00b7sagt", "in", "ho\u00b7hen", "Schw\u00fc\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber als der Tag entschwand,", "tokens": ["A\u00b7ber", "als", "der", "Tag", "ent\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lie\u00dfen sie das Inselland \u2013", "tokens": ["Lie\u00b7\u00dfen", "sie", "das", "In\u00b7sel\u00b7land", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle Segel schleunigst aufgezogen \u2013", "tokens": ["Al\u00b7le", "Se\u00b7gel", "schleu\u00b7nigst", "auf\u00b7ge\u00b7zo\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "VVPP", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Seitw\u00e4rts liegen in den Wogen.", "tokens": ["Seit\u00b7w\u00e4rts", "lie\u00b7gen", "in", "den", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie berieten sich im Kreise", "tokens": ["Sie", "be\u00b7rie\u00b7ten", "sich", "im", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und erwogen her und hin,", "tokens": ["Und", "er\u00b7wo\u00b7gen", "her", "und", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Wie sie wohl zum h\u00f6chsten Preise", "tokens": ["Wie", "sie", "wohl", "zum", "h\u00f6chs\u00b7ten", "Prei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihn verkaufen m\u00f6chten, ihn,", "tokens": ["Ihn", "ver\u00b7kau\u00b7fen", "m\u00f6ch\u00b7ten", ",", "ihn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "$,", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der so hold und sanft erschien.", "tokens": ["Der", "so", "hold", "und", "sanft", "er\u00b7schien", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gold in F\u00fclle w\u00fcrden selbst die Scythen", "tokens": ["Gold", "in", "F\u00fcl\u00b7le", "w\u00fcr\u00b7den", "selbst", "die", "Scyt\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "F\u00fcr den sch\u00f6nen J\u00fcngling bieten.", "tokens": ["F\u00fcr", "den", "sch\u00f6\u00b7nen", "J\u00fcng\u00b7ling", "bie\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Drauf nach Asien hin das Steuer", "tokens": ["Drauf", "nach", "A\u00b7sien", "hin", "das", "Steu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "NE", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lenkten sie, gewinnbet\u00f6rt;", "tokens": ["Lenk\u00b7ten", "sie", ",", "ge\u00b7winn\u00b7be\u00b7t\u00f6rt", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch da z\u00fcckten ringsum Feuer,", "tokens": ["Doch", "da", "z\u00fcck\u00b7ten", "ring\u00b7sum", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn er hatte sie geh\u00f6rt", "tokens": ["Denn", "er", "hat\u00b7te", "sie", "ge\u00b7h\u00f6rt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und von edlem Zorn emp\u00f6rt", "tokens": ["Und", "von", "ed\u00b7lem", "Zorn", "em\u00b7p\u00f6rt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die verr\u00e4terischen Raubgenossen", "tokens": ["Die", "ver\u00b7r\u00e4\u00b7te\u00b7ri\u00b7schen", "Raub\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Zu bestrafen schon beschlossen.", "tokens": ["Zu", "be\u00b7stra\u00b7fen", "schon", "be\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sieh! es biegen sich die Stangen,", "tokens": ["Sieh", "!", "es", "bie\u00b7gen", "sich", "die", "Stan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mast und Ruder kr\u00fcmmen sich", "tokens": ["Mast", "und", "Ru\u00b7der", "kr\u00fcm\u00b7men", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verwandeln sich in Schlangen;", "tokens": ["Und", "ver\u00b7wan\u00b7deln", "sich", "in", "Schlan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die Segel abendlich", "tokens": ["Wo", "die", "Se\u00b7gel", "a\u00b7bend\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kaum vorher der Wind bestrich,", "tokens": ["Kaum", "vor\u00b7her", "der", "Wind", "be\u00b7strich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Winden um den Kiel und um die Planken", "tokens": ["Win\u00b7den", "um", "den", "Kiel", "und", "um", "die", "Plan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NE", "KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Reben sich und Efeuranken.", "tokens": ["Re\u00b7ben", "sich", "und", "E\u00b7feu\u00b7ran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Immer st\u00e4rkre Zweige packen", "tokens": ["Im\u00b7mer", "st\u00e4r\u00b7kre", "Zwei\u00b7ge", "pa\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen nach dem Andern fest;", "tokens": ["Ei\u00b7nen", "nach", "dem", "An\u00b7dern", "fest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Strauchelnd sehn sie Arm und Nacken", "tokens": ["Strau\u00b7chelnd", "sehn", "sie", "Arm", "und", "Na\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Bande Joch gepre\u00dft,", "tokens": ["In", "der", "Ban\u00b7de", "Joch", "ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Horch! und wie zu frohem Fest", "tokens": ["Horch", "!", "und", "wie", "zu", "fro\u00b7hem", "Fest"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "T\u00f6nen unsichtbar dazu Ges\u00e4nge,", "tokens": ["T\u00f6\u00b7nen", "un\u00b7sicht\u00b7bar", "da\u00b7zu", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PAV", "NN", "$,"], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Cymbeln und Oboenkl\u00e4nge.", "tokens": ["Cym\u00b7beln", "und", "O\u00b7boen\u00b7kl\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Das Verdeck wird von M\u00e4naden,", "tokens": ["Das", "Ver\u00b7deck", "wird", "von", "M\u00e4\u00b7na\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Panthern und Bacchanten voll,", "tokens": ["Pan\u00b7thern", "und", "Bac\u00b7chan\u00b7ten", "voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo den Trauben hochgeladen", "tokens": ["Wo", "den", "Trau\u00b7ben", "hoch\u00b7ge\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcberall nun Wein entquoll;", "tokens": ["\u00dc\u00b7be\u00b7rall", "nun", "Wein", "ent\u00b7quoll", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber Jene, schreckentoll,", "tokens": ["A\u00b7ber", "Je\u00b7ne", ",", "schre\u00b7cken\u00b7toll", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "PDS", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "St\u00fcrzen, an den Ranken fortgezogen,", "tokens": ["St\u00fcr\u00b7zen", ",", "an", "den", "Ran\u00b7ken", "fort\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Sich kopf\u00fcber in die Wogen.", "tokens": ["Sich", "kopf\u00b7\u00fc\u00b7ber", "in", "die", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch als Schw\u00e4rme von Delphinen", "tokens": ["Doch", "als", "Schw\u00e4r\u00b7me", "von", "Del\u00b7phi\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tauchen sie sogleich empor,", "tokens": ["Tau\u00b7chen", "sie", "sog\u00b7leich", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tummeln, wie dem Gott zu dienen,", "tokens": ["Tum\u00b7meln", ",", "wie", "dem", "Gott", "zu", "die\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach den T\u00f6nen sich im Chor.", "tokens": ["Nach", "den", "T\u00f6\u00b7nen", "sich", "im", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Einer eilt dem Schiffe vor,", "tokens": ["Ei\u00b7ner", "eilt", "dem", "Schif\u00b7fe", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Um die Andern schlingt mit hellem Liede", "tokens": ["Um", "die", "An\u00b7dern", "schlingt", "mit", "hel\u00b7lem", "Lie\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Triton sich und Nereide.", "tokens": ["Tri\u00b7ton", "sich", "und", "Ne\u00b7rei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbdeiner Macht soll innewerden,", "tokens": ["\u00bb", "dei\u00b7ner", "Macht", "soll", "in\u00b7ne\u00b7wer\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VMFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Siegesheld Dionysos,", "tokens": ["Sie\u00b7ge\u00b7sheld", "Di\u00b7o\u00b7ny\u00b7sos", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was im Meer lebt und auf Erden!\u00ab", "tokens": ["Was", "im", "Meer", "lebt", "und", "auf", "Er\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "APPRART", "NN", "VVFIN",