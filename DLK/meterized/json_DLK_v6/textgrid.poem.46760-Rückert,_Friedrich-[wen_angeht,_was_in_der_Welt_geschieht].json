{"textgrid.poem.46760": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wen angeht, was in der Welt geschieht]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wen angeht, was in der Welt geschieht,", "tokens": ["Wen", "an\u00b7geht", ",", "was", "in", "der", "Welt", "ge\u00b7schieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Vergi\u00dft was er f\u00fchlt, \u00fcber dem was er sieht;", "tokens": ["Ver\u00b7gi\u00dft", "was", "er", "f\u00fchlt", ",", "\u00fc\u00b7ber", "dem", "was", "er", "sieht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "PPER", "VVFIN", "$,", "APPR", "ART", "PWS", "PPER", "VVFIN", "$."], "meter": "-+--++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mich geht nichts an all' dessen,", "tokens": ["Mich", "geht", "nichts", "an", "all'", "des\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PIAT", "PDS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drum werd' ich euch nie vergessen.", "tokens": ["Drum", "werd'", "ich", "euch", "nie", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Es ist um mich ein stiller Chor,", "tokens": ["Es", "ist", "um", "mich", "ein", "stil\u00b7ler", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sagt mir euere Namen vor,", "tokens": ["Der", "sagt", "mir", "eu\u00b7e\u00b7re", "Na\u00b7men", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und will in tausend Bildern", "tokens": ["Und", "will", "in", "tau\u00b7send", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir euer Leben schildern.", "tokens": ["Mir", "eu\u00b7er", "Le\u00b7ben", "schil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es ist der altbekannte Chor,", "tokens": ["Es", "ist", "der", "alt\u00b7be\u00b7kann\u00b7te", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gewohnt dem Auge, vertraut dem Ohr,", "tokens": ["Ge\u00b7wohnt", "dem", "Au\u00b7ge", ",", "ver\u00b7traut", "dem", "Ohr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nichts ist darin des Neuen,", "tokens": ["Nichts", "ist", "da\u00b7rin", "des", "Neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das mich k\u00f6nnte zerstreuen.", "tokens": ["Das", "mich", "k\u00f6nn\u00b7te", "zer\u00b7streu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VMFIN", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Hinter des Fr\u00fchlings gr\u00fcnem Flor", "tokens": ["Hin\u00b7ter", "des", "Fr\u00fch\u00b7lings", "gr\u00fc\u00b7nem", "Flor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sieht wie die Rose mein Schmerz hervor,", "tokens": ["Sieht", "wie", "die", "Ro\u00b7se", "mein", "Schmerz", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den Anblick werd' ich nicht scheuen,", "tokens": ["Den", "An\u00b7blick", "werd'", "ich", "nicht", "scheu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Er soll mein Herz erfreuen.", "tokens": ["Er", "soll", "mein", "Herz", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wer gewohnt ist wie ich zu thun,", "tokens": ["Wer", "ge\u00b7wohnt", "ist", "wie", "ich", "zu", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "VAFIN", "KOKOM", "PPER", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mit sich in der Natur zu ruhn,", "tokens": ["Mit", "sich", "in", "der", "Na\u00b7tur", "zu", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Was immer ihn mag ber\u00fchren,", "tokens": ["Was", "im\u00b7mer", "ihn", "mag", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wird er ges\u00e4nftigt sp\u00fcren.", "tokens": ["Wird", "er", "ge\u00b7s\u00e4nf\u00b7tigt", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Lust wird ihm lachen minder grell,", "tokens": ["Die", "Lust", "wird", "ihm", "la\u00b7chen", "min\u00b7der", "grell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch wird sie auch schwinden minder schnell.", "tokens": ["Doch", "wird", "sie", "auch", "schwin\u00b7den", "min\u00b7der", "schnell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Schmerz wird ihn schwer nicht pressen,", "tokens": ["Der", "Schmerz", "wird", "ihn", "schwer", "nicht", "pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Doch wird er nicht leicht ihn vergessen.", "tokens": ["Doch", "wird", "er", "nicht", "leicht", "ihn", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Als wie des Himmels, der Landschaft Bild", "tokens": ["Als", "wie", "des", "Him\u00b7mels", ",", "der", "Land\u00b7schaft", "Bild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ruht verkl\u00e4rt in der Seefluth mild,", "tokens": ["Ruht", "ver\u00b7kl\u00e4rt", "in", "der", "See\u00b7fluth", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Geschirmt vor Sturm und Winden,", "tokens": ["Ge\u00b7schirmt", "vor", "Sturm", "und", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie sollte das Bild verschwinden?", "tokens": ["Wie", "soll\u00b7te", "das", "Bild", "ver\u00b7schwin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Ich hab' geruht in der stillen Bucht,", "tokens": ["Ich", "hab'", "ge\u00b7ruht", "in", "der", "stil\u00b7len", "Bucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und nicht den Braus der Welt gesucht", "tokens": ["Und", "nicht", "den", "Braus", "der", "Welt", "ge\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In meinen frohen Tagen;", "tokens": ["In", "mei\u00b7nen", "fro\u00b7hen", "Ta\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun sollt ich's in meinen Klagen?", "tokens": ["Nun", "sollt", "ich's", "in", "mei\u00b7nen", "Kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}