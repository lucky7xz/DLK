{"dta.poem.922": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "EiN HERBSTABEND", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Das braune Dorf. Ein Dunkles zeigt im Schreiten", "tokens": ["Das", "brau\u00b7ne", "Dorf", ".", "Ein", "Dunk\u00b7les", "zeigt", "im", "Schrei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich oft an Mauern, die im Herbste stehn,", "tokens": ["Sich", "oft", "an", "Mau\u00b7ern", ",", "die", "im", "Herbs\u00b7te", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gestalten: Mann wie Weib, Verstorbene gehn", "tokens": ["Ge\u00b7stal\u00b7ten", ":", "Mann", "wie", "Weib", ",", "Ver\u00b7stor\u00b7be\u00b7ne", "gehn"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "NN", "KOKOM", "NN", "$,", "NN", "VVINF"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "In k\u00fchlen Stuben jener Bett bereiten.", "tokens": ["In", "k\u00fch\u00b7len", "Stu\u00b7ben", "je\u00b7ner", "Bett", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hier spielen Knaben. Schwere Schatten breiten", "tokens": ["Hier", "spie\u00b7len", "Kna\u00b7ben", ".", "Schwe\u00b7re", "Schat\u00b7ten", "brei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "$.", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich \u00fcber braune Jauche. M\u00e4gde gehn", "tokens": ["Sich", "\u00fc\u00b7ber", "brau\u00b7ne", "Jau\u00b7che", ".", "M\u00e4g\u00b7de", "gehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "APPR", "ADJA", "NN", "$.", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch feuchte Bl\u00e4ue und bisweilen sehn", "tokens": ["Durch", "feuch\u00b7te", "Bl\u00e4u\u00b7e", "und", "bis\u00b7wei\u00b7len", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADV", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus Augen sie, erf\u00fcllt von Nachtgel\u00e4uten.", "tokens": ["Aus", "Au\u00b7gen", "sie", ",", "er\u00b7f\u00fcllt", "von", "Nacht\u00b7ge\u00b7l\u00e4u\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "$,", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "F\u00fcr Einsames ist eine Schenke da;", "tokens": ["F\u00fcr", "Ein\u00b7sa\u00b7mes", "ist", "ei\u00b7ne", "Schen\u00b7ke", "da", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das s\u00e4umt geduldig unter dunklen Bogen,", "tokens": ["Das", "s\u00e4umt", "ge\u00b7dul\u00b7dig", "un\u00b7ter", "dunk\u00b7len", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von goldenem Tabaksgew\u00f6lk umzogen.", "tokens": ["Von", "gol\u00b7de\u00b7nem", "Ta\u00b7baks\u00b7ge\u00b7w\u00f6lk", "um\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch immer ist das Eigne schwarz und nah.", "tokens": ["Doch", "im\u00b7mer", "ist", "das", "Eig\u00b7ne", "schwarz", "und", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Trunkne sinnt im Schatten alter Bogen", "tokens": ["Der", "Trunk\u00b7ne", "sinnt", "im", "Schat\u00b7ten", "al\u00b7ter", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den wilden V\u00f6geln nach, die ferngezogen.", "tokens": ["Den", "wil\u00b7den", "V\u00f6\u00b7geln", "nach", ",", "die", "fern\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}