{"textgrid.poem.60519": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Einem kritischen Nachtarbeiter", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil ein Wort der Di\u00e4tetik", "tokens": ["Weil", "ein", "Wort", "der", "Di\u00b7\u00e4\u00b7te\u00b7tik"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Besser noch mir mag gelingen,", "tokens": ["Bes\u00b7ser", "noch", "mir", "mag", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ein Wort dir der \u00c4sthetik,", "tokens": ["Als", "ein", "Wort", "dir", "der", "\u00c4s\u00b7the\u00b7tik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Will ich einen Rat dir bringen.", "tokens": ["Will", "ich", "ei\u00b7nen", "Rat", "dir", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hast du auf des Tages Bahnen", "tokens": ["Hast", "du", "auf", "des", "Ta\u00b7ges", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00fcd gelaufen deine Glieder,", "tokens": ["M\u00fcd", "ge\u00b7lau\u00b7fen", "dei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zupft mit wohlgemeintem Mahnen", "tokens": ["Zupft", "mit", "wohl\u00b7ge\u00b7mein\u00b7tem", "Mah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir der Schlaf die Augenlider:", "tokens": ["Dir", "der", "Schlaf", "die", "Au\u00b7gen\u00b7li\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wolle nicht, hin\u00fcberduselnd,", "tokens": ["Wol\u00b7le", "nicht", ",", "hin\u00b7\u00fc\u00b7berd\u00b7u\u00b7selnd", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr die Welt geschwind noch richten,", "tokens": ["F\u00fcr", "die", "Welt", "ge\u00b7schwind", "noch", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hegelisch-\u00e4sthetisch nuselnd,", "tokens": ["He\u00b7ge\u00b7lisch\u00e4s\u00b7the\u00b7tisch", "nu\u00b7selnd", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Was du nicht verstehst, mein Dichten;", "tokens": ["Was", "du", "nicht", "ver\u00b7stehst", ",", "mein", "Dich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schlagen nicht das Haupt vom Rumpfe", "tokens": ["Schla\u00b7gen", "nicht", "das", "Haupt", "vom", "Rump\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meinem Werk mit plumpen Scherzen,", "tokens": ["Mei\u00b7nem", "Werk", "mit", "plum\u00b7pen", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schnell, beim letzten Flackerstumpfe", "tokens": ["Schnell", ",", "beim", "letz\u00b7ten", "Fla\u00b7cker\u00b7stump\u00b7fe"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner abgebrannten Kerzen.", "tokens": ["Dei\u00b7ner", "ab\u00b7ge\u00b7brann\u00b7ten", "Ker\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Denn dir leuchten zum Erkennen", "tokens": ["Denn", "dir", "leuch\u00b7ten", "zum", "Er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.2": {"text": "Keine hellen Kunstgestirne;", "tokens": ["Kei\u00b7ne", "hel\u00b7len", "Kunst\u00b7ges\u00b7tir\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Armer Kauz, du scheinst zu brennen", "tokens": ["Ar\u00b7mer", "Kauz", ",", "du", "scheinst", "zu", "bren\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Talg im Leuchter und Gehirne.", "tokens": ["Talg", "im", "Leuch\u00b7ter", "und", "Ge\u00b7hir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Darum halte dich geschieden", "tokens": ["Da\u00b7rum", "hal\u00b7te", "dich", "ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von den kritischen Bezirken,", "tokens": ["Von", "den", "kri\u00b7ti\u00b7schen", "Be\u00b7zir\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leg aufs Ohr dich, g\u00f6nn dir Frieden,", "tokens": ["Leg", "aufs", "Ohr", "dich", ",", "g\u00f6nn", "dir", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dein Beruf ist Werkelwirken.", "tokens": ["Dein", "Be\u00b7ruf", "ist", "Wer\u00b7kel\u00b7wir\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}