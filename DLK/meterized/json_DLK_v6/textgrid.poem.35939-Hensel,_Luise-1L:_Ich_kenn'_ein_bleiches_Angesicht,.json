{"textgrid.poem.35939": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich kenn' ein bleiches Angesicht,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kenn' ein bleiches Angesicht,", "tokens": ["Ich", "kenn'", "ein", "blei\u00b7ches", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das seh' ich gerne an.", "tokens": ["Das", "seh'", "ich", "ger\u00b7ne", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es sieht, als sagt' es: \u00bbWeine nicht!", "tokens": ["Es", "sieht", ",", "als", "sagt'", "es", ":", "\u00bb", "Wei\u00b7ne", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "$.", "$(", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist ", "tokens": ["Ist"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Oft h\u00fcllt es sich in Schleier dicht,", "tokens": ["Oft", "h\u00fcllt", "es", "sich", "in", "Schlei\u00b7er", "dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich's nicht sehen kann;", "tokens": ["Da\u00df", "ich's", "nicht", "se\u00b7hen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Oft wendet es sein mildes Licht", "tokens": ["Oft", "wen\u00b7det", "es", "sein", "mil\u00b7des", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wandelt ferne Bahn. \u2013", "tokens": ["Und", "wan\u00b7delt", "fer\u00b7ne", "Bahn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wird mich Keiner je verstehn", "tokens": ["Und", "wird", "mich", "Kei\u00b7ner", "je", "ver\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PIS", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als nur der traute Mond;", "tokens": ["Als", "nur", "der", "trau\u00b7te", "Mond", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der hat mir oft in's Herz gesehn,", "tokens": ["Der", "hat", "mir", "oft", "in's", "Herz", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wei\u00df all, was drinnen wohnt.", "tokens": ["Wei\u00df", "all", ",", "was", "drin\u00b7nen", "wohnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}