{"textgrid.poem.41120": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weinen mu\u00df ich, \u2013 sollt' es nicht!", "tokens": ["Wei\u00b7nen", "mu\u00df", "ich", ",", "\u2013", "sollt'", "es", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "$(", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df du bist vorangegangen,", "tokens": ["Da\u00df", "du", "bist", "vor\u00b7an\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hin, wo Freiheit ist und Licht,", "tokens": ["Hin", ",", "wo", "Frei\u00b7heit", "ist", "und", "Licht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "VAFIN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4hrend ich noch hier gefangen.", "tokens": ["W\u00e4h\u00b7rend", "ich", "noch", "hier", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber sieh! es ist mir jetzt,", "tokens": ["A\u00b7ber", "sieh", "!", "es", "ist", "mir", "jetzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PPER", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seit du dich von mir gerissen,", "tokens": ["Seit", "du", "dich", "von", "mir", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie dem Kinde, ausgesetzt,", "tokens": ["Wie", "dem", "Kin\u00b7de", ",", "aus\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Elternlos in Finsternissen.", "tokens": ["El\u00b7tern\u00b7los", "in", "Fins\u00b7ter\u00b7nis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Heimatlich war es mir nur,", "tokens": ["Hei\u00b7mat\u00b7lich", "war", "es", "mir", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PPER", "ADV", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Bruderherz! durch dich hienieden,", "tokens": ["Bru\u00b7der\u00b7herz", "!", "durch", "dich", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fremd blickt an mich die Natur,", "tokens": ["Fremd", "blickt", "an", "mich", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Seit du bist aus ihr geschieden.", "tokens": ["Seit", "du", "bist", "aus", "ihr", "ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Wuchsen wir in langen Jahren \u2013", "tokens": ["Wuch\u00b7sen", "wir", "in", "lan\u00b7gen", "Jah\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freudig trug ich jede Last,", "tokens": ["Freu\u00b7dig", "trug", "ich", "je\u00b7de", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wu\u00dft' ich's nur, da\u00df du's erfahren.", "tokens": ["Wu\u00dft'", "ich's", "nur", ",", "da\u00df", "du's", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "KOUS", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Trug dich, andern unbewu\u00dft,", "tokens": ["Trug", "dich", ",", "an\u00b7dern", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mitten unter tiefen Schmerzen,", "tokens": ["Mit\u00b7ten", "un\u00b7ter", "tie\u00b7fen", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen Stoff von sel'ger Lust", "tokens": ["Ei\u00b7nen", "Stoff", "von", "sel'\u00b7ger", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Still in meinem wunden Herzen.", "tokens": ["Still", "in", "mei\u00b7nem", "wun\u00b7den", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Alles dies dein Tod zerbrach! \u2013", "tokens": ["Al\u00b7les", "dies", "dein", "Tod", "zer\u00b7brach", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "PDS", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch du z\u00fcrnst und rufst zum Staube:", "tokens": ["Doch", "du", "z\u00fcrnst", "und", "rufst", "zum", "Stau\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbbruderherz, wie lange, ach!", "tokens": ["\u00bb", "bru\u00b7der\u00b7herz", ",", "wie", "lan\u00b7ge", ",", "ach", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "NN", "$,", "PWAV", "ADV", "$,", "ITJ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist ein schwankend Rohr dein Glaube!\u00ab", "tokens": ["Ist", "ein", "schwan\u00b7kend", "Rohr", "dein", "Glau\u00b7be", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "ADJD", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}