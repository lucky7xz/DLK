{"textgrid.poem.42522": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Vers Dieu vais", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich gehe Gott entgegen, sagt die Flamme,", "tokens": ["Ich", "ge\u00b7he", "Gott", "ent\u00b7ge\u00b7gen", ",", "sagt", "die", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und fri\u00dft dabei das gro\u00dfe Holzger\u00fcst,", "tokens": ["Und", "fri\u00dft", "da\u00b7bei", "das", "gro\u00b7\u00dfe", "Holz\u00b7ge\u00b7r\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu Gott empor aus dieser Erdenklamme,", "tokens": ["Zu", "Gott", "em\u00b7por", "aus", "die\u00b7ser", "Er\u00b7den\u00b7klam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo alles unnachgiebig droht: Ihr m\u00fc\u00dft!", "tokens": ["Wo", "al\u00b7les", "un\u00b7nach\u00b7gie\u00b7big", "droht", ":", "Ihr", "m\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VVFIN", "$.", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ins Friedensreich, hin zu dem hohen Stamme,", "tokens": ["Ins", "Frie\u00b7dens\u00b7reich", ",", "hin", "zu", "dem", "ho\u00b7hen", "Stam\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo Christus von den Engeln ward gek\u00fc\u00dft.", "tokens": ["Wo", "Chris\u00b7tus", "von", "den", "En\u00b7geln", "ward", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dort ist es still, und hinter jenem Damme", "tokens": ["Dort", "ist", "es", "still", ",", "und", "hin\u00b7ter", "je\u00b7nem", "Dam\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$,", "KON", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "St\u00f6rt nichts die Ruhe, ihr habt abgeb\u00fc\u00dft.", "tokens": ["St\u00f6rt", "nichts", "die", "Ru\u00b7he", ",", "ihr", "habt", "ab\u00b7ge\u00b7b\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}