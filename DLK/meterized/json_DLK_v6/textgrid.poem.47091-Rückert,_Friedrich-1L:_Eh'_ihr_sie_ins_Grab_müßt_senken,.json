{"textgrid.poem.47091": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eh' ihr sie ins Grab m\u00fc\u00dft senken,", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eh' ihr sie ins Grab m\u00fc\u00dft senken,", "tokens": ["Eh'", "ihr", "sie", "ins", "Grab", "m\u00fc\u00dft", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gebet mir die Locke nur!", "tokens": ["Ge\u00b7bet", "mir", "die", "Lo\u00b7cke", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "G\u00f6nnet meinem Angedenken", "tokens": ["G\u00f6n\u00b7net", "mei\u00b7nem", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["NE", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese einz'ge dunkle Spur!", "tokens": ["Die\u00b7se", "einz'\u00b7ge", "dunk\u00b7le", "Spur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dunkle Locke, du von ihren", "tokens": ["Dunk\u00b7le", "Lo\u00b7cke", ",", "du", "von", "ih\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "APPR", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reizen einst der Schatten blo\u00df;", "tokens": ["Rei\u00b7zen", "einst", "der", "Schat\u00b7ten", "blo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da sie all ihr Licht verlieren,", "tokens": ["Da", "sie", "all", "ihr", "Licht", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O wie scheint uns deins so gro\u00df!", "tokens": ["O", "wie", "scheint", "uns", "deins", "so", "gro\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Von des Todes Bann gefodert", "tokens": ["Von", "des", "To\u00b7des", "Bann", "ge\u00b7fo\u00b7dert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle m\u00fcssen in die Gruft,", "tokens": ["Al\u00b7le", "m\u00fcs\u00b7sen", "in", "die", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du allein darfst unvermodert", "tokens": ["Du", "al\u00b7lein", "darfst", "un\u00b7ver\u00b7mo\u00b7dert"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "PAV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spielen in des Himmels Luft.", "tokens": ["Spie\u00b7len", "in", "des", "Him\u00b7mels", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Du allein bist nun geblieben,", "tokens": ["Du", "al\u00b7lein", "bist", "nun", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst so schwach, nun stark genug,", "tokens": ["Einst", "so", "schwach", ",", "nun", "stark", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um zu tragen all mein Lieben,", "tokens": ["Um", "zu", "tra\u00b7gen", "all", "mein", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "PIAT", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das ein ganzer Himmel trug.", "tokens": ["Das", "ein", "gan\u00b7zer", "Him\u00b7mel", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Denn wie einst an dir, o Locke,", "tokens": ["Denn", "wie", "einst", "an", "dir", ",", "o", "Lo\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "APPR", "PPER", "$,", "FM", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "All die s\u00fc\u00dfe Sch\u00f6nheit hing,", "tokens": ["All", "die", "s\u00fc\u00b7\u00dfe", "Sch\u00f6n\u00b7heit", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So zum Trotz der Sterbeglocke", "tokens": ["So", "zum", "Trotz", "der", "Ster\u00b7be\u00b7glo\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4ngt sie noch an diesem Ring.", "tokens": ["H\u00e4ngt", "sie", "noch", "an", "die\u00b7sem", "Ring", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie den Ring ich magisch drehe,", "tokens": ["Wie", "den", "Ring", "ich", "ma\u00b7gisch", "dre\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieht er sie vom Grab empor", "tokens": ["Zieht", "er", "sie", "vom", "Grab", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor mein Antlitz, und ich sehe,", "tokens": ["Vor", "mein", "Ant\u00b7litz", ",", "und", "ich", "se\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df mein Herz sie nicht verlor.", "tokens": ["Da\u00df", "mein", "Herz", "sie", "nicht", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}