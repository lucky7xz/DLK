{"dta.poem.8843": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxII .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Was ihn wieder hat befangen, ist ein Becher Wein ge-", "tokens": ["Was", "ihn", "wie\u00b7der", "hat", "be\u00b7fan\u00b7gen", ",", "ist", "ein", "Be\u00b7cher", "Wein", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "VVPP", "$,", "VAFIN", "ART", "NN", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Fr\u00fchlingshauch aus goldnen Locken lockte mich in ehr'ne", "tokens": ["Fr\u00fch\u00b7lings\u00b7hauch", "aus", "gold\u00b7nen", "Lo\u00b7cken", "lock\u00b7te", "mich", "in", "ehr'\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Denn ihr Anbeginn ist Irrthum, und ihr Ende Pein", "tokens": ["Denn", "ihr", "An\u00b7be\u00b7ginn", "ist", "Irr\u00b7thum", ",", "und", "ihr", "En\u00b7de", "Pein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "$,", "KON", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "An bemalten Schaugerichten wollt' ich meinen Hunger", "tokens": ["An", "be\u00b7mal\u00b7ten", "Schau\u00b7ge\u00b7rich\u00b7ten", "wollt'", "ich", "mei\u00b7nen", "Hun\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Aber was mir Brod geschienen ist ein kalter Stein ge-", "tokens": ["A\u00b7ber", "was", "mir", "Brod", "ge\u00b7schie\u00b7nen", "ist", "ein", "kal\u00b7ter", "Stein", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "NN", "VVPP", "VAFIN", "ART", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Gold und Silber wollt' ich f\u00f6rdern auf im Traum ge-", "tokens": ["Gold", "und", "Sil\u00b7ber", "wollt'", "ich", "f\u00f6r\u00b7dern", "auf", "im", "Traum", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VMFIN", "PPER", "VVFIN", "APPR", "APPRART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Aber was ich ausgegraben ist ein morsch Gebein gewesen.", "tokens": ["A\u00b7ber", "was", "ich", "aus\u00b7ge\u00b7gra\u00b7ben", "ist", "ein", "morsch", "Ge\u00b7bein", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVPP", "VAFIN", "ART", "ADJD", "NN", "VAPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Will mich dennoch, aus der Ferne, deine Huld und", "tokens": ["Will", "mich", "den\u00b7noch", ",", "aus", "der", "Fer\u00b7ne", ",", "dei\u00b7ne", "Huld", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "$,", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "KON"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Soll mir theurer seyn die Trennung, als es der Verein", "tokens": ["Soll", "mir", "theu\u00b7rer", "seyn", "die", "Tren\u00b7nung", ",", "als", "es", "der", "Ver\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD", "VAINF", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.10": {"text": "Flattersinnig, unbest\u00e4ndig lie\u00df ich zwar das Auge schweifen,", "tokens": ["Flat\u00b7ter\u00b7sin\u00b7nig", ",", "un\u00b7be\u00b7st\u00e4n\u00b7dig", "lie\u00df", "ich", "zwar", "das", "Au\u00b7ge", "schwei\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Doch es ist das Herz im Stillen, ganz im Stillen, dein", "tokens": ["Doch", "es", "ist", "das", "Herz", "im", "Stil\u00b7len", ",", "ganz", "im", "Stil\u00b7len", ",", "dein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$,", "ADV", "APPRART", "NN", "$,", "PPOSAT"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.12": {"text": "Was zu dir mich hingezogen, war Geschick und Gegenliebe,", "tokens": ["Was", "zu", "dir", "mich", "hin\u00b7ge\u00b7zo\u00b7gen", ",", "war", "Ge\u00b7schick", "und", "Ge\u00b7gen\u00b7lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "PRF", "VVPP", "$,", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Was an Jene mich gefesselt, ist ein falscher Schein ge-", "tokens": ["Was", "an", "Je\u00b7ne", "mich", "ge\u00b7fes\u00b7selt", ",", "ist", "ein", "fal\u00b7scher", "Schein", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PDAT", "PPER", "VVPP", "$,", "VAFIN", "ART", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.14": {"text": "Richte nicht zu streng die Lieder, die ich nicht an dich", "tokens": ["Rich\u00b7te", "nicht", "zu", "streng", "die", "Lie\u00b7der", ",", "die", "ich", "nicht", "an", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "PTKZU", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "APPR", "PPER"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.15": {"text": "Freylich, solcher Lieder w\u00fcrdig w\u00e4rst du ganz allein ge-", "tokens": ["Frey\u00b7lich", ",", "sol\u00b7cher", "Lie\u00b7der", "w\u00fcr\u00b7dig", "w\u00e4rst", "du", "ganz", "al\u00b7lein", "ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PIAT", "NN", "ADJD", "VAFIN", "PPER", "ADV", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}