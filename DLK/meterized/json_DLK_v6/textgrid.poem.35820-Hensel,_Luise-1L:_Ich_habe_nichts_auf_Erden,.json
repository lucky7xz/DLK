{"textgrid.poem.35820": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich habe nichts auf Erden,", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe nichts auf Erden,", "tokens": ["Ich", "ha\u00b7be", "nichts", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Himmel nichts als Dich;", "tokens": ["Im", "Him\u00b7mel", "nichts", "als", "Dich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "KOKOM", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was hier noch mein will werden \u2013", "tokens": ["Was", "hier", "noch", "mein", "will", "wer\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "PPOSAT", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es ist dahin f\u00fcr mich.", "tokens": ["Es", "ist", "da\u00b7hin", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O Herr! ich will ja gerne", "tokens": ["O", "Herr", "!", "ich", "will", "ja", "ger\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PPER", "VMFIN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr Dich, f\u00fcr Dich nur gl\u00fchn;", "tokens": ["F\u00fcr", "Dich", ",", "f\u00fcr", "Dich", "nur", "gl\u00fchn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O wende, o entferne,", "tokens": ["O", "wen\u00b7de", ",", "o", "ent\u00b7fer\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "FM", "FM", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was mich zur\u00fcck will ziehn.", "tokens": ["Was", "mich", "zu\u00b7r\u00fcck", "will", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKVZ", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Umfasse denn von Herzen", "tokens": ["Um\u00b7fas\u00b7se", "denn", "von", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sehnsuchtsvolle Braut.", "tokens": ["Die", "sehn\u00b7suchts\u00b7vol\u00b7le", "Braut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl sind ihr tausend Schmerzen", "tokens": ["Wohl", "sind", "ihr", "tau\u00b7send", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Dir, Herr! angetraut.", "tokens": ["Mit", "Dir", ",", "Herr", "!", "an\u00b7ge\u00b7traut", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NN", "$.", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie kann ja nicht die Plagen,", "tokens": ["Sie", "kann", "ja", "nicht", "die", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Sorgen mancherlei,", "tokens": ["Die", "Sor\u00b7gen", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Angst, den Schmerz ertragen,", "tokens": ["Die", "Angst", ",", "den", "Schmerz", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Stehst Du, Herr! ihr nicht bei.", "tokens": ["Stehst", "Du", ",", "Herr", "!", "ihr", "nicht", "bei", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Wer kann die Qualen nennen,", "tokens": ["Wer", "kann", "die", "Qua\u00b7len", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die unser Herz erf\u00e4hrt,", "tokens": ["Die", "un\u00b7ser", "Herz", "er\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis heil'ger Sehnsucht Brennen", "tokens": ["Bis", "heil'\u00b7ger", "Sehn\u00b7sucht", "Bren\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Erdenstoff verzehrt,", "tokens": ["Den", "Er\u00b7den\u00b7stoff", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bis siebenfaches Feuer", "tokens": ["Bis", "sie\u00b7ben\u00b7fa\u00b7ches", "Feu\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein reines Gold bew\u00e4hrt,", "tokens": ["Ein", "rei\u00b7nes", "Gold", "be\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis unsern dunkeln Schleier", "tokens": ["Bis", "un\u00b7sern", "dun\u00b7keln", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Himmels Glanz verkl\u00e4rt?", "tokens": ["Des", "Him\u00b7mels", "Glanz", "ver\u00b7kl\u00e4rt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So bin ich denn mit Freuden", "tokens": ["So", "bin", "ich", "denn", "mit", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr Tod und Leben Dein.", "tokens": ["F\u00fcr", "Tod", "und", "Le\u00b7ben", "Dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Stehst du mir bei im Leiden,", "tokens": ["Stehst", "du", "mir", "bei", "im", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So bin ich nicht allein,", "tokens": ["So", "bin", "ich", "nicht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So trag' ich stark und gerne,", "tokens": ["So", "trag'", "ich", "stark", "und", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was soll getragen sein;", "tokens": ["Was", "soll", "ge\u00b7tra\u00b7gen", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es kann ja nicht mehr ferne", "tokens": ["Es", "kann", "ja", "nicht", "mehr", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Laufes Ende sein.", "tokens": ["Des", "Lau\u00b7fes", "En\u00b7de", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}