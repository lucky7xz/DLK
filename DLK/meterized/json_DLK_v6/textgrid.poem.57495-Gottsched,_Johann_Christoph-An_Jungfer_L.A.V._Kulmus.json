 "es", "einst", "ge\u00b7schieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADJD", "VAINF", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df auch die Welt dein Lob in meinen Reimen sieht.", "tokens": ["Da\u00df", "auch", "die", "Welt", "dein", "Lob", "in", "mei\u00b7nen", "Rei\u00b7men", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "La\u00df deine Aeltern sehn, was dir dein Knecht geschrieben.", "tokens": ["La\u00df", "dei\u00b7ne", "A\u00b7el\u00b7tern", "sehn", ",", "was", "dir", "dein", "Knecht", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "$,", "PWS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und sprich, er werde dich stets r\u00fchmen, ehren, lieben!", "tokens": ["Und", "sprich", ",", "er", "wer\u00b7de", "dich", "stets", "r\u00fch\u00b7men", ",", "eh\u00b7ren", ",", "lie\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil dein Verstand und Witz, der mehr sein Herze zwingt,", "tokens": ["Weil", "dein", "Ver\u00b7stand", "und", "Witz", ",", "der", "mehr", "sein", "Her\u00b7ze", "zwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "$,", "PRELS", "ADV", "VAINF", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als alle Sch\u00f6nheit thut, auch in die Ferne dringt;", "tokens": ["Als", "al\u00b7le", "Sch\u00f6n\u00b7heit", "thut", ",", "auch", "in", "die", "Fer\u00b7ne", "dringt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und mit vergn\u00fcgter Brust gestehen, und bekennen,", "tokens": ["Und", "mit", "ver\u00b7gn\u00fcg\u00b7ter", "Brust", "ge\u00b7ste\u00b7hen", ",", "und", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP", "$,", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er sch\u00e4tze sich begl\u00fcckt, sich deinen Knecht zu nennen.", "tokens": ["Er", "sch\u00e4t\u00b7ze", "sich", "be\u00b7gl\u00fcckt", ",", "sich", "dei\u00b7nen", "Knecht", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "VVPP", "$,", "PRF", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}