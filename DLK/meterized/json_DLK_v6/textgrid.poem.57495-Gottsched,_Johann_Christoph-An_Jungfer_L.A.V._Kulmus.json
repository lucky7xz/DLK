{"textgrid.poem.57495": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "An Jungfer L.A.V. Kulmus", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Poetinn! z\u00fcrne nicht, da\u00df sich ein Fremder wagt,", "tokens": ["Poe\u00b7tinn", "!", "z\u00fcr\u00b7ne", "nicht", ",", "da\u00df", "sich", "ein", "Frem\u00b7der", "wagt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PTKNEG", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Und dir den treusten Dank in schlechten Reimen sagt,", "tokens": ["Und", "dir", "den", "treus\u00b7ten", "Dank", "in", "schlech\u00b7ten", "Rei\u00b7men", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den dein Geschenk verdient. Die allerliebsten Zeilen,", "tokens": ["Den", "dein", "Ge\u00b7schenk", "ver\u00b7dient", ".", "Die", "al\u00b7ler\u00b7liebs\u00b7ten", "Zei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die du durch unsern Freund mir neulich zu ertheilen", "tokens": ["Die", "du", "durch", "un\u00b7sern", "Freund", "mir", "neu\u00b7lich", "zu", "er\u00b7thei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Geneigt beliebet hast, erfodern zweifelsfrey,", "tokens": ["Ge\u00b7neigt", "be\u00b7lie\u00b7bet", "hast", ",", "er\u00b7fo\u00b7dern", "zwei\u00b7fels\u00b7frey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "VAFIN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df ich dir, sch\u00f6nes Kind! daf\u00fcr erkenntlich sey.", "tokens": ["Da\u00df", "ich", "dir", ",", "sch\u00f6\u00b7nes", "Kind", "!", "da\u00b7f\u00fcr", "er\u00b7kennt\u00b7lich", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "ADJA", "NN", "$.", "PAV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Allein, wie stell ichs an? Was giebt dein Knecht dir wieder?", "tokens": ["Al\u00b7lein", ",", "wie", "stell", "ichs", "an", "?", "Was", "giebt", "dein", "Knecht", "dir", "wie\u00b7der", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ADJD", "PIS", "PTKVZ", "$.", "PWS", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich lese ganz entz\u00fcckt die geisterf\u00fcllten Lieder,", "tokens": ["Ich", "le\u00b7se", "ganz", "ent\u00b7z\u00fcckt", "die", "geis\u00b7ter\u00b7f\u00fcll\u00b7ten", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die du mir zugesandt, und seufz in meinem Sinn:", "tokens": ["Die", "du", "mir", "zu\u00b7ge\u00b7sandt", ",", "und", "seufz", "in", "mei\u00b7nem", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVPP", "$,", "KON", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach schade, da\u00df ich doch so weit von Danzig bin!", "tokens": ["Ach", "scha\u00b7de", ",", "da\u00df", "ich", "doch", "so", "weit", "von", "Dan\u00b7zig", "bin", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADV", "ADJD", "APPR", "NE", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie z\u00e4rtlich wollt ich dir allda zu danken wissen?", "tokens": ["Wie", "z\u00e4rt\u00b7lich", "wollt", "ich", "dir", "all\u00b7da", "zu", "dan\u00b7ken", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "PPER", "PAV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie z\u00e4rtlich wollt ich nicht die sch\u00f6nen H\u00e4nde k\u00fcssen,", "tokens": ["Wie", "z\u00e4rt\u00b7lich", "wollt", "ich", "nicht", "die", "sch\u00f6\u00b7nen", "H\u00e4n\u00b7de", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die ein so muntrer Geist belebt, bewegt und r\u00fchrt;", "tokens": ["Die", "ein", "so", "mun\u00b7trer", "Geist", "be\u00b7lebt", ",", "be\u00b7wegt", "und", "r\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "ADJA", "NN", "VVPP", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wann er den Dichterkiel, trotz allen M\u00e4nnern, f\u00fchrt.", "tokens": ["Wann", "er", "den", "Dich\u00b7ter\u00b7kiel", ",", "trotz", "al\u00b7len", "M\u00e4n\u00b7nern", ",", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "$,", "APPR", "PIAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Jedoch, ich bin entfernt! Wer kann durch achtzig Meilen,", "tokens": ["Je\u00b7doch", ",", "ich", "bin", "ent\u00b7fernt", "!", "Wer", "kann", "durch", "acht\u00b7zig", "Mei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "ADJD", "$.", "PWS", "VMFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dir alle Dankbarkeit, die du verdienst, ertheilen?", "tokens": ["Dir", "al\u00b7le", "Dank\u00b7bar\u00b7keit", ",", "die", "du", "ver\u00b7dienst", ",", "er\u00b7thei\u00b7len", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich kenne dich so gar nur deinem Geiste nach:", "tokens": ["Ich", "ken\u00b7ne", "dich", "so", "gar", "nur", "dei\u00b7nem", "Geis\u00b7te", "nach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dem Geiste, der so stark aus jeder Zeile brach,", "tokens": ["Dem", "Geis\u00b7te", ",", "der", "so", "stark", "aus", "je\u00b7der", "Zei\u00b7le", "brach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die du mir zugesandt; so, da\u00df ich schweren sollen,", "tokens": ["Die", "du", "mir", "zu\u00b7ge\u00b7sandt", ";", "so", ",", "da\u00df", "ich", "schwe\u00b7ren", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVPP", "$.", "ADV", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df deine Mutter sich in dir verj\u00fcngen wollen.", "tokens": ["Da\u00df", "dei\u00b7ne", "Mut\u00b7ter", "sich", "in", "dir", "ver\u00b7j\u00fcn\u00b7gen", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich weis, was Fischer mir von ihrer Klugheit pries;", "tokens": ["Ich", "weis", ",", "was", "Fi\u00b7scher", "mir", "von", "ih\u00b7rer", "Klug\u00b7heit", "pries", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "PWS", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als er und Gerlach sich in Leipzig sehen lie\u00df.", "tokens": ["Als", "er", "und", "Ger\u00b7lach", "sich", "in", "Leip\u00b7zig", "se\u00b7hen", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "NN", "PRF", "APPR", "NE", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So spricht schon Famens Mund von ihrem seltnen Wesen!", "tokens": ["So", "spricht", "schon", "Fa\u00b7mens", "Mund", "von", "ih\u00b7rem", "selt\u00b7nen", "We\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wiewohl ich hab es selbst von ihrer Hand gelesen,", "tokens": ["Wie\u00b7wohl", "ich", "hab", "es", "selbst", "von", "ih\u00b7rer", "Hand", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was sie f\u00fcr Geist, Vernunft und Wissenschaft besitzt,", "tokens": ["Was", "sie", "f\u00fcr", "Geist", ",", "Ver\u00b7nunft", "und", "Wis\u00b7sen\u00b7schaft", "be\u00b7sitzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wie die Weisheit ihr aus allen Worten blitzt.", "tokens": ["Und", "wie", "die", "Weis\u00b7heit", "ihr", "aus", "al\u00b7len", "Wor\u00b7ten", "blitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Von solcher M\u00fctter Zucht entstehen solche Kinder,", "tokens": ["Von", "sol\u00b7cher", "M\u00fct\u00b7ter", "Zucht", "ent\u00b7ste\u00b7hen", "sol\u00b7che", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als du, o Sch\u00f6ne! bist. Vor allem, wenn nicht minder", "tokens": ["Als", "du", ",", "o", "Sch\u00f6\u00b7ne", "!", "bist", ".", "Vor", "al\u00b7lem", ",", "wenn", "nicht", "min\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "FM", "NN", "$.", "VAFIN", "$.", "APPR", "PIS", "$,", "KOUS", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die V\u00e4ter eifrig sind, sie kl\u00fcglich zu erziehn:", "tokens": ["Die", "V\u00e4\u00b7ter", "eif\u00b7rig", "sind", ",", "sie", "kl\u00fcg\u00b7lich", "zu", "er\u00b7ziehn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da m\u00fcssen in der That dergleichen Pflanzen bl\u00fchn.", "tokens": ["Da", "m\u00fcs\u00b7sen", "in", "der", "That", "derg\u00b7lei\u00b7chen", "Pflan\u00b7zen", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "ART", "NN", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ach d\u00f6rft ich nur nicht selbst mein Unverm\u00f6gen schelten!", "tokens": ["Ach", "d\u00f6rft", "ich", "nur", "nicht", "selbst", "mein", "Un\u00b7ver\u00b7m\u00f6\u00b7gen", "schel\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie gerne wollt ich dir die G\u00fctigkeit vergelten,", "tokens": ["Wie", "ger\u00b7ne", "wollt", "ich", "dir", "die", "G\u00fc\u00b7tig\u00b7keit", "ver\u00b7gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die du mir schon erzeigt. Nimm diese Bl\u00e4tter an,", "tokens": ["Die", "du", "mir", "schon", "er\u00b7zeigt", ".", "Nimm", "die\u00b7se", "Bl\u00e4t\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "VVPP", "$.", "NE", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bis ich dir, sch\u00f6nes Kind! was bessers liefern kann.", "tokens": ["Bis", "ich", "dir", ",", "sch\u00f6\u00b7nes", "Kind", "!", "was", "bes\u00b7sers", "lie\u00b7fern", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "ADJA", "NN", "$.", "PWS", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das ist, du wei\u00dft es schon, die Poesie der Preu\u00dfen;", "tokens": ["Das", "ist", ",", "du", "wei\u00dft", "es", "schon", ",", "die", "Poe\u00b7sie", "der", "Preu\u00b7\u00dfen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Dadurch ich auch so gar die Nymphen hier in Mei\u00dfen", "tokens": ["Da\u00b7durch", "ich", "auch", "so", "gar", "die", "Nym\u00b7phen", "hier", "in", "Mei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "ADV", "ADV", "ADV", "ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Einst schamroth machen will. Denn sind dieselben gleich", "tokens": ["Einst", "scham\u00b7roth", "ma\u00b7chen", "will", ".", "Denn", "sind", "die\u00b7sel\u00b7ben", "gleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVINF", "VMFIN", "$.", "KON", "VAFIN", "PDAT", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "An Geist und Artigkeit, Verstand und Sch\u00f6nheit reich:", "tokens": ["An", "Geist", "und", "Ar\u00b7tig\u00b7keit", ",", "Ver\u00b7stand", "und", "Sch\u00f6n\u00b7heit", "reich", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So ist doch keine dir, so viel ich weis, in Sachsen,", "tokens": ["So", "ist", "doch", "kei\u00b7ne", "dir", ",", "so", "viel", "ich", "weis", ",", "in", "Sach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "PPER", "$,", "ADV", "ADV", "PPER", "PTKVZ", "$,", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So jung und zart du bist, im Dichten recht gewachsen.", "tokens": ["So", "jung", "und", "zart", "du", "bist", ",", "im", "Dich\u00b7ten", "recht", "ge\u00b7wach\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "PPER", "VAFIN", "$,", "APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du ehrst dein Vaterland durch deinen sch\u00f6nen Kiel,", "tokens": ["Du", "ehrst", "dein", "Va\u00b7ter\u00b7land", "durch", "dei\u00b7nen", "sch\u00f6\u00b7nen", "Kiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und Preu\u00dfen wird dereinst dein reines Seytenspiel", "tokens": ["Und", "Preu\u00b7\u00dfen", "wird", "de\u00b7reinst", "dein", "rei\u00b7nes", "Sey\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Aus Stolz verewigen. Drum fahre fort im Singen,", "tokens": ["Aus", "Stolz", "ve\u00b7re\u00b7wi\u00b7gen", ".", "Drum", "fah\u00b7re", "fort", "im", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$.", "PAV", "VVFIN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Du Muse deiner Zeit! Denn deine Lieder klingen", "tokens": ["Du", "Mu\u00b7se", "dei\u00b7ner", "Zeit", "!", "Denn", "dei\u00b7ne", "Lie\u00b7der", "klin\u00b7gen"], "