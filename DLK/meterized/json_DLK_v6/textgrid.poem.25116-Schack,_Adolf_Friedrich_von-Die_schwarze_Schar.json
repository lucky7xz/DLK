{"textgrid.poem.25116": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die schwarze Schar", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit dunklen Tschakos alle und Totenk\u00f6pfen drauf", "tokens": ["Mit", "dunk\u00b7len", "Tscha\u00b7kos", "al\u00b7le", "und", "To\u00b7ten\u00b7k\u00f6p\u00b7fen", "drauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PIAT", "KON", "NN", "PAV"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Eilten bei H\u00f6rnerschalle sie nach dem Zelte zuhauf.", "tokens": ["Eil\u00b7ten", "bei", "H\u00f6r\u00b7ner\u00b7schal\u00b7le", "sie", "nach", "dem", "Zel\u00b7te", "zu\u00b7hauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Und ehe sie drinnen waren, rief freudlich der Herzog schon:", "tokens": ["Und", "e\u00b7he", "sie", "drin\u00b7nen", "wa\u00b7ren", ",", "rief", "freud\u00b7lich", "der", "Her\u00b7zog", "schon", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VAFIN", "$,", "VVFIN", "ADJD", "ART", "NN", "ADV", "$."], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "\u00bbgegr\u00fc\u00dft, ihr schwarzen Husaren! Gegr\u00fc\u00dft, meine Rachelegion!\u00ab", "tokens": ["\u00bb", "ge\u00b7gr\u00fc\u00dft", ",", "ihr", "schwar\u00b7zen", "Hu\u00b7sa\u00b7ren", "!", "Ge\u00b7gr\u00fc\u00dft", ",", "mei\u00b7ne", "Ra\u00b7che\u00b7le\u00b7gi\u00b7on", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "$.", "NN", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+--+--+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Braven hie\u00df er sich setzen: \u00bbAchtsam eu'r Ohr mir geliehn!", "tokens": ["Die", "Bra\u00b7ven", "hie\u00df", "er", "sich", "set\u00b7zen", ":", "\u00bb", "Acht\u00b7sam", "eu'r", "Ohr", "mir", "ge\u00b7liehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$.", "$(", "FM.la", "FM.la", "FM.la", "PPER", "VVINF", "$."], "meter": "-+-+--+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mir sendete diesen Fetzen der Kaiser eben aus Wien;", "tokens": ["Mir", "sen\u00b7de\u00b7te", "die\u00b7sen", "Fet\u00b7zen", "der", "Kai\u00b7ser", "e\u00b7ben", "aus", "Wi\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "ART", "NN", "ADV", "APPR", "NE", "$."], "meter": "+-+-+-+--+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Mehr liebt er auf B\u00e4llen das Tanzen als Waffentanz in der Schlacht;", "tokens": ["Mehr", "liebt", "er", "auf", "B\u00e4l\u00b7len", "das", "Tan\u00b7zen", "als", "Waf\u00b7fen\u00b7tanz", "in", "der", "Schlacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+-+--+", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Drum hat er bei Znaym mit den Franzen jetzt seinen Frieden gemacht.", "tokens": ["Drum", "hat", "er", "bei", "Znaym", "mit", "den", "Fran\u00b7zen", "jetzt", "sei\u00b7nen", "Frie\u00b7den", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "APPR", "NE", "APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+--+--+-+--+", "measure": "amphibrach.tetra.plus"}}, "stanza.3": {"line.1": {"text": "\u00bbdamit ich ihn unterschreibe, schickt er den Wisch mir nun;", "tokens": ["\u00bb", "da\u00b7mit", "ich", "ihn", "un\u00b7ter\u00b7schrei\u00b7be", ",", "schickt", "er", "den", "Wisch", "mir", "nun", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Er denkt wohl, mit einem Weibe, wie ", "tokens": ["Er", "denkt", "wohl", ",", "mit", "ei\u00b7nem", "Wei\u00b7be", ",", "wie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "ART", "NN", "$,", "PWAV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch, da\u00df man Schurke mich hei\u00dfe, da\u00df Schande mich treffen mag,", "tokens": ["Doch", ",", "da\u00df", "man", "Schur\u00b7ke", "mich", "hei\u00b7\u00dfe", ",", "da\u00df", "Schan\u00b7de", "mich", "tref\u00b7fen", "mag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "NN", "PPER", "VVFIN", "$,", "KOUS", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn ich das Blatt nicht zerrei\u00dfe! Da liege, verfluchter Vertrag!\u00ab", "tokens": ["Wenn", "ich", "das", "Blatt", "nicht", "zer\u00b7rei\u00b7\u00dfe", "!", "Da", "lie\u00b7ge", ",", "ver\u00b7fluch\u00b7ter", "Ver\u00b7trag", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$.", "ADV", "VVFIN", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+--+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Er rief's, und zerrissen stoben umher die St\u00fccke Papier;", "tokens": ["Er", "rie\u00b7f's", ",", "und", "zer\u00b7ris\u00b7sen", "sto\u00b7ben", "um\u00b7her", "die", "St\u00fc\u00b7cke", "Pa\u00b7pier", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVPP", "VVFIN", "PTKVZ", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+--+-+--+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Jubelnden Ruf erhoben Gemeiner und Offizier;", "tokens": ["Ju\u00b7beln\u00b7den", "Ruf", "er\u00b7ho\u00b7ben", "Ge\u00b7mei\u00b7ner", "und", "Of\u00b7fi\u00b7zier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "NN", "KON", "NN", "$."], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Er aber: \u00bbMein Blut f\u00fchl' ich sieden und Glut auf den Wangen mir lohn,", "tokens": ["Er", "a\u00b7ber", ":", "\u00bb", "Mein", "Blut", "f\u00fchl'", "ich", "sie\u00b7den", "und", "Glut", "auf", "den", "Wan\u00b7gen", "mir", "lohn", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$.", "$(", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "KON", "NN", "APPR", "ART", "NN", "PPER", "NN", "$,"], "meter": "-+--+--++-+--+--+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Sobald ich h\u00f6re von Frieden mit dem Unhold Napoleon.", "tokens": ["So\u00b7bald", "ich", "h\u00f6\u00b7re", "von", "Frie\u00b7den", "mit", "dem", "Un\u00b7hold", "Na\u00b7po\u00b7le\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+--+-+-+--+-+", "measure": "iambic.septa.relaxed"}}, "stanza.5": {"line.1": {"text": "Den Vater mir hat er erschlagen, mein Braunschweig mir geraubt;", "tokens": ["Den", "Va\u00b7ter", "mir", "hat", "er", "er\u00b7schla\u00b7gen", ",", "mein", "Braun\u00b7schweig", "mir", "ge\u00b7raubt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "PPER", "VVPP", "$,", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.2": {"text": "Nicht mochte mein Weib das tragen, fr\u00fch sank ihr bl\u00fchendes Haupt;", "tokens": ["Nicht", "moch\u00b7te", "mein", "Weib", "das", "tra\u00b7gen", ",", "fr\u00fch", "sank", "ihr", "bl\u00fc\u00b7hen\u00b7des", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPOSAT", "NN", "PDS", "VVINF", "$,", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dann \u00fcber dem Grab meiner Lieben sah ich von den Alpen zum Meer,", "tokens": ["Dann", "\u00fc\u00b7ber", "dem", "Grab", "mei\u00b7ner", "Lie\u00b7ben", "sah", "ich", "von", "den", "Al\u00b7pen", "zum", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPOSAT", "ADJA", "VVFIN", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+-+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Von H\u00f6llengeistern getrieben, hinjagen sein w\u00fctendes Heer.", "tokens": ["Von", "H\u00f6l\u00b7len\u00b7geis\u00b7tern", "ge\u00b7trie\u00b7ben", ",", "hin\u00b7ja\u00b7gen", "sein", "w\u00fc\u00b7ten\u00b7des", "Heer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Wie schreit noch aus D\u00f6rfern und St\u00e4dten zum Himmel um Rache der Brand!", "tokens": ["Wie", "schreit", "noch", "aus", "D\u00f6r\u00b7fern", "und", "St\u00e4d\u00b7ten", "zum", "Him\u00b7mel", "um", "Ra\u00b7che", "der", "Brand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "APPR", "NN", "KON", "NN", "APPRART", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "Wie hat dich der W\u00fctrich zertreten, mein deutsches Vaterland!", "tokens": ["Wie", "hat", "dich", "der", "W\u00fct\u00b7rich", "zer\u00b7tre\u00b7ten", ",", "mein", "deut\u00b7sches", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "Wie deine S\u00f6hne gesch\u00e4ndet, betrogen, verf\u00fchrt, entzweit,", "tokens": ["Wie", "dei\u00b7ne", "S\u00f6h\u00b7ne", "ge\u00b7sch\u00e4n\u00b7det", ",", "be\u00b7tro\u00b7gen", ",", "ver\u00b7f\u00fchrt", ",", "ent\u00b7zweit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVPP", "$,", "VVPP", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bis sie einander verblendet w\u00fcrgten im m\u00f6rdrischen Streit!", "tokens": ["Bis", "sie", "ein\u00b7an\u00b7der", "ver\u00b7blen\u00b7det", "w\u00fcrg\u00b7ten", "im", "m\u00f6rd\u00b7ri\u00b7schen", "Streit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+--+--+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Deine F\u00fcrsten, die stolzen Schildhalter von Kaiser und Reich,", "tokens": ["Dei\u00b7ne", "F\u00fcrs\u00b7ten", ",", "die", "stol\u00b7zen", "Schild\u00b7hal\u00b7ter", "von", "Kai\u00b7ser", "und", "Reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+---+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wie ist ihre Gr\u00f6\u00dfe geschmolzen, wie ward ihre Ehre so bleich!", "tokens": ["Wie", "ist", "ih\u00b7re", "Gr\u00f6\u00b7\u00dfe", "ge\u00b7schmol\u00b7zen", ",", "wie", "ward", "ih\u00b7re", "Eh\u00b7re", "so", "bleich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.3": {"text": "Vom fremden Unterdr\u00fccker nahmen zu Lehn sie den Thron", "tokens": ["Vom", "frem\u00b7den", "Un\u00b7ter\u00b7dr\u00fc\u00b7cker", "nah\u00b7men", "zu", "Lehn", "sie", "den", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "APPR", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und preisen ihn Weltbegl\u00fccker, indes sie zermalmt sein Hohn.", "tokens": ["Und", "prei\u00b7sen", "ihn", "Welt\u00b7be\u00b7gl\u00fc\u00b7cker", ",", "in\u00b7des", "sie", "zer\u00b7malmt", "sein", "Hohn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,", "KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.8": {"line.1": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Fort! fort! Sonst wird mich ersticken die deutsche Kerkerluft;", "tokens": ["Fort", "!", "fort", "!", "Sonst", "wird", "mich", "er\u00b7sti\u00b7cken", "die", "deut\u00b7sche", "Ker\u00b7ker\u00b7luft", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Hindurch uns zu schlagen zum Meere, ihr Freunde, f\u00fchr' ich euch an,", "tokens": ["Hin\u00b7durch", "uns", "zu", "schla\u00b7gen", "zum", "Mee\u00b7re", ",", "ihr", "Freun\u00b7de", ",", "f\u00fchr'", "ich", "euch", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "APPRART", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+--+-+--+", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Und fall' ich, so fall' ich mit Ehre als deutscher F\u00fcrst und Mann!\u00ab", "tokens": ["Und", "fall'", "ich", ",", "so", "fall'", "ich", "mit", "Eh\u00b7re", "als", "deut\u00b7scher", "F\u00fcrst", "und", "Mann", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN", "KOUS", "ADJA", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}}, "stanza.9": {"line.1": {"text": "Also der k\u00fchne Welfe; und rings auf sein Aufgebot", "tokens": ["Al\u00b7so", "der", "k\u00fch\u00b7ne", "Wel\u00b7fe", ";", "und", "rings", "auf", "sein", "Auf\u00b7ge\u00b7bot"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$.", "KON", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Erscholl es: \u00bbDa\u00df Gott uns helfe, wir folgen dir bis zum Tod!\u00ab", "tokens": ["Er\u00b7scholl", "es", ":", "\u00bb", "Da\u00df", "Gott", "uns", "hel\u00b7fe", ",", "wir", "fol\u00b7gen", "dir", "bis", "zum", "Tod", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "PPER", "$.", "$(", "KOUS", "NN", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$.", "$("], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Hand ihm zu k\u00fcssen dr\u00e4ngte sich J\u00e4ger heran und Husar,", "tokens": ["Die", "Hand", "ihm", "zu", "k\u00fcs\u00b7sen", "dr\u00e4ng\u00b7te", "sich", "J\u00e4\u00b7ger", "he\u00b7ran", "und", "Hu\u00b7sar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "VVFIN", "PRF", "NN", "PTKVZ", "KON", "NE", "$,"], "meter": "-+--+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und hurtig von dannen sprengte der Herzog mit seiner Schar.", "tokens": ["Und", "hur\u00b7tig", "von", "dan\u00b7nen", "spreng\u00b7te", "der", "Her\u00b7zog", "mit", "sei\u00b7ner", "Schar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.10": {"line.1": {"text": "Im Sturme vorw\u00e4rts brausend auf sch\u00e4umenden Rossen ging's;", "tokens": ["Im", "Stur\u00b7me", "vor\u00b7w\u00e4rts", "brau\u00b7send", "auf", "sch\u00e4u\u00b7men\u00b7den", "Ros\u00b7sen", "ging's", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Kaum waren sie ihrer tausend und der Feind unz\u00e4hlbar rings;", "tokens": ["Kaum", "wa\u00b7ren", "sie", "ih\u00b7rer", "tau\u00b7send", "und", "der", "Feind", "un\u00b7z\u00e4hl\u00b7bar", "rings", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "CARD", "KON", "ART", "NN", "ADJD", "ADV", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Doch ob st\u00e4rker ums Hundertfache, scheu lie\u00df er sie ziehn f\u00fcrba\u00df:", "tokens": ["Doch", "ob", "st\u00e4r\u00b7ker", "ums", "Hun\u00b7dert\u00b7fa\u00b7che", ",", "scheu", "lie\u00df", "er", "sie", "ziehn", "f\u00fcr\u00b7ba\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJD", "APPRART", "NN", "$,", "ADJD", "VVFIN", "PPER", "PPER", "VVINF", "VVFIN", "$."], "meter": "--+--+-+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "\u00bbweh, weh, das Corps der Rache, die schwarze Legion ist das!\u00ab", "tokens": ["\u00bb", "weh", ",", "weh", ",", "das", "Corps", "der", "Ra\u00b7che", ",", "die", "schwar\u00b7ze", "Le\u00b7gi\u00b7on", "ist", "das", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$,", "PTKVZ", "$,", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "VAFIN", "PDS", "$.", "$("], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.11": {"line.1": {"text": "Stach aber einen der Kitzel, sie zu hemmen auf ihrer Fahrt,", "tokens": ["Stach", "a\u00b7ber", "ei\u00b7nen", "der", "Kit\u00b7zel", ",", "sie", "zu", "hem\u00b7men", "auf", "ih\u00b7rer", "Fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ART", "NN", "$,", "PPER", "PTKZU", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+-++-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Bald hat er in dem Scharm\u00fctzel die welfische Kraft gewahrt!", "tokens": ["Bald", "hat", "er", "in", "dem", "Schar\u00b7m\u00fct\u00b7zel", "die", "wel\u00b7fi\u00b7sche", "Kraft", "ge\u00b7wahrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Denen, die heim geblieben, wenn er im Kampf nicht fiel,", "tokens": ["De\u00b7nen", ",", "die", "heim", "ge\u00b7blie\u00b7ben", ",", "wenn", "er", "im", "Kampf", "nicht", "fiel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ADJD", "VVPP", "$,", "KOUS", "PPER", "APPRART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Wu\u00dft' er von deutschen Hieben hinfort zu erz\u00e4hlen viel.", "tokens": ["Wu\u00dft'", "er", "von", "deut\u00b7schen", "Hie\u00b7ben", "hin\u00b7fort", "zu", "er\u00b7z\u00e4h\u00b7len", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.12": {"line.1": {"text": "\u00bbauf, Halberstadt zu erst\u00fcrmen!\u00ab erschallt's aus des Herzogs Mund.", "tokens": ["\u00bb", "auf", ",", "Hal\u00b7ber\u00b7stadt", "zu", "er\u00b7st\u00fcr\u00b7men", "!", "\u00ab", "er\u00b7schallt's", "aus", "des", "Her\u00b7zogs", "Mund", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "NN", "PTKZU", "VVINF", "$.", "$(", "PIS", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Erzspeiend von Mauern und T\u00fcrmen kracht der Kanonen Schlund;", "tokens": ["Erz\u00b7spei\u00b7end", "von", "Mau\u00b7ern", "und", "T\u00fcr\u00b7men", "kracht", "der", "Ka\u00b7no\u00b7nen", "Schlund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+--+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Aber den Flammen entgegen, die den Tod auf sie spr\u00fchn,", "tokens": ["A\u00b7ber", "den", "Flam\u00b7men", "ent\u00b7ge\u00b7gen", ",", "die", "den", "Tod", "auf", "sie", "spr\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKVZ", "$,", "PRELS", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Dem zischenden Kugelregen werfen die Schwarzen sich k\u00fchn.", "tokens": ["Dem", "zi\u00b7schen\u00b7den", "Ku\u00b7gel\u00b7re\u00b7gen", "wer\u00b7fen", "die", "Schwar\u00b7zen", "sich", "k\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PRF", "ADJD", "$."], "meter": "-+--+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "Der F\u00fchrer st\u00fcrmt, der kecke, den andern voran zum Thor,", "tokens": ["Der", "F\u00fch\u00b7rer", "st\u00fcrmt", ",", "der", "ke\u00b7cke", ",", "den", "an\u00b7dern", "vo\u00b7ran", "zum", "Thor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "$,", "ART", "ADJA", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Unter ihm sinkt sein Schecke, zu Fu\u00df dann dringt er vor;", "tokens": ["Un\u00b7ter", "ihm", "sinkt", "sein", "Sche\u00b7cke", ",", "zu", "Fu\u00df", "dann", "dringt", "er", "vor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "NN", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Schon ist eine Bresche geschossen, er wirft sich, der erste, hinein:", "tokens": ["Schon", "ist", "ei\u00b7ne", "Bre\u00b7sche", "ge\u00b7schos\u00b7sen", ",", "er", "wirft", "sich", ",", "der", "ers\u00b7te", ",", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "wor