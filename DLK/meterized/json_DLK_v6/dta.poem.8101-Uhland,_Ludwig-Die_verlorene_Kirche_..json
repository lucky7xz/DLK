{"dta.poem.8101": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die verlorene Kirche .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.57", "sv:0.42"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Man h\u00f6ret oft im fernen Wald", "tokens": ["Man", "h\u00f6\u00b7ret", "oft", "im", "fer\u00b7nen", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von obenher ein dumpfes L\u00e4uten,", "tokens": ["Von", "o\u00b7ben\u00b7her", "ein", "dum\u00b7pfes", "L\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Niemand wei\u00df, von wann es hallt,", "tokens": ["Doch", "Nie\u00b7mand", "wei\u00df", ",", "von", "wann", "es", "hallt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "APPR", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kaum die Sage kann es deuten.", "tokens": ["Und", "kaum", "die", "Sa\u00b7ge", "kann", "es", "deu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von der verlornen Kirche soll", "tokens": ["Von", "der", "ver\u00b7lor\u00b7nen", "Kir\u00b7che", "soll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Klang ert\u00f6nen mit den Winden;", "tokens": ["Der", "Klang", "er\u00b7t\u00f6\u00b7nen", "mit", "den", "Win\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Einst war der Pfad von Wallern voll,", "tokens": ["Einst", "war", "der", "Pfad", "von", "Wal\u00b7lern", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nun wei\u00df ihn Keiner mehr zu finden.", "tokens": ["Nun", "wei\u00df", "ihn", "Kei\u00b7ner", "mehr", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "J\u00fcngst ging ich in dem Walde weit,", "tokens": ["J\u00fcngst", "ging", "ich", "in", "dem", "Wal\u00b7de", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo kein betretner Steig sich dehnet,", "tokens": ["Wo", "kein", "be\u00b7tret\u00b7ner", "Steig", "sich", "deh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Aus der Verderbni\u00df dieser Zeit", "tokens": ["Aus", "der", "Ver\u00b7der\u00b7bni\u00df", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hatt\u2019 ich zu Gott mich hingesehnet.", "tokens": ["Hatt'", "ich", "zu", "Gott", "mich", "hin\u00b7ge\u00b7seh\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo in der Wildni\u00df Alles schwieg,", "tokens": ["Wo", "in", "der", "Wild\u00b7ni\u00df", "Al\u00b7les", "schwieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vernahm ich das Gel\u00e4ute wieder,", "tokens": ["Ver\u00b7nahm", "ich", "das", "Ge\u00b7l\u00e4u\u00b7te", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Je h\u00f6her meine Sehnsucht stieg,", "tokens": ["Je", "h\u00f6\u00b7her", "mei\u00b7ne", "Sehn\u00b7sucht", "stieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Je n\u00e4her, voller klang es nieder.", "tokens": ["Je", "n\u00e4\u00b7her", ",", "vol\u00b7ler", "klang", "es", "nie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJA", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Geist war so in sich gekehrt,", "tokens": ["Mein", "Geist", "war", "so", "in", "sich", "ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPR", "PRF", "VVPP", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Mein Sinn vom Klange hingenommen,", "tokens": ["Mein", "Sinn", "vom", "Klan\u00b7ge", "hin\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df mir es immer unerkl\u00e4rt,", "tokens": ["Da\u00df", "mir", "es", "im\u00b7mer", "un\u00b7er\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ich so hoch hinauf gekommen.", "tokens": ["Wie", "ich", "so", "hoch", "hin\u00b7auf", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mir schien es mehr denn hundert Jahr\u2019,", "tokens": ["Mir", "schien", "es", "mehr", "denn", "hun\u00b7dert", "Jahr'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich so hingetr\u00e4umet h\u00e4tte:", "tokens": ["Da\u00df", "ich", "so", "hin\u00b7ge\u00b7tr\u00e4u\u00b7met", "h\u00e4t\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als \u00fcber Nebeln, sonneklar,", "tokens": ["Als", "\u00fc\u00b7ber", "Ne\u00b7beln", ",", "son\u00b7ne\u00b7klar", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich \u00f6ffnet\u2019 eine freie St\u00e4tte.", "tokens": ["Sich", "\u00f6ff\u00b7net'", "ei\u00b7ne", "frei\u00b7e", "St\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Himmel war so dunkelblau,", "tokens": ["Der", "Him\u00b7mel", "war", "so", "dun\u00b7kel\u00b7blau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sonne war so voll und gl\u00fchend,", "tokens": ["Die", "Son\u00b7ne", "war", "so", "voll", "und", "gl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und eines M\u00fcnsters stolzer Bau", "tokens": ["Und", "ei\u00b7nes", "M\u00fcns\u00b7ters", "stol\u00b7zer", "Bau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stand in dem goldnen Lichte bl\u00fchend.", "tokens": ["Stand", "in", "dem", "gold\u00b7nen", "Lich\u00b7te", "bl\u00fc\u00b7hend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mir d\u00fcnkten helle Wolken ihn,", "tokens": ["Mir", "d\u00fcnk\u00b7ten", "hel\u00b7le", "Wol\u00b7ken", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gleich Fittigen, emporzuheben,", "tokens": ["Gleich", "Fit\u00b7ti\u00b7gen", ",", "em\u00b7por\u00b7zu\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und seines Thurmes Spitze schien", "tokens": ["Und", "sei\u00b7nes", "Thur\u00b7mes", "Spit\u00b7ze", "schien"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im sel\u2019gen Himmel zu verschweben.", "tokens": ["Im", "sel'\u00b7gen", "Him\u00b7mel", "zu", "ver\u00b7schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Glocke wonnevoller Klang", "tokens": ["Der", "Glo\u00b7cke", "won\u00b7ne\u00b7vol\u00b7ler", "Klang"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ert\u00f6nte sch\u00fctternd in dem Thurme,", "tokens": ["Er\u00b7t\u00f6n\u00b7te", "sch\u00fct\u00b7ternd", "in", "dem", "Thur\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch zog nicht Menschenhand den Strang,", "tokens": ["Doch", "zog", "nicht", "Men\u00b7schen\u00b7hand", "den", "Strang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie ward bewegt von heil\u2019gem Sturme.", "tokens": ["Sie", "ward", "be\u00b7wegt", "von", "heil'\u00b7gem", "Stur\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mir war\u2019s, derselbe Sturm und Strom", "tokens": ["Mir", "wa\u00b7r's", ",", "der\u00b7sel\u00b7be", "Sturm", "und", "Strom"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PDAT", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "H\u00e4tt\u2019 an mein klopfend Herz geschlagen,", "tokens": ["H\u00e4tt'", "an", "mein", "klop\u00b7fend", "Herz", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So trat ich in den hohen Dom", "tokens": ["So", "trat", "ich", "in", "den", "ho\u00b7hen", "Dom"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit schwankem Schritt und freud\u2019gem Zagen.", "tokens": ["Mit", "schwan\u00b7kem", "Schritt", "und", "freu\u00b7d'\u00b7gem", "Za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Wie mir in jenen Hallen war,", "tokens": ["Wie", "mir", "in", "je\u00b7nen", "Hal\u00b7len", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PDAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das kann ich nicht mit Worten schildern.", "tokens": ["Das", "kann", "ich", "nicht", "mit", "Wor\u00b7ten", "schil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Fenster gl\u00fchten dunkelklar", "tokens": ["Die", "Fens\u00b7ter", "gl\u00fch\u00b7ten", "dun\u00b7kel\u00b7klar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit aller M\u00e4rtrer frommen Bildern;", "tokens": ["Mit", "al\u00b7ler", "M\u00e4r\u00b7trer", "from\u00b7men", "Bil\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dann sah ich, wundersam erhellt,", "tokens": ["Dann", "sah", "ich", ",", "wun\u00b7der\u00b7sam", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Bild zum Leben sich erweitern,", "tokens": ["Das", "Bild", "zum", "Le\u00b7ben", "sich", "er\u00b7wei\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich sah hinaus in eine Welt", "tokens": ["Ich", "sah", "hin\u00b7aus", "in", "ei\u00b7ne", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von heil\u2019gen Frauen, Gottesstreitern.", "tokens": ["Von", "heil'\u00b7gen", "Frau\u00b7en", ",", "Got\u00b7tes\u00b7strei\u00b7tern", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich kniete nieder am Altar,", "tokens": ["Ich", "knie\u00b7te", "nie\u00b7der", "am", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Lieb\u2019 und Andacht ganz durchstralet.", "tokens": ["Von", "Lieb'", "und", "An\u00b7dacht", "ganz", "durch\u00b7stra\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hoch oben an der Decke war", "tokens": ["Hoch", "o\u00b7ben", "an", "der", "De\u00b7cke", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Himmels Glorie gemalet;", "tokens": ["Des", "Him\u00b7mels", "Glo\u00b7rie", "ge\u00b7ma\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Doch als ich wieder sah empor,", "tokens": ["Doch", "als", "ich", "wie\u00b7der", "sah", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da war gesprengt der Kuppel Bogen,", "tokens": ["Da", "war", "ge\u00b7sprengt", "der", "Kup\u00b7pel", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ge\u00f6ffnet war des Himmels Thor", "tokens": ["Ge\u00b7\u00f6ff\u00b7net", "war", "des", "Him\u00b7mels", "Thor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und jede H\u00fclle weggezogen.", "tokens": ["Und", "je\u00b7de", "H\u00fcl\u00b7le", "weg\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Was ich f\u00fcr Herrlichkeit geschaut", "tokens": ["Was", "ich", "f\u00fcr", "Herr\u00b7lich\u00b7keit", "ge\u00b7schaut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit still anbetendem Erstaunen,", "tokens": ["Mit", "still", "an\u00b7be\u00b7ten\u00b7dem", "Er\u00b7stau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was ich geh\u00f6rt f\u00fcr sel\u2019gen Laut,", "tokens": ["Was", "ich", "ge\u00b7h\u00f6rt", "f\u00fcr", "sel'\u00b7gen", "Laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Orgel mehr und als Posaunen:", "tokens": ["Als", "Or\u00b7gel", "mehr", "und", "als", "Po\u00b7sau\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "KON", "KOUS", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das steht nicht in der Worte Macht,", "tokens": ["Das", "steht", "nicht", "in", "der", "Wor\u00b7te", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": 