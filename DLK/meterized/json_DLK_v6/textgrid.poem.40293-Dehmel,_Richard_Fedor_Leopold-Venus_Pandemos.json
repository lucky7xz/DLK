{"textgrid.poem.40293": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Venus Pandemos", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und jenes letzte Mal. Im Nachtcaf\u00e9", "tokens": ["Und", "je\u00b7nes", "letz\u00b7te", "Mal", ".", "Im", "Nacht\u00b7ca\u00b7f\u00e9"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN", "$.", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der Vorstadt wieder, m\u00fcde vom Geruch", "tokens": ["der", "Vor\u00b7stadt", "wie\u00b7der", ",", "m\u00fc\u00b7de", "vom", "Ge\u00b7ruch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der schw\u00fclen Sofapl\u00fcsche und des Punsches,", "tokens": ["der", "schw\u00fc\u00b7len", "So\u00b7fa\u00b7pl\u00fc\u00b7sche", "und", "des", "Pun\u00b7sches", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "der vor mir gl\u00fchte, und vom Frauendunst", "tokens": ["der", "vor", "mir", "gl\u00fch\u00b7te", ",", "und", "vom", "Frau\u00b7en\u00b7dunst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "der feuchten Winterkleider; m\u00fcde, l\u00fcstern.", "tokens": ["der", "feuch\u00b7ten", "Win\u00b7ter\u00b7klei\u00b7der", ";", "m\u00fc\u00b7de", ",", "l\u00fcs\u00b7tern", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADJD", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Tabakswolken schwankten vom Gel\u00e4chter", "tokens": ["Die", "Ta\u00b7baks\u00b7wol\u00b7ken", "schwank\u00b7ten", "vom", "Ge\u00b7l\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und feilschenden Gekreisch der bunten Dirnen", "tokens": ["und", "feil\u00b7schen\u00b7den", "Ge\u00b7kreisch", "der", "bun\u00b7ten", "Dir\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "und Derer, die drum warben; das Gerassel", "tokens": ["und", "De\u00b7rer", ",", "die", "drum", "war\u00b7ben", ";", "das", "Ge\u00b7ras\u00b7sel"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "PRELS", "PAV", "VVINF", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "der Alfenidel\u00f6ffel am B\u00fcffett", "tokens": ["der", "Al\u00b7fe\u00b7ni\u00b7de\u00b7l\u00f6f\u00b7fel", "am", "B\u00fcf\u00b7fett"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "ermunterte den L\u00e4rm des Liebesmarktes,", "tokens": ["er\u00b7mun\u00b7ter\u00b7te", "den", "L\u00e4rm", "des", "Lie\u00b7bes\u00b7mark\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "ununterbrochen, wie ein Tamburin.", "tokens": ["un\u00b7un\u00b7ter\u00b7bro\u00b7chen", ",", "wie", "ein", "Tam\u00b7bu\u00b7rin", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ich sa\u00df, den langen Mittelgang betrachtend,", "tokens": ["Ich", "sa\u00df", ",", "den", "lan\u00b7gen", "Mit\u00b7tel\u00b7gang", "be\u00b7trach\u00b7tend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "und lauschte, wie das Licht des Gaskronleuchters,", "tokens": ["und", "lauschte", ",", "wie", "das", "Licht", "des", "Gas\u00b7kron\u00b7leuch\u00b7ters", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "der dr\u00fcber hing, sich m\u00fchsam mit den Farben", "tokens": ["der", "dr\u00fc\u00b7ber", "hing", ",", "sich", "m\u00fch\u00b7sam", "mit", "den", "Far\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PAV", "VVFIN", "$,", "PRF", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "auf den Gesichter um die Marmortische", "tokens": ["auf", "den", "Ge\u00b7sich\u00b7ter", "um", "die", "Mar\u00b7mor\u00b7ti\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "++-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.16": {"text": "in seiner gelben Sprache unterhielt;", "tokens": ["in", "sei\u00b7ner", "gel\u00b7ben", "Spra\u00b7che", "un\u00b7ter\u00b7hielt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "wozu der schwarze Marmor blank auflachte.", "tokens": ["wo\u00b7zu", "der", "schwar\u00b7ze", "Mar\u00b7mor", "blank", "auf\u00b7lach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Ich war schon bei der Wahl. Da teilte sich", "tokens": ["Ich", "war", "schon", "bei", "der", "Wahl", ".", "Da", "teil\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "$.", "ADV", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "die rote Th\u00fcrgardine neben mir:", "tokens": ["die", "ro\u00b7te", "Th\u00fcr\u00b7gar\u00b7di\u00b7ne", "ne\u00b7ben", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "ein neues Paar trat ein. Ein kalter Zug", "tokens": ["ein", "neu\u00b7es", "Paar", "trat", "ein", ".", "Ein", "kal\u00b7ter", "Zug"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "schnitt durch den hei\u00dfen Raum, und Einer fluchte;", "tokens": ["schnitt", "durch", "den", "hei\u00b7\u00dfen", "Raum", ",", "und", "Ei\u00b7ner", "fluch\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "die Beiden schritten ruhig durch den Schwarm.", "tokens": ["die", "Bei\u00b7den", "schrit\u00b7ten", "ru\u00b7hig", "durch", "den", "Schwarm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Mir grade gegen\u00fcber, quer am Ende", "tokens": ["Mir", "gra\u00b7de", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ",", "quer", "am", "En\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "PTKVZ", "$,", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "des Ganges, als beherrschten sie den Saal,", "tokens": ["des", "Gan\u00b7ges", ",", "als", "be\u00b7herrschten", "sie", "den", "Saal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.25": {"text": "nahmen sie Platz; der broncene Kronleuchter", "tokens": ["nah\u00b7men", "sie", "Platz", ";", "der", "bron\u00b7ce\u00b7ne", "Kron\u00b7leuch\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.26": {"text": "hing \u00fcber ihnen wie ein schwerer, alter", "tokens": ["hing", "\u00fc\u00b7ber", "ih\u00b7nen", "wie", "ein", "schwe\u00b7rer", ",", "al\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "APPR", "PPER", "KOKOM", "ART", "ADJA", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Thronhimmel; Keiner schien das Paar zu kennen.", "tokens": ["Thron\u00b7him\u00b7mel", ";", "Kei\u00b7ner", "schien", "das", "Paar", "zu", "ken\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PIS", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Doch h\u00f6rt'ich rechts von mir ein heisres Stimmchen:", "tokens": ["Doch", "h\u00f6rt'\u00b7ich", "rechts", "von", "mir", "ein", "heis\u00b7res", "Stimm\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "\u00bbbejejent mu\u00df ik Die woll schon wo sein!\u00ab \u2013", "tokens": ["\u00bb", "be\u00b7je\u00b7jent", "mu\u00df", "ik", "Die", "woll", "schon", "wo", "sein", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVPP", "VMFIN", "NE", "ART", "ADV", "ADV", "PWAV", "VAINF", "$.", "$(", "$("], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.30": {"text": "Er sa\u00df ganz still. Das laute Grau der Luft", "tokens": ["Er", "sa\u00df", "ganz", "still", ".", "Das", "lau\u00b7te", "Grau", "der", "Luft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$.", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "schrak fast zur\u00fcck vor seiner krassen Stirne,", "tokens": ["schrak", "fast", "zu\u00b7r\u00fcck", "vor", "sei\u00b7ner", "kras\u00b7sen", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "die wachsbleich an die schwachen Haare stie\u00df;", "tokens": ["die", "wachs\u00b7bleich", "an", "die", "schwa\u00b7chen", "Haa\u00b7re", "stie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "die gro\u00dfen, blassen Augenlider waren", "tokens": ["die", "gro\u00b7\u00dfen", ",", "blas\u00b7sen", "Au\u00b7gen\u00b7li\u00b7der", "wa\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "tief zugeklappt, auf beiden Seiten lag", "tokens": ["tief", "zu\u00b7ge\u00b7klappt", ",", "auf", "bei\u00b7den", "Sei\u00b7ten", "lag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "APPR", "PIAT", "NN", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.35": {"text": "ihr Schatten um die eingeknickte Nase,", "tokens": ["ihr", "Schat\u00b7ten", "um", "die", "ein\u00b7ge\u00b7knick\u00b7te", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "der d\u00fcrre Vollbart lie\u00df die Haut durchscheinen.", "tokens": ["der", "d\u00fcr\u00b7re", "Voll\u00b7bart", "lie\u00df", "die", "Haut", "durch\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Nur wenn die \u00fcppig kleinere Gef\u00e4hrtin", "tokens": ["Nur", "wenn", "die", "\u00fcp\u00b7pig", "klei\u00b7ne\u00b7re", "Ge\u00b7f\u00e4hr\u00b7tin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "ihm kichernd einen Satz zuzischelte,", "tokens": ["ihm", "ki\u00b7chernd", "ei\u00b7nen", "Satz", "zu\u00b7zi\u00b7schel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.39": {"text": "sah man sein eines schwarzes Auge halb", "tokens": ["sah", "man", "sein", "ei\u00b7nes", "schwar\u00b7zes", "Au\u00b7ge", "halb"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPOSAT", "ART", "ADJA", "NN", "ADJD"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.40": {"text": "und drehte sich sein langer, d\u00fcnner Hals,", "tokens": ["und", "dreh\u00b7te", "sich", "sein", "lan\u00b7ger", ",", "d\u00fcn\u00b7ner", "Hals", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "langsam, und kroch der nackte Kehlkopf hoch,", "tokens": ["lang\u00b7sam", ",", "und", "kroch", "der", "nack\u00b7te", "Kehl\u00b7kopf", "hoch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.42": {"text": "wie wenn ein Geier nach dem Aase ruckt.", "tokens": ["wie", "wenn", "ein", "Gei\u00b7er", "nach", "dem", "Aa\u00b7se", "ruckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Es wurde immer stiller durch den Raum;", "tokens": ["Es", "wur\u00b7de", "im\u00b7mer", "stil\u00b7ler", "durch", "den", "Raum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "sie sahen Alle auf den stummen Mann", "tokens": ["sie", "sa\u00b7hen", "Al\u00b7le", "auf", "den", "stum\u00b7men", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "und auf das sonderbar geduckte Weib.", "tokens": ["und", "auf", "das", "son\u00b7der\u00b7bar", "ge\u00b7duck\u00b7te", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "\u00bbsie ist ganz jung\u00ab, war um mich her ein Fl\u00fcstern;", "tokens": ["\u00bb", "sie", "ist", "ganz", "jung", "\u00ab", ",", "war", "um", "mich", "her", "ein", "Fl\u00fcs\u00b7tern", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADJD", "$(", "$,", "VAFIN", "APPR", "PRF", "APZR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "auch trank sie Milch, und gierig wie ein Kind.", "tokens": ["auch", "trank", "sie", "Milch", ",", "und", "gie\u00b7rig", "wie", "ein", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "KON", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Doch schien sie mir fast alt, so oft die Zunge", "tokens": ["Doch", "schien", "sie", "mir", "fast", "alt", ",", "so", "oft", "die", "Zun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word