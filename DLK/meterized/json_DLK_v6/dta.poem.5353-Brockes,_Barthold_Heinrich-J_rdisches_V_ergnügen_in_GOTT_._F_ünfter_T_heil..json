{"dta.poem.5353": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mich \u00fcbereilte j\u00fcngst hier\u00fcber, als ich so lag, ein stilles", "tokens": ["Mich", "\u00fc\u00b7be\u00b7reil\u00b7te", "j\u00fcngst", "hier\u00b7\u00fc\u00b7ber", ",", "als", "ich", "so", "lag", ",", "ein", "stil\u00b7les"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Grauen,", "tokens": ["Grau\u00b7en", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Mich in so einer hohlen Tieffe und Boden-losen Abgrunds-", "tokens": ["Mich", "in", "so", "ei\u00b7ner", "hoh\u00b7len", "Tief\u00b7fe", "und", "Bo\u00b7den\u00b7lo\u00b7sen", "Ab\u00b7grunds"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADV", "ART", "ADJA", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Gr\u00fcnden,", "tokens": ["Gr\u00fcn\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Umringt von nichts als dunckeln Schatten und Finster-", "tokens": ["Um\u00b7ringt", "von", "nichts", "als", "dun\u00b7ckeln", "Schat\u00b7ten", "und", "Fins\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PIS", "KOKOM", "ADJA", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "ni\u00df, allein zu finden:", "tokens": ["ni\u00df", ",", "al\u00b7lein", "zu", "fin\u00b7den", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Allein wie bald verschwand es wieder, als ich, mit fast", "tokens": ["Al\u00b7lein", "wie", "bald", "ver\u00b7schwand", "es", "wie\u00b7der", ",", "als", "ich", ",", "mit", "fast"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "KOKOM", "ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "$,", "APPR", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "halb-seel'ger Lust,", "tokens": ["halb\u00b7seel'\u00b7ger", "Lust", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "In dieser Boden-losen Tieffe, worin ich gleichsam schien", "tokens": ["In", "die\u00b7ser", "Bo\u00b7den\u00b7lo\u00b7sen", "Tief\u00b7fe", ",", "wo\u00b7rin", "ich", "gleich\u00b7sam", "schien"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN", "$,", "PWAV", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "zu schweben,", "tokens": ["zu", "schwe\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Von der allgegenw\u00e4rtgen GOttheit mich \u00fcberall bedeckt,", "tokens": ["Von", "der", "all\u00b7ge\u00b7gen\u00b7w\u00e4rt\u00b7gen", "Got\u00b7theit", "mich", "\u00fc\u00b7be\u00b7rall", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "ADV", "ADJD", "$,"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.12": {"text": "umgeben,", "tokens": ["um\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Ja fast ber\u00fchret gleichsam f\u00fchlte! Ich, der ich, meinem", "tokens": ["Ja", "fast", "be\u00b7r\u00fch\u00b7ret", "gleich\u00b7sam", "f\u00fchl\u00b7te", "!", "Ich", ",", "der", "ich", ",", "mei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["PTKANT", "ADV", "VVFIN", "ADJD", "VVFIN", "$.", "PPER", "$,", "PRELS", "PPER", "$,", "PPOSAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "GoTT so nah,", "tokens": ["GoTT", "so", "nah", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Beym dunckeln Lichte der Natur, mit hellen Glaubens-", "tokens": ["Beym", "dun\u00b7ckeln", "Lich\u00b7te", "der", "Na\u00b7tur", ",", "mit", "hel\u00b7len", "Glau\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Augen sah;", "tokens": ["Au\u00b7gen", "sah", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Verehrte Danck-und Demuths-voll den hell- und doch ver-", "tokens": ["Ver\u00b7ehr\u00b7te", "Dan\u00b7ck\u00b7\u00b7und", "De\u00b7muths\u00b7voll", "den", "hell", "und", "doch", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "NE", "ART", "TRUNC", "KON", "ADV", "TRUNC"], "meter": "-+-+--+---+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "borgnen Schein", "tokens": ["borg\u00b7nen", "Schein"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Des ewgen Sch\u00f6pfers aller Dinge; ergab mich seiner Gnad", "tokens": ["Des", "ew\u00b7gen", "Sch\u00f6p\u00b7fers", "al\u00b7ler", "Din\u00b7ge", ";", "er\u00b7gab", "mich", "sei\u00b7ner", "Gnad"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "$.", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "allein,", "tokens": ["al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "Und schlieff, mit kindlichem Vertrauen, in seiner Hut ge-", "tokens": ["Und", "schlieff", ",", "mit", "kind\u00b7li\u00b7chem", "Ver\u00b7trau\u00b7en", ",", "in", "sei\u00b7ner", "Hut", "ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "lassen ein.", "tokens": ["las\u00b7sen", "ein", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}