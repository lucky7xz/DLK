{"textgrid.poem.43400": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jeder sch\u00f6pft aus seiner Quelle,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jeder sch\u00f6pft aus seiner Quelle,", "tokens": ["Je\u00b7der", "sch\u00f6pft", "aus", "sei\u00b7ner", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil sie ihm am n\u00e4chsten ist;", "tokens": ["Weil", "sie", "ihm", "am", "n\u00e4chs\u00b7ten", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "ADJA", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder mi\u00dft nach seiner Elle,", "tokens": ["Je\u00b7der", "mi\u00dft", "nach", "sei\u00b7ner", "El\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil er so am liebsten mi\u00dft.", "tokens": ["Weil", "er", "so", "am", "liebs\u00b7ten", "mi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKA", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fremde Stiefel passen selten,", "tokens": ["Frem\u00b7de", "Stie\u00b7fel", "pas\u00b7sen", "sel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem Kopf kauft man den Hut.", "tokens": ["Nach", "dem", "Kopf", "kauft", "man", "den", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIS", "ART", "NN", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Nur das Eigne l\u00e4\u00dft man gelten,", "tokens": ["Nur", "das", "Eig\u00b7ne", "l\u00e4\u00dft", "man", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn Gewohnheit macht es gut.", "tokens": ["Denn", "Ge\u00b7wohn\u00b7heit", "macht", "es", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und so bleibt uns fremdes Gute", "tokens": ["Und", "so", "bleibt", "uns", "frem\u00b7des", "Gu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fern von unsrer Eigenheit,", "tokens": ["Fern", "von", "uns\u00b7rer", "Ei\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Eigne wird die Ruthe,", "tokens": ["Und", "das", "Eig\u00b7ne", "wird", "die", "Ru\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die uns z\u00fcchtigt allezeit.", "tokens": ["Die", "uns", "z\u00fcch\u00b7tigt", "al\u00b7le\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}