{"textgrid.poem.44949": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Gro\u00dfe-M\u00e4nnerschaft", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Friedrich der Einzige? Nun, meiner Treu!", "tokens": ["Zwei", "Fried\u00b7rich", "der", "Ein\u00b7zi\u00b7ge", "?", "Nun", ",", "mei\u00b7ner", "Treu", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NE", "ART", "ADJA", "$.", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Fall w\u00e4re einzig und wahrhaft neu.", "tokens": ["Der", "Fall", "w\u00e4\u00b7re", "ein\u00b7zig", "und", "wahr\u00b7haft", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und da nun der Erste der Zweite schon war,", "tokens": ["Und", "da", "nun", "der", "Ers\u00b7te", "der", "Zwei\u00b7te", "schon", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "ART", "NN", "ADV", "VAFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "So k\u00e4me das Einmaleins in Gefahr.", "tokens": ["So", "k\u00e4\u00b7me", "das", "Ein\u00b7mal\u00b7eins", "in", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}