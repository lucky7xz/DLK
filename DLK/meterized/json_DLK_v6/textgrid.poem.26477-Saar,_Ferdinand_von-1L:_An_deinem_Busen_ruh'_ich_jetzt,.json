{"textgrid.poem.26477": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: An deinem Busen ruh' ich jetzt,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An deinem Busen ruh' ich jetzt,", "tokens": ["An", "dei\u00b7nem", "Bu\u00b7sen", "ruh'", "ich", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Natur, in stummer Gluth;", "tokens": ["Na\u00b7tur", ",", "in", "stum\u00b7mer", "Gluth", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Aug' noch eine Thr\u00e4ne netzt", "tokens": ["Mein", "Aug'", "noch", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "netzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von kaum versiegter Fluth.", "tokens": ["Von", "kaum", "ver\u00b7sieg\u00b7ter", "Fluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du aber willst es ganz befrei'n,", "tokens": ["Du", "a\u00b7ber", "willst", "es", "ganz", "be\u00b7frei'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du willst es trocknen auch,", "tokens": ["Du", "willst", "es", "trock\u00b7nen", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und saugst den herben Tropfen ein,", "tokens": ["Und", "saugst", "den", "her\u00b7ben", "Trop\u00b7fen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Inde\u00df mich k\u00fc\u00dft dein Hauch.", "tokens": ["In\u00b7de\u00df", "mich", "k\u00fc\u00dft", "dein", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie viele Thr\u00e4nen stiegen so", "tokens": ["Wie", "vie\u00b7le", "Thr\u00e4\u00b7nen", "stie\u00b7gen", "so"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon in des \u00c4thers Blau \u2013", "tokens": ["Schon", "in", "des", "\u00c4\u00b7thers", "Blau", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und fielen nieder licht und froh", "tokens": ["Und", "fie\u00b7len", "nie\u00b7der", "licht", "und", "froh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "NN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann auf die Welt als Thau!", "tokens": ["Dann", "auf", "die", "Welt", "als", "Thau", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}