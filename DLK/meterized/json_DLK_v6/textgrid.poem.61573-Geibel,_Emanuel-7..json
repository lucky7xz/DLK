{"textgrid.poem.61573": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Wind verhallt Trompetenton", "tokens": ["Im", "Wind", "ver\u00b7hallt", "Trom\u00b7pe\u00b7ten\u00b7ton"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ferner Paukenschlag;", "tokens": ["Und", "fer\u00b7ner", "Pau\u00b7ken\u00b7schlag", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es zieht durchs Feld die Prozession", "tokens": ["Es", "zieht", "durchs", "Feld", "die", "Pro\u00b7zes\u00b7si\u00b7on"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Am sch\u00f6nsten Fr\u00fchlingstag.", "tokens": ["Am", "sch\u00f6ns\u00b7ten", "Fr\u00fch\u00b7lings\u00b7tag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Fahnen wehn im Sonnenschein,", "tokens": ["Die", "Fah\u00b7nen", "wehn", "im", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kreuze blinken vorn;", "tokens": ["Die", "Kreu\u00b7ze", "blin\u00b7ken", "vorn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von tausend Stimmen murmelt's drein,", "tokens": ["Von", "tau\u00b7send", "Stim\u00b7men", "mur\u00b7melt's", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie flehn um Wein und Korn.", "tokens": ["Sie", "flehn", "um", "Wein", "und", "Korn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Weit hinterm Zug, versp\u00e4tet, geht", "tokens": ["Weit", "hin\u00b7term", "Zug", ",", "ver\u00b7sp\u00e4\u00b7tet", ",", "geht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADJD", "APPRART", "NN", "$,", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchs bl\u00fchnde Saatgewind',", "tokens": ["Durchs", "bl\u00fchn\u00b7de", "Saat\u00b7ge\u00b7wind'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Versunken in ihr still Gebet,", "tokens": ["Ver\u00b7sun\u00b7ken", "in", "ihr", "still", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein hold blau\u00e4ugig Kind.", "tokens": ["Ein", "hold", "blau\u00b7\u00e4u\u00b7gig", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr rosig Antlitz ist so klar,", "tokens": ["Ihr", "ro\u00b7sig", "Ant\u00b7litz", "ist", "so", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr wei\u00df Gewand so rein,", "tokens": ["Ihr", "wei\u00df", "Ge\u00b7wand", "so", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um ihre Stirn das goldne Haar", "tokens": ["Um", "ih\u00b7re", "Stirn", "das", "gold\u00b7ne", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Flie\u00dft wie ein Glorienschein.", "tokens": ["Flie\u00dft", "wie", "ein", "Glo\u00b7ri\u00b7en\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "So wallt sie hin, das s\u00fc\u00dfe Bild,", "tokens": ["So", "wallt", "sie", "hin", ",", "das", "s\u00fc\u00b7\u00dfe", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Palmzweig in der Hand,", "tokens": ["Den", "Palm\u00b7zweig", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als z\u00f6g' ein Engel durchs Gefild'", "tokens": ["Als", "z\u00f6g'", "ein", "En\u00b7gel", "durchs", "Ge\u00b7fild'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und segnete das Land.", "tokens": ["Und", "seg\u00b7ne\u00b7te", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}