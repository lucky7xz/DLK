{"textgrid.poem.27977": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "92.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mamm\u00e6a funckelt her an Sch\u00f6nheit wie die Sterne,", "tokens": ["Mam\u00b7m\u00e6a", "fun\u00b7ckelt", "her", "an", "Sch\u00f6n\u00b7heit", "wie", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Doch, welches seltsam ist, weicht Hoffart von ihr ferne;", "tokens": ["Doch", ",", "wel\u00b7ches", "selt\u00b7sam", "ist", ",", "weicht", "Hof\u00b7fart", "von", "ihr", "fer\u00b7ne", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ADJD", "VAFIN", "$,", "VVFIN", "NN", "APPR", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dann da\u00df sie gar nicht sich als andre besser deucht,", "tokens": ["Dann", "da\u00df", "sie", "gar", "nicht", "sich", "als", "and\u00b7re", "bes\u00b7ser", "deucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "PTKNEG", "PRF", "KOUS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das macht, da\u00df Fleisch und Blut sie auch im Busem reucht.", "tokens": ["Das", "macht", ",", "da\u00df", "Fleisch", "und", "Blut", "sie", "auch", "im", "Bu\u00b7sem", "reucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "NN", "KON", "NN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dahin nun grieff ein Freund gar unbedachten Mutes,", "tokens": ["Da\u00b7hin", "nun", "grieff", "ein", "Freund", "gar", "un\u00b7be\u00b7dach\u00b7ten", "Mu\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da fand er zwar viel Fleisch, Milch aber stat des Blutes.", "tokens": ["Da", "fand", "er", "zwar", "viel", "Fleisch", ",", "Milch", "a\u00b7ber", "stat", "des", "Blu\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}