{"dta.poem.18961": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "39.  \n  Leben/ Nebel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch/ bist du klug/", "tokens": ["Mensch", "/", "bist", "du", "klug", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VAFIN", "PPER", "ADJD", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Vnd wilt recht wissen was dein Leben;", "tokens": ["Vnd", "wilt", "recht", "wis\u00b7sen", "was", "dein", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So merck das w\u00f6rtlein ", "tokens": ["So", "merck", "das", "w\u00f6rt\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Da hast du gnug;", "tokens": ["Da", "hast", "du", "gnug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Li\u00df es zu ruck/ so wirst du sehen/", "tokens": ["Li\u00df", "es", "zu", "ruck", "/", "so", "wirst", "du", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$(", "ADV", "VAFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was es vnd wie es thut vergehen.", "tokens": ["Was", "es", "vnd", "wie", "es", "thut", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KON", "PWAV", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}