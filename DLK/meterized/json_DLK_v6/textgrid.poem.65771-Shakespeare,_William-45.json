{"textgrid.poem.65771": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "45", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die andern, lose Luft und l\u00e4uternd Feuer, hangen,", "tokens": ["Die", "an\u00b7dern", ",", "lo\u00b7se", "Luft", "und", "l\u00e4u\u00b7ternd", "Feu\u00b7er", ",", "han\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "KON", "ADJD", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo ich auch sein mag, immerfort an dir;", "tokens": ["Wo", "ich", "auch", "sein", "mag", ",", "im\u00b7mer\u00b7fort", "an", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAINF", "VMFIN", "$,", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Luft, mein Gedanke; Feuer, mein Verlangen,", "tokens": ["Luft", ",", "mein", "Ge\u00b7dan\u00b7ke", ";", "Feu\u00b7er", ",", "mein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Im schnellsten Flug sind sie bald dort, bald hier.", "tokens": ["Im", "schnells\u00b7ten", "Flug", "sind", "sie", "bald", "dort", ",", "bald", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "ADV", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn sie, die leichtern Elemente, eben", "tokens": ["Wenn", "sie", ",", "die", "leich\u00b7tern", "E\u00b7le\u00b7men\u00b7te", ",", "e\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "$,", "ART", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mit zarter Liebesbotschaft zu dir ziehn,", "tokens": ["Mit", "zar\u00b7ter", "Lie\u00b7bes\u00b7bot\u00b7schaft", "zu", "dir", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sinkt mein aus vieren gleichgeschaffnes Leben", "tokens": ["Sinkt", "mein", "aus", "vie\u00b7ren", "gleich\u00b7ge\u00b7schaff\u00b7nes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit zween allein in Todesschwermut hin:", "tokens": ["Mit", "zween", "al\u00b7lein", "in", "To\u00b7des\u00b7schwer\u00b7mut", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Bis sich die Lebensstoffe neu vereinen,", "tokens": ["Bis", "sich", "die", "Le\u00b7bens\u00b7stof\u00b7fe", "neu", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Mit jener raschen Boten Wiederkehr,", "tokens": ["Mit", "je\u00b7ner", "ra\u00b7schen", "Bo\u00b7ten", "Wie\u00b7der\u00b7kehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die eben jetzt von dir zur\u00fcck erscheinen,", "tokens": ["Die", "e\u00b7ben", "jetzt", "von", "dir", "zu\u00b7r\u00fcck", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "PPER", "PTKVZ", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Von deinem Wohlsein bringend sichre M\u00e4hr.", "tokens": ["Von", "dei\u00b7nem", "Wohl\u00b7sein", "brin\u00b7gend", "sich\u00b7re", "M\u00e4hr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Entz\u00fcckt vernehm' ich's, aber froh nicht lang,", "tokens": ["Ent\u00b7z\u00fcckt", "ver\u00b7nehm'", "ich's", ",", "a\u00b7ber", "froh", "nicht", "lang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "$,", "ADV", "ADJD", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Send' ich sie gleich zur\u00fcck, und bin gleich wieder bang.", "tokens": ["Send'", "ich", "sie", "gleich", "zu\u00b7r\u00fcck", ",", "und", "bin", "gleich", "wie\u00b7der", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$,", "KON", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}