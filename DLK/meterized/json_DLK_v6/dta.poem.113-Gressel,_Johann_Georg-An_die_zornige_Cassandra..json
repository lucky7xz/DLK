{"dta.poem.113": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die zornige  Cassandra.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Erz\u00fcrnte schauet doch zu euren zarten F\u00fcssen", "tokens": ["Er\u00b7z\u00fcrn\u00b7te", "schau\u00b7et", "doch", "zu", "eu\u00b7ren", "zar\u00b7ten", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Sclaven/ der sein Gl\u00fcck durch Unverstand ver-", "tokens": ["Den", "Scla\u00b7ven", "/", "der", "sein", "Gl\u00fcck", "durch", "Un\u00b7ver\u00b7stand", "ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "PPOSAT", "NN", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "schertzt;", "tokens": ["schertzt", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Den dreisten F\u00fcrwitz mu\u00df er gar zu herbe b\u00fcssen/", "tokens": ["Den", "dreis\u00b7ten", "F\u00fcr\u00b7witz", "mu\u00df", "er", "gar", "zu", "her\u00b7be", "b\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und eur entbrandter Grimm ihn in der Seelen schmertzt.", "tokens": ["Und", "eur", "ent\u00b7brand\u00b7ter", "Grimm", "ihn", "in", "der", "See\u00b7len", "schmertzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NE", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo ist der s\u00fcsse Blick/ der mich vor dem erquicket?", "tokens": ["Wo", "ist", "der", "s\u00fcs\u00b7se", "Blick", "/", "der", "mich", "vor", "dem", "er\u00b7quic\u00b7ket", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "$(", "PRELS", "PRF", "APPR", "ART", "VVFIN", "$."], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Ach Schmertz! ach Weh! er ist in strengen Blitz verkehrt/", "tokens": ["Ach", "Schmertz", "!", "ach", "Weh", "!", "er", "ist", "in", "stren\u00b7gen", "Blitz", "ver\u00b7kehrt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "XY", "NN", "$.", "PPER", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein ungemeiner Zorn mich mit Verbannung dr\u00fccket/", "tokens": ["Ein", "un\u00b7ge\u00b7mei\u00b7ner", "Zorn", "mich", "mit", "Ver\u00b7ban\u00b7nung", "dr\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Seele wird durch Angst und Unmuth gantz verzehrt.", "tokens": ["Die", "See\u00b7le", "wird", "durch", "Angst", "und", "Un\u00b7muth", "gantz", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich lebe fast nicht mehr weil ich so elend lebe/", "tokens": ["Ich", "le\u00b7be", "fast", "nicht", "mehr", "weil", "ich", "so", "e\u00b7lend", "le\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADV", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Kein Redner dr\u00fccket aus was meine Sinnen kr\u00e4nck/", "tokens": ["Kein", "Red\u00b7ner", "dr\u00fc\u00b7cket", "aus", "was", "mei\u00b7ne", "Sin\u00b7nen", "kr\u00e4nck", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PRELS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Tod umnebelt mich mit dicken Spinn-Gewebe/", "tokens": ["Der", "Tod", "um\u00b7ne\u00b7belt", "mich", "mit", "di\u00b7cken", "Spinn\u00b7Ge\u00b7we\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich bin in Angst und Noht bis an den Hals versenckt.", "tokens": ["Ich", "bin", "in", "Angst", "und", "Noht", "bis", "an", "den", "Hals", "ver\u00b7senckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "APPR", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich leugne nicht mein Schatz/ da\u00df ich mich grob versehen/", "tokens": ["Ich", "leug\u00b7ne", "nicht", "mein", "Schatz", "/", "da\u00df", "ich", "mich", "grob", "ver\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$(", "KOUS", "PPER", "PRF", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mein Fehler wird von mir mit grosser Reu erkannt/", "tokens": ["Mein", "Feh\u00b7ler", "wird", "von", "mir", "mit", "gros\u00b7ser", "Reu", "er\u00b7kannt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das krancke Hertze seuffzt/ die matten Lippen flehen/", "tokens": ["Das", "kran\u00b7cke", "Hert\u00b7ze", "seuffzt", "/", "die", "mat\u00b7ten", "Lip\u00b7pen", "fle\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$(", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und haben/ ach umsonst! viel Opffer angebrannt.", "tokens": ["Und", "ha\u00b7ben", "/", "ach", "um\u00b7sonst", "!", "viel", "Opf\u00b7fer", "an\u00b7ge\u00b7brannt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$(", "ITJ", "ADV", "$.", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Jhr z\u00fcrnet allzuviel/ und stellt euch mein Verbrechen", "tokens": ["Ihr", "z\u00fcr\u00b7net", "all\u00b7zu\u00b7viel", "/", "und", "stellt", "euch", "mein", "Ver\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "$(", "KON", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Viel gr\u00f6sser als es ist in solchem Eyfer f\u00fcr:", "tokens": ["Viel", "gr\u00f6s\u00b7ser", "als", "es", "ist", "in", "sol\u00b7chem", "Ey\u00b7fer", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "VAFIN", "APPR", "PIAT", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wie lange wolt ihr euch an eurem Sclaven r\u00e4chen?", "tokens": ["Wie", "lan\u00b7ge", "wolt", "ihr", "euch", "an", "eu\u00b7rem", "Scla\u00b7ven", "r\u00e4\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie lange spielt eur Zorn so wunderlich mit mir?", "tokens": ["Wie", "lan\u00b7ge", "spielt", "eur", "Zorn", "so", "wun\u00b7der\u00b7lich", "mit", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ach kehret doch zur\u00fcck! ach kehret! kehret wieder!", "tokens": ["Ach", "keh\u00b7ret", "doch", "zu\u00b7r\u00fcck", "!", "ach", "keh\u00b7ret", "!", "keh\u00b7ret", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "PTKVZ", "$.", "ADV", "VVFIN", "$.", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Hemmt meine Hertzens-Angst/ verk\u00fcrtzet meine Noht/", "tokens": ["Hemmt", "mei\u00b7ne", "Hert\u00b7zens\u00b7Angst", "/", "ver\u00b7k\u00fcrt\u00b7zet", "mei\u00b7ne", "Noht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Beseelt durch einen Blick die schon erstorbnen Glieder/", "tokens": ["Be\u00b7seelt", "durch", "ei\u00b7nen", "Blick", "die", "schon", "ers\u00b7torb\u00b7nen", "Glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Erquickt den matten Geist/ vertreibt den nahen Tod.", "tokens": ["Er\u00b7quickt", "den", "mat\u00b7ten", "Geist", "/", "ver\u00b7treibt", "den", "na\u00b7hen", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$(", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Was meine Hand ver\u00fcbt ist nicht durch sie vollf\u00fchret/", "tokens": ["Was", "mei\u00b7ne", "Hand", "ver\u00b7\u00fcbt", "ist", "nicht", "durch", "sie", "voll\u00b7f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "VAFIN", "PTKNEG", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der netten Glieder-Zier/ und was euch sch\u00f6ne macht", "tokens": ["Der", "net\u00b7ten", "Glie\u00b7der\u00b7Zier", "/", "und", "was", "euch", "sch\u00f6\u00b7ne", "macht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "PWS", "PPER", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Hat sie durch einen Trieb/ der m\u00e4chtig war/ regieret/", "tokens": ["Hat", "sie", "durch", "ei\u00b7nen", "Trieb", "/", "der", "m\u00e4ch\u00b7tig", "war", "/", "re\u00b7gie\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "$(", "ART", "ADJD", "VAFIN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Da\u00df sie den Griff gewagt/ eh ich es recht bedacht.", "tokens": ["Da\u00df", "sie", "den", "Griff", "ge\u00b7wagt", "/", "eh", "ich", "es", "recht", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$(", "KOUS", "PPER", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Erwegt nun ob ihr mich so hefftig hassen k\u00f6nnet/", "tokens": ["Er\u00b7wegt", "nun", "ob", "ihr", "mich", "so", "heff\u00b7tig", "has\u00b7sen", "k\u00f6n\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOUS", "PPER", "PRF", "ADV", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da euer Sch\u00f6nheit-Pracht den Zwang an mir ver\u00fcbt/", "tokens": ["Da", "eu\u00b7er", "Sch\u00f6n\u00b7heit\u00b7Pracht", "den", "Zwang", "an", "mir", "ver\u00b7\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Betrachtet ob ihr den mit Recht und anckbahr nennet/", "tokens": ["Be\u00b7trach\u00b7tet", "ob", "ihr", "den", "mit", "Recht", "und", "an\u00b7ck\u00b7bahr", "nen\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "PPER", "ART", "APPR", "NN", "KON", "ADJD", "VVFIN", "$("], "meter": "-+--+--+-+-+--", "measure": "amphibrach.tri.plus"}, "line.33": {"text": "Der euch aufs z\u00e4rtlichste mehr als sich selber liebt.", "tokens": ["Der", "euch", "aufs", "z\u00e4rt\u00b7lichs\u00b7te", "mehr", "als", "sich", "sel\u00b7ber", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "ADV", "KOUS", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Jhr thut mir Uberlast mehr als ich es verbrochen/", "tokens": ["Ihr", "thut", "mir", "U\u00b7ber\u00b7last", "mehr", "als", "ich", "es", "ver\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "KOUS", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Straffe ist zu hart damit ihr mich belegt;", "tokens": ["Die", "Straf\u00b7fe", "ist", "zu", "hart", "da\u00b7mit", "ihr", "mich", "be\u00b7legt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "PAV", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wenn ihr nun meine Schuld durch meinem Tod gerochen/", "tokens": ["Wenn", "ihr", "nun", "mei\u00b7ne", "Schuld", "durch", "mei\u00b7nem", "Tod", "ge\u00b7ro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So glaubt nicht/ da\u00df die Welt Hochachtung vor euch tr\u00e4gt.", "tokens": ["So", "glaubt", "nicht", "/", "da\u00df", "die", "Welt", "Hoch\u00b7ach\u00b7tung", "vor", "euch", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "$(", "KOUS", "ART", "NN", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Man wird eur Angesicht als einen Leuen fliehen/", "tokens": ["Man", "wird", "eur", "An\u00b7ge\u00b7sicht", "als", "ei\u00b7nen", "Leu\u00b7en", "flie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPOSAT", "NN", "KOKOM", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der mehr vergrelte Wuth als Beyleids-Zeichen zeigt/", "tokens": ["Der", "mehr", "ver\u00b7grel\u00b7te", "Wuth", "als", "Bey\u00b7leids\u00b7Zei\u00b7chen", "zeigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "KOUS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Man wird sich nicht zu sehr um eure Gunst bem\u00fchen/", "tokens": ["Man", "wird", "sich", "nicht", "zu", "sehr", "um", "eu\u00b7re", "Gunst", "be\u00b7m\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PRF", "PTKNEG", "PTKA", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Das Meer-Weib fliehet man/ weil sein Gesang be