{"textgrid.poem.48764": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "51. In ein Stambuch, zu Niesoway in Schirvan am Kaspischen Strande", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier braust mein naher Tod! und du, mein wahres Leben,", "tokens": ["Hier", "braust", "mein", "na\u00b7her", "Tod", "!", "und", "du", ",", "mein", "wah\u00b7res", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "KON", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "weinst, Bruder, lachend hier. Ich zweifle fast noch dran,", "tokens": ["weinst", ",", "Bru\u00b7der", ",", "la\u00b7chend", "hier", ".", "Ich", "zweif\u00b7le", "fast", "noch", "dran", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "ADJD", "ADV", "$.", "PPER", "VVFIN", "ADV", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ob ich der Warheit selbst itzt glauben darf und kan.", "tokens": ["ob", "ich", "der", "War\u00b7heit", "selbst", "itzt", "glau\u00b7ben", "darf", "und", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADV", "VVINF", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So hat mich Angst und Lust auf einmal ganz umgeben.", "tokens": ["So", "hat", "mich", "Angst", "und", "Lust", "auf", "ein\u00b7mal", "ganz", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "APPR", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du machst mir Freud und Trost, di\u00df Wasser Furcht und Beben,", "tokens": ["Du", "machst", "mir", "Freud", "und", "Trost", ",", "di\u00df", "Was\u00b7ser", "Furcht", "und", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,", "PDS", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach Freund, ach schaue mich, ach schaue dich doch an,", "tokens": ["Ach", "Freund", ",", "ach", "schau\u00b7e", "mich", ",", "ach", "schau\u00b7e", "dich", "doch", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "ITJ", "VVFIN", "PPER", "$,", "ITJ", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "was hat das Gl\u00fccke doch uns beiden nicht getan?", "tokens": ["was", "hat", "das", "Gl\u00fc\u00b7cke", "doch", "uns", "bei\u00b7den", "nicht", "ge\u00b7tan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "PPER", "PIS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gott aber habe Dank, der uns l\u00e4\u00dft oben schweben.", "tokens": ["Gott", "a\u00b7ber", "ha\u00b7be", "Dank", ",", "der", "uns", "l\u00e4\u00dft", "o\u00b7ben", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "NN", "$,", "PRELS", "PPER", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Euryalus ist tot, sein Nisus lebt mehr nicht.", "tokens": ["Eu\u00b7ry\u00b7a\u00b7lus", "ist", "tot", ",", "sein", "Ni\u00b7sus", "lebt", "mehr", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "$,", "PPOSAT", "NE", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Orestes h\u00f6rt nicht mehr, was Pylades verspricht.", "tokens": ["O\u00b7res\u00b7tes", "h\u00f6rt", "nicht", "mehr", ",", "was", "Py\u00b7la\u00b7des", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ADV", "$,", "PRELS", "NE", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Uns beide sol die Welt so treu', als jene preisen.", "tokens": ["Uns", "bei\u00b7de", "sol", "die", "Welt", "so", "treu'", ",", "als", "je\u00b7ne", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VMFIN", "ART", "NN", "ADV", "ADJD", "$,", "KOUS", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dein aber, o Hyrkan, und deiner Wellen Macht", "tokens": ["Dein", "a\u00b7ber", ",", "o", "Hyr\u00b7kan", ",", "und", "dei\u00b7ner", "Wel\u00b7len", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADV", "$,", "FM", "NE", "$,", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "sol bei uns allezeit nach W\u00fcrden sein gedacht.", "tokens": ["sol", "bei", "uns", "al\u00b7le\u00b7zeit", "nach", "W\u00fcr\u00b7den", "sein", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "ADV", "APPR", "NN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf, Freund, auf, mehr als Freund! itzt la\u00df uns weiter reisen.", "tokens": ["Auf", ",", "Freund", ",", "auf", ",", "mehr", "als", "Freund", "!", "itzt", "la\u00df", "uns", "wei\u00b7ter", "rei\u00b7sen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "NN", "$,", "PTKVZ", "$,", "ADV", "KOUS", "NN", "$.", "ADV", "VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}