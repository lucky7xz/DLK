{"dta.poem.23541": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Rechts und links.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Norden, S\u00fcden, Wein und Bier,", "tokens": ["Nor\u00b7den", ",", "S\u00fc\u00b7den", ",", "Wein", "und", "Bier", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Plattdeutsch dort und Hochdeutsch hier,", "tokens": ["Platt\u00b7deutsch", "dort", "und", "Hoch\u00b7deutsch", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "KON", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Katholik und Protestant,", "tokens": ["Ka\u00b7tho\u00b7lik", "und", "Pro\u00b7tes\u00b7tant", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mancher F\u00fcrst und manches Land", "tokens": ["Man\u00b7cher", "F\u00fcrst", "und", "man\u00b7ches", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer das nicht vergessen kann,", "tokens": ["Wer", "das", "nicht", "ver\u00b7ges\u00b7sen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist f\u00fcrwahr kein deutscher Mann;", "tokens": ["Ist", "f\u00fcr\u00b7wahr", "kein", "deut\u00b7scher", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn er's gut mit dir auch meint,", "tokens": ["Wenn", "er's", "gut", "mit", "dir", "auch", "meint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vaterland, er ist dein Feind!", "tokens": ["Va\u00b7ter\u00b7land", ",", "er", "ist", "dein", "Feind", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Das bedenket jeder Zeit,", "tokens": ["Das", "be\u00b7den\u00b7ket", "je\u00b7der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ihr strebt nach Einigkeit,", "tokens": ["Wenn", "ihr", "strebt", "nach", "Ei\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deutsche F\u00fcrsten, deutscher Bund,", "tokens": ["Deut\u00b7sche", "F\u00fcrs\u00b7ten", ",", "deut\u00b7scher", "Bund", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutsche Lai'n und Pfaffen, und \u2014", "tokens": ["Deut\u00b7sche", "Lai'n", "und", "Pfaf\u00b7fen", ",", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,", "KON", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}