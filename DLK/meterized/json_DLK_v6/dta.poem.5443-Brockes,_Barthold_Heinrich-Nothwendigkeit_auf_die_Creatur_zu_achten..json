{"dta.poem.5443": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nothwendigkeit auf die Creatur  \n zu achten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie, sonder Licht, ob er gleich noch so sch\u00f6n,", "tokens": ["Wie", ",", "son\u00b7der", "Licht", ",", "ob", "er", "gleich", "noch", "so", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KON", "NN", "$,", "KOUS", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Augen keinen C\u00f6rper sehn;", "tokens": ["Die", "Au\u00b7gen", "kei\u00b7nen", "C\u00f6r\u00b7per", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie auch, sonder Gegenschlag", "tokens": ["Und", "wie", "auch", ",", "son\u00b7der", "Ge\u00b7gen\u00b7schlag"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "ADV", "$,", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der C\u00f6rper, man das Licht selbst nicht zu sehn vermag:", "tokens": ["Der", "C\u00f6r\u00b7per", ",", "man", "das", "Licht", "selbst", "nicht", "zu", "sehn", "ver\u00b7mag", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIS", "ART", "NN", "ADV", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So sieht man, sonder GOtt, auch keine Creatur,", "tokens": ["So", "sieht", "man", ",", "son\u00b7der", "Gott", ",", "auch", "kei\u00b7ne", "Crea\u00b7tur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KON", "NN", "$,", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Und, sonder Creatur, vom Sch\u00f6pfer keine Spur.", "tokens": ["Und", ",", "son\u00b7der", "Crea\u00b7tur", ",", "vom", "Sch\u00f6p\u00b7fer", "kei\u00b7ne", "Spur", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KON", "NN", "$,", "APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}