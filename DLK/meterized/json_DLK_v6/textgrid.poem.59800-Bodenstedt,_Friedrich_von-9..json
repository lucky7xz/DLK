{"textgrid.poem.59800": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kind, was tust du so erschrocken,", "tokens": ["Kind", ",", "was", "tust", "du", "so", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was hebt sch\u00fcchtern sich dein Fu\u00df!", "tokens": ["Was", "hebt", "sch\u00fcch\u00b7tern", "sich", "dein", "Fu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Fass' ich t\u00e4ndelnd deine Locken,", "tokens": ["Fass'", "ich", "t\u00e4n\u00b7delnd", "dei\u00b7ne", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Naht mein Mund sich dir zum Ku\u00df \u2013", "tokens": ["Naht", "mein", "Mund", "sich", "dir", "zum", "Ku\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PRF", "PPER", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was ich biete, was ich suche,", "tokens": ["Was", "ich", "bie\u00b7te", ",", "was", "ich", "su\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df dich's, M\u00e4dchen, nicht betr\u00fcben:", "tokens": ["La\u00df", "dich's", ",", "M\u00e4d\u00b7chen", ",", "nicht", "be\u00b7tr\u00fc\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn so steht's im Schicksalsbuche", "tokens": ["Denn", "so", "steht's", "im", "Schick\u00b7sals\u00b7bu\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir urzeitlich vorgeschrieben!", "tokens": ["Mir", "ur\u00b7zeit\u00b7lich", "vor\u00b7ge\u00b7schrie\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja, voll hohem Glauben bin ich,", "tokens": ["Ja", ",", "voll", "ho\u00b7hem", "Glau\u00b7ben", "bin", "ich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "ADJA", "NN", "VAFIN", "PPER", "$,"], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.2": {"text": "Glaub' an Allah und Koran!", "tokens": ["Glaub'", "an", "Al\u00b7lah", "und", "Ko\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Glaube, da\u00df ich dich herzinnig", "tokens": ["Glau\u00b7be", ",", "da\u00df", "ich", "dich", "her\u00b7zin\u00b7nig"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieben mu\u00df und lieben kann!", "tokens": ["Lie\u00b7ben", "mu\u00df", "und", "lie\u00b7ben", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VMFIN", "KON", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn so steht's im Schicksalsbuche", "tokens": ["Denn", "so", "steht's", "im", "Schick\u00b7sals\u00b7bu\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir urzeitlich vorgeschrieben!", "tokens": ["Mir", "ur\u00b7zeit\u00b7lich", "vor\u00b7ge\u00b7schrie\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Beut die Liebe dir Bedr\u00e4ngnis?", "tokens": ["Beut", "die", "Lie\u00b7be", "dir", "Be\u00b7dr\u00e4ng\u00b7nis", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scheuche l\u00e4chelnd Angst und Pein,", "tokens": ["Scheu\u00b7che", "l\u00e4\u00b7chelnd", "Angst", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn erf\u00fcllt mu\u00df das Verh\u00e4ngnis", "tokens": ["Denn", "er\u00b7f\u00fcllt", "mu\u00df", "das", "Ver\u00b7h\u00e4ng\u00b7nis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VMFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meines stolzen Herzens sein!", "tokens": ["Mei\u00b7nes", "stol\u00b7zen", "Her\u00b7zens", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob ich sinne, ob ich suche,", "tokens": ["Ob", "ich", "sin\u00b7ne", ",", "ob", "ich", "su\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Keine andre kann ich lieben:", "tokens": ["Kei\u00b7ne", "and\u00b7re", "kann", "ich", "lie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn so steht's im Schicksalsbuche", "tokens": ["Denn", "so", "steht's", "im", "Schick\u00b7sals\u00b7bu\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir urzeitlich vorgeschrieben!", "tokens": ["Mir", "ur\u00b7zeit\u00b7lich", "vor\u00b7ge\u00b7schrie\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hoffst du einst dort auf Belohnung", "tokens": ["Hoffst", "du", "einst", "dort", "auf", "Be\u00b7loh\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach vollbrachter Erdenbahn,", "tokens": ["Nach", "voll\u00b7brach\u00b7ter", "Er\u00b7den\u00b7bahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimm dich selbst auch hier voll Schonung", "tokens": ["Nimm", "dich", "selbst", "auch", "hier", "voll", "Scho\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ADV", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meines armen Herzens an!", "tokens": ["Mei\u00b7nes", "ar\u00b7men", "Her\u00b7zens", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Keines andern Minne suche,", "tokens": ["Kei\u00b7nes", "an\u00b7dern", "Min\u00b7ne", "su\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcge, zwing dich, ", "tokens": ["F\u00fc\u00b7ge", ",", "zwing", "dich", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Denn so steht's im Schicksalsbuche", "tokens": ["Denn", "so", "steht's", "im", "Schick\u00b7sals\u00b7bu\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir urzeitlich vorgeschrieben!", "tokens": ["Mir", "ur\u00b7zeit\u00b7lich", "vor\u00b7ge\u00b7schrie\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nimm dies duft'ge Lied und lies es,", "tokens": ["Nimm", "dies", "duft'\u00b7ge", "Lied", "und", "lies", "es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "ADJA", "NN", "KON", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lausche seinem Zauberton \u2013", "tokens": ["Lau\u00b7sche", "sei\u00b7nem", "Zau\u00b7ber\u00b7ton", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es verhei\u00dft des Paradieses", "tokens": ["Es", "ver\u00b7hei\u00dft", "des", "Pa\u00b7ra\u00b7die\u00b7ses"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seligkeit auf Erden schon!", "tokens": ["Se\u00b7lig\u00b7keit", "auf", "Er\u00b7den", "schon", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Andres Gl\u00fcck dort oben suche,", "tokens": ["And\u00b7res", "Gl\u00fcck", "dort", "o\u00b7ben", "su\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch hienieden la\u00df uns lieben:", "tokens": ["Doch", "hien\u00b7ie\u00b7den", "la\u00df", "uns", "lie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn so steht's im Schicksalsbuche", "tokens": ["Denn", "so", "steht's", "im", "Schick\u00b7sals\u00b7bu\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Uns urzeitlich vorgeschrieben!", "tokens": ["Uns", "ur\u00b7zeit\u00b7lich", "vor\u00b7ge\u00b7schrie\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Wie vom Hauch des Morgenwindes", "tokens": ["Wie", "vom", "Hauch", "des", "Mor\u00b7gen\u00b7win\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich der Kelch der Rose regt,", "tokens": ["Sich", "der", "Kelch", "der", "Ro\u00b7se", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei das Herz des lieben Kindes", "tokens": ["Sei", "das", "Herz", "des", "lie\u00b7ben", "Kin\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von des Liedes Hauch bewegt!", "tokens": ["Von", "des", "Lie\u00b7des", "Hauch", "be\u00b7wegt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie gew\u00e4hre, was ich suche,", "tokens": ["Sie", "ge\u00b7w\u00e4h\u00b7re", ",", "was", "ich", "su\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was mich toll zu ihr getrieben:", "tokens": ["Was", "mich", "toll", "zu", "ihr", "ge\u00b7trie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "APPR", "PPOSAT", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn so steht's im Schicksalsbuche", "tokens": ["Denn", "so", "steht's", "im", "Schick\u00b7sals\u00b7bu\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihr urzeitlich vorgeschrieben!", "tokens": ["Ihr", "ur\u00b7zeit\u00b7lich", "vor\u00b7ge\u00b7schrie\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}