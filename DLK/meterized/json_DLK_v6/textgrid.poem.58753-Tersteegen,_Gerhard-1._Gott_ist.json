{"textgrid.poem.58753": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1. Gott ist", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott ist, Gott ist, Halleluja!", "tokens": ["Gott", "ist", ",", "Gott", "ist", ",", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "NN", "VAFIN", "$,", "ITJ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Des bin ich froh, du Gott, bist da,", "tokens": ["Des", "bin", "ich", "froh", ",", "du", "Gott", ",", "bist", "da", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "VAFIN", "PPER", "ADJD", "$,", "PPER", "NN", "$,", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Notwendig, wahr und lauter Wesen.", "tokens": ["Not\u00b7wen\u00b7dig", ",", "wahr", "und", "lau\u00b7ter", "We\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbich bin\u00ab, sprichst du; o ja, du bist!", "tokens": ["\u00bb", "ich", "bin", "\u00ab", ",", "sprichst", "du", ";", "o", "ja", ",", "du", "bist", "!"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "$(", "$,", "VVFIN", "PPER", "$.", "FM", "ADV", "$,", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "H\u00f6rt's, Engel, Mensch, und was da ist:", "tokens": ["H\u00f6rt's", ",", "En\u00b7gel", ",", "Mensch", ",", "und", "was", "da", "ist", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NN", "$,", "KON", "PWS", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "W\u00e4r' Gott nicht, ihr w\u00e4rt nie gewesen.", "tokens": ["W\u00e4r'", "Gott", "nicht", ",", "ihr", "w\u00e4rt", "nie", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKNEG", "$,", "PPER", "VAFIN", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wohl mir, mein Gott, da\u00df ich dich wei\u00df,", "tokens": ["Wohl", "mir", ",", "mein", "Gott", ",", "da\u00df", "ich", "dich", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Nichts gesteh, dein Dasein preis',", "tokens": ["Mein", "Nichts", "ge\u00b7steh", ",", "dein", "Da\u00b7sein", "preis'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Halleluja! Halleluja!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ITJ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}