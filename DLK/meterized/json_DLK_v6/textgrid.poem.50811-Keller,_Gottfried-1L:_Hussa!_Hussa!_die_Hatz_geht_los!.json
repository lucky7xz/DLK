{"textgrid.poem.50811": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hussa! Hussa! die Hatz geht los!", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hussa! Hussa! die Hatz geht los!", "tokens": ["Hus\u00b7sa", "!", "Hus\u00b7sa", "!", "die", "Hatz", "geht", "los", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Es kommt geritten klein und gro\u00df;", "tokens": ["Es", "kommt", "ge\u00b7rit\u00b7ten", "klein", "und", "gro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das springt und purzelt gar behend,", "tokens": ["Das", "springt", "und", "pur\u00b7zelt", "gar", "be\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Das kreischt und zetert ohne End \u2013", "tokens": ["Das", "kreischt", "und", "ze\u00b7tert", "oh\u00b7ne", "End", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie kommen, die Jesuiten!", "tokens": ["Sie", "kom\u00b7men", ",", "die", "Je\u00b7su\u00b7i\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Da reiten sie auf Schl\u00e4ngelein", "tokens": ["Da", "rei\u00b7ten", "sie", "auf", "Schl\u00e4n\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hintennach auf Drach und Schwein;", "tokens": ["Und", "hin\u00b7ten\u00b7nach", "auf", "Drach", "und", "Schwein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was das f\u00fcr muntre Bursche sind!", "tokens": ["Was", "das", "f\u00fcr", "mun\u00b7tre", "Bur\u00b7sche", "sind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl graut im Mutterleib dem Kind:", "tokens": ["Wohl", "graut", "im", "Mut\u00b7ter\u00b7leib", "dem", "Kind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie kommen, die Jesuiten!", "tokens": ["Sie", "kom\u00b7men", ",", "die", "Je\u00b7su\u00b7i\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Huh! wie das krabbelt, kneipt und kriecht!", "tokens": ["Huh", "!", "wie", "das", "krab\u00b7belt", ",", "kneipt", "und", "kriecht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "PDS", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie's so infernalisch riecht!", "tokens": ["Und", "wie's", "so", "in\u00b7fer\u00b7na\u00b7lisch", "riecht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt fahre hin, du gute Ruh!", "tokens": ["Jetzt", "fah\u00b7re", "hin", ",", "du", "gu\u00b7te", "Ruh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geh, Grete, mach das Fenster zu!", "tokens": ["Geh", ",", "Gre\u00b7te", ",", "mach", "das", "Fens\u00b7ter", "zu", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie kommen, die Jesuiten!", "tokens": ["Sie", "kom\u00b7men", ",", "die", "Je\u00b7su\u00b7i\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Von Kreuz und Fahne angef\u00fchrt,", "tokens": ["Von", "Kreuz", "und", "Fah\u00b7ne", "an\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Giftsack hinten aufgeschn\u00fcrt,", "tokens": ["Den", "Gift\u00b7sack", "hin\u00b7ten", "auf\u00b7ge\u00b7schn\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Fanatismus als Profo\u00df,", "tokens": ["Der", "Fa\u00b7na\u00b7tis\u00b7mus", "als", "Pro\u00b7fo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Dummheit folgt als Betteltro\u00df:", "tokens": ["Die", "Dumm\u00b7heit", "folgt", "als", "Bet\u00b7tel\u00b7tro\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So kommen die Jesuiten!", "tokens": ["So", "kom\u00b7men", "die", "Je\u00b7su\u00b7i\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "O Schweizerland, du sch\u00f6ne Braut,", "tokens": ["O", "Schwei\u00b7zer\u00b7land", ",", "du", "sch\u00f6\u00b7ne", "Braut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist dem Teufel angetraut!", "tokens": ["Du", "bist", "dem", "Teu\u00b7fel", "an\u00b7ge\u00b7traut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja, weine nur, du armes Kind!", "tokens": ["Ja", ",", "wei\u00b7ne", "nur", ",", "du", "ar\u00b7mes", "Kind", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Gotthard weht ein schlimmer Wind \u2013", "tokens": ["Vom", "Got\u00b7thard", "weht", "ein", "schlim\u00b7mer", "Wind", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie kommen, die Jesuiten!", "tokens": ["Sie", "kom\u00b7men", ",", "die", "Je\u00b7su\u00b7i\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}