{"dta.poem.16335": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Oft fa\u00dft mich, wenn ich seh ein zartes Kinderleben,", "tokens": ["Oft", "fa\u00dft", "mich", ",", "wenn", "ich", "seh", "ein", "zar\u00b7tes", "Kin\u00b7der\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wehmuth, wie ihm die Zeit wird M\u00fch und Dornen weben.", "tokens": ["Weh\u00b7muth", ",", "wie", "ihm", "die", "Zeit", "wird", "M\u00fch", "und", "Dor\u00b7nen", "we\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "ART", "NN", "VAFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Viel seltner f\u00fcllet mich sein Anblick, mit Behagen", "tokens": ["Viel", "selt\u00b7ner", "f\u00fcl\u00b7let", "mich", "sein", "An\u00b7blick", ",", "mit", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Fr\u00fccht' und Bl\u00fcten, die ihm noch die Welt wird tragen.", "tokens": ["Der", "Fr\u00fccht'", "und", "Bl\u00fc\u00b7ten", ",", "die", "ihm", "noch", "die", "Welt", "wird", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "PRELS", "PPER", "ADV", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich schlie\u00dfe nicht daraus, da\u00df eitel sei die Welt,", "tokens": ["Ich", "schlie\u00b7\u00dfe", "nicht", "da\u00b7raus", ",", "da\u00df", "ei\u00b7tel", "sei", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PAV", "$,", "KOUS", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch da\u00df sie mir nunmehr als eitel dar sich stellt.", "tokens": ["Doch", "da\u00df", "sie", "mir", "nun\u00b7mehr", "als", "ei\u00b7tel", "dar", "sich", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "KOUS", "ADJD", "PTKVZ", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}