{"dta.poem.2295": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An den Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Tod, kanst du dich auch verlieben?", "tokens": ["Tod", ",", "kanst", "du", "dich", "auch", "ver\u00b7lie\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Warum holst du denn mein M\u00e4dchen?", "tokens": ["Wa\u00b7rum", "holst", "du", "denn", "mein", "M\u00e4d\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kanst du nicht die Mutter holen?", "tokens": ["Kanst", "du", "nicht", "die", "Mut\u00b7ter", "ho\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn die sieht dir doch noch \u00e4hnlich.", "tokens": ["Denn", "die", "sieht", "dir", "doch", "noch", "\u00e4hn\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Frische rosenrote Wangen,", "tokens": ["Fri\u00b7sche", "ro\u00b7sen\u00b7ro\u00b7te", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die mein Wunsch so sch\u00f6n gef\u00e4rbet,", "tokens": ["Die", "mein", "Wunsch", "so", "sch\u00f6n", "ge\u00b7f\u00e4r\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bl\u00fchen nicht f\u00fcr blasse Knochen,", "tokens": ["Bl\u00fc\u00b7hen", "nicht", "f\u00fcr", "blas\u00b7se", "Kno\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bl\u00fchen nicht f\u00fcr deine Lippen.", "tokens": ["Bl\u00fc\u00b7hen", "nicht", "f\u00fcr", "dei\u00b7ne", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Tod! was wilst du mit dem M\u00e4dchen?", "tokens": ["Tod", "!", "was", "wilst", "du", "mit", "dem", "M\u00e4d\u00b7chen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mit den Z\u00e4hnen ohne Lippen", "tokens": ["Mit", "den", "Z\u00e4h\u00b7nen", "oh\u00b7ne", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Kanst du es ia doch nicht k\u00fcssen.", "tokens": ["Kanst", "du", "es", "i\u00b7a", "doch", "nicht", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}