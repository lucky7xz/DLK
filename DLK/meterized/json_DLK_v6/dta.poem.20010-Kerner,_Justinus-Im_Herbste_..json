{"dta.poem.20010": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Im Herbste .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hoch von Bergen t\u00f6nt zu Thal", "tokens": ["Hoch", "von", "Ber\u00b7gen", "t\u00f6nt", "zu", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Freudenruf und Jubellied:", "tokens": ["Freu\u00b7den\u00b7ruf", "und", "Ju\u00b7bel\u00b7lied", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sey gegr\u00fc\u00dft du heil'ger Strahl", "tokens": ["Sey", "ge\u00b7gr\u00fc\u00dft", "du", "heil'\u00b7ger", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der auch unsern Berg durchgl\u00fcht.", "tokens": ["Der", "auch", "un\u00b7sern", "Berg", "durch\u00b7gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "L\u00e4ngs des Neckars, l\u00e4ngs des Rheins", "tokens": ["L\u00e4ngs", "des", "Ne\u00b7ckars", ",", "l\u00e4ngs", "des", "Rheins"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "ART", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00f6net solcher Freude Schall,", "tokens": ["T\u00f6\u00b7net", "sol\u00b7cher", "Freu\u00b7de", "Schall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Prei\u00dft den m\u00e4cht'gen Gott des Weins,", "tokens": ["Prei\u00dft", "den", "m\u00e4cht'\u00b7gen", "Gott", "des", "Weins", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der gekr\u00f6nt die H\u00fcgel all'.", "tokens": ["Der", "ge\u00b7kr\u00f6nt", "die", "H\u00fc\u00b7gel", "all'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "PIAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Evoe! ", "tokens": ["E\u00b7voe", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.2": {"text": "Ich auch dieses Glas mit Wein!", "tokens": ["Ich", "auch", "die\u00b7ses", "Glas", "mit", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PDAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gold des Neckars! \u2014 Doch woher", "tokens": ["Gold", "des", "Ne\u00b7ckars", "!", "Doch", "wo\u00b7her"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "$(", "KON", "PWAV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "F\u00e4llt ein Tropfen Blut hinein?", "tokens": ["F\u00e4llt", "ein", "Trop\u00b7fen", "Blut", "hin\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Freunde! Das ist Griechenblut!", "tokens": ["Freun\u00b7de", "!", "Das", "ist", "Grie\u00b7chen\u00b7blut", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PDS", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stellt Gesang und Jubel ein!", "tokens": ["Stellt", "Ge\u00b7sang", "und", "Ju\u00b7bel", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blickt zu Thal, mit tr\u00fcbem Muth", "tokens": ["Blickt", "zu", "Thal", ",", "mit", "tr\u00fc\u00b7bem", "Muth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf die Erde, kalt wie Stein.", "tokens": ["Auf", "die", "Er\u00b7de", ",", "kalt", "wie", "Stein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Evoe, Ruf, der einmal", "tokens": ["E\u00b7voe", ",", "Ruf", ",", "der", "ein\u00b7mal"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "PRELS", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Froh get\u00f6nt durch Hellas Land,", "tokens": ["Froh", "ge\u00b7t\u00f6nt", "durch", "Hel\u00b7las", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00f6ntest mir jezt Hellas Qual \u2014", "tokens": ["T\u00f6n\u00b7test", "mir", "jezt", "Hel\u00b7las", "Qual"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NE", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Glas entf\u00e4llt der Hand.", "tokens": ["Und", "das", "Glas", "ent\u00b7f\u00e4llt", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}