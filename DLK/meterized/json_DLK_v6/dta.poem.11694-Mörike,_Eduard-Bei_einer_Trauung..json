{"dta.poem.11694": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Bei einer Trauung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Copulirt man ihrer Zwei;", "tokens": ["Co\u00b7pu\u00b7lirt", "man", "ih\u00b7rer", "Zwei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "PPOSAT", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Orgel h\u00e4ngt voll Geigen,", "tokens": ["Die", "Or\u00b7gel", "h\u00e4ngt", "voll", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Himmel nicht, mein' Treu!", "tokens": ["Der", "Him\u00b7mel", "nicht", ",", "mein'", "Treu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "O weh', ", "tokens": ["O", "weh'", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Denn leider, freilich, freilich", "tokens": ["Denn", "lei\u00b7der", ",", "frei\u00b7lich", ",", "frei\u00b7lich"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ADV", "$,", "ADV", "$,", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Keine Lieb' ist nicht dabei.", "tokens": ["Kei\u00b7ne", "Lieb'", "ist", "nicht", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKNEG", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}