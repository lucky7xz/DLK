{"dta.poem.23346": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Porc \u00e0 la mode.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr geht nach neuestem Geschmacke,", "tokens": ["Ihr", "geht", "nach", "neu\u00b7es\u00b7tem", "Ge\u00b7schma\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcrwahr, es steht euch alles gut:", "tokens": ["F\u00fcr\u00b7wahr", ",", "es", "steht", "euch", "al\u00b7les", "gut", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie prunkt der Leib im engen Fracke,", "tokens": ["Wie", "prunkt", "der", "Leib", "im", "en\u00b7gen", "Fra\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie zierlich sitzt der runde Hut!", "tokens": ["Wie", "zier\u00b7lich", "sitzt", "der", "run\u00b7de", "Hut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Weste schillert, die moderne,", "tokens": ["Die", "Wes\u00b7te", "schil\u00b7lert", ",", "die", "mo\u00b7der\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Vorhemd ist so hell und klar,", "tokens": ["Das", "Vor\u00b7hemd", "ist", "so", "hell", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Kn\u00f6pfe blitzen wie die Sterne,", "tokens": ["Die", "Kn\u00f6p\u00b7fe", "blit\u00b7zen", "wie", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und glatt gestr\u00e4hlt ist Bart und Haar.", "tokens": ["Und", "glatt", "ge\u00b7str\u00e4hlt", "ist", "Bart", "und", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wohl sauber sind die lieben Bl\u00fcmchen", "tokens": ["Wohl", "sau\u00b7ber", "sind", "die", "lie\u00b7ben", "Bl\u00fcm\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die dr\u00fcben auf der Heide stehn,", "tokens": ["Die", "dr\u00fc\u00b7ben", "auf", "der", "Hei\u00b7de", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ist an euch kein Fleck, kein Kr\u00fcmchen,", "tokens": ["Doch", "ist", "an", "euch", "kein", "Fleck", ",", "kein", "Kr\u00fcm\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Staub, kein Federchen zu sehn.", "tokens": ["Kein", "Staub", ",", "kein", "Fe\u00b7der\u00b7chen", "zu", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Man kann euch in den Glasschrank stellen,", "tokens": ["Man", "kann", "euch", "in", "den", "Glas\u00b7schrank", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So h\u00fcbsch seid ihr, so nett und rein!", "tokens": ["So", "h\u00fcbsch", "seid", "ihr", ",", "so", "nett", "und", "rein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ach! ihr sauberen Gesellen,", "tokens": ["Und", "ach", "!", "ihr", "sau\u00b7be\u00b7ren", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr sprecht das Deutsche wie ein Schwein.", "tokens": ["Ihr", "sprecht", "das", "Deut\u00b7sche", "wie", "ein", "Schwein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}