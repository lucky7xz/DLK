{"textgrid.poem.26663": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An einen Moralisten", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Betagter Renegat der l\u00e4chelnden Dione!", "tokens": ["Be\u00b7tag\u00b7ter", "Re\u00b7ne\u00b7gat", "der", "l\u00e4\u00b7cheln\u00b7den", "Dio\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du lehrst, da\u00df ", "tokens": ["Du", "lehrst", ",", "da\u00df"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Blickst von des Alters Winterwolkenthrone", "tokens": ["Blickst", "von", "des", "Al\u00b7ters", "Win\u00b7ter\u00b7wol\u00b7ken\u00b7thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schm\u00e4lest auf den goldnen Mai.", "tokens": ["Und", "schm\u00e4\u00b7lest", "auf", "den", "gold\u00b7nen", "Mai", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Erkennt Natur auch Schreibepultgesetze?", "tokens": ["Er\u00b7kennt", "Na\u00b7tur", "auch", "Schrei\u00b7be\u00b7pult\u00b7ge\u00b7set\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr eine warme Welt \u2013 taugt ein erfrorner Sinn?", "tokens": ["F\u00fcr", "ei\u00b7ne", "war\u00b7me", "Welt", "\u2013", "taugt", "ein", "er\u00b7fror\u00b7ner", "Sinn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Armut ist, nach dem Aesop, der Sch\u00e4tze", "tokens": ["Die", "Ar\u00b7mut", "ist", ",", "nach", "dem", "A\u00b7e\u00b7sop", ",", "der", "Sch\u00e4t\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "APPR", "ART", "NN", "$,", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Verd\u00e4chtige Ver\u00e4chterin.", "tokens": ["Ver\u00b7d\u00e4ch\u00b7ti\u00b7ge", "Ver\u00b7\u00e4ch\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Einst, als du noch das Nymphenvolk bekriegtest,", "tokens": ["Einst", ",", "als", "du", "noch", "das", "Nym\u00b7phen\u00b7volk", "be\u00b7krieg\u00b7test", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein F\u00fcrst des Karnevals den teutschen Wirbel flogst,", "tokens": ["Ein", "F\u00fcrst", "des", "Kar\u00b7ne\u00b7vals", "den", "teut\u00b7schen", "Wir\u00b7bel", "flogst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Himmelreich in beiden Armen wiegtest,", "tokens": ["Ein", "Him\u00b7mel\u00b7reich", "in", "bei\u00b7den", "Ar\u00b7men", "wieg\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Nektarduft von M\u00e4dchenlippen zogst?", "tokens": ["Und", "Nek\u00b7tar\u00b7duft", "von", "M\u00e4d\u00b7chen\u00b7lip\u00b7pen", "zogst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ha Seladon! wenn damals aus den Achsen", "tokens": ["Ha", "Se\u00b7la\u00b7don", "!", "wenn", "da\u00b7mals", "aus", "den", "Ach\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$.", "KOUS", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gewichen w\u00e4r so Erd- als Sonnenball,", "tokens": ["Ge\u00b7wi\u00b7chen", "w\u00e4r", "so", "Erd", "als", "Son\u00b7nen\u00b7ball", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Wirbelschwung mit Julien verwachsen,", "tokens": ["In", "Wir\u00b7bel\u00b7schwung", "mit", "Ju\u00b7li\u00b7en", "ver\u00b7wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du h\u00e4ttest \u00fcberh\u00f6rt den Fall,", "tokens": ["Du", "h\u00e4t\u00b7test", "\u00fc\u00b7ber\u00b7h\u00f6rt", "den", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wenn nach manchen fehlgesprengten Minen", "tokens": ["Und", "wenn", "nach", "man\u00b7chen", "fehl\u00b7ge\u00b7spreng\u00b7ten", "Mi\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr eignes Blut, von wilder Lust gegl\u00fcht,", "tokens": ["Ihr", "eig\u00b7nes", "Blut", ",", "von", "wil\u00b7der", "Lust", "ge\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die stolze Tugend deiner Sch\u00f6nen", "tokens": ["Die", "stol\u00b7ze", "Tu\u00b7gend", "dei\u00b7ner", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zuletzt an deine Brust verriet?", "tokens": ["Zu\u00b7letzt", "an", "dei\u00b7ne", "Brust", "ver\u00b7riet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie? oder wenn romantisch im Geh\u00f6lze", "tokens": ["Wie", "?", "o\u00b7der", "wenn", "ro\u00b7man\u00b7tisch", "im", "Ge\u00b7h\u00f6l\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "KON", "KOUS", "ADJD", "APPRART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein leiser Laut zu deinen Ohren drang,", "tokens": ["Ein", "lei\u00b7ser", "Laut", "zu", "dei\u00b7nen", "Oh\u00b7ren", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und in der Wellen silbernem Gew\u00e4lze", "tokens": ["Und", "in", "der", "Wel\u00b7len", "sil\u00b7ber\u00b7nem", "Ge\u00b7w\u00e4l\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein M\u00e4dchen Sammetglieder schwang?", "tokens": ["Ein", "M\u00e4d\u00b7chen", "Sam\u00b7met\u00b7glie\u00b7der", "schwang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie schlug dein Herz! wie st\u00fcrmete! wie kochte", "tokens": ["Wie", "schlug", "dein", "Herz", "!", "wie", "st\u00fcr\u00b7me\u00b7te", "!", "wie", "koch\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$.", "PWAV", "VVFIN", "$.", "PWAV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aufr\u00fchrerisch das scharfgejagte Blut!", "tokens": ["Auf\u00b7r\u00fch\u00b7re\u00b7risch", "das", "scharf\u00b7ge\u00b7jag\u00b7te", "Blut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zuckt jede Senn \u2013 und jeder Muskel pochte", "tokens": ["Zuckt", "je\u00b7de", "Senn", "\u2013", "und", "je\u00b7der", "Mus\u00b7kel", "poch\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "$(", "KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Woll\u00fcstig in die Flut!", "tokens": ["Wol\u00b7l\u00fcs\u00b7tig", "in", "die", "Flut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Wenn dann gewahr des Diebs, der sie belauschte,", "tokens": ["Wenn", "dann", "ge\u00b7wahr", "des", "Diebs", ",", "der", "sie", "be\u00b7lauschte", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Purpurisch angehaucht von j\u00fcngferlicher Scham,", "tokens": ["Pur\u00b7pu\u00b7risch", "an\u00b7ge\u00b7haucht", "von", "j\u00fcng\u00b7fer\u00b7li\u00b7cher", "Scham", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ins blaue Bett die Sch\u00f6ne niederrauschte,", "tokens": ["Ins", "blau\u00b7e", "Bett", "die", "Sch\u00f6\u00b7ne", "nie\u00b7der\u00b7rauschte", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und hintennach mein strenger Zeno \u2013 schwamm,", "tokens": ["Und", "hin\u00b7ten\u00b7nach", "mein", "stren\u00b7ger", "Ze\u00b7no", "\u2013", "schwamm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NE", "$(", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ja hintennach \u2013 und seis auch nur zu baden!", "tokens": ["Ja", "hin\u00b7ten\u00b7nach", "\u2013", "und", "seis", "auch", "nur", "zu", "ba\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$(", "KON", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Rock und Kamisol und Strumpf \u2013", "tokens": ["Mit", "Rock", "und", "Ka\u00b7mi\u00b7sol", "und", "Strumpf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}}, "stanza.10": {"line.1": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.2": {"text": "Leis fl\u00f6teten die l\u00fcsternen Najaden", "tokens": ["Leis", "fl\u00f6\u00b7te\u00b7ten", "die", "l\u00fcs\u00b7ter\u00b7nen", "Na\u00b7ja\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Grazien Triumph!", "tokens": ["Der", "Gra\u00b7zi\u00b7en", "Tri\u00b7umph", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "O denk zur\u00fcck nach ", "tokens": ["O", "denk", "zu\u00b7r\u00fcck", "nach"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKVZ", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und lerne, die Philosophie", "tokens": ["Und", "ler\u00b7ne", ",", "die", "Phi\u00b7lo\u00b7so\u00b7phie"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schl\u00e4gt um, wie unsre Pulse anders schlagen,", "tokens": ["Schl\u00e4gt", "um", ",", "wie", "uns\u00b7re", "Pul\u00b7se", "an\u00b7ders", "schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PWAV", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu G\u00f6ttern schaffst du Menschen nie.", "tokens": ["Zu", "G\u00f6t\u00b7tern", "schaffst", "du", "Men\u00b7schen", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Wohl! wenn ins Eis des kl\u00fcgelnden Verstandes", "tokens": ["Wohl", "!", "wenn", "ins", "Eis", "des", "kl\u00fc\u00b7geln\u00b7den", "Ver\u00b7stan\u00b7des"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "KOUS", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das warme Blut ein bi\u00dfchen muntrer springt!", "tokens": ["Das", "war\u00b7me", "Blut", "ein", "bi\u00df\u00b7chen", "mun\u00b7trer", "springt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df den Bewohnern eines ", "tokens": ["La\u00df", "den", "Be\u00b7woh\u00b7nern", "ei\u00b7nes"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "ART"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Was ewig nie dem ", "tokens": ["Was", "e\u00b7wig", "nie", "dem"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADJD", "ADV", "ART"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Zwingt doch der tierische Gef\u00e4hrte", "tokens": ["Zwingt", "doch", "der", "tie\u00b7ri\u00b7sche", "Ge\u00b7f\u00e4hr\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den gottgebornen Geist in Sklavenmauren ein \u2013", "tokens": ["Den", "gott\u00b7ge\u00b7bor\u00b7nen", "Geist", "in", "Skla\u00b7ven\u00b7mau\u00b7ren", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er wehrt mir, da\u00df ich ", "tokens": ["Er", "wehrt", "mir", ",", "da\u00df", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ich will ihm folgen, ", "tokens": ["Ich", "will", "ihm", "fol\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}}}}