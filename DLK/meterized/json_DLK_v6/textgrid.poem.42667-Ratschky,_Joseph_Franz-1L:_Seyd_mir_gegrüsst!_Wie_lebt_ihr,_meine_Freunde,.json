{"textgrid.poem.42667": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seyd mir gegr\u00fcsst! Wie lebt ihr, meine Freunde,", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seyd mir gegr\u00fcsst! Wie lebt ihr, meine Freunde,", "tokens": ["Seyd", "mir", "ge\u00b7gr\u00fcs\u00b7st", "!", "Wie", "lebt", "ihr", ",", "mei\u00b7ne", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "VVPP", "$.", "PWAV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Seit ich im Land der wilden Lechen bin?", "tokens": ["Seit", "ich", "im", "Land", "der", "wil\u00b7den", "Le\u00b7chen", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bringt ihr, vereint in friedlicher Gemeinde,", "tokens": ["Bringt", "ihr", ",", "ver\u00b7eint", "in", "fried\u00b7li\u00b7cher", "Ge\u00b7mein\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Abend noch mit Vater Bachus hin?", "tokens": ["Den", "A\u00b7bend", "noch", "mit", "Va\u00b7ter", "Ba\u00b7chus", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ist euer Kreis noch st\u00e4ts an s\u00fcssen Schw\u00e4nken,", "tokens": ["Ist", "eu\u00b7er", "Kreis", "noch", "st\u00e4ts", "an", "s\u00fcs\u00b7sen", "Schw\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "An Liederchen und Epigrammen reich?", "tokens": ["An", "Lie\u00b7der\u00b7chen", "und", "E\u00b7pig\u00b7ram\u00b7men", "reich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.7": {"text": "Liebt ihr mich noch, und ist mein Angedenken", "tokens": ["Liebt", "ihr", "mich", "noch", ",", "und", "ist", "mein", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$,", "KON", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Noch unentweiht und heilig unter euch?", "tokens": ["Noch", "un\u00b7ent\u00b7weiht", "und", "hei\u00b7lig", "un\u00b7ter", "euch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun denn, ihr Herrn! hier send' ich euch zum Pfande,", "tokens": ["Nun", "denn", ",", "ihr", "Herrn", "!", "hier", "send'", "ich", "euch", "zum", "Pfan\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dass euer Freund sein Handwerk nicht vergisst,", "tokens": ["Dass", "eu\u00b7er", "Freund", "sein", "Hand\u00b7werk", "nicht", "ver\u00b7gisst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Diess Pr\u00f6bchen zu, gereimt in einem Lande,", "tokens": ["Diess", "Pr\u00f6b\u00b7chen", "zu", ",", "ge\u00b7reimt", "in", "ei\u00b7nem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKVZ", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das wahrlich nicht der Musen Heimath ist.", "tokens": ["Das", "wahr\u00b7lich", "nicht", "der", "Mu\u00b7sen", "Hei\u00b7math", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PTKNEG", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Tr\u00e4gt hie und da ein Verschen einen Flecken,", "tokens": ["Tr\u00e4gt", "hie", "und", "da", "ein", "Ver\u00b7schen", "ei\u00b7nen", "Fle\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So fegt ihn aus, wenn's eurer Feile gl\u00fcckt:", "tokens": ["So", "fegt", "ihn", "aus", ",", "wenn's", "eu\u00b7rer", "Fei\u00b7le", "gl\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch f\u00fcgt es sich, dass ihr an allen Ecken", "tokens": ["Doch", "f\u00fcgt", "es", "sich", ",", "dass", "ihr", "an", "al\u00b7len", "E\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "$,", "KOUS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Geistlosigkeit und lahme Vers' erblickt,", "tokens": ["Geist\u00b7lo\u00b7sig\u00b7keit", "und", "lah\u00b7me", "Ver\u00b7s'", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "So z\u00fccket k\u00fchn den Recensentens\u00e4bel;", "tokens": ["So", "z\u00fc\u00b7cket", "k\u00fchn", "den", "Re\u00b7cen\u00b7sen\u00b7ten\u00b7s\u00e4\u00b7bel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ein schlecht Gedicht verdienet keine Huld.", "tokens": ["Ein", "schlecht", "Ge\u00b7dicht", "ver\u00b7die\u00b7net", "kei\u00b7ne", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Nur meiner schont! an meines Geistes Nebel", "tokens": ["Nur", "mei\u00b7ner", "schont", "!", "an", "mei\u00b7nes", "Geis\u00b7tes", "Ne\u00b7bel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "VVFIN", "$.", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ist bloss der Dunst des dicken Klima Schuld.", "tokens": ["Ist", "bloss", "der", "Dunst", "des", "di\u00b7cken", "Kli\u00b7ma", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Drum m\u00e4ssigt euch! des Dichters Flamme lodert", "tokens": ["Drum", "m\u00e4s\u00b7sigt", "euch", "!", "des", "Dich\u00b7ters", "Flam\u00b7me", "lo\u00b7dert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "$.", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Am Ister selbst in eurem attischen", "tokens": ["Am", "Is\u00b7ter", "selbst", "in", "eu\u00b7rem", "at\u00b7ti\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Revier noch schwach: zum Geyer! und ihr fodert", "tokens": ["Re\u00b7vier", "noch", "schwach", ":", "zum", "Ge\u00b7yer", "!", "und", "ihr", "fo\u00b7dert"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "ADJD", "$.", "APPRART", "NN", "$.", "KON", "PPER", "VVFIN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.16": {"text": "Horazens Geist hier in B\u00f6ozien?", "tokens": ["Ho\u00b7ra\u00b7zens", "Geist", "hier", "in", "B\u00f6o\u00b7zi\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "APPR", "NE", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Doch diess beyseit'! Erz\u00e4hlt mir, was hanthieren", "tokens": ["Doch", "diess", "bey\u00b7seit'", "!", "Er\u00b7z\u00e4hlt", "mir", ",", "was", "han\u00b7thie\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDS", "PTKVZ", "$.", "VVFIN", "PPER", "$,", "PWS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Skribler Wiens? was macht die Bonzenzunft?", "tokens": ["Die", "Skrib\u00b7ler", "Wiens", "?", "was", "macht", "die", "Bon\u00b7zen\u00b7zunft", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$.", "PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verheert die Flut tollsinniger Brosch\u00fcren", "tokens": ["Ver\u00b7heert", "die", "Flut", "toll\u00b7sin\u00b7ni\u00b7ger", "Bro\u00b7sch\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch immerhin die Fr\u00fcchte der Vernunft?", "tokens": ["Noch", "im\u00b7mer\u00b7hin", "die", "Fr\u00fcch\u00b7te", "der", "Ver\u00b7nunft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wird viel vom Gr\u00e4ul des j\u00fcngsten Tags gepredigt,", "tokens": ["Wird", "viel", "vom", "Gr\u00e4ul", "des", "j\u00fcng\u00b7sten", "Tags", "ge\u00b7pre\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und Witzlingen die H\u00f6lle heiss gemacht?", "tokens": ["Und", "Witz\u00b7lin\u00b7gen", "die", "H\u00f6l\u00b7le", "heiss", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Hat Pochlin sich die Lunge schon besch\u00e4digt,", "tokens": ["Hat", "Poch\u00b7lin", "sich", "die", "Lun\u00b7ge", "schon", "be\u00b7sch\u00e4\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PRF", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und ist zeither kein neuer Fast erwacht?", "tokens": ["Und", "ist", "zeit\u00b7her", "kein", "neu\u00b7er", "Fast", "er\u00b7wacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "O ganz gewiss! denn eure Kanzelhelden", "tokens": ["O", "ganz", "ge\u00b7wiss", "!", "denn", "eu\u00b7re", "Kan\u00b7zel\u00b7hel\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "ADJD", "$.", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Sind st\u00e4ts bereit zum geistlichen Turnier.", "tokens": ["Sind", "st\u00e4ts", "be\u00b7reit", "zum", "geist\u00b7li\u00b7chen", "Tur\u00b7nier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Da lob' ich mir, nicht ohne Ruhm zu melden,", "tokens": ["Da", "lob'", "ich", "mir", ",", "nicht", "oh\u00b7ne", "Ruhm", "zu", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "PTKNEG", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die Priesterschaar der Russniaken hier.", "tokens": ["Die", "Pries\u00b7ter\u00b7schaar", "der", "Russ\u00b7ni\u00b7a\u00b7ken", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "All das Gez\u00e4nk um l\u00e4cherliche Grillen,", "tokens": ["All", "das", "Ge\u00b7z\u00e4nk", "um", "l\u00e4\u00b7cher\u00b7li\u00b7che", "Gril\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.14": {"text": "Wodurch bey euch sich Pater Zipf und Zopf", "tokens": ["Wo\u00b7durch", "bey", "euch", "sich", "Pa\u00b7ter", "Zipf", "und", "Zopf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPER", "PRF", "NN", "NE", "KON", "NN"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.15": {"text": "Ihr K\u00fcrbisshaupt mit Hirngespinnsten f\u00fcllen,", "tokens": ["Ihr", "K\u00fcr\u00b7biss\u00b7haupt", "mit", "Hirn\u00b7ge\u00b7spinns\u00b7ten", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Erhitzet nie des tr\u00e4gen Popen Kopf.", "tokens": ["Er\u00b7hit\u00b7zet", "nie", "des", "tr\u00e4\u00b7gen", "Po\u00b7pen", "Kopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Ihm gilt es gleich, was eigentlich das Manna,", "tokens": ["Ihm", "gilt", "es", "gleich", ",", "was", "ei\u00b7gent\u00b7lich", "das", "Man\u00b7na", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PRELS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Ob's Butterteig, ob's Pfefferkuchen war:", "tokens": ["Ob's", "But\u00b7ter\u00b7teig", ",", "ob's", "Pfef\u00b7fer\u00b7ku\u00b7chen", "war", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KOUS", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Nach welchem Schnitt die badende Susanna", "tokens": ["Nach", "wel\u00b7chem", "Schnitt", "die", "ba\u00b7den\u00b7de", "Su\u00b7san\u00b7na"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Ihr Hemdchen trug, macht ihm kein graues Haar:", "tokens": ["Ihr", "Hemd\u00b7chen", "trug", ",", "macht", "ihm", "kein", "grau\u00b7es", "Haar", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.21": {"text": "Er zankt sich nicht, um wie viel Simsons Wade", "tokens": ["Er", "zankt", "sich", "nicht", ",", "um", "wie", "viel", "Sim\u00b7sons", "Wa\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "$,", "KOUI", "KOKOM", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Mehr Pfunde wog, als ein Philisterwanst,", "tokens": ["Mehr", "Pfun\u00b7de", "wog", ",", "als", "ein", "Phi\u00b7lis\u00b7ter\u00b7wanst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Ob David auch, als er der Bundeslade", "tokens": ["Ob", "Da\u00b7vid", "auch", ",", "als", "er", "der", "Bun\u00b7des\u00b7la\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.pl