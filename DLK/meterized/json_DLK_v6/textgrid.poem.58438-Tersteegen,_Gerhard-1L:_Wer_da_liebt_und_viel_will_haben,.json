{"textgrid.poem.58438": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer da liebt und viel will haben,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer da liebt und viel will haben,", "tokens": ["Wer", "da", "liebt", "und", "viel", "will", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "KON", "ADV", "VMFIN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebt sich selbst und Gottes ", "tokens": ["Liebt", "sich", "selbst", "und", "Got\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Lieben mit viel Trost und Licht", "tokens": ["Lie\u00b7ben", "mit", "viel", "Trost", "und", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist die beste Liebe nicht.", "tokens": ["Ist", "die", "bes\u00b7te", "Lie\u00b7be", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe ", "tokens": ["Lie\u00b7be"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "La\u00df dir alles andre rauben,", "tokens": ["La\u00df", "dir", "al\u00b7les", "and\u00b7re", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIS", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kehrt Gott selbst sich auch von dir \u2013", "tokens": ["Kehrt", "Gott", "selbst", "sich", "auch", "von", "dir", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "PRF", "ADV", "APPR", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Lieb, und Gott um Gott verlier!", "tokens": ["Lieb", ",", "und", "Gott", "um", "Gott", "ver\u00b7lier", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}