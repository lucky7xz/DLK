{"textgrid.poem.54949": {"metadata": {"author": {"name": "Scheerbart, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun la\u00dft uns wieder preisen", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun la\u00dft uns wieder preisen", "tokens": ["Nun", "la\u00dft", "uns", "wie\u00b7der", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die gro\u00dfe pr\u00e4chtige Sommernacht!", "tokens": ["Die", "gro\u00b7\u00dfe", "pr\u00e4ch\u00b7ti\u00b7ge", "Som\u00b7mer\u00b7nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nun la\u00dft uns wieder trinken", "tokens": ["Nun", "la\u00dft", "uns", "wie\u00b7der", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den schweren Feuertrank!", "tokens": ["Den", "schwe\u00b7ren", "Feu\u00b7er\u00b7trank", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun la\u00dft uns wieder jubeln!", "tokens": ["Nun", "la\u00dft", "uns", "wie\u00b7der", "ju\u00b7beln", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wir sind ja gar nicht m\u00fcd und krank.", "tokens": ["Wir", "sind", "ja", "gar", "nicht", "m\u00fcd", "und", "krank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKNEG", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nun la\u00dft uns wieder dichten", "tokens": ["Nun", "la\u00dft", "uns", "wie\u00b7der", "dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den wildesten tollsten Bacchantengesang!", "tokens": ["Den", "wil\u00b7des\u00b7ten", "tolls\u00b7ten", "Bac\u00b7chan\u00b7ten\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Nun la\u00dft uns lustig selig sein!", "tokens": ["Nun", "la\u00dft", "uns", "lus\u00b7tig", "se\u00b7lig", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wein! Wein in die alte Laube hinein!", "tokens": ["Wein", "!", "Wein", "in", "die", "al\u00b7te", "Lau\u00b7be", "hin\u00b7ein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Schon funkeln die Sterne da oben.", "tokens": ["Schon", "fun\u00b7keln", "die", "Ster\u00b7ne", "da", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Hei! St\u00fcrmisch das Glas erhoben!", "tokens": ["Hei", "!", "St\u00fcr\u00b7misch", "das", "Glas", "er\u00b7ho\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sommernacht, sei gepriesen!", "tokens": ["Som\u00b7mer\u00b7nacht", ",", "sei", "ge\u00b7prie\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Die bunten Lampen bringt auch herbei!", "tokens": ["Die", "bun\u00b7ten", "Lam\u00b7pen", "bringt", "auch", "her\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und auch die besten Zigarren!", "tokens": ["Und", "auch", "die", "bes\u00b7ten", "Zi\u00b7gar\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "In einer pr\u00e4chtigen Sommernacht", "tokens": ["In", "ei\u00b7ner", "pr\u00e4ch\u00b7ti\u00b7gen", "Som\u00b7mer\u00b7nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Soll man prassen, schlemmen und schwelgen!", "tokens": ["Soll", "man", "pras\u00b7sen", ",", "schlem\u00b7men", "und", "schwel\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}