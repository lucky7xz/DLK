{"textgrid.poem.37835": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Von den klugen Jungfrauen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wachet auf, ruft uns die Stimme", "tokens": ["Wa\u00b7chet", "auf", ",", "ruft", "uns", "die", "Stim\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der W\u00e4chter sehr hoch auf der Zinne,", "tokens": ["Der", "W\u00e4ch\u00b7ter", "sehr", "hoch", "auf", "der", "Zin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wach auf du Stadt Jerusalem,", "tokens": ["Wach", "auf", "du", "Stadt", "Je\u00b7ru\u00b7sa\u00b7lem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "NN", "NE", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Mitternacht hei\u00dft diese Stunde,", "tokens": ["Mit\u00b7ter\u00b7nacht", "hei\u00dft", "die\u00b7se", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie rufen uns mit hellem Munde:", "tokens": ["Sie", "ru\u00b7fen", "uns", "mit", "hel\u00b7lem", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbwo seyd ihr klugen Jungfrauen?", "tokens": ["\u00bb", "wo", "seyd", "ihr", "klu\u00b7gen", "Jung\u00b7frau\u00b7en", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wohlan der Br\u00e4utigam k\u00f6mmt,", "tokens": ["Wo\u00b7hlan", "der", "Br\u00e4u\u00b7ti\u00b7gam", "k\u00f6mmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Steht auf, die Lampen nehmt!", "tokens": ["Steht", "auf", ",", "die", "Lam\u00b7pen", "nehmt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Halleluja!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Macht euch bereit", "tokens": ["Macht", "euch", "be\u00b7reit"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "ADJD"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Zu der Hochzeit,", "tokens": ["Zu", "der", "Hoch\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Ihr m\u00fcsset ihm entgegen gehn.\u00ab", "tokens": ["Ihr", "m\u00fcs\u00b7set", "ihm", "ent\u00b7ge\u00b7gen", "gehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKVZ", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie h\u00f6rn die W\u00e4chter singen,", "tokens": ["Sie", "h\u00f6rn", "die", "W\u00e4ch\u00b7ter", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Herzen all vor Freuden springen,", "tokens": ["Die", "Her\u00b7zen", "all", "vor", "Freu\u00b7den", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wachen und stehn eilend auf;", "tokens": ["Sie", "wa\u00b7chen", "und", "stehn", "ei\u00b7lend", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Freund der kommt vom Himmel pr\u00e4chtig,", "tokens": ["Ihr", "Freund", "der", "kommt", "vom", "Him\u00b7mel", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von Gnaden stark, von Wahrheit m\u00e4chtig,", "tokens": ["Von", "Gna\u00b7den", "stark", ",", "von", "Wahr\u00b7heit", "m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Licht wird hell, ihr Stern geht auf.", "tokens": ["Ihr", "Licht", "wird", "hell", ",", "ihr", "Stern", "geht", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbnun komm du werthe Kron,", "tokens": ["\u00bb", "nun", "komm", "du", "wert\u00b7he", "Kron", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "VVFIN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Herr Jesu, Gottes Sohn!", "tokens": ["Herr", "Je\u00b7su", ",", "Got\u00b7tes", "Sohn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hosiana:", "tokens": ["Ho\u00b7si\u00b7a\u00b7na", ":"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Wir folgen all,", "tokens": ["Wir", "fol\u00b7gen", "all", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Zum Freuden-Saal,", "tokens": ["Zum", "Freu\u00b7den\u00b7Saal", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und halten mit das Abendmahl.\u00ab", "tokens": ["Und", "hal\u00b7ten", "mit", "das", "A\u00b7bend\u00b7mahl", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}