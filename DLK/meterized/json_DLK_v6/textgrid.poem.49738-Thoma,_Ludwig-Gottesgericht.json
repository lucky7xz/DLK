{"textgrid.poem.49738": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Gottesgericht", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Enterich hat j\u00fcngst im Freien", "tokens": ["Ein", "En\u00b7te\u00b7rich", "hat", "j\u00fcngst", "im", "Frei\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Liebe ohne Scheu gefr\u00f6nt.", "tokens": ["Der", "Lie\u00b7be", "oh\u00b7ne", "Scheu", "ge\u00b7fr\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nat\u00fcrlich waren sie zu zweien,", "tokens": ["Na\u00b7t\u00fcr\u00b7lich", "wa\u00b7ren", "sie", "zu", "zwei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und was sie taten, ist verp\u00f6nt.", "tokens": ["Und", "was", "sie", "ta\u00b7ten", ",", "ist", "ver\u00b7p\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er hatte das Rezept gefunden", "tokens": ["Er", "hat\u00b7te", "das", "Re\u00b7zept", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu jenem alten Wonnespiel,", "tokens": ["Zu", "je\u00b7nem", "al\u00b7ten", "Won\u00b7nes\u00b7piel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wobei er oben und sie unten", "tokens": ["Wo\u00b7bei", "er", "o\u00b7ben", "und", "sie", "un\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "KON", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ins Auge des Betrachters fiel.", "tokens": ["Ins", "Au\u00b7ge", "des", "Be\u00b7trach\u00b7ters", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ha! Wie ihm alle Sinne schwinden,", "tokens": ["Ha", "!", "Wie", "ihm", "al\u00b7le", "Sin\u00b7ne", "schwin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da schien es manchem offenbar,", "tokens": ["Da", "schien", "es", "man\u00b7chem", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df jedes ethische Empfinden", "tokens": ["Da\u00df", "je\u00b7des", "et\u00b7hi\u00b7sche", "Emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "In diesem Tier erloschen war.", "tokens": ["In", "die\u00b7sem", "Tier", "er\u00b7lo\u00b7schen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein solches Beispiel \u00f6ffentlicher", "tokens": ["Ein", "sol\u00b7ches", "Bei\u00b7spiel", "\u00f6f\u00b7fent\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ADJA"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Verdorbenheit kommt selten vor.", "tokens": ["Ver\u00b7dor\u00b7ben\u00b7heit", "kommt", "sel\u00b7ten", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Gottes M\u00fchlen mahlen sicher,", "tokens": ["Doch", "Got\u00b7tes", "M\u00fch\u00b7len", "mah\u00b7len", "si\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVINF", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier war es ein Benzinmotor.", "tokens": ["Hier", "war", "es", "ein", "Ben\u00b7zin\u00b7mo\u00b7tor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das Rad zerquetscht sie in der Rinne", "tokens": ["Das", "Rad", "zer\u00b7quetscht", "sie", "in", "der", "Rin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und pre\u00dft den Enterich auf sie,", "tokens": ["Und", "pre\u00dft", "den", "En\u00b7te\u00b7rich", "auf", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es war wohl in gewissem Sinne", "tokens": ["Es", "war", "wohl", "in", "ge\u00b7wis\u00b7sem", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch eine Schicksalsironie.", "tokens": ["Auch", "ei\u00b7ne", "Schick\u00b7sal\u00b7si\u00b7ro\u00b7nie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}