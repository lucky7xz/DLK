{"textgrid.poem.34827": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine starke, schwarze Barke", "tokens": ["Ei\u00b7ne", "star\u00b7ke", ",", "schwar\u00b7ze", "Bar\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Segelt trauervoll dahin.", "tokens": ["Se\u00b7gelt", "trau\u00b7er\u00b7voll", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die vermummten und verstummten", "tokens": ["Die", "ver\u00b7mumm\u00b7ten", "und", "ver\u00b7stumm\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leichenh\u00fcter sitzen drin.", "tokens": ["Lei\u00b7chen\u00b7h\u00fc\u00b7ter", "sit\u00b7zen", "drin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Toter Dichter, stille liegt er,", "tokens": ["To\u00b7ter", "Dich\u00b7ter", ",", "stil\u00b7le", "liegt", "er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit entbl\u00f6\u00dftem Angesicht;", "tokens": ["Mit", "ent\u00b7bl\u00f6\u00df\u00b7tem", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine blauen Augen schauen", "tokens": ["Sei\u00b7ne", "blau\u00b7en", "Au\u00b7gen", "schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer noch zum Himmelslicht.", "tokens": ["Im\u00b7mer", "noch", "zum", "Him\u00b7mels\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aus der Tiefe klingt's, als riefe", "tokens": ["Aus", "der", "Tie\u00b7fe", "klingt's", ",", "als", "rie\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine kranke Nixenbraut,", "tokens": ["Ei\u00b7ne", "kran\u00b7ke", "Ni\u00b7xen\u00b7braut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Wellen, sie zerschellen", "tokens": ["Und", "die", "Wel\u00b7len", ",", "sie", "zer\u00b7schel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An dem Kahn, wie Klagelaut.", "tokens": ["An", "dem", "Kahn", ",", "wie", "Kla\u00b7ge\u00b7laut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}