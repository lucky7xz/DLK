{"dta.poem.3018": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "64.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gewis ist was der Mund der heil'gen Lieder spricht,", "tokens": ["Ge\u00b7wis", "ist", "was", "der", "Mund", "der", "heil'\u00b7gen", "Lie\u00b7der", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PWS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob einstimmt fremde Kund' und ob sie widerspricht.", "tokens": ["Ob", "ein\u00b7stimmt", "frem\u00b7de", "Kund'", "und", "ob", "sie", "wi\u00b7der\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "KON", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gewis, allein f\u00fcr wen? f\u00fcr den allein der glaubt;", "tokens": ["Ge\u00b7wis", ",", "al\u00b7lein", "f\u00fcr", "wen", "?", "f\u00fcr", "den", "al\u00b7lein", "der", "glaubt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "PWS", "$.", "APPR", "ART", "ADV", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn durch Unglauben ist Gewisheit gleich geraubt.", "tokens": ["Denn", "durch", "Un\u00b7glau\u00b7ben", "ist", "Ge\u00b7wis\u00b7heit", "gleich", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So ist denn dir gewis, was in den Wedas steht,", "tokens": ["So", "ist", "denn", "dir", "ge\u00b7wis", ",", "was", "in", "den", "We\u00b7das", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ADV", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Moslem, was hervor ihm aus dem Koran geht,", "tokens": ["Dem", "Mos\u00b7lem", ",", "was", "her\u00b7vor", "ihm", "aus", "dem", "Ko\u00b7ran", "geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWS", "PTKVZ", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dem Christen aber nur, was seine Bibel sagt;", "tokens": ["Dem", "Chris\u00b7ten", "a\u00b7ber", "nur", ",", "was", "sei\u00b7ne", "Bi\u00b7bel", "sagt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun seht, wie dreierlei Gewisheit ihr vertragt.", "tokens": ["Nun", "seht", ",", "wie", "drei\u00b7er\u00b7lei", "Ge\u00b7wis\u00b7heit", "ihr", "ver\u00b7tragt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PIAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Gewis ist f\u00fcr den Geist Gewisses nichts zu stiften,", "tokens": ["Ge\u00b7wis", "ist", "f\u00fcr", "den", "Geist", "Ge\u00b7wis\u00b7ses", "nichts", "zu", "stif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN", "NN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn die Gewisheit ruht auf ungewissen Schriften.", "tokens": ["Wenn", "die", "Ge\u00b7wis\u00b7heit", "ruht", "auf", "un\u00b7ge\u00b7wis\u00b7sen", "Schrif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}