{"textgrid.poem.34033": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Italiens erster Gru\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heliogabalus lie\u00df die G\u00e4ste ersticken mit Veilchen:", "tokens": ["He\u00b7lio\u00b7ga\u00b7ba\u00b7lus", "lie\u00df", "die", "G\u00e4s\u00b7te", "er\u00b7sti\u00b7cken", "mit", "Veil\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Sch\u00f6nes Italien, drohst du mir ein \u00e4hnliches Loos?", "tokens": ["Sch\u00f6\u00b7nes", "I\u00b7ta\u00b7li\u00b7en", ",", "drohst", "du", "mir", "ein", "\u00e4hn\u00b7li\u00b7ches", "Loos", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+---+-+-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Deiner F\u00fclle erlieg' ich! Sie ist f\u00fcr G\u00f6tter und K\u00e4fer!", "tokens": ["Dei\u00b7ner", "F\u00fcl\u00b7le", "er\u00b7lieg'", "ich", "!", "Sie", "ist", "f\u00fcr", "G\u00f6t\u00b7ter", "und", "K\u00e4\u00b7fer", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "G\u00f6ttern bin ich nicht gleich, K\u00e4fern noch minder verwandt!", "tokens": ["G\u00f6t\u00b7tern", "bin", "ich", "nicht", "gleich", ",", "K\u00e4\u00b7fern", "noch", "min\u00b7der", "ver\u00b7wandt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "ADV", "$,", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}