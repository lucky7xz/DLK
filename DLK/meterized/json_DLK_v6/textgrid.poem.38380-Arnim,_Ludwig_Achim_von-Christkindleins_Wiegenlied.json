{"textgrid.poem.38380": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Christkindleins Wiegenlied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.57", "ro:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Jesulein zart,", "tokens": ["O", "Je\u00b7su\u00b7lein", "zart", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "O Jesulein zart,", "tokens": ["O", "Je\u00b7su\u00b7lein", "zart", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Das Kripplein ist hart,", "tokens": ["Das", "Krip\u00b7plein", "ist", "hart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wie liegst du so hart,", "tokens": ["Wie", "liegst", "du", "so", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ach schlaf, ach thu die A\u00fcgelein zu,", "tokens": ["Ach", "schlaf", ",", "ach", "thu", "die", "A\u00b7\u00fc\u00b7ge\u00b7lein", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "ITJ", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Schlaf, und gieb uns die ewige Ruh.", "tokens": ["Schlaf", ",", "und", "gieb", "uns", "die", "e\u00b7wi\u00b7ge", "Ruh", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Schlaf Jesulein wohl,", "tokens": ["Schlaf", "Je\u00b7su\u00b7lein", "wohl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Nichts hindern soll", "tokens": ["Nichts", "hin\u00b7dern", "soll"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VVINF", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ochs Esel und Schaf,", "tokens": ["Ochs", "E\u00b7sel", "und", "Schaf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Sind alle im Schlaf.", "tokens": ["Sind", "al\u00b7le", "im", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Schlaf Kind schlaf, thue dein A\u00fcglein zu,", "tokens": ["Schlaf", "Kind", "schlaf", ",", "thue", "dein", "A\u00b7\u00fc\u00b7glein", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Schlaf und gieb uns die ewige Ruh.", "tokens": ["Schlaf", "und", "gieb", "uns", "die", "e\u00b7wi\u00b7ge", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Dir Seraphim singt,", "tokens": ["Dir", "Se\u00b7ra\u00b7phim", "singt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NE", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und Cherubim klingt,", "tokens": ["Und", "Che\u00b7ru\u00b7bim", "klingt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Viel Engel im Stall,", "tokens": ["Viel", "En\u00b7gel", "im", "Stall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die wiegen dich all.", "tokens": ["Die", "wie\u00b7gen", "dich", "all", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Schlaf Kind schlaf, thu dein A\u00fcglein zu,", "tokens": ["Schlaf", "Kind", "schlaf", ",", "thu", "dein", "A\u00b7\u00fc\u00b7glein", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Schlaf und gieb uns die ewige Ruh.", "tokens": ["Schlaf", "und", "gieb", "uns", "die", "e\u00b7wi\u00b7ge", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Sieh Jesulein sieh,", "tokens": ["Sieh", "Je\u00b7su\u00b7lein", "sieh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Sankt Joseph ist hie,", "tokens": ["Sankt", "Jo\u00b7se\u00b7ph", "ist", "hie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bleib auch hiebei,", "tokens": ["Ich", "bleib", "auch", "hie\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Schlaf sicher und frei.", "tokens": ["Schlaf", "si\u00b7cher", "und", "frei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "++--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Schlaf Kind schlaf, thu dein A\u00fcglein zu,", "tokens": ["Schlaf", "Kind", "schlaf", ",", "thu", "dein", "A\u00b7\u00fc\u00b7glein", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Schlaf, und gieb uns die ewige Ruh.", "tokens": ["Schlaf", ",", "und", "gieb", "uns", "die", "e\u00b7wi\u00b7ge", "Ruh", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Schweig Eselein still,", "tokens": ["Schweig", "E\u00b7sel\u00b7ein", "still", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Das Kind schlafen will,", "tokens": ["Das", "Kind", "schla\u00b7fen", "will", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ey Oechsle nicht br\u00fcll,", "tokens": ["Ey", "O\u00b7echs\u00b7le", "nicht", "br\u00fcll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PTKNEG", "ADJD", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Da