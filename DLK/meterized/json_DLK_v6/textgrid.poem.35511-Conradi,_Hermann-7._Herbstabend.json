{"textgrid.poem.35511": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "7. Herbstabend", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kehrt' aus engen Gassen", "tokens": ["Ich", "kehrt'", "aus", "en\u00b7gen", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich durch das alte Tor ...", "tokens": ["Mich", "durch", "das", "al\u00b7te", "Tor", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Waldpfade \u2013 wie verlassen!", "tokens": ["Wald\u00b7pfa\u00b7de", "\u2013", "wie", "ver\u00b7las\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "PWAV", "VVPP", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Blauwei\u00dfe D\u00fcnste schweben,", "tokens": ["Blau\u00b7wei\u00b7\u00dfe", "D\u00fcns\u00b7te", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und die Gedanken geben", "tokens": ["Und", "die", "Ge\u00b7dan\u00b7ken", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sich dem, was ich verlor ...", "tokens": ["Sich", "dem", ",", "was", "ich", "ver\u00b7lor", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "$,", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Welch wundersames Feiern \u2013", "tokens": ["Welch", "wun\u00b7der\u00b7sa\u00b7mes", "Fei\u00b7ern", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie still am Waldessaum!", "tokens": ["Wie", "still", "am", "Wal\u00b7des\u00b7saum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verh\u00fcllt von wei\u00dfen Schleiern", "tokens": ["Ver\u00b7h\u00fcllt", "von", "wei\u00b7\u00dfen", "Schlei\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Entschwebet mir das Leben ...", "tokens": ["Ent\u00b7schwe\u00b7bet", "mir", "das", "Le\u00b7ben", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Just wie ein sanfter Traum \u2013", "tokens": ["Just", "wie", "ein", "sanf\u00b7ter", "Traum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und ich beklag' es kaum ...", "tokens": ["Und", "ich", "be\u00b7klag'", "es", "kaum", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und was in Schmerzen ich verlor:", "tokens": ["Und", "was", "in", "Schmer\u00b7zen", "ich", "ver\u00b7lor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinnimmt's zum andern Male", "tokens": ["Hin\u00b7nimmt's", "zum", "an\u00b7dern", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Beim letzten Abendstrahle", "tokens": ["Beim", "letz\u00b7ten", "A\u00b7bend\u00b7strah\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Schatten Schicksalschor ...", "tokens": ["Der", "Schat\u00b7ten", "Schick\u00b7sal\u00b7schor", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}