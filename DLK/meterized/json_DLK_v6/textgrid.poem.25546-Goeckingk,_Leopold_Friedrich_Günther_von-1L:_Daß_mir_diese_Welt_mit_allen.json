{"textgrid.poem.25546": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df mir diese Welt mit allen", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df mir diese Welt mit allen", "tokens": ["Da\u00df", "mir", "die\u00b7se", "Welt", "mit", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDAT", "NN", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihren Narren, wohlgef\u00e4llt;", "tokens": ["Ih\u00b7ren", "Nar\u00b7ren", ",", "wohl\u00b7ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df, vom Dummkopf' angefallen,", "tokens": ["Da\u00df", ",", "vom", "Dumm\u00b7kopf'", "an\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem Neider angebellt,", "tokens": ["Von", "dem", "Nei\u00b7der", "an\u00b7ge\u00b7bellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Rach' und Spott zur\u00fccke prallen:", "tokens": ["Rach'", "und", "Spott", "zu\u00b7r\u00fc\u00b7cke", "pral\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Daf\u00fcr nimm, du Zauberin!", "tokens": ["Da\u00b7f\u00fcr", "nimm", ",", "du", "Zau\u00b7be\u00b7rin", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Diesen Ku\u00df zum Danke hin!", "tokens": ["Die\u00b7sen", "Ku\u00df", "zum", "Dan\u00b7ke", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df ich keine Sorgen n\u00e4hre,", "tokens": ["Da\u00df", "ich", "kei\u00b7ne", "Sor\u00b7gen", "n\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Titel nicht erschmeicheln mag,", "tokens": ["Ti\u00b7tel", "nicht", "er\u00b7schmei\u00b7cheln", "mag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Bunt Gepr\u00e4nge gern entbehre,", "tokens": ["Bunt", "Ge\u00b7pr\u00e4n\u00b7ge", "gern", "ent\u00b7beh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Kurz, da\u00df mir ein froher Tag", "tokens": ["Kurz", ",", "da\u00df", "mir", "ein", "fro\u00b7her", "Tag"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Mehr ist, als ein Jahr voll Ehre:", "tokens": ["Mehr", "ist", ",", "als", "ein", "Jahr", "voll", "Eh\u00b7re", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "KOUS", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Daf\u00fcr, holde Schmeichlerin!", "tokens": ["Da\u00b7f\u00fcr", ",", "hol\u00b7de", "Schmeich\u00b7le\u00b7rin", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Nimm die\u00df Lied zum Danke hin!", "tokens": ["Nimm", "die\u00df", "Lied", "zum", "Dan\u00b7ke", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Da\u00df in z\u00e4rtlichen Ges\u00e4ngen", "tokens": ["Da\u00df", "in", "z\u00e4rt\u00b7li\u00b7chen", "Ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Deine Liebe sanften Schmerz", "tokens": ["Dei\u00b7ne", "Lie\u00b7be", "sanf\u00b7ten", "Schmerz"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Mit der Freude wei\u00df zu mengen,", "tokens": ["Mit", "der", "Freu\u00b7de", "wei\u00df", "zu", "men\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "So da\u00df Schauer in mein Herz", "tokens": ["So", "da\u00df", "Schau\u00b7er", "in", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "NE", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Sich wie Meereswogen dr\u00e4ngen:", "tokens": ["Sich", "wie", "Mee\u00b7res\u00b7wo\u00b7gen", "dr\u00e4n\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "KOKOM", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Daf\u00fcr nimm, du S\u00e4ngerin,", "tokens": ["Da\u00b7f\u00fcr", "nimm", ",", "du", "S\u00e4n\u00b7ge\u00b7rin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Thr\u00e4nen statt des Dankes hin!", "tokens": ["Thr\u00e4\u00b7nen", "statt", "des", "Dan\u00b7kes", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Da\u00df ich oft zur Sternenh\u00f6he", "tokens": ["Da\u00df", "ich", "oft", "zur", "Ster\u00b7nen\u00b7h\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Bald mit heiterm Angesicht',", "tokens": ["Bald", "mit", "hei\u00b7term", "An\u00b7ge\u00b7sicht'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Bald mit stillen Thr\u00e4nen sehe;", "tokens": ["Bald", "mit", "stil\u00b7len", "Thr\u00e4\u00b7nen", "se\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Da\u00df ich dann um G\u00fcter nicht,", "tokens": ["Da\u00df", "ich", "dann", "um", "G\u00fc\u00b7ter", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Nur um dich und Weisheit flehe:", "tokens": ["Nur", "um", "dich", "und", "Weis\u00b7heit", "fle\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Daf\u00fcr, du Bekehrerin!", "tokens": ["Da\u00b7f\u00fcr", ",", "du", "Be\u00b7keh\u00b7re\u00b7rin", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Nimm mein Herz zum Danke hin!", "tokens": ["Nimm", "mein", "Herz", "zum", "Dan\u00b7ke", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}