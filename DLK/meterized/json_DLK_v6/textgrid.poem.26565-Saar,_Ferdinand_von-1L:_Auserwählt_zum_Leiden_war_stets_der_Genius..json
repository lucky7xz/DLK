{"textgrid.poem.26565": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auserw\u00e4hlt zum Leiden war stets der Genius.", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auserw\u00e4hlt zum Leiden war stets der Genius.", "tokens": ["Au\u00b7ser\u00b7w\u00e4hlt", "zum", "Lei\u00b7den", "war", "stets", "der", "Ge\u00b7nius", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ihr wi\u00dft es,", "tokens": ["Ihr", "wi\u00dft", "es", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und keinen von euch h\u00e4lt es ab,", "tokens": ["Und", "kei\u00b7nen", "von", "euch", "h\u00e4lt", "es", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "APPR", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Pfade zu gehen,", "tokens": ["Pfa\u00b7de", "zu", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die And're vor euch gegangen \u2013", "tokens": ["Die", "An\u00b7d'\u00b7re", "vor", "euch", "ge\u00b7gan\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVPP", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und gleich ihnen", "tokens": ["Und", "gleich", "ih\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Zu siegen oder zu fallen.", "tokens": ["Zu", "sie\u00b7gen", "o\u00b7der", "zu", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Aber Eines bedenkt:", "tokens": ["A\u00b7ber", "Ei\u00b7nes", "be\u00b7denkt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ung\u00fcnst'ger als die, in der wir leben,", "tokens": ["Un\u00b7g\u00fcnst'\u00b7ger", "als", "die", ",", "in", "der", "wir", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "War keine Zeit noch dem Dichter,", "tokens": ["War", "kei\u00b7ne", "Zeit", "noch", "dem", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ob auch der Geist in ihr", "tokens": ["Ob", "auch", "der", "Geist", "in", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lauteste Triumphe feiert.", "tokens": ["Lau\u00b7tes\u00b7te", "Tri\u00b7um\u00b7phe", "fei\u00b7ert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Denn seht:", "tokens": ["Denn", "seht", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Schaffend und empf\u00e4nglich zugleich", "tokens": ["Schaf\u00b7fend", "und", "emp\u00b7f\u00e4ng\u00b7lich", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Ist der Geist,", "tokens": ["Ist", "der", "Geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Wenn er als schimmernde Bl\u00fcthe noch", "tokens": ["Wenn", "er", "als", "schim\u00b7mern\u00b7de", "Bl\u00fc\u00b7the", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOUS", "ADJA", "NN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Liebreich herab sich zur Wurzel senkt,", "tokens": ["Lieb\u00b7reich", "her\u00b7ab", "sich", "zur", "Wur\u00b7zel", "senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Ahnungsvoll die dunkle beleuchtend.", "tokens": ["Ah\u00b7nungs\u00b7voll", "die", "dunk\u00b7le", "be\u00b7leuch\u00b7tend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "VVPP", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Mehr und mehr jedoch", "tokens": ["Mehr", "und", "mehr", "je\u00b7doch"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "KON", "ADV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wird er zur Flamme schon,", "tokens": ["Wird", "er", "zur", "Flam\u00b7me", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die sie verzehrt,", "tokens": ["Die", "sie", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Um in sich selber zu brennen \u2013", "tokens": ["Um", "in", "sich", "sel\u00b7ber", "zu", "bren\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "PRF", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und endlich auch in sich selbst zu verloh'n.", "tokens": ["Und", "end\u00b7lich", "auch", "in", "sich", "selbst", "zu", "ver\u00b7loh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ADV", "ADV", "APPR", "PRF", "ADV", "APPR", "NE", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Weggeschwunden ist", "tokens": ["Weg\u00b7ge\u00b7schwun\u00b7den", "ist"], "token_info": ["word", "word"], "pos": ["NN", "VAFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Unter dem Fu\u00dfe der Boden euch,", "tokens": ["Un\u00b7ter", "dem", "Fu\u00b7\u00dfe", "der", "Bo\u00b7den", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PPER", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Wie der Menschheit,", "tokens": ["Wie", "der", "Menschheit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Die, entwachsen der Vergangenheit", "tokens": ["Die", ",", "ent\u00b7wach\u00b7sen", "der", "Ver\u00b7gan\u00b7gen\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Und losgel\u00f6s't von Jahrtausenden,", "tokens": ["Und", "los\u00b7ge\u00b7l\u00f6s't", "von", "Jahr\u00b7tau\u00b7sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Nach neuem Leben verlangt \u2013", "tokens": ["Nach", "neu\u00b7em", "Le\u00b7ben", "ver\u00b7langt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Und doch vielleicht nur in's Leere greift.", "tokens": ["Und", "doch", "viel\u00b7leicht", "nur", "in's", "Lee\u00b7re", "greift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Dies sagt ein Dichter euch,", "tokens": ["Dies", "sagt", "ein", "Dich\u00b7ter", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dessen letztes Lied", "tokens": ["Des\u00b7sen", "letz\u00b7tes", "Lied"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wehm\u00fcthig noch ert\u00f6nt", "tokens": ["Weh\u00b7m\u00fct\u00b7hig", "noch", "er\u00b7t\u00f6nt"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Am Rande des Abgrunds.", "tokens": ["Am", "Ran\u00b7de", "des", "Ab\u00b7grunds", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}