{"dta.poem.5981": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df eine Insul einst in Rom erzeuget ward,", "tokens": ["Da\u00df", "ei\u00b7ne", "In\u00b7sul", "einst", "in", "Rom", "er\u00b7zeu\u00b7get", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "NE", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus Garben, welche man von ohngefehr", "tokens": ["Aus", "Gar\u00b7ben", ",", "wel\u00b7che", "man", "von", "ohn\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PIS", "APPR", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hatt\u2019 in die Tiber eingesenckt,", "tokens": ["Hatt'", "in", "die", "Ti\u00b7ber", "ein\u00b7ge\u00b7senckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wovon die eine erst den Grund ber\u00fchret,", "tokens": ["Wo\u00b7von", "die", "ei\u00b7ne", "erst", "den", "Grund", "be\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Daran die andern sich hernachmahls angeh\u00e4ngt.", "tokens": ["Da\u00b7ran", "die", "an\u00b7dern", "sich", "her\u00b7nach\u00b7mahls", "an\u00b7ge\u00b7h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Schlamm, der durch die Fluht darauf gef\u00fchret,", "tokens": ["Der", "Schlamm", ",", "der", "durch", "die", "Fluht", "da\u00b7rauf", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Macht, da\u00df die Masse stets verdickt ward, und gen\u00e4hrt;", "tokens": ["Macht", ",", "da\u00df", "die", "Mas\u00b7se", "stets", "ver\u00b7dickt", "ward", ",", "und", "ge\u00b7n\u00e4hrt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "ADV", "VVPP", "VAFIN", "$,", "KON", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Worauf ein neuer Schlamm,", "tokens": ["Wo\u00b7rauf", "ein", "neu\u00b7er", "Schlamm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Der alle Tage darauf kam,", "tokens": ["Der", "al\u00b7le", "Ta\u00b7ge", "da\u00b7rauf", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sie mehr befestigt und vermehrt.", "tokens": ["Sie", "mehr", "be\u00b7fes\u00b7tigt", "und", "ver\u00b7mehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die Insel aber nun der neu gebohrnen Erden,", "tokens": ["Die", "In\u00b7sel", "a\u00b7ber", "nun", "der", "neu", "ge\u00b7bohr\u00b7nen", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Indem sie auf dem Grunde ruht;", "tokens": ["In\u00b7dem", "sie", "auf", "dem", "Grun\u00b7de", "ruht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "So konnte sie auch von der Fluht", "tokens": ["So", "konn\u00b7te", "sie", "auch", "von", "der", "Fluht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.14": {"text": "Nicht rund, wie ein Planet, der schwimmt, formiret werden.", "tokens": ["Nicht", "rund", ",", "wie", "ein", "Pla\u00b7net", ",", "der", "schwimmt", ",", "for\u00b7mi\u00b7ret", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "PWAV", "ART", "NN", "$,", "PRELS", "VVFIN", "$,", "VVPP", "VAINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Das Wasser, welches sie umgiebt, da an der Seiten", "tokens": ["Das", "Was\u00b7ser", ",", "wel\u00b7ches", "sie", "um\u00b7giebt", ",", "da", "an", "der", "Sei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Wellen immer gleiten,", "tokens": ["Die", "Wel\u00b7len", "im\u00b7mer", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Macht sie, durch seinen steten Drang,", "tokens": ["Macht", "sie", ",", "durch", "sei\u00b7nen", "ste\u00b7ten", "Drang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.18": {"text": "Als wie ein Schiff oval und lang.", "tokens": ["Als", "wie", "ein", "Schiff", "o\u00b7val", "und", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "NE", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die Jahre haben es befestigt, nicht zerst\u00f6ret.", "tokens": ["Die", "Jah\u00b7re", "ha\u00b7ben", "es", "be\u00b7fes\u00b7tigt", ",", "nicht", "zer\u00b7st\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sie trug sehr wichtige Geb\u00e4ude, ja den Grund,", "tokens": ["Sie", "trug", "sehr", "wich\u00b7ti\u00b7ge", "Ge\u00b7b\u00e4u\u00b7de", ",", "ja", "den", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Worauf des Aesculaps erhabner Tempel stund.", "tokens": ["Wo\u00b7rauf", "des", "A\u00b7e\u00b7scu\u00b7laps", "er\u00b7hab\u00b7ner", "Tem\u00b7pel", "stund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Wie manch bedeckter Gang ward nicht drauf gebauet,", "tokens": ["Wie", "manch", "be\u00b7deck\u00b7ter", "Gang", "ward", "nicht", "drauf", "ge\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "VAFIN", "PTKNEG", "PAV", "VVPP", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Wie man das Alterthum daran noch itzo schauet? ", "tokens": ["Wie", "man", "das", "Al\u00b7ter\u00b7thum", "da\u00b7ran", "noch", "it\u00b7zo", "schau\u00b7et", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "PAV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ja man sieht itzt noch grosse H\u00f6hen", "tokens": ["Ja", "man", "sieht", "itzt", "noch", "gros\u00b7se", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PIS", "VVFIN", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Von heil\u2019gen Tempeln auf ihr stehen.", "tokens": ["Von", "heil'\u00b7gen", "Tem\u00b7peln", "auf", "ihr", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}