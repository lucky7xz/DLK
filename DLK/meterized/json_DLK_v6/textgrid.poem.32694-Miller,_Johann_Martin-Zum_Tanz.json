{"textgrid.poem.32694": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Zum Tanz", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt, ihr Frauen, auf den Plan,", "tokens": ["Kommt", ",", "ihr", "Frau\u00b7en", ",", "auf", "den", "Plan", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der, euch zu gefallen,", "tokens": ["Der", ",", "euch", "zu", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sich mit Blumen angethan,", "tokens": ["Sich", "mit", "Blu\u00b7men", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo, im kleinen Wald daran,", "tokens": ["Wo", ",", "im", "klei\u00b7nen", "Wald", "da\u00b7ran", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPRART", "ADJA", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vogellieder schallen!", "tokens": ["Vo\u00b7gel\u00b7lie\u00b7der", "schal\u00b7len", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Bunte Blumen wollen wir", "tokens": ["Bun\u00b7te", "Blu\u00b7men", "wol\u00b7len", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VMFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Scho\u00df euch streuen!", "tokens": ["In", "den", "Scho\u00df", "euch", "streu\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Euch zu Kr\u00e4nzen sollet ihr", "tokens": ["Euch", "zu", "Kr\u00e4n\u00b7zen", "sol\u00b7let", "ihr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "VMFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie, mit wonniglicher Zier,", "tokens": ["Sie", ",", "mit", "won\u00b7nig\u00b7li\u00b7cher", "Zier", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aneinander reihen!", "tokens": ["An\u00b7ein\u00b7an\u00b7der", "rei\u00b7hen", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Alles haben wir bestellt,", "tokens": ["Al\u00b7les", "ha\u00b7ben", "wir", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was erfreut die Sinnen;", "tokens": ["Was", "er\u00b7freut", "die", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und, so bald es euch gef\u00e4llt,", "tokens": ["Und", ",", "so", "bald", "es", "euch", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ADV", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6nnen wir, auf gr\u00fcnem Feld,", "tokens": ["K\u00f6n\u00b7nen", "wir", ",", "auf", "gr\u00fc\u00b7nem", "Feld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Einen Tanz beginnen.", "tokens": ["Ei\u00b7nen", "Tanz", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Bis es d\u00e4mmert auf der Au,", "tokens": ["Bis", "es", "d\u00e4m\u00b7mert", "auf", "der", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind wir beieinander,", "tokens": ["Sind", "wir", "bei\u00b7ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und dann geht, im k\u00fchlen Tau,", "tokens": ["Und", "dann", "geht", ",", "im", "k\u00fch\u00b7len", "Tau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der minniglichen Frau", "tokens": ["Mit", "der", "min\u00b7nig\u00b7li\u00b7chen", "Frau"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeder Freund selbander.", "tokens": ["Je\u00b7der", "Freund", "sel\u00b7ban\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}