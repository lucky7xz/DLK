{"textgrid.poem.63079": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Wikinger", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sind von einem fernen Nord geschwommen", "tokens": ["Wir", "sind", "von", "ei\u00b7nem", "fer\u00b7nen", "Nord", "ge\u00b7schwom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie wilde Schw\u00e4ne, s\u00fcdige Welt zu sehn,", "tokens": ["Wie", "wil\u00b7de", "Schw\u00e4\u00b7ne", ",", "s\u00fc\u00b7di\u00b7ge", "Welt", "zu", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und sind zu Menschen gekommen,", "tokens": ["Und", "sind", "zu", "Men\u00b7schen", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "An deren Schritten keine Fl\u00fcgel wehn.", "tokens": ["An", "de\u00b7ren", "Schrit\u00b7ten", "kei\u00b7ne", "Fl\u00fc\u00b7gel", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihre F\u00fc\u00dfe sind plump, ihre Haare sind schwarz,", "tokens": ["Ih\u00b7re", "F\u00fc\u00b7\u00dfe", "sind", "plump", ",", "ih\u00b7re", "Haa\u00b7re", "sind", "schwarz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ihre Weiber sind dick wie Walrosse.", "tokens": ["Ih\u00b7re", "Wei\u00b7ber", "sind", "dick", "wie", "Wal\u00b7ros\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wir schenken sie unserm Trosse.", "tokens": ["Wir", "schen\u00b7ken", "sie", "un\u00b7serm", "Tros\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wir sind B\u00e4ume. Aus unsern blonden B\u00e4rten tropft Harz.", "tokens": ["Wir", "sind", "B\u00e4u\u00b7me", ".", "Aus", "un\u00b7sern", "blon\u00b7den", "B\u00e4r\u00b7ten", "tropft", "Harz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$.", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "NE", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Wir schlingen die M\u00f6we roh in unsern Rachen.", "tokens": ["Wir", "schlin\u00b7gen", "die", "M\u00f6\u00b7we", "roh", "in", "un\u00b7sern", "Ra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Unsere Drachen-", "tokens": ["Un\u00b7se\u00b7re", "Dra\u00b7chen"], "token_info": ["word", "word"], "pos": ["PPOSAT", "TRUNC"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Schiffe", "tokens": ["Schif\u00b7fe"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Und wir", "tokens": ["Und", "wir"], "token_info": ["word", "word"], "pos": ["KON", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Haben scharfen Zahn.", "tokens": ["Ha\u00b7ben", "schar\u00b7fen", "Zahn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Wir hacken ihn ohne List und Kniffe", "tokens": ["Wir", "ha\u00b7cken", "ihn", "oh\u00b7ne", "List", "und", "Knif\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "In feindlich Mensch und Tier.", "tokens": ["In", "feind\u00b7lich", "Mensch", "und", "Tier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und in unsrer Gattin Galan.", "tokens": ["Und", "in", "uns\u00b7rer", "Gat\u00b7tin", "Ga\u00b7lan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Aber wenn wir an dem neuen Strande zechen", "tokens": ["A\u00b7ber", "wenn", "wir", "an", "dem", "neu\u00b7en", "Stran\u00b7de", "ze\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und den Fraun im Spa\u00df die Schenkel brechen \u2013", "tokens": ["Und", "den", "Fraun", "im", "Spa\u00df", "die", "Schen\u00b7kel", "bre\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Algenmoosumkr\u00e4nzt,", "tokens": ["Al\u00b7gen\u00b7mo\u00b7o\u00b7sum\u00b7kr\u00e4nzt", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Sklaven sind die Sassen, wir sind Lorde:", "tokens": ["Skla\u00b7ven", "sind", "die", "Sas\u00b7sen", ",", "wir", "sind", "Lor\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Rauschts in unsern Augen blaue Fjorde,", "tokens": ["Rauschts", "in", "un\u00b7sern", "Au\u00b7gen", "blau\u00b7e", "Fjor\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die das Nordlicht rosa \u00fcbergl\u00e4nzt.", "tokens": ["Die", "das", "Nord\u00b7licht", "ro\u00b7sa", "\u00fc\u00b7ber\u00b7gl\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NE", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}