{"textgrid.poem.40856": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Karls Tod", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne, die gar manche der sch\u00f6nen L\u00e4nder gesehn,", "tokens": ["Die", "Son\u00b7ne", ",", "die", "gar", "man\u00b7che", "der", "sch\u00f6\u00b7nen", "L\u00e4n\u00b7der", "ge\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PIS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Bleibt, um ihr Aug' zu weiden, gern in Burgund doch stehn;", "tokens": ["Bleibt", ",", "um", "ihr", "Aug'", "zu", "wei\u00b7den", ",", "gern", "in", "Bur\u00b7gund", "doch", "stehn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "ADV", "APPR", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Der Mond, der schon gek\u00fcsset manch sch\u00f6nen s\u00fc\u00dfen Mund,", "tokens": ["Der", "Mond", ",", "der", "schon", "ge\u00b7k\u00fcs\u00b7set", "manch", "sch\u00f6\u00b7nen", "s\u00fc\u00b7\u00dfen", "Mund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP", "PIAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "K\u00fc\u00dft nie doch satt die Lippen Mariens von Burgund.", "tokens": ["K\u00fc\u00dft", "nie", "doch", "satt", "die", "Lip\u00b7pen", "Ma\u00b7ri\u00b7ens", "von", "Bur\u00b7gund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "ART", "NN", "NE", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Reich ist der Burgunderherzog an Landen hehr und sch\u00f6n,", "tokens": ["Reich", "ist", "der", "Bur\u00b7gun\u00b7der\u00b7her\u00b7zog", "an", "Lan\u00b7den", "hehr", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Voll Aehren stehn die Fl\u00e4chen, voll Reben gl\u00fchn die H\u00f6hn,", "tokens": ["Voll", "A\u00b7eh\u00b7ren", "stehn", "die", "Fl\u00e4\u00b7chen", ",", "voll", "Re\u00b7ben", "gl\u00fchn", "die", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "ART", "NN", "$,", "ADJD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Da spiegelt reiche St\u00e4dte und frohes Volk der Flu\u00df,", "tokens": ["Da", "spie\u00b7gelt", "rei\u00b7che", "St\u00e4d\u00b7te", "und", "fro\u00b7hes", "Volk", "der", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "KON", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und Segen ist hier Schnitter, und Winzer Ueberflu\u00df.", "tokens": ["Und", "Se\u00b7gen", "ist", "hier", "Schnit\u00b7ter", ",", "und", "Win\u00b7zer", "Ue\u00b7berf\u00b7lu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "NN", "$,", "KON", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Reich ist er auch an Sch\u00e4tzen, Gestein und blankem Erz,", "tokens": ["Reich", "ist", "er", "auch", "an", "Sch\u00e4t\u00b7zen", ",", "Ge\u00b7stein", "und", "blan\u00b7kem", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "APPR", "NN", "$,", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ihm hat die Erd' er\u00f6ffnet ihr warmes, reiches Herz,", "tokens": ["Ihm", "hat", "die", "Erd'", "er\u00b7\u00f6ff\u00b7net", "ihr", "war\u00b7mes", ",", "rei\u00b7ches", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ihm winkt in stolzem Baue manch Schlo\u00df voll Prunk und Gold,", "tokens": ["Ihm", "winkt", "in", "stol\u00b7zem", "Bau\u00b7e", "manch", "Schlo\u00df", "voll", "Prunk", "und", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PIAT", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und aus dem goldnen Schlosse die Tochter bl\u00fchend hold.", "tokens": ["Und", "aus", "dem", "gold\u00b7nen", "Schlos\u00b7se", "die", "Toch\u00b7ter", "bl\u00fc\u00b7hend", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Lande mu\u00df er schirmen mit kampfgest\u00e4hltem Schwert,", "tokens": ["Die", "Lan\u00b7de", "mu\u00df", "er", "schir\u00b7men", "mit", "kampf\u00b7ge\u00b7st\u00e4hl\u00b7tem", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df nicht ihr Garten welke, von Feindesbrand verzehrt;", "tokens": ["Da\u00df", "nicht", "ihr", "Gar\u00b7ten", "wel\u00b7ke", ",", "von", "Fein\u00b7des\u00b7brand", "ver\u00b7zehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Sch\u00e4tze mu\u00df er pflegen, da\u00df sie noch f\u00fcrder dauern,", "tokens": ["Die", "Sch\u00e4t\u00b7ze", "mu\u00df", "er", "pfle\u00b7gen", ",", "da\u00df", "sie", "noch", "f\u00fcr\u00b7der", "dau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn l\u00e4ngst der treue Pfleger hinsank in Todesschauern.", "tokens": ["Wenn", "l\u00e4ngst", "der", "treu\u00b7e", "Pfle\u00b7ger", "hin\u00b7sank", "in", "To\u00b7des\u00b7schau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbleb' wohl! und kehr' ich nimmer, dann Tochter zage nicht!", "tokens": ["\u00bb", "leb'", "wohl", "!", "und", "kehr'", "ich", "nim\u00b7mer", ",", "dann", "Toch\u00b7ter", "za\u00b7ge", "nicht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "$.", "KON", "VVFIN", "PPER", "ADV", "$,", "ADV", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gib deinen Fr\u00fchlingskeimen ein neues Sonnenlicht;", "tokens": ["Gib", "dei\u00b7nen", "Fr\u00fch\u00b7lings\u00b7kei\u00b7men", "ein", "neu\u00b7es", "Son\u00b7nen\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "In Oestreich mag's dir gl\u00e4nzen auf Fluren segensreich,", "tokens": ["In", "O\u00b7e\u00b7streich", "mag's", "dir", "gl\u00e4n\u00b7zen", "auf", "Flu\u00b7ren", "se\u00b7gens\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "PPER", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dort bl\u00fcht die Heldenblume, der keine andre gleich.\u00ab", "tokens": ["Dort", "bl\u00fcht", "die", "Hel\u00b7den\u00b7blu\u00b7me", ",", "der", "kei\u00b7ne", "and\u00b7re", "gleich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PIAT", "PIS", "ADV", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Wohl sieht in stillen Thr\u00e4nen ihm lang die Tochter nach,", "tokens": ["Wohl", "sieht", "in", "stil\u00b7len", "Thr\u00e4\u00b7nen", "ihm", "lang", "die", "Toch\u00b7ter", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Als auf mit seinen Schaaren der k\u00fchne Vater brach;", "tokens": ["Als", "auf", "mit", "sei\u00b7nen", "Schaa\u00b7ren", "der", "k\u00fch\u00b7ne", "Va\u00b7ter", "brach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie Donnerruf und Gluthblick des Herrn durch Wolkenritze,", "tokens": ["Wie", "Don\u00b7ner\u00b7ruf", "und", "Gluth\u00b7blick", "des", "Herrn", "durch", "Wol\u00b7ken\u00b7rit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So hier aus Staubgew\u00f6lken Feldruf und Waffenblitze! \u2013", "tokens": ["So", "hier", "aus", "Staub\u00b7ge\u00b7w\u00f6l\u00b7ken", "Feld\u00b7ruf", "und", "Waf\u00b7fen\u00b7blit\u00b7ze", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Vor Nancy ward den Raben ein Festmahl aufgeschichtet,", "tokens": ["Vor", "Nan\u00b7cy", "ward", "den", "Ra\u00b7ben", "ein", "Fest\u00b7mahl", "auf\u00b7ge\u00b7schich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da ging ins Gericht der Herzog, da hat er blutig gerichtet,", "tokens": ["Da", "ging", "ins", "Ge\u00b7richt", "der", "Her\u00b7zog", ",", "da", "hat", "er", "blu\u00b7tig", "ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NE", "$,", "ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+--+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da sanken Stamm und Aeste des Heldenbaums Burgund,", "tokens": ["Da", "san\u00b7ken", "Stamm", "und", "A\u00b7es\u00b7te", "des", "Hel\u00b7den\u00b7baums", "Bur\u00b7gund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Der Schweiz und Lothrings Blume, verwelkt zur selben Stund'.", "tokens": ["Der", "Schweiz", "und", "Loth\u00b7rings", "Blu\u00b7me", ",", "ver\u00b7welkt", "zur", "sel\u00b7ben", "Stund'", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "KON", "NE", "NN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Die erst gegen\u00fcber standen, durch Farb' und Zeichen feind,", "tokens": ["Die", "erst", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "stan\u00b7den", ",", "durch", "Fa\u00b7rb'", "und", "Zei\u00b7chen", "feind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Die liegen jetzt beisammen, durch Farb' und Zeichen vereint,", "tokens": ["Die", "lie\u00b7gen", "jetzt", "bei\u00b7sam\u00b7men", ",", "durch", "Fa\u00b7rb'", "und", "Zei\u00b7chen", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKVZ", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie K\u00f6n'ge in Purpurm\u00e4nteln von dampfendem Blute roth;", "tokens": ["Wie", "K\u00f6n'\u00b7ge", "in", "Pur\u00b7pur\u00b7m\u00e4n\u00b7teln", "von", "damp\u00b7fen\u00b7dem", "Blu\u00b7te", "roth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wi\u00dft ihr, wer so vers\u00f6hnte? \u2013 Der Friedensrichter Tod!", "tokens": ["Wi\u00dft", "ihr", ",", "wer", "so", "ver\u00b7s\u00f6hn\u00b7te", "?", "\u2013", "Der", "Frie\u00b7dens\u00b7rich\u00b7ter", "Tod", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "ADV", "VVFIN", "$.", "$(", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}}, "stanza.9": {"line.1": {"text": "Zu Nancy liegt im Dome ein Leichenstein ganz neu,", "tokens": ["Zu", "Nan\u00b7cy", "liegt", "im", "Do\u00b7me", "ein", "Lei\u00b7chen\u00b7stein", "ganz", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "APPRART", "NN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es lehnt, gleich einem Denkbild, ein blasses Weib dabei,", "tokens": ["Es", "lehnt", ",", "gleich", "ei\u00b7nem", "Denk\u00b7bild", ",", "ein", "blas\u00b7ses", "Weib", "da\u00b7bei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Aus Aug' und Antlitz dunkelt ein endlos tiefes Leid,", "tokens": ["Aus", "Aug'", "und", "Ant\u00b7litz", "dun\u00b7kelt", "ein", "end\u00b7los", "tie\u00b7fes", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Man sieht, da\u00df hier dem Vater die Tochter Thr\u00e4nen weiht.", "tokens": ["Man", "sieht", ",", "da\u00df", "hier", "dem", "Va\u00b7ter", "die", "Toch\u00b7ter", "Thr\u00e4\u00b7nen", "weiht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Zu Nancy an dem Grabe da ist viel Volk zu schau'n,", "tokens": ["Zu", "Nan\u00b7cy", "an", "dem", "Gra\u00b7be", "da", "ist", "viel", "Volk", "zu", "schau'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "ADV", "VAFIN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gef\u00fchrt an Schmerzensbanden aus nah' und fernen Gau'n.", "tokens": ["Ge\u00b7f\u00fchrt", "an", "Schmer\u00b7zens\u00b7ban\u00b7den", "aus", "nah'", "und", "fer\u00b7nen", "Gau'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbre