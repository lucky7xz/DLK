{"dta.poem.10128": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Sch\u00f6pfer aus dem Gesch\u00f6pf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie wir das Licht an sich nicht sonder C\u00f6rper sehn;", "tokens": ["Wie", "wir", "das", "Licht", "an", "sich", "nicht", "son\u00b7der", "C\u00f6r\u00b7per", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "PRF", "PTKNEG", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So kann man, von dem ew\u2019gen Licht\u2019,", "tokens": ["So", "kann", "man", ",", "von", "dem", "ew'\u00b7gen", "Licht'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn von der Creatur der Vorwurff uns gebricht,", "tokens": ["Wenn", "von", "der", "Crea\u00b7tur", "der", "Vor\u00b7wurff", "uns", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "So wenig da\u00df-als was Es sey, verstehn.", "tokens": ["So", "we\u00b7nig", "da\u00df\u00b7als", "was", "Es", "sey", ",", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PWS", "PPER", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}