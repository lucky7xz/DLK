{"dta.poem.11998": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O ew'ger Mittelpunkt des Seyns und der Gedanken,", "tokens": ["O", "ew'\u00b7ger", "Mit\u00b7tel\u00b7punkt", "des", "Seyns", "und", "der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um den sie kreisen, und ihm k\u00f6nnen nicht entwanken!", "tokens": ["Um", "den", "sie", "krei\u00b7sen", ",", "und", "ihm", "k\u00f6n\u00b7nen", "nicht", "ent\u00b7wan\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PPER", "VVINF", "$,", "KON", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Anweisen wollen sie dir einen Raum und Ort,", "tokens": ["An\u00b7wei\u00b7sen", "wol\u00b7len", "sie", "dir", "ei\u00b7nen", "Raum", "und", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch du bist dort und hie, und bist nicht hie noch dort.", "tokens": ["Doch", "du", "bist", "dort", "und", "hie", ",", "und", "bist", "nicht", "hie", "noch", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "KON", "ADV", "$,", "KON", "VAFIN", "PTKNEG", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du bist der Punkt, aus dem sich Kreis auf Kreis ergie\u00dft,", "tokens": ["Du", "bist", "der", "Punkt", ",", "aus", "dem", "sich", "Kreis", "auf", "Kreis", "er\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PRF", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du bist der Punkt, der in sich alle Kreise schlie\u00dft.", "tokens": ["Du", "bist", "der", "Punkt", ",", "der", "in", "sich", "al\u00b7le", "Krei\u00b7se", "schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "PRF", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was ist der Kreis? ein Punkt der aus sich selber trat.", "tokens": ["Was", "ist", "der", "Kreis", "?", "ein", "Punkt", "der", "aus", "sich", "sel\u00b7ber", "trat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "ART", "NN", "ART", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ist der Punkt? ein Kreis der keinen Umfang hat.", "tokens": ["Was", "ist", "der", "Punkt", "?", "ein", "Kreis", "der", "kei\u00b7nen", "Um\u00b7fang", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "ART", "NN", "ART", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Darum bist du der Punkt, denn du bist umfanglos,", "tokens": ["Da\u00b7rum", "bist", "du", "der", "Punkt", ",", "denn", "du", "bist", "um\u00b7fang\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ART", "NN", "$,", "KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bist der Kreis, denn du umf\u00e4ngest Klein und Gro\u00df.", "tokens": ["Und", "bist", "der", "Kreis", ",", "denn", "du", "um\u00b7f\u00e4n\u00b7gest", "Klein", "und", "Gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "NE", "KON", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du bist der Punkt im All, und bist der Punkt in allen,", "tokens": ["Du", "bist", "der", "Punkt", "im", "All", ",", "und", "bist", "der", "Punkt", "in", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$,", "KON", "VAFIN", "ART", "NN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Lebenspunkt, der Licht- und Schwerpunkt unserm Wallen.", "tokens": ["Der", "Le\u00b7ben\u00b7spunkt", ",", "der", "Licht", "und", "Schwer\u00b7punkt", "un\u00b7serm", "Wal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "TRUNC", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Du bist in allen und die alle sind in dir,", "tokens": ["Du", "bist", "in", "al\u00b7len", "und", "die", "al\u00b7le", "sind", "in", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "KON", "ART", "PIS", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In allen f\u00fchl' ich dich, dich f\u00fchl' ich auch in mir.", "tokens": ["In", "al\u00b7len", "f\u00fchl'", "ich", "dich", ",", "dich", "f\u00fchl'", "ich", "auch", "in", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "PRF", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "La\u00df meinen Lebenspunkt nicht stocken, nicht erkranken", "tokens": ["La\u00df", "mei\u00b7nen", "Le\u00b7ben\u00b7spunkt", "nicht", "sto\u00b7cken", ",", "nicht", "er\u00b7kran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Seele Lichtpunkt, noch des Herzens Schwerpunkt wanken!", "tokens": ["Der", "See\u00b7le", "Licht\u00b7punkt", ",", "noch", "des", "Her\u00b7zens", "Schwer\u00b7punkt", "wan\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}