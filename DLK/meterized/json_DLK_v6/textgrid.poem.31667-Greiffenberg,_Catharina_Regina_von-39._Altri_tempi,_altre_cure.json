{"textgrid.poem.31667": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "39. Altri tempi, altre cure", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Fr\u00fcling sorget man / die Erden zu bes\u00e4en:", "tokens": ["Im", "Fr\u00fc\u00b7ling", "sor\u00b7get", "man", "/", "die", "Er\u00b7den", "zu", "be\u00b7s\u00e4\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "$(", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "im Sommer / wie man m\u00f6g / das nunmehr reiffe / m\u00e4hen.", "tokens": ["im", "Som\u00b7mer", "/", "wie", "man", "m\u00f6g", "/", "das", "nun\u00b7mehr", "reif\u00b7fe", "/", "m\u00e4\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$(", "PWAV", "PIS", "VMFIN", "$(", "PDS", "ADV", "VVFIN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer im Fr\u00fcling seiner Jugend Tugend auszus\u00e4en pflegt /", "tokens": ["Wer", "im", "Fr\u00fc\u00b7ling", "sei\u00b7ner", "Ju\u00b7gend", "Tu\u00b7gend", "aus\u00b7zu\u00b7s\u00e4\u00b7en", "pflegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "PPOSAT", "NN", "NN", "VVIZU", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "einen sch\u00f6nen Kranz / im Alter / von den Ehren Aehern tr\u00e4gt.", "tokens": ["ei\u00b7nen", "sch\u00f6\u00b7nen", "Kranz", "/", "im", "Al\u00b7ter", "/", "von", "den", "Eh\u00b7ren", "A\u00b7e\u00b7hern", "tr\u00e4