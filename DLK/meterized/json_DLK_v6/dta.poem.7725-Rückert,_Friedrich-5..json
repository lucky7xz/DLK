{"dta.poem.7725": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Prinz! die Schmeichler sind gef\u00e4hrlicher als Raben,", "tokens": ["Mein", "Prinz", "!", "die", "Schmeich\u00b7ler", "sind", "ge\u00b7f\u00e4hr\u00b7li\u00b7cher", "als", "Ra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ART", "NN", "VAFIN", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die pflegen Todten nur die Augen auszugraben,", "tokens": ["Die", "pfle\u00b7gen", "Tod\u00b7ten", "nur", "die", "Au\u00b7gen", "aus\u00b7zu\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Inde\u00df der Schmeichler sie dem Lebenden entwendet,", "tokens": ["In\u00b7de\u00df", "der", "Schmeich\u00b7ler", "sie", "dem", "Le\u00b7ben\u00b7den", "ent\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und den scharfsichtigsten mit falschen K\u00fcnsten blendet.", "tokens": ["Und", "den", "scharf\u00b7sich\u00b7tigs\u00b7ten", "mit", "fal\u00b7schen", "K\u00fcns\u00b7ten", "blen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Wer in der Jugend so hat das Gesicht verloren,", "tokens": ["Wer", "in", "der", "Ju\u00b7gend", "so", "hat", "das", "Ge\u00b7sicht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erlangts nie mehr, und bleibt als sei er blind geboren.", "tokens": ["Er\u00b7langts", "nie", "mehr", ",", "und", "bleibt", "als", "sei", "er", "blind", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "KON", "VVFIN", "KOKOM", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}