{"textgrid.poem.31691": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Morgenstern gl\u00e4nzt", "tokens": ["Wenn", "der", "Mor\u00b7gens\u00b7tern", "gl\u00e4nzt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und das Morgenrot gl\u00fcht:", "tokens": ["Und", "das", "Mor\u00b7gen\u00b7rot", "gl\u00fcht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da wei\u00df ich, wer singend", "tokens": ["Da", "wei\u00df", "ich", ",", "wer", "sin\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ins Tal hinabzieht.", "tokens": ["Ins", "Tal", "hin\u00b7ab\u00b7zieht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Da wei\u00df ich, wer reitet", "tokens": ["Da", "wei\u00df", "ich", ",", "wer", "rei\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Zum frischgr\u00fcnen Grund,", "tokens": ["Zum", "frischgr\u00fc\u00b7nen", "Grund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Da wei\u00df ich, wer k\u00fc\u00dft", "tokens": ["Da", "wei\u00df", "ich", ",", "wer", "k\u00fc\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Einen frischroten Mund!", "tokens": ["Ei\u00b7nen", "frisc\u00b7hro\u00b7ten", "Mund", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Du Herzallerliebste,", "tokens": ["Du", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Du lustiges Kind,", "tokens": ["Du", "lus\u00b7ti\u00b7ges", "Kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mach auf deine H\u00fctte,", "tokens": ["Mach", "auf", "dei\u00b7ne", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "La\u00df ein mich geschwind!", "tokens": ["La\u00df", "ein", "mich", "ge\u00b7schwind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Deiner Augen Gefunkel,", "tokens": ["Dei\u00b7ner", "Au\u00b7gen", "Ge\u00b7fun\u00b7kel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Deiner Locken Geroll", "tokens": ["Dei\u00b7ner", "Lo\u00b7cken", "Ge\u00b7roll"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Macht Dumme verst\u00e4ndig,", "tokens": ["Macht", "Dum\u00b7me", "ver\u00b7st\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Verst\u00e4ndige toll!", "tokens": ["Ver\u00b7st\u00e4n\u00b7di\u00b7ge", "toll", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "F\u00fcr dich schl\u00e4gt mein Herz!", "tokens": ["F\u00fcr", "dich", "schl\u00e4gt", "mein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "F\u00fcr dich blitzt mein Schwert!", "tokens": ["F\u00fcr", "dich", "blitzt", "mein", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "F\u00fcr dich la\u00df ich tanzen", "tokens": ["F\u00fcr", "dich", "la\u00df", "ich", "tan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Mein nu\u00dfbraunes Pferd!", "tokens": ["Mein", "nu\u00df\u00b7brau\u00b7nes", "Pferd", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "F\u00fcr dich litt ich willig", "tokens": ["F\u00fcr", "dich", "litt", "ich", "wil\u00b7lig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Spott, Schande und Not!", "tokens": ["Spott", ",", "Schan\u00b7de", "und", "Not", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "F\u00fcr dich lief ich barfu\u00df", "tokens": ["F\u00fcr", "dich", "lief", "ich", "bar\u00b7fu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Durch H\u00f6lle und Tod!", "tokens": ["Durch", "H\u00f6l\u00b7le", "und", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Drum auf deine H\u00fctte!", "tokens": ["Drum", "auf", "dei\u00b7ne", "H\u00fct\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "La\u00df wallen dein Haar!", "tokens": ["La\u00df", "wal\u00b7len", "dein", "Haar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "La\u00df leuchten dein Auge,", "tokens": ["La\u00df", "leuch\u00b7ten", "dein", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Dein Auge so klar!", "tokens": ["Dein", "Au\u00b7ge", "so", "klar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ich pre\u00df an die Brust dich,", "tokens": ["Ich", "pre\u00df", "an", "die", "Brust", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ich schwinge den Stahl \u2013", "tokens": ["Ich", "schwin\u00b7ge", "den", "Stahl", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Vielleicht da\u00df ich k\u00fc\u00df dich", "tokens": ["Viel\u00b7leicht", "da\u00df", "ich", "k\u00fc\u00df", "dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Zum letzten Mal.", "tokens": ["Zum", "letz\u00b7ten", "Mal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}