{"textgrid.poem.57888": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich ging zum Konzerte, es sollten mein Ohr", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging zum Konzerte, es sollten mein Ohr", "tokens": ["Ich", "ging", "zum", "Kon\u00b7zer\u00b7te", ",", "es", "soll\u00b7ten", "mein", "Ohr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "S\u00fc\u00df klingende T\u00f6ne umschweben,", "tokens": ["S\u00fc\u00df", "klin\u00b7gen\u00b7de", "T\u00f6\u00b7ne", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und trat zur Garderobe hin,", "tokens": ["Und", "trat", "zur", "Gar\u00b7de\u00b7ro\u00b7be", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Meinen \u00dcberrock abzugeben.", "tokens": ["Mei\u00b7nen", "\u00dc\u00b7ber\u00b7rock", "ab\u00b7zu\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIZU", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Donna, die mir den Rock abnahm,", "tokens": ["Die", "Don\u00b7na", ",", "die", "mir", "den", "Rock", "ab\u00b7nahm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-++-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Frug ich: \u00bbWas kostet die Sache?\u00ab", "tokens": ["Frug", "ich", ":", "\u00bb", "Was", "kos\u00b7tet", "die", "Sa\u00b7che", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "$.", "$(", "PWS", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "\u00bbdas steht ganz in Ihrem Belieben, mein Herr,\u00ab", "tokens": ["\u00bb", "das", "steht", "ganz", "in", "Ih\u00b7rem", "Be\u00b7lie\u00b7ben", ",", "mein", "Herr", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sprach sie mit holdem Gelache.", "tokens": ["Sprach", "sie", "mit", "hol\u00b7dem", "Ge\u00b7la\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "---+--+-", "measure": "iambic.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Zehn Pfennige, dachte ich, ist wohl genug,", "tokens": ["Zehn", "Pfen\u00b7ni\u00b7ge", ",", "dach\u00b7te", "ich", ",", "ist", "wohl", "ge\u00b7nug", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und legte sie hin auf den Tresen;", "tokens": ["Und", "leg\u00b7te", "sie", "hin", "auf", "den", "Tre\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch schleunigst verfinsterte sich ihr Gesicht,", "tokens": ["Doch", "schleu\u00b7nigst", "ver\u00b7fins\u00b7ter\u00b7te", "sich", "ihr", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Das eben so freundlich gewesen.", "tokens": ["Das", "e\u00b7ben", "so", "freund\u00b7lich", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADJD", "VAPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Und eilig bem\u00fchte die Holde sich,", "tokens": ["Und", "ei\u00b7lig", "be\u00b7m\u00fch\u00b7te", "die", "Hol\u00b7de", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PRF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das Geld mir zur\u00fcckzuschieben:", "tokens": ["Das", "Geld", "mir", "zu\u00b7r\u00fcck\u00b7zu\u00b7schie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbnein, f\u00fcnfzehn Pfennig ist Taxe, mein Herr,", "tokens": ["\u00bb", "nein", ",", "f\u00fcnf\u00b7zehn", "Pfen\u00b7nig", "ist", "Ta\u00b7xe", ",", "mein", "Herr", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "CARD", "NN", "VAFIN", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was dr\u00fcber ist, steht im Belieben!\u00ab", "tokens": ["Was", "dr\u00fc\u00b7ber", "ist", ",", "steht", "im", "Be\u00b7lie\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PAV", "VAFIN", "$,", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}}}}