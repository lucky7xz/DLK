{"textgrid.poem.66243": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der deutsche Geist ist stark in Thaten und in Reden,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der deutsche Geist ist stark in Thaten und in Reden,", "tokens": ["Der", "deut\u00b7sche", "Geist", "ist", "stark", "in", "Tha\u00b7ten", "und", "in", "Re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Klumpen liebt er's Gold und nicht in d\u00fcnnen F\u00e4den.", "tokens": ["In", "Klum\u00b7pen", "liebt", "er's", "Gold", "und", "nicht", "in", "d\u00fcn\u00b7nen", "F\u00e4\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJA", "NN", "KON", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er sieht das Gro\u00dfe gern mit eignem Augenlicht,", "tokens": ["Er", "sieht", "das", "Gro\u00b7\u00dfe", "gern", "mit", "eig\u00b7nem", "Au\u00b7gen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch Galliens Lorgnette nicht.", "tokens": ["Durch", "Gal\u00b7li\u00b7ens", "Lorg\u00b7net\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}