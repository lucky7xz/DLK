{"textgrid.poem.33434": {"metadata": {"author": {"name": "Novalis", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn alle untreu werden,", "tokens": ["Wenn", "al\u00b7le", "un\u00b7treu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So bleib' ich dir doch treu;", "tokens": ["So", "bleib'", "ich", "dir", "doch", "treu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Dankbarkeit auf Erden", "tokens": ["Da\u00df", "Dank\u00b7bar\u00b7keit", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht ausgestorben sey.", "tokens": ["Nicht", "aus\u00b7ge\u00b7stor\u00b7ben", "sey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr mich umfing dich Leiden,", "tokens": ["F\u00fcr", "mich", "um\u00b7fing", "dich", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vergingst f\u00fcr mich in Schmerz;", "tokens": ["Ver\u00b7gingst", "f\u00fcr", "mich", "in", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drum geb' ich dir mit Freuden", "tokens": ["Drum", "geb'", "ich", "dir", "mit", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf ewig dieses Herz.", "tokens": ["Auf", "e\u00b7wig", "die\u00b7ses", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Oft mu\u00df ich bitter weinen,", "tokens": ["Oft", "mu\u00df", "ich", "bit\u00b7ter", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df du gestorben bist,", "tokens": ["Da\u00df", "du", "ge\u00b7stor\u00b7ben", "bist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mancher von den Deinen", "tokens": ["Und", "man\u00b7cher", "von", "den", "Dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "APPR", "ART", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich lebenslang vergi\u00dft.", "tokens": ["Dich", "le\u00b7bens\u00b7lang", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von Liebe nur durchdrungen", "tokens": ["Von", "Lie\u00b7be", "nur", "durch\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hast du so viel gethan,", "tokens": ["Hast", "du", "so", "viel", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und doch bist du verklungen,", "tokens": ["Und", "doch", "bist", "du", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und keiner denkt daran.", "tokens": ["Und", "kei\u00b7ner", "denkt", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du stehst voll treuer Liebe", "tokens": ["Du", "stehst", "voll", "treu\u00b7er", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch immer jedem bey,", "tokens": ["Noch", "im\u00b7mer", "je\u00b7dem", "bey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn dir keiner bliebe,", "tokens": ["Und", "wenn", "dir", "kei\u00b7ner", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So bleibst du dennoch treu;", "tokens": ["So", "bleibst", "du", "den\u00b7noch", "treu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die treuste Liebe sieget,", "tokens": ["Die", "treus\u00b7te", "Lie\u00b7be", "sie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Am Ende f\u00fchlt man sie,", "tokens": ["Am", "En\u00b7de", "f\u00fchlt", "man", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Weint bitterlich und schmieget", "tokens": ["Weint", "bit\u00b7ter\u00b7lich", "und", "schmie\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sich kindlich an dein Knie.", "tokens": ["Sich", "kind\u00b7lich", "an", "dein", "Knie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich habe dich empfunden,", "tokens": ["Ich", "ha\u00b7be", "dich", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O! lasse nicht von mir;", "tokens": ["O", "!", "las\u00b7se", "nicht", "von", "mir", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df innig mich verbunden", "tokens": ["La\u00df", "in\u00b7nig", "mich", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf ewig seyn mit dir.", "tokens": ["Auf", "e\u00b7wig", "seyn", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VAINF", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Einst schauen meine Br\u00fcder", "tokens": ["Einst", "schau\u00b7en", "mei\u00b7ne", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auch wieder himmelw\u00e4rts,", "tokens": ["Auch", "wie\u00b7der", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.7": {"text": "Und sinken liebend nieder,", "tokens": ["Und", "sin\u00b7ken", "lie\u00b7bend", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und fallen dir ans Herz.", "tokens": ["Und", "fal\u00b7len", "dir", "ans", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}