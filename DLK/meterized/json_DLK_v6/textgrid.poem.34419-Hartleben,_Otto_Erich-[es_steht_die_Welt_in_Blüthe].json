{"textgrid.poem.34419": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[es steht die Welt in Bl\u00fcthe]", "genre": "verse", "period": "N.A.", "pub_year": 1885, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es steht die Welt in Bl\u00fcthe,", "tokens": ["Es", "steht", "die", "Welt", "in", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "in Bl\u00fcthe steht dein Herz!", "tokens": ["in", "Bl\u00fc\u00b7the", "steht", "dein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie nun der Sturm auch w\u00fcthe,", "tokens": ["Wie", "nun", "der", "Sturm", "auch", "w\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "es schmolz der starre Schmerz.", "tokens": ["es", "schmolz", "der", "star\u00b7re", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Denn das ist Lenzes St\u00fcrmen,", "tokens": ["Denn", "das", "ist", "Len\u00b7zes", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "das ist des Lenzes Wuth:", "tokens": ["das", "ist", "des", "Len\u00b7zes", "Wuth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "der r\u00fcttelt an den Th\u00fcrmen,", "tokens": ["der", "r\u00fct\u00b7telt", "an", "den", "Th\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "der peitscht das Meer zur Fluth,", "tokens": ["der", "peitscht", "das", "Meer", "zur", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "der zieht durch aller Seelen", "tokens": ["der", "zieht", "durch", "al\u00b7ler", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und weht in aller Brust \u2013", "tokens": ["und", "weht", "in", "al\u00b7ler", "Brust", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "bis ihm aus tausend Kehlen", "tokens": ["bis", "ihm", "aus", "tau\u00b7send", "Keh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "die Erde jauchzt in Lust!", "tokens": ["die", "Er\u00b7de", "jauchzt", "in", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}