{"textgrid.poem.31772": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Schlu\u00dflied", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heller ward es im Osten.", "tokens": ["Hel\u00b7ler", "ward", "es", "im", "Os\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Da machte sich auf der Morgenwind,", "tokens": ["Da", "mach\u00b7te", "sich", "auf", "der", "Mor\u00b7gen\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vom Schlummer zu wecken", "tokens": ["Vom", "Schlum\u00b7mer", "zu", "we\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Des Fr\u00fchlings lieblichste Kinder:", "tokens": ["Des", "Fr\u00fch\u00b7lings", "lieb\u00b7lichs\u00b7te", "Kin\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Maililien und wilde", "tokens": ["Mai\u00b7li\u00b7li\u00b7en", "und", "wil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Zartrosa Rosen.", "tokens": ["Zar\u00b7tro\u00b7sa", "Ro\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "---+-", "measure": "unknown.measure.single"}, "line.7": {"text": "Sacht durchzog er das junge", "tokens": ["Sacht", "durch\u00b7zog", "er", "das", "jun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Gras und das gr\u00fcne", "tokens": ["Gras", "und", "das", "gr\u00fc\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Sprossende Korn, da\u00df die \u00c4hren", "tokens": ["Spros\u00b7sen\u00b7de", "Korn", ",", "da\u00df", "die", "\u00c4h\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.10": {"text": "Leis zu nicken begannen und weithin", "tokens": ["Leis", "zu", "ni\u00b7cken", "be\u00b7gan\u00b7nen", "und", "weit\u00b7hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "VVINF", "KON", "ADV"], "meter": "+-+--+--++", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Wogten hinab", "tokens": ["Wog\u00b7ten", "hin\u00b7ab"], "token_info": ["word", "word"], "pos": ["NN", "PTKVZ"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Zu der Felder Umz\u00e4unung.", "tokens": ["Zu", "der", "Fel\u00b7der", "Um\u00b7z\u00e4u\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Munter fuhr er einher", "tokens": ["Mun\u00b7ter", "fuhr", "er", "ein\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.14": {"text": "An der Seite des H\u00fcgels,", "tokens": ["An", "der", "Sei\u00b7te", "des", "H\u00fc\u00b7gels", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Jagte den Duft empor", "tokens": ["Jag\u00b7te", "den", "Duft", "em\u00b7por"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKVZ"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.16": {"text": "Von den Apfelbl\u00fcten und tanzte", "tokens": ["Von", "den", "Ap\u00b7fel\u00b7bl\u00fc\u00b7ten", "und", "tanz\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "VVFIN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "\u00dcber die G\u00e4rten hinweg", "tokens": ["\u00dc\u00b7ber", "die", "G\u00e4r\u00b7ten", "hin\u00b7weg"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APZR"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.18": {"text": "In den Forst dann,", "tokens": ["In", "den", "Forst", "dann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Spielend hier mit der wei\u00dflichen", "tokens": ["Spie\u00b7lend", "hier", "mit", "der", "wei\u00df\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPR", "ART", "ADJA"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.20": {"text": "Birke Gezweig, mit dem Wipfel", "tokens": ["Bir\u00b7ke", "Ge\u00b7zweig", ",", "mit", "dem", "Wip\u00b7fel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.21": {"text": "Der Tanne dort und des Buchbaums", "tokens": ["Der", "Tan\u00b7ne", "dort", "und", "des", "Buch\u00b7baums"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "KON", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Pr\u00e4chtiger Krone;", "tokens": ["Pr\u00e4ch\u00b7ti\u00b7ger", "Kro\u00b7ne", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "S\u00e4useln und Rauschen begann", "tokens": ["S\u00e4u\u00b7seln", "und", "Rau\u00b7schen", "be\u00b7gann"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.24": {"text": "Im Tal, auf den H\u00f6hen.", "tokens": ["Im", "Tal", ",", "auf", "den", "H\u00f6\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "Demanten gl\u00e4nzte der Tau", "tokens": ["De\u00b7man\u00b7ten", "gl\u00e4nz\u00b7te", "der", "Tau"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.26": {"text": "Im wachsenden Licht;", "tokens": ["Im", "wach\u00b7sen\u00b7den", "Licht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.27": {"text": "Aus Bl\u00e4ttern und Kelchen", "tokens": ["Aus", "Bl\u00e4t\u00b7tern", "und", "Kel\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "Rollt er, und Leben entsteht", "tokens": ["Rollt", "er", ",", "und", "Le\u00b7ben", "ent\u00b7steht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "NN", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.29": {"text": "Und Bewegung jetzt, \u00fcberall, \u00fcberall!", "tokens": ["Und", "Be\u00b7we\u00b7gung", "jetzt", ",", "\u00fc\u00b7be\u00b7rall", ",", "\u00fc\u00b7be\u00b7rall", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "ADV", "$,", "ADV", "$,", "ADV", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Tr\u00e4umrisch erhebt ihr K\u00f6pfchen", "tokens": ["Tr\u00e4um\u00b7risch", "er\u00b7hebt", "ihr", "K\u00f6pf\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Taube; es springt", "tokens": ["Die", "Tau\u00b7be", ";", "es", "springt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Von Ast zu Ast die schwarzwei\u00dfe", "tokens": ["Von", "Ast", "zu", "Ast", "die", "schwarz\u00b7wei\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "ART", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Elster; die Falken erwachen", "tokens": ["Els\u00b7ter", ";", "die", "Fal\u00b7ken", "er\u00b7wa\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ART", "NN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Im Dickicht, und horchend", "tokens": ["Im", "Di\u00b7ckicht", ",", "und", "hor\u00b7chend"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "KON", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Reckt der Hirsch sich empor", "tokens": ["Reckt", "der", "Hirsch", "sich", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PRF", "PTKVZ"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "An dem sprudelnden Waldbach.", "tokens": ["An", "dem", "spru\u00b7deln\u00b7den", "Wald\u00b7bach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Vorbei die Stille der Nacht!", "tokens": ["Vor\u00b7bei", "die", "Stil\u00b7le", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Es erwachen", "tokens": ["Es", "er\u00b7wa\u00b7chen"], "token_info": ["word", "word"], "pos": ["PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Die Lieder in jeder Brust,", "tokens": ["Die", "Lie\u00b7der", "in", "je\u00b7der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So Natur zum Gesange gestimmt hat.", "tokens": ["So", "Na\u00b7tur", "zum", "Ge\u00b7san\u00b7ge", "ge\u00b7stimmt", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Ein Ruf jetzt \u2013 ein Schrei des Entz\u00fcckens!", "tokens": ["Ein", "Ruf", "jetzt", "\u2013", "ein", "Schrei", "des", "Ent\u00b7z\u00fc\u00b7ckens", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$(", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und hell zu der W\u00e4lder Gerausch", "tokens": ["Und", "hell", "zu", "der", "W\u00e4l\u00b7der", "Ge\u00b7rausch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Erschallt in melodischem Chor", "tokens": ["Er\u00b7schallt", "in", "me\u00b7lo\u00b7di\u00b7schem", "Chor"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Das Festlied aller Lebend'gen!", "tokens": ["Das", "Fest\u00b7lied", "al\u00b7ler", "Le\u00b7ben\u00b7d'\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Triumph! da\u00df du kamst,", "tokens": ["Tri\u00b7umph", "!", "da\u00df", "du", "kamst", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "O strahlende Sonne; ein neuer", "tokens": ["O", "strah\u00b7len\u00b7de", "Son\u00b7ne", ";", "ein", "neu\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "ART", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Tag geht auf den V\u00f6lkern der Erde.", "tokens": ["Tag", "geht", "auf", "den", "V\u00f6l\u00b7kern", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Mag alles froh dich begr\u00fc\u00dfen,", "tokens": ["Mag", "al\u00b7les", "froh", "dich", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mag alles liebend dir nachschaun,", "tokens": ["Mag", "al\u00b7les", "lie\u00b7bend", "dir", "nach\u00b7schaun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn wundervoll", "tokens": ["Wenn", "wun\u00b7der\u00b7voll"], "token_info": ["word", "word"], "pos": ["KOUS", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Nach vollbrachtem Lauf", "tokens": ["Nach", "voll\u00b7brach\u00b7tem", "Lauf"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Du leuchtend wieder hinabsinkst.", "tokens": ["Du", "leuch\u00b7tend", "wie\u00b7der", "hin\u00b7ab\u00b7sinkst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "ADV", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}