{"dta.poem.5361": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erbarm dich aller Menschen doch! vermehre F\u00e4higkeit und", "tokens": ["Er\u00b7barm", "dich", "al\u00b7ler", "Men\u00b7schen", "doch", "!", "ver\u00b7meh\u00b7re", "F\u00e4\u00b7hig\u00b7keit", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "PIAT", "NN", "ADV", "$.", "VVFIN", "NN", "KON"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Willen,", "tokens": ["Wil\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Da\u00df wir, zu deines Nahmens Ruhm, den grossen End-", "tokens": ["Da\u00df", "wir", ",", "zu", "dei\u00b7nes", "Nah\u00b7mens", "Ruhm", ",", "den", "gros\u00b7sen", "En\u00b7d"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "zweck hier erf\u00fcllen,", "tokens": ["zweck", "hier", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wozu du uns beschieden hast! gieb, da\u00df im Spiegel dei-", "tokens": ["Wo\u00b7zu", "du", "uns", "be\u00b7schie\u00b7den", "hast", "!", "gieb", ",", "da\u00df", "im", "Spie\u00b7gel", "dei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "VVPP", "VAFIN", "$.", "VVIMP", "$,", "KOUS", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "ner Wercke,", "tokens": ["ner", "Wer\u00b7cke", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Voll Lust, Bewunderung und Andacht, man deine wahre", "tokens": ["Voll", "Lust", ",", "Be\u00b7wun\u00b7de\u00b7rung", "und", "An\u00b7dacht", ",", "man", "dei\u00b7ne", "wah\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "NN", "KON", "NN", "$,", "PIS", "PPOSAT", "ADJA"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Gottheit mercke!", "tokens": ["Got\u00b7theit", "mer\u00b7cke", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+--", "measure": "dactylic.init"}, "line.9": {"text": "La\u00df uns im Creaturen-Spiegel hier, als in einem duncklen", "tokens": ["La\u00df", "uns", "im", "Crea\u00b7tu\u00b7ren\u00b7Spie\u00b7gel", "hier", ",", "als", "in", "ei\u00b7nem", "dunck\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "ADV", "$,", "KOUS", "APPR", "ART", "ADJA"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Wort,", "tokens": ["Wort", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Dein ", "tokens": ["Dein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "nebst den Engeln, dort,", "tokens": ["nebst", "den", "En\u00b7geln", ",", "dort", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Nach abgelegter Sterblichkeit, in jenen seel\u2019gen Himmels-", "tokens": ["Nach", "ab\u00b7ge\u00b7leg\u00b7ter", "Sterb\u00b7lich\u00b7keit", ",", "in", "je\u00b7nen", "seel'\u00b7gen", "Him\u00b7mels"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "PDAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "H\u00f6hen,", "tokens": ["H\u00f6\u00b7hen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Dich, in selbst\u00e4nd\u2019ger Majest\u00e4t, in einem ewig-seel\u2019gen", "tokens": ["Dich", ",", "in", "selb\u00b7st\u00e4n\u00b7d'\u00b7ger", "Ma\u00b7jes\u00b7t\u00e4t", ",", "in", "ei\u00b7nem", "e\u00b7wig\u00b7seel'\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Licht,", "tokens": ["Licht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Und der Drey-Einigkeit Geheimni\u00df, von Angesicht zu An-", "tokens": ["Und", "der", "Drey\u00b7Ei\u00b7nig\u00b7keit", "Ge\u00b7heim\u00b7ni\u00df", ",", "von", "An\u00b7ge\u00b7sicht", "zu", "An"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "$,", "APPR", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "gesicht,", "tokens": ["ge\u00b7sicht", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "In unaussprechlich-heller Klarheit, ununterbrochen, ewig", "tokens": ["In", "un\u00b7aus\u00b7sprech\u00b7lich\u00b7hel\u00b7ler", "Klar\u00b7heit", ",", "un\u00b7un\u00b7ter\u00b7bro\u00b7chen", ",", "e\u00b7wig"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "sehen!", "tokens": ["se\u00b7hen", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}