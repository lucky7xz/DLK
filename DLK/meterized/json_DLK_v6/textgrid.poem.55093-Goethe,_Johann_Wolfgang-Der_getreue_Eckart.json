{"textgrid.poem.55093": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Der getreue Eckart", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbo w\u00e4ren wir weiter, o w\u00e4r ich zu Haus!", "tokens": ["\u00bb", "o", "w\u00e4\u00b7ren", "wir", "wei\u00b7ter", ",", "o", "w\u00e4r", "ich", "zu", "Haus", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VAFIN", "PPER", "ADV", "$,", "FM", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie kommen, da kommt schon der n\u00e4chtliche Graus;", "tokens": ["Sie", "kom\u00b7men", ",", "da", "kommt", "schon", "der", "n\u00e4cht\u00b7li\u00b7che", "Graus", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie sind's, die unholdigen Schwestern.", "tokens": ["Sie", "sin\u00b7d's", ",", "die", "un\u00b7hol\u00b7di\u00b7gen", "Schwes\u00b7tern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie streifen heran, und sie finden uns hier,", "tokens": ["Sie", "strei\u00b7fen", "he\u00b7ran", ",", "und", "sie", "fin\u00b7den", "uns", "hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Sie trinken das m\u00fchsam geholte, das Bier,", "tokens": ["Sie", "trin\u00b7ken", "das", "m\u00fch\u00b7sam", "ge\u00b7hol\u00b7te", ",", "das", "Bier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und lassen nur leer uns die Kr\u00fcge.\u00ab", "tokens": ["Und", "las\u00b7sen", "nur", "leer", "uns", "die", "Kr\u00fc\u00b7ge", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PPER", "ART", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "So sprechen die Kinder und dr\u00fccken sich schnell;", "tokens": ["So", "spre\u00b7chen", "die", "Kin\u00b7der", "und", "dr\u00fc\u00b7cken", "sich", "schnell", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da zeigt sich vor ihnen ein alter Gesell:", "tokens": ["Da", "zeigt", "sich", "vor", "ih\u00b7nen", "ein", "al\u00b7ter", "Ge\u00b7sell", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "\u00bbnur stille, Kind! Kinderlein, stille!", "tokens": ["\u00bb", "nur", "stil\u00b7le", ",", "Kind", "!", "Kin\u00b7der\u00b7lein", ",", "stil\u00b7le", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "$,", "NN", "$.", "NN", "$,", "VVFIN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Hulden, sie kommen von durstiger Jagd,", "tokens": ["Die", "Hul\u00b7den", ",", "sie", "kom\u00b7men", "von", "durs\u00b7ti\u00b7ger", "Jagd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Und la\u00dft ihr sie trinken, wie's jeder behagt,", "tokens": ["Und", "la\u00dft", "ihr", "sie", "trin\u00b7ken", ",", "wie's", "je\u00b7der", "be\u00b7hagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PPER", "VVINF", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Dann sind sie euch hold, die Unholden.\u00ab", "tokens": ["Dann", "sind", "sie", "euch", "hold", ",", "die", "Un\u00b7hol\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADJD", "$,", "ART", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Gesagt so geschehn! und da naht sich der Graus", "tokens": ["Ge\u00b7sagt", "so", "ge\u00b7schehn", "!", "und", "da", "naht", "sich", "der", "Graus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "ADV", "VVPP", "$.", "KON", "ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und siehet so grau und so schattenhaft aus,", "tokens": ["Und", "sie\u00b7het", "so", "grau", "und", "so", "schat\u00b7ten\u00b7haft", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "KON", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Doch schl\u00fcrft es und schlampft es aufs beste.", "tokens": ["Doch", "schl\u00fcrft", "es", "und", "schlampft", "es", "aufs", "bes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "APPRART", "ADJA", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das Bier ist verschwunden, die Kr\u00fcge sind leer;", "tokens": ["Das", "Bier", "ist", "ver\u00b7schwun\u00b7den", ",", "die", "Kr\u00fc\u00b7ge", "sind", "leer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Nun saust es und braust es, das w\u00fctige Heer,", "tokens": ["Nun", "saust", "es", "und", "braust", "es", ",", "das", "w\u00fc\u00b7ti\u00b7ge", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Ins weite Getal und Gebirge.", "tokens": ["Ins", "wei\u00b7te", "Ge\u00b7tal", "und", "Ge\u00b7bir\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Die Kinderlein \u00e4ngstlich gen Hause so schnell,", "tokens": ["Die", "Kin\u00b7der\u00b7lein", "\u00e4ngst\u00b7lich", "gen", "Hau\u00b7se", "so", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Gesellt sich zu ihnen der fromme Gesell:", "tokens": ["Ge\u00b7sellt", "sich", "zu", "ih\u00b7nen", "der", "from\u00b7me", "Ge\u00b7sell", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "\u00bbihr P\u00fcppchen, nur seid mir nicht traurig.\u00ab", "tokens": ["\u00bb", "ihr", "P\u00fcpp\u00b7chen", ",", "nur", "seid", "mir", "nicht", "trau\u00b7rig", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "ADJD", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "\u00bbwir kriegen nun Schelten und Streich' bis aufs Blut.\u00ab", "tokens": ["\u00bb", "wir", "krie\u00b7gen", "nun", "Schel\u00b7ten", "und", "Streich'", "bis", "aufs", "Blut", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "NN", "KON", "NN", "APPR", "APPRART", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "\u00bbnein, keineswegs, alles geht herrlich und gut,", "tokens": ["\u00bb", "nein", ",", "kei\u00b7nes\u00b7wegs", ",", "al\u00b7les", "geht", "herr\u00b7lich", "und", "gut", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "ADV", "$,", "PIS", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Nur schweiget und horchet wie M\u00e4uslein.", "tokens": ["Nur", "schwei\u00b7get", "und", "hor\u00b7chet", "wie", "M\u00e4us\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+--+-++-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und der es euch anr\u00e4t und der es befiehlt,", "tokens": ["Und", "der", "es", "euch", "an\u00b7r\u00e4t", "und", "der", "es", "be\u00b7fiehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "PPER", "VVFIN", "KON", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Er ist es, der gern mit den Kindelein spielt,", "tokens": ["Er", "ist", "es", ",", "der", "gern", "mit", "den", "Kin\u00b7de\u00b7lein", "spielt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der alte Getreue, der Eckart.", "tokens": ["Der", "al\u00b7te", "Ge\u00b7treu\u00b7e", ",", "der", "E\u00b7ckart", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vom Wundermann hat man euch immer erz\u00e4hlt,", "tokens": ["Vom", "Wun\u00b7der\u00b7mann", "hat", "man", "euch", "im\u00b7mer", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Nur hat die Best\u00e4tigung jedem gefehlt,", "tokens": ["Nur", "hat", "die", "Be\u00b7st\u00e4\u00b7ti\u00b7gung", "je\u00b7dem", "ge\u00b7fehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PIS", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Die habt ihr nun k\u00f6stlich in H\u00e4nden.\u00ab", "tokens": ["Die", "habt", "ihr", "nun", "k\u00f6st\u00b7lich", "in", "H\u00e4n\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Sie kommen nach Hause, sie setzen den Krug", "tokens": ["Sie", "kom\u00b7men", "nach", "Hau\u00b7se", ",", "sie", "set\u00b7zen", "den", "Krug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein jedes den Eltern bescheiden genug", "tokens": ["Ein", "je\u00b7des", "den", "El\u00b7tern", "be\u00b7schei\u00b7den", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "ART", "NN", "ADJD", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und harren der Schl\u00e4g und der Schelten.", "tokens": ["Und", "har\u00b7ren", "der", "Schl\u00e4g", "und", "der", "Schel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Doch siehe, man kostet: ein herrliches Bier!", "tokens": ["Doch", "sie\u00b7he", ",", "man", "kos\u00b7tet", ":", "ein", "herr\u00b7li\u00b7ches", "Bier", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "PIS", "VVFIN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Man trinkt in die Runde schon dreimal und vier,", "tokens": ["Man", "trinkt", "in", "die", "Run\u00b7de", "schon", "drei\u00b7mal", "und", "vier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "ADV", "ADV", "KON", "CARD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und noch nimmt der Krug nicht ein Ende.", "tokens": ["Und", "noch", "nimmt", "der", "Krug", "nicht", "ein", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PTKNEG", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Das Wunder, es dauert zum morgenden Tag.", "tokens": ["Das", "Wun\u00b7der", ",", "es", "dau\u00b7ert", "zum", "mor\u00b7gen\u00b7den", "Tag", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Doch fraget, wer immer zu fragen vermag:", "tokens": ["Doch", "fra\u00b7get", ",", "wer", "im\u00b7mer", "zu", "fra\u00b7gen", "ver\u00b7mag", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "\u00bbwie ist's mit den Kr\u00fcgen ergangen?\u00ab", "tokens": ["\u00bb", "wie", "ist's", "mit", "den", "Kr\u00fc\u00b7gen", "er\u00b7gan\u00b7gen", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOKOM", "VAFIN", "APPR", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die M\u00e4uslein, sie l\u00e4cheln, im stillen ergetzt;", "tokens": ["Die", "M\u00e4us\u00b7lein", ",", "sie", "l\u00e4\u00b7cheln", ",", "im", "stil\u00b7len", "er\u00b7getzt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "$,", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Sie stammeln und stottern und schwatzen zuletzt,", "tokens": ["Sie", "stam\u00b7meln", "und", "stot\u00b7tern", "und", "schwat\u00b7zen", "zu\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVINF", "KON", "VVFIN", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und gleich sind vertrocknet die Kr\u00fcge.", "tokens": ["Und", "gleich", "sind", "ver\u00b7trock\u00b7net", "die", "Kr\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Und wenn euch, ihr Kinder, mit treuem Gesicht", "tokens": ["Und", "wenn", "euch", ",", "ihr", "Kin\u00b7der", ",", "mit", "treu\u00b7em", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein Vater, ein Lehrer, ein Aldermann spricht,", "tokens": ["Ein", "Va\u00b7ter", ",", "ein", "Leh\u00b7rer", ",", "ein", "Al\u00b7der\u00b7mann", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "So horchet und folget ihm p\u00fcnktlich!", "tokens": ["So", "hor\u00b7chet", "und", "