{"textgrid.poem.41466": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Zweite Erz\u00e4hlung", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die arme Frau erblasset, seufzt und schweigt;", "tokens": ["Die", "ar\u00b7me", "Frau", "er\u00b7blas\u00b7set", ",", "seufzt", "und", "schweigt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der frohe Mann bewundert ihre Stille.", "tokens": ["Der", "fro\u00b7he", "Mann", "be\u00b7wun\u00b7dert", "ih\u00b7re", "Stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Allein ihr Aug', ihr wildes Auge, zeigt,", "tokens": ["Al\u00b7lein", "ihr", "Aug'", ",", "ihr", "wil\u00b7des", "Au\u00b7ge", ",", "zeigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df nichts, als Zorn, ihr ganzes Herz erf\u00fclle.", "tokens": ["Da\u00df", "nichts", ",", "als", "Zorn", ",", "ihr", "gan\u00b7zes", "Herz", "er\u00b7f\u00fcl\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "KOUS", "NN", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Grieche schreibt, das weibliche Geschlecht", "tokens": ["Ein", "Grie\u00b7che", "schreibt", ",", "das", "weib\u00b7li\u00b7che", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Empfinde mehr, als wir, bei jedem Triebe,", "tokens": ["Emp\u00b7fin\u00b7de", "mehr", ",", "als", "wir", ",", "bei", "je\u00b7dem", "Trie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "PPER", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und es besitz' ein angebornes Recht", "tokens": ["Und", "es", "be\u00b7sitz'", "ein", "an\u00b7ge\u00b7bor\u00b7nes", "Recht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zur Obermacht im Ha\u00df und in der Liebe.", "tokens": ["Zur", "O\u00b7ber\u00b7macht", "im", "Ha\u00df", "und", "in", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wer aber kennt die Sch\u00f6nen alter Zeit?", "tokens": ["Wer", "a\u00b7ber", "kennt", "die", "Sch\u00f6\u00b7nen", "al\u00b7ter", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "O w\u00fc\u00dften wir nur unsre g'nug zu kennen!", "tokens": ["O", "w\u00fc\u00df\u00b7ten", "wir", "nur", "uns\u00b7re", "g'\u00b7nug", "zu", "ken\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--++--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Wie? Ist denn nicht auch die Empfindlichkeit", "tokens": ["Wie", "?", "Ist", "denn", "nicht", "auch", "die", "Emp\u00b7find\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "VAFIN", "ADV", "PTKNEG", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "An Zeit und Ort oft vortheilhaft zu nennen?", "tokens": ["An", "Zeit", "und", "Ort", "oft", "vor\u00b7theil\u00b7haft", "zu", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Sie schweigt, und geht in ihr Gemach zur\u00fcck.", "tokens": ["Sie", "schweigt", ",", "und", "geht", "in", "ihr", "Ge\u00b7mach", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dort l\u00e4\u00dft ihr Leid die ersten Z\u00e4hren flie\u00dfen,", "tokens": ["Dort", "l\u00e4\u00dft", "ihr", "Leid", "die", "ers\u00b7ten", "Z\u00e4h\u00b7ren", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Hannchen folgt, und weissagt ihr das Gl\u00fcck,", "tokens": ["Ihr", "Hann\u00b7chen", "folgt", ",", "und", "weis\u00b7sagt", "ihr", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Rache Lust in Kurzem zu genie\u00dfen.", "tokens": ["Der", "Ra\u00b7che", "Lust", "in", "Kur\u00b7zem", "zu", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und sie versetzt: Mein Mann verfahre nur", "tokens": ["Und", "sie", "ver\u00b7setzt", ":", "Mein", "Mann", "ver\u00b7fah\u00b7re", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVPP", "$.", "PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nach jedem Punkt der \u00fcbereilten Wette!", "tokens": ["Nach", "je\u00b7dem", "Punkt", "der", "\u00fc\u00b7be\u00b7reil\u00b7ten", "Wet\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich r\u00e4che mich. So will es die Natur,", "tokens": ["Ich", "r\u00e4\u00b7che", "mich", ".", "So", "will", "es", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ADV", "VMFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Weil ich zugleich der Weiber Leumund rette.", "tokens": ["Weil", "ich", "zu\u00b7gleich", "der", "Wei\u00b7ber", "Leu\u00b7mund", "ret\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nichts \u00fcbertreff' auch jetzt die Frauenlist,", "tokens": ["Nichts", "\u00fc\u00b7ber\u00b7treff'", "auch", "jetzt", "die", "Frau\u00b7en\u00b7list", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Nichts meine Kunst, mich gl\u00fccklich zu verstellen,", "tokens": ["Nichts", "mei\u00b7ne", "Kunst", ",", "mich", "gl\u00fcck\u00b7lich", "zu", "ver\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und einem Herrn, der so unfehlbar ist,", "tokens": ["Und", "ei\u00b7nem", "Herrn", ",", "der", "so", "un\u00b7fehl\u00b7bar", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.12": {"text": "Die weitre Lust zum Wetten zu verg\u00e4llen!", "tokens": ["Die", "weit\u00b7re", "Lust", "zum", "Wet\u00b7ten", "zu", "ver\u00b7g\u00e4l\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie bildet sich, nach ihres Spiegels Rath,", "tokens": ["Sie", "bil\u00b7det", "sich", ",", "nach", "ih\u00b7res", "Spie\u00b7gels", "Rath", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den bl\u00f6den Blick, die traurigen Geberden,", "tokens": ["Den", "bl\u00f6\u00b7den", "Blick", ",", "die", "trau\u00b7ri\u00b7gen", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schleicht zum Gemahl, und sagt, die Missethat", "tokens": ["Schleicht", "zum", "Ge\u00b7mahl", ",", "und", "sagt", ",", "die", "Mis\u00b7se\u00b7that"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "KON", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sei ewig werth, vermaledeit zu werden,", "tokens": ["Sei", "e\u00b7wig", "werth", ",", "ver\u00b7ma\u00b7le\u00b7deit", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADJD", "$,", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und f\u00fcgt hinzu: Mich lehrt mein Eigensinn,", "tokens": ["Und", "f\u00fcgt", "hin\u00b7zu", ":", "Mich", "lehrt", "mein", "Ei\u00b7gen\u00b7sinn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie sehr auch ich der Even angeh\u00f6re.", "tokens": ["Wie", "sehr", "auch", "ich", "der", "E\u00b7ven", "an\u00b7ge\u00b7h\u00f6\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "PPER", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Verdamme mich, mein Richter; denn ich bin", "tokens": ["Ver\u00b7dam\u00b7me", "mich", ",", "mein", "Rich\u00b7ter", ";", "denn", "ich", "bin"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "KON", "PPER", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Frauen Schimpf, und keines Mannes Ehre.", "tokens": ["Der", "Frau\u00b7en", "Schimpf", ",", "und", "kei\u00b7nes", "Man\u00b7nes", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KON", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ich will daher, zur Tilgung meiner Schuld,", "tokens": ["Ich", "will", "da\u00b7her", ",", "zur", "Til\u00b7gung", "mei\u00b7ner", "Schuld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "$,", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die Weiber selbst, die ich besch\u00e4me, fliehen,", "tokens": ["Die", "Wei\u00b7ber", "selbst", ",", "die", "ich", "be\u00b7sch\u00e4\u00b7me", ",", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und auf ein Jahr, in einsamer Geduld,", "tokens": ["Und", "auf", "ein", "Jahr", ",", "in", "ein\u00b7sa\u00b7mer", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Mich deinem Arm und deinem Ku\u00df entziehen.", "tokens": ["Mich", "dei\u00b7nem", "Arm", "und", "dei\u00b7nem", "Ku\u00df", "ent\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}