{"textgrid.poem.24111": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Ritter Hahn und Bauer Enterich", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Haushahn, Herr von Stakelsteif,", "tokens": ["Der", "Haus\u00b7hahn", ",", "Herr", "von", "Sta\u00b7kel\u00b7steif", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit rotem Kamm und gr\u00fcnem Schweif,", "tokens": ["Mit", "ro\u00b7tem", "Kamm", "und", "gr\u00fc\u00b7nem", "Schweif", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erhob ein laut Gekr\u00e4he;", "tokens": ["Er\u00b7hob", "ein", "laut", "Ge\u00b7kr\u00e4\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zerbarst sich schier den langen Hals,", "tokens": ["Zer\u00b7barst", "sich", "schier", "den", "lan\u00b7gen", "Hals", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schrie, da\u00df er statt des D\u00fcngerwalls", "tokens": ["Schrie", ",", "da\u00df", "er", "statt", "des", "D\u00fcn\u00b7ger\u00b7walls"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Gern etwas Reinres s\u00e4he.", "tokens": ["Gern", "et\u00b7was", "Rein\u00b7res", "s\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbich trage Sporen,\u00ab sprach der Hahn,", "tokens": ["\u00bb", "ich", "tra\u00b7ge", "Spo\u00b7ren", ",", "\u00ab", "sprach", "der", "Hahn", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "NN", "$,", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbund seht doch mein Gefieder an!", "tokens": ["\u00bb", "und", "seht", "doch", "mein", "Ge\u00b7fie\u00b7der", "an", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ists nicht bewundernsw\u00fcrdig?", "tokens": ["Ists", "nicht", "be\u00b7wun\u00b7derns\u00b7w\u00fcr\u00b7dig", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich bin von adeligem Stamm,", "tokens": ["Ich", "bin", "von", "a\u00b7de\u00b7li\u00b7gem", "Stamm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein zackig aufgeschwollener Kamm", "tokens": ["Mein", "za\u00b7ckig", "auf\u00b7ge\u00b7schwol\u00b7le\u00b7ner", "Kamm"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Zeigt, da\u00df ich ritterb\u00fcrtig.\u00ab", "tokens": ["Zeigt", ",", "da\u00df", "ich", "rit\u00b7ter\u00b7b\u00fcr\u00b7tig", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbmit Eurer Gnaden Permittenz\u00ab", "tokens": ["\u00bb", "mit", "Eu\u00b7rer", "Gna\u00b7den", "Per\u00b7mit\u00b7tenz", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach drauf Herr Erpel Schwenkeschwenz,", "tokens": ["Sprach", "drauf", "Herr", "Er\u00b7pel", "Schwen\u00b7ke\u00b7schwenz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbich bin zwar nur ein Bauer,", "tokens": ["\u00bb", "ich", "bin", "zwar", "nur", "ein", "Bau\u00b7er", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jedoch, was Euch betrifft und Mist,", "tokens": ["Je\u00b7doch", ",", "was", "Euch", "be\u00b7tr\u00b7ifft", "und", "Mist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "PPER", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wei\u00df ich, was gut und dienlich ist,", "tokens": ["Wei\u00df", "ich", ",", "was", "gut", "und", "dien\u00b7lich", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Mist und Euch, genauer.", "tokens": ["Dem", "Mist", "und", "Euch", ",", "ge\u00b7nau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "KON", "PPER", "$,", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Gewi\u00df seid Ihr ein Edelmann", "tokens": ["Ge\u00b7wi\u00df", "seid", "Ihr", "ein", "E\u00b7del\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der seine Sporen tragen kann", "tokens": ["Der", "sei\u00b7ne", "Spo\u00b7ren", "tra\u00b7gen", "kann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seine Farben zeigen.", "tokens": ["Und", "sei\u00b7ne", "Far\u00b7ben", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Indessen: erst der Mist verleiht", "tokens": ["In\u00b7des\u00b7sen", ":", "erst", "der", "Mist", "ver\u00b7leiht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Euch Eure hohe Adligkeit;", "tokens": ["Euch", "Eu\u00b7re", "ho\u00b7he", "Ad\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor ihm sollt Ihr Euch neigen.", "tokens": ["Vor", "ihm", "sollt", "Ihr", "Euch", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Nur auf dem Mist seid Ihr Baron,", "tokens": ["Nur", "auf", "dem", "Mist", "seid", "Ihr", "Ba\u00b7ron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und nur der Mist ist Euer Thron;", "tokens": ["Und", "nur", "der", "Mist", "ist", "Eu\u00b7er", "Thron", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wollt Ihr den Mist verlassen,", "tokens": ["Wollt", "Ihr", "den", "Mist", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So wird Euch Heimweh nach dem Mist,", "tokens": ["So", "wird", "Euch", "Heim\u00b7weh", "nach", "dem", "Mist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Eurer Ahnen Hochschlo\u00df ist,", "tokens": ["Der", "Eu\u00b7rer", "Ah\u00b7nen", "Hoch\u00b7schlo\u00df", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mistheimweh wird Euch fassen.", "tokens": ["Mist\u00b7heim\u00b7weh", "wird", "Euch", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich bitt Euch, bleibt dem Mist getreu;", "tokens": ["Ich", "bitt", "Euch", ",", "bleibt", "dem", "Mist", "ge\u00b7treu", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist er auch nur verdautes Heu,", "tokens": ["Ist", "er", "auch", "nur", "ver\u00b7dau\u00b7tes", "Heu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist er doch weich und w\u00e4rmlich.\u00ab", "tokens": ["Ist", "er", "doch", "weich", "und", "w\u00e4rm\u00b7lich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da sch\u00fcttelte den gr\u00fcnen Schweif", "tokens": ["Da", "sch\u00fct\u00b7tel\u00b7te", "den", "gr\u00fc\u00b7nen", "Schweif"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der sch\u00f6ne Herr von Stakelsteif:", "tokens": ["Der", "sch\u00f6\u00b7ne", "Herr", "von", "Sta\u00b7kel\u00b7steif", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbdas Pack bleibt stets erb\u00e4rmlich.", "tokens": ["\u00bb", "das", "Pack", "bleibt", "stets", "er\u00b7b\u00e4rm\u00b7lich", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich wei\u00df, weshalb der Bauer fleht:", "tokens": ["Ich", "wei\u00df", ",", "we\u00b7shalb", "der", "Bau\u00b7er", "fleht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er braucht mich hier als Majest\u00e4t,", "tokens": ["Er", "braucht", "mich", "hier", "als", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich sein Sein begl\u00e4nze.", "tokens": ["Da\u00df", "ich", "sein", "Sein", "be\u00b7gl\u00e4n\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mon dieu, mon dieu, Kikerikih!", "tokens": ["Mon", "di\u00b7eu", ",", "mon", "di\u00b7eu", ",", "Ki\u00b7ke\u00b7ri\u00b7kih", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "FM.fr", "FM.fr", "$,", "NE", "$."], "meter": "+-+-+----+", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Es reibt die Aristokratie", "tokens": ["Es", "reibt", "die", "A\u00b7ris\u00b7to\u00b7kra\u00b7tie"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich auf f\u00fcr Schwenkeschwenze.\u00ab", "tokens": ["Sich", "auf", "f\u00fcr", "Schwen\u00b7ke\u00b7schwen\u00b7ze", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "APPR", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}