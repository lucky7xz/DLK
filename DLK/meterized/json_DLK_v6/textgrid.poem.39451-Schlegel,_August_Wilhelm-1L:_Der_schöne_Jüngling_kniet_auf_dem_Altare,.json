{"textgrid.poem.39451": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der sch\u00f6ne J\u00fcngling kniet auf dem Altare,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der sch\u00f6ne J\u00fcngling kniet auf dem Altare,", "tokens": ["Der", "sch\u00f6\u00b7ne", "J\u00fcng\u00b7ling", "kniet", "auf", "dem", "Al\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nackt, bla\u00df, gebeugt, die Arme auf dem R\u00fccken,", "tokens": ["Nackt", ",", "bla\u00df", ",", "ge\u00b7beugt", ",", "die", "Ar\u00b7me", "auf", "dem", "R\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "VVPP", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein banges Weh in den erhobnen Blicken,", "tokens": ["Ein", "ban\u00b7ges", "Weh", "in", "den", "er\u00b7hob\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als ob schon Tod mit Todesfurcht sich paare.", "tokens": ["Als", "ob", "schon", "Tod", "mit", "To\u00b7des\u00b7furcht", "sich", "paa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ADV", "NN", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Vater steht, kraftvoll in greisem Haare,", "tokens": ["Der", "Va\u00b7ter", "steht", ",", "kraft\u00b7voll", "in", "grei\u00b7sem", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gesch\u00fcrzt mit Glauben, sich in Gott zu schicken;", "tokens": ["Ge\u00b7sch\u00fcrzt", "mit", "Glau\u00b7ben", ",", "sich", "in", "Gott", "zu", "schi\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "PRF", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den fest ergriffnen Stahl, er will ihn z\u00fccken,", "tokens": ["Den", "fest", "er\u00b7griff\u00b7nen", "Stahl", ",", "er", "will", "ihn", "z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und morden allen Trost verwaister Jahre.", "tokens": ["Und", "mor\u00b7den", "al\u00b7len", "Trost", "ver\u00b7wais\u00b7ter", "Jah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch, wie er seine Stirn nach oben wendet,", "tokens": ["Doch", ",", "wie", "er", "sei\u00b7ne", "Stirn", "nach", "o\u00b7ben", "wen\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "PPOSAT", "NN", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als spr\u00e4ch er: du befahlst es, Hort und Rather!", "tokens": ["Als", "spr\u00e4ch", "er", ":", "du", "be\u00b7fahlst", "es", ",", "Hort", "und", "Ra\u00b7ther", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Rauscht ihm der Fl\u00fcgel eines Himmelsboten.", "tokens": ["Rauscht", "ihm", "der", "Fl\u00fc\u00b7gel", "ei\u00b7nes", "Him\u00b7mels\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mit deinem Wollen ist die That vollendet.", "tokens": ["Mit", "dei\u00b7nem", "Wol\u00b7len", "ist", "die", "That", "voll\u00b7en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allein behielt sich's vor der ew'ge Vater,", "tokens": ["Al\u00b7lein", "be\u00b7hielt", "sich's", "vor", "der", "ew'\u00b7ge", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Sohn zu opfern f\u00fcr die ewig Todten.", "tokens": ["Den", "Sohn", "zu", "op\u00b7fern", "f\u00fcr", "die", "e\u00b7wig", "Tod\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}