{"textgrid.poem.43677": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Buttermilch mein Leibgericht,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Buttermilch mein Leibgericht,", "tokens": ["Die", "But\u00b7ter\u00b7milch", "mein", "Leib\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Buttermilch mein Leben!", "tokens": ["Die", "But\u00b7ter\u00b7milch", "mein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Frau Nachbarin kann Bess'res nicht", "tokens": ["Frau", "Nach\u00b7ba\u00b7rin", "kann", "Bess'\u00b7res", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VMFIN", "PIS", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir zum Geschenke geben.", "tokens": ["Mir", "zum", "Ge\u00b7schen\u00b7ke", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Was aber schenk' ich ihr daf\u00fcr,", "tokens": ["Was", "a\u00b7ber", "schenk'", "ich", "ihr", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "PPER", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich dankbar zu erweisen?", "tokens": ["Mich", "dank\u00b7bar", "zu", "er\u00b7wei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich geh' und will vor ihrer Th\u00fcr", "tokens": ["Ich", "geh'", "und", "will", "vor", "ih\u00b7rer", "Th\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Buttermilch lobpreisen.", "tokens": ["Die", "But\u00b7ter\u00b7milch", "lob\u00b7prei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Buttermilch, wie schmeckest du", "tokens": ["O", "But\u00b7ter\u00b7milch", ",", "wie", "schme\u00b7ckest", "du"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PWAV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So gut, wie bist du labend!", "tokens": ["So", "gut", ",", "wie", "bist", "du", "la\u00b7bend", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dich k\u00f6nnt' ich trinken immerzu", "tokens": ["Dich", "k\u00f6nnt'", "ich", "trin\u00b7ken", "im\u00b7mer\u00b7zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Morgen bis zum Abend", "tokens": ["Vom", "Mor\u00b7gen", "bis", "zum", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}