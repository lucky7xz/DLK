{"textgrid.poem.41800": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch einmal sprangen auf die Kerkerpforten,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch einmal sprangen auf die Kerkerpforten,", "tokens": ["Noch", "ein\u00b7mal", "spran\u00b7gen", "auf", "die", "Ker\u00b7ker\u00b7pfor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die um den freisten Geist sich einst geschlossen,", "tokens": ["Die", "um", "den", "freis\u00b7ten", "Geist", "sich", "einst", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu neuer Freiheit f\u00fchren neue Sprossen \u2013", "tokens": ["Zu", "neu\u00b7er", "Frei\u00b7heit", "f\u00fch\u00b7ren", "neu\u00b7e", "Spros\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und \u00bb", "tokens": ["Und", "\u00bb"], "token_info": ["word", "punct"], "pos": ["KON", "$("], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "So wie ein Wintersturm aus kaltem Norden", "tokens": ["So", "wie", "ein", "Win\u00b7ter\u00b7sturm", "aus", "kal\u00b7tem", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Blumen naht mit t\u00f6tenden Geschossen,", "tokens": ["Den", "Blu\u00b7men", "naht", "mit", "t\u00f6\u00b7ten\u00b7den", "Ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So kam der Tod, das Gl\u00fcck, das wir genossen,", "tokens": ["So", "kam", "der", "Tod", ",", "das", "Gl\u00fcck", ",", "das", "wir", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit einem einz'gen Schlage hinzumorden.", "tokens": ["Mit", "ei\u00b7nem", "einz'\u00b7gen", "Schla\u00b7ge", "hin\u00b7zu\u00b7mor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gefangen noch, allein auf \u00f6der Erde,", "tokens": ["Ge\u00b7fan\u00b7gen", "noch", ",", "al\u00b7lein", "auf", "\u00f6\u00b7der", "Er\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Du verlie\u00dft, mein herlicher Gef\u00e4hrte!", "tokens": ["Die", "Du", "ver\u00b7lie\u00dft", ",", "mein", "her\u00b7li\u00b7cher", "Ge\u00b7f\u00e4hr\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und m\u00f6cht' ich Dir wie einst am Herzen hangen,", "tokens": ["Und", "m\u00f6cht'", "ich", "Dir", "wie", "einst", "am", "Her\u00b7zen", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "KOKOM", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Blumen schm\u00fccken Deine kahle Zelle,", "tokens": ["Mit", "Blu\u00b7men", "schm\u00fc\u00b7cken", "Dei\u00b7ne", "kah\u00b7le", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So leg ich sie auf Deines Grabes Schwelle.", "tokens": ["So", "leg", "ich", "sie", "auf", "Dei\u00b7nes", "Gra\u00b7bes", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "PPER", "PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}