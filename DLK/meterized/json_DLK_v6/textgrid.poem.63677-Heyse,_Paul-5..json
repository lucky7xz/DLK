{"textgrid.poem.63677": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So w\u00e4r' denn Spiel und Tanz f\u00fcr mich vorbei,", "tokens": ["So", "w\u00e4r'", "denn", "Spiel", "und", "Tanz", "f\u00fcr", "mich", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "KON", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch nach Verj\u00fcngung f\u00fchl' ich kein Verlangen.", "tokens": ["Doch", "nach", "Ver\u00b7j\u00fcn\u00b7gung", "f\u00fchl'", "ich", "kein", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich w\u00fc\u00dft' auch nichts gescheiter anzufangen,", "tokens": ["Ich", "w\u00fc\u00dft'", "auch", "nichts", "ge\u00b7schei\u00b7ter", "an\u00b7zu\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erlebt' ich wieder, was es immer sei.", "tokens": ["Er\u00b7lebt'", "ich", "wie\u00b7der", ",", "was", "es", "im\u00b7mer", "sei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vor\u00fcber ist vor\u00fcber! ", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "ist", "vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gesch\u00f6pfe, die dem Weltengeist entsprangen,", "tokens": ["Ge\u00b7sch\u00f6p\u00b7fe", ",", "die", "dem", "Wel\u00b7ten\u00b7geist", "ent\u00b7spran\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sind ewig nicht wie er. Was Tr\u00e4umer sangen", "tokens": ["Sind", "e\u00b7wig", "nicht", "wie", "er", ".", "Was", "Tr\u00e4u\u00b7mer", "san\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "PTKNEG", "PWAV", "PPER", "$.", "PWS", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von ew'ger Wiederkehr, ist Phantasei.", "tokens": ["Von", "ew'\u00b7ger", "Wie\u00b7der\u00b7kehr", ",", "ist", "Phan\u00b7ta\u00b7sei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch k\u00f6nnte durch ein Wunder auferstehn", "tokens": ["Doch", "k\u00f6nn\u00b7te", "durch", "ein", "Wun\u00b7der", "auf\u00b7er\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von allen l\u00e4ngst verblichnen Stunden ", "tokens": ["Von", "al\u00b7len", "l\u00e4ngst", "ver\u00b7blich\u00b7nen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht w\u00fcrd' ich lang mit mir zu Rate gehn.", "tokens": ["Nicht", "w\u00fcrd'", "ich", "lang", "mit", "mir", "zu", "Ra\u00b7te", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "PPER", "ADJD", "APPR", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Erhellt, das holde Wunder mir geschehn,", "tokens": ["Er\u00b7hellt", ",", "das", "hol\u00b7de", "Wun\u00b7der", "mir", "ge\u00b7schehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ich erkannte: Diese oder keine!", "tokens": ["Da\u00df", "ich", "er\u00b7kann\u00b7te", ":", "Die\u00b7se", "o\u00b7der", "kei\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "PDAT", "KON", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}