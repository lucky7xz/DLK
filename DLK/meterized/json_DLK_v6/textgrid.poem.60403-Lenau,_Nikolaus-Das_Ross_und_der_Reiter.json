{"textgrid.poem.60403": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Das Ross und der Reiter", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die frische Quelle rinnt herab am Steingesenke,", "tokens": ["Die", "fri\u00b7sche", "Quel\u00b7le", "rinnt", "her\u00b7ab", "am", "Stein\u00b7ge\u00b7sen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Reiter f\u00fchrt sein Ro\u00df zur lang ersehnten Tr\u00e4nke", "tokens": ["Der", "Rei\u00b7ter", "f\u00fchrt", "sein", "Ro\u00df", "zur", "lang", "er\u00b7sehn\u00b7ten", "Tr\u00e4n\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Aus Bergesadern k\u00fchl die klaren Fluten flie\u00dfen,", "tokens": ["Aus", "Ber\u00b7ge\u00b7sa\u00b7dern", "k\u00fchl", "die", "kla\u00b7ren", "Flu\u00b7ten", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In hei\u00dfe Adern sich des Pferdes zu ergie\u00dfen.", "tokens": ["In", "hei\u00b7\u00dfe", "A\u00b7dern", "sich", "des", "Pfer\u00b7des", "zu", "er\u00b7gie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Reiter schaut sein Ro\u00df mit innigem Vergn\u00fcgen,", "tokens": ["Der", "Rei\u00b7ter", "schaut", "sein", "Ro\u00df", "mit", "in\u00b7ni\u00b7gem", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie es die Flut einzieht in lustgedehnten Z\u00fcgen;", "tokens": ["Wie", "es", "die", "Flut", "ein\u00b7zieht", "in", "lust\u00b7ge\u00b7dehn\u00b7ten", "Z\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und wie die Wellen ihm die M\u00e4hne wiegend sp\u00fclen,", "tokens": ["Und", "wie", "die", "Wel\u00b7len", "ihm", "die", "M\u00e4h\u00b7ne", "wie\u00b7gend", "sp\u00fc\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "ART", "NN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie sie eingeschl\u00fcrft das hei\u00dfe Blut ihm k\u00fchlen.", "tokens": ["Und", "wie", "sie", "ein\u00b7ge\u00b7schl\u00fcrft", "das", "hei\u00b7\u00dfe", "Blut", "ihm", "k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Rappe m\u00f6chte gern im durstenden Verlangen", "tokens": ["Der", "Rap\u00b7pe", "m\u00f6ch\u00b7te", "gern", "im", "durs\u00b7ten\u00b7den", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jeglichen Wasser gu\u00df, der ihm enteilt, empfangen;", "tokens": ["Jeg\u00b7li\u00b7chen", "Was\u00b7ser", "gu\u00df", ",", "der", "ihm", "ent\u00b7eilt", ",", "emp\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Doch wie er unten trinkt, h\u00f6rt oben schon sein Lauschen", "tokens": ["Doch", "wie", "er", "un\u00b7ten", "trinkt", ",", "h\u00f6rt", "o\u00b7ben", "schon", "sein", "Lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den reichen \u00dcberflu\u00df verhei\u00dfend niederrauschen.", "tokens": ["Den", "rei\u00b7chen", "\u00dc\u00b7berf\u00b7lu\u00df", "ver\u00b7hei\u00b7\u00dfend", "nie\u00b7der\u00b7rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der Reiter hat sich auch am Quelle k\u00fchl getrunken,", "tokens": ["Der", "Rei\u00b7ter", "hat", "sich", "auch", "am", "Quel\u00b7le", "k\u00fchl", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "ADV", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Steht nun im gro\u00dfen Blick des Hochgebirgs versunken.", "tokens": ["Steht", "nun", "im", "gro\u00b7\u00dfen", "Blick", "des", "Hoch\u00b7ge\u00b7birgs", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Er starrt auf Alpen hin, ihr seliges Umnachten,", "tokens": ["Er", "starrt", "auf", "Al\u00b7pen", "hin", ",", "ihr", "se\u00b7li\u00b7ges", "Um\u00b7nach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das leise Zauberspiel des Lichtes zu betrachten;", "tokens": ["Das", "lei\u00b7se", "Zau\u00b7ber\u00b7spiel", "des", "Lich\u00b7tes", "zu", "be\u00b7trach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Wie mit den fernen H\u00f6hn die Strahlen dort verkehren", "tokens": ["Wie", "mit", "den", "fer\u00b7nen", "H\u00f6hn", "die", "Strah\u00b7len", "dort", "ver\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sich in stiller Glut im letzten Ku\u00df verzehren.", "tokens": ["Und", "sich", "in", "stil\u00b7ler", "Glut", "im", "letz\u00b7ten", "Ku\u00df", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Und auf den Wandrer sinkt, den d\u00fcstern, sehnsuchtkranken,", "tokens": ["Und", "auf", "den", "Wand\u00b7rer", "sinkt", ",", "den", "d\u00fcs\u00b7tern", ",", "sehn\u00b7sucht\u00b7kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,", "ART", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der frische Seelentau der himmlischen Gedanken,", "tokens": ["Der", "fri\u00b7sche", "See\u00b7len\u00b7tau", "der", "himm\u00b7li\u00b7schen", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Es str\u00f6mt auf ihn herab die ewge Liebesquelle,", "tokens": ["Es", "str\u00f6mt", "auf", "ihn", "her\u00b7ab", "die", "ew\u00b7ge", "Lie\u00b7bes\u00b7quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es kann sein durstend Herz nicht fassen jede Welle;", "tokens": ["Es", "kann", "sein", "durs\u00b7tend", "Herz", "nicht", "fas\u00b7sen", "je\u00b7de", "Wel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "VVPP", "NN", "PTKNEG", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Doch kann sein Herz auch nicht den ganzen Strom behausen,", "tokens": ["Doch", "kann", "sein", "Herz", "auch", "nicht", "den", "gan\u00b7zen", "Strom", "be\u00b7hau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So h\u00f6rt er oben schon die ewge F\u00fclle brausen.", "tokens": ["So", "h\u00f6rt", "er", "o\u00b7ben", "schon", "die", "ew\u00b7ge", "F\u00fcl\u00b7le", "brau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}