{"textgrid.poem.63577": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "[vor Tage weckte mich]", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor Tage weckte mich", "tokens": ["Vor", "Ta\u00b7ge", "weck\u00b7te", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein klopfend Herz.", "tokens": ["Mein", "klop\u00b7fend", "Herz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Herz, und was klopfst du?", "tokens": ["Herz", ",", "und", "was", "klopfst", "du", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PWS", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Gl\u00fcck oder Schmerz?", "tokens": ["Gl\u00fcck", "o\u00b7der", "Schmerz", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Rings s\u00e4useln die B\u00e4ume", "tokens": ["Rings", "s\u00e4u\u00b7seln", "die", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Im kalten Tau,", "tokens": ["Im", "kal\u00b7ten", "Tau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das letzte Sternlein", "tokens": ["Das", "letz\u00b7te", "Stern\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Erlischt im Blau.", "tokens": ["Er\u00b7lischt", "im", "Blau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Horch! unterm Schindeldach", "tokens": ["Horch", "!", "un\u00b7term", "Schin\u00b7del\u00b7dach"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Marder schleicht;", "tokens": ["Der", "Mar\u00b7der", "schleicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein schlafend Schw\u00e4lblein", "tokens": ["Ein", "schla\u00b7fend", "Schw\u00e4lb\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Hascht er vielleicht.", "tokens": ["Hascht", "er", "viel\u00b7leicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "\u00dcber die Wehre st\u00fcrzt", "tokens": ["\u00dc\u00b7ber", "die", "Weh\u00b7re", "st\u00fcrzt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Der Wildbach nieder;", "tokens": ["Der", "Wild\u00b7bach", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Schlaftrunken r\u00fchrt sich", "tokens": ["Schlaf\u00b7trun\u00b7ken", "r\u00fchrt", "sich"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PRF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Das M\u00fchlrad wieder.", "tokens": ["Das", "M\u00fchl\u00b7rad", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Und dort \u2013 ein Hahnenschrei,", "tokens": ["Und", "dort", "\u2013", "ein", "Hah\u00b7nen\u00b7schrei", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und bald wird's licht.", "tokens": ["Und", "bald", "wird's", "licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Tag, o wie grau ist", "tokens": ["Tag", ",", "o", "wie", "grau", "ist"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "FM", "KOKOM", "ADJD", "VAFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Dein Angesicht!", "tokens": ["Dein", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Tag, der so lieblos", "tokens": ["Tag", ",", "der", "so", "lieb\u00b7los"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zwei Liebste trennt!", "tokens": ["Zwei", "Liebs\u00b7te", "trennt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ach, bis zum Wiedersehn,", "tokens": ["Ach", ",", "bis", "zum", "Wie\u00b7der\u00b7sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wer schlafen k\u00f6nnt'!", "tokens": ["Wer", "schla\u00b7fen", "k\u00f6nnt'", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}