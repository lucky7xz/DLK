{"textgrid.poem.39252": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wei\u00df es wohl, Ihr liebt es nicht,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df es wohl, Ihr liebt es nicht,", "tokens": ["Ich", "wei\u00df", "es", "wohl", ",", "Ihr", "liebt", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu h\u00f6ren alte Geschichten,", "tokens": ["Zu", "h\u00f6\u00b7ren", "al\u00b7te", "Ge\u00b7schich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Allein der Wald ist gar zu gr\u00fcn,", "tokens": ["Al\u00b7lein", "der", "Wald", "ist", "gar", "zu", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu pr\u00e4chtig rauschen die Fichten!", "tokens": ["Zu", "pr\u00e4ch\u00b7tig", "rau\u00b7schen", "die", "Fich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df es wohl, Ihr h\u00f6rt's nicht gern,", "tokens": ["Ich", "wei\u00df", "es", "wohl", ",", "Ihr", "h\u00f6rt's", "nicht", "gern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch kann ich Euch nicht dienen,", "tokens": ["Doch", "kann", "ich", "Euch", "nicht", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier gellt kein Laut der geschw\u00e4tzigen Welt,", "tokens": ["Hier", "gellt", "kein", "Laut", "der", "ge\u00b7schw\u00e4t\u00b7zi\u00b7gen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nur Hirsche grasen im Gr\u00fcnen. \u2013", "tokens": ["Nur", "Hir\u00b7sche", "gra\u00b7sen", "im", "Gr\u00fc\u00b7nen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "NN", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ins Gr\u00fcne ritt Herr Edelfried;", "tokens": ["Ins", "Gr\u00fc\u00b7ne", "ritt", "Herr", "E\u00b7del\u00b7fried", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es bl\u00fchte sein Mund im Scherze,", "tokens": ["Es", "bl\u00fch\u00b7te", "sein", "Mund", "im", "Scher\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihm unterm Sattel tanzte sein Ro\u00df,", "tokens": ["Ihm", "un\u00b7term", "Sat\u00b7tel", "tanz\u00b7te", "sein", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und innen tanzte sein Herze.", "tokens": ["Und", "in\u00b7nen", "tanz\u00b7te", "sein", "Her\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "VAINF", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich kenne Dich wohl, Herr Edelfried,", "tokens": ["Ich", "ken\u00b7ne", "Dich", "wohl", ",", "Herr", "E\u00b7del\u00b7fried", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und wie Dir ist zu Sinne,", "tokens": ["Und", "wie", "Dir", "ist", "zu", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein Herz, Dein neunzehnj\u00e4hriges Herz,", "tokens": ["Dein", "Herz", ",", "Dein", "neun\u00b7zehn\u00b7j\u00e4h\u00b7ri\u00b7ges", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Es tanzt vor lachender Minne!", "tokens": ["Es", "tanzt", "vor", "la\u00b7chen\u00b7der", "Min\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbim Gr\u00fcnen schlummert die Sommernacht,", "tokens": ["\u00bb", "im", "Gr\u00fc\u00b7nen", "schlum\u00b7mert", "die", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Mond webt Silberflitter,", "tokens": ["Der", "Mond", "webt", "Sil\u00b7ber\u00b7flit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wem heute das Herz nicht vor Minne tanzt,", "tokens": ["Wem", "heu\u00b7te", "das", "Herz", "nicht", "vor", "Min\u00b7ne", "tanzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das ist ein schlechter Ritter!\u00ab", "tokens": ["Das", "ist", "ein", "schlech\u00b7ter", "Rit\u00b7ter", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ins Gr\u00fcne sprang Herr Edelfried,", "tokens": ["Ins", "Gr\u00fc\u00b7ne", "sprang", "Herr", "E\u00b7del\u00b7fried", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Zaum ins Ge\u00e4ste schlang er,", "tokens": ["Den", "Zaum", "ins", "Ge\u00b7\u00e4s\u00b7te", "schlang", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er stellte sich in den Elfenring,", "tokens": ["Er", "stell\u00b7te", "sich", "in", "den", "El\u00b7fen\u00b7ring", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Horn an die Lippe schwang er.", "tokens": ["Das", "Horn", "an", "die", "Lip\u00b7pe", "schwang", "er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Im Nachtwind wehte sein Reiherbusch,", "tokens": ["Im", "Nacht\u00b7wind", "weh\u00b7te", "sein", "Rei\u00b7her\u00b7busch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er stand gelehnt am Schwerte,", "tokens": ["Er", "stand", "ge\u00b7lehnt", "am", "Schwer\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er blies den allers\u00fc\u00dfesten Reim,", "tokens": ["Er", "blies", "den", "al\u00b7ler\u00b7s\u00fc\u00b7\u00dfes\u00b7ten", "Reim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ich wei\u00df nicht, wer's ihm lehrte.", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wer's", "ihm", "lehr\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Doch wer ihm immer das Lied gelehrt,", "tokens": ["Doch", "wer", "ihm", "im\u00b7mer", "das", "Lied", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er hat's nicht lange geblasen,", "tokens": ["Er", "hat's", "nicht", "lan\u00b7ge", "ge\u00b7bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihn zog ein wunderschlanker Arm", "tokens": ["Ihn", "zog", "ein", "wun\u00b7der\u00b7schlan\u00b7ker", "Arm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hernieder in den Rasen:", "tokens": ["Her\u00b7nie\u00b7der", "in", "den", "Ra\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbdu wei\u00dfe Fee, Du listige Fee,", "tokens": ["\u00bb", "du", "wei\u00b7\u00dfe", "Fee", ",", "Du", "lis\u00b7ti\u00b7ge", "Fee", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wie bin ich vor Dir erschrocken!\u00ab", "tokens": ["Wie", "bin", "ich", "vor", "Dir", "er\u00b7schro\u00b7cken", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Schwert versank im wehenden Gras,", "tokens": ["Das", "Schwert", "ver\u00b7sank", "im", "we\u00b7hen\u00b7den", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Zusammen flossen die Locken.", "tokens": ["Zu\u00b7sam\u00b7men", "flos\u00b7sen", "die", "Lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Ein langer Ku\u00df, \u2013 o edler Wald!", "tokens": ["Ein", "lan\u00b7ger", "Ku\u00df", ",", "\u2013", "o", "ed\u00b7ler", "Wald", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "$(", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er starb in den s\u00e4uselnden Bl\u00e4ttern,", "tokens": ["Er", "starb", "in", "den", "s\u00e4u\u00b7seln\u00b7den", "Bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und wer die Beiden verraten hat,", "tokens": ["Und", "wer", "die", "Bei\u00b7den", "ver\u00b7ra\u00b7ten", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den m\u00f6gen die Wipfel zerschmettern!", "tokens": ["Den", "m\u00f6\u00b7gen", "die", "Wip\u00b7fel", "zer\u00b7schmet\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}