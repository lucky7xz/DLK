{"dta.poem.8197": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Sturmnacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Ueber Urgro\u00dfmutters Tisch und B\u00e4nke,", "tokens": ["Ue\u00b7ber", "Ur\u00b7gro\u00df\u00b7mut\u00b7ters", "Tisch", "und", "B\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ueber die alten Schatullen und Schr\u00e4nke", "tokens": ["Ue\u00b7ber", "die", "al\u00b7ten", "Scha\u00b7tul\u00b7len", "und", "Schr\u00e4n\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Wandelt der zitternde Mondenstrahl.", "tokens": ["Wan\u00b7delt", "der", "zit\u00b7tern\u00b7de", "Mon\u00b7dens\u00b7trahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Vom Wald kommt der Wind,", "tokens": ["Vom", "Wald", "kommt", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und f\u00e4hrt an die Scheiben;", "tokens": ["Und", "f\u00e4hrt", "an", "die", "Schei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und geschwind, geschwind", "tokens": ["Und", "ge\u00b7schwind", ",", "ge\u00b7schwind"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "ADJD", "$,", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Schwatzt er ein Wort,", "tokens": ["Schwatzt", "er", "ein", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Und dann wieder fort", "tokens": ["Und", "dann", "wie\u00b7der", "fort"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "PTKVZ"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Zum Wald \u00fcber F\u00f6hren und Eiben.", "tokens": ["Zum", "Wald", "\u00fc\u00b7ber", "F\u00f6h\u00b7ren", "und", "Ei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Da wird auch das alte verzauberte Holz", "tokens": ["Da", "wird", "auch", "das", "al\u00b7te", "ver\u00b7zau\u00b7ber\u00b7te", "Holz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.11": {"text": "Da drinnen lebendig;", "tokens": ["Da", "drin\u00b7nen", "le\u00b7ben\u00b7dig", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.12": {"text": "Wie sonst im Walde will es stolz", "tokens": ["Wie", "sonst", "im", "Wal\u00b7de", "will", "es", "stolz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "NN", "VMFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die Kronen sch\u00fctteln unb\u00e4ndig,", "tokens": ["Die", "Kro\u00b7nen", "sch\u00fct\u00b7teln", "un\u00b7b\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Mit den Aesten greifen hinaus in die Nacht,", "tokens": ["Mit", "den", "A\u00b7es\u00b7ten", "grei\u00b7fen", "hin\u00b7aus", "in", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.15": {"text": "Mit dem Sturm sich schaukeln in brausender Jagd,", "tokens": ["Mit", "dem", "Sturm", "sich", "schau\u00b7keln", "in", "brau\u00b7sen\u00b7der", "Jagd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Mit den Bl\u00e4ttern im Uebermuth rauschen;", "tokens": ["Mit", "den", "Bl\u00e4t\u00b7tern", "im", "Ue\u00b7ber\u00b7muth", "rau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.17": {"text": "Beim Tanz im Flug", "tokens": ["Beim", "Tanz", "im", "Flug"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Durch Wolkenzug", "tokens": ["Durch", "Wol\u00b7ken\u00b7zug"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Mit dem Mondlicht silberne Blicke tauschen.", "tokens": ["Mit", "dem", "Mond\u00b7licht", "sil\u00b7ber\u00b7ne", "Bli\u00b7cke", "tau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.20": {"text": "Da m\u00fcht sich der Lehnstuhl die Arme zu recken,", "tokens": ["Da", "m\u00fcht", "sich", "der", "Lehn\u00b7stuhl", "die", "Ar\u00b7me", "zu", "re\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.21": {"text": "Den Roccocofu\u00df will das Kanapee strecken,", "tokens": ["Den", "Roc\u00b7co\u00b7co\u00b7fu\u00df", "will", "das", "Ka\u00b7na\u00b7pee", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "In der Kommode die Schubf\u00e4cher dr\u00e4ngen", "tokens": ["In", "der", "Kom\u00b7mo\u00b7de", "die", "Schub\u00b7f\u00e4\u00b7cher", "dr\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Und wollen die rostigen Schl\u00f6sser sprengen;", "tokens": ["Und", "wol\u00b7len", "die", "ros\u00b7ti\u00b7gen", "Schl\u00f6s\u00b7ser", "spren\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.24": {"text": "Der Eichschrank unter dem kleinen Tro\u00df", "tokens": ["Der", "Eich\u00b7schrank", "un\u00b7ter", "dem", "klei\u00b7nen", "Tro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Steht da, ein finsterer Kolo\u00df.", "tokens": ["Steht", "da", ",", "ein", "fins\u00b7te\u00b7rer", "Ko\u00b7lo\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Traumhaft regt er die Klauen an,", "tokens": ["Traum\u00b7haft", "regt", "er", "die", "Klau\u00b7en", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.27": {"text": "Ihm zuckt's in der verlornen Krone;", "tokens": ["Ihm", "zuckt's", "in", "der", "ver\u00b7lor\u00b7nen", "Kro\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Doch bricht er nicht den schweren Bann.", "tokens": ["Doch", "bricht", "er", "nicht", "den", "schwe\u00b7ren", "Bann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und drau\u00dfen pfeift ihm der Wind zum Hohne,", "tokens": ["Und", "drau\u00b7\u00dfen", "pfeift", "ihm", "der", "Wind", "zum", "Hoh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Und f\u00e4hrt an die L\u00e4den und r\u00fcttelt mit Macht,", "tokens": ["Und", "f\u00e4hrt", "an", "die", "L\u00e4\u00b7den", "und", "r\u00fct\u00b7telt", "mit", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.31": {"text": "Bl\u00e4s't durch die Ritzen, grunzt und lacht,", "tokens": ["Bl\u00e4s't", "durch", "die", "Rit\u00b7zen", ",", "grunzt", "und", "lacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Schmei\u00dft die Flederm\u00e4use, die kleinen Gespenster", "tokens": ["Schmei\u00dft", "die", "Fle\u00b7der\u00b7m\u00e4u\u00b7se", ",", "die", "klei\u00b7nen", "Ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.33": {"text": "Klitschend gegen die rasselnden Fenster.", "tokens": ["Klit\u00b7schend", "ge\u00b7gen", "die", "ras\u00b7seln\u00b7den", "Fens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.34": {"text": "Die glupen dumm neugierig hinein \u2014", "tokens": ["Die", "glu\u00b7pen", "dumm", "neu\u00b7gie\u00b7rig", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.35": {"text": "Da drinn' steht voll der Mondenschein.", "tokens": ["Da", "drinn'", "steht", "voll", "der", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Aber droben im Haus", "tokens": ["A\u00b7ber", "dro\u00b7ben", "im", "Haus"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.37": {"text": "Im behaglichen Zimmer", "tokens": ["Im", "be\u00b7hag\u00b7li\u00b7chen", "Zim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.38": {"text": "Beim Sturmgebraus", "tokens": ["Beim", "Sturm\u00b7ge\u00b7braus"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.39": {"text": "Sa\u00dfen und schwatzten die Alten noch immer,", "tokens": ["Sa\u00b7\u00dfen", "und", "schwatz\u00b7ten", "die", "Al\u00b7ten", "noch", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.40": {"text": "Nicht h\u00f6rend, wie drunten die Saalth\u00fcr sprang,", "tokens": ["Nicht", "h\u00f6\u00b7rend", ",", "wie", "drun\u00b7ten", "die", "Saalt\u00b7h\u00fcr", "sprang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.41": {"text": "Wie ein Klang war erwacht", "tokens": ["Wie", "ein", "Klang", "war", "er\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VAFIN", "VVPP"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.42": {"text": "Aus der lautlosen Nacht,", "tokens": ["Aus", "der", "laut\u00b7lo\u00b7sen", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.43": {"text": "Der schollernd drang", "tokens": ["Der", "schol\u00b7lernd", "drang"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.44": {"text": "Ueber Trepp' und Gang,", "tokens": ["Ue\u00b7ber", "Trepp'", "und", "Gang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.45": {"text": "Da\u00df dran in der Kammer die Kinder mit Schrecken", "tokens": ["Da\u00df", "dran", "in", "der", "Kam\u00b7mer", "die", "Kin\u00b7der", "mit", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PAV", "APPR", "ART", "NN", "ART", "NN", "APPR", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.46": {"text": "Auffuhren und schl\u00fcpften unter die Decken.", "tokens": ["Auf\u00b7fuh\u00b7ren", "und", "schl\u00fcpf\u00b7ten", "un\u00b7ter", "die", "De\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}