{"textgrid.poem.51078": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der erste Tannenbaum, den ich gesehn,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der erste Tannenbaum, den ich gesehn,", "tokens": ["Der", "ers\u00b7te", "Tan\u00b7nen\u00b7baum", ",", "den", "ich", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das war ein Weihnachtsbaum im Kerzenschimmer;", "tokens": ["Das", "war", "ein", "Weih\u00b7nachts\u00b7baum", "im", "Ker\u00b7zen\u00b7schim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch seh ich lieblich glimmend vor mir stehn", "tokens": ["Noch", "seh", "ich", "lieb\u00b7lich", "glim\u00b7mend", "vor", "mir", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJD", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das gr\u00fcne Wunder im erhellten Zimmer.", "tokens": ["Das", "gr\u00fc\u00b7ne", "Wun\u00b7der", "im", "er\u00b7hell\u00b7ten", "Zim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da war ich t\u00e4glich mit dem fr\u00fchsten wach,", "tokens": ["Da", "war", "ich", "t\u00e4g\u00b7lich", "mit", "dem", "fr\u00fchs\u00b7ten", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Zweigen gl\u00e4ubig ihren Schmuck zu rauben;", "tokens": ["Den", "Zwei\u00b7gen", "gl\u00e4u\u00b7big", "ih\u00b7ren", "Schmuck", "zu", "rau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch als die letzte s\u00fc\u00dfe Frucht ich brach,", "tokens": ["Doch", "als", "die", "letz\u00b7te", "s\u00fc\u00b7\u00dfe", "Frucht", "ich", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ging es zugleich an meinen Wunderglauben.", "tokens": ["Ging", "es", "zu\u00b7gleich", "an", "mei\u00b7nen", "Wun\u00b7der\u00b7glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dann aber, als im Lenz zum ersten Mal", "tokens": ["Dann", "a\u00b7ber", ",", "als", "im", "Lenz", "zum", "ers\u00b7ten", "Mal"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "APPRART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In einen Nadelwald ich mich verirrte,", "tokens": ["In", "ei\u00b7nen", "Na\u00b7del\u00b7wald", "ich", "mich", "ver\u00b7irr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich durch die hohen stillen S\u00e4ulen stahl,", "tokens": ["Mich", "durch", "die", "ho\u00b7hen", "stil\u00b7len", "S\u00e4u\u00b7len", "stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bis sich der Hain zu jungem Schlag entwirrte:", "tokens": ["Bis", "sich", "der", "Hain", "zu", "jun\u00b7gem", "Schlag", "ent\u00b7wirr\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O Freudigkeit! wie ich da ungesehn", "tokens": ["O", "Freu\u00b7dig\u00b7keit", "!", "wie", "ich", "da", "un\u00b7ge\u00b7sehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PWAV", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In einem Forst von Weihnachtsb\u00e4umchen spielte,", "tokens": ["In", "ei\u00b7nem", "Forst", "von", "Weih\u00b7nachts\u00b7b\u00e4um\u00b7chen", "spiel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dicht um mein Haar ihr zartes Wipfelwehn,", "tokens": ["Dicht", "um", "mein", "Haar", "ihr", "zar\u00b7tes", "Wip\u00b7fel\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das \u00fcberragend mir den Scheitel k\u00fchlte.", "tokens": ["Das", "\u00fc\u00b7berr\u00b7a\u00b7gend", "mir", "den", "Schei\u00b7tel", "k\u00fchl\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein kleiner Riese in dem kleinen Tann,", "tokens": ["Ein", "klei\u00b7ner", "Rie\u00b7se", "in", "dem", "klei\u00b7nen", "Tann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sah ich vergn\u00fcgt, wo Weihnachtsb\u00e4ume sprie\u00dfen;", "tokens": ["Sah", "ich", "ver\u00b7gn\u00fcgt", ",", "wo", "Weih\u00b7nachts\u00b7b\u00e4u\u00b7me", "sprie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$,", "PWAV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich packte keck ein winzig T\u00e4nnlein an", "tokens": ["Ich", "pack\u00b7te", "keck", "ein", "win\u00b7zig", "T\u00e4nn\u00b7lein", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJD", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und bog es m\u00e4chtig ringend mir zu F\u00fc\u00dfen.", "tokens": ["Und", "bog", "es", "m\u00e4ch\u00b7tig", "rin\u00b7gend", "mir", "zu", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVPP", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und \u00fcber mir war nichts als blauer Raum;", "tokens": ["Und", "\u00fc\u00b7ber", "mir", "war", "nichts", "als", "blau\u00b7er", "Raum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "PIS", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch als ich mich dicht an die Erde schmiegte,", "tokens": ["Doch", "als", "ich", "mich", "dicht", "an", "die", "Er\u00b7de", "schmieg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sah unten ich durch d\u00fcnner St\u00e4mmchen Saum,", "tokens": ["Sah", "un\u00b7ten", "ich", "durch", "d\u00fcn\u00b7ner", "St\u00e4mm\u00b7chen", "Saum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie Land und See im Silberduft sich wiegte.", "tokens": ["Wie", "Land", "und", "See", "im", "Sil\u00b7ber\u00b7duft", "sich", "wieg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wie ich so lag, da rauscht' und stob's herbei,", "tokens": ["Wie", "ich", "so", "lag", ",", "da", "rauscht'", "und", "stob's", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "KON", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df mir der Lufthauch durch die Locken sauste,", "tokens": ["Da\u00df", "mir", "der", "Luft\u00b7hauch", "durch", "die", "Lo\u00b7cken", "saus\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und aus der H\u00f6h scho\u00df senkrecht her der Weih,", "tokens": ["Und", "aus", "der", "H\u00f6h", "scho\u00df", "sen\u00b7krecht", "her", "der", "Weih", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da\u00df seiner Schwingen Schlag im Ohr mir brauste.", "tokens": ["Da\u00df", "sei\u00b7ner", "Schwin\u00b7gen", "Schlag", "im", "Ohr", "mir", "braus\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Als schwebend er nah ob dem Haupt mir stand,", "tokens": ["Als", "schwe\u00b7bend", "er", "nah", "ob", "dem", "Haupt", "mir", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ADJD", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Funkelt' sein Aug gleich dunklen Edelsteinen;", "tokens": ["Fun\u00b7kelt'", "sein", "Aug", "gleich", "dunk\u00b7len", "E\u00b7del\u00b7stei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Zu\u00e4u\u00dferst an der Fl\u00fcgel d\u00fcnnem Rand", "tokens": ["Zu\u00b7\u00e4u\u00b7\u00dferst", "an", "der", "Fl\u00fc\u00b7gel", "d\u00fcn\u00b7nem", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sah ich die Sonne durch die Kiele scheinen.", "tokens": ["Sah", "ich", "die", "Son\u00b7ne", "durch", "die", "Kie\u00b7le", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Auf meinem Angesicht sein Schatten ruht'", "tokens": ["Auf", "mei\u00b7nem", "An\u00b7ge\u00b7sicht", "sein", "Schat\u00b7ten", "ruht'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und lie\u00df die gl\u00fchen Wangen mir erkalten \u2013", "tokens": ["Und", "lie\u00df", "die", "gl\u00fc\u00b7hen", "Wan\u00b7gen", "mir", "er\u00b7kal\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob welchem Inderf\u00fcrst von hei\u00dfem Blut", "tokens": ["Ob", "wel\u00b7chem", "In\u00b7der\u00b7f\u00fcrst", "von", "hei\u00b7\u00dfem", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PWAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ward solch ein Sonnenschirm emporgehalten?", "tokens": ["Ward", "solch", "ein", "Son\u00b7nen\u00b7schirm", "em\u00b7por\u00b7ge\u00b7hal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wie ich so lag, erschaut ich pl\u00f6tzlich nah,", "tokens": ["Wie", "ich", "so", "lag", ",", "er\u00b7schaut", "ich", "pl\u00f6tz\u00b7lich", "nah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie eine Eidechs mit neugier'gem Blicke", "tokens": ["Wie", "ei\u00b7ne", "Ei\u00b7dechs", "mit", "neu\u00b7gier'\u00b7gem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vom n\u00e4chsten Zweig ins Aug mir niedersah,", "tokens": ["Vom", "n\u00e4chs\u00b7ten", "Zweig", "ins", "Aug", "mir", "nie\u00b7der\u00b7sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie in die Flut ein Kind auf schwanker Br\u00fccke.", "tokens": ["Wie", "in", "die", "Flut", "ein", "Kind", "auf", "schwan\u00b7ker", "Br\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Nie hab ich mehr solch guten Blick gesehn", "tokens": ["Nie", "hab", "ich", "mehr", "solch", "gu\u00b7ten", "Blick", "ge\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und so lebendig ruhig, fein und gl\u00fchend;", "tokens": ["Und", "so", "le\u00b7ben\u00b7dig", "ru\u00b7hig", ",", "fein", "und", "gl\u00fc\u00b7hend", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Hellgr\u00fcn war sie, ich sah den Odem gehn", "tokens": ["Hell\u00b7gr\u00fcn", "war", "sie", ",", "ich", "sah", "den", "O\u00b7dem", "gehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In zarter Brust, bla\u00df wie ein R\u00f6schen bl\u00fchend.", "tokens": ["In", "zar\u00b7ter", "Brust", ",", "bla\u00df", "wie", "ein", "R\u00f6\u00b7schen", "bl\u00fc\u00b7hend", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Ob sie mein blaues Auge niederzog?", "tokens": ["Ob", "sie", "mein", "blau\u00b7es", "Au\u00b7ge", "nie\u00b7der\u00b7zog", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie lie\u00df vom Zweig sich auf die Stirn mir nieder,", "tokens": ["Sie", "lie\u00df", "vom", "Zweig", "sich", "auf", "die", "Stirn", "mir", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PRF", "APPR", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schritt abw\u00e4rts, bis sie um den Hals mir bog,", "tokens": ["Schritt", "ab\u00b7w\u00e4rts", ",", "bis", "sie", "um", "den", "Hals", "mir", "bog", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein fein Geschmeide, ruhend, ihre Glieder.", "tokens": ["Ein", "fein", "Ge\u00b7schmei\u00b7de", ",", "ru\u00b7hend", ",", "ih\u00b7re", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "VVPP", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Ich hielt mich reglos, und mit lindem Druck", "tokens": ["Ich", "hielt", "mich", "reg\u00b7los", ",", "und", "mit", "lin\u00b7dem", "Druck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchlt ich den leisen Puls am Halse schlagen;", "tokens": ["F\u00fchlt", "ich", "den", "lei\u00b7sen", "Puls", "am", "Hal\u00b7se", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das war der einzige und sch\u00f6nste Schmuck,", "tokens": ["Das", "war", "der", "ein\u00b7zi\u00b7ge", "und", "sch\u00f6ns\u00b7te", "Schmuck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den ich in meinem Leben je getragen!", "tokens": ["Den", "ich", "in", "mei\u00b7nem", "Le\u00b7ben", "je", "ge\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Damals war ich ein kleiner Pantheist", "tokens": ["Da\u00b7mals", "war", "ich", "ein", "klei\u00b7ner", "Pan\u00b7theist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und ruhte selig in den jungen B\u00e4umen;", "tokens": ["Und", "ruh\u00b7te", "se\u00b7lig", "in", "den", "jun\u00b7gen", "B\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch nimmer ahnte mir zu jener Frist,", "tokens": ["Doch", "nim\u00b7mer", "ahn\u00b7te", "mir", "zu", "je\u00b7ner", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df in den St\u00e4mmchen solche Bretter keimen!", "tokens": ["Da\u00df", "in", "den", "St\u00e4mm\u00b7chen", "sol\u00b7che", "Bret\u00b7ter", "kei\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}