{"textgrid.poem.45819": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[wie steigst Du so ganz leise]", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie steigst Du so ganz leise", "tokens": ["Wie", "steigst", "Du", "so", "ganz", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Still aus der Erd' heraus", "tokens": ["Still", "aus", "der", "Erd'", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und lischst das sch\u00f6ne, wei\u00dfe,", "tokens": ["Und", "lischst", "das", "sch\u00f6\u00b7ne", ",", "wei\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hell Tageslichtlein aus.", "tokens": ["Hell", "Ta\u00b7ges\u00b7licht\u00b7lein", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du machst das ganze Leben", "tokens": ["Du", "machst", "das", "gan\u00b7ze", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So dunkel und so tot;", "tokens": ["So", "dun\u00b7kel", "und", "so", "tot", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Willst mir wohl Ruhe geben,", "tokens": ["Willst", "mir", "wohl", "Ru\u00b7he", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und tr\u00f6sten meine Not?", "tokens": ["Und", "tr\u00f6s\u00b7ten", "mei\u00b7ne", "Not", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da bist Du gar betrogen,", "tokens": ["Da", "bist", "Du", "gar", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Giebst Dir verlorne M\u00fch';", "tokens": ["Giebst", "Dir", "ver\u00b7lor\u00b7ne", "M\u00fch'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den aus der Not gezogen,", "tokens": ["Den", "aus", "der", "Not", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat keine mich, als sie,", "tokens": ["Hat", "kei\u00b7ne", "mich", ",", "als", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPER", "$,", "KOUS", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die jetzt in stillen Stunden", "tokens": ["Die", "jetzt", "in", "stil\u00b7len", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein kleines Bildnis schaut,", "tokens": ["Mein", "klei\u00b7nes", "Bild\u00b7nis", "schaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Liebe zu gesunden,", "tokens": ["Der", "Lie\u00b7be", "zu", "ge\u00b7sun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Manch Hoffnungsschlo\u00df erbaut.", "tokens": ["Manch", "Hoff\u00b7nungs\u00b7schlo\u00df", "er\u00b7baut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die treue Lieb' bewachet,", "tokens": ["Die", "treu\u00b7e", "Lieb'", "be\u00b7wa\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat schw\u00e4rzer Haar als Du,", "tokens": ["Hat", "schw\u00e4r\u00b7zer", "Haar", "als", "Du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "KOUS", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ihre Sternlein machet", "tokens": ["Und", "ih\u00b7re", "Stern\u00b7lein", "ma\u00b7chet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie jetzo balde zu.", "tokens": ["Sie", "jet\u00b7zo", "bal\u00b7de", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich geh' in stillen Gr\u00fcnden,", "tokens": ["Ich", "geh'", "in", "stil\u00b7len", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein' Liebe sinnt von ihr", "tokens": ["Mein'", "Lie\u00b7be", "sinnt", "von", "ihr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Viel Blumen; daraus winden", "tokens": ["Viel", "Blu\u00b7men", ";", "da\u00b7raus", "win\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$.", "PAV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich will ein Kr\u00e4nzlein mir.", "tokens": ["Ich", "will", "ein", "Kr\u00e4nz\u00b7lein", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Bl\u00fcmlein sind Gedanken,", "tokens": ["Die", "Bl\u00fcm\u00b7lein", "sind", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die schnelle all vergehn;", "tokens": ["Die", "schnel\u00b7le", "all", "ver\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIAT", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch eins hab' ich zu danken,", "tokens": ["Doch", "eins", "hab'", "ich", "zu", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das bleibet ewig stehn;", "tokens": ["Das", "blei\u00b7bet", "e\u00b7wig", "stehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich hab' ein sch\u00f6n Geschmeide", "tokens": ["Ich", "hab'", "ein", "sch\u00f6n", "Ge\u00b7schmei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit wundersamer Kraft,", "tokens": ["Mit", "wun\u00b7der\u00b7sa\u00b7mer", "Kraft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das alles Gl\u00fcck und Freude", "tokens": ["Das", "al\u00b7les", "Gl\u00fcck", "und", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir bald zu Diensten schafft.", "tokens": ["Mir", "bald", "zu", "Diens\u00b7ten", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wohnt doch die Wahrheit drinnen,", "tokens": ["Wohnt", "doch", "die", "Wahr\u00b7heit", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und zweifeln kann ich nicht,", "tokens": ["Und", "zwei\u00b7feln", "kann", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil hell zu meinen Sinnen", "tokens": ["Weil", "hell", "zu", "mei\u00b7nen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Wunderblume spricht.", "tokens": ["Die", "Wun\u00b7der\u00b7blu\u00b7me", "spricht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}