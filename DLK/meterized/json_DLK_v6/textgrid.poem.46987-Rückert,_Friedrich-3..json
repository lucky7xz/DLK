{"textgrid.poem.46987": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da kamen, von dem Namen", "tokens": ["Da", "ka\u00b7men", ",", "von", "dem", "Na\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des deutschen Feldmarschalls", "tokens": ["Des", "deut\u00b7schen", "Feld\u00b7mar\u00b7schalls"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gelockt, die britt'schen Damen", "tokens": ["Ge\u00b7lockt", ",", "die", "britt'\u00b7schen", "Da\u00b7men"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herbei nun ebenfalls.", "tokens": ["Her\u00b7bei", "nun", "e\u00b7ben\u00b7falls", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Begehrten von den Haaren", "tokens": ["Be\u00b7gehr\u00b7ten", "von", "den", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des alten Feldmarschalls,", "tokens": ["Des", "al\u00b7ten", "Feld\u00b7mar\u00b7schalls", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als Schmuck sie zu bewahren", "tokens": ["Als", "Schmuck", "sie", "zu", "be\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am Busen, um den Hals.", "tokens": ["Am", "Bu\u00b7sen", ",", "um", "den", "Hals", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUI", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da zog er ohne Stocken", "tokens": ["Da", "zog", "er", "oh\u00b7ne", "Sto\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Hut vom Haupte fein,", "tokens": ["Den", "Hut", "vom", "Haup\u00b7te", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und zeigte, da\u00df die Locken", "tokens": ["Und", "zeig\u00b7te", ",", "da\u00df", "die", "Lo\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm ausgegangen sei'n.", "tokens": ["Ihm", "aus\u00b7ge\u00b7gan\u00b7gen", "sei'", "n."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Verzeihung, sch\u00f6ne Damen,", "tokens": ["Ver\u00b7zei\u00b7hung", ",", "sch\u00f6\u00b7ne", "Da\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich mit solchem Flor", "tokens": ["Da\u00df", "ich", "mit", "sol\u00b7chem", "Flor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht dienen kann, es kamen", "tokens": ["Nicht", "die\u00b7nen", "kann", ",", "es", "ka\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Euch andre schon zuvor;", "tokens": ["Euch", "and\u00b7re", "schon", "zu\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die mir die Locken nahmen,", "tokens": ["Die", "mir", "die", "Lo\u00b7cken", "nah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und stritten drum zumal;", "tokens": ["Und", "strit\u00b7ten", "drum", "zu\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Jahre, sch\u00f6ne Damen,", "tokens": ["Die", "Jah\u00b7re", ",", "sch\u00f6\u00b7ne", "Da\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sind's, die mich machten kahl.", "tokens": ["Sin\u00b7d's", ",", "die", "mich", "mach\u00b7ten", "kahl", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Die kriegerischen Jahre,", "tokens": ["Die", "krie\u00b7ge\u00b7ri\u00b7schen", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie nahmen alles schier,", "tokens": ["Sie", "nah\u00b7men", "al\u00b7les", "schier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und diesen Rest nur spare", "tokens": ["Und", "die\u00b7sen", "Rest", "nur", "spa\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich noch f\u00fcr Deutschland hier:", "tokens": ["Ich", "noch", "f\u00fcr", "Deutschland", "hier", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NE", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Da\u00df, wenn mir altem Tropfe", "tokens": ["Da\u00df", ",", "wenn", "mir", "al\u00b7tem", "Trop\u00b7fe"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wird dort mein Lorbeerkranz,", "tokens": ["Wird", "dort", "mein", "Lor\u00b7beer\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er auf dem kahlen Kopfe", "tokens": ["Er", "auf", "dem", "kah\u00b7len", "Kop\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sei ohne Halt nicht ganz.", "tokens": ["Sei", "oh\u00b7ne", "Halt", "nicht", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}