{"textgrid.poem.24110": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Eine Parabel vom Mond und dem Riesen", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinter dem Berge", "tokens": ["Hin\u00b7ter", "dem", "Ber\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die tausend Zwerge", "tokens": ["Die", "tau\u00b7send", "Zwer\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ART", "CARD", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Mit den gro\u00dfen Sch\u00e4deln gescheidt und frech", "tokens": ["Mit", "den", "gro\u00b7\u00dfen", "Sch\u00e4\u00b7deln", "ge\u00b7scheidt", "und", "frech"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "KON", "ADJD"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Lassen wieder glei\u00dfen", "tokens": ["Las\u00b7sen", "wie\u00b7der", "glei\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Im grellen, wei\u00dfen", "tokens": ["Im", "grel\u00b7len", ",", "wei\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPRART", "ADJA", "$,", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Scheine das runde, blinkende Blech.", "tokens": ["Schei\u00b7ne", "das", "run\u00b7de", ",", "blin\u00b7ken\u00b7de", "Blech", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Gespannt den Bogen!", "tokens": ["Ge\u00b7spannt", "den", "Bo\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die Sehne gezogen!", "tokens": ["Die", "Seh\u00b7ne", "ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ich treffe das blitzende, glitzende Ding.", "tokens": ["Ich", "tref\u00b7fe", "das", "blit\u00b7zen\u00b7de", ",", "glit\u00b7zen\u00b7de", "Ding", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Was soll das Geblecher!", "tokens": ["Was", "soll", "das", "Ge\u00b7ble\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zum Abendtrunkbecher", "tokens": ["Zum", "A\u00b7bend\u00b7trunk\u00b7be\u00b7cher"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.6": {"text": "Brauch ich kein zitterndes Flimmergeblink.", "tokens": ["Brauch", "ich", "kein", "zit\u00b7tern\u00b7des", "Flim\u00b7mer\u00b7ge\u00b7blink", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Es saust von der Sehne", "tokens": ["Es", "saust", "von", "der", "Seh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der Pfeil, seine M\u00e4hne", "tokens": ["Der", "Pfeil", ",", "sei\u00b7ne", "M\u00e4h\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wirft r\u00fcckw\u00e4rts der Riese und wartet gespannt.", "tokens": ["Wirft", "r\u00fcck\u00b7w\u00e4rts", "der", "Rie\u00b7se", "und", "war\u00b7tet", "ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "KON", "VVFIN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Dann br\u00fcllt er: Daneben!", "tokens": ["Dann", "br\u00fcllt", "er", ":", "Da\u00b7ne\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PAV", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "So will ich es heben", "tokens": ["So", "will", "ich", "es", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Das Ding aus der H\u00f6he mit eigener Hand.", "tokens": ["Das", "Ding", "aus", "der", "H\u00f6\u00b7he", "mit", "ei\u00b7ge\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Es soll nicht dort hangen!", "tokens": ["Es", "soll", "nicht", "dort", "han\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ich will es mir fangen,", "tokens": ["Ich", "will", "es", "mir", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ich will von den Zwergen nichts glitzerndes sehn!", "tokens": ["Ich", "will", "von", "den", "Zwer\u00b7gen", "nichts", "glit\u00b7zern\u00b7des", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "PIS", "ADJA", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich wills ihnen weisen!", "tokens": ["Ich", "wills", "ih\u00b7nen", "wei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ich will es zerschmei\u00dfen,", "tokens": ["Ich", "will", "es", "zer\u00b7schmei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Klirr soll es in tausend Kleinst\u00fccke mir gehn!", "tokens": ["Klirr", "soll", "es", "in", "tau\u00b7send", "Klein\u00b7st\u00fc\u00b7cke", "mir", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "APPR", "CARD", "NN", "PPER", "VVINF", "$."], "meter": "++--+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Es rannte der Riese", "tokens": ["Es", "rann\u00b7te", "der", "Rie\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wild \u00fcber die Wiese,", "tokens": ["Wild", "\u00fc\u00b7ber", "die", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ueber Berge und Th\u00e4ler, durch S\u00fcmpfe und Kot.", "tokens": ["Ue\u00b7ber", "Ber\u00b7ge", "und", "Th\u00e4\u00b7ler", ",", "durch", "S\u00fcmp\u00b7fe", "und", "Kot", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "\u00bbfort! Fort mit dem Scheine!\u00ab", "tokens": ["\u00bb", "fort", "!", "Fort", "mit", "dem", "Schei\u00b7ne", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Er brach sich die Beine.", "tokens": ["Er", "brach", "sich", "die", "Bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der Mond h\u00e4ngt noch oben, der Riese ist tot.", "tokens": ["Der", "Mond", "h\u00e4ngt", "noch", "o\u00b7ben", ",", "der", "Rie\u00b7se", "ist", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}