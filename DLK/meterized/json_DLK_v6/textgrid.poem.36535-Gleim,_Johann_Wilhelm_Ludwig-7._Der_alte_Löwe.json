{"textgrid.poem.36535": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "7. Der alte L\u00f6we", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein L\u00f6we, der ein Held in seiner Jugend war,", "tokens": ["Ein", "L\u00f6\u00b7we", ",", "der", "ein", "Held", "in", "sei\u00b7ner", "Ju\u00b7gend", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lag nun, im h\u00f6chsten Stufenjahr,", "tokens": ["Lag", "nun", ",", "im", "h\u00f6chs\u00b7ten", "Stu\u00b7fen\u00b7jahr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht ohne Gicht und ohne Schmerz,", "tokens": ["Nicht", "oh\u00b7ne", "Gicht", "und", "oh\u00b7ne", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In seiner H\u00f6hle hinterw\u00e4rts.", "tokens": ["In", "sei\u00b7ner", "H\u00f6h\u00b7le", "hin\u00b7ter\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar f\u00fchlt' er noch sein gro\u00dfes Herz", "tokens": ["Zwar", "f\u00fchlt'", "er", "noch", "sein", "gro\u00b7\u00dfes", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und seinen Heldenmut;", "tokens": ["Und", "sei\u00b7nen", "Hel\u00b7den\u00b7mut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Allein, erloschen war der Augen Glut,", "tokens": ["Al\u00b7lein", ",", "er\u00b7lo\u00b7schen", "war", "der", "Au\u00b7gen", "Glut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVPP", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Stumpf seine Klau, schwach sein Geh\u00f6r;", "tokens": ["Stumpf", "sei\u00b7ne", "Klau", ",", "schwach", "sein", "Ge\u00b7h\u00f6r", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Z\u00e4hne hatt' er gar nicht mehr.", "tokens": ["Und", "Z\u00e4h\u00b7ne", "hatt'", "er", "gar", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "An einen nahen Bach, den letzten Trunk zu thun;", "tokens": ["An", "ei\u00b7nen", "na\u00b7hen", "Bach", ",", "den", "letz\u00b7ten", "Trunk", "zu", "thun", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Spricht: ", "tokens": ["Spricht", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Herr L\u00f6we, ja, das wirst du wohl! Der Tod,", "tokens": ["Herr", "L\u00f6\u00b7we", ",", "ja", ",", "das", "wirst", "du", "wohl", "!", "Der", "Tod", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PTKANT", "$,", "PDS", "VAFIN", "PPER", "ADV", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der dir mit seiner Hippe droht,", "tokens": ["Der", "dir", "mit", "sei\u00b7ner", "Hip\u00b7pe", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie du mit z\u00e4hnevollem Rachen", "tokens": ["Wie", "du", "mit", "z\u00e4h\u00b7ne\u00b7vol\u00b7lem", "Ra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vasallen drohtest, der wird dir das Ende machen!", "tokens": ["Va\u00b7sal\u00b7len", "droh\u00b7test", ",", "der", "wird", "dir", "das", "En\u00b7de", "ma\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Darum, Herr L\u00f6we, ha! noch einmal trink dich satt!", "tokens": ["Da\u00b7rum", ",", "Herr", "L\u00f6\u00b7we", ",", "ha", "!", "noch", "ein\u00b7mal", "trink", "dich", "satt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "NE", "$,", "ITJ", "$.", "ADV", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er thuts, er l\u00f6scht den Durst, nimmt seine Lagerstatt", "tokens": ["Er", "thuts", ",", "er", "l\u00f6scht", "den", "Durst", ",", "nimmt", "sei\u00b7ne", "La\u00b7ger\u00b7statt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Bache, kehrt sich um, und seufzet: ", "tokens": ["Am", "Ba\u00b7che", ",", "kehrt", "sich", "um", ",", "und", "seuf\u00b7zet", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "PRF", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und als der Unterthanen Schar,", "tokens": ["Und", "als", "der", "Un\u00b7ter\u00b7tha\u00b7nen", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sonst in Furcht gesetzt vor seinem Anblick war,", "tokens": ["Die", "sonst", "in", "Furcht", "ge\u00b7setzt", "vor", "sei\u00b7nem", "An\u00b7blick", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVPP", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den m\u00e4chtigen Monarchen da", "tokens": ["Den", "m\u00e4ch\u00b7ti\u00b7gen", "Mon\u00b7ar\u00b7chen", "da"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun still, und ohne Kraft, am Bache liegen sah;", "tokens": ["Nun", "still", ",", "und", "oh\u00b7ne", "Kraft", ",", "am", "Ba\u00b7che", "lie\u00b7gen", "sah", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "KON", "APPR", "NN", "$,", "APPRART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da gingen ihrer viel', und forderten ihn aus.", "tokens": ["Da", "gin\u00b7gen", "ih\u00b7rer", "viel'", ",", "und", "for\u00b7der\u00b7ten", "ihn", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Ein Schimmel sagte: ", "tokens": ["Ein", "Schim\u00b7mel", "sag\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Ging r\u00fcckw\u00e4rts auf ihn los,", "tokens": ["Ging", "r\u00fcck\u00b7w\u00e4rts", "auf", "ihn", "los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und schmi\u00df ihn mit dem Huf;", "tokens": ["Und", "schmi\u00df", "ihn", "mit", "dem", "Huf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ein Stier versetzt' ihm einen Sto\u00df,", "tokens": ["Ein", "Stier", "ver\u00b7setzt'", "ihm", "ei\u00b7nen", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein Wolf bi\u00df ihn!", "tokens": ["Ein", "Wolf", "bi\u00df", "ihn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Herr K\u00f6nig, dein Beruf", "tokens": ["Herr", "K\u00f6\u00b7nig", ",", "dein", "Be\u00b7ruf"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Ist, dich zu wehren; wehre dich!", "tokens": ["Ist", ",", "dich", "zu", "weh\u00b7ren", ";", "weh\u00b7re", "dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PPER", "PTKZU", "VVINF", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er kann nicht, er bereitet sich", "tokens": ["Er", "kann", "nicht", ",", "er", "be\u00b7rei\u00b7tet", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "$,", "PPER", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum nahen Tode. Traurig, stumm,", "tokens": ["Zum", "na\u00b7hen", "To\u00b7de", ".", "Trau\u00b7rig", ",", "stumm", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sieht er sich um,", "tokens": ["Sieht", "er", "sich", "um", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und hat, im Innersten der Seele,", "tokens": ["Und", "hat", ",", "im", "In\u00b7ners\u00b7ten", "der", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Schon Abschied von der Welt genommen,", "tokens": ["Schon", "Ab\u00b7schied", "von", "der", "Welt", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So wie von seiner H\u00f6hle;", "tokens": ["So", "wie", "von", "sei\u00b7ner", "H\u00f6h\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Schon stirbt er still!", "tokens": ["Schon", "stirbt", "er", "still", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ach! aber ach! Zu seiner Qual,", "tokens": ["Ach", "!", "a\u00b7ber", "ach", "!", "Zu", "sei\u00b7ner", "Qual", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADV", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sieht er von weiten her den Esel kommen,", "tokens": ["Sieht", "er", "von", "wei\u00b7ten", "her", "den", "E\u00b7sel", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "APZR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der endlich auch an ihm zum Ritter werden will;", "tokens": ["Der", "end\u00b7lich", "auch", "an", "ihm", "zum", "Rit\u00b7ter", "wer\u00b7den", "will", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "PPER", "APPRART", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}