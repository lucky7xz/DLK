{"dta.poem.8136": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O sie kommen herauf! M\u00fchsam wandelten sie", "tokens": ["O", "sie", "kom\u00b7men", "her\u00b7auf", "!", "M\u00fch\u00b7sam", "wan\u00b7del\u00b7ten", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPER", "VVFIN", "PTKVZ", "$.", "NE", "VVFIN", "PPER"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "In des Tods bangem Nachtpfad; gl\u00fcckliche, befreyt,", "tokens": ["In", "des", "Tods", "ban\u00b7gem", "Nacht\u00b7pfad", ";", "gl\u00fcck\u00b7li\u00b7che", ",", "be\u00b7freyt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$.", "ADJA", "$,", "VVPP", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Entfl\u00f6hn sind sie weit weg vom Elend! und Entz\u00fcckung", "tokens": ["Ent\u00b7fl\u00f6hn", "sind", "sie", "weit", "weg", "vom", "E\u00b7lend", "!", "und", "Ent\u00b7z\u00fc\u00b7ckung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "ADV", "APPRART", "NN", "$.", "KON", "NN"], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ist ihr Weinen da herauf, Wehmut himlischer Ruh!", "tokens": ["Ist", "ihr", "Wei\u00b7nen", "da", "her\u00b7auf", ",", "Weh\u00b7mut", "him\u00b7li\u00b7scher", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$,", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}}}}