{"dta.poem.21192": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O frommer Alter, zeig uns dann", "tokens": ["O", "from\u00b7mer", "Al\u00b7ter", ",", "zeig", "uns", "dann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die theure Stelle, wo der Pfahl gestanden,", "tokens": ["Die", "theu\u00b7re", "Stel\u00b7le", ",", "wo", "der", "Pfahl", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Woran den Gottmensch M\u00f6rder banden;", "tokens": ["Wo\u00b7ran", "den", "Gott\u00b7mensch", "M\u00f6r\u00b7der", "ban\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Damit ich fromm die H\u00e4nde", "tokens": ["Da\u00b7mit", "ich", "fromm", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von da gen Himmel breite,", "tokens": ["Von", "da", "gen", "Him\u00b7mel", "brei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und die Gel\u00fcbde ganz vollende,", "tokens": ["Und", "die", "Ge\u00b7l\u00fcb\u00b7de", "ganz", "voll\u00b7en\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit welchen ich dem Herrn mich weihte.", "tokens": ["Mit", "wel\u00b7chen", "ich", "dem", "Herrn", "mich", "weih\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPER", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Aberglaube gab mir nicht", "tokens": ["Der", "A\u00b7berg\u00b7lau\u00b7be", "gab", "mir", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Den Pilgerstab zu dieser Reise;", "tokens": ["Den", "Pil\u00b7ger\u00b7stab", "zu", "die\u00b7ser", "Rei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ich weis, der wahre Christ", "tokens": ["Ich", "weis", ",", "der", "wah\u00b7re", "Christ"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Kan, ohne diese Wallfahrt anzutreten,", "tokens": ["Kan", ",", "oh\u00b7ne", "die\u00b7se", "Wall\u00b7fahrt", "an\u00b7zu\u00b7tre\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "KOUI", "PDAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "So feuriger, so frommer Weise", "tokens": ["So", "feu\u00b7ri\u00b7ger", ",", "so", "from\u00b7mer", "Wei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Zu seinem Heiland aller Orten beten,", "tokens": ["Zu", "sei\u00b7nem", "Hei\u00b7land", "al\u00b7ler", "Or\u00b7ten", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Als wir auf Golgatha;", "tokens": ["Als", "wir", "auf", "Gol\u00b7ga\u00b7tha", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Doch sollte nicht die H\u00f6h,", "tokens": ["Doch", "soll\u00b7te", "nicht", "die", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Worauf das gr\u00f6\u00dfte Wunderwerk geschah,", "tokens": ["Wo\u00b7rauf", "das", "gr\u00f6\u00df\u00b7te", "Wun\u00b7der\u00b7werk", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Der Wandrer fromme Neugier mehr verdienen,", "tokens": ["Der", "Wand\u00b7rer", "from\u00b7me", "Neu\u00b7gier", "mehr", "ver\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Als alle pr\u00e4chtigen Ruinen", "tokens": ["Als", "al\u00b7le", "pr\u00e4ch\u00b7ti\u00b7gen", "Ru\u00b7i\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Der K\u00f6nigsgr\u00e4ber, und der stolzen Mauren,", "tokens": ["Der", "K\u00f6\u00b7nigs\u00b7gr\u00e4\u00b7ber", ",", "und", "der", "stol\u00b7zen", "Mau\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Mit Menschenblut erbaut,", "tokens": ["Mit", "Men\u00b7schen\u00b7blut", "er\u00b7baut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Die, tiefgest\u00fcrzt, nunmehr im Staube trauren?", "tokens": ["Die", ",", "tief\u00b7ge\u00b7st\u00fcrzt", ",", "nun\u00b7mehr", "im", "Stau\u00b7be", "trau\u00b7ren", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "$,", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}