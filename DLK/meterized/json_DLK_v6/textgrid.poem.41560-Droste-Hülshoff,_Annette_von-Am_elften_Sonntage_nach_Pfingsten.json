{"textgrid.poem.41560": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am elften Sonntage nach Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Jesus hat geweint um seine Stadt,", "tokens": ["Mein", "Je\u00b7sus", "hat", "ge\u00b7weint", "um", "sei\u00b7ne", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ach, auch gewi\u00df um mich hat er geweinet;", "tokens": ["Ach", ",", "auch", "ge\u00b7wi\u00df", "um", "mich", "hat", "er", "ge\u00b7wei\u00b7net", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADV", "APPR", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wu\u00dft' er nicht damals schon, wie tr\u00fcb und matt,", "tokens": ["Wu\u00dft'", "er", "nicht", "da\u00b7mals", "schon", ",", "wie", "tr\u00fcb", "und", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "$,", "PWAV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie h\u00fclflos meine Seele heut erscheinet?", "tokens": ["Wie", "h\u00fcl\u00b7flos", "mei\u00b7ne", "See\u00b7le", "heut", "er\u00b7schei\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von allem was die heil'ge Bibel tr\u00e4gt", "tokens": ["Von", "al\u00b7lem", "was", "die", "heil'\u00b7ge", "Bi\u00b7bel", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "PWS", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Hat nichts so tief, so r\u00fchrend mich bewegt.", "tokens": ["Hat", "nichts", "so", "tief", ",", "so", "r\u00fch\u00b7rend", "mich", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADJD", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O, k\u00f6nnt' ich seine teuren Tr\u00e4nen nur", "tokens": ["O", ",", "k\u00f6nnt'", "ich", "sei\u00b7ne", "teu\u00b7ren", "Tr\u00e4\u00b7nen", "nur"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In einem Kelche, einem Tuche fassen,", "tokens": ["In", "ei\u00b7nem", "Kel\u00b7che", ",", "ei\u00b7nem", "Tu\u00b7che", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie er Veroniken die heil'ge Spur", "tokens": ["Wie", "er", "Ve\u00b7ro\u00b7ni\u00b7ken", "die", "heil'\u00b7ge", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Von seinem blut'gen Antlitz wollte lassen;", "tokens": ["Von", "sei\u00b7nem", "blut'\u00b7gen", "Ant\u00b7litz", "woll\u00b7te", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie war die Hochbegnadete vom Herrn,", "tokens": ["Sie", "war", "die", "Hoch\u00b7be\u00b7gna\u00b7de\u00b7te", "vom", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch auch der \u00e4rmste Bettler tr\u00e4umt ja gern!", "tokens": ["Doch", "auch", "der", "\u00e4rms\u00b7te", "Bett\u00b7ler", "tr\u00e4umt", "ja", "gern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zu solchem Kelche g\u00e4b' ich freudig her", "tokens": ["Zu", "sol\u00b7chem", "Kel\u00b7che", "g\u00e4b'", "ich", "freu\u00b7dig", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was ich an kleinen Sch\u00e4tzen mag besitzen;", "tokens": ["Was", "ich", "an", "klei\u00b7nen", "Sch\u00e4t\u00b7zen", "mag", "be\u00b7sit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von meinem Golde w\u00fcrd' er reich und schwer,", "tokens": ["Von", "mei\u00b7nem", "Gol\u00b7de", "w\u00fcrd'", "er", "reich", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und meine Edelsteine sollten blitzen.", "tokens": ["Und", "mei\u00b7ne", "E\u00b7del\u00b7stei\u00b7ne", "soll\u00b7ten", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O z\u00fcrne, Herr, nicht meiner Albernheit,", "tokens": ["O", "z\u00fcr\u00b7ne", ",", "Herr", ",", "nicht", "mei\u00b7ner", "Al\u00b7bern\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "$,", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zum Kinde macht mich deine G\u00fcte heut!", "tokens": ["Zum", "Kin\u00b7de", "macht", "mich", "dei\u00b7ne", "G\u00fc\u00b7te", "heut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbweh w\u00fc\u00dftest du, was dir zur Rettung ist!\u00ab", "tokens": ["\u00bb", "weh", "w\u00fc\u00df\u00b7test", "du", ",", "was", "dir", "zur", "Ret\u00b7tung", "ist", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "APPRART", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ja w\u00fc\u00dft' ich es, wohl w\u00e4r' es mir zum Frommen!", "tokens": ["Ja", "w\u00fc\u00dft'", "ich", "es", ",", "wohl", "w\u00e4r'", "es", "mir", "zum", "From\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "PPER", "$,", "ADV", "VAFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch du, du wei\u00dft es ja mein Jesus Christ,", "tokens": ["Doch", "du", ",", "du", "wei\u00dft", "es", "ja", "mein", "Je\u00b7sus", "Christ", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "NE", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und nur von dir kann mir die Kunde kommen,", "tokens": ["Und", "nur", "von", "dir", "kann", "mir", "die", "Kun\u00b7de", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So rede denn, du meines Herzens Hort!", "tokens": ["So", "re\u00b7de", "denn", ",", "du", "mei\u00b7nes", "Her\u00b7zens", "Hort", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich stehe hier und horche auf dein Wort.", "tokens": ["Ich", "ste\u00b7he", "hier", "und", "hor\u00b7che", "auf", "dein", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "F\u00fcrwahr ich mu\u00df in deinem heil'gen Buch", "tokens": ["F\u00fcr\u00b7wahr", "ich", "mu\u00df", "in", "dei\u00b7nem", "heil'\u00b7gen", "Buch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Viel mehr nach deiner Liebe Zeichen suchen,", "tokens": ["Viel", "mehr", "nach", "dei\u00b7ner", "Lie\u00b7be", "Zei\u00b7chen", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als wo dein Eifer spricht und, weh! dein Fluch!", "tokens": ["Als", "wo", "dein", "Ei\u00b7fer", "spricht", "und", ",", "weh", "!", "dein", "Fluch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPOSAT", "NN", "VVFIN", "KON", "$,", "PTKVZ", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich knicke wie ein Halm, h\u00f6r' ich dich fluchen;", "tokens": ["Ich", "kni\u00b7cke", "wie", "ein", "Halm", ",", "h\u00f6r'", "ich", "dich", "flu\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$,", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nicht heilsam aufger\u00fcttelt, todesmatt", "tokens": ["Nicht", "heil\u00b7sam", "auf\u00b7ge\u00b7r\u00fct\u00b7telt", ",", "to\u00b7des\u00b7matt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PTKNEG", "ADJD", "VVPP", "$,", "KOUI"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Lieg' ich am Grunde wie ein d\u00fcrres Blatt.", "tokens": ["Lieg'", "ich", "am", "Grun\u00b7de", "wie", "ein", "d\u00fcr\u00b7res", "Blatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ein saftlos Erdreich bin ich, dem nicht mag", "tokens": ["Ein", "saft\u00b7los", "Er\u00b7dreich", "bin", "ich", ",", "dem", "nicht", "mag"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VAFIN", "PPER", "$,", "PRELS", "PTKNEG", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Kalkes Brand, der Asche Beize taugen;", "tokens": ["Des", "Kal\u00b7kes", "Brand", ",", "der", "A\u00b7sche", "Bei\u00b7ze", "tau\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein d\u00fcrrer Sand treib' ich dem Winde nach:", "tokens": ["Ein", "d\u00fcr\u00b7rer", "Sand", "treib'", "ich", "dem", "Win\u00b7de", "nach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So will ich deine Himmelstropfen saugen,", "tokens": ["So", "will", "ich", "dei\u00b7ne", "Him\u00b7mel\u00b7strop\u00b7fen", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und in dem Tranke gibst du mir vielleicht", "tokens": ["Und", "in", "dem", "Tran\u00b7ke", "gibst", "du", "mir", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Was meinem irrenden Bewu\u00dftsein reicht.", "tokens": ["Was", "mei\u00b7nem", "ir\u00b7ren\u00b7den", "Be\u00b7wu\u00df\u00b7tsein", "reicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Gibst mir ins Herz was ich beginnen soll,", "tokens": ["Gibst", "mir", "ins", "Herz", "was", "ich", "be\u00b7gin\u00b7nen", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ob trauernd stehn, ob hoffend f\u00fcrder schreiten,", "tokens": ["Ob", "trau\u00b7ernd", "stehn", ",", "ob", "hof\u00b7fend", "f\u00fcr\u00b7der", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVINF", "$,", "KOUS", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Gnade ist ja nicht der St\u00e4rke Zoll,", "tokens": ["Die", "Gna\u00b7de", "ist", "ja", "nicht", "der", "St\u00e4r\u00b7ke", "Zoll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auch zu dem Siechen mag sie niedergleiten.", "tokens": ["Auch", "zu", "dem", "Sie\u00b7chen", "mag", "sie", "nie\u00b7derg\u00b7lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du der des Allerschw\u00e4chsten Sch\u00f6pfer bist,", "tokens": ["Du", "der", "des", "Al\u00b7ler\u00b7schw\u00e4chs\u00b7ten", "Sch\u00f6p\u00b7fer", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Hast auch f\u00fcr ihn ein Heil, mein Jesu Christ!", "tokens": ["Hast", "auch", "f\u00fcr", "ihn", "ein", "Heil", ",", "mein", "Je\u00b7su", "Christ", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "ART", "NN", "$,", "PPOSAT", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Drum, wenn die Wolke wieder mich umgibt", "tokens": ["Drum", ",", "wenn", "die", "Wol\u00b7ke", "wie\u00b7der", "mich", "um\u00b7gibt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "KOUS", "ART", "NN", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und fast verzweifelnd meine Arm' ermatten,", "tokens": ["Und", "fast", "ver\u00b7zwei\u00b7felnd", "mei\u00b7ne", "Arm'", "er\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann will ich denken, da\u00df er hat geliebt,", "tokens": ["Dann", "will", "ich", "den\u00b7ken", ",", "da\u00df", "er", "hat", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und meine Wimper heben durch die Schatten.", "tokens": ["Und", "mei\u00b7ne", "Wim\u00b7per", "he\u00b7ben", "durch", "die", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.pen