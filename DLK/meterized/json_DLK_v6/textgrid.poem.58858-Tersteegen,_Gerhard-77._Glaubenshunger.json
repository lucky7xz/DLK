{"textgrid.poem.58858": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "77. Glaubenshunger", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Hunger, der von Welt und S\u00fcnden", "tokens": ["Der", "Hun\u00b7ger", ",", "der", "von", "Welt", "und", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich nur in Jesus kehret ein,", "tokens": ["Sich", "nur", "in", "Je\u00b7sus", "keh\u00b7ret", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird Gnade, Kraft und Ruhe finden;", "tokens": ["Wird", "Gna\u00b7de", ",", "Kraft", "und", "Ru\u00b7he", "fin\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dies soll, o Seel', dein Glaube sein.", "tokens": ["Dies", "soll", ",", "o", "Seel'", ",", "dein", "Glau\u00b7be", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}