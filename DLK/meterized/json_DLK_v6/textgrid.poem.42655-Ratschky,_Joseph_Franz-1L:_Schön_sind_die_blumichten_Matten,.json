{"textgrid.poem.42655": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00f6n sind die blumichten Matten,", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n sind die blumichten Matten,", "tokens": ["Sch\u00f6n", "sind", "die", "blu\u00b7mich\u00b7ten", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Hold ist das bl\u00fchende Reis,", "tokens": ["Hold", "ist", "das", "bl\u00fc\u00b7hen\u00b7de", "Reis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Mild sind, im k\u00fchlenden Schatten", "tokens": ["Mild", "sind", ",", "im", "k\u00fch\u00b7len\u00b7den", "Schat\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "APPRART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Gaukelnd, die L\u00fcfte des Mays.", "tokens": ["Gau\u00b7kelnd", ",", "die", "L\u00fcf\u00b7te", "des", "Mays", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "ART", "NE", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Aber dir weichen, o Beste!", "tokens": ["A\u00b7ber", "dir", "wei\u00b7chen", ",", "o", "Bes\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$,", "FM", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Blumen und bl\u00fchendes Reis,", "tokens": ["Blu\u00b7men", "und", "bl\u00fc\u00b7hen\u00b7des", "Reis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Weichet die Milde der Weste,", "tokens": ["Wei\u00b7chet", "die", "Mil\u00b7de", "der", "Wes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Weichet die Anmuth des Mays.", "tokens": ["Wei\u00b7chet", "die", "An\u00b7muth", "des", "Mays", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NE", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Und o mein Alles! an Treue", "tokens": ["Und", "o", "mein", "Al\u00b7les", "!", "an", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "FM", "PPOSAT", "PIS", "$.", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gleicht dir kein Weib in der Welt.", "tokens": ["Gleicht", "dir", "kein", "Weib", "in", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Arm bist du zwar: doch ich freye", "tokens": ["Arm", "bist", "du", "zwar", ":", "doch", "ich", "frey\u00b7e"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$.", "KON", "PPER", "ADJA"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Weder nach W\u00fcrde, noch Geld.", "tokens": ["We\u00b7der", "nach", "W\u00fcr\u00b7de", ",", "noch", "Geld", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "ADV", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "M\u00fcsst' ich auch alles ertragen,", "tokens": ["M\u00fcsst'", "ich", "auch", "al\u00b7les", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "W\u00e4hlen den schm\u00e4hlichsten Stand,", "tokens": ["W\u00e4h\u00b7len", "den", "schm\u00e4h\u00b7lichs\u00b7ten", "Stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Br\u00fcdern und Freunden entsagen,", "tokens": ["Br\u00fc\u00b7dern", "und", "Freun\u00b7den", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Fliehen mein m\u00fctterlich Land;", "tokens": ["Flie\u00b7hen", "mein", "m\u00fct\u00b7ter\u00b7lich", "Land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "M\u00fcsst' ich in Wildnissen wohnen,", "tokens": ["M\u00fcsst'", "ich", "in", "Wild\u00b7nis\u00b7sen", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "H\u00e4tt' ich zur d\u00fcrftigen Kost", "tokens": ["H\u00e4tt'", "ich", "zur", "d\u00fcrf\u00b7ti\u00b7gen", "Kost"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "T\u00e4glich nur Wurzel und Bohnen,", "tokens": ["T\u00e4g\u00b7lich", "nur", "Wur\u00b7zel", "und", "Boh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Alles ertr\u00fcg' ich getrost.", "tokens": ["Al\u00b7les", "er\u00b7tr\u00fcg'", "ich", "ge\u00b7trost", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Alles ertr\u00fcg' ich zufrieden;", "tokens": ["Al\u00b7les", "er\u00b7tr\u00fcg'", "ich", "zu\u00b7frie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Denn was dem Gl\u00fccklichsten hier", "tokens": ["Denn", "was", "dem", "Gl\u00fcck\u00b7lichs\u00b7ten", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Je das Verh\u00e4ngniss beschieden,", "tokens": ["Je", "das", "Ver\u00b7h\u00e4ng\u00b7niss", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Alles das fand ich in dir.", "tokens": ["Al\u00b7les", "das", "fand", "ich", "in", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PDS", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Drum sollt' ich je dich verlassen,", "tokens": ["Drum", "sollt'", "ich", "je", "dich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dich, die allein mir gef\u00e4llt,", "tokens": ["Dich", ",", "die", "al\u00b7lein", "mir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "PPER", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Dann mag der Himmel mich hassen,", "tokens": ["Dann", "mag", "der", "Him\u00b7mel", "mich", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und mich verachten die Welt.", "tokens": ["Und", "mich", "ver\u00b7ach\u00b7ten", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}