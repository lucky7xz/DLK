{"dta.poem.14481": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abend .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Der Abend streut rosige Flocken,", "tokens": ["Der", "A\u00b7bend", "streut", "ro\u00b7si\u00b7ge", "Flo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Verh\u00fcllet die Erde nun ganz,", "tokens": ["Ver\u00b7h\u00fcl\u00b7let", "die", "Er\u00b7de", "nun", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und durch des Schlummernden Locken", "tokens": ["Und", "durch", "des", "Schlum\u00b7mern\u00b7den", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zieh'n Sterne den heiligen Kranz.", "tokens": ["Zieh'n", "Ster\u00b7ne", "den", "hei\u00b7li\u00b7gen", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}