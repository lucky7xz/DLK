{"textgrid.poem.25511": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie manche Freud' in ihr verborgen liegt", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie manche Freud' in ihr verborgen liegt", "tokens": ["Wie", "man\u00b7che", "Freud'", "in", "ihr", "ver\u00b7bor\u00b7gen", "liegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "APPR", "PPER", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr unser Aug'! Erschaffend, hat sie Leben", "tokens": ["F\u00fcr", "un\u00b7ser", "Aug'", "!", "Er\u00b7schaf\u00b7fend", ",", "hat", "sie", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "VVPP", "$,", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Traube, die den Vogel selbst betr\u00fcgt,", "tokens": ["Der", "Trau\u00b7be", ",", "die", "den", "Vo\u00b7gel", "selbst", "be\u00b7tr\u00fcgt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Rose, die die Nase t\u00e4uscht, gegeben.", "tokens": ["Der", "Ro\u00b7se", ",", "die", "die", "Na\u00b7se", "t\u00e4uscht", ",", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Welch ein Genu\u00df liegt nicht in ihr versteckt", "tokens": ["Welch", "ein", "Ge\u00b7nu\u00df", "liegt", "nicht", "in", "ihr", "ver\u00b7steckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "VVFIN", "PTKNEG", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr unser Ohr! Gern h\u00f6ren selbst die M\u00fcden,", "tokens": ["F\u00fcr", "un\u00b7ser", "Ohr", "!", "Gern", "h\u00f6\u00b7ren", "selbst", "die", "M\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ber\u00fchret sie die Harfe, sich geweckt,", "tokens": ["Be\u00b7r\u00fch\u00b7ret", "sie", "die", "Har\u00b7fe", ",", "sich", "ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Denn sie verleiht dem Herzen tiefen Frieden.", "tokens": ["Denn", "sie", "ver\u00b7leiht", "dem", "Her\u00b7zen", "tie\u00b7fen", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "De\u00df freuet sich mit Andern Amarant;", "tokens": ["De\u00df", "freu\u00b7et", "sich", "mit", "An\u00b7dern", "A\u00b7ma\u00b7rant", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Noch froher kann durch sie nur Einer werden.", "tokens": ["Noch", "fro\u00b7her", "kann", "durch", "sie", "nur", "Ei\u00b7ner", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "APPR", "PPER", "ADV", "PIS", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}}}}}