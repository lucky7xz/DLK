{"dta.poem.18919": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Traum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sah in meinem schlaff ein bild gleich einem", "tokens": ["Ich", "sah", "in", "mei\u00b7nem", "schlaff", "ein", "bild", "gleich", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "VVFIN", "ART", "NN", "ADV", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Got/", "tokens": ["Got", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Auff einem reichen thron gantz pr\u00e4chtiglich er-", "tokens": ["Auff", "ei\u00b7nem", "rei\u00b7chen", "thron", "gantz", "pr\u00e4ch\u00b7tig\u00b7lich", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "haben/", "tokens": ["ha\u00b7ben", "/"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "In dessen dienst vn\u0303 schutz zugleich au\u00df lust vn\u0303 noht", "tokens": ["In", "des\u00b7sen", "dienst", "v\u00f1", "schutz", "zu\u00b7gleich", "au\u00df", "lust", "v\u00f1", "noht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sich die Torrechte leut stehts hauffen wei\u00df begabe\u0304.", "tokens": ["Sich", "die", "Tor\u00b7rech\u00b7te", "leut", "stehts", "hauf\u00b7fen", "wei\u00df", "be\u00b7gab\u0113", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVFIN", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "--++-+-+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Ich sah wie dieses bild dem wahren Got zu spot", "tokens": ["Ich", "sah", "wie", "die\u00b7ses", "bild", "dem", "wah\u00b7ren", "Got", "zu", "spot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "PDAT", "NN", "ART", "ADJA", "NN", "APPR", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Empfieng (zwar niemahl sat) gel\u00fcbd/ lob/ opfer-", "tokens": ["Emp\u00b7fi\u00b7eng", "(", "zwar", "nie\u00b7mahl", "sat", ")", "ge\u00b7l\u00fcbd", "/", "lob", "/", "op\u00b7fer"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NE", "$(", "ADV", "ADV", "VVFIN", "$(", "VVPP", "$(", "NN", "$(", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "gaben;", "tokens": ["ga\u00b7ben", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Vnd gab auch wem es wolt das leben vnd de\u0304 tod/", "tokens": ["Vnd", "gab", "auch", "wem", "es", "wolt", "das", "le\u00b7ben", "vnd", "d\u0113", "tod", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PWS", "PPER", "VMFIN", "PDS", "VVINF", "KON", "NE", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vnd pflage sich mit raach vnd bo\u00dfheit zuerlaben.", "tokens": ["Vnd", "pfla\u00b7ge", "sich", "mit", "raach", "vnd", "bo\u00df\u00b7heit", "zu\u00b7er\u00b7la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vnd ob der him\u0303el schon offtmahl des bilds vndanck", "tokens": ["Vnd", "ob", "der", "him\u0303el", "schon", "offt\u00b7mahl", "des", "bilds", "vn\u00b7danck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zustrafen/ seine stern versamlete mit wunder;", "tokens": ["Zu\u00b7stra\u00b7fen", "/", "sei\u00b7ne", "stern", "ver\u00b7sam\u00b7le\u00b7te", "mit", "wun\u00b7der", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So war doch des bildes stim noch lauter dan der", "tokens": ["So", "war", "doch", "des", "bil\u00b7des", "stim", "noch", "lau\u00b7ter", "dan", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "VVFIN", "ADV", "ADJD", "ADV", "ART"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "dunder:", "tokens": ["dun\u00b7der", ":"], "token_info": ["word", "punct"], "pos": ["ADJA", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Bi\u00df endlich/ als sein stoltz war in de\u0304 h\u00f6chsten schwa\u0303ck/", "tokens": ["Bi\u00df", "end\u00b7lich", "/", "als", "sein", "stoltz", "war", "in", "d\u0113", "h\u00f6chs\u00b7ten", "schw\u00e3ck", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$(", "KOUS", "PPOSAT", "ADJD", "VAFIN", "APPR", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da schlug ein schneller plitz das sch\u00f6ne bild her-", "tokens": ["Da", "schlug", "ein", "schnel\u00b7ler", "plitz", "das", "sch\u00f6\u00b7ne", "bild", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "vnder/", "tokens": ["vn\u00b7der", "/"], "token_info": ["word", "punct"], "pos": ["KON", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Verkehrend seinen pracht in koht/ w\u00fcrm vnd ge-", "tokens": ["Ver\u00b7keh\u00b7rend", "sei\u00b7nen", "pracht", "in", "koht", "/", "w\u00fcrm", "vnd", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "PPOSAT", "NN", "APPR", "NN", "$(", "VAFIN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "stanck.", "tokens": ["stanck", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}}}}