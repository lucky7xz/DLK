{"textgrid.poem.55677": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn grauenhaft Dir meine Verse scheinen", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn grauenhaft Dir meine Verse scheinen", "tokens": ["Wenn", "grau\u00b7en\u00b7haft", "Dir", "mei\u00b7ne", "Ver\u00b7se", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ungelenk und w\u00fcst, o so verzeih'!", "tokens": ["Und", "un\u00b7ge\u00b7lenk", "und", "w\u00fcst", ",", "o", "so", "ver\u00b7zeih'", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "VVFIN", "$,", "FM", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du wei\u00dft es ja, es ist mit mir vorbei,", "tokens": ["Du", "wei\u00dft", "es", "ja", ",", "es", "ist", "mit", "mir", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und hast Du Lust, so kannst Du mich beweinen.", "tokens": ["Und", "hast", "Du", "Lust", ",", "so", "kannst", "Du", "mich", "be\u00b7wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "$,", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dir lagen offen meiner Seele Schl\u00fcnde,", "tokens": ["Dir", "la\u00b7gen", "of\u00b7fen", "mei\u00b7ner", "See\u00b7le", "Schl\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du hast mein reines Wollen ganz gesehn,", "tokens": ["Du", "hast", "mein", "rei\u00b7nes", "Wol\u00b7len", "ganz", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du sahst auch dann mein Ich zu nichts vergehn,", "tokens": ["Du", "sahst", "auch", "dann", "mein", "Ich", "zu", "nichts", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PPOSAT", "PPER", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zuerst im Elend, dann im Rausch der S\u00fcnde.", "tokens": ["Zu\u00b7erst", "im", "E\u00b7lend", ",", "dann", "im", "Rausch", "der", "S\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nimm diese Verse denn als Liebeszeichen,", "tokens": ["Nimm", "die\u00b7se", "Ver\u00b7se", "denn", "als", "Lie\u00b7bes\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "KON", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als letztes wehmutsvolles Gr\u00fc\u00dfen an,", "tokens": ["Als", "letz\u00b7tes", "weh\u00b7muts\u00b7vol\u00b7les", "Gr\u00fc\u00b7\u00dfen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von einem seelensiechen, armen Mann,", "tokens": ["Von", "ei\u00b7nem", "see\u00b7len\u00b7sie\u00b7chen", ",", "ar\u00b7men", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Alles oder gar nichts wollt' erreichen.", "tokens": ["Der", "Al\u00b7les", "o\u00b7der", "gar", "nichts", "wollt'", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "KON", "ADV", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}