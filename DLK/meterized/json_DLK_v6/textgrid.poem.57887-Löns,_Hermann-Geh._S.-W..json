{"textgrid.poem.57887": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Geh. S.-W.", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo sind die Sanit\u00e4tswachen geblieben,", "tokens": ["Wo", "sind", "die", "Sa\u00b7ni\u00b7t\u00e4ts\u00b7wa\u00b7chen", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Von denen man, wie mir deuchte, las,", "tokens": ["Von", "de\u00b7nen", "man", ",", "wie", "mir", "deuch\u00b7te", ",", "las", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da\u00df sie beschlossene Sache w\u00e4ren?", "tokens": ["Da\u00df", "sie", "be\u00b7schlos\u00b7se\u00b7ne", "Sa\u00b7che", "w\u00e4\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ist's wahr, da\u00df man sie total verga\u00df?", "tokens": ["Ist's", "wahr", ",", "da\u00df", "man", "sie", "to\u00b7tal", "ver\u00b7ga\u00df", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "KOUS", "PIS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Man bricht sich doch immer noch die Beine,", "tokens": ["Man", "bricht", "sich", "doch", "im\u00b7mer", "noch", "die", "Bei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "L\u00e4\u00dft immer noch fahren sich \u00fcber den Hals,", "tokens": ["L\u00e4\u00dft", "im\u00b7mer", "noch", "fah\u00b7ren", "sich", "\u00fc\u00b7ber", "den", "Hals", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Liegt damit dann lange auf der Stra\u00dfe \u2013", "tokens": ["Liegt", "da\u00b7mit", "dann", "lan\u00b7ge", "auf", "der", "Stra\u00b7\u00dfe", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gro\u00dfst\u00e4dtisch ist das keinesfalls.", "tokens": ["Gro\u00df\u00b7st\u00e4d\u00b7tisch", "ist", "das", "kei\u00b7nes\u00b7falls", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "PIAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was man beschlossen hat, zu errichten,", "tokens": ["Was", "man", "be\u00b7schlos\u00b7sen", "hat", ",", "zu", "er\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVPP", "VAFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Errichte man auch, und zwar m\u00f6glichst bald,", "tokens": ["Er\u00b7rich\u00b7te", "man", "auch", ",", "und", "zwar", "m\u00f6g\u00b7lichst", "bald", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "KON", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und nicht in d\u00fcrftiger, nein, wom\u00f6glich", "tokens": ["Und", "nicht", "in", "d\u00fcrf\u00b7ti\u00b7ger", ",", "nein", ",", "wo\u00b7m\u00f6g\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "$,", "PTKANT", "$,", "PWAV"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In allervollkommenster Gestalt.", "tokens": ["In", "al\u00b7ler\u00b7voll\u00b7kom\u00b7mens\u00b7ter", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Denn z\u00f6gert man noch ein Jahr oder zweie,", "tokens": ["Denn", "z\u00f6\u00b7gert", "man", "noch", "ein", "Jahr", "o\u00b7der", "zwei\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "ART", "NN", "KON", "CARD", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So wird vielleicht der Witz beliebt:", "tokens": ["So", "wird", "viel\u00b7leicht", "der", "Witz", "be\u00b7liebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geheime Sanit\u00e4tswachen gibt's in Hannover,", "tokens": ["Ge\u00b7hei\u00b7me", "Sa\u00b7ni\u00b7t\u00e4ts\u00b7wa\u00b7chen", "gibt's", "in", "Han\u00b7no\u00b7ver", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "NE", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So wie es geheime Sanit\u00e4tsr\u00e4te gibt.", "tokens": ["So", "wie", "es", "ge\u00b7hei\u00b7me", "Sa\u00b7ni\u00b7t\u00e4ts\u00b7r\u00e4\u00b7te", "gibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}