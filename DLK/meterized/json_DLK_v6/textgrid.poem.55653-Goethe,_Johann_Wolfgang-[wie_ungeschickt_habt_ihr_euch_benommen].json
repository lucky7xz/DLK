{"textgrid.poem.55653": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "[wie ungeschickt habt ihr euch benommen]", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwie ungeschickt habt ihr euch benommen,", "tokens": ["\u00bb", "wie", "un\u00b7ge\u00b7schickt", "habt", "ihr", "euch", "be\u00b7nom\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJD", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da euch das Gl\u00fcck ins Haus gekommen!\u00ab", "tokens": ["Da", "euch", "das", "Gl\u00fcck", "ins", "Haus", "ge\u00b7kom\u00b7men", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das M\u00e4dchen hat's nicht \u00fcbelgenommen", "tokens": ["Das", "M\u00e4d\u00b7chen", "hat's", "nicht", "\u00fc\u00b7bel\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und ist noch ein paarmal wiedergekommen.", "tokens": ["Und", "ist", "noch", "ein", "paar\u00b7mal", "wie\u00b7der\u00b7ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "ADV", "VVPP", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}