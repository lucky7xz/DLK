{"textgrid.poem.24340": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf eben dieselbe", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo der von Dessan sicht/ da spritzt der Feinde Blut.", "tokens": ["Wo", "der", "von", "Des\u00b7san", "sicht", "/", "da", "spritzt", "der", "Fein\u00b7de", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "APPR", "ART", "NN", "$(", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durch alles Feuer geht mit ihm der Preu\u00dfen Muht;", "tokens": ["Durch", "al\u00b7les", "Feu\u00b7er", "geht", "mit", "ihm", "der", "Preu\u00b7\u00dfen", "Muht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ihm der lange Krieg viel tausend Palmen bindet.", "tokens": ["Da\u00df", "ihm", "der", "lan\u00b7ge", "Krieg", "viel", "tau\u00b7send", "Pal\u00b7men", "bin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Tugend sprach: Er hat sich tapfer gnug erzeigt/", "tokens": ["Die", "Tu\u00b7gend", "sprach", ":", "Er", "hat", "sich", "tap\u00b7fer", "gnug", "er\u00b7zeigt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "PRF", "ADJD", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Ende sey gekr\u00f6nt. Worauff er M\u00f6ers ersteigt/", "tokens": ["Das", "En\u00b7de", "sey", "ge\u00b7kr\u00f6nt", ".", "Wo\u00b7rauff", "er", "M\u00f6ers", "er\u00b7steigt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "PWAV", "PPER", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und seines K\u00f6nigs Recht mit Klugheit \u00fcberwindet.", "tokens": ["Und", "sei\u00b7nes", "K\u00f6\u00b7nigs", "Recht", "mit", "Klug\u00b7heit", "\u00fc\u00b7berw\u00b7in\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}