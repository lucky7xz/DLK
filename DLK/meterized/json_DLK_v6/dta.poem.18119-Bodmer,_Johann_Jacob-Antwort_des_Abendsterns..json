{"dta.poem.18119": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Antwort des Abendsterns.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sonsten Hesperus, hab jetzund mich gewendet,", "tokens": ["Ich", "sons\u00b7ten", "Hes\u00b7pe\u00b7rus", ",", "hab", "je\u00b7tzund", "mich", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und werde Lucifer, ich bin vorher gesendet,", "tokens": ["Und", "wer\u00b7de", "Lu\u00b7ci\u00b7fer", ",", "ich", "bin", "vor\u00b7her", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NE", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sey nicht best\u00fcrtzt, da\u00df ich den alten Lauf verkehr,", "tokens": ["Sey", "nicht", "be\u00b7st\u00fcrtzt", ",", "da\u00df", "ich", "den", "al\u00b7ten", "Lauf", "ver\u00b7kehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil deine Sonne kommt, so geh ich f\u00fcr ihr her.", "tokens": ["Weil", "dei\u00b7ne", "Son\u00b7ne", "kommt", ",", "so", "geh", "ich", "f\u00fcr", "ihr", "her", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}