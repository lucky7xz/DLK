{"textgrid.poem.48102": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Der Knabe von Budapest", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbarbeit gebt uns und gebt uns Brot!", "tokens": ["\u00bb", "ar\u00b7beit", "gebt", "uns", "und", "gebt", "uns", "Brot", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "wir leiden K\u00e4lte, wir leiden Not:", "tokens": ["wir", "lei\u00b7den", "K\u00e4l\u00b7te", ",", "wir", "lei\u00b7den", "Not", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "wir haben ein Recht aufs Leben \u2013", "tokens": ["wir", "ha\u00b7ben", "ein", "Recht", "aufs", "Le\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "das Recht nur sollt ihr uns geben!\u00ab", "tokens": ["das", "Recht", "nur", "sollt", "ihr", "uns", "ge\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein Heer von abertausend Mann,", "tokens": ["Ein", "Heer", "von", "a\u00b7bert\u00b7au\u00b7send", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mit dr\u00f6hnenden Schritten r\u00fcckt es an,", "tokens": ["mit", "dr\u00f6h\u00b7nen\u00b7den", "Schrit\u00b7ten", "r\u00fcckt", "es", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "zur Seite ihm Gier und Schrecken \u2013", "tokens": ["zur", "Sei\u00b7te", "ihm", "Gier", "und", "Schre\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "NN", "KON", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "der Sturm rast um die Ecken.", "tokens": ["der", "Sturm", "rast", "um", "die", "E\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Schneesturm pfeift. Aus dem warmen Haus", "tokens": ["Der", "Schnee\u00b7sturm", "pfeift", ".", "Aus", "dem", "war\u00b7men", "Haus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "lugen Tschako und Helm heraus;", "tokens": ["lu\u00b7gen", "Tscha\u00b7ko", "und", "Helm", "he\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "im kalten Schneelicht blitzen", "tokens": ["im", "kal\u00b7ten", "Schnee\u00b7licht", "blit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "goldene Kn\u00f6pfe und Litzen.", "tokens": ["gol\u00b7de\u00b7ne", "Kn\u00f6p\u00b7fe", "und", "Lit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "\u2013 \u00bbZur\u00fcck! wir gaben euch Holz und Brot,", "tokens": ["\u2013", "\u00bb", "Zu\u00b7r\u00fcck", "!", "wir", "ga\u00b7ben", "euch", "Holz", "und", "Brot", ","], "token_info": ["punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "wir hatten Erbarmen mit eurer Not;", "tokens": ["wir", "hat\u00b7ten", "Er\u00b7bar\u00b7men", "mit", "eu\u00b7rer", "Not", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "doch ein Recht auf Arbeit und Leben,", "tokens": ["doch", "ein", "Recht", "auf", "Ar\u00b7beit", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "wer hat euch das gegeben? \u2013\u00ab", "tokens": ["wer", "hat", "euch", "das", "ge\u00b7ge\u00b7ben", "?", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PDS", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und l\u00e4hmende Stille. Ein Wetterschlag", "tokens": ["Und", "l\u00e4h\u00b7men\u00b7de", "Stil\u00b7le", ".", "Ein", "Wet\u00b7ter\u00b7schlag"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "$.", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "fuhr aus dem Himmel am k\u00e4ltesten Tag.", "tokens": ["fuhr", "aus", "dem", "Him\u00b7mel", "am", "k\u00e4l\u00b7tes\u00b7ten", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Dann \u2013 dr\u00f6hnend wie Donnergrollen:", "tokens": ["Dann", "\u2013", "dr\u00f6h\u00b7nend", "wie", "Don\u00b7ner\u00b7grol\u00b7len", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADJD", "KOKOM", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u00bbwir haben es, wenn wir wollen!\u00ab", "tokens": ["\u00bb", "wir", "ha\u00b7ben", "es", ",", "wenn", "wir", "wol\u00b7len", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "$,", "KOUS", "PPER", "VMFIN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Und h\u00e4mmernd das Blut in den Schl\u00e4fen braust,", "tokens": ["Und", "h\u00e4m\u00b7mernd", "das", "Blut", "in", "den", "Schl\u00e4\u00b7fen", "braust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "an dem T\u00fcrgriff r\u00fcttelt die Schwielenfaust,", "tokens": ["an", "dem", "T\u00fcr\u00b7griff", "r\u00fct\u00b7telt", "die", "Schwie\u00b7len\u00b7faust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "auf dem Estrich krachen die Sohlen:", "tokens": ["auf", "dem", "Est\u00b7rich", "kra\u00b7chen", "die", "Soh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "\u00bbwir werden das Recht uns holen!\u00ab", "tokens": ["\u00bb", "wir", "wer\u00b7den", "das", "Recht", "uns", "ho\u00b7len", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "\u2013 \u00bbZur\u00fcck! \u2013 Gewalt denn wider Gewalt!\u00ab \u2013", "tokens": ["\u2013", "\u00bb", "Zu\u00b7r\u00fcck", "!", "\u2013", "Ge\u00b7walt", "denn", "wi\u00b7der", "Ge\u00b7walt", "!", "\u00ab", "\u2013"], "token_info": ["punct", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "$(", "PTKVZ", "$.", "$(", "NN", "KON", "APPR", "NN", "$.", "$(", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und der S\u00e4bel klirrt und die B\u00fcchse knallt.", "tokens": ["Und", "der", "S\u00e4\u00b7bel", "klirrt", "und", "die", "B\u00fcch\u00b7se", "knallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbzur\u00fcck!\u00ab \u2013 und die St\u00fcrmenden weichen", "tokens": ["\u00bb", "zu\u00b7r\u00fcck", "!", "\u00ab", "\u2013", "und", "die", "St\u00fcr\u00b7men\u00b7den", "wei\u00b7chen"], "token_info": ["punct", "word", "punct", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKVZ", "$.", "$(", "$(", "KON", "ART", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "\u00fcber Wunde zur\u00fcck und Leichen.", "tokens": ["\u00fc\u00b7ber", "Wun\u00b7de", "zu\u00b7r\u00fcck", "und", "Lei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "KON", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Aufbr\u00fcllt die Menge vor Scham und Wut \u2013", "tokens": ["Auf\u00b7br\u00fcllt", "die", "Men\u00b7ge", "vor", "Scham", "und", "Wut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "und mitten in tosender K\u00e4mpferflut", "tokens": ["und", "mit\u00b7ten", "in", "to\u00b7sen\u00b7der", "K\u00e4mp\u00b7fer\u00b7flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "mit blonden, flatternden Haaren", "tokens": ["mit", "blon\u00b7den", ",", "flat\u00b7tern\u00b7den", "Haa\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "ein Knabe von dreizehn Jahren!", "tokens": ["ein", "Kna\u00b7be", "von", "drei\u00b7zehn", "Jah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Der hebt die wehrende Hand \u2013 ein Schrei! \u2013", "tokens": ["Der", "hebt", "die", "weh\u00b7ren\u00b7de", "Hand", "\u2013", "ein", "Schrei", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$(", "ART", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ein Stauen und Stampfen . . . nun ist's vorbei:", "tokens": ["ein", "Stau\u00b7en", "und", "Stamp\u00b7fen", ".", ".", ".", "nun", "ist's", "vor\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$.", "$.", "$.", "ADV", "NE", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "im Stra\u00dfengew\u00fchl zertreten!", "tokens": ["im", "Stra\u00b7\u00dfen\u00b7ge\u00b7w\u00fchl", "zer\u00b7tre\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und fluchende Lippen beten . . .", "tokens": ["Und", "flu\u00b7chen\u00b7de", "Lip\u00b7pen", "be\u00b7ten", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$.", "$.", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbund wenn das Recht von Gottes Hand", "tokens": ["\u00bb", "und", "wenn", "das", "Recht", "von", "Got\u00b7tes", "Hand"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "KOUS", "ART", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "genagelt w\u00e4r' an des Himmels Wand,", "tokens": ["ge\u00b7na\u00b7gelt", "w\u00e4r'", "an", "des", "Him\u00b7mels", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "so w\u00fcchsen uns Geierschwingen \u2013", "tokens": ["so", "w\u00fcch\u00b7sen", "uns", "Gei\u00b7er\u00b7schwin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "wir werden das Recht erringen!", "tokens": ["wir", "wer\u00b7den", "das", "Recht", "er\u00b7rin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Zur\u00fcck \u2013 f\u00fcr heute!! \u2013 was heulst du, Weib?", "tokens": ["Zu\u00b7r\u00fcck", "\u2013", "f\u00fcr", "heu\u00b7te", "!!", "\u2013", "was", "heulst", "du", ",", "Weib", "?"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKVZ", "$(", "APPR", "ADV", "$.", "$(", "PWS", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf diesem zuckenden Kinderleib", "tokens": ["Auf", "die\u00b7sem", "zu\u00b7cken\u00b7den", "Kin\u00b7der\u00b7leib"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "soll unser Recht auf das Leben,", "tokens": ["soll", "un\u00b7ser", "Recht", "auf", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "ein blutendes Mal, sich heben! \u2013\u00ab", "tokens": ["ein", "blu\u00b7ten\u00b7des", "Mal", ",", "sich", "he\u00b7ben", "!", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRF", "VVINF", "$.", "$(", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Des F\u00fchrers Wort grollt hart und hei\u00df;", "tokens": ["Des", "F\u00fch\u00b7rers", "Wort", "grollt", "hart", "und", "hei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und still wird es und leer im Kreis.", "tokens": ["und", "still", "wird", "es", "und", "leer", "im", "Kreis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er kniet im fegenden Winde", "tokens": ["Er", "kniet", "im", "fe\u00b7gen\u00b7den", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "bei seinem toten Kinde.", "tokens": ["bei", "sei\u00b7nem", "to\u00b7ten", "Kin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "In wehenden Wirbeln treibt der Schnee", "tokens": ["In", "we\u00b7hen\u00b7den", "Wir\u00b7beln", "treibt", "der", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und k\u00fchlt die Wunden und l\u00f6scht das Weh,", "tokens": ["und", "k\u00fchlt", "die", "Wun\u00b7den", "und", "l\u00f6scht", "das", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "