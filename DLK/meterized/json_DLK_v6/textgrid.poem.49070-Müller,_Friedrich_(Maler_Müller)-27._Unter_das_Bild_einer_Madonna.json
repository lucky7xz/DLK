{"textgrid.poem.49070": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "27. Unter das Bild einer Madonna", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, aller Frauen Glanz und Zier,", "tokens": ["Du", ",", "al\u00b7ler", "Frau\u00b7en", "Glanz", "und", "Zier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PIAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du, jeder Tugend Thron,", "tokens": ["Du", ",", "je\u00b7der", "Tu\u00b7gend", "Thron", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Maria! sei gesegnet hier", "tokens": ["Ma\u00b7ria", "!", "sei", "ge\u00b7seg\u00b7net", "hier"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "VAFIN", "VVPP", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit deinem holden Sohn!", "tokens": ["Mit", "dei\u00b7nem", "hol\u00b7den", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du trugst das zarte Opferlamm", "tokens": ["Du", "trugst", "das", "zar\u00b7te", "Op\u00b7fer\u00b7lamm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In deinem keuschen Schoos,", "tokens": ["In", "dei\u00b7nem", "keu\u00b7schen", "Schoos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Maria! Schmuck aus Davids Stamm,", "tokens": ["Ma\u00b7ria", "!", "Schmuck", "aus", "Da\u00b7vids", "Stamm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie selig war dein Loos!", "tokens": ["Wie", "se\u00b7lig", "war", "dein", "Loos", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Geschaffen zu der Menschheit Gl\u00fcck,", "tokens": ["Ge\u00b7schaf\u00b7fen", "zu", "der", "Menschheit", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Gr\u00fc\u00dft Sonn' und Mond dich laut,", "tokens": ["Gr\u00fc\u00dft", "Sonn'", "und", "Mond", "dich", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Maria! hohes Meisterst\u00fcck,", "tokens": ["Ma\u00b7ria", "!", "ho\u00b7hes", "Meis\u00b7ter\u00b7st\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Des Himmels s\u00fc\u00dfe Braut!", "tokens": ["Des", "Him\u00b7mels", "s\u00fc\u00b7\u00dfe", "Braut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du Stern der Hoffnung, leuchtend mild", "tokens": ["Du", "Stern", "der", "Hoff\u00b7nung", ",", "leuch\u00b7tend", "mild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "ART", "NN", "$,", "VVPP", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem S\u00fcnder in der Not;", "tokens": ["Dem", "S\u00fcn\u00b7der", "in", "der", "Not", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Maria! sichres Rettungsschild,", "tokens": ["Ma\u00b7ria", "!", "sich\u00b7res", "Ret\u00b7tungs\u00b7schild", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn uns das Ende droht!", "tokens": ["Wenn", "uns", "das", "En\u00b7de", "droht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O K\u00f6nigin der Engel, reich", "tokens": ["O", "K\u00f6\u00b7ni\u00b7gin", "der", "En\u00b7gel", ",", "reich"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "NN", "ART", "NN", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Gnade, Liebe, Huld;", "tokens": ["An", "Gna\u00b7de", ",", "Lie\u00b7be", ",", "Huld", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Maria! reinster Lilie gleich,", "tokens": ["Ma\u00b7ria", "!", "reins\u00b7ter", "Li\u00b7lie", "gleich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hilfst tragen unsre Schuld!", "tokens": ["Hilfst", "tra\u00b7gen", "uns\u00b7re", "Schuld", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Entzieh' uns nicht dein Angesicht,", "tokens": ["Ent\u00b7zieh'", "uns", "nicht", "dein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blick' huldreich du uns an;", "tokens": ["Blick'", "huld\u00b7reich", "du", "uns", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Maria! klares Seelenlicht", "tokens": ["Ma\u00b7ria", "!", "kla\u00b7res", "See\u00b7len\u00b7licht"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dunkler Lebensbahn!", "tokens": ["Auf", "dunk\u00b7ler", "Le\u00b7bens\u00b7bahn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Bitt' deinen Sohn f\u00fcr uns, da\u00df er", "tokens": ["Bitt'", "dei\u00b7nen", "Sohn", "f\u00fcr", "uns", ",", "da\u00df", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPER", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uns gn\u00e4dig immer sei;", "tokens": ["Uns", "gn\u00e4\u00b7dig", "im\u00b7mer", "sei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und von dem S\u00fcnderjoche schwer,", "tokens": ["Und", "von", "dem", "S\u00fcn\u00b7der\u00b7jo\u00b7che", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Voll Mitleid uns befrei'!", "tokens": ["Voll", "Mit\u00b7leid", "uns", "be\u00b7frei'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wir gr\u00fc\u00dfen dich, o Eigentum", "tokens": ["Wir", "gr\u00fc\u00b7\u00dfen", "dich", ",", "o", "Ei\u00b7gen\u00b7tum"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Glorie, in der Zeit;", "tokens": ["Der", "Glo\u00b7rie", ",", "in", "der", "Zeit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Engel singen deinen Ruhm", "tokens": ["Die", "En\u00b7gel", "sin\u00b7gen", "dei\u00b7nen", "Ruhm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In alle Ewigkeit!", "tokens": ["In", "al\u00b7le", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}