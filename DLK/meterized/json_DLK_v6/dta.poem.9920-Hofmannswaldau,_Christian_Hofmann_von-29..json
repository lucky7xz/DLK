{"dta.poem.9920": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die natur pfleg ich zu \u00e4ndern gleich mit meinem element/", "tokens": ["Die", "na\u00b7tur", "pfleg", "ich", "zu", "\u00e4n\u00b7dern", "gleich", "mit", "mei\u00b7nem", "e\u00b7le\u00b7ment", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Die vernunfft/ die stutzt hier\u00fcber/ wenn sie di\u00df an mir erken\u0303t;", "tokens": ["Die", "ver\u00b7nunfft", "/", "die", "stutzt", "hier\u00b7\u00fc\u00b7ber", "/", "wenn", "sie", "di\u00df", "an", "mir", "er\u00b7ke\u00f1t", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "VVFIN", "PAV", "$(", "KOUS", "PPER", "PDS", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Denn sie mercket/ da\u00df die sonne nur mit einem blicke kan", "tokens": ["Denn", "sie", "mer\u00b7cket", "/", "da\u00df", "die", "son\u00b7ne", "nur", "mit", "ei\u00b7nem", "bli\u00b7cke", "kan"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$(", "KOUS", "ART", "NN", "ADV", "APPR", "ART", "VVFIN", "VMFIN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Mich in felsen so verwandeln/ wie Meduse hat gethan.", "tokens": ["Mich", "in", "fel\u00b7sen", "so", "ver\u00b7wan\u00b7deln", "/", "wie", "Me\u00b7du\u00b7se", "hat", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADV", "VVINF", "$(", "KOKOM", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}