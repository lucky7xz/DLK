{"textgrid.poem.50962": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht da den Vogel mit gerupften Schwingen,", "tokens": ["Seht", "da", "den", "Vo\u00b7gel", "mit", "ge\u00b7rupf\u00b7ten", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Halb flattert er, halb h\u00fcpft er hin zum Neste,", "tokens": ["Halb", "flat\u00b7tert", "er", ",", "halb", "h\u00fcpft", "er", "hin", "zum", "Nes\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "ADJD", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich einzubaun in eine Liebesfeste,", "tokens": ["Sich", "ein\u00b7zu\u00b7baun", "in", "ei\u00b7ne", "Lie\u00b7bes\u00b7fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wohin kein rauhes L\u00fcftchen mehr soll dringen!", "tokens": ["Wo\u00b7hin", "kein", "rau\u00b7hes", "L\u00fcft\u00b7chen", "mehr", "soll", "drin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch war er gro\u00df und mochte Ruhm erringen,", "tokens": ["Doch", "war", "er", "gro\u00df", "und", "moch\u00b7te", "Ruhm", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "KON", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihm gr\u00fcnt' und bl\u00fcht' der Lorbeer auf das beste,", "tokens": ["Ihm", "gr\u00fcnt'", "und", "bl\u00fcht'", "der", "Lor\u00b7beer", "auf", "das", "bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In seinen Schatten lud er stolz die G\u00e4ste", "tokens": ["In", "sei\u00b7nen", "Schat\u00b7ten", "lud", "er", "stolz", "die", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und war so recht ein Thema zum Besingen.", "tokens": ["Und", "war", "so", "recht", "ein", "The\u00b7ma", "zum", "Be\u00b7sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nur als den Zweig dem freien Feld er raubte,", "tokens": ["Nur", "als", "den", "Zweig", "dem", "frei\u00b7en", "Feld", "er", "raub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus Luft und Sonne, drin er aufgeschossen,", "tokens": ["Aus", "Luft", "und", "Son\u00b7ne", ",", "drin", "er", "auf\u00b7ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sachte sich mit zu salvieren glaubte:", "tokens": ["Und", "sach\u00b7te", "sich", "mit", "zu", "sal\u00b7vie\u00b7ren", "glaub\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da war der Traum bald wie ein Schaum zerflossen,", "tokens": ["Da", "war", "der", "Traum", "bald", "wie", "ein", "Schaum", "zer\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Reis stand ab, das schon so gr\u00fcn belaubte \u2013", "tokens": ["Das", "Reis", "stand", "ab", ",", "das", "schon", "so", "gr\u00fcn", "be\u00b7laub\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PRELS", "ADV", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da geht er heim nun schl\u00e4frig und verdrossen.", "tokens": ["Da", "geht", "er", "heim", "nun", "schl\u00e4f\u00b7rig", "und", "ver\u00b7dros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "ADV", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}