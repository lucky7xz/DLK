{"textgrid.poem.39581": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Aufforderung", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Schwoabe, paut ein Monument", "tokens": ["Ihr", "Schwoa\u00b7be", ",", "paut", "ein", "Mo\u00b7nu\u00b7ment"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Schiller, den ihr Euer nennt!", "tokens": ["Dem", "Schil\u00b7ler", ",", "den", "ihr", "Eu\u00b7er", "nennt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "PPER", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn die\u00df, wie ich, die Teutsche wenschten,", "tokens": ["Wenn", "die\u00df", ",", "wie", "ich", ",", "die", "Teut\u00b7sche", "wenschten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "PWAV", "PPER", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So st\u00fchnde sie euch gern zu Tentschten.", "tokens": ["So", "st\u00fchn\u00b7de", "sie", "euch", "gern", "zu", "Tentschten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}