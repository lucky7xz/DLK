{"dta.poem.2383": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Betr\u00fcbter Himmel sag was du doch damit meinest/", "tokens": ["Be\u00b7tr\u00fcb\u00b7ter", "Him\u00b7mel", "sag", "was", "du", "doch", "da\u00b7mit", "mei\u00b7nest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PWS", "PPER", "ADV", "PAV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du so mildiglich an diesem Feste weinest?", "tokens": ["Da\u00df", "du", "so", "mil\u00b7dig\u00b7lich", "an", "die\u00b7sem", "Fes\u00b7te", "wei\u00b7nest", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df auch dein F\u00f6bus selbst ein graugew\u00f6lktes Kleid", "tokens": ["Da\u00df", "auch", "dein", "F\u00f6\u00b7bus", "selbst", "ein", "grau\u00b7ge\u00b7w\u00f6lk\u00b7tes", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um sich geh\u00fcllet hat vor grosser Traurigkeit?", "tokens": ["Um", "sich", "ge\u00b7h\u00fcl\u00b7let", "hat", "vor", "gros\u00b7ser", "Trau\u00b7rig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "VVPP", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dann ist die Noht erst gro\u00df wenn G\u00f6tter trauren m\u00fcssen!", "tokens": ["Dann", "ist", "die", "Noht", "erst", "gro\u00df", "wenn", "G\u00f6t\u00b7ter", "trau\u00b7ren", "m\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "KOUS", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie ietzt dein F\u00f6bus thut. Dr\u00fcm la\u00df es mich doch wissen", "tokens": ["Wie", "ietzt", "dein", "F\u00f6\u00b7bus", "thut", ".", "Dr\u00fcm", "la\u00df", "es", "mich", "doch", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VVFIN", "$.", "NN", "VVFIN", "PPER", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was dein Betr\u00fcbn\u00fc\u00df sey. Es steht ja alle Welt", "tokens": ["Was", "dein", "Be\u00b7tr\u00fcb\u00b7n\u00fc\u00df", "sey", ".", "Es", "steht", "ja", "al\u00b7le", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "VAFIN", "$.", "PPER", "VVFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In voller Fr\u00f6ligkeit/ weil Christus unser Held", "tokens": ["In", "vol\u00b7ler", "Fr\u00f6\u00b7lig\u00b7keit", "/", "weil", "Chris\u00b7tus", "un\u00b7ser", "Held"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "KOUS", "NE", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Aus seinem finstern Grab\u2019 ist wieder aufferstanden/", "tokens": ["Aus", "sei\u00b7nem", "fins\u00b7tern", "Grab'", "ist", "wie\u00b7der", "auf\u00b7fer\u00b7stan\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und also auffgel\u00f6st des Todes Todtenbanden;", "tokens": ["Und", "al\u00b7so", "auff\u00b7ge\u00b7l\u00f6st", "des", "To\u00b7des", "Tod\u00b7ten\u00b7ban\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Heut hat Er uns geschenkt Ergetzligkeit f\u00fcr Noht;", "tokens": ["Heut", "hat", "Er", "uns", "ge\u00b7schenkt", "Er\u00b7getz\u00b7lig\u00b7keit", "f\u00fcr", "Noht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "F\u00fcr Schrekken Freud\u2019 und Trost/ das Leben f\u00fcr den Tod;", "tokens": ["F\u00fcr", "Schrek\u00b7ken", "Freud'", "und", "Trost", "/", "das", "Le\u00b7ben", "f\u00fcr", "den", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$(", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er hat uns Sterblichen die hohen Himmelsth\u00fcren", "tokens": ["Er", "hat", "uns", "Sterb\u00b7li\u00b7chen", "die", "ho\u00b7hen", "Him\u00b7mel\u00b7sth\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Er\u00f6ffnet Angelweit wodurch Er uns wil f\u00fchren", "tokens": ["Er\u00b7\u00f6ff\u00b7net", "An\u00b7gel\u00b7weit", "wo\u00b7durch", "Er", "uns", "wil", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PWAV", "PPER", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "In sein ererbtes Reich. Und heut\u2019 ist die\u00df geschehn.", "tokens": ["In", "sein", "er\u00b7erb\u00b7tes", "Reich", ".", "Und", "heut'", "ist", "die\u00df", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "KON", "ADV", "VAFIN", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wer wolte denn wie du in solchem Leide stehn?", "tokens": ["Wer", "wol\u00b7te", "denn", "wie", "du", "in", "sol\u00b7chem", "Lei\u00b7de", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "KON", "PWAV", "PPER", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch halt! ich wei\u00df woher die milden Thr\u00e4nen rinnen/", "tokens": ["Doch", "halt", "!", "ich", "wei\u00df", "wo\u00b7her", "die", "mil\u00b7den", "Thr\u00e4\u00b7nen", "rin\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VVFIN", "PWAV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die du so heuffig weinst. Vielleicht weil wir nicht k\u00f6nnen/", "tokens": ["Die", "du", "so", "heuf\u00b7fig", "weinst", ".", "Viel\u00b7leicht", "weil", "wir", "nicht", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$.", "ADV", "KOUS", "PPER", "PTKNEG", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "(doch was nicht k\u00f6nnen! ja/ wir wollen leider nicht)", "tokens": ["(", "doch", "was", "nicht", "k\u00f6n\u00b7nen", "!", "ja", "/", "wir", "wol\u00b7len", "lei\u00b7der", "nicht", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWS", "PTKNEG", "VMFIN", "$.", "ADV", "$(", "PPER", "VMFIN", "ADV", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mit Dank erkennen das/ was unser Seelenliecht", "tokens": ["Mit", "Dank", "er\u00b7ken\u00b7nen", "das", "/", "was", "un\u00b7ser", "See\u00b7len\u00b7liecht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVINF", "ART", "$(", "PWS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der grosse Siegesf\u00fcrst uns diesen Tag erwiesen/", "tokens": ["Der", "gros\u00b7se", "Sie\u00b7ges\u00b7f\u00fcrst", "uns", "die\u00b7sen", "Tag", "er\u00b7wie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wof\u00fcr Er billich solt\u2019 im Hertzen seyn gepriesen/", "tokens": ["Wo\u00b7f\u00fcr", "Er", "bil\u00b7lich", "solt'", "im", "Hert\u00b7zen", "seyn", "ge\u00b7prie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VMFIN", "APPRART", "NN", "PPOSAT", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Weil sag\u2019 ich mancher mehr an dieser Osterzeit/", "tokens": ["Weil", "sag'", "ich", "man\u00b7cher", "mehr", "an", "die\u00b7ser", "Os\u00b7ter\u00b7zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PIS", "ADV", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Auff schn\u00f6de Luft bedacht als auff die Seligkeit.", "tokens": ["Auff", "schn\u00f6\u00b7de", "Luft", "be\u00b7dacht", "als", "auff", "die", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "KOKOM", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dr\u00fcm ist kein Wunder nicht/ weil wir ja so verblendet/", "tokens": ["Dr\u00fcm", "ist", "kein", "Wun\u00b7der", "nicht", "/", "weil", "wir", "ja", "so", "ver\u00b7blen\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "PTKNEG", "$(", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und hart verstokket sind/ da\u00df die Natur sich wendet", "tokens": ["Und", "hart", "ver\u00b7stok\u00b7ket", "sind", "/", "da\u00df", "die", "Na\u00b7tur", "sich", "wen\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "VAFIN", "$(", "KOUS", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da\u00df sich der Himmel selbst hier\u00fcber so entsetzt/", "tokens": ["Da\u00df", "sich", "der", "Him\u00b7mel", "selbst", "hier\u00b7\u00fc\u00b7ber", "so", "ent\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "PAV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und die verboste Welt mit vielen Z\u00e4hren netzt.", "tokens": ["Und", "die", "ver\u00b7bos\u00b7te", "Welt", "mit", "vie\u00b7len", "Z\u00e4h\u00b7ren", "netzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}