{"textgrid.poem.33685": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Abschied vom Karneval", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr zierlichen F\u00fc\u00dfchen in silberner Seide", "tokens": ["Ihr", "zier\u00b7li\u00b7chen", "F\u00fc\u00df\u00b7chen", "in", "sil\u00b7ber\u00b7ner", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Von Rhythmen des Tanzes so reizend bewegt,", "tokens": ["Von", "Rhyth\u00b7men", "des", "Tan\u00b7zes", "so", "rei\u00b7zend", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ihr holden Gestalten im blumichten Kleide,", "tokens": ["Ihr", "hol\u00b7den", "Ge\u00b7stal\u00b7ten", "im", "blu\u00b7mich\u00b7ten", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Vom Taumel der Freude so lieblich erregt,", "tokens": ["Vom", "Tau\u00b7mel", "der", "Freu\u00b7de", "so", "lieb\u00b7lich", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ihr z\u00fcndenden Blicke voll Liebesgl\u00fcck,", "tokens": ["Ihr", "z\u00fcn\u00b7den\u00b7den", "Bli\u00b7cke", "voll", "Lie\u00b7bes\u00b7gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Lebt wohl, mir kehrt ihr nie wieder zur\u00fcck!", "tokens": ["Lebt", "wohl", ",", "mir", "kehrt", "ihr", "nie", "wie\u00b7der", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Lebt wohl, ihr sch\u00f6nen, entz\u00fcckenden Stunden!", "tokens": ["Lebt", "wohl", ",", "ihr", "sch\u00f6\u00b7nen", ",", "ent\u00b7z\u00fc\u00b7cken\u00b7den", "Stun\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es winkt mir hinweg vom festlichen Saal;", "tokens": ["Es", "winkt", "mir", "hin\u00b7weg", "vom", "fest\u00b7li\u00b7chen", "Saal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Lebt wohl, und seid ihr mir heut entschwunden,", "tokens": ["Lebt", "wohl", ",", "und", "seid", "ihr", "mir", "heut", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KON", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So war es ein Gr\u00fc\u00dfen zum letztenmal!", "tokens": ["So", "war", "es", "ein", "Gr\u00fc\u00b7\u00dfen", "zum", "letz\u00b7ten\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "APPRART", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Es hat mir die Schl\u00e4fe der D\u00e4mon ber\u00fchrt,", "tokens": ["Es", "hat", "mir", "die", "Schl\u00e4\u00b7fe", "der", "D\u00e4\u00b7mon", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Der alles vom Reigen des Lebens entf\u00fchrt.", "tokens": ["Der", "al\u00b7les", "vom", "Rei\u00b7gen", "des", "Le\u00b7bens", "ent\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, mitten in all dem Jubelgedr\u00e4nge", "tokens": ["Ach", ",", "mit\u00b7ten", "in", "all", "dem", "Ju\u00b7bel\u00b7ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "APPR", "PIAT", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verbirgt sich die heimliche Klage so leicht!", "tokens": ["Ver\u00b7birgt", "sich", "die", "heim\u00b7li\u00b7che", "Kla\u00b7ge", "so", "leicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und wenn ich nicht l\u00e4nger die Tr\u00e4ne bezw\u00e4nge,", "tokens": ["Und", "wenn", "ich", "nicht", "l\u00e4n\u00b7ger", "die", "Tr\u00e4\u00b7ne", "be\u00b7zw\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wie jetzt mich der bangste Gedanke beschleicht,", "tokens": ["Wie", "jetzt", "mich", "der", "bangs\u00b7te", "Ge\u00b7dan\u00b7ke", "be\u00b7schleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Wer sieht es, wo Jeder sein Herz verlor?", "tokens": ["Wer", "sieht", "es", ",", "wo", "Je\u00b7der", "sein", "Herz", "ver\u00b7lor", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PWAV", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Ich nehme die lachende Maske noch vor.", "tokens": ["Ich", "neh\u00b7me", "die", "la\u00b7chen\u00b7de", "Mas\u00b7ke", "noch", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "O k\u00e4mt ihr mir wieder, wie wollt' ich genie\u00dfen", "tokens": ["O", "k\u00e4mt", "ihr", "mir", "wie\u00b7der", ",", "wie", "wollt'", "ich", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PPER", "ADV", "$,", "PWAV", "VMFIN", "PPER", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Euch Stunden in schwelgendem \u00dcbermut,", "tokens": ["Euch", "Stun\u00b7den", "in", "schwel\u00b7gen\u00b7dem", "\u00dc\u00b7ber\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wie z\u00e4rtlich die schlanke Gef\u00e4hrtin umschlie\u00dfen", "tokens": ["Wie", "z\u00e4rt\u00b7lich", "die", "schlan\u00b7ke", "Ge\u00b7f\u00e4hr\u00b7tin", "um\u00b7schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und k\u00fcssen die Lippen mit doppelter Glut", "tokens": ["Und", "k\u00fcs\u00b7sen", "die", "Lip\u00b7pen", "mit", "dop\u00b7pel\u00b7ter", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und fesseln jedwede vers\u00e4umte Lust,", "tokens": ["Und", "fes\u00b7seln", "jed\u00b7we\u00b7de", "ver\u00b7s\u00e4um\u00b7te", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Mir doppelt der fl\u00fcchtigen Freude bewu\u00dft!", "tokens": ["Mir", "dop\u00b7pelt", "der", "fl\u00fcch\u00b7ti\u00b7gen", "Freu\u00b7de", "be\u00b7wu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Doch was ist gestern und was ist heute?", "tokens": ["Doch", "was", "ist", "ge\u00b7stern", "und", "was", "ist", "heu\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ADJD", "KON", "PWS", "VAFIN", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "So wie man sich einst im Reigen schwang", "tokens": ["So", "wie", "man", "sich", "einst", "im", "Rei\u00b7gen", "schwang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PIS", "PRF", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wie man sich vor Jahrhunderten freute,", "tokens": ["Und", "wie", "man", "sich", "vor", "Jahr\u00b7hun\u00b7der\u00b7ten", "freu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So wird man sich freuen jahrhundertelang.", "tokens": ["So", "wird", "man", "sich", "freu\u00b7en", "jahr\u00b7hun\u00b7der\u00b7te\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PRF", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Verwelken die Kr\u00e4nze, bringt neue der Mai;", "tokens": ["Ver\u00b7wel\u00b7ken", "die", "Kr\u00e4n\u00b7ze", ",", "bringt", "neu\u00b7e", "der", "Mai", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ADJA", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Schwebt, bl\u00fchende Paare, schwebt selig vorbei!", "tokens": ["Schwebt", ",", "bl\u00fc\u00b7hen\u00b7de", "Paa\u00b7re", ",", "schwebt", "se\u00b7lig", "vor\u00b7bei", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}