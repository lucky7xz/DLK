{"textgrid.poem.64816": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn ich frage, was sich noch errungen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich frage, was sich noch errungen", "tokens": ["Wenn", "ich", "fra\u00b7ge", ",", "was", "sich", "noch", "er\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PRELS", "PRF", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wer gewandelt in dem Erdenthale,", "tokens": ["Wer", "ge\u00b7wan\u00b7delt", "in", "dem", "Er\u00b7dent\u00b7ha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn ich forsch' in den Erinnerungen", "tokens": ["Wenn", "ich", "for\u00b7sch'", "in", "den", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis zum heut'gen Abendsonnenstrahle,", "tokens": ["Bis", "zum", "heut'\u00b7gen", "A\u00b7bend\u00b7son\u00b7nen\u00b7strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Zuckt mir's h\u00f6hnend durch die tiefste Brust;", "tokens": ["Zuckt", "mir's", "h\u00f6h\u00b7nend", "durch", "die", "tiefs\u00b7te", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Was ich lang' geahnt, wird mir bewu\u00dft:", "tokens": ["Was", "ich", "lang'", "ge\u00b7ahnt", ",", "wird", "mir", "be\u00b7wu\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Da\u00df des Lebens bittre M\u00fch' verschwendet \u2013", "tokens": ["Da\u00df", "des", "Le\u00b7bens", "bitt\u00b7re", "M\u00fch'", "ver\u00b7schwen\u00b7det", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Alles endet!", "tokens": ["Al\u00b7les", "en\u00b7det", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Das Geschick liebt's, Kronen zu zersplittern", "tokens": ["Das", "Ge\u00b7schick", "liebt's", ",", "Kro\u00b7nen", "zu", "zer\u00b7split\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Ob von Gold nun, Lorbeer oder Rosen,", "tokens": ["Ob", "von", "Gold", "nun", ",", "Lor\u00b7beer", "o\u00b7der", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ADV", "$,", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Und mit seinen hei\u00dfen Ungewittern", "tokens": ["Und", "mit", "sei\u00b7nen", "hei\u00b7\u00dfen", "Un\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Die geweihten Stirnen zu umtosen;", "tokens": ["Die", "ge\u00b7weih\u00b7ten", "Stir\u00b7nen", "zu", "um\u00b7to\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Lilienbl\u00e4tter wie der Cedern Laub", "tokens": ["Li\u00b7li\u00b7en\u00b7bl\u00e4t\u00b7ter", "wie", "der", "Ce\u00b7dern", "Laub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.14": {"text": "Werden seinem Samumshauch zum Raub;", "tokens": ["Wer\u00b7den", "sei\u00b7nem", "Sa\u00b7mums\u00b7hauch", "zum", "Raub", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Was sich k\u00fchn und g\u00f6ttergleich erhoben, \u2013", "tokens": ["Was", "sich", "k\u00fchn", "und", "g\u00f6t\u00b7ter\u00b7gleich", "er\u00b7ho\u00b7ben", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PRF", "ADJD", "KON", "ADJD", "VVPP", "$,", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Ist zerstoben!", "tokens": ["Ist", "zer\u00b7sto\u00b7ben", "!"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "An Onufrio's dunkler Pforte frage,", "tokens": ["An", "O\u00b7nufrio's", "dunk\u00b7ler", "Pfor\u00b7te", "fra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ob die Kunst, die himmlische, begl\u00fccke?", "tokens": ["Ob", "die", "Kunst", ",", "die", "himm\u00b7li\u00b7sche", ",", "be\u00b7gl\u00fc\u00b7cke", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Frag' an meines Herzens Sarkophage", "tokens": ["Frag'", "an", "mei\u00b7nes", "Her\u00b7zens", "Sar\u00b7ko\u00b7pha\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Ob der Liebe Spenden ohne T\u00fccke? \u2013", "tokens": ["Ob", "der", "Lie\u00b7be", "Spen\u00b7den", "oh\u00b7ne", "T\u00fc\u00b7cke", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Sieh zerstreut in bangem Pilgerzug", "tokens": ["Sieh", "zer\u00b7streut", "in", "ban\u00b7gem", "Pil\u00b7ger\u00b7zug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.22": {"text": "Jenen Stamm, der einstens Kronen trug!", "tokens": ["Je\u00b7nen", "Stamm", ",", "der", "eins\u00b7tens", "Kro\u00b7nen", "trug", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "W\u00e4hn' dann noch in Weltensturmes Wehen", "tokens": ["W\u00e4hn'", "dann", "noch", "in", "Wel\u00b7ten\u00b7stur\u00b7mes", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Festzustehen!", "tokens": ["Fest\u00b7zu\u00b7ste\u00b7hen", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.25": {"text": "Meine Rechnung hab ich abgeschlossen,", "tokens": ["Mei\u00b7ne", "Rech\u00b7nung", "hab", "ich", "ab\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Will nichts mehr verlangen noch gew\u00e4hren;", "tokens": ["Will", "nichts", "mehr", "ver\u00b7lan\u00b7gen", "noch", "ge\u00b7w\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VVFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "F\u00fcr das L\u00e4cheln ist mein Mund verschlossen", "tokens": ["F\u00fcr", "das", "L\u00e4\u00b7cheln", "ist", "mein", "Mund", "ver\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Und mein Aug' vertrocknet f\u00fcr die Z\u00e4hren.", "tokens": ["Und", "mein", "Aug'", "ver\u00b7trock\u00b7net", "f\u00fcr", "die", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "Ist das Ziel, nach dem der Wunsch begehrt,", "tokens": ["Ist", "das", "Ziel", ",", "nach", "dem", "der", "Wunsch", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "APPR", "ART", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.30": {"text": "Denn des Ringens und der K\u00e4mpfe werth?", "tokens": ["Denn", "des", "Rin\u00b7gens", "und", "der", "K\u00e4mp\u00b7fe", "werth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.31": {"text": "Werth, da\u00df Sorg' und M\u00fche man dran wendet? \u2013", "tokens": ["Werth", ",", "da\u00df", "Sor\u00b7g'", "und", "M\u00fc\u00b7he", "man", "dran", "wen\u00b7det", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "KON", "NN", "PIS", "PAV", "VVFIN", "$.", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.32": {"text": "Alles endet!", "tokens": ["Al\u00b7les", "en\u00b7det", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}