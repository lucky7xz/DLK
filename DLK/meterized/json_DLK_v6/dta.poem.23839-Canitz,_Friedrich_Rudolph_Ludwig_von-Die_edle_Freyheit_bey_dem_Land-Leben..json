{"dta.poem.23839": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Die edle Freyheit bey dem Land-Leben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Sinn der nur nach seinem Ursprung schmeckt/", "tokens": ["Ein", "Sinn", "der", "nur", "nach", "sei\u00b7nem", "Ur\u00b7sprung", "schmeckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sich nicht in den Schlam\u0303 der Eitelkeit versteckt/", "tokens": ["Und", "sich", "nicht", "in", "den", "Schlam\u0303", "der", "Ei\u00b7tel\u00b7keit", "ver\u00b7steckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PTKNEG", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kan/ was der P\u00f6bel sucht/ mit leichter M\u00fch vergessen.", "tokens": ["Kan", "/", "was", "der", "P\u00f6\u00b7bel", "sucht", "/", "mit", "leich\u00b7ter", "M\u00fch", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$(", "PWS", "ART", "NN", "VVFIN", "$(", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Weisen ist sein Vaterland die Welt/", "tokens": ["Dem", "Wei\u00b7sen", "ist", "sein", "Va\u00b7ter\u00b7land", "die", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er bleibet unbewegt/ wenn alles bricht und f\u00e4llt/", "tokens": ["Er", "blei\u00b7bet", "un\u00b7be\u00b7wegt", "/", "wenn", "al\u00b7les", "bricht", "und", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$(", "KOUS", "PIS", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wil sein Gl\u00fcck nach nichts als seiner Freyheit messen.", "tokens": ["Und", "wil", "sein", "Gl\u00fcck", "nach", "nichts", "als", "sei\u00b7ner", "Frey\u00b7heit", "mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "APPR", "PIS", "KOKOM", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es kan ein solcher Mann sich an sich selbst vergn\u00fcgen;", "tokens": ["Es", "kan", "ein", "sol\u00b7cher", "Mann", "sich", "an", "sich", "selbst", "ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "PIAT", "NN", "PRF", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat ein gekr\u00f6ntes Haupt ihm etwan wohl gewollt/", "tokens": ["Hat", "ein", "ge\u00b7kr\u00f6n\u00b7tes", "Haupt", "ihm", "et\u00b7wan", "wohl", "ge\u00b7wollt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PPER", "ADV", "ADV", "VMPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist ihm das Vatican um seiner Tugend hold/", "tokens": ["Ist", "ihm", "das", "Va\u00b7ti\u00b7can", "um", "sei\u00b7ner", "Tu\u00b7gend", "hold", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ja wil ein Fridrich selbst nach seinem Urtheil kriegen/", "tokens": ["Ja", "wil", "ein", "Frid\u00b7rich", "selbst", "nach", "sei\u00b7nem", "Ur\u00b7theil", "krie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VMFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wird Er doch von Cron und Purpur nie beth\u00f6rt;", "tokens": ["So", "wird", "Er", "doch", "von", "Cron", "und", "Pur\u00b7pur", "nie", "be\u00b7th\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NE", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kein Wechsel kommt/ der sein Gem\u00fchte st\u00f6hrt;", "tokens": ["Kein", "Wech\u00b7sel", "kommt", "/", "der", "sein", "Ge\u00b7m\u00fch\u00b7te", "st\u00f6hrt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Drum wenn das Gl\u00fcck ihm endlich kehrt den R\u00fccken/", "tokens": ["Drum", "wenn", "das", "Gl\u00fcck", "ihm", "end\u00b7lich", "kehrt", "den", "R\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "ART", "NN", "PPER", "ADV", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Kan er mit eben dieser Hand/", "tokens": ["Kan", "er", "mit", "e\u00b7ben", "die\u00b7ser", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADV", "PDAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die gantzer L\u00e4nder Zin\u00df zur Pracht hat aufgewandt/", "tokens": ["Die", "gant\u00b7zer", "L\u00e4n\u00b7der", "Zin\u00df", "zur", "Pracht", "hat", "auf\u00b7ge\u00b7wandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "APPRART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In Demuth und Gedult/ ihm selbst die Hosen flicken.", "tokens": ["In", "De\u00b7muth", "und", "Ge\u00b7dult", "/", "ihm", "selbst", "die", "Ho\u00b7sen", "fli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$(", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Sein Hoff wird ihm ein Hoff; sein Acker seine Freude;", "tokens": ["Sein", "Hoff", "wird", "ihm", "ein", "Hoff", ";", "sein", "A\u00b7cker", "sei\u00b7ne", "Freu\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ART", "NN", "$.", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein finstrer Dannen-W\u00e4ld sein Pomerantzen-Haus;", "tokens": ["Ein", "finst\u00b7rer", "Dan\u00b7nen\u00b7W\u00e4ld", "sein", "Po\u00b7me\u00b7rant\u00b7zen\u00b7Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Heerde theilet er als denn die fette Weide/", "tokens": ["Der", "Heer\u00b7de", "thei\u00b7let", "er", "als", "denn", "die", "fet\u00b7te", "Wei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KOUS", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie sonst dem Krieges-Heer/ mit treuer Sorgfalt aus/", "tokens": ["Wie", "sonst", "dem", "Krie\u00b7ge\u00b7sHeer", "/", "mit", "treu\u00b7er", "Sorg\u00b7falt", "aus", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "$(", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der F\u00fcrwitz treibt ihn nicht viel neues mehr zu wissen/", "tokens": ["Der", "F\u00fcr\u00b7witz", "treibt", "ihn", "nicht", "viel", "neu\u00b7es", "mehr", "zu", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJA", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als was sein Meyer bringt; Er kehrt sich wenig dran", "tokens": ["Als", "was", "sein", "Me\u00b7yer", "bringt", ";", "Er", "kehrt", "sich", "we\u00b7nig", "dran"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PRF", "PIS", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer dort in einer Schlacht zu Boden wird geschmissen/", "tokens": ["Wer", "dort", "in", "ei\u00b7ner", "Schlacht", "zu", "Bo\u00b7den", "wird", "ge\u00b7schmis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn er in Sicherheit die Garben binden kan.", "tokens": ["Wenn", "er", "in", "Si\u00b7cher\u00b7heit", "die", "Gar\u00b7ben", "bin\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ist ihm nicht mehr verg\u00f6nnt zu k\u00fcssen eine Docke/", "tokens": ["Ist", "ihm", "nicht", "mehr", "ver\u00b7g\u00f6nnt", "zu", "k\u00fcs\u00b7sen", "ei\u00b7ne", "Do\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "VVPP", "PTKZU", "VVINF", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die ihre freche Stirn mit Th\u00fcrmen \u00fcberh\u00e4ufft/", "tokens": ["Die", "ih\u00b7re", "fre\u00b7che", "Stirn", "mit", "Th\u00fcr\u00b7men", "\u00fc\u00b7berh\u00b7\u00e4ufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So thuts ihm ja so wohl/ wenn er nach einer greifft/", "tokens": ["So", "thuts", "ihm", "ja", "so", "wohl", "/", "wenn", "er", "nach", "ei\u00b7ner", "greifft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$(", "KOUS", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit schlecht-geflochtnem Haar und aufgesch\u00fcrtztem", "tokens": ["Mit", "schlecht\u00b7ge\u00b7flocht\u00b7nem", "Haar", "und", "auf\u00b7ge\u00b7sch\u00fcrtz\u00b7tem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn ihn zuweilen auch ein kleiner Kummer dr\u00fcckt/", "tokens": ["Wenn", "ihn", "zu\u00b7wei\u00b7len", "auch", "ein", "klei\u00b7ner", "Kum\u00b7mer", "dr\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird er nicht weniger entz\u00fcckt/", "tokens": ["Wird", "er", "nicht", "we\u00b7ni\u00b7ger", "ent\u00b7z\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "So bald der Dudel-Sack in seiner Schencke klinget/", "tokens": ["So", "bald", "der", "Du\u00b7del\u00b7Sack", "in", "sei\u00b7ner", "Schen\u00b7cke", "klin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als wenn Bellerophon von seiner Liebe singet;", "tokens": ["Als", "wenn", "Bel\u00b7le\u00b7ro\u00b7phon", "von", "sei\u00b7ner", "Lie\u00b7be", "sin\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Und kan er nicht ein ", "tokens": ["Und", "kan", "er", "nicht", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Noch auch ", "tokens": ["Noch", "auch"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Auf seiner Taffel haben/", "tokens": ["Auf", "sei\u00b7ner", "Taf\u00b7fel", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So wird er sich an Gloms und an Pomocheln laben.", "tokens": ["So", "wird", "er", "sich", "an", "Gloms", "und", "an", "Po\u00b7mo\u00b7cheln", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPR", "NE", "KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nun edles Preussen du/ du kriegst so einen Gast/", "tokens": ["Nun", "ed\u00b7les", "Preus\u00b7sen", "du", "/", "du", "kriegst", "so", "ei\u00b7nen", "Gast", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPER", "$(", "PPER", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den du gewi\u00df zu lieben Ursach hast.", "tokens": ["Den", "du", "ge\u00b7wi\u00df", "zu", "lie\u00b7ben", "Ur\u00b7sach", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du bist begl\u00fcckt/ dieweil du Ihn gebohren/", "tokens": ["Du", "bist", "be\u00b7gl\u00fcckt", "/", "die\u00b7weil", "du", "Ihn", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "KOUS", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Begl\u00fcckter/ da\u00df er dich zum Ruh-Platz hat erkohren/", "tokens": ["Be\u00b7gl\u00fcck\u00b7ter", "/", "da\u00df", "er", "dich", "zum", "Ruh\u00b7Platz", "hat", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "PRF", "APPRART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Worin Er/ was sein Geist an Sch\u00e4tzen bey sich tr\u00e4gt/", "tokens": ["Wo\u00b7rin", "Er", "/", "was", "sein", "Geist", "an", "Sch\u00e4t\u00b7zen", "bey", "sich", "tr\u00e4gt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$(", "PWS", "PPOSAT", "NN", "APPR", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als in der Mutter-Schoo\u00df/ verwahrlich niederlegt.", "tokens": ["Als", "in", "der", "Mut\u00b7ter\u00b7Schoo\u00df", "/", "ver\u00b7wahr\u00b7lich", "nie\u00b7der\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$(", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Das Land von Mancha mag sich immerhin erheben/", "tokens": ["Das", "Land", "von", "Man\u00b7cha", "mag", "sich", "im\u00b7mer\u00b7hin", "er\u00b7he\u00b7ben", "/"], "