{"dta.poem.8519": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "An meine Mutter.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So gern h&#228;tt' ich ein sch\u00f6nes Lied gemacht,", "tokens": ["So", "gern", "h", "&#228;", "tt'", "ich", "ein", "sch\u00f6\u00b7nes", "Lied", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$(", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Von deiner Liebe, deiner treuen Weise,", "tokens": ["Von", "dei\u00b7ner", "Lie\u00b7be", ",", "dei\u00b7ner", "treu\u00b7en", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Gabe, die f\u00fcr Andre immer wacht,", "tokens": ["Die", "Ga\u00b7be", ",", "die", "f\u00fcr", "And\u00b7re", "im\u00b7mer", "wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "H\u00e4tt\u2019 ich so gern geweckt zu deinem Preise.", "tokens": ["H\u00e4tt'", "ich", "so", "gern", "ge\u00b7weckt", "zu", "dei\u00b7nem", "Prei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch wie ich auch gesonnen mehr und mehr,", "tokens": ["Doch", "wie", "ich", "auch", "ge\u00b7son\u00b7nen", "mehr", "und", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wie ich auch die Reime mochte stellen,", "tokens": ["Und", "wie", "ich", "auch", "die", "Rei\u00b7me", "moch\u00b7te", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Herzens Fluthen wallten dr\u00fcber her,", "tokens": ["Des", "Her\u00b7zens", "Flut\u00b7hen", "wall\u00b7ten", "dr\u00fc\u00b7ber", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zerst\u00f6rten mir des Liedes zarte Wellen.", "tokens": ["Zer\u00b7st\u00f6r\u00b7ten", "mir", "des", "Lie\u00b7des", "zar\u00b7te", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So nimm die einfach schlichte Gabe hin,", "tokens": ["So", "nimm", "die", "ein\u00b7fach", "schlich\u00b7te", "Ga\u00b7be", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "ADJD", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von einfach ungeschm\u00fccktem Wort getragen,", "tokens": ["Von", "ein\u00b7fach", "un\u00b7ge\u00b7schm\u00fcck\u00b7tem", "Wort", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und meine ganze Seele nimm darin;", "tokens": ["Und", "mei\u00b7ne", "gan\u00b7ze", "See\u00b7le", "nimm", "da\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo man am meisten f\u00fchlt, wei\u00df man nicht viel zu sagen.", "tokens": ["Wo", "man", "am", "meis\u00b7ten", "f\u00fchlt", ",", "wei\u00df", "man", "nicht", "viel", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPRART", "PIS", "VVFIN", "$,", "VVFIN", "PIS", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}