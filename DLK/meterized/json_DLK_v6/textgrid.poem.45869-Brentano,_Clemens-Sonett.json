{"textgrid.poem.45869": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O schwerer hei\u00dfer Tag, ihr leichtes Leben", "tokens": ["O", "schwe\u00b7rer", "hei\u00b7\u00dfer", "Tag", ",", "ihr", "leich\u00b7tes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schlie\u00dft m\u00fcde weinend seine Augenlider,", "tokens": ["Schlie\u00dft", "m\u00fc\u00b7de", "wei\u00b7nend", "sei\u00b7ne", "Au\u00b7gen\u00b7li\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schon senkt der Schlaf das tauende Gefieder,", "tokens": ["Schon", "senkt", "der", "Schlaf", "das", "tau\u00b7en\u00b7de", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Um solche Sch\u00f6nheit k\u00fchl ein Dach zu weben. \u2013", "tokens": ["Um", "sol\u00b7che", "Sch\u00f6n\u00b7heit", "k\u00fchl", "ein", "Dach", "zu", "we\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "PIAT", "NN", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von ihren Lippen leise Worte schweben,", "tokens": ["Von", "ih\u00b7ren", "Lip\u00b7pen", "lei\u00b7se", "Wor\u00b7te", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbdu Liebe s\u00fc\u00dfer Tr\u00e4ume kehre wieder!\u00ab", "tokens": ["\u00bb", "du", "Lie\u00b7be", "s\u00fc\u00b7\u00dfer", "Tr\u00e4u\u00b7me", "keh\u00b7re", "wie\u00b7der", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "NN", "ADJA", "NN", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da l\u00e4\u00dft sich ihr der Traum der Liebe nieder,", "tokens": ["Da", "l\u00e4\u00dft", "sich", "ihr", "der", "Traum", "der", "Lie\u00b7be", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPER", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um ihres Schlummers kranke Lust zu heben. \u2013", "tokens": ["Um", "ih\u00b7res", "Schlum\u00b7mers", "kran\u00b7ke", "Lust", "zu", "he\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbdu Traum! \u2013 ich bin kein Traum, spricht er mit Bangen,", "tokens": ["\u00bb", "du", "Traum", "!", "\u2013", "ich", "bin", "kein", "Traum", ",", "spricht", "er", "mit", "Ban\u00b7gen", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "NN", "$.", "$(", "PPER", "VAFIN", "PIAT", "NN", "$,", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O la\u00df uns nicht so holdes Gl\u00fcck vers\u00e4umen!\u00ab", "tokens": ["O", "la\u00df", "uns", "nicht", "so", "hol\u00b7des", "Gl\u00fcck", "ver\u00b7s\u00e4u\u00b7men", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da weckt er sie, und wollte sie umfangen. \u2013", "tokens": ["Da", "weckt", "er", "sie", ",", "und", "woll\u00b7te", "sie", "um\u00b7fan\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "KON", "VMFIN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}