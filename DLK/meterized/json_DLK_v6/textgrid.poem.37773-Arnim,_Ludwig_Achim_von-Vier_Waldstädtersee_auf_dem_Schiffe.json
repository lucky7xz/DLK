{"textgrid.poem.37773": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Vier Waldst\u00e4dtersee auf dem Schiffe", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort dr\u00fcben in den Bergen,", "tokens": ["Dort", "dr\u00fc\u00b7ben", "in", "den", "Ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da gr\u00e4bt der Wind sein Haus,", "tokens": ["Da", "gr\u00e4bt", "der", "Wind", "sein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er mu\u00df sich heute bergen,", "tokens": ["Er", "mu\u00df", "sich", "heu\u00b7te", "ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein W\u00f6lklein bl\u00e4\u00dft ihn aus:", "tokens": ["Kein", "W\u00f6lk\u00b7lein", "bl\u00e4\u00dft", "ihn", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gel\u00e4hmet ist sein Wille,", "tokens": ["Ge\u00b7l\u00e4h\u00b7met", "ist", "sein", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Wasser scheint so glatt,", "tokens": ["Das", "Was\u00b7ser", "scheint", "so", "glatt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es t\u00f6nt durch hei\u00dfe Stille", "tokens": ["Es", "t\u00f6nt", "durch", "hei\u00b7\u00dfe", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihr Seufzen leise matt.", "tokens": ["Ihr", "Seuf\u00b7zen", "lei\u00b7se", "matt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wallfahrer traurig singen", "tokens": ["Wall\u00b7fah\u00b7rer", "trau\u00b7rig", "sin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr wechselndes Gebet,", "tokens": ["Ihr", "wech\u00b7seln\u00b7des", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Doch mit dem Teufel ringen,", "tokens": ["Doch", "mit", "dem", "Teu\u00b7fel", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wohl nimmer einer th\u00e4t:", "tokens": ["Wohl", "nim\u00b7mer", "ei\u00b7ner", "th\u00e4t", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So dr\u00e4ngen b\u00f6se Zeiten", "tokens": ["So", "dr\u00e4n\u00b7gen", "b\u00f6\u00b7se", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Heuschrecken, Schrecken ein,", "tokens": ["Heu\u00b7schre\u00b7cken", ",", "Schre\u00b7cken", "ein", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer wird sich heut bereiten", "tokens": ["Wer", "wird", "sich", "heut", "be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von Schmach uns zu befrein.", "tokens": ["Von", "Schmach", "uns", "zu", "be\u00b7fr\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Bretter schwitzen Thr\u00e4nen,", "tokens": ["Die", "Bret\u00b7ter", "schwit\u00b7zen", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Ruderer beim Wein", "tokens": ["Die", "Ru\u00b7de\u00b7rer", "beim", "Wein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verschlafen in den K\u00e4hnen", "tokens": ["Ver\u00b7schla\u00b7fen", "in", "den", "K\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des nahen Sturmes Dr\u00e4un:", "tokens": ["Des", "na\u00b7hen", "Stur\u00b7mes", "Dr\u00e4un", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du schaust mein Herz die Platte,", "tokens": ["Du", "schaust", "mein", "Herz", "die", "Plat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie rettete den Tell,", "tokens": ["Sie", "ret\u00b7te\u00b7te", "den", "Tell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Muth, den Tell der Gatte,", "tokens": ["Den", "Muth", ",", "den", "Tell", "der", "Gat\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dem Schmerz entgegen stell.", "tokens": ["Dem", "Schmerz", "ent\u00b7ge\u00b7gen", "stell", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Sturm trieb auf und nieder,", "tokens": ["Der", "Sturm", "trieb", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein Schiff zur Schreckens-Bahn,", "tokens": ["Sein", "Schiff", "zur", "Schre\u00b7ckens\u00b7Bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Tell sang frohe Lieder,", "tokens": ["Der", "Tell", "sang", "fro\u00b7he", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gefangen in dem Kahn:", "tokens": ["Ge\u00b7fan\u00b7gen", "in", "dem", "Kahn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Der ihn gefangen h\u00e4lt,", "tokens": ["Der", "ihn", "ge\u00b7fan\u00b7gen", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den B\u00f6sen treiben Drachen,", "tokens": ["Den", "B\u00f6\u00b7sen", "trei\u00b7ben", "Dra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wenn Tod ihm Netze stellt.", "tokens": ["Wenn", "Tod", "ihm", "Net\u00b7ze", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er l\u00f6\u00dft dem ", "tokens": ["Er", "l\u00f6\u00dft", "dem"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Er soll bek\u00e4mpfen Tod,", "tokens": ["Er", "soll", "be\u00b7k\u00e4mp\u00b7fen", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den sie mit Schimpf umwanden,", "tokens": ["Den", "sie", "mit", "Schimpf", "um\u00b7wan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach tiefer Vaternoth:", "tokens": ["Nach", "tie\u00b7fer", "Va\u00b7ter\u00b7noth", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er soll sie jetzt erretten,", "tokens": ["Er", "soll", "sie", "jetzt", "er\u00b7ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der nur f\u00fcr Freiheit stritt,", "tokens": ["Der", "nur", "f\u00fcr", "Frei\u00b7heit", "stritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den sie gelegt in Ketten,", "tokens": ["Den", "sie", "ge\u00b7legt", "in", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zum Kerker f\u00fchrten mit.", "tokens": ["Zum", "Ker\u00b7ker", "f\u00fchr\u00b7ten", "mit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die St\u00fcrme von den Bergen,", "tokens": ["Die", "St\u00fcr\u00b7me", "von", "den", "Ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie waren frei zum Kampf,", "tokens": ["Sie", "wa\u00b7ren", "frei", "zum", "Kampf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es fluthet von den Bergen,", "tokens": ["Es", "flut\u00b7het", "von", "den", "Ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der grauen Wolken Dampf", "tokens": ["Der", "grau\u00b7en", "Wol\u00b7ken", "Dampf"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hinaus hin\u00fcber \u00fcber", "tokens": ["Hin\u00b7aus", "hin\u00b7\u00fc\u00b7ber", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In Wind und Gegenwind;", "tokens": ["In", "Wind", "und", "Ge\u00b7gen\u00b7wind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Liebe Himmel tr\u00fcber,", "tokens": ["Der", "Lie\u00b7be", "Him\u00b7mel", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Felsen deckt geschwind.", "tokens": ["Die", "Fel\u00b7sen", "deckt", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wer giebt dem ", "tokens": ["Wer", "giebt", "dem"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Wer giebt ihm solchen Muth,", "tokens": ["Wer", "giebt", "ihm", "sol\u00b7chen", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es sind der Engel Kr\u00e4fte,", "tokens": ["Es", "sind", "der", "En\u00b7gel", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ihn besch\u00fctzt beim Huth:", "tokens": ["Die", "ihn", "be\u00b7sch\u00fctzt", "beim", "Huth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schon ist er nah am Ufer,", "tokens": ["Schon", "ist", "er", "nah", "am", "U\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Jetzt springt er aus dem Kahn,", "tokens": ["Jetzt", "springt", "er", "aus", "dem", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und st\u00f6\u00dft ihn rasch vom Ufer,", "tokens": ["Und", "st\u00f6\u00dft", "ihn", "rasch", "vom", "U\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zur\u00fcck zur Todes-Bahn.", "tokens": ["Zu\u00b7r\u00fcck", "zur", "To\u00b7des\u00b7Bahn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Sklaven sollen vergehen,", "tokens": ["Die", "Skla\u00b7ven", "sol\u00b7len", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gelobt sei Wilhelm ", "tokens": ["Ge\u00b7lobt", "sei", "Wil\u00b7helm"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "VAFIN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Durch sie Tyrannen bestehen,", "tokens": ["Durch", "sie", "Ty\u00b7ran\u00b7nen", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Die Freiheit steht durch ", "tokens": ["Die", "Frei\u00b7heit", "steht", "durch"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "So wurden frei die Br\u00fcder,", "tokens": ["So", "wur\u00b7den", "frei", "die", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Himmel ward ihr Haus,", "tokens": ["Der", "Him\u00b7mel", "ward", "ihr", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es schallten frohe Lieder", "tokens": ["Es", "schall\u00b7ten", "fro\u00b7he", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "W\u00e4r diese Zeit nicht aus!", "tokens": ["W\u00e4r", "die\u00b7se", "Zeit", "nicht", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}