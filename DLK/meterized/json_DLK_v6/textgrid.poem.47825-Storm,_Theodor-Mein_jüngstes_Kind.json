{"textgrid.poem.47825": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Mein j\u00fcngstes Kind", "genre": "verse", "period": "N.A.", "pub_year": 1863, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wanderte schon lange,", "tokens": ["Ich", "wan\u00b7der\u00b7te", "schon", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da kamest du daher;", "tokens": ["Da", "ka\u00b7mest", "du", "da\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun gingen wir zusammen,", "tokens": ["Nun", "gin\u00b7gen", "wir", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich sah dich nie vorher.", "tokens": ["Ich", "sah", "dich", "nie", "vor\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Noch eine kurze Strecke", "tokens": ["Noch", "ei\u00b7ne", "kur\u00b7ze", "Stre\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u2013 Das Herz wird mir so schwer \u2013,", "tokens": ["\u2013", "Das", "Herz", "wird", "mir", "so", "schwer", "\u2013", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$(", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du hast noch weit zu gehen,", "tokens": ["Du", "hast", "noch", "weit", "zu", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich kann nicht weiter mehr.", "tokens": ["Ich", "kann", "nicht", "wei\u00b7ter", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}