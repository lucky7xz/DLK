{"dta.poem.14780": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Reitersmann .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und zwischen den Bergesbogen,", "tokens": ["Und", "zwi\u00b7schen", "den", "Ber\u00b7ges\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wohl \u00fcber den gr\u00fcnen Plan,", "tokens": ["Wohl", "\u00fc\u00b7ber", "den", "gr\u00fc\u00b7nen", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein J\u00e4gersmann kam geflogen,", "tokens": ["Ein", "J\u00e4\u00b7gers\u00b7mann", "kam", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der sah mich so muthig an.", "tokens": ["Der", "sah", "mich", "so", "mut\u00b7hig", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}