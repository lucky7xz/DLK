{"dta.poem.9178": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n Verwirte Reise-Gedancken.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Nun wei\u00df ich weder aus noch ein/", "tokens": ["Nun", "wei\u00df", "ich", "we\u00b7der", "aus", "noch", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "APPR", "ADV", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor der bedr\u00e4ngten seelen-pein/", "tokens": ["Vor", "der", "be\u00b7dr\u00e4ng\u00b7ten", "see\u00b7len\u00b7pein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann mein verh\u00e4ngn\u00fc\u00df macht den schlu\u00df/", "tokens": ["Dann", "mein", "ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "macht", "den", "schlu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich aus - - - - wandern mu\u00df.", "tokens": ["Da\u00df", "ich", "aus", "wan\u00b7dern", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "$(", "$(", "$(", "$(", "VVFIN", "VMFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ach seele was betr\u00fcbst du dich/", "tokens": ["Ach", "see\u00b7le", "was", "be\u00b7tr\u00fcbst", "du", "dich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PIS", "VVFIN", "PPER", "PRF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Warumb ist dir so wunderlich/", "tokens": ["Wa\u00b7rumb", "ist", "dir", "so", "wun\u00b7der\u00b7lich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was ist denn nun dieselbe lust/", "tokens": ["Was", "ist", "denn", "nun", "die\u00b7sel\u00b7be", "lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "PDAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die du zur\u00fccke lassen must?", "tokens": ["Die", "du", "zu\u00b7r\u00fc\u00b7cke", "las\u00b7sen", "must", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Ist es die wunder-sch\u00f6ne stadt:", "tokens": ["Ist", "es", "die", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "stadt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So dich bi\u00dfher ergetzet hat?", "tokens": ["So", "dich", "bi\u00df\u00b7her", "er\u00b7get\u00b7zet", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach nein/ vielleicht ist anderswo/", "tokens": ["Ach", "nein", "/", "viel\u00b7leicht", "ist", "an\u00b7ders\u00b7wo", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$(", "ADV", "VAFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der st\u00e4dte zierrath eben so:", "tokens": ["Der", "st\u00e4d\u00b7te", "zier\u00b7rath", "e\u00b7ben", "so", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Beliebt dir die gelegenheit/", "tokens": ["Be\u00b7liebt", "dir", "die", "ge\u00b7le\u00b7gen\u00b7heit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu aller zarten h\u00f6flichkeit/", "tokens": ["Zu", "al\u00b7ler", "zar\u00b7ten", "h\u00f6f\u00b7lich\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach nein/ ich habe diesen pracht/", "tokens": ["Ach", "nein", "/", "ich", "ha\u00b7be", "die\u00b7sen", "pracht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$(", "PPER", "VAFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der heucheley vor l\u00e4ngst veracht.", "tokens": ["Der", "heu\u00b7che\u00b7ley", "vor", "l\u00e4ngst", "ver\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Willstu dem Pindus hier allein", "tokens": ["Will\u00b7stu", "dem", "Pin\u00b7dus", "hier", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ergeben und verbunden seyn?", "tokens": ["Er\u00b7ge\u00b7ben", "und", "ver\u00b7bun\u00b7den", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach nein/ ich habe gnug studirt/", "tokens": ["Ach", "nein", "/", "ich", "ha\u00b7be", "gnug", "stu\u00b7dirt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$(", "PPER", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer viel kalmeusert/ wird verf\u00fchrt.", "tokens": ["Wer", "viel", "kal\u00b7meu\u00b7sert", "/", "wird", "ver\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "$(", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du sihst vielleicht die stellen an/", "tokens": ["Du", "sihst", "viel\u00b7leicht", "die", "stel\u00b7len", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dahin man dich bef\u00f6rdern kan?", "tokens": ["Da\u00b7hin", "man", "dich", "be\u00b7f\u00f6r\u00b7dern", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach nein/ die kunst ist doch bekannt/", "tokens": ["Ach", "nein", "/", "die", "kunst", "ist", "doch", "be\u00b7kannt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$(", "ART", "NN", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und findet wohl ihr vaterland.", "tokens": ["Und", "fin\u00b7det", "wohl", "ihr", "va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Macht dir die sch\u00f6ne compagnie", "tokens": ["Macht", "dir", "die", "sch\u00f6\u00b7ne", "com\u00b7pag\u00b7nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des frauenzimmers solche m\u00fch?", "tokens": ["Des", "frau\u00b7en\u00b7zim\u00b7mers", "sol\u00b7che", "m\u00fch", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach nein/ es ist die gantze welt/", "tokens": ["Ach", "nein", "/", "es", "ist", "die", "gant\u00b7ze", "welt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit solcher zucht durchaus bestellt.", "tokens": ["Mit", "sol\u00b7cher", "zucht", "durc\u00b7haus", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist etwan hier ein bruder-spiel/", "tokens": ["Ist", "et\u00b7wan", "hier", "ein", "bru\u00b7der\u00b7spiel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das dir nicht aus dem sinne will?", "tokens": ["Das", "dir", "nicht", "aus", "dem", "sin\u00b7ne", "will", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKNEG", "APPR", "ART", "NN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach nein/ auf einer fremden bahn/", "tokens": ["Ach", "nein", "/", "auf", "ei\u00b7ner", "frem\u00b7den", "bahn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$(", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Trifft man auch gute br\u00fcder an.", "tokens": ["Trifft", "man", "auch", "gu\u00b7te", "br\u00fc\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Ist irgend ein vertrauter freund/", "tokens": ["Ist", "ir\u00b7gend", "ein", "ver\u00b7trau\u00b7ter", "freund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der dich von gantzen hertzen meint?", "tokens": ["Der", "dich", "von", "gant\u00b7zen", "hert\u00b7zen", "meint", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja wohl/ doch ihn verlier ich nicht.", "tokens": ["Ja", "wohl", "/", "doch", "ihn", "ver\u00b7lier", "ich", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$(", "ADV", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verlier ich gleich sein angesicht.", "tokens": ["Ver\u00b7lier", "ich", "gleich", "sein", "an\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach meine seele sage doch/", "tokens": ["Ach", "mei\u00b7ne", "see\u00b7le", "sa\u00b7ge", "doch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was dr\u00fcckt dich sonsten vor ein joch?", "tokens": ["Was", "dr\u00fcckt", "dich", "sons\u00b7ten", "vor", "ein", "joch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du bist betr\u00fcbt/ was ist denn di\u00df?", "tokens": ["Du", "bist", "be\u00b7tr\u00fcbt", "/", "was", "ist", "denn", "di\u00df", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "PWS", "VAFIN", "ADV", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nichts/ freylich nichts/ als \u2012 \u2012 \u2012 \u2012 \u2012 \u2012 \u2012", "tokens": ["Nichts", "/", "frey\u00b7lich", "nichts", "/", "als", "\u2012", "\u2012", "\u2012", "\u2012", "\u2012", "\u2012", "\u2012"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "$(", "ADV", "PIS", "$(", "KOUS", "$(", "$(", "$(", "$(", "$(", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}