{"textgrid.poem.31757": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ein Jahr", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah dich zuerst im Januar", "tokens": ["Ich", "sah", "dich", "zu\u00b7erst", "im", "Ja\u00b7nu\u00b7ar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und gr\u00fc\u00dfte zuerst dich im Februar;", "tokens": ["Und", "gr\u00fc\u00df\u00b7te", "zu\u00b7erst", "dich", "im", "Feb\u00b7ru\u00b7ar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch um die Mitte des M\u00e4rzen,", "tokens": ["Doch", "um", "die", "Mit\u00b7te", "des", "M\u00e4r\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da f\u00fchlte ich Lieb im Herzen.", "tokens": ["Da", "f\u00fchl\u00b7te", "ich", "Lieb", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und schlich vor deine T\u00fcre still", "tokens": ["Und", "schlich", "vor", "dei\u00b7ne", "T\u00fc\u00b7re", "still"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl in den N\u00e4chten des April;", "tokens": ["Wohl", "in", "den", "N\u00e4ch\u00b7ten", "des", "Ap\u00b7ril", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Erst in der Bl\u00fcte des Maien,", "tokens": ["Erst", "in", "der", "Bl\u00fc\u00b7te", "des", "Mai\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Da t\u00e4te ich um dich freien.", "tokens": ["Da", "t\u00e4\u00b7te", "ich", "um", "dich", "frei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Und gab dir, ach, so manchen Ku\u00df", "tokens": ["Und", "gab", "dir", ",", "ach", ",", "so", "man\u00b7chen", "Ku\u00df"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "ITJ", "$,", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im holden Monat Junius;", "tokens": ["Im", "hol\u00b7den", "Mo\u00b7nat", "Ju\u00b7nius", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und beide f\u00fchlten wir hei\u00df Verlangen,", "tokens": ["Und", "bei\u00b7de", "f\u00fchl\u00b7ten", "wir", "hei\u00df", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als drauf der Juli kam gegangen.", "tokens": ["Als", "drauf", "der", "Ju\u00b7li", "kam", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Juli kam; und im August", "tokens": ["Der", "Ju\u00b7li", "kam", ";", "und", "im", "Au\u00b7gust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schwelgten wir selig Brust an Brust;", "tokens": ["Schwelg\u00b7ten", "wir", "se\u00b7lig", "Brust", "an", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Im September t\u00e4t ich dich entf\u00fchren;", "tokens": ["Im", "Sep\u00b7tem\u00b7ber", "t\u00e4t", "ich", "dich", "ent\u00b7f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im Oktober lie\u00dfen wir kopulieren", "tokens": ["Im", "Ok\u00b7to\u00b7ber", "lie\u00b7\u00dfen", "wir", "ko\u00b7pu\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Und w\u00e4lzten uns im Novembrium", "tokens": ["Und", "w\u00e4lz\u00b7ten", "uns", "im", "No\u00b7vemb\u00b7ri\u00b7um"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als Eheleute im Bett herum \u2013", "tokens": ["Als", "E\u00b7he\u00b7leu\u00b7te", "im", "Bett", "he\u00b7rum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "PTKVZ", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Jetzt ist es Dezember, die Wolken schneien,", "tokens": ["Jetzt", "ist", "es", "De\u00b7zem\u00b7ber", ",", "die", "Wol\u00b7ken", "schnei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Die Raben kr\u00e4chzen, die Kater schreien.", "tokens": ["Die", "Ra\u00b7ben", "kr\u00e4ch\u00b7zen", ",", "die", "Ka\u00b7ter", "schrei\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}