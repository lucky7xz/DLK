{"textgrid.poem.66289": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als der Mittler Gottes auf dem R\u00fccken,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als der Mittler Gottes auf dem R\u00fccken,", "tokens": ["Als", "der", "Mitt\u00b7ler", "Got\u00b7tes", "auf", "dem", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Den die Gei\u00dfel blutig schlug,", "tokens": ["Den", "die", "Gei\u00b7\u00dfel", "blu\u00b7tig", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einst sein Kreuz zur Sch\u00e4delst\u00e4tte trug,", "tokens": ["Einst", "sein", "Kreuz", "zur", "Sch\u00e4\u00b7del\u00b7st\u00e4t\u00b7te", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ach, da sankst du; denn das Dr\u00fccken", "tokens": ["Ach", ",", "da", "sankst", "du", ";", "denn", "das", "Dr\u00fc\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "$.", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deines Kreuzes war zu schwer;", "tokens": ["Dei\u00b7nes", "Kreu\u00b7zes", "war", "zu", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach, da sankst du, Heiliger!", "tokens": ["Ach", ",", "da", "sankst", "du", ",", "Hei\u00b7li\u00b7ger", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ein Wandrer, Simon von Kyrene,", "tokens": ["Und", "ein", "Wand\u00b7rer", ",", "Si\u00b7mon", "von", "Ky\u00b7re\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "NE", "APPR", "NE", "$,"], "meter": "--+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nahm von dir das Kreuzgewicht;", "tokens": ["Nahm", "von", "dir", "das", "Kreuz\u00b7ge\u00b7wicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trug's voll Mitleid \u2013 Hell im Angesicht", "tokens": ["Trug's", "voll", "Mit\u00b7leid", "\u2013", "Hell", "im", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJD", "NN", "$(", "NE", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Schimmerte des Pilgers Thr\u00e4ne \u2013", "tokens": ["Schim\u00b7mer\u00b7te", "des", "Pil\u00b7gers", "Thr\u00e4\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Trug's den Golgatha hinauf,", "tokens": ["Trug's", "den", "Gol\u00b7ga\u00b7tha", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Richtet's unter Sch\u00e4deln auf.", "tokens": ["Rich\u00b7tet's", "un\u00b7ter", "Sch\u00e4\u00b7deln", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mittler, der ich auch im Z\u00e4hrenthale", "tokens": ["Mitt\u00b7ler", ",", "der", "ich", "auch", "im", "Z\u00e4h\u00b7ren\u00b7tha\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Trage meines Kreuzes Last,", "tokens": ["Tra\u00b7ge", "mei\u00b7nes", "Kreu\u00b7zes", "Last", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der du mir es vorgetragen hast,", "tokens": ["Der", "du", "mir", "es", "vor\u00b7ge\u00b7tra\u00b7gen", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Hilf mir, eh' ich unterm Pfahle", "tokens": ["Hilf", "mir", ",", "eh'", "ich", "un\u00b7term", "Pfah\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sink' und lieg'! ich flehe dir;", "tokens": ["Sink'", "und", "lieg'", "!", "ich", "fle\u00b7he", "dir", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "KON", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mittler Gottes, trag es mir.", "tokens": ["Mitt\u00b7ler", "Got\u00b7tes", ",", "trag", "es", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}