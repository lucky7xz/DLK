{"textgrid.poem.54772": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "86. An Lydum", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vorweilen mahlet man den G\u00f6ttern H\u00f6rner an/", "tokens": ["Vor\u00b7wei\u00b7len", "mah\u00b7let", "man", "den", "G\u00f6t\u00b7tern", "H\u00f6r\u00b7ner", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "ART", "NN", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die gibt mir itzt dein Weib. Wann du/ du gutter Mann/", "tokens": ["Die", "gibt", "mir", "itzt", "dein", "Weib", ".", "Wann", "du", "/", "du", "gut\u00b7ter", "Mann", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$.", "PWAV", "PPER", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu jener Zeitt gelebt so hetten dich die Alten/", "tokens": ["Zu", "je\u00b7ner", "Zeitt", "ge\u00b7lebt", "so", "het\u00b7ten", "dich", "die", "Al\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "ADV", "VAFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die nicht so arg als wir/ f\u00fcr einen Gott gehalten.", "tokens": ["Die", "nicht", "so", "arg", "als", "wir", "/", "f\u00fcr", "ei\u00b7nen", "Gott", "ge\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "ADJD", "KOKOM", "PPER", "$(", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}