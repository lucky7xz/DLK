{"dta.poem.5332": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Himmels-Spiegel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einer stillen Nacht, als, leer von Dunst und Duft,", "tokens": ["In", "ei\u00b7ner", "stil\u00b7len", "Nacht", ",", "als", ",", "leer", "von", "Dunst", "und", "Duft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KOUS", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die duncklen zwar doch klaren Schatten", "tokens": ["Die", "dunck\u00b7len", "zwar", "doch", "kla\u00b7ren", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den obern Theil der Welt und untern Theil der Lust", "tokens": ["Den", "o\u00b7bern", "Theil", "der", "Welt", "und", "un\u00b7tern", "Theil", "der", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "KON", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erf\u00fcllet und verh\u00fcllet hatten,", "tokens": ["Er\u00b7f\u00fcl\u00b7let", "und", "ver\u00b7h\u00fcl\u00b7let", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Befand ich mich, an sanfter Anmuth reich,", "tokens": ["Be\u00b7fand", "ich", "mich", ",", "an", "sanf\u00b7ter", "An\u00b7muth", "reich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "An einem grossen Garten-Teich.", "tokens": ["An", "ei\u00b7nem", "gros\u00b7sen", "Gar\u00b7ten\u00b7Teich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Desselben Fluth,", "tokens": ["Des\u00b7sel\u00b7ben", "Fluth", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Die, durch der Winde Ruh, in sanfter Stille ruht,", "tokens": ["Die", ",", "durch", "der", "Win\u00b7de", "Ruh", ",", "in", "sanf\u00b7ter", "Stil\u00b7le", "ruht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "NN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "War einem glatten Spiegel gleich.", "tokens": ["War", "ei\u00b7nem", "glat\u00b7ten", "Spie\u00b7gel", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Man kunte sie zwar selbst, f\u00fcr Dunckelheit, nicht sehn;", "tokens": ["Man", "kun\u00b7te", "sie", "zwar", "selbst", ",", "f\u00fcr", "Dun\u00b7ckel\u00b7heit", ",", "nicht", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "ADV", "$,", "APPR", "NN", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Man sahe wundersch\u00f6n", "tokens": ["Man", "sa\u00b7he", "wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Das blaue Firmament voll Sterne, sonder Zahl,", "tokens": ["Das", "blau\u00b7e", "Fir\u00b7ma\u00b7ment", "voll", "Ster\u00b7ne", ",", "son\u00b7der", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Jm Wiederschein,", "tokens": ["Jm", "Wie\u00b7der\u00b7schein", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Und zwar so hell, so rein, so klar,", "tokens": ["Und", "zwar", "so", "hell", ",", "so", "rein", ",", "so", "klar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Da\u00df zwischen der Copie und dem Original", "tokens": ["Da\u00df", "zwi\u00b7schen", "der", "Co\u00b7pie", "und", "dem", "O\u00b7rig\u00b7i\u00b7nal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "KON", "ART", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.17": {"text": "Fast gar kein Unterscheid, an Glantz und Schimmer, war.", "tokens": ["Fast", "gar", "kein", "Un\u00b7ter\u00b7scheid", ",", "an", "Glantz", "und", "Schim\u00b7mer", ",", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "$,", "APPR", "NN", "KON", "NN", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Es kam mir vor (da wir sonst insgemein", "tokens": ["Es", "kam", "mir", "vor", "(", "da", "wir", "sonst", "ins\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$(", "KOUS", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "So wol mit Blick, als Geist, nicht weiter gehn", "tokens": ["So", "wol", "mit", "Blick", ",", "als", "Geist", ",", "nicht", "wei\u00b7ter", "gehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "$,", "KOUS", "NN", "$,", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und nur den halben Theil des hohen Himmels sehn,", "tokens": ["Und", "nur", "den", "hal\u00b7ben", "Theil", "des", "ho\u00b7hen", "Him\u00b7mels", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Indem wir von der dichten Erden,", "tokens": ["In\u00b7dem", "wir", "von", "der", "dich\u00b7ten", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Den Himmel \u00fcberall zu sehn, behindert werden)", "tokens": ["Den", "Him\u00b7mel", "\u00fc\u00b7be\u00b7rall", "zu", "sehn", ",", "be\u00b7hin\u00b7dert", "wer\u00b7den", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$,", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Als wenn ich hier des Himmels gantze R\u00fcnde", "tokens": ["Als", "wenn", "ich", "hier", "des", "Him\u00b7mels", "gant\u00b7ze", "R\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Mir deutlich vorgestellet f\u00fcnde.", "tokens": ["Mir", "deut\u00b7lich", "vor\u00b7ge\u00b7stel\u00b7let", "f\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Mich deucht, ich seh\u2019 in ungemessner Ferne,", "tokens": ["Mich", "deucht", ",", "ich", "seh'", "in", "un\u00b7ge\u00b7mess\u00b7ner", "Fer\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "So \u00fcber mir, als unter mir,", "tokens": ["So", "\u00fc\u00b7ber", "mir", ",", "als", "un\u00b7ter", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "KOUS", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "In funckelnder und Flammen-reicher Zier,", "tokens": ["In", "fun\u00b7ckeln\u00b7der", "und", "Flam\u00b7men\u00b7rei\u00b7cher", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Ein\u2019 ungezehlte Anzahl Sterne.", "tokens": ["Ein'", "un\u00b7ge\u00b7zehl\u00b7te", "An\u00b7zahl", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Jrrt nun mein Auge gleich; so irren die Gedancken", "tokens": ["Jrrt", "nun", "mein", "Au\u00b7ge", "gleich", ";", "so", "ir\u00b7ren", "die", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ADV", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Jedoch deswegen nicht.", "tokens": ["Je\u00b7doch", "des\u00b7we\u00b7gen", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PAV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}