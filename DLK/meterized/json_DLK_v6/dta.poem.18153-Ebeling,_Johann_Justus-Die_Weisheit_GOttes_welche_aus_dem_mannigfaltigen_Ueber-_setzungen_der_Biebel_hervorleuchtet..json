{"dta.poem.18153": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die Weisheit  GOttes  \n welche aus dem mannigfaltigen Ueber-  \n setzungen der Biebel hervorleuchtet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich bei mir verwunderns voll bedachte,", "tokens": ["Als", "ich", "bei", "mir", "ver\u00b7wun\u00b7derns", "voll", "be\u00b7dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie mancherlei die Sprachen in der", "tokens": ["Wie", "man\u00b7cher\u00b7lei", "die", "Spra\u00b7chen", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ART", "NN", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Welt,", "tokens": ["Welt", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Wie jedes Volk gantz andre W\u00f6rter", "tokens": ["Wie", "je\u00b7des", "Volk", "gantz", "and\u00b7re", "W\u00f6r\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "machte,", "tokens": ["mach\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Nachdem im Gaum der Dinge Aus-", "tokens": ["Nach\u00b7dem", "im", "Gaum", "der", "Din\u00b7ge", "Aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "druk f\u00e4llt:", "tokens": ["druk", "f\u00e4llt", ":"], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "So sprach ich bei mir selbst, wenn alle Zungen,", "tokens": ["So", "sprach", "ich", "bei", "mir", "selbst", ",", "wenn", "al\u00b7le", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ADV", "$,", "KOUS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Von Ost, S\u00fcd, Nord und West, in einem mahl,", "tokens": ["Von", "Ost", ",", "S\u00fcd", ",", "Nord", "und", "West", ",", "in", "ei\u00b7nem", "mahl", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "$,", "NE", "KON", "NN", "$,", "APPR", "ART", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "An einem Ort in ihrer Art erklungen,", "tokens": ["An", "ei\u00b7nem", "Ort", "in", "ih\u00b7rer", "Art", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Welch eine Harmonie von dieser Zahl", "tokens": ["Welch", "ei\u00b7ne", "Har\u00b7mo\u00b7nie", "von", "die\u00b7ser", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "W\u00e4r dann geh\u00f6rt? Ein jeder w\u00fcrde klagen", "tokens": ["W\u00e4r", "dann", "ge\u00b7h\u00f6rt", "?", "Ein", "je\u00b7der", "w\u00fcr\u00b7de", "kla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "VVFIN", "$.", "ART", "PIS", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Den Mischmasch, der aus Babels Thurm entsteht,", "tokens": ["Den", "Mischmasch", ",", "der", "aus", "Ba\u00b7bels", "Thurm", "ent\u00b7steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "K\u00f6nn sein Geh\u00f6r ohnm\u00f6glich lang ertragen.", "tokens": ["K\u00f6nn", "sein", "Ge\u00b7h\u00f6r", "ohn\u00b7m\u00f6g\u00b7lich", "lang", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Doch fiel mir ein, dem Heiland der erh\u00f6ht,", "tokens": ["Doch", "fiel", "mir", "ein", ",", "dem", "Hei\u00b7land", "der", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Jm Himmel nun zur Rechten ", "tokens": ["Jm", "Him\u00b7mel", "nun", "zur", "Rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Gef\u00e4llt der Thon, der unterschiedlich hallt;", "tokens": ["Ge\u00b7f\u00e4llt", "der", "Thon", ",", "der", "un\u00b7ter\u00b7schied\u00b7lich", "hallt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Weil das Gebet der Zungen ihn vergn\u00fcget,", "tokens": ["Weil", "das", "Ge\u00b7bet", "der", "Zun\u00b7gen", "ihn", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Das aus den Hertz, das ihn erkennet, schallt.", "tokens": ["Das", "aus", "den", "Hertz", ",", "das", "ihn", "er\u00b7ken\u00b7net", ",", "schallt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Der H\u00f6chste will, der g\u00fctig, heilig, weise,", "tokens": ["Der", "H\u00f6chs\u00b7te", "will", ",", "der", "g\u00fc\u00b7tig", ",", "hei\u00b7lig", ",", "wei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "PRELS", "ADJD", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Da\u00df er als HErr sei allem Volk bekant:", "tokens": ["Da\u00df", "er", "als", "Herr", "sei", "al\u00b7lem", "Volk", "be\u00b7kant", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "VAFIN", "PIS", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Drum hat er auch in alle Erden Kreise", "tokens": ["Drum", "hat", "er", "auch", "in", "al\u00b7le", "Er\u00b7den", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Sein Evangelium herumgesand.", "tokens": ["Sein", "E\u00b7van\u00b7ge\u00b7li\u00b7um", "her\u00b7um\u00b7ge\u00b7sand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.24": {"text": "Die Boten hatten auch der Zungen Gaben,", "tokens": ["Die", "Bo\u00b7ten", "hat\u00b7ten", "auch", "der", "Zun\u00b7gen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Recht wundernsvoll, da\u00df man darob erstaunt;", "tokens": ["Recht", "wun\u00b7derns\u00b7voll", ",", "da\u00df", "man", "da\u00b7rob", "er\u00b7staunt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "KOUS", "PIS", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Und was wir nur vor eine Sprache haben,", "tokens": ["Und", "was", "wir", "nur", "vor", "ei\u00b7ne", "Spra\u00b7che", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Darinnen ward der Heiland ausposaunt.", "tokens": ["Da\u00b7rin\u00b7nen", "ward", "der", "Hei\u00b7land", "aus\u00b7po\u00b7saunt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Nicht minder ists ein Werk von ", "tokens": ["Nicht", "min\u00b7der", "ists", "ein", "Werk", "von"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VAFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Da\u00df dieses Buch, darin sein Wort uns lehrt,", "tokens": ["Da\u00df", "die\u00b7ses", "Buch", ",", "da\u00b7rin", "sein", "Wort", "uns", "lehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "PAV", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "In so viel Sprachen \u00fcberall zu lesen", "tokens": ["In", "so", "viel", "Spra\u00b7chen", "\u00fc\u00b7be\u00b7rall", "zu", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Als V\u00f6lker sind, die sich zu ihm bekehrt.", "tokens": ["Als", "V\u00f6l\u00b7ker", "sind", ",", "die", "sich", "zu", "ihm", "be\u00b7kehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "$,", "PRELS", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Gleich anfangs war des Hebers Zung erkohren", "tokens": ["Gleich", "an\u00b7fangs", "war", "des", "He\u00b7bers", "Zung", "er\u00b7koh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.34": {"text": "Als diese Sprach in Babel meist verlohren,", "tokens": ["Als", "die\u00b7se", "Sprach", "in", "Ba\u00b7bel", "meist", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "APPR", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Da ward sie auch Chaldeisch \u00fcbersetzt.", "tokens": ["Da", "ward", "sie", "auch", "Chal\u00b7deisch", "\u00fc\u00b7bers\u00b7etzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "Und da das Judenthum herum zertheilet", "tokens": ["Und", "da", "das", "Ju\u00b7den\u00b7thum", "he\u00b7rum", "zer\u00b7thei\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APZR", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Und in Egypten Land so gar zerstreut,", "tokens": ["Und", "in", "E\u00b7gyp\u00b7ten", "Land", "so", "gar", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "So ward da Josephs Schade auch geheilet", "tokens": ["So", "ward", "da", "Jo\u00b7se\u00b7phs", "Scha\u00b7de", "auch", "ge\u00b7hei\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "NE", "ADJD", "ADV", "VVPP"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.40": {"text": "Des Neuen Testaments gegebne Schriften,", "tokens": ["Des", "Neu\u00b7en", "Tes\u00b7ta\u00b7ments", "ge\u00b7geb\u00b7ne", "Schrif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Die von des Heilands Lehr und seinen Mund,", "tokens": ["Die", "von", "des", "Hei\u00b7lands", "Lehr", "und", "sei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Und der Apostel Geist, ein Denkmahl stiften,", "tokens": ["Und", "der", "A\u00b7pos\u00b7tel", "Geist", ",", "ein", "Denk\u00b7mahl", "stif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Die wurden Griegsch am allerersten kund:", "tokens": ["Die", "wur\u00b7den", "Griegsch", "am", "al\u00b7le\u00b7rers\u00b7ten", "kund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Die Mundart war vor andern angenommen,", "tokens": ["Die", "Mund\u00b7art", "war", "vor", "an\u00b7dern", "an\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Weil damahls dies die beste Sprache war,", "tokens": ["Weil", "da\u00b7mahls", "dies", "die", "bes\u00b7te", "Spra\u00b7che", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDS", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Die \u00fcberall in Flor und Ansehn kommen,", "tokens": ["Die", "\u00fc\u00b7be\u00b7rall", "in", "Flor", "und", "An\u00b7sehn", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Wird dadurch nicht die Weisheit offenb\u00e4hr,", "tokens": ["Wird", "da\u00b7durch", "nicht", "die", "Weis\u00b7heit", "of\u00b7fen\u00b7b\u00e4hr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "PTKNEG", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Die bei der Ausbreitung des Heils regieret,", "tokens": ["Die", "bei", "der", "Aus\u00b7brei\u00b7tung", "des", "Heils", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.49": {"text": "Da\u00df sie die Sprach zum Mittel auserw\u00e4hlt,", "tokens": ["Da\u00df", "sie", "die", "Sprach", "zum", "Mit\u00b7tel", "au\u00b7ser\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.50": {"text": "Die damahls war am meisten ausgezieret,", "tokens": ["Die", "da\u00b7mahls", "war", "am", "meis\u00b7ten", "aus\u00b7ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "PTKA", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.51": {"text": "Und unter die Bekantesten gez\u00e4hlt?", "tokens": ["Und", "un\u00b7ter", "die", "Be\u00b7kan\u00b7tes\u00b7ten", "ge\u00b7z\u00e4hlt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.52": {"text": "Gleichwie ein Flus sich mehr und mehr ergiesset,", "tokens": ["Gleich\u00b7wie", "ein", "Flus", "sich", "mehr", "und