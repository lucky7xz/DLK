{"dta.poem.19946": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Guter Rath .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00e4lt Armer dich gefangen noch", "tokens": ["H\u00e4lt", "Ar\u00b7mer", "dich", "ge\u00b7fan\u00b7gen", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "PRF", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Erdentreibens Lust,", "tokens": ["Des", "Er\u00b7den\u00b7trei\u00b7bens", "Lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So dr\u00fccke, dich zu retten, doch", "tokens": ["So", "dr\u00fc\u00b7cke", ",", "dich", "zu", "ret\u00b7ten", ",", "doch"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Kindlein an die Brust.", "tokens": ["Dein", "Kin\u00b7dlein", "an", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Blick' ihm in's Auge unverwandt,", "tokens": ["Blick'", "ihm", "in's", "Au\u00b7ge", "un\u00b7ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tief in den seel'gen Grund,", "tokens": ["Tief", "in", "den", "seel'\u00b7gen", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hab Acht! du siehst das beste Land", "tokens": ["Hab", "Acht", "!", "du", "siehst", "das", "bes\u00b7te", "Land"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "CARD", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Allein in seinem Rund.", "tokens": ["Al\u00b7lein", "in", "sei\u00b7nem", "Rund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dann dr\u00fcck' es fester an das Herz,", "tokens": ["Dann", "dr\u00fcck", "es", "fes\u00b7ter", "an", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo's anschl\u00e4gt bang und laut,", "tokens": ["Wo's", "an\u00b7schl\u00e4gt", "bang", "und", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hab Acht! es zieht heraus den Schmerz", "tokens": ["Hab", "Acht", "!", "es", "zieht", "he\u00b7raus", "den", "Schmerz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "CARD", "$.", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Recht wie ein heilend Kraut.", "tokens": ["Recht", "wie", "ein", "hei\u00b7lend", "Kraut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann leg' es ganz in's Herz hinein,", "tokens": ["Dann", "leg'", "es", "ganz", "in's", "Herz", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schlie\u00df das Herze zu,", "tokens": ["Und", "schlie\u00df", "das", "Her\u00b7ze", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00df nichts anders zu ihm ein,", "tokens": ["Und", "la\u00df", "nichts", "an\u00b7ders", "zu", "ihm", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PIS", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hab Acht! \u2014 so heilest du.", "tokens": ["Hab", "Acht", "!", "so", "hei\u00b7lest", "du", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "$.", "$(", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}