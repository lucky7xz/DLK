{"textgrid.poem.60377": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "T\u00e4uschung", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das K\u00e4uzlein traurig ruft in \u00f6der Felsenritze", "tokens": ["Das", "K\u00e4uz\u00b7lein", "trau\u00b7rig", "ruft", "in", "\u00f6\u00b7der", "Fel\u00b7sen\u00b7rit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gr\u00fc\u00dft mit seinem Lied des Himmels wilde Blitze.", "tokens": ["Und", "gr\u00fc\u00dft", "mit", "sei\u00b7nem", "Lied", "des", "Him\u00b7mels", "wil\u00b7de", "Blit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Als wie ein schwarzer Aar, des Fl\u00fcgel Feuer fingen,", "tokens": ["Als", "wie", "ein", "schwar\u00b7zer", "Aar", ",", "des", "Fl\u00fc\u00b7gel", "Feu\u00b7er", "fin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schl\u00e4gt die schwarze Nacht die feuervollen Schwingen.", "tokens": ["So", "schl\u00e4gt", "die", "schwar\u00b7ze", "Nacht", "die", "feu\u00b7er\u00b7vol\u00b7len", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es gl\u00e4nzt die Regenflut, der finstern Nacht entsunken,", "tokens": ["Es", "gl\u00e4nzt", "die", "Re\u00b7gen\u00b7flut", ",", "der", "fins\u00b7tern", "Nacht", "ent\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Manchmal im Wetterschein wie diamantne Funken.", "tokens": ["Manch\u00b7mal", "im", "Wet\u00b7ter\u00b7schein", "wie", "di\u00b7a\u00b7mant\u00b7ne", "Fun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "KOKOM", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "So kann in banger Nacht ein Strom von hei\u00dfen Z\u00e4hren", "tokens": ["So", "kann", "in", "ban\u00b7ger", "Nacht", "ein", "Strom", "von", "hei\u00b7\u00dfen", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "ADJA", "NN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im hellen Wetterschein des Ungl\u00fccks sich verkl\u00e4ren.", "tokens": ["Im", "hel\u00b7len", "Wet\u00b7ter\u00b7schein", "des", "Un\u00b7gl\u00fccks", "sich", "ver\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Verfangen in der Schlucht, die lauten Winde rasen,", "tokens": ["Ver\u00b7fan\u00b7gen", "in", "der", "Schlucht", ",", "die", "lau\u00b7ten", "Win\u00b7de", "ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die zu der Wolkenschlacht die Riesentuba blasen.", "tokens": ["Die", "zu", "der", "Wol\u00b7ken\u00b7schlacht", "die", "Rie\u00b7sen\u00b7tu\u00b7ba", "bla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Mit Stimmen mannigfalt h\u00f6r ich den Gie\u00dfbach klingen,", "tokens": ["Mit", "Stim\u00b7men", "man\u00b7nig\u00b7falt", "h\u00f6r", "ich", "den", "Gie\u00df\u00b7bach", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie Donner, Kauz und Wind scheint er zugleich zu singen. \u2013", "tokens": ["Wie", "Don\u00b7ner", ",", "Kauz", "und", "Wind", "scheint", "er", "zu\u00b7gleich", "zu", "sin\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "KON", "NN", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Doch nein! mich t\u00e4uscht mein Sinn, als ob zum Wettergrimme", "tokens": ["Doch", "nein", "!", "mich", "t\u00e4uscht", "mein", "Sinn", ",", "als", "ob", "zum", "Wet\u00b7ter\u00b7grim\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKANT", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "KOKOM", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit kl\u00e4glichem Geschrei das Felsenk\u00e4uzlein stimme;", "tokens": ["Mit", "kl\u00e4g\u00b7li\u00b7chem", "Ge\u00b7schrei", "das", "Fel\u00b7sen\u00b7k\u00e4uz\u00b7lein", "stim\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Da\u00df Wolkenschlachtmusik die lauten Winde keuchten,", "tokens": ["Da\u00df", "Wol\u00b7ken\u00b7schlacht\u00b7mu\u00b7sik", "die", "lau\u00b7ten", "Win\u00b7de", "keuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df der Blitz geflammt, den Regen zu beleuchten;", "tokens": ["Und", "da\u00df", "der", "Blitz", "ge\u00b7flammt", ",", "den", "Re\u00b7gen", "zu", "be\u00b7leuch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Und da\u00df der Felsenbach den Wetterstimmen allen", "tokens": ["Und", "da\u00df", "der", "Fel\u00b7sen\u00b7bach", "den", "Wet\u00b7ter\u00b7stim\u00b7men", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Antworten will zugleich in dumpfen Widerhallen.", "tokens": ["Ant\u00b7wor\u00b7ten", "will", "zu\u00b7gleich", "in", "dum\u00b7pfen", "Wi\u00b7der\u00b7hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Einsame Klagen sinds, wei\u00df keine von der andern,", "tokens": ["Ein\u00b7sa\u00b7me", "Kla\u00b7gen", "sinds", ",", "wei\u00df", "kei\u00b7ne", "von", "der", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "VVFIN", "PIAT", "APPR", "ART", "ADJA", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wenn sie zusammen auch im wilden Chore wandern.", "tokens": ["Wenn", "sie", "zu\u00b7sam\u00b7men", "auch", "im", "wil\u00b7den", "Cho\u00b7re", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Drum ist die Erde ja ums Paradies betrogen,", "tokens": ["Drum", "ist", "die", "Er\u00b7de", "ja", "ums", "Pa\u00b7ra\u00b7dies", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ihre Luft ert\u00f6nt von dunklen Monologen.", "tokens": ["Da\u00df", "ih\u00b7re", "Luft", "er\u00b7t\u00f6nt", "von", "dunk\u00b7len", "Mo\u00b7no\u00b7lo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Wenn alle Klagen einst in diesen Erdengr\u00fcnden,", "tokens": ["Wenn", "al\u00b7le", "Kla\u00b7gen", "einst", "in", "die\u00b7sen", "Er\u00b7den\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was jede heimlich meint, einander sich verst\u00fcnden:", "tokens": ["Was", "je\u00b7de", "heim\u00b7lich", "meint", ",", "ein\u00b7an\u00b7der", "sich", "ver\u00b7st\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJD", "VVFIN", "$,", "PRF", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Dann w\u00e4re ja zur\u00fcck das Paradies gewonnen,", "tokens": ["Dann", "w\u00e4\u00b7re", "ja", "zu\u00b7r\u00fcck", "das", "Pa\u00b7ra\u00b7dies", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In einen Freudenschrei das Klaggewirr zerronnen. \u2013", "tokens": ["In", "ei\u00b7nen", "Freu\u00b7den\u00b7schrei", "das", "Klag\u00b7ge\u00b7wirr", "zer\u00b7ron\u00b7nen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Trotz allem Freundeswort, und Mitgef\u00fchlsgeb\u00e4rden,", "tokens": ["Trotz", "al\u00b7lem", "Freun\u00b7des\u00b7wort", ",", "und", "Mit\u00b7ge\u00b7f\u00fchls\u00b7ge\u00b7b\u00e4r\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bleibt jeder tiefe Schmerz ein Eremit auf Erden.", "tokens": ["Bleibt", "je\u00b7der", "tie\u00b7fe", "Schmerz", "ein", "E\u00b7re\u00b7mit", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}