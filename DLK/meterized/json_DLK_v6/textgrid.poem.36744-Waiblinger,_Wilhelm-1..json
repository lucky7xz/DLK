{"textgrid.poem.36744": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00f6\u00dferer Bildner, es \u00f6ffnete dir die verschlossene Vorwelt,", "tokens": ["Gr\u00f6\u00b7\u00dfe\u00b7rer", "Bild\u00b7ner", ",", "es", "\u00f6ff\u00b7ne\u00b7te", "dir", "die", "ver\u00b7schlos\u00b7se\u00b7ne", "Vor\u00b7welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Deinem gelichtetern Blick, tiefer verborgenes Gold,", "tokens": ["Dei\u00b7nem", "ge\u00b7lich\u00b7te\u00b7tern", "Blick", ",", "tie\u00b7fer", "ver\u00b7bor\u00b7ge\u00b7nes", "Gold", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und nicht den sinnlichen Reiz, den erhabenen Sinn und die Hoheit,", "tokens": ["Und", "nicht", "den", "sinn\u00b7li\u00b7chen", "Reiz", ",", "den", "er\u00b7ha\u00b7be\u00b7nen", "Sinn", "und", "die", "Ho\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Geist und Sch\u00f6nheit, Verstand nahmst du begeistert heraus.", "tokens": ["Geist", "und", "Sch\u00f6n\u00b7heit", ",", "Ver\u00b7stand", "nahmst", "du", "be\u00b7geis\u00b7tert", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}}}}