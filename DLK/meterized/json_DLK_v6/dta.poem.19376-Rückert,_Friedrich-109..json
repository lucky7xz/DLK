{"dta.poem.19376": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "109.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu Gottes Angesicht wie steigt sichs schwer empor!", "tokens": ["Zu", "Got\u00b7tes", "An\u00b7ge\u00b7sicht", "wie", "steigt", "sichs", "schwer", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KOKOM", "VVFIN", "PIS", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn sieben Himmel sind, und jeder hat ein Thor.", "tokens": ["Denn", "sie\u00b7ben", "Him\u00b7mel", "sind", ",", "und", "je\u00b7der", "hat", "ein", "Thor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VAFIN", "$,", "KON", "PIS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und ist durchs eine Thor gegeben frei der Lauf,", "tokens": ["Und", "ist", "durchs", "ei\u00b7ne", "Thor", "ge\u00b7ge\u00b7ben", "frei", "der", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "ART", "NN", "VVPP", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So thun deswegen sich noch nicht die andern auf.", "tokens": ["So", "thun", "des\u00b7we\u00b7gen", "sich", "noch", "nicht", "die", "an\u00b7dern", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "PRF", "ADV", "PTKNEG", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was g\u00fcltig ist als Pa\u00df, durch dieses Thor zu kommen,", "tokens": ["Was", "g\u00fcl\u00b7tig", "ist", "als", "Pa\u00df", ",", "durch", "die\u00b7ses", "Thor", "zu", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "KOKOM", "NN", "$,", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird nicht gleich ebenso bei jenem angenommen.", "tokens": ["Wird", "nicht", "gleich", "e\u00b7ben\u00b7so", "bei", "je\u00b7nem", "an\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADV", "APPR", "PDAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Vielmehr wird Reineres von Thor zu Thor begehrt,", "tokens": ["Viel\u00b7mehr", "wird", "Rei\u00b7ne\u00b7res", "von", "Thor", "zu", "Thor", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "APPR", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Reinstes droben sei von Gottes Blick verkl\u00e4rt.", "tokens": ["Da\u00df", "Reins\u00b7tes", "dro\u00b7ben", "sei", "von", "Got\u00b7tes", "Blick", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VAFIN", "APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Engel, die aufs Werk des Menschen merken, tragen", "tokens": ["Die", "En\u00b7gel", ",", "die", "aufs", "Werk", "des", "Men\u00b7schen", "mer\u00b7ken", ",", "tra\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "VVINF", "$,", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Heut eins von ihm empor zum ersten Thor, und sagen:", "tokens": ["Heut", "eins", "von", "ihm", "em\u00b7por", "zum", "ers\u00b7ten", "Thor", ",", "und", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "PPER", "PTKVZ", "APPRART", "ADJA", "NN", "$,", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Thorh\u00fcter, la\u00df uns ein! dis Werk ist sch\u00f6n und rein;", "tokens": ["Thor\u00b7h\u00fc\u00b7ter", ",", "la\u00df", "uns", "ein", "!", "dis", "Werk", "ist", "sch\u00f6n", "und", "rein", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "PTKVZ", "$.", "PDS", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu Gottes Angesicht soll es getragen seyn.", "tokens": ["Zu", "Got\u00b7tes", "An\u00b7ge\u00b7sicht", "soll", "es", "ge\u00b7tra\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der H\u00fcter aber spricht: Wie? ist es fleckenfrei?", "tokens": ["Der", "H\u00fc\u00b7ter", "a\u00b7ber", "spricht", ":", "Wie", "?", "ist", "es", "fle\u00b7cken\u00b7frei", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$.", "PWAV", "$.", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O nein, das ist es nicht, es ist voll Heuchelei.", "tokens": ["O", "nein", ",", "das", "ist", "es", "nicht", ",", "es", "ist", "voll", "Heu\u00b7che\u00b7lei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$,", "PDS", "VAFIN", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Vor Gottes Angesicht kommt ihr mit diesem nicht;", "tokens": ["Vor", "Got\u00b7tes", "An\u00b7ge\u00b7sicht", "kommt", "ihr", "mit", "die\u00b7sem", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "PPER", "APPR", "PDAT", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nehmt es und werft es dort dem Menschen ins Gesicht.", "tokens": ["Nehmt", "es", "und", "werft", "es", "dort", "dem", "Men\u00b7schen", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}