{"dta.poem.75": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die Splitter-Richter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr/ die ihr alles wolt nach euren Urtheil richten/", "tokens": ["Ihr", "/", "die", "ihr", "al\u00b7les", "wolt", "nach", "eu\u00b7ren", "Ur\u00b7theil", "rich\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "PPER", "PIS", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alles fort verwersst/ was euch nicht Probe h\u00e4lt/", "tokens": ["Und", "al\u00b7les", "fort", "ver\u00b7wersst", "/", "was", "euch", "nicht", "Pro\u00b7be", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PTKVZ", "VVFIN", "$(", "PWS", "PPER", "PTKNEG", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gedencket richten ist vielleichter als was dichten/", "tokens": ["Ge\u00b7den\u00b7cket", "rich\u00b7ten", "ist", "viel\u00b7leich\u00b7ter", "als", "was", "dich\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VAFIN", "ADV", "KOUS", "PIS", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und da\u00df des Him\u0303els Thun nicht allen gleich gef\u00e4lt.", "tokens": ["Und", "da\u00df", "des", "Him\u0303els", "Thun", "nicht", "al\u00b7len", "gleich", "ge\u00b7f\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "PTKNEG", "PIS", "ADV", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Gedenckt wer allzu k\u00fchn den Richter-Stuhl besteiget/", "tokens": ["Ge\u00b7denckt", "wer", "all\u00b7zu", "k\u00fchn", "den", "Rich\u00b7ter\u00b7Stuhl", "be\u00b7stei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PWS", "PTKA", "ADJD", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der richtet sich selbst mit/ und zeiget kl\u00e4hrlich an/", "tokens": ["Der", "rich\u00b7tet", "sich", "selbst", "mit", "/", "und", "zei\u00b7get", "kl\u00e4hr\u00b7lich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "APPR", "$(", "KON", "VVFIN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie weit er es versteht/ wozu sein Hertz geneiget/", "tokens": ["Wie", "weit", "er", "es", "ver\u00b7steht", "/", "wo\u00b7zu", "sein", "Hertz", "ge\u00b7nei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPER", "VVFIN", "$(", "PWAV", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wie man an dem Knall ein St\u00fcck erkennen kan.", "tokens": ["So", "wie", "man", "an", "dem", "Knall", "ein", "St\u00fcck", "er\u00b7ken\u00b7nen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PIS", "APPR", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}