{"dta.poem.661": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Zusatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Doch! gesetzt da\u00df auff der Erden ", "tokens": ["Doch", "!", "ge\u00b7setzt", "da\u00df", "auff", "der", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$.", "VVPP", "KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Solch ein Vnmensch/ solch ein stein/ ", "tokens": ["Solch", "ein", "Vn\u00b7mensch", "/", "solch", "ein", "stein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$(", "PIAT", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wol m\u00f6g' anzutreffen seyn/ ", "tokens": ["Wol", "m\u00f6g'", "an\u00b7zu\u00b7tref\u00b7fen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVIZU", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Solch ein Weib/ die nicht will fragen ", "tokens": ["Solch", "ein", "Weib", "/", "die", "nicht", "will", "fra\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "$(", "PRELS", "PTKNEG", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nach dem was jhr Leib getragen/ ", "tokens": ["Nach", "dem", "was", "jhr", "Leib", "ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PRELS", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wiltu darumb trawrig werden? ", "tokens": ["Wil\u00b7tu", "da\u00b7rumb", "traw\u00b7rig", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PAV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nein Sion! wo natur vnd blut ja triegen kan! ", "tokens": ["Nein", "Si\u00b7on", "!", "wo", "na\u00b7tur", "vnd", "blut", "ja", "trie\u00b7gen", "kan", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NE", "$.", "PWAV", "NN", "KON", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nimbt eine Mutter gleich jhr eigen Kindt nicht an: ", "tokens": ["Nimbt", "ei\u00b7ne", "Mut\u00b7ter", "gleich", "jhr", "ei\u00b7gen", "Kindt", "nicht", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PPOSAT", "ADJA", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So glaube doch! ich lasse nicht von dir ", "tokens": ["So", "glau\u00b7be", "doch", "!", "ich", "las\u00b7se", "nicht", "von", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "PTKNEG", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Du bleibest mein: ich sorge f\u00fcr vnd f\u00fcr/ ", "tokens": ["Du", "blei\u00b7best", "mein", ":", "ich", "sor\u00b7ge", "f\u00fcr", "vnd", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$.", "PPER", "NN", "APPR", "KON", "APPR", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "F\u00fcr dich mein Kindt/ di\u00df sey der trewe pfandt: ", "tokens": ["F\u00fcr", "dich", "mein", "Kindt", "/", "di\u00df", "sey", "der", "tre\u00b7we", "pfandt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "$(", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ich habe dich gezeichnet in die Handt. ", "tokens": ["Ich", "ha\u00b7be", "dich", "ge\u00b7zeich\u00b7net", "in", "die", "Handt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}