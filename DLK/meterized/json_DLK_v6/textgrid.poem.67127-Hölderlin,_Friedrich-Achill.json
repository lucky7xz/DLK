{"textgrid.poem.67127": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Achill", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herrlicher G\u00f6ttersohn! da du die Geliebte verloren,", "tokens": ["Herr\u00b7li\u00b7cher", "G\u00f6t\u00b7ter\u00b7sohn", "!", "da", "du", "die", "Ge\u00b7lieb\u00b7te", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Gingst du ans Meergestad, weintest hinaus in die Flut,", "tokens": ["Gingst", "du", "ans", "Meer\u00b7ge\u00b7stad", ",", "wein\u00b7test", "hin\u00b7aus", "in", "die", "Flut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Weheklagend hinab verlangt' in den heiligen Abgrund,", "tokens": ["We\u00b7he\u00b7kla\u00b7gend", "hin\u00b7ab", "ver\u00b7langt'", "in", "den", "hei\u00b7li\u00b7gen", "Ab\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "In die Stille dein Herz, wo, von der Schiffe Gel\u00e4rm", "tokens": ["In", "die", "Stil\u00b7le", "dein", "Herz", ",", "wo", ",", "von", "der", "Schif\u00b7fe", "Ge\u00b7l\u00e4rm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,", "PWAV", "$,", "APPR", "ART", "NN", "NN"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Fern, tief unter den Wogen, in friedlicher Grotte die blaue", "tokens": ["Fern", ",", "tief", "un\u00b7ter", "den", "Wo\u00b7gen", ",", "in", "fried\u00b7li\u00b7cher", "Grot\u00b7te", "die", "blau\u00b7e"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "APPR", "ART", "NN", "$,", "APPR", "ADJA", "NN", "ART", "ADJA"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Thetis wohnte, die dich sch\u00fctzte, die G\u00f6ttin des Meers.", "tokens": ["The\u00b7tis", "wohn\u00b7te", ",", "die", "dich", "sch\u00fctz\u00b7te", ",", "die", "G\u00f6t\u00b7tin", "des", "Meers", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Mutter war dem J\u00fcnglinge sie, die m\u00e4chtige G\u00f6ttin,", "tokens": ["Mut\u00b7ter", "war", "dem", "J\u00fcng\u00b7lin\u00b7ge", "sie", ",", "die", "m\u00e4ch\u00b7ti\u00b7ge", "G\u00f6t\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-++-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Hatte den Knaben einst liebend, am Felsengestad", "tokens": ["Hat\u00b7te", "den", "Kna\u00b7ben", "einst", "lie\u00b7bend", ",", "am", "Fel\u00b7sen\u00b7ge\u00b7stad"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJD", "$,", "APPRART", "NN"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.9": {"text": "Seiner Insel, ges\u00e4ugt, mit dem kr\u00e4ftigen Liede der Welle", "tokens": ["Sei\u00b7ner", "In\u00b7sel", ",", "ge\u00b7s\u00e4ugt", ",", "mit", "dem", "kr\u00e4f\u00b7ti\u00b7gen", "Lie\u00b7de", "der", "Wel\u00b7le"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "$,", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Und im st\u00e4rkenden Bad ihn zum Hero\u00ebn gen\u00e4hrt.", "tokens": ["Und", "im", "st\u00e4r\u00b7ken\u00b7den", "Bad", "ihn", "zum", "He\u00b7ro\u00ebn", "ge\u00b7n\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NE", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "--+--+--++-+", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Und die Mutter vernahm die Weheklage des J\u00fcnglings,", "tokens": ["Und", "die", "Mut\u00b7ter", "ver\u00b7nahm", "die", "We\u00b7he\u00b7kla\u00b7ge", "des", "J\u00fcng\u00b7lings", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Stieg vom Grunde der See, trauernd, wie W\u00f6lkchen, herauf,", "tokens": ["Stieg", "vom", "Grun\u00b7de", "der", "See", ",", "trau\u00b7ernd", ",", "wie", "W\u00f6lk\u00b7chen", ",", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,", "VVPP", "$,", "PWAV", "NN", "$,", "PTKVZ", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Stillte mit z\u00e4rtlichem Umfangen die Schmerzen des Lieblings,", "tokens": ["Still\u00b7te", "mit", "z\u00e4rt\u00b7li\u00b7chem", "Um\u00b7fan\u00b7gen", "die", "Schmer\u00b7zen", "des", "Lieb\u00b7lings", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.14": {"text": "Und er h\u00f6rte, wie sie schmeichelnd zu helfen versprach.", "tokens": ["Und", "er", "h\u00f6r\u00b7te", ",", "wie", "sie", "schmei\u00b7chelnd", "zu", "hel\u00b7fen", "ver\u00b7sprach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "PPER", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "G\u00f6ttersohn! o w\u00e4r ich, wie du, so k\u00f6nnt ich vertraulich", "tokens": ["G\u00f6t\u00b7ter\u00b7sohn", "!", "o", "w\u00e4r", "ich", ",", "wie", "du", ",", "so", "k\u00f6nnt", "ich", "ver\u00b7trau\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "FM", "VAFIN", "PPER", "$,", "PWAV", "PPER", "$,", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+-++--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Einem der Himmlischen klagen mein heimliches Leid.", "tokens": ["Ei\u00b7nem", "der", "Himm\u00b7li\u00b7schen", "kla\u00b7gen", "mein", "heim\u00b7li\u00b7ches", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Sehen soll ich es nicht, soll tragen die Schmach, als geh\u00f6rt ich", "tokens": ["Se\u00b7hen", "soll", "ich", "es", "nicht", ",", "soll", "tra\u00b7gen", "die", "Schmach", ",", "als", "ge\u00b7h\u00f6rt", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "VMFIN", "PPER", "PPER", "PTKNEG", "$,", "VMFIN", "VVINF", "ART", "NN", "$,", "KOUS", "VVFIN", "PPER"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Nimmer zu ihr, die doch meiner mit Tr\u00e4nen gedenkt.", "tokens": ["Nim\u00b7mer", "zu", "ihr", ",", "die", "doch", "mei\u00b7ner", "mit", "Tr\u00e4\u00b7nen", "ge\u00b7denkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "PRELS", "ADV", "PPOSAT", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Gute G\u00f6tter! doch h\u00f6rt ihr jegliches Flehen des Menschen,", "tokens": ["Gu\u00b7te", "G\u00f6t\u00b7ter", "!", "doch", "h\u00f6rt", "ihr", "jeg\u00b7li\u00b7ches", "Fle\u00b7hen", "des", "Men\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Ach! und innig und fromm liebt ich dich heiliges Licht,", "tokens": ["Ach", "!", "und", "in\u00b7nig", "und", "fromm", "liebt", "ich", "dich", "hei\u00b7li\u00b7ges", "Licht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "ADJD", "KON", "ADJD", "VVFIN", "PPER", "PRF", "ADJA", "NN", "$,"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Seit ich lebe, dich Erd und deine Quellen und W\u00e4lder,", "tokens": ["Seit", "ich", "le\u00b7be", ",", "dich", "Erd", "und", "dei\u00b7ne", "Quel\u00b7len", "und", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PPER", "NN", "KON", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-++--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Vater Aether, und dich f\u00fchlte zu sehnend und rein", "tokens": ["Va\u00b7ter", "A\u00b7e\u00b7ther", ",", "und", "dich", "f\u00fchl\u00b7te", "zu", "seh\u00b7nend", "und", "rein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "KON", "PPER", "VVFIN", "PTKA", "ADJD", "KON", "ADJD"], "meter": "+-+----+--+--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Dieses Herz \u2013 o s\u00e4nftiget mir, ihr Guten, mein Leiden,", "tokens": ["Die\u00b7ses", "Herz", "\u2013", "o", "s\u00e4nf\u00b7ti\u00b7get", "mir", ",", "ihr", "Gu\u00b7ten", ",", "mein", "Lei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$(", "FM", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Da\u00df die Seele mir nicht allzu fr\u00fche verstummt,", "tokens": ["Da\u00df", "die", "See\u00b7le", "mir", "nicht", "all\u00b7zu", "fr\u00fc\u00b7he", "ver\u00b7stummt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PTKNEG", "PTKA", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.11": {"text": "Da\u00df ich lebe und euch, ihr hohen himmlischen M\u00e4chte,", "tokens": ["Da\u00df", "ich", "le\u00b7be", "und", "euch", ",", "ihr", "ho\u00b7hen", "himm\u00b7li\u00b7schen", "M\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "PPER", "$,", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Noch am fliehenden Tag danke mit frommem Gesang,", "tokens": ["Noch", "am", "flie\u00b7hen\u00b7den", "Tag", "dan\u00b7ke", "mit", "from\u00b7mem", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Danke f\u00fcr voriges Gut, f\u00fcr Freuden vergangener Jugend,", "tokens": ["Dan\u00b7ke", "f\u00fcr", "vo\u00b7ri\u00b7ges", "Gut", ",", "f\u00fcr", "Freu\u00b7den", "ver\u00b7gan\u00b7ge\u00b7ner", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ADJA", "NN", "$,", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Und dann nehmet zu euch g\u00fctig den Einsamen auf.", "tokens": ["Und", "dann", "neh\u00b7met", "zu", "euch", "g\u00fc\u00b7tig", "den", "Ein\u00b7sa\u00b7men", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}