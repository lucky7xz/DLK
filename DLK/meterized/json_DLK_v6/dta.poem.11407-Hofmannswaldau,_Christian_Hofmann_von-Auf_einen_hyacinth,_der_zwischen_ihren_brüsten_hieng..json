{"dta.poem.11407": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen hyacinth, der zwischen ihren  \n br\u00fcsten hieng.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beliebter hyacinth! der auf den rosen-h\u00fcgeln,", "tokens": ["Be\u00b7lieb\u00b7ter", "hya\u00b7cin\u00b7th", "!", "der", "auf", "den", "ro\u00b7sen\u00b7h\u00fc\u00b7geln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ART", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um die Cupido lauscht, und auf die hertzen pa\u00dft,", "tokens": ["Um", "die", "Cu\u00b7pi\u00b7do", "lauscht", ",", "und", "auf", "die", "hert\u00b7zen", "pa\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NE", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die crone sencket und erbla\u00dft,", "tokens": ["Die", "cro\u00b7ne", "sen\u00b7cket", "und", "er\u00b7bla\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es kan mein zustand sich recht in dem deinen spiegeln.", "tokens": ["Es", "kan", "mein", "zu\u00b7stand", "sich", "recht", "in", "dem", "dei\u00b7nen", "spie\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "VVFIN", "PRF", "ADJD", "APPR", "ART", "PPOSAT", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Denn Amarillis ist uns allen beyden scharff:", "tokens": ["Denn", "A\u00b7ma\u00b7ril\u00b7lis", "ist", "uns", "al\u00b7len", "bey\u00b7den", "scharff", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "PPER", "PIAT", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dich hat sie von dem stock und mir das hertz entrissen;", "tokens": ["Dich", "hat", "sie", "von", "dem", "stock", "und", "mir", "das", "hertz", "ent\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "KON", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Allein dir ist verg\u00f6nnt, das paradies zu k\u00fcssen,", "tokens": ["Al\u00b7lein", "dir", "ist", "ver\u00b7g\u00f6nnt", ",", "das", "pa\u00b7ra\u00b7dies", "zu", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "VVPP", "$,", "PRELS", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dahin sich nicht einmahl mein auge wagen darff.", "tokens": ["Da\u00b7hin", "sich", "nicht", "ein\u00b7mahl", "mein", "au\u00b7ge", "wa\u00b7gen", "darff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "PTKNEG", "ADV", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}