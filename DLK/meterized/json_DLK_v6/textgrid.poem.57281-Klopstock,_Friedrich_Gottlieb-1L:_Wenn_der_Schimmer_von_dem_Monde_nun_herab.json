{"textgrid.poem.57281": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn der Schimmer von dem Monde nun herab", "genre": "verse", "period": "N.A.", "pub_year": 1766, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Schimmer von dem Monde nun herab", "tokens": ["Wenn", "der", "Schim\u00b7mer", "von", "dem", "Mon\u00b7de", "nun", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NE", "ADV", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "In die W\u00e4lder sich eigiesst, und Ger\u00fcche", "tokens": ["In", "die", "W\u00e4l\u00b7der", "sich", "ei\u00b7giesst", ",", "und", "Ge\u00b7r\u00fc\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "PRF", "ADV", "$,", "KON", "NN"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mit den D\u00fcften von der Linde", "tokens": ["Mit", "den", "D\u00fcf\u00b7ten", "von", "der", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den K\u00fchlungen wehn;", "tokens": ["In", "den", "K\u00fch\u00b7lun\u00b7gen", "wehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "So umschatten mich Gedanken an das Grab", "tokens": ["So", "um\u00b7schat\u00b7ten", "mich", "Ge\u00b7dan\u00b7ken", "an", "das", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Der Geliebten, und ich seh in dem Walde", "tokens": ["Der", "Ge\u00b7lieb\u00b7ten", ",", "und", "ich", "seh", "in", "dem", "Wal\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nur es d\u00e4mmern, und es weht mir", "tokens": ["Nur", "es", "d\u00e4m\u00b7mern", ",", "und", "es", "weht", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVINF", "$,", "KON", "PPER", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der Bl\u00fcthe nicht her.", "tokens": ["Von", "der", "Bl\u00fc\u00b7the", "nicht", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Ich genoss einst, o ihr Todten, es mit euch!", "tokens": ["Ich", "ge\u00b7noss", "einst", ",", "o", "ihr", "Tod\u00b7ten", ",", "es", "mit", "euch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "FM", "PPOSAT", "NN", "$,", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wie umwehten uns der Duft und die K\u00fchlung,", "tokens": ["Wie", "um\u00b7weh\u00b7ten", "uns", "der", "Duft", "und", "die", "K\u00fch\u00b7lung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie versch\u00f6nt warst von dem Monde,", "tokens": ["Wie", "ver\u00b7sch\u00f6nt", "warst", "von", "dem", "Mon\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAFIN", "APPR", "ART", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du, o sch\u00f6ne Natur!", "tokens": ["Du", ",", "o", "sch\u00f6\u00b7ne", "Na\u00b7tur", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}