{"textgrid.poem.60026": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: W\u00e4lze dich hinweg, du wildes Feuer!", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4lze dich hinweg, du wildes Feuer!", "tokens": ["W\u00e4l\u00b7ze", "dich", "hin\u00b7weg", ",", "du", "wil\u00b7des", "Feu\u00b7er", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APZR", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Diese Saiten hat ein Gott gekr\u00f6nt;", "tokens": ["Die\u00b7se", "Sai\u00b7ten", "hat", "ein", "Gott", "ge\u00b7kr\u00f6nt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Er, mit welchem jedes Ungeheuer,", "tokens": ["Er", ",", "mit", "wel\u00b7chem", "je\u00b7des", "Un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "PRELS", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und vielleicht die H\u00f6lle sich vers\u00f6hnt.", "tokens": ["Und", "viel\u00b7leicht", "die", "H\u00f6l\u00b7le", "sich", "ver\u00b7s\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Meine Saiten stimmte seine Rechte:", "tokens": ["Mei\u00b7ne", "Sai\u00b7ten", "stimm\u00b7te", "sei\u00b7ne", "Rech\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fcrchterliche Schatten, flieht!", "tokens": ["F\u00fcrch\u00b7ter\u00b7li\u00b7che", "Schat\u00b7ten", ",", "flieht", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr winselnden Bewohner dieser N\u00e4chte,", "tokens": ["Und", "ihr", "win\u00b7seln\u00b7den", "Be\u00b7woh\u00b7ner", "die\u00b7ser", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Horchet auf mein Lied!", "tokens": ["Hor\u00b7chet", "auf", "mein", "Lied", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Von der Erde, wo die Sonne leuchtet,", "tokens": ["Von", "der", "Er\u00b7de", ",", "wo", "die", "Son\u00b7ne", "leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und der stille Mond;", "tokens": ["Und", "der", "stil\u00b7le", "Mond", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wo der Thau das junge Moos befeuchtet,", "tokens": ["Wo", "der", "Thau", "das", "jun\u00b7ge", "Moos", "be\u00b7feuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo Gesang im gr\u00fcnen Felde wohnt;", "tokens": ["Wo", "Ge\u00b7sang", "im", "gr\u00fc\u00b7nen", "Fel\u00b7de", "wohnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Aus der Menschen s\u00fc\u00dfem Vaterlande,", "tokens": ["Aus", "der", "Men\u00b7schen", "s\u00fc\u00b7\u00dfem", "Va\u00b7ter\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo der Himmel euch so frohe Blicke gab,", "tokens": ["Wo", "der", "Him\u00b7mel", "euch", "so", "fro\u00b7he", "Bli\u00b7cke", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Ziehen mich die sch\u00f6nsten Bande,", "tokens": ["Zie\u00b7hen", "mich", "die", "sch\u00f6ns\u00b7ten", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ziehet mich die Liebe selbst herab.", "tokens": ["Zie\u00b7het", "mich", "die", "Lie\u00b7be", "selbst", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Meine Klage t\u00f6nt in eure Klage;", "tokens": ["Mei\u00b7ne", "Kla\u00b7ge", "t\u00f6nt", "in", "eu\u00b7re", "Kla\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weit von hier geflohen ist das Gl\u00fcck;", "tokens": ["Weit", "von", "hier", "ge\u00b7flo\u00b7hen", "ist", "das", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADV", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aber denkt an jene Tage,", "tokens": ["A\u00b7ber", "denkt", "an", "je\u00b7ne", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schaut in jene Welt zur\u00fcck!", "tokens": ["Schaut", "in", "je\u00b7ne", "Welt", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn ihr da nur einen Leidenden umarmtet,", "tokens": ["Wenn", "ihr", "da", "nur", "ei\u00b7nen", "Lei\u00b7den\u00b7den", "um\u00b7arm\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "----+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "O so f\u00fchlt die Wollust noch einmal;", "tokens": ["O", "so", "f\u00fchlt", "die", "Wol\u00b7lust", "noch", "ein\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und der Augenblick, in dem ihr euch erbarmtet,", "tokens": ["Und", "der", "Au\u00b7gen\u00b7blick", ",", "in", "dem", "ihr", "euch", "er\u00b7barm\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Lindre diese lange Qual!", "tokens": ["Lind\u00b7re", "die\u00b7se", "lan\u00b7ge", "Qual", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "O ich sehe Thr\u00e4nen flie\u00dfen!", "tokens": ["O", "ich", "se\u00b7he", "Thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Finsternisse bricht", "tokens": ["Durch", "die", "Fins\u00b7ter\u00b7nis\u00b7se", "bricht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun ein Strahl von Hoffnung; ewig b\u00fc\u00dfen", "tokens": ["Nun", "ein", "Strahl", "von", "Hoff\u00b7nung", ";", "e\u00b7wig", "b\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "$.", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lassen euch die guten G\u00f6tter nicht!", "tokens": ["Las\u00b7sen", "euch", "die", "gu\u00b7ten", "G\u00f6t\u00b7ter", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "G\u00f6tter, die f\u00fcr euch die Erde schufen,", "tokens": ["G\u00f6t\u00b7ter", ",", "die", "f\u00fcr", "euch", "die", "Er\u00b7de", "schu\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Werden aus der tiefen Nacht", "tokens": ["Wer\u00b7den", "aus", "der", "tie\u00b7fen", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Euch in selige Gefilde rufen,", "tokens": ["Euch", "in", "se\u00b7li\u00b7ge", "Ge\u00b7fil\u00b7de", "ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo die Tugend unter Rosen lacht.", "tokens": ["Wo", "die", "Tu\u00b7gend", "un\u00b7ter", "Ro\u00b7sen", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}