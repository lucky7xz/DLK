{"textgrid.poem.45220": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[wird ein Gef\u00e4ss gesch\u00fcttelt]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wird ein Gef\u00e4ss gesch\u00fcttelt,", "tokens": ["Wird", "ein", "Ge\u00b7f\u00e4ss", "ge\u00b7sch\u00fct\u00b7telt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So steigt nach oben der Bodensatz,", "tokens": ["So", "steigt", "nach", "o\u00b7ben", "der", "Bo\u00b7den\u00b7satz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bleibts dann ein Weilchen unger\u00fcttelt,", "tokens": ["Bleibts", "dann", "ein", "Weil\u00b7chen", "un\u00b7ge\u00b7r\u00fct\u00b7telt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So nimmt er wieder unten Platz.", "tokens": ["So", "nimmt", "er", "wie\u00b7der", "un\u00b7ten", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}