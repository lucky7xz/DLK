{"textgrid.poem.67570": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht des verlebten Tages Zier,", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht des verlebten Tages Zier,", "tokens": ["Nicht", "des", "ver\u00b7leb\u00b7ten", "Ta\u00b7ges", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Psyche, keine welke Rose", "tokens": ["O", "Psy\u00b7che", ",", "kei\u00b7ne", "wel\u00b7ke", "Ro\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sei Traueropfer Dir", "tokens": ["Sei", "Trau\u00b7er\u00b7op\u00b7fer", "Dir"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Auf todtem Moose!", "tokens": ["Auf", "tod\u00b7tem", "Moo\u00b7se", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Welch Opfer! welch Altar! und d\u00fcster", "tokens": ["Welch", "Op\u00b7fer", "!", "welch", "Al\u00b7tar", "!", "und", "d\u00fcs\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$.", "PWAT", "NN", "$.", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Gegend! und ein irrer G\u00f6tzenpriester,", "tokens": ["Die", "Ge\u00b7gend", "!", "und", "ein", "ir\u00b7rer", "G\u00f6t\u00b7zen\u00b7pries\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der diesen Fels erstieg und ungeweiht ihn sang", "tokens": ["Der", "die\u00b7sen", "Fels", "er\u00b7stieg", "und", "un\u00b7ge\u00b7weiht", "ihn", "sang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PDAT", "NN", "VVFIN", "KON", "ADJD", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und frecher Hand ihm ein den Namen zwang", "tokens": ["Und", "fre\u00b7cher", "Hand", "ihm", "ein", "den", "Na\u00b7men", "zwang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "PPER", "ART", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und traurig Opfer Dir befahl!", "tokens": ["Und", "trau\u00b7rig", "Op\u00b7fer", "Dir", "be\u00b7fahl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Psyche, sieh von Fels und Oed' einmal", "tokens": ["O", "Psy\u00b7che", ",", "sieh", "von", "Fels", "und", "O\u00b7ed'", "ein\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "APPR", "NE", "KON", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gen Himmel! Wie er weit", "tokens": ["Gen", "Him\u00b7mel", "!", "Wie", "er", "weit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$.", "PWAV", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und blau und sch\u00f6n sich w\u00f6lbt und ohne Maa\u00df und Zahl", "tokens": ["Und", "blau", "und", "sch\u00f6n", "sich", "w\u00f6lbt", "und", "oh\u00b7ne", "Maa\u00df", "und", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "PRF", "VVFIN", "KON", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Umher die Strahlen Gottes streut", "tokens": ["Um\u00b7her", "die", "Strah\u00b7len", "Got\u00b7tes", "streut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und alle Welt umfa\u00dft und weit und breit", "tokens": ["Und", "al\u00b7le", "Welt", "um\u00b7fa\u00dft", "und", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Hier eine H\u00fctte, dort ein kleines Thal", "tokens": ["Hier", "ei\u00b7ne", "H\u00fct\u00b7te", ",", "dort", "ein", "klei\u00b7nes", "Thal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zu Elyseer Freuden weiht", "tokens": ["Zu", "E\u00b7ly\u00b7seer", "Freu\u00b7den", "weiht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und l\u00e4\u00dft den Felsen stehn!", "tokens": ["Und", "l\u00e4\u00dft", "den", "Fel\u00b7sen", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und, Psyche, siehe dort,", "tokens": ["Und", ",", "Psy\u00b7che", ",", "sie\u00b7he", "dort", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "VVIMP", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie V\u00f6gel hin zu fernen Himmeln ziehn", "tokens": ["Wie", "V\u00f6\u00b7gel", "hin", "zu", "fer\u00b7nen", "Him\u00b7meln", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und finden \u2013 Trauerort", "tokens": ["Und", "fin\u00b7den", "\u2013", "Trau\u00b7er\u00b7ort"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "VVINF", "$(", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und \u2013 wohnen ihn!", "tokens": ["Und", "\u2013", "woh\u00b7nen", "ihn", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$(", "PWAV", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und lassen aus den Winter toben", "tokens": ["Und", "las\u00b7sen", "aus", "den", "Win\u00b7ter", "to\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und lassen auf den Fr\u00fchling bl\u00fchn", "tokens": ["Und", "las\u00b7sen", "auf", "den", "Fr\u00fch\u00b7ling", "bl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und erst ein Laubdach sich um diese Wilde ziehn;", "tokens": ["Und", "erst", "ein", "Laub\u00b7dach", "sich", "um", "die\u00b7se", "Wil\u00b7de", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PRF", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dann kommt mit Loben", "tokens": ["Dann", "kommt", "mit", "Lo\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Des Herrn die Nachtigall", "tokens": ["Des", "Herrn", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Und f\u00fcllt mit tausend neuem Schall", "tokens": ["Und", "f\u00fcllt", "mit", "tau\u00b7send", "neu\u00b7em", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die sch\u00f6ne Wilde! Busch und Thal,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Wil\u00b7de", "!", "Busch", "und", "Thal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und Busch und Thal und Mitternacht", "tokens": ["Und", "Busch", "und", "Thal", "und", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ist Lobgesang. \u2013 O Du,", "tokens": ["Ist", "Lob\u00b7ge\u00b7sang", ".", "\u2013", "O", "Du", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$.", "$(", "NE", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Dort an den Fels gelehnt, Du seufzend M\u00e4dchen, blicke", "tokens": ["Dort", "an", "den", "Fels", "ge\u00b7lehnt", ",", "Du", "seuf\u00b7zend", "M\u00e4d\u00b7chen", ",", "bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,", "PPER", "VVPP", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mit Deinem schwimmenden, zerflossnen Thr\u00e4nenblicke", "tokens": ["Mit", "Dei\u00b7nem", "schwim\u00b7men\u00b7den", ",", "zer\u00b7floss\u00b7nen", "Thr\u00e4\u00b7nen\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nicht in die Ferne! Sie ist Nacht,", "tokens": ["Nicht", "in", "die", "Fer\u00b7ne", "!", "Sie", "ist", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ist Nebel! Aber Himmelsnacht,", "tokens": ["Ist", "Ne\u00b7bel", "!", "A\u00b7ber", "Him\u00b7mels\u00b7nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$.", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "O M\u00e4dchen, ist dem tr\u00fcben Menschenblicke", "tokens": ["O", "M\u00e4d\u00b7chen", ",", "ist", "dem", "tr\u00fc\u00b7ben", "Men\u00b7schen\u00b7bli\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Nur Strahlennebel vorgewebet, Ruh", "tokens": ["Nur", "Strah\u00b7len\u00b7ne\u00b7bel", "vor\u00b7ge\u00b7we\u00b7bet", ",", "Ruh"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "NN", "VVFIN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Des Auges, da\u00df dort ungesehn", "tokens": ["Des", "Au\u00b7ges", ",", "da\u00df", "dort", "un\u00b7ge\u00b7sehn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die Palmen bl\u00fchn.", "tokens": ["Die", "Pal\u00b7men", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Und wenn Du nahst, und wenn die Nebel fliehn,", "tokens": ["Und", "wenn", "Du", "nahst", ",", "und", "wenn", "die", "Ne\u00b7bel", "fliehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "KON", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und hier der Morgen sch\u00f6n,", "tokens": ["Und", "hier", "der", "Mor\u00b7gen", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "O Psyche, sch\u00f6n erwacht,", "tokens": ["O", "Psy\u00b7che", ",", "sch\u00f6n", "er\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Sieh, welch Elysium erwacht!", "tokens": ["Sieh", ",", "welch", "E\u00b7ly\u00b7si\u00b7um", "er\u00b7wacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dort an den Fels gelehnt, Du seufzend M\u00e4dchen, h\u00f6re", "tokens": ["Dort", "an", "den", "Fels", "ge\u00b7lehnt", ",", "Du", "seuf\u00b7zend", "M\u00e4d\u00b7chen", ",", "h\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,", "PPER", "VVPP", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Deiner Oede ferne Ch\u00f6re", "tokens": ["In", "Dei\u00b7ner", "O\u00b7e\u00b7de", "fer\u00b7ne", "Ch\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Wehmuthslieb' und Trauersangs", "tokens": ["Der", "Weh\u00b7muths\u00b7lieb'", "und", "Trau\u00b7er\u00b7sangs"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie s\u00fc\u00dfer Saiten \u2013 s\u00fc\u00dfen Klangs", "tokens": ["Wie", "s\u00fc\u00b7\u00dfer", "Sai\u00b7ten", "\u2013", "s\u00fc\u00b7\u00dfen", "Klangs"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "$(", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Thr\u00e4nen, wie, in Thr\u00e4nen", "tokens": ["Der", "Thr\u00e4\u00b7nen", ",", "wie", ",", "in", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "$,", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gebadet, dort von fernher flie\u00dft", "tokens": ["Ge\u00b7ba\u00b7det", ",", "dort", "von", "fern\u00b7her", "flie\u00dft"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "ADV", "APPR", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Lilie und Sehnen", "tokens": ["Die", "Li\u00b7lie", "und", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Umher ergie\u00dft!", "tokens": ["Um\u00b7her", "er\u00b7gie\u00dft", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Wer seid Ihr T\u00f6ne", "tokens": ["Wer", "seid", "Ihr", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Der Trauerlustharf'? \u00bbSind die S\u00f6hne", "tokens": ["Der", "Trau\u00b7e\u00b7rlust\u00b7ha\u00b7rf'", "?", "\u00bb", "Sind", "die", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "$(", "VAFIN", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Des edlen Nordens aus der H\u00f6hle", "tokens": ["Des", "ed\u00b7len", "Nor\u00b7dens", "aus", "der", "H\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Gekommen Ossian's, und sehnen", "tokens": ["Ge\u00b7kom\u00b7men", "Os\u00b7si\u00b7an's", ",", "und", "seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "$,", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Uns, suchen edle Seele", "tokens": ["Uns", ",", "su\u00b7chen", "ed\u00b7le", "See\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Zu tr\u00f6sten?\u00ab", "tokens": ["Zu", "tr\u00f6s\u00b7ten", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "An den Fels gelehnt, 