{"dta.poem.13556": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Er fragt nicht viel, wie ihn die Menge richte,", "tokens": ["Er", "fragt", "nicht", "viel", ",", "wie", "ihn", "die", "Men\u00b7ge", "rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der eignen Ehr' nur in der Brust vereidet;", "tokens": ["Der", "eig\u00b7nen", "Ehr'", "nur", "in", "der", "Brust", "ver\u00b7ei\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn wo begeistert er die Blicke weidet,", "tokens": ["Denn", "wo", "be\u00b7geis\u00b7tert", "er", "die", "Bli\u00b7cke", "wei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gr\u00fc\u00dft ihn der Weltkreis mit verwandtem Lichte.", "tokens": ["Gr\u00fc\u00dft", "ihn", "der", "Welt\u00b7kreis", "mit", "ver\u00b7wand\u00b7tem", "Lich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}