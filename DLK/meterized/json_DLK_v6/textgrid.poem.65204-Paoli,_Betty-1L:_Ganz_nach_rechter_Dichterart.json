{"textgrid.poem.65204": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ganz nach rechter Dichterart", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ganz nach rechter Dichterart", "tokens": ["Ganz", "nach", "rech\u00b7ter", "Dich\u00b7ter\u00b7art"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist in deines Wesens F\u00fclle", "tokens": ["Ist", "in", "dei\u00b7nes", "We\u00b7sens", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ernst mit Heiterkeit gepaart.", "tokens": ["Ernst", "mit", "Hei\u00b7ter\u00b7keit", "ge\u00b7paart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Leicht dein Sinn und fest dein Wille,", "tokens": ["Leicht", "dein", "Sinn", "und", "fest", "dein", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Klar dein Blick und frisch dein Mut,", "tokens": ["Klar", "dein", "Blick", "und", "frisch", "dein", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dein des Herzens tiefe Glut.", "tokens": ["Dein", "des", "Her\u00b7zens", "tie\u00b7fe", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00e4hrend wir auf steilen Pfaden", "tokens": ["W\u00e4h\u00b7rend", "wir", "auf", "stei\u00b7len", "Pfa\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "M\u00fchsam wandern und beladen,", "tokens": ["M\u00fch\u00b7sam", "wan\u00b7dern", "und", "be\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Tr\u00e4gt beschwingt dich der Humor", "tokens": ["Tr\u00e4gt", "be\u00b7schwingt", "dich", "der", "Hu\u00b7mor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.10": {"text": "In sein lichtes Reich empor.", "tokens": ["In", "sein", "lich\u00b7tes", "Reich", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Von den k\u00f6stlichsten Geschenken,", "tokens": ["Von", "den", "k\u00f6st\u00b7lichs\u00b7ten", "Ge\u00b7schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--++--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Womit G\u00f6tter uns bedenken,", "tokens": ["Wo\u00b7mit", "G\u00f6t\u00b7ter", "uns", "be\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Ist dir kein's versagt geblieben \u2013", "tokens": ["Ist", "dir", "kein's", "ver\u00b7sagt", "ge\u00b7blie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "VVFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und man sollte dich nicht lieben?!", "tokens": ["Und", "man", "soll\u00b7te", "dich", "nicht", "lie\u00b7ben", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}