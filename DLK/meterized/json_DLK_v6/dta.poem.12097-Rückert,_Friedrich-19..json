{"dta.poem.12097": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im Garten sah ich B\u00e4um' auf eigne Art benutzt,", "tokens": ["Im", "Gar\u00b7ten", "sah", "ich", "B\u00e4um'", "auf", "eig\u00b7ne", "Art", "be\u00b7nutzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Seiten\u00e4ste samt dem Wipfel weggestutzt.", "tokens": ["Die", "Sei\u00b7te\u00b7n\u00e4s\u00b7te", "samt", "dem", "Wip\u00b7fel", "weg\u00b7ges\u00b7tutzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Verwundert fragt' ich, was die St\u00fcmmlung soll bedeuten?", "tokens": ["Ver\u00b7wun\u00b7dert", "fragt'", "ich", ",", "was", "die", "St\u00fcmm\u00b7lung", "soll", "be\u00b7deu\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und angegeben ward der Grund mir von den Leuten:", "tokens": ["Und", "an\u00b7ge\u00b7ge\u00b7ben", "ward", "der", "Grund", "mir", "von", "den", "Leu\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nach dieser Seite fiel das Obst dem Waldbach zu,", "tokens": ["Nach", "die\u00b7ser", "Sei\u00b7te", "fiel", "das", "Obst", "dem", "Wald\u00b7bach", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und oben kam allein des Vogels Flug dazu.", "tokens": ["Und", "o\u00b7ben", "kam", "al\u00b7lein", "des", "Vo\u00b7gels", "Flug", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was wir von Aesten hier und droben weggenommen,", "tokens": ["Was", "wir", "von", "A\u00b7es\u00b7ten", "hier", "und", "dro\u00b7ben", "weg\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "ADV", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auf andern Seiten wird es uns zu Statten kommen.", "tokens": ["Auf", "an\u00b7dern", "Sei\u00b7ten", "wird", "es", "uns", "zu", "Stat\u00b7ten", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wir ziehn nicht unsern Baum zur Sch\u00f6nheit wild und frei;", "tokens": ["Wir", "ziehn", "nicht", "un\u00b7sern", "Baum", "zur", "Sch\u00f6n\u00b7heit", "wild", "und", "frei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir ziehn f\u00fcr uns das Obst, wie schief der Astwuchs sei.", "tokens": ["Wir", "ziehn", "f\u00fcr", "uns", "das", "Obst", ",", "wie", "schief", "der", "Ast\u00b7wuchs", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$,", "PWAV", "VVFIN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}