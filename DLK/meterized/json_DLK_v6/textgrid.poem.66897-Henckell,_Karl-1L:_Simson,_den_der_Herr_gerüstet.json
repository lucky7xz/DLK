{"textgrid.poem.66897": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Simson, den der Herr ger\u00fcstet", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Simson, den der Herr ger\u00fcstet", "tokens": ["Sim\u00b7son", ",", "den", "der", "Herr", "ge\u00b7r\u00fcs\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit Gewalt erschrecklich gro\u00df,", "tokens": ["Mit", "Ge\u00b7walt", "er\u00b7schreck\u00b7lich", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprach zum Weibe: \u00bbMich gel\u00fcstet!\u00ab", "tokens": ["Sprach", "zum", "Wei\u00b7be", ":", "\u00bb", "Mich", "ge\u00b7l\u00fcs\u00b7tet", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "APPRART", "NN", "$.", "$(", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er lag in ihrem Scho\u00df.", "tokens": ["Und", "er", "lag", "in", "ih\u00b7rem", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und das Weib qu\u00e4lt ihn am Tage,", "tokens": ["Und", "das", "Weib", "qu\u00e4lt", "ihn", "am", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und es plagt ihn bei der Nacht:", "tokens": ["Und", "es", "plagt", "ihn", "bei", "der", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbsage, lieber Simson, sage,", "tokens": ["\u00bb", "sa\u00b7ge", ",", "lie\u00b7ber", "Sim\u00b7son", ",", "sa\u00b7ge", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "ADV", "NE", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Worin stecket deine Macht?\u00ab", "tokens": ["Wo\u00b7rin", "ste\u00b7cket", "dei\u00b7ne", "Macht", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und der den Philistern Keile", "tokens": ["Und", "der", "den", "Phi\u00b7lis\u00b7tern", "Kei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gab mit einem Tiergebi\u00df,", "tokens": ["Gab", "mit", "ei\u00b7nem", "Tier\u00b7ge\u00b7bi\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der die allerdicksten Seile", "tokens": ["Der", "die", "al\u00b7ler\u00b7dicks\u00b7ten", "Sei\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schier wie Spinneweb zerri\u00df,", "tokens": ["Schier", "wie", "Spin\u00b7ne\u00b7web", "zer\u00b7ri\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der, um seine Kraft zu kosten,", "tokens": ["Der", ",", "um", "sei\u00b7ne", "Kraft", "zu", "kos\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An 'nem H\u00fcrlein nicht genug,", "tokens": ["An", "'nem", "H\u00fcr\u00b7lein", "nicht", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Noch ein Stadttor samt den Pfosten", "tokens": ["Noch", "ein", "Stadt\u00b7tor", "samt", "den", "Pfos\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "N\u00e4chtlich auf die Berge trug:", "tokens": ["N\u00e4cht\u00b7lich", "auf", "die", "Ber\u00b7ge", "trug", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dieser vorsintflutlich starke,", "tokens": ["Die\u00b7ser", "vor\u00b7sint\u00b7flut\u00b7lich", "star\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wunderbar gefeite Mann", "tokens": ["Wun\u00b7der\u00b7bar", "ge\u00b7fei\u00b7te", "Mann"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sp\u00fcrte Delila im Marke,", "tokens": ["Sp\u00fcr\u00b7te", "De\u00b7li\u00b7la", "im", "Mar\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als er st\u00f6hnend einst begann:", "tokens": ["Als", "er", "st\u00f6h\u00b7nend", "einst", "be\u00b7gann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbm\u00fcd und matt ist meine Seele,", "tokens": ["\u00bb", "m\u00fcd", "und", "matt", "ist", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine Suada macht mich tot,", "tokens": ["Dei\u00b7ne", "Sua\u00b7da", "macht", "mich", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ich's l\u00e4nger dir verhehle,", "tokens": ["Da\u00df", "ich's", "l\u00e4n\u00b7ger", "dir", "ver\u00b7heh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Halt' ich nicht mehr aus. O Not!\u00ab", "tokens": ["Halt'", "ich", "nicht", "mehr", "aus", ".", "O", "Not", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADV", "PTKVZ", "$.", "NE", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbsimson, lieber Simson, sage\u00ab \u2013", "tokens": ["\u00bb", "sim\u00b7son", ",", "lie\u00b7ber", "Sim\u00b7son", ",", "sa\u00b7ge", "\u00ab", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "XY", "$,", "ADV", "NE", "$,", "VVFIN", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie setzt sich ihm aufs Knie \u2013", "tokens": ["Und", "sie", "setzt", "sich", "ihm", "aufs", "Knie", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "PPER", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbgib mir Antwort auf die Frage,", "tokens": ["\u00bb", "gib", "mir", "Ant\u00b7wort", "auf", "die", "Fra\u00b7ge", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Kraft, wo stecket sie?\u00ab", "tokens": ["Dei\u00b7ne", "Kraft", ",", "wo", "ste\u00b7cket", "sie", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VVFIN", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbdelila, du sollst es wissen,", "tokens": ["\u00bb", "de\u00b7li\u00b7la", ",", "du", "sollst", "es", "wis\u00b7sen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Du indessen ganz allein,", "tokens": ["Du", "in\u00b7des\u00b7sen", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schwert und Schild, das kann ich missen,", "tokens": ["Schwert", "und", "Schild", ",", "das", "kann", "ich", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "PDS", "VMFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meine sieben Locken \u2013 nein!", "tokens": ["Mei\u00b7ne", "sie\u00b7ben", "Lo\u00b7cken", "\u2013", "nein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PTKANT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Niemals sind sie mir geschoren,", "tokens": ["Nie\u00b7mals", "sind", "sie", "mir", "ge\u00b7scho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn vom Mutterleibe bin", "tokens": ["Denn", "vom", "Mut\u00b7ter\u00b7lei\u00b7be", "bin"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich der Allmacht zugeschworen,", "tokens": ["Ich", "der", "All\u00b7macht", "zu\u00b7ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So da fuhr ins Eselskinn.", "tokens": ["So", "da", "fuhr", "ins", "E\u00b7sels\u00b7kinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "So da Stricke rei\u00dft wie Faden,", "tokens": ["So", "da", "Stri\u00b7cke", "rei\u00dft", "wie", "Fa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Solche Kraft sitzt mir im Haar,", "tokens": ["Sol\u00b7che", "Kraft", "sitzt", "mir", "im", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und du kannst mir gr\u00e4\u00dflich schaden,", "tokens": ["Und", "du", "kannst", "mir", "gr\u00e4\u00df\u00b7lich", "scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "K\u00e4mst du mit dem Scherenpaar.", "tokens": ["K\u00e4mst", "du", "mit", "dem", "Scher\u00b7en\u00b7paar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Meine Kr\u00e4fte w\u00fcrden weichen,", "tokens": ["Mei\u00b7ne", "Kr\u00e4f\u00b7te", "w\u00fcr\u00b7den", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ja, ich w\u00fcrde eins, zwei, drei", "tokens": ["Ja", ",", "ich", "w\u00fcr\u00b7de", "eins", ",", "zwei", ",", "drei"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PIS", "$,", "CARD", "$,", "CARD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedem Durchschnittsmanne gleichen,", "tokens": ["Je\u00b7dem", "Durch\u00b7schnitts\u00b7man\u00b7ne", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch mit Simson w\u00e4r's vorbei.\u00ab", "tokens": ["Doch", "mit", "Sim\u00b7son", "w\u00e4r's", "vor\u00b7bei", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "NE", "VAFIN", "PTKVZ", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Also sprach er zu dem Weibe", "tokens": ["Al\u00b7so", "sprach", "er", "zu", "dem", "Wei\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Offenherzig, ungewarnt,", "tokens": ["Of\u00b7fen\u00b7her\u00b7zig", ",", "un\u00b7ge\u00b7warnt", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Das mit dem Philisterleibe", "tokens": ["Das", "mit", "dem", "Phi\u00b7lis\u00b7ter\u00b7lei\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einen Enakssohn umgarnt.", "tokens": ["Ei\u00b7nen", "En\u00b7aks\u00b7sohn", "um\u00b7garnt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und das Geld, das sie bedungen,", "tokens": ["Und", "das", "Geld", ",", "das", "sie", "be\u00b7dun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nahm sie aus der Spie\u00dfer Hand,", "tokens": ["Nahm", "sie", "aus", "der", "Spie\u00b7\u00dfer", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hielt ihn dann im Schlaf umschlungen,", "tokens": ["Hielt", "ihn", "dann", "im", "Schlaf", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er ja sich nicht entwand.", "tokens": ["Da\u00df", "er", "ja", "sich", "nicht", "ent\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und in l\u00fcsternem Begehren \u2013", "tokens": ["Und", "in", "l\u00fcs\u00b7ter\u00b7nem", "Be\u00b7geh\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Keine Tr\u00e4ne ward geweint \u2013", "tokens": ["Kei\u00b7ne", "Tr\u00e4\u00b7ne", "ward", "ge\u00b7weint", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sieben Locken lie\u00df sie scheren", "tokens": ["Sie\u00b7ben", "Lo\u00b7cken", "lie\u00df", "sie", "sche\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihrer Sippschaft \u00e4rgstem Feind.", "tokens": ["Ih\u00b7rer", "Sipp\u00b7schaft", "\u00e4rgs\u00b7tem", "Feind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Eh er aus dem Schlaf erwachte,", "tokens": ["Eh", "er", "aus", "dem", "Schlaf", "er\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "word", "