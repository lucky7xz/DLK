{"textgrid.poem.34803": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbkaum hab ich die Welt zu schaffen begonnen,", "tokens": ["\u00bb", "kaum", "hab", "ich", "die", "Welt", "zu", "schaf\u00b7fen", "be\u00b7gon\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In einer Woche war's abgetan.", "tokens": ["In", "ei\u00b7ner", "Wo\u00b7che", "wa\u00b7r's", "ab\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch hatt ich vorher tief ausgesonnen", "tokens": ["Doch", "hatt", "ich", "vor\u00b7her", "tief", "aus\u00b7ge\u00b7son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Jahrtausendlang den Sch\u00f6pfungsplan.", "tokens": ["Jahr\u00b7tau\u00b7send\u00b7lang", "den", "Sch\u00f6p\u00b7fungs\u00b7plan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Schaffen selbst ist eitel Bewegung,", "tokens": ["Das", "Schaf\u00b7fen", "selbst", "ist", "ei\u00b7tel", "Be\u00b7we\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das st\u00fcmpert sich leicht in kurzer Frist;", "tokens": ["Das", "st\u00fcm\u00b7pert", "sich", "leicht", "in", "kur\u00b7zer", "Frist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Jedoch der Plan, die \u00dcberlegung,", "tokens": ["Je\u00b7doch", "der", "Plan", ",", "die", "\u00dc\u00b7ber\u00b7le\u00b7gung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das zeigt erst, wer ein K\u00fcnstler ist.", "tokens": ["Das", "zeigt", "erst", ",", "wer", "ein", "K\u00fcnst\u00b7ler", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "PWS", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich hab allein dreihundert Jahre", "tokens": ["Ich", "hab", "al\u00b7lein", "drei\u00b7hun\u00b7dert", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tagt\u00e4glich dr\u00fcber nachgedacht,", "tokens": ["Tag\u00b7t\u00e4g\u00b7lich", "dr\u00fc\u00b7ber", "nach\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie man am besten Doctores juris", "tokens": ["Wie", "man", "am", "bes\u00b7ten", "Doc\u00b7to\u00b7res", "ju\u00b7ris"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "APPRART", "ADJA", "NN", "NE"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.4": {"text": "Und gar die kleinen Fl\u00f6he macht.\u00ab", "tokens": ["Und", "gar", "die", "klei\u00b7nen", "Fl\u00f6\u00b7he", "macht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}