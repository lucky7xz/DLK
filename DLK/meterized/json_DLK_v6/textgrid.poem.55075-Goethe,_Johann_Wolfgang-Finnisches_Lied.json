{"textgrid.poem.55075": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Finnisches Lied", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00e4m der liebe Wohlbekannte,", "tokens": ["K\u00e4m", "der", "lie\u00b7be", "Wohl\u00b7be\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "V\u00f6llig so wie er geschieden,", "tokens": ["V\u00f6l\u00b7lig", "so", "wie", "er", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "KOKOM", "PPER", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Ku\u00df erkl\u00e4ng an seinen Lippen,", "tokens": ["Ku\u00df", "er\u00b7kl\u00e4ng", "an", "sei\u00b7nen", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4tt auch Wolfsblut sie ger\u00f6tet;", "tokens": ["H\u00e4tt", "auch", "Wolfs\u00b7blut", "sie", "ge\u00b7r\u00f6\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihm den Handschlag g\u00e4b ich, w\u00e4ren", "tokens": ["Ihm", "den", "Hand\u00b7schlag", "g\u00e4b", "ich", ",", "w\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "ART", "NN", "VVFIN", "PPER", "$,", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Fingerspitzen Schlangen.", "tokens": ["Sei\u00b7ne", "Fin\u00b7ger\u00b7spit\u00b7zen", "Schlan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wind! o h\u00e4ttest du Verst\u00e4ndnis,", "tokens": ["Wind", "!", "o", "h\u00e4t\u00b7test", "du", "Ver\u00b7st\u00e4nd\u00b7nis", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "FM", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wort' um Worte tr\u00fcgst du wechselnd,", "tokens": ["Wort'", "um", "Wor\u00b7te", "tr\u00fcgst", "du", "wech\u00b7selnd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollt auch einiges verhallen,", "tokens": ["Sollt", "auch", "ei\u00b7ni\u00b7ges", "ver\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwischen zwei entfernten Liebchen.", "tokens": ["Zwi\u00b7schen", "zwei", "ent\u00b7fern\u00b7ten", "Lieb\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gern entbehrt ich gute Bissen,", "tokens": ["Gern", "ent\u00b7behrt", "ich", "gu\u00b7te", "Bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Priesters Tafelfleisch verg\u00e4\u00df ich", "tokens": ["Pries\u00b7ters", "Ta\u00b7fel\u00b7fleisch", "ver\u00b7g\u00e4\u00df", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eher, als dem Freund entsagen,", "tokens": ["E\u00b7her", ",", "als", "dem", "Freund", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den ich Sommers rasch bezwungen,", "tokens": ["Den", "ich", "Som\u00b7mers", "rasch", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Winters langer Weis bez\u00e4hmte.", "tokens": ["Win\u00b7ters", "lan\u00b7ger", "Weis", "be\u00b7z\u00e4hm\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}