{"textgrid.poem.59362": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Welt treibt fort ihr Wesen,", "tokens": ["Die", "Welt", "treibt", "fort", "ihr", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Leute kommen und gehn,", "tokens": ["Die", "Leu\u00b7te", "kom\u00b7men", "und", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Als w\u00e4rst du nie gewesen,", "tokens": ["Als", "w\u00e4rst", "du", "nie", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als w\u00e4re nichts geschehn.", "tokens": ["Als", "w\u00e4\u00b7re", "nichts", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie sehn ich mich aufs neue", "tokens": ["Wie", "sehn", "ich", "mich", "aufs", "neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinaus in Wald und Flur!", "tokens": ["Hin\u00b7aus", "in", "Wald", "und", "Flur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob ich mich gr\u00e4m, mich freue,", "tokens": ["Ob", "ich", "mich", "gr\u00e4m", ",", "mich", "freu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du bleibst mir treu, Natur.", "tokens": ["Du", "bleibst", "mir", "treu", ",", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da klagt vor tiefem Sehnen", "tokens": ["Da", "klagt", "vor", "tie\u00b7fem", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schluchzend die Nachtigall,", "tokens": ["Schluch\u00b7zend", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Es schimmern rings von Tr\u00e4nen", "tokens": ["Es", "schim\u00b7mern", "rings", "von", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Blumen \u00fcberall.", "tokens": ["Die", "Blu\u00b7men", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und \u00fcber alle Gipfel", "tokens": ["Und", "\u00fc\u00b7ber", "al\u00b7le", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Bl\u00fctent\u00e4ler zieht", "tokens": ["Und", "Bl\u00fc\u00b7ten\u00b7t\u00e4\u00b7ler", "zieht"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch stillen Waldes Wipfel", "tokens": ["Durch", "stil\u00b7len", "Wal\u00b7des", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein heimlich Klagelied.", "tokens": ["Ein", "heim\u00b7lich", "Kla\u00b7ge\u00b7lied", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da sp\u00fcr ich's recht im Herzen,", "tokens": ["Da", "sp\u00fcr", "ich's", "recht", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df du's, Herr, drau\u00dfen bist \u2013", "tokens": ["Da\u00df", "du's", ",", "Herr", ",", "drau\u00b7\u00dfen", "bist", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "NN", "$,", "ADV", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du wei\u00dft's, wie mir von Schmerzen", "tokens": ["Du", "wei\u00dft's", ",", "wie", "mir", "von", "Schmer\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Herz zerrissen ist!", "tokens": ["Mein", "Herz", "zer\u00b7ris\u00b7sen", "ist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}