{"textgrid.poem.42868": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "An Hans Siemsen", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Uns trennt wohl vieles,", "tokens": ["Uns", "trennt", "wohl", "vie\u00b7les", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Doch nicht viel,", "tokens": ["Doch", "nicht", "viel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Gewi\u00df nicht das Ziel,", "tokens": ["Ge\u00b7wi\u00df", "nicht", "das", "Ziel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ich meine die Vorstellung unseres Zieles.", "tokens": ["Ich", "mei\u00b7ne", "die", "Vor\u00b7stel\u00b7lung", "un\u00b7se\u00b7res", "Zie\u00b7les", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--++-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Du bist zart und weich", "tokens": ["Du", "bist", "zart", "und", "weich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Und ein Mann von hohem Geschmack.", "tokens": ["Und", "ein", "Mann", "von", "ho\u00b7hem", "Ge\u00b7schmack", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dieses Gedicht ist ein freundlicher Schnack.", "tokens": ["Die\u00b7ses", "Ge\u00b7dicht", "ist", "ein", "freund\u00b7li\u00b7cher", "Schnack", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Aber wir treffen uns wieder", "tokens": ["A\u00b7ber", "wir", "tref\u00b7fen", "uns", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Im Himmelreich.", "tokens": ["Im", "Him\u00b7mel\u00b7reich", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}