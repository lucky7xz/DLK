{"dta.poem.11772": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Rohr im Winde seufzt aus Sehnsucht nach dem Sch\u00f6nen,", "tokens": ["Das", "Rohr", "im", "Win\u00b7de", "seufzt", "aus", "Sehn\u00b7sucht", "nach", "dem", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df es als Fl\u00f6te m\u00f6g' am Mund des Menschen t\u00f6nen.", "tokens": ["Da\u00df", "es", "als", "Fl\u00f6\u00b7te", "m\u00f6g'", "am", "Mund", "des", "Men\u00b7schen", "t\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "VMFIN", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So seufzet die Natur in jeder Fr\u00fchlingsbl\u00fcte,", "tokens": ["So", "seuf\u00b7zet", "die", "Na\u00b7tur", "in", "je\u00b7der", "Fr\u00fch\u00b7lings\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie vom Menschen m\u00f6g' empfangen ihr Gem\u00fcte.", "tokens": ["Da\u00df", "sie", "vom", "Men\u00b7schen", "m\u00f6g'", "emp\u00b7fan\u00b7gen", "ihr", "Ge\u00b7m\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VMFIN", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die sch\u00f6nste Landschaft seufzt, alsob ihr etwas fehle,", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Land\u00b7schaft", "seufzt", ",", "al\u00b7sob", "ihr", "et\u00b7was", "feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df der beseelte Blick der Liebe sie beseele.", "tokens": ["Da\u00df", "der", "be\u00b7seel\u00b7te", "Blick", "der", "Lie\u00b7be", "sie", "be\u00b7see\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}