{"dta.poem.4179": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Abend-Vergn\u00fcgen in dem Walde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem ich nun, mit s\u00fcsser Lust, des angelegten Waldes", "tokens": ["Nach\u00b7dem", "ich", "nun", ",", "mit", "s\u00fcs\u00b7ser", "Lust", ",", "des", "an\u00b7ge\u00b7leg\u00b7ten", "Wal\u00b7des"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Pracht,", "tokens": ["Pracht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Nebst vielen Fremd- und Eingesessnen, des Tages ofter-", "tokens": ["Nebst", "vie\u00b7len", "Frem\u00b7d", "und", "Ein\u00b7ge\u00b7sess\u00b7nen", ",", "des", "Ta\u00b7ges", "of\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "TRUNC", "KON", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "---+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "mahl genossen,", "tokens": ["mahl", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Ward einst von mir, ein\u2019 Abend-Freude und ein Vergn\u00fc-", "tokens": ["Ward", "einst", "von", "mir", ",", "ein'", "A\u00b7ben\u00b7dFreu\u00b7de", "und", "ein", "Ver\u00b7gn\u00fc"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "$,", "ART", "NN", "KON", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "gen in der Nacht", "tokens": ["gen", "in", "der", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Beym Mond-Schein auch darin zu suchen, mit allen Mei-", "tokens": ["Beym", "Mon\u00b7dSchein", "auch", "da\u00b7rin", "zu", "su\u00b7chen", ",", "mit", "al\u00b7len", "Mei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "PAV", "PTKZU", "VVINF", "$,", "APPR", "PIAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "nigen, beschlossen.", "tokens": ["ni\u00b7gen", ",", "be\u00b7schlos\u00b7sen", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Vorsatz nun ward durch das Wetter, das still und", "tokens": ["Der", "Vor\u00b7satz", "nun", "ward", "durch", "das", "Wet\u00b7ter", ",", "das", "still", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "KON"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "lieblich, so begl\u00fcckt,", "tokens": ["lieb\u00b7lich", ",", "so", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df die Gesellschaft, durch die Anmuht der hellen Schat-", "tokens": ["Da\u00df", "die", "Ge\u00b7sell\u00b7schaft", ",", "durch", "die", "An\u00b7muht", "der", "hel\u00b7len", "Schat"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "APPR", "ART", "NN", "ART", "ADJA", "TRUNC"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "ten, recht erquickt", "tokens": ["ten", ",", "recht", "er\u00b7quickt"], "token_info": ["word", "punct", "word", "word"], "pos": ["FM", "$,", "ADJD", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und ungemein vergn\u00fcget ward. Wir sahn zuerst recht", "tokens": ["Und", "un\u00b7ge\u00b7mein", "ver\u00b7gn\u00fc\u00b7get", "ward", ".", "Wir", "sahn", "zu\u00b7erst", "recht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "$.", "PPER", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "wundersch\u00f6n,", "tokens": ["wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Durch das erleuchtete Geb\u00fcsche, der Sonnen herrlichs", "tokens": ["Durch", "das", "er\u00b7leuch\u00b7te\u00b7te", "Ge\u00b7b\u00fc\u00b7sche", ",", "der", "Son\u00b7nen", "herr\u00b7lichs"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "NE"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Untergehn,", "tokens": ["Un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Das, wo nicht alle, doch verschiedne zu einer s\u00fcssen An-", "tokens": ["Das", ",", "wo", "nicht", "al\u00b7le", ",", "doch", "ver\u00b7schied\u00b7ne", "zu", "ei\u00b7ner", "s\u00fcs\u00b7sen", "An"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PWAV", "PTKNEG", "PIS", "$,", "ADV", "ADJA", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "dacht f\u00fchrte,", "tokens": ["dacht", "f\u00fchr\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Indem man in der Herrlichkeit die Majest\u00e4t des Sch\u00f6p-", "tokens": ["In\u00b7dem", "man", "in", "der", "Herr\u00b7lich\u00b7keit", "die", "Ma\u00b7jes\u00b7t\u00e4t", "des", "Sch\u00f6p"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "fers sp\u00fcrte.", "tokens": ["fers", "sp\u00fcr\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Kaum sah man ihrer Strahlen Gold mit Farben, Gluht", "tokens": ["Kaum", "sah", "man", "ih\u00b7rer", "Strah\u00b7len", "Gold", "mit", "Far\u00b7ben", ",", "Gluht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "NN", "NN", "APPR", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und Glanz verschwinden,", "tokens": ["und", "Glanz", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Fing ein geschw\u00e4chtes D\u00e4mmrungs-Licht sich \u00fcberall an", "tokens": ["Fing", "ein", "ge\u00b7schw\u00e4ch\u00b7tes", "D\u00e4mm\u00b7rungs\u00b7Licht", "sich", "\u00fc\u00b7be\u00b7rall", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PRF", "ADV", "APPR"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "einzufinden.", "tokens": ["ein\u00b7zu\u00b7fin\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Den an sich k\u00fchlen, schattigten, belaubten und verwachs-", "tokens": ["Den", "an", "sich", "k\u00fch\u00b7len", ",", "schat\u00b7tig\u00b7ten", ",", "be\u00b7laub\u00b7ten", "und", "ver\u00b7wachs"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "PRF", "VVINF", "$,", "VVFIN", "$,", "ADJA", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "nen Wald", "tokens": ["nen", "Wald"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Beherrschte, wie das schnelle Heer der Schatten sich", "tokens": ["Be\u00b7herrschte", ",", "wie", "das", "schnel\u00b7le", "Heer", "der", "Schat\u00b7ten", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "ART", "NN", "PRF"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "verlohren hatte,", "tokens": ["ver\u00b7loh\u00b7ren", "hat\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Fast sonder Schw\u00e4rz und Dunkelheit, ein allgemeiner", "tokens": ["Fast", "son\u00b7der", "Schw\u00e4rz", "und", "Dun\u00b7kel\u00b7heit", ",", "ein", "all\u00b7ge\u00b7mei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "klarer Schatte,", "tokens": ["kla\u00b7rer", "Schat\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Der unvermerkt sich sanft vermehrte, und m\u00e4hlig immer", "tokens": ["Der", "un\u00b7ver\u00b7merkt", "sich", "sanft", "ver\u00b7mehr\u00b7te", ",", "und", "m\u00e4h\u00b7lig", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "PRF", "ADJD", "ADJA", "$,", "KON", "ADJD", "ADV"], "meter": "-+-+-+-+--+-++", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "starker kam,", "tokens": ["star\u00b7ker", "kam", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Inzwischen der vergeh\u2019nde Glanz die sch\u00f6nen Farben mit", "tokens": ["I\u00b7nzwi\u00b7schen", "der", "ver\u00b7geh'n\u00b7de", "Glanz", "die", "sch\u00f6\u00b7nen", "Far\u00b7ben", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "APPR"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.14": {"text": "sich nahm.", "tokens": ["sich", "nahm", "."], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Ein ungewisses d\u00e4mmrigt Licht umgab, durchdrang, be-", "tokens": ["Ein", "un\u00b7ge\u00b7wis\u00b7ses", "d\u00e4mm\u00b7rigt", "Licht", "um\u00b7gab", ",", "durch\u00b7drang", ",", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "ADJA", "VVFIN", "NN", "VVFIN", "$,", "VVFIN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "sa\u00df, bedeckte", "tokens": ["sa\u00df", ",", "be\u00b7deck\u00b7te"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Die Wipfel, Aeste, Bl\u00e4tter, Zweige und St\u00e4mm\u2019 an ei-", "tokens": ["Die", "Wip\u00b7fel", ",", "A\u00b7es\u00b7te", ",", "Bl\u00e4t\u00b7ter", ",", "Zwei\u00b7ge", "und", "St\u00e4mm'", "an", "ei"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "APPR", "TRUNC"], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "nem jeden Baum,", "tokens": ["nem", "je\u00b7den", "Baum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "An diesem angenehmen Ort, erf\u00fcllete den ganzen Raum", "tokens": ["An", "die\u00b7sem", "an\u00b7ge\u00b7neh\u00b7men", "Ort", ",", "er\u00b7f\u00fcl\u00b7le\u00b7te", "den", "gan\u00b7zen", "Raum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.20": {"text": "Mit einer sanften holden Stille und s\u00fcssem Grauen, das", "tokens": ["Mit", "ei\u00b7ner", "sanf\u00b7ten", "hol\u00b7den", "Stil\u00b7le", "und", "s\u00fcs\u00b7sem", "Grau\u00b7en", ",", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "KON", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.21": {"text": "nicht schreckte,", "tokens": ["nicht", "schreck\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Wohl aber, nebst der linden K\u00fchlung, dem Geist und", "tokens": ["Wohl", "a\u00b7ber", ",", "nebst", "der", "lin\u00b7den", "K\u00fch\u00b7lung", ",", "dem", "Geist", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "KON"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "C\u00f6rper Lust erweckte,", "tokens": ["C\u00f6r\u00b7per", "Lust", "er\u00b7weck\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.24": {"text": "Die uns zu einer innern sanften und linden Anmuht An-", "tokens": ["Die", "uns", "zu", "ei\u00b7ner", "in\u00b7nern", "sanf\u00b7ten", "und", "lin\u00b7den", "An\u00b7muht", "An"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "ADJA", "ADJA", "KON", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.25": {"text": "la\u00df gab.", "tokens": ["la\u00df", "gab", "."], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}