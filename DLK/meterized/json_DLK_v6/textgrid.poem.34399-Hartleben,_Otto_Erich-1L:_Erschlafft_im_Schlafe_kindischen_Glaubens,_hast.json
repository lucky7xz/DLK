{"textgrid.poem.34399": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erschlafft im Schlafe kindischen Glaubens, hast", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erschlafft im Schlafe kindischen Glaubens, hast", "tokens": ["Er\u00b7schlafft", "im", "Schla\u00b7fe", "kin\u00b7di\u00b7schen", "Glau\u00b7bens", ",", "hast"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPRART", "NN", "ADJA", "NN", "$,", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "du lang genug jetzt, duldendes Volk, geruht.", "tokens": ["du", "lang", "ge\u00b7nug", "jetzt", ",", "dul\u00b7den\u00b7des", "Volk", ",", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "ADV", "$,", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ermannet euch \u2013 und eurer Ketten", "tokens": ["Er\u00b7man\u00b7net", "euch", "\u2013", "und", "eu\u00b7rer", "Ket\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$(", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "rostige Reife, sie werden brechen!", "tokens": ["ros\u00b7ti\u00b7ge", "Rei\u00b7fe", ",", "sie", "wer\u00b7den", "bre\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Nicht l\u00e4nger betend winselt in leere Luft,", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "be\u00b7tend", "win\u00b7selt", "in", "lee\u00b7re", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVPP", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "auf dieser Erde wirkt und erschafft das Heil.", "tokens": ["auf", "die\u00b7ser", "Er\u00b7de", "wirkt", "und", "er\u00b7schafft", "das", "Heil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Verlacht der Pfaffen schn\u00f6de L\u00fcge,", "tokens": ["Ver\u00b7lacht", "der", "Pfaf\u00b7fen", "schn\u00f6\u00b7de", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die da vertr\u00f6stet aufs bessre Jenseits!", "tokens": ["die", "da", "ver\u00b7tr\u00f6s\u00b7tet", "aufs", "bess\u00b7re", "Jen\u00b7seits", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Fort mit dem Trugbild ewiger Seligkeit,", "tokens": ["Fort", "mit", "dem", "Trug\u00b7bild", "e\u00b7wi\u00b7ger", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "das aus dem Leben, drin es zu leben galt,", "tokens": ["das", "aus", "dem", "Le\u00b7ben", ",", "drin", "es", "zu", "le\u00b7ben", "galt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "$,", "ADV", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "euch thatenlose, freudelose,", "tokens": ["euch", "tha\u00b7ten\u00b7lo\u00b7se", ",", "freu\u00b7de\u00b7lo\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "lockt in die schweigende Nacht des Todes!", "tokens": ["lockt", "in", "die", "schwei\u00b7gen\u00b7de", "Nacht", "des", "To\u00b7des", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}