{"dta.poem.6303": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "De la Chaleur et de la.  \n Froideur.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Regel, wie wir es ja durch Erfahrung fassen,", "tokens": ["Die", "Re\u00b7gel", ",", "wie", "wir", "es", "ja", "durch", "Er\u00b7fah\u00b7rung", "fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird sich zugleich auf heiss\u2019 und kalte C\u00f6rper passen.", "tokens": ["Wird", "sich", "zu\u00b7gleich", "auf", "heiss'", "und", "kal\u00b7te", "C\u00f6r\u00b7per", "pas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "APPR", "NE", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn man sie untersucht, wird aller Zweisel weichen,", "tokens": ["Wenn", "man", "sie", "un\u00b7ter\u00b7sucht", ",", "wird", "al\u00b7ler", "Zwei\u00b7sel", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVPP", "$,", "VAFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sie sich unter sich nicht sollten v\u00f6llig gleichen.", "tokens": ["Da\u00df", "sie", "sich", "un\u00b7ter", "sich", "nicht", "soll\u00b7ten", "v\u00f6l\u00b7lig", "glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PRF", "PTKNEG", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Hitz\u2019 und K\u00e4lt\u2019 entstehn, wenn wir es \u00fcberlegen,", "tokens": ["Die", "Hitz'", "und", "K\u00e4lt'", "ent\u00b7stehn", ",", "wenn", "wir", "es", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$,", "KOUS", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gantz offenbar aus ", "tokens": ["Gantz", "of\u00b7fen\u00b7bar", "aus"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "PTKVZ"], "meter": "-+---", "measure": "dactylic.init"}}}}}