{"textgrid.poem.52367": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "17. T\u00e4uw, di will ick betahlen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.57", "sv:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbgu'n Morgen, Vadder! Segg mi doch man mal,", "tokens": ["\u00bb", "gu'n", "Mor\u00b7gen", ",", "Vad\u00b7der", "!", "Segg", "mi", "doch", "man", "mal", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "NE", "$.", "NE", "NE", "KON", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dor k\u00fcmmt de Avkat Besendahl", "tokens": ["Dor", "k\u00fcmmt", "de", "Av\u00b7kat", "Be\u00b7sen\u00b7dahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NE", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00fct morgen wedder nah mi hen:", "tokens": ["H\u00fct", "mor\u00b7gen", "wed\u00b7der", "nah", "mi", "hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADJD", "FM", "FM", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "\u203abon jour! Bon jour, mein lieber Schwenn!\u2039", "tokens": ["\u203a", "bon", "jour", "!", "Bon", "jour", ",", "mein", "lie\u00b7ber", "Schwenn", "!", "\u2039"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "NE", "$.", "NN", "NE", "$,", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Un kickt mi an so frech un stur", "tokens": ["Un", "kickt", "mi", "an", "so", "frech", "un", "stur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NE", "APPR", "ADV", "ADJD", "FM", "FM"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Un sett' sick denn taum Fr\u00fchst\u00fcck dal.", "tokens": ["Un", "sett'", "sick", "denn", "taum", "Fr\u00fch\u00b7st\u00fcck", "dal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "KON", "ADV", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ick ded't so vel bi mi all \u00e4werleggen:", "tokens": ["Ick", "ded't", "so", "vel", "bi", "mi", "all", "\u00e4\u00b7wer\u00b7leg\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Wat meint de Kirl mit sin \u203abon jour\u2039?", "tokens": ["Wat", "meint", "de", "Kirl", "mit", "sin", "\u203a", "bon", "jour", "\u2039", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wat will hei mit \u203abon jour\u2039 woll seggen?\u00ab", "tokens": ["Wat", "will", "hei", "mit", "\u203a", "bon", "jour", "\u2039", "woll", "seg\u00b7gen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$(", "VMFIN", "VVINF", "$.", "$("], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.10": {"text": "\u00bbdu dumme Deuwel!\u00ab seggt sin Nahwer Glandt,", "tokens": ["\u00bb", "du", "dum\u00b7me", "Deu\u00b7wel", "!", "\u00ab", "seggt", "sin", "Nah\u00b7wer", "Glandt", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$.", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "\u00bbbon jour, dat is di nich bekannt? \u2013", "tokens": ["\u00bb", "bon", "jour", ",", "dat", "is", "di", "nich", "be\u00b7kannt", "?", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "NE", "$,", "ART", "FM", "FM", "PTKNEG", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Dat is dat niedertr\u00e4chtigst Wurd,", "tokens": ["Dat", "is", "dat", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tigst", "Wurd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.nl", "FM.nl", "FM.nl", "FM.nl", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wat ein den annern seggen kann.", "tokens": ["Wat", "ein", "den", "an\u00b7nern", "seg\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Un du steihst dor un h\u00fcrst dat ruhig an", "tokens": ["Un", "du", "steihst", "dor", "un", "h\u00fcrst", "dat", "ru\u00b7hig", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "ADJD", "PTKVZ"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Un b\u00fctst em noch \u203aSch\u00f6n Dank\u2039 in allen G\u00e4uden", "tokens": ["Un", "b\u00fctst", "em", "noch", "\u203a", "Sch\u00f6n", "Dank", "\u2039", "in", "al\u00b7len", "G\u00e4u\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIS", "ADV", "$(", "NE", "APPR", "$(", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Un lettst di von den Kirl dat beiden?\u00ab", "tokens": ["Un", "lettst", "di", "von", "den", "Kirl", "dat", "bei\u00b7den", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "NE", "APPR", "ART", "NN", "ART", "PIAT", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "\u00bbden Kirl, den sall de Deuwel halen!\u00ab", "tokens": ["\u00bb", "den", "Kirl", ",", "den", "sall", "de", "Deu\u00b7wel", "ha\u00b7len", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "$,", "ART", "NN", "NE", "NE", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Seggt Swenn, \u00bbt\u00e4uw, di will ick betahlen!", "tokens": ["Seggt", "Swenn", ",", "\u00bb", "t\u00e4uw", ",", "di", "will", "ick", "be\u00b7tah\u00b7len", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "$(", "NE", "$,", "NE", "VMFIN", "PPER", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.19": {"text": "Du kumm mi wedder, du verfluchte Strick!", "tokens": ["Du", "kumm", "mi", "wed\u00b7der", ",", "du", "ver\u00b7fluch\u00b7te", "Strick", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NE", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Ick hau di mit de Fust in't Gnick!\u00ab", "tokens": ["Ick", "hau", "di", "mit", "de", "Fust", "in't", "Gnick", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "\u00bbne, Vadder, ne! Man jo kein Sl\u00e4geri,", "tokens": ["\u00bb", "ne", ",", "Vad\u00b7der", ",", "ne", "!", "Man", "jo", "kein", "Sl\u00e4\u00b7ge\u00b7ri", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "NE", "$,", "NE", "$.", "PIS", "NE", "PIAT", "NN", "$,"], "meter": "-+---+-+--", "measure": "dactylic.init"}, "line.22": {"text": "Un jo nich mit Avkaten, rad ick di.", "tokens": ["Un", "jo", "nich", "mit", "Av\u00b7ka\u00b7ten", ",", "rad", "ick", "di."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["NE", "NE", "PTKNEG", "APPR", "NN", "$,", "FM.la", "FM.la", "FM.la"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.23": {"text": "Ne! Wenn de Kirl mal wedder tau di k\u00fcmmt", "tokens": ["Ne", "!", "Wenn", "de", "Kirl", "mal", "wed\u00b7der", "tau", "di", "k\u00fcmmt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "KOUS", "NE", "NE", "ADV", "KON", "NE", "NE", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Un sick dat wedder rute nimmt", "tokens": ["Un", "sick", "dat", "wed\u00b7der", "ru\u00b7te", "nimmt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ART", "NN", "VVFIN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.25": {"text": "Un will di mit \u203abon jour\u2039 traktieren,", "tokens": ["Un", "will", "di", "mit", "\u203a", "bon", "jour", "\u2039", "trak\u00b7tie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VMFIN", "NE", "APPR", "NE", "NE", "NE", "$(", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Denn kumm du wedder em verquer,", "tokens": ["Denn", "kumm", "du", "wed\u00b7der", "em", "ver\u00b7quer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Un gor kein anner Wurt deihst du verlieren,", "tokens": ["Un", "gor", "kein", "an\u00b7ner", "Wurt", "deihst", "du", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PIAT", "ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Du seggst tau em blot \u203aserviteur!\u2039,", "tokens": ["Du", "seggst", "tau", "em", "blot", "\u203a", "ser\u00b7vi\u00b7teur", "!", "\u2039", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["PPER", "FM", "FM", "FM", "FM", "FM", "FM", "$.", "$(", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.29": {"text": "Un wider redst du nich en Happen,", "tokens": ["Un", "wi\u00b7der", "redst", "du", "nich", "en", "Hap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "VVFIN", "PPER", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Denn hett hei sinen richt'gen Tappen.\u00ab", "tokens": ["Denn", "hett", "hei", "si\u00b7nen", "richt'\u00b7gen", "Tap\u00b7pen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-----+-+-", "measure": "unknown.measure.di"}, "line.31": {"text": "\u00bbna, t\u00e4uw!\u00ab seggt Swenn, \u00bbdu Racker, t\u00e4uw! Ick lur", "tokens": ["\u00bb", "na", ",", "t\u00e4uw", "!", "\u00ab", "seggt", "Swenn", ",", "\u00bb", "du", "Ra\u00b7cker", ",", "t\u00e4uw", "!", "Ick", "lur"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["$(", "ITJ", "$,", "NE", "$.", "$(", "VVFIN", "NE", "$,", "$(", "PPER", "NN", "$,", "NE", "$.", "XY", "XY"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Man blot jitzt up din oll \u203abon jour\u2039;", "tokens": ["Man", "blot", "jitzt", "up", "din", "oll", "\u203a", "bon", "jour", "\u2039", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Kumm du mi blot man wedder her,", "tokens": ["Kumm", "du", "mi", "blot", "man", "wed\u00b7der", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "VVFIN", "PIS", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Du niedertr\u00e4chtige, entfahmte serviteur!\u00ab", "tokens": ["Du", "nie\u00b7der\u00b7tr\u00e4ch\u00b7ti\u00b7ge", ",", "ent\u00b7fahm\u00b7te", "ser\u00b7vi\u00b7teur", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "$,", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den annern Morgen k\u00fcmmt nu de Avkat,", "tokens": ["Den", "an\u00b7nern", "Mor\u00b7gen", "k\u00fcmmt", "nu", "de", "Av\u00b7kat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "NE", "NE", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Oll Swenn steiht \u00e4werst ok all p'rat.", "tokens": ["Oll", "Swenn", "steiht", "\u00e4\u00b7werst", "ok", "all", "p'\u00b7rat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbbon jour, bon jour, mein lieber Meister Schwenn.\u00ab", "tokens": ["\u00bb", "bon", "jour", ",", "bon", "jour", ",", "mein", "lie\u00b7ber", "Meis\u00b7ter", "Schwenn", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "NE", "$,", "ADV", "NE", "$,", "PPOSAT", "ADJA", "NN", "NE", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbbon jour, bon jour? Wat meint Hei denn?", "tokens": ["\u00bb", "bon", "jour", ",", "bon", "jour", "?", "Wat", "meint", "Hei", "denn", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NE", "$,", "ADV", "NE", "$.", "NN", "VVFIN", "NE", "KON", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hei serviteur! \u2013 Ja, kik mi an!", "tokens": ["Hei", "ser\u00b7vi\u00b7teur", "!", "\u2013", "Ja", ",", "kik", "mi", "an", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "$.", "$(", "PTKANT", "$,", "NN", "NE", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Un noch mal serviteur! \u2013 Ja, kumm man 'ran!", "tokens": ["Un", "noch", "mal", "ser\u00b7vi\u00b7teur", "!", "\u2013", "Ja", ",", "kumm", "man", "'ran", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADV", "$.", "$(", "PTKANT", "$,", "ADJD", "PIS", "NE", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Ick heww di lang naug in den Magen,", "tokens": ["Ick", "heww", "di", "lang", "naug", "in", "den", "Ma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nu kannst du gahn un mi verklagen!\u00ab", "tokens": ["Nu", "kannst", "du", "gahn", "un", "mi", "ver\u00b7kla\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "FM", "FM", "FM", "VVINF", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}