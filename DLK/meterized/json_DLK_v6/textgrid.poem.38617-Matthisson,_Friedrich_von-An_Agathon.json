{"textgrid.poem.38617": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Agathon", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Leben, welch ein seliger G\u00f6ttertraum!", "tokens": ["Dein", "Le\u00b7ben", ",", "welch", "ein", "se\u00b7li\u00b7ger", "G\u00f6t\u00b7ter\u00b7traum", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Mirthenhain, wo Psyche und Amor sich", "tokens": ["Im", "Mir\u00b7then\u00b7hain", ",", "wo", "Psy\u00b7che", "und", "A\u00b7mor", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "NN", "KON", "NE", "PRF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Umarmen, opferst du, von Hebens", "tokens": ["Um\u00b7ar\u00b7men", ",", "op\u00b7ferst", "du", ",", "von", "He\u00b7bens"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "APPR", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Blumen umduftet, den Huldg\u00f6ttinnen.", "tokens": ["Blu\u00b7men", "um\u00b7duf\u00b7tet", ",", "den", "Huld\u00b7g\u00f6t\u00b7tin\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Gleich P\u00e4stums Rosen duftet und bl\u00fcht der Kranz", "tokens": ["Gleich", "P\u00e4s\u00b7tums", "Ro\u00b7sen", "duf\u00b7tet", "und", "bl\u00fcht", "der", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "NE", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der deine Stirn beschattet; doch Hebe flieht,", "tokens": ["Der", "dei\u00b7ne", "Stirn", "be\u00b7schat\u00b7tet", ";", "doch", "He\u00b7be", "flieht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$.", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und ihre Zauberblumen sterben", "tokens": ["Und", "ih\u00b7re", "Zau\u00b7ber\u00b7blu\u00b7men", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lange vor Hesperus mildem Glanze.", "tokens": ["Lan\u00b7ge", "vor", "Hes\u00b7pe\u00b7rus", "mil\u00b7dem", "Glan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Apollons Lorbeern gr\u00fcnen wenn alles welkt!", "tokens": ["A\u00b7pol\u00b7lons", "Lor\u00b7beern", "gr\u00fc\u00b7nen", "wenn", "al\u00b7les", "welkt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Drum brich den Spr\u00f6\u00dfling, welchen die Muse dir", "tokens": ["Drum", "brich", "den", "Spr\u00f6\u00df\u00b7ling", ",", "wel\u00b7chen", "die", "Mu\u00b7se", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,", "PWAT", "ART", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erzog, die seit der Vorwelt S\u00e4ngern", "tokens": ["Er\u00b7zog", ",", "die", "seit", "der", "Vor\u00b7welt", "S\u00e4n\u00b7gern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenigen holder als dir gel\u00e4chelt.", "tokens": ["We\u00b7ni\u00b7gen", "hol\u00b7der", "als", "dir", "ge\u00b7l\u00e4\u00b7chelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "KOUS", "PPER", "VVPP", "$."], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Wie einst an Orpheus heiliger Urne, klagt,", "tokens": ["Wie", "einst", "an", "Or\u00b7pheus", "hei\u00b7li\u00b7ger", "Ur\u00b7ne", ",", "klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NE", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wann sp\u00e4t, o Freund, der Seligen Inseln dich", "tokens": ["Wann", "sp\u00e4t", ",", "o", "Freund", ",", "der", "Se\u00b7li\u00b7gen", "In\u00b7seln", "dich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "FM", "NN", "$,", "ART", "ADJA", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Empfangen, dann bei deinem Grabe", "tokens": ["Emp\u00b7fan\u00b7gen", ",", "dann", "bei", "dei\u00b7nem", "Gra\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4nger und z\u00e4rtlicher Philomele.", "tokens": ["L\u00e4n\u00b7ger", "und", "z\u00e4rt\u00b7li\u00b7cher", "Phi\u00b7lo\u00b7me\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}}}}