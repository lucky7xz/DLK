{"textgrid.poem.60065": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn die G\u00f6tter in's Geb\u00fcsch", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die G\u00f6tter in's Geb\u00fcsch", "tokens": ["Wenn", "die", "G\u00f6t\u00b7ter", "in's", "Ge\u00b7b\u00fcsch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch zu Hirten k\u00e4men,", "tokens": ["Noch", "zu", "Hir\u00b7ten", "k\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Noch vorlieb am kleinen Tisch", "tokens": ["Noch", "vor\u00b7lieb", "am", "klei\u00b7nen", "Tisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter ihnen n\u00e4hmen;", "tokens": ["Un\u00b7ter", "ih\u00b7nen", "n\u00e4h\u00b7men", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "O, sie w\u00fcrden, glaube mir!", "tokens": ["O", ",", "sie", "w\u00fcr\u00b7den", ",", "glau\u00b7be", "mir", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bald hernieder steigen,", "tokens": ["Bald", "her\u00b7nie\u00b7der", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "W\u00fcrden sich an deiner Th\u00fcr,", "tokens": ["W\u00fcr\u00b7den", "sich", "an", "dei\u00b7ner", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Liebe Chloe! zeigen.", "tokens": ["Lie\u00b7be", "Chloe", "!", "zei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NE", "$.", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Auch als Pilger, unbekannt,", "tokens": ["Auch", "als", "Pil\u00b7ger", ",", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sie dir erschienen,", "tokens": ["Wie", "sie", "dir", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "W\u00fcrdest du mit frommer Hand", "tokens": ["W\u00fcr\u00b7dest", "du", "mit", "from\u00b7mer", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Willig sie bedienen.", "tokens": ["Wil\u00b7lig", "sie", "be\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Und du f\u00fchltest innerlich", "tokens": ["Und", "du", "f\u00fchl\u00b7test", "in\u00b7ner\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Heiliges Entz\u00fccken;", "tokens": ["Hei\u00b7li\u00b7ges", "Ent\u00b7z\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Aber sie durchschauten dich", "tokens": ["A\u00b7ber", "sie", "durch\u00b7schau\u00b7ten", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit den G\u00f6tter-Blicken;", "tokens": ["Mit", "den", "G\u00f6t\u00b7ter\u00b7Bli\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Forschten in dein Herz hinein,", "tokens": ["Forschten", "in", "dein", "Herz", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Pr\u00fcften alle Triebe;", "tokens": ["Pr\u00fcf\u00b7ten", "al\u00b7le", "Trie\u00b7be", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "F\u00e4nden deine Seele rein,", "tokens": ["F\u00e4n\u00b7den", "dei\u00b7ne", "See\u00b7le", "rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00e4hen lauter Liebe;", "tokens": ["S\u00e4\u00b7hen", "lau\u00b7ter", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "G\u00f6nnten eine Bitte dir;", "tokens": ["G\u00f6nn\u00b7ten", "ei\u00b7ne", "Bit\u00b7te", "dir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich wei\u00df die Bitte:", "tokens": ["Und", "ich", "wei\u00df", "die", "Bit\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Still vereinigt wohnten wir", "tokens": ["Still", "ver\u00b7ei\u00b7nigt", "wohn\u00b7ten", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dann in armer H\u00fctte!", "tokens": ["Dann", "in", "ar\u00b7mer", "H\u00fct\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}