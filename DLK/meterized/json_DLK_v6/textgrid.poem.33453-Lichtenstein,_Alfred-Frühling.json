{"textgrid.poem.33453": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.57", "af:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein gewisser Rudolf rief:", "tokens": ["Ein", "ge\u00b7wis\u00b7ser", "Ru\u00b7dolf", "rief", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich hab' viel zu viel gegessen.", "tokens": ["Ich", "hab'", "viel", "zu", "viel", "ge\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKA", "PIS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob's bek\u00f6mmlich ist sehr fraglich.", "tokens": ["Ob's", "be\u00b7k\u00f6mm\u00b7lich", "ist", "sehr", "frag\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach so fettem Mittagessen", "tokens": ["Nach", "so", "fet\u00b7tem", "Mit\u00b7ta\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchl' ich mich recht unbehaglich.", "tokens": ["F\u00fchl'", "ich", "mich", "recht", "un\u00b7be\u00b7hag\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch ich r\u00fclpse h\u00fcbsch und rauche", "tokens": ["Doch", "ich", "r\u00fclp\u00b7se", "h\u00fcbsch", "und", "rau\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zigaretten hin und wieder.", "tokens": ["Zi\u00b7ga\u00b7ret\u00b7ten", "hin", "und", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Liegend auf dem schweren Bauche", "tokens": ["Lie\u00b7gend", "auf", "dem", "schwe\u00b7ren", "Bau\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pieps ich lauter Fr\u00fchlingslieder.", "tokens": ["Pieps", "ich", "lau\u00b7ter", "Fr\u00fch\u00b7lings\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sehnsuchtsvoll wie auf der Rampe", "tokens": ["Sehn\u00b7suchts\u00b7voll", "wie", "auf", "der", "Ram\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Quietscht die Stimme aus der Kehle.", "tokens": ["Qui\u00b7etscht", "die", "Stim\u00b7me", "aus", "der", "Keh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und wie eine alte Lampe", "tokens": ["Und", "wie", "ei\u00b7ne", "al\u00b7te", "Lam\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blakt der Wind die saure Seele.", "tokens": ["Blakt", "der", "Wind", "die", "sau\u00b7re", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}