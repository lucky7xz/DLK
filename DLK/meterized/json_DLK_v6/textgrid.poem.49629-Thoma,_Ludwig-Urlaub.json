{"textgrid.poem.49629": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Urlaub", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es knospete in allen B\u00e4umen,", "tokens": ["Es", "knos\u00b7pe\u00b7te", "in", "al\u00b7len", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und B\u00fclow ging nach Norderney.", "tokens": ["Und", "B\u00fc\u00b7low", "ging", "nach", "Nor\u00b7der\u00b7ney", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er f\u00fchlt sich, wo die Wellen sch\u00e4umen,", "tokens": ["Er", "f\u00fchlt", "sich", ",", "wo", "die", "Wel\u00b7len", "sch\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entschieden gut und sorgenfrei.", "tokens": ["Ent\u00b7schie\u00b7den", "gut", "und", "sor\u00b7gen\u00b7frei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es wird ihm die Gesundheit fester", "tokens": ["Es", "wird", "ihm", "die", "Ge\u00b7sund\u00b7heit", "fes\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im meerumsp\u00fclten Norderney.", "tokens": ["Im", "meer\u00b7um\u00b7sp\u00fcl\u00b7ten", "Nor\u00b7der\u00b7ney", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zudem gen\u00fcgt wohl ein Semester", "tokens": ["Zu\u00b7dem", "ge\u00b7n\u00fcgt", "wohl", "ein", "Se\u00b7mes\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr eine deutsche Kanzlerei.", "tokens": ["F\u00fcr", "ei\u00b7ne", "deut\u00b7sche", "Kanz\u00b7le\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Fr\u00fchling wich den Sommertagen,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "wich", "den", "Som\u00b7mer\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf allen Wiesen lag das Heu.", "tokens": ["Auf", "al\u00b7len", "Wie\u00b7sen", "lag", "das", "Heu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da lag nun B\u00fclow mit Behagen", "tokens": ["Da", "lag", "nun", "B\u00fc\u00b7low", "mit", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "NE", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und lang gestreckt in Norderney.", "tokens": ["Und", "lang", "ge\u00b7streckt", "in", "Nor\u00b7der\u00b7ney", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Man h\u00f6rt Ger\u00e4usche in der Ferne,", "tokens": ["Man", "h\u00f6rt", "Ge\u00b7r\u00e4u\u00b7sche", "in", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es gibt wo eine Hauerei.", "tokens": ["Es", "gibt", "wo", "ei\u00b7ne", "Hau\u00b7e\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt sp\u00fcrt man noch einmal so gerne,", "tokens": ["Jetzt", "sp\u00fcrt", "man", "noch", "ein\u00b7mal", "so", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie's ruhig ist in Norderney.", "tokens": ["Wie's", "ru\u00b7hig", "ist", "in", "Nor\u00b7der\u00b7ney", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Schon f\u00e4rbt der Herbst die Buchenbl\u00e4tter.", "tokens": ["Schon", "f\u00e4rbt", "der", "Herbst", "die", "Bu\u00b7chen\u00b7bl\u00e4t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ja, B\u00fclow, sagen Sie uns frei:", "tokens": ["Ja", ",", "B\u00fc\u00b7low", ",", "sa\u00b7gen", "Sie", "uns", "frei", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NE", "$,", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was tun Sie bei dem k\u00fchlen Wetter", "tokens": ["Was", "tun", "Sie", "bei", "dem", "k\u00fch\u00b7len", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn immer noch in Norderney?", "tokens": ["Denn", "im\u00b7mer", "noch", "in", "Nor\u00b7der\u00b7ney", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}