{"dta.poem.2211": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Poesie ist eine Kunst, das Wesen der Natur zu", "tokens": ["Die", "Poe\u00b7sie", "ist", "ei\u00b7ne", "Kunst", ",", "das", "We\u00b7sen", "der", "Na\u00b7tur", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "PTKZU"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "schildern,", "tokens": ["schil\u00b7dern", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Indem sie unserm Geist die Farben von K\u00f6rpern,", "tokens": ["In\u00b7dem", "sie", "un\u00b7serm", "Geist", "die", "Far\u00b7ben", "von", "K\u00f6r\u00b7pern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Formen und von Bildern,", "tokens": ["For\u00b7men", "und", "von", "Bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Den K\u00f6rpern die Lebhaftigkeit zusammt dem Feur des", "tokens": ["Den", "K\u00f6r\u00b7pern", "die", "Leb\u00b7haf\u00b7tig\u00b7keit", "zu\u00b7sammt", "dem", "Feur", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Geistes, giebt.", "tokens": ["Geis\u00b7tes", ",", "giebt", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Was Wunder, da\u00df die Menschheit stets, ja die Barba-", "tokens": ["Was", "Wun\u00b7der", ",", "da\u00df", "die", "Menschheit", "stets", ",", "ja", "die", "Ba\u00b7rba"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "NN", "$,", "KOUS", "ART", "NN", "ADV", "$,", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "ren, sie geliebt!", "tokens": ["ren", ",", "sie", "ge\u00b7liebt", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Sie weis der zwo beliebtsten K\u00fcnste, der Tonkunst und", "tokens": ["Sie", "weis", "der", "zwo", "be\u00b7liebts\u00b7ten", "K\u00fcns\u00b7te", ",", "der", "Ton\u00b7kunst", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "ART", "CARD", "ADJA", "NN", "$,", "ART", "NN", "KON"], "meter": "-+-+-+-+--++-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "der Mahlerey", "tokens": ["der", "Mah\u00b7le\u00b7rey"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Vortrefflichkeiten zu vereinen. Sie ahmt des Pinsels", "tokens": ["Vor\u00b7treff\u00b7lich\u00b7kei\u00b7ten", "zu", "ver\u00b7ei\u00b7nen", ".", "Sie", "ahmt", "des", "Pin\u00b7sels"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+--+---", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Zauberschlag", "tokens": ["Zau\u00b7ber\u00b7schlag"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "In ihren Bildern, und dabey", "tokens": ["In", "ih\u00b7ren", "Bil\u00b7dern", ",", "und", "da\u00b7bey"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In ihrem Wohllaut der Musik beliebt- und s\u00fc\u00dfen T\u00f6nen nach.", "tokens": ["In", "ih\u00b7rem", "Wohl\u00b7laut", "der", "Mu\u00b7sik", "be\u00b7lieb\u00b7t", "und", "s\u00fc\u00b7\u00dfen", "T\u00f6\u00b7nen", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "TRUNC", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+---+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.15": {"text": "Nun ist dem Menschen der Geschmack zur Mahlerey und", "tokens": ["Nun", "ist", "dem", "Men\u00b7schen", "der", "Ge\u00b7schmack", "zur", "Mah\u00b7le\u00b7rey", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "APPRART", "NN", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "s\u00fc\u00dfen Ch\u00f6ren", "tokens": ["s\u00fc\u00b7\u00dfen", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Nat\u00fcrlich, und so gut als wie die Kraft zu sehen und zu h\u00f6ren,", "tokens": ["Na\u00b7t\u00fcr\u00b7lich", ",", "und", "so", "gut", "als", "wie", "die", "Kraft", "zu", "se\u00b7hen", "und", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "ADV", "ADJD", "KOKOM", "KOKOM", "ART", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.18": {"text": "So da\u00df es fast unm\u00f6glich ist, da\u00df uns, mit Ohren und", "tokens": ["So", "da\u00df", "es", "fast", "un\u00b7m\u00f6g\u00b7lich", "ist", ",", "da\u00df", "uns", ",", "mit", "Oh\u00b7ren", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "$,", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.19": {"text": "mit Augen,", "tokens": ["mit", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Ein Klang, ein wohlgeformtes Bild, nicht sollte zu ver-", "tokens": ["Ein", "Klang", ",", "ein", "wohl\u00b7ge\u00b7form\u00b7tes", "Bild", ",", "nicht", "soll\u00b7te", "zu", "ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "PTKNEG", "VMFIN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "gn\u00fcgen taugen.", "tokens": ["gn\u00fc\u00b7gen", "tau\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.22": {"text": "Es ist demnach erlaubt zu schlie\u00dfen,", "tokens": ["Es", "ist", "dem\u00b7nach", "er\u00b7laubt", "zu", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Da\u00df unser Geist, vom s\u00fc\u00dfen Eindruck des H\u00f6rens und", "tokens": ["Da\u00df", "un\u00b7ser", "Geist", ",", "vom", "s\u00fc\u00b7\u00dfen", "Ein\u00b7druck", "des", "H\u00f6\u00b7rens", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "APPRART", "ADJA", "NN", "ART", "NN", "KON"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "des Sehns ger\u00fchrt,", "tokens": ["des", "Sehns", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Die holde Kunst der Poesie nothwendig hab erfinden m\u00fcssen,", "tokens": ["Die", "hol\u00b7de", "Kunst", "der", "Poe\u00b7sie", "noth\u00b7wen\u00b7dig", "hab", "er\u00b7fin\u00b7den", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJD", "VAFIN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.26": {"text": "Da man ein\u2019 Art von Mahlerey und von Musik in ihr", "tokens": ["Da", "man", "ein'", "Art", "von", "Mah\u00b7le\u00b7rey", "und", "von", "Mu\u00b7sik", "in", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.27": {"text": "versp\u00fcrt,", "tokens": ["ver\u00b7sp\u00fcrt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.28": {"text": "Daher entsteht f\u00fcr alle Menschen ein allgemeiner Reiz.", "tokens": ["Da\u00b7her", "ent\u00b7steht", "f\u00fcr", "al\u00b7le", "Men\u00b7schen", "ein", "all\u00b7ge\u00b7mei\u00b7ner", "Reiz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.29": {"text": "Ein jeder", "tokens": ["Ein", "je\u00b7der"], "token_info": ["word", "word"], "pos": ["ART", "PIAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.30": {"text": "Liebt Verse, Mahlerey und Lieder.", "tokens": ["Liebt", "Ver\u00b7se", ",", "Mah\u00b7le\u00b7rey", "und", "Lie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}