{"textgrid.poem.38130": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "5. Der Bischof giebt das Salz", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da er nun Abschied hat genommen,", "tokens": ["Da", "er", "nun", "Ab\u00b7schied", "hat", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Lotter wiederum gekommen,", "tokens": ["Nach", "Lot\u00b7ter", "wie\u00b7de\u00b7rum", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wollt eine Zeitlang ruhen fein", "tokens": ["Wollt", "ei\u00b7ne", "Zeit\u00b7lang", "ru\u00b7hen", "fein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bey seiner Frau und Kinderlein,", "tokens": ["Bey", "sei\u00b7ner", "Frau", "und", "Kin\u00b7der\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Bischof ihm von Halberstadt", "tokens": ["Der", "Bi\u00b7schof", "ihm", "von", "Hal\u00b7ber\u00b7stadt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Freundschaft aufgesaget hat,", "tokens": ["Die", "Freund\u00b7schaft", "auf\u00b7ge\u00b7sa\u00b7get", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er mocht wohl seyn der Narr gewesen,", "tokens": ["Er", "mocht", "wohl", "seyn", "der", "Narr", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der schlechten Rath dem F\u00fcrst gegeben.", "tokens": ["Der", "schlech\u00b7ten", "Rath", "dem", "F\u00fcrst", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Er wollt nicht ruhen, bis er br\u00e4cht", "tokens": ["Er", "wollt", "nicht", "ru\u00b7hen", ",", "bis", "er", "br\u00e4cht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Um alle G\u00fcter sein Geschlecht.", "tokens": ["Um", "al\u00b7le", "G\u00fc\u00b7ter", "sein", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Herr Thedel sprach: \u00bbIch freue mich,", "tokens": ["Herr", "The\u00b7del", "sprach", ":", "\u00bb", "Ich", "freu\u00b7e", "mich", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der Bischof hat viel mehr als ich,", "tokens": ["Der", "Bi\u00b7schof", "hat", "viel", "mehr", "als", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Das man ihm nehmen kann und rauben,", "tokens": ["Das", "man", "ihm", "neh\u00b7men", "kann", "und", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "PPER", "VVINF", "VMFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Das sag ich ihm mit gutem Glauben.\u00ab", "tokens": ["Das", "sag", "ich", "ihm", "mit", "gu\u00b7tem", "Glau\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Mit Reitern hat er sich bemannt,", "tokens": ["Mit", "Rei\u00b7tern", "hat", "er", "sich", "be\u00b7mannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Drey hundert starke M\u00e4nner fand,", "tokens": ["Drey", "hun\u00b7dert", "star\u00b7ke", "M\u00e4n\u00b7ner", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "CARD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wohl \u00fcber funfzig D\u00f6rfer und St\u00e4dt,", "tokens": ["Wohl", "\u00fc\u00b7ber", "funf\u00b7zig", "D\u00f6r\u00b7fer", "und", "St\u00e4dt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.18": {"text": "Des Junker Thedels Panner weht,", "tokens": ["Des", "Jun\u00b7ker", "The\u00b7dels", "Pan\u00b7ner", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und gingen nun den geraden Weg", "tokens": ["Und", "gin\u00b7gen", "nun", "den", "ge\u00b7ra\u00b7den", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Und nahmen alles Vieh hinweg;", "tokens": ["Und", "nah\u00b7men", "al\u00b7les", "Vieh", "hin\u00b7weg", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Der Bischof auch gefangen ward,", "tokens": ["Der", "Bi\u00b7schof", "auch", "ge\u00b7fan\u00b7gen", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und sitzt in Lotter wohl ein Jahr,", "tokens": ["Und", "sitzt", "in", "Lot\u00b7ter", "wohl", "ein", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Er wollt das Vieh gern wieder haben,", "tokens": ["Er", "wollt", "das", "Vieh", "gern", "wie\u00b7der", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Und mu\u00dft dazu das Salz bezahlen.", "tokens": ["Und", "mu\u00dft", "da\u00b7zu", "das", "Salz", "be\u00b7zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}