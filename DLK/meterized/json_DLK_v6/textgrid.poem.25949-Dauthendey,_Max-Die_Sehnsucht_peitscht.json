{"textgrid.poem.25949": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die Sehnsucht peitscht", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sehnsucht peitscht mit scharfem Dorn,", "tokens": ["Die", "Sehn\u00b7sucht", "peitscht", "mit", "schar\u00b7fem", "Dorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie reitet mich wild", "tokens": ["Sie", "rei\u00b7tet", "mich", "wild"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und gibt mir den Sporn,", "tokens": ["Und", "gibt", "mir", "den", "Sporn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und ob mein Herz streitet,", "tokens": ["Und", "ob", "mein", "Herz", "strei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Sie macht mir die H\u00e4nde zu Hufen aus Horn", "tokens": ["Sie", "macht", "mir", "die", "H\u00e4n\u00b7de", "zu", "Hu\u00b7fen", "aus", "Horn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "APPR", "NE"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und rennt mit mir durch die W\u00e4nde.", "tokens": ["Und", "rennt", "mit", "mir", "durch", "die", "W\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Sehnsucht, sie ist wie Salz im Meer,", "tokens": ["Die", "Sehn\u00b7sucht", ",", "sie", "ist", "wie", "Salz", "im", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "KOKOM", "NN", "APPRART", "NN", "$,"], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die Zunge wird mir bitter,", "tokens": ["Die", "Zun\u00b7ge", "wird", "mir", "bit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Durst klebt schwer", "tokens": ["Und", "Durst", "klebt", "schwer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "In Gaumen und Brust.", "tokens": ["In", "Gau\u00b7men", "und", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und wie der Schaum auf Wellen lebt,", "tokens": ["Und", "wie", "der", "Schaum", "auf", "Wel\u00b7len", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So mir die Sehnsucht am Munde schwebt.", "tokens": ["So", "mir", "die", "Sehn\u00b7sucht", "am", "Mun\u00b7de", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wie Wellen, die sich erdr\u00fccken m\u00fcssen,", "tokens": ["Wie", "Wel\u00b7len", ",", "die", "sich", "er\u00b7dr\u00fc\u00b7cken", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "PRF", "VVINF", "VMINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Erdr\u00fccken sich meine verlassenen Lippen", "tokens": ["Er\u00b7dr\u00fc\u00b7cken", "sich", "mei\u00b7ne", "ver\u00b7las\u00b7se\u00b7nen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "In Sehnsucht nach deinen K\u00fcssen.", "tokens": ["In", "Sehn\u00b7sucht", "nach", "dei\u00b7nen", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}