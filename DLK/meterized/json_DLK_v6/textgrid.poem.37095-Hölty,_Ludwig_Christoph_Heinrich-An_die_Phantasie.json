{"textgrid.poem.37095": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Phantasie", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ewig tr\u00e4ufle dein Kelch, Zauberin Phantasie.", "tokens": ["E\u00b7wig", "tr\u00e4uf\u00b7le", "dein", "Kelch", ",", "Zau\u00b7be\u00b7rin", "Phan\u00b7ta\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "NN", "NN", "$."], "meter": "+-+-+-+--++-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Seinen Himmel auf mich herab;", "tokens": ["Sei\u00b7nen", "Him\u00b7mel", "auf", "mich", "her\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Ewig l\u00e4chle dein Blick deinem Geweyheten,", "tokens": ["E\u00b7wig", "l\u00e4ch\u00b7le", "dein", "Blick", "dei\u00b7nem", "Ge\u00b7wey\u00b7he\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Der an deinem Altare kniet!", "tokens": ["Der", "an", "dei\u00b7nem", "Al\u00b7ta\u00b7re", "kniet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Dein unsterblicher Fu\u00df weilet, o K\u00f6nigin,", "tokens": ["Dein", "uns\u00b7terb\u00b7li\u00b7cher", "Fu\u00df", "wei\u00b7let", ",", "o", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,", "FM", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "An den Quellen des Morgenroths;", "tokens": ["An", "den", "Quel\u00b7len", "des", "Mor\u00b7gen\u00b7roths", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Du entsch\u00f6pfest dem Quell liebliches Rosenlicht,", "tokens": ["Du", "ent\u00b7sch\u00f6p\u00b7fest", "dem", "Quell", "lieb\u00b7li\u00b7ches", "Ro\u00b7sen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und bestrahlest die Erdenwelt.", "tokens": ["Und", "be\u00b7strah\u00b7lest", "die", "Er\u00b7den\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Dein allm\u00e4chtiger Wink winket den Himmel schnell", "tokens": ["Dein", "all\u00b7m\u00e4ch\u00b7ti\u00b7ger", "Wink", "win\u00b7ket", "den", "Him\u00b7mel", "schnell"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "ADJD"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Auf die trauernde Erd herab;", "tokens": ["Auf", "die", "trau\u00b7ern\u00b7de", "Erd", "her\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "Streut ein Tempe mir hin, bauet mir Lauben auf,", "tokens": ["Streut", "ein", "Tem\u00b7pe", "mir", "hin", ",", "bau\u00b7et", "mir", "Lau\u00b7ben", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Bettet Betten von Rosen mir.", "tokens": ["Bet\u00b7tet", "Bet\u00b7ten", "von", "Ro\u00b7sen", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "Du entpfl\u00fcckest dem Thal Edens, o K\u00f6nigin,", "tokens": ["Du", "ent\u00b7pfl\u00fc\u00b7ckest", "dem", "Thal", "E\u00b7dens", ",", "o", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "$,", "FM", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Aetherblumen zum Kranze dir;", "tokens": ["A\u00b7e\u00b7ther\u00b7blu\u00b7men", "zum", "Kran\u00b7ze", "dir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Und ums\u00e4uselst die Stirn deiner Erkohrenen", "tokens": ["Und", "um\u00b7s\u00e4u\u00b7selst", "die", "Stirn", "dei\u00b7ner", "Er\u00b7koh\u00b7re\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "--+--++--+--", "measure": "anapaest.di.plus"}, "line.16": {"text": "Mit dem tr\u00e4ufelnden Strahlenkranz.", "tokens": ["Mit", "dem", "tr\u00e4u\u00b7feln\u00b7den", "Strah\u00b7len\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Eine Grazie h\u00fcpft, leicht wie ein Rosenblatt,", "tokens": ["Ei\u00b7ne", "Gra\u00b7zie", "h\u00fcpft", ",", "leicht", "wie", "ein", "Ro\u00b7sen\u00b7blatt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.18": {"text": "Liebel\u00e4chelnd mir auf den Schoo\u00df;", "tokens": ["Lie\u00b7be\u00b7l\u00e4\u00b7chelnd", "mir", "auf", "den", "Schoo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.19": {"text": "Und ich senke mein Haupt an die geliebte Brust,", "tokens": ["Und", "ich", "sen\u00b7ke", "mein", "Haupt", "an", "die", "ge\u00b7lieb\u00b7te", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Schweb in Tr\u00e4umen Elysiums.", "tokens": ["Schweb", "in", "Tr\u00e4u\u00b7men", "E\u00b7ly\u00b7si\u00b7ums", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.21": {"text": "Trunken wandl' ich mit ihr, str\u00f6met das Abendroth,", "tokens": ["Trun\u00b7ken", "wandl'", "ich", "mit", "ihr", ",", "str\u00f6\u00b7met", "das", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Durch die schlummernden Blumen hin;", "tokens": ["Durch", "die", "schlum\u00b7mern\u00b7den", "Blu\u00b7men", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.23": {"text": "Durch den purpurnen Hain, durch das Geb\u00fcsch von Gold,", "tokens": ["Durch", "den", "pur\u00b7pur\u00b7nen", "Hain", ",", "durch", "das", "Ge\u00b7b\u00fcsch", "von", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Durch das schlummernde Mondenlicht;", "tokens": ["Durch", "das", "schlum\u00b7mern\u00b7de", "Mon\u00b7den\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.25": {"text": "Und aus Rosengew\u00f6lk l\u00e4chelt der Abendstern", "tokens": ["Und", "aus", "Ro\u00b7sen\u00b7ge\u00b7w\u00f6lk", "l\u00e4\u00b7chelt", "der", "A\u00b7bends\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.26": {"text": "Meiner Wallerin ins Gesicht.", "tokens": ["Mei\u00b7ner", "Wal\u00b7le\u00b7rin", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.27": {"text": "Ewig tr\u00e4ufle dein Kelch, Zauberin Phantasie,", "tokens": ["E\u00b7wig", "tr\u00e4uf\u00b7le", "dein", "Kelch", ",", "Zau\u00b7be\u00b7rin", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "NN", "NN", "$,"], "meter": "+-+-+-+--++-", "measure": "trochaic.hexa.relaxed"}, "line.28": {"text": "Seinen Himmel herab auf mich!", "tokens": ["Sei\u00b7nen", "Him\u00b7mel", "her\u00b7ab", "auf", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}