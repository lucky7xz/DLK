pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hochgebenedeyte Nacht/", "tokens": ["Hoch\u00b7ge\u00b7be\u00b7ne\u00b7dey\u00b7te", "Nacht", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sey willkommen/ sey willkommen/", "tokens": ["Sey", "will\u00b7kom\u00b7men", "/", "sey", "will\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$(", "VAFIN", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du hast uns ein M\u00e4nnlein bracht/", "tokens": ["Du", "hast", "uns", "ein", "M\u00e4nn\u00b7lein", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Aller Welt zu Nutz und Frommen/", "tokens": ["Al\u00b7ler", "Welt", "zu", "Nutz", "und", "From\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles Heer der Sternen wachet/", "tokens": ["Al\u00b7les", "Heer", "der", "Ster\u00b7nen", "wa\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freuden voll am blauen lachet.", "tokens": ["Freu\u00b7den", "voll", "am", "blau\u00b7en", "la\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Au\u00df dem Hohen Himmels Thron/", "tokens": ["Au\u00df", "dem", "Ho\u00b7hen", "Him\u00b7mels", "Thron", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat sich heint herab gelassen", "tokens": ["Hat", "sich", "heint", "her\u00b7ab", "ge\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "VVFIN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gottes einig lieber Sohn/", "tokens": ["Got\u00b7tes", "ei\u00b7nig", "lie\u00b7ber", "Sohn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer kan di\u00df Geheimnu\u00df fassen/", "tokens": ["Wer", "kan", "di\u00df", "Ge\u00b7heim\u00b7nu\u00df", "fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "NN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Seine Zuckers\u00fcsse Lippen", "tokens": ["Sei\u00b7ne", "Zu\u00b7cker\u00b7s\u00fcs\u00b7se", "Lip\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weinen in der Ochsenkrippen.", "tokens": ["Wei\u00b7nen", "in", "der", "Och\u00b7sen\u00b7krip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "We\u00df ist diese Melodey:", "tokens": ["We\u00df", "ist", "die\u00b7se", "Me\u00b7lo\u00b7dey", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was sind das vor Musicanten:", "tokens": ["Was", "sind", "das