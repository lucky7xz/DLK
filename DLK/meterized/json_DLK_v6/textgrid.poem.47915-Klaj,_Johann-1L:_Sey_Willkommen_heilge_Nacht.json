": {"text": "Alles webet/ alles bebt/", "tokens": ["Al\u00b7les", "we\u00b7bet", "/", "al\u00b7les", "bebt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "PIS", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer l\u00e4st die Predigt h\u00f6ren:", "tokens": ["Ei\u00b7ner", "l\u00e4st", "die", "Pre\u00b7digt", "h\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Heut ist Gottes Sohn gebohren/", "tokens": ["Heut", "ist", "Got\u00b7tes", "Sohn", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aller Welt zum Heil erkohren.", "tokens": ["Al\u00b7ler", "Welt", "zum", "Heil", "er\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Kurtz und gut! die Music klingt:", "tokens": ["Kurtz", "und", "gut", "!", "die", "Mu\u00b7sic", "klingt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ehre sey Gott in der H\u00f6he/", "tokens": ["Eh\u00b7re", "sey", "Gott", "in", "der", "H\u00f6\u00b7he", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "APPR", "ART", "NN", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Echo gegen \u00fcber singt:", "tokens": ["E\u00b7cho", "ge\u00b7gen", "\u00fc\u00b7ber", "singt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df der Friede hier bestehe/", "tokens": ["Da\u00df", "der", "Frie\u00b7de", "hier", "be\u00b7ste\u00b7he", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Welt zum wolgefallen/", "tokens": ["Al\u00b7le", "Welt", "zum", "wol\u00b7ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00e4sset ihm ein Loblied schallen.", "tokens": ["L\u00e4s\u00b7set", "ihm", "ein", "Lob\u00b7lied", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ihr/ Ihr Lampen/ die jhr wacht.", "tokens": ["Ihr", "/", "Ihr", "Lam\u00b7pen", "/", "die", "jhr", "wacht", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PPOSAT", "NN", "$(", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Laufft geschwinder auff und nider/", "tokens": ["Laufft", "ge\u00b7schwin\u00b7der", "auff", "und", "ni\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bringet doch die heilige Nacht", "tokens": ["Brin\u00b7get", "doch", "die", "hei\u00b7li\u00b7ge", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "\u00fcber kurtz mit freuden wider/", "tokens": ["\u00fc\u00b7ber", "kurtz", "mit", "freu\u00b7den", "wi\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPR", "NN", "PTKVZ", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn will ich sie sch\u00f6ner preisen/", "tokens": ["Denn", "will", "ich", "sie", "sch\u00f6\u00b7ner", "prei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "mit verneuten Himmelsweisen.", "tokens": ["mit", "ver\u00b7neu\u00b7ten", "Him\u00b7mels\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}