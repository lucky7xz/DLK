{"textgrid.poem.51834": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mensch, wo du ledig bist, und nichts nicht wilt gen\u00fcssen,", "tokens": ["Mensch", ",", "wo", "du", "le\u00b7dig", "bist", ",", "und", "nichts", "nicht", "wilt", "ge\u00b7n\u00fcs\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$,", "KON", "PIS", "PTKNEG", "VMFIN", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Er woll ie oder nicht: Gott mu\u00df sich in dich schl\u00fcssen.", "tokens": ["Er", "woll", "ie", "o\u00b7der", "nicht", ":", "Gott", "mu\u00df", "sich", "in", "dich", "schl\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "KON", "PTKNEG", "$.", "NN", "VMFIN", "PRF", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}