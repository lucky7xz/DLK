{"textgrid.poem.48686": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "7. Ich begehre aufgel\u00f6st u.s.w", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach schau, o Himmel doch, wie hart ich bin gebunden,", "tokens": ["Ach", "schau", ",", "o", "Him\u00b7mel", "doch", ",", "wie", "hart", "ich", "bin", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "FM", "NN", "ADV", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "von deiner Schwester hier, der ungerechten Welt,", "tokens": ["von", "dei\u00b7ner", "Schwes\u00b7ter", "hier", ",", "der", "un\u00b7ge\u00b7rech\u00b7ten", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die aber nicht bei dir als eine Schwester h\u00e4lt,", "tokens": ["die", "a\u00b7ber", "nicht", "bei", "dir", "als", "ei\u00b7ne", "Schwes\u00b7ter", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "APPR", "PPER", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "indem sie stets verirrt, was du hast wiederfunden.", "tokens": ["in\u00b7dem", "sie", "stets", "ver\u00b7irrt", ",", "was", "du", "hast", "wie\u00b7der\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$,", "PWS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie spannt die Seelen ein, die ledig f\u00fcr dir stunden,", "tokens": ["Sie", "spannt", "die", "See\u00b7len", "ein", ",", "die", "le\u00b7dig", "f\u00fcr", "dir", "stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "selbst Ursach ihres Jochs. Tritt vor das, was sie stellt,", "tokens": ["selbst", "Ur\u00b7sach", "ih\u00b7res", "Jochs", ".", "Tritt", "vor", "das", ",", "was", "sie", "stellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPOSAT", "NN", "$.", "NN", "APPR", "PDS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "bis da\u00df der schwache Geist in ihre Stricke f\u00e4llt.", "tokens": ["bis", "da\u00df", "der", "schwa\u00b7che", "Geist", "in", "ih\u00b7re", "Stri\u00b7cke", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da liegt, da zappelt er, durch sich selbst \u00fcberwunden.", "tokens": ["Da", "liegt", ",", "da", "zap\u00b7pelt", "er", ",", "durch", "sich", "selbst", "\u00fc\u00b7berw\u00b7un\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,", "APPR", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich kenn' und kan sie doch, die falsche, nicht verneiden.", "tokens": ["Ich", "kenn'", "und", "kan", "sie", "doch", ",", "die", "fal\u00b7sche", ",", "nicht", "ver\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VMFIN", "PPER", "ADV", "$,", "ART", "ADJA", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich f\u00fchle meinen Zwang und mu\u00df ihn willig leiden,", "tokens": ["Ich", "f\u00fch\u00b7le", "mei\u00b7nen", "Zwang", "und", "mu\u00df", "ihn", "wil\u00b7lig", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wo Zwang auch Willen hat. O Heiland mach mich frei!", "tokens": ["wo", "Zwang", "auch", "Wil\u00b7len", "hat", ".", "O", "Hei\u00b7land", "mach", "mich", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "NN", "VAFIN", "$.", "NE", "NE", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich bin es, der ich mich auch selbsten also binde.", "tokens": ["Ich", "bin", "es", ",", "der", "ich", "mich", "auch", "selbs\u00b7ten", "al\u00b7so", "bin\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "PPER", "PRF", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mach, da\u00df ich los von mir bei dir noch heut empfinde,", "tokens": ["Mach", ",", "da\u00df", "ich", "los", "von", "mir", "bei", "dir", "noch", "heut", "emp\u00b7fin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADJD", "APPR", "PPER", "APPR", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "was ungebunden sein f\u00fcr eine Freiheit sei!", "tokens": ["was", "un\u00b7ge\u00b7bun\u00b7den", "sein", "f\u00fcr", "ei\u00b7ne", "Frei\u00b7heit", "sei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAINF", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}