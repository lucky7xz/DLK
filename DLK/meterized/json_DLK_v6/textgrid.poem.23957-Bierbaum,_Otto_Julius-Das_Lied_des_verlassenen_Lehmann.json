{"textgrid.poem.23957": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied des verlassenen Lehmann", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab ein sch\u00f6nes M\u00e4dchen", "tokens": ["Ich", "hab", "ein", "sch\u00f6\u00b7nes", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gehabt;", "tokens": ["Ge\u00b7habt", ";"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Das hat mich mit viel Liebe", "tokens": ["Das", "hat", "mich", "mit", "viel", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gelabt.", "tokens": ["Ge\u00b7labt", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Ach Gott, wie war sie niedlich,", "tokens": ["Ach", "Gott", ",", "wie", "war", "sie", "nied\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Oh Gott, wie war sie nett!", "tokens": ["Oh", "Gott", ",", "wie", "war", "sie", "nett", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["XY", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich kaufte ihr aus Rosenholz", "tokens": ["Ich", "kauf\u00b7te", "ihr", "aus", "Ro\u00b7sen\u00b7holz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Himmelbett.", "tokens": ["Ein", "Him\u00b7mel\u00b7bett", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich kaufte ihr auch Kleider", "tokens": ["Ich", "kauf\u00b7te", "ihr", "auch", "Klei\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Schuh;", "tokens": ["Und", "Schuh", ";"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Die Unterr\u00f6ckchen machten", "tokens": ["Die", "Un\u00b7ter\u00b7r\u00f6ck\u00b7chen", "mach\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Frou-frou.", "tokens": ["Frou\u00b7frou", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Sie war, beim Himmel, sauber", "tokens": ["Sie", "war", ",", "beim", "Him\u00b7mel", ",", "sau\u00b7ber"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "$,", "APPRART", "NN", "$,", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und reizend anzusehn,", "tokens": ["Und", "rei\u00b7zend", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es konnte mit ihr jeder Prinz", "tokens": ["Es", "konn\u00b7te", "mit", "ihr", "je\u00b7der", "Prinz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu Tanze gehn.", "tokens": ["Zu", "Tan\u00b7ze", "gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Da machte mich die Liebe", "tokens": ["Da", "mach\u00b7te", "mich", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verdreht;", "tokens": ["Ver\u00b7dreht", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Ich ging mit ihr zum Pfarrer,", "tokens": ["Ich", "ging", "mit", "ihr", "zum", "Pfar\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O b\u00eate!", "tokens": ["O", "b\u00eate", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Sie hat mirs nie verziehen,", "tokens": ["Sie", "hat", "mirs", "nie", "ver\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df ich sie so verkannt:", "tokens": ["Da\u00df", "ich", "sie", "so", "ver\u00b7kannt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ist mit dem ersten besten Kerl", "tokens": ["Ist", "mit", "dem", "ers\u00b7ten", "bes\u00b7ten", "Kerl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Davon gerannt.", "tokens": ["Da\u00b7von", "ge\u00b7rannt", "."], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Das ist doch niedertr\u00e4chtig,", "tokens": ["Das", "ist", "doch", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht wahr?", "tokens": ["Nicht", "wahr", "?"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Ich raufe mir den Bart und", "tokens": ["Ich", "rau\u00b7fe", "mir", "den", "Bart", "und"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Haar.", "tokens": ["Das", "Haar", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Die R\u00f6ckchen, H\u00f6schen, Sch\u00fchchen", "tokens": ["Die", "R\u00f6ck\u00b7chen", ",", "H\u00f6\u00b7schen", ",", "Sch\u00fch\u00b7chen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und auch das Himmelbett", "tokens": ["Und", "auch", "das", "Him\u00b7mel\u00b7bett"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hat nun der miserable Schuft,", "tokens": ["Hat", "nun", "der", "mi\u00b7se\u00b7rab\u00b7le", "Schuft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Oh Schwerebrett!", "tokens": ["Oh", "Schwe\u00b7re\u00b7brett", "!"], "token_info": ["word", "word", "punct"], "pos": ["XY", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Und alles das von wegen", "tokens": ["Und", "al\u00b7les", "das", "von", "we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ART", "APPR", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Ring,", "tokens": ["Dem", "Ring", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Den sie von mir beim Pfarrer", "tokens": ["Den", "sie", "von", "mir", "beim", "Pfar\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Empfing.", "tokens": ["Emp\u00b7fing", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Oh, welch ein gro\u00dfer Esel", "tokens": ["Oh", ",", "welch", "ein", "gro\u00b7\u00dfer", "E\u00b7sel"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "War ich und Pavian!", "tokens": ["War", "ich", "und", "Pa\u00b7vi\u00b7an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Legitimit\u00e4t hat mir", "tokens": ["Die", "Le\u00b7gi\u00b7ti\u00b7mi\u00b7t\u00e4t", "hat", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das angethan.", "tokens": ["Das", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und darf ich sie denn schelten?", "tokens": ["Und", "darf", "ich", "sie", "denn", "schel\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Oh nein.", "tokens": ["Oh", "nein", "."], "token_info": ["word", "word", "punct"], "pos": ["XY", "PTKANT", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Es mu\u00dfte ganz nat\u00fcrlich", "tokens": ["Es", "mu\u00df\u00b7te", "ganz", "na\u00b7t\u00fcr\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sein.", "tokens": ["So", "sein", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VAINF", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Sie hatte f\u00fcr die Ehe", "tokens": ["Sie", "hat\u00b7te", "f\u00fcr", "die", "E\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nun einmal kein Talent;", "tokens": ["Nun", "ein\u00b7mal", "kein", "Ta\u00b7lent", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das Vari\u00e9t\u00e9 der Liebe war", "tokens": ["Das", "Va\u00b7ri\u00e9t\u00e9", "der", "Lie\u00b7be", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ihr Element.", "tokens": ["Ihr", "E\u00b7le\u00b7ment", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Mag sie zum Teufel tanzen,", "tokens": ["Mag", "sie", "zum", "Teu\u00b7fel", "tan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ade!", "tokens": ["A\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Mir thun davon die Beine", "tokens": ["Mir", "thun", "da\u00b7von", "die", "Bei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht weh.", "tokens": ["Nicht", "weh", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Ich sitze im Parkette", "tokens": ["Ich", "sit\u00b7ze", "im", "Par\u00b7ket\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Vergn\u00fcgt voll Spannung da:", "tokens": ["Ver\u00b7gn\u00fcgt", "voll", "Span\u00b7nung", "da", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie hat den f\u00fcnften Partner schon \u2013", "tokens": ["Sie", "hat", "den", "f\u00fcnf\u00b7ten", "Part\u00b7ner", "schon", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Halleluja!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}