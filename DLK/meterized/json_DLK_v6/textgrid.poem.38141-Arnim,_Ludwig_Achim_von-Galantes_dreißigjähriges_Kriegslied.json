{"textgrid.poem.38141": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Galantes drei\u00dfigj\u00e4hriges Kriegslied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amor, erheb dich edler Held!", "tokens": ["A\u00b7mor", ",", "er\u00b7heb", "dich", "ed\u00b7ler", "Held", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Begebe dich mit mir ins Feld,", "tokens": ["Be\u00b7ge\u00b7be", "dich", "mit", "mir", "ins", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frisch auf!", "tokens": ["Frisch", "auf", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Mein Liebchen ist ger\u00fcst'", "tokens": ["Mein", "Lieb\u00b7chen", "ist", "ge\u00b7r\u00fcst'"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als ob sie mit mir streiten m\u00fcst',", "tokens": ["Als", "ob", "sie", "mit", "mir", "strei\u00b7ten", "m\u00fcst'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie hat nichts Guts im Sinn.", "tokens": ["Sie", "hat", "nichts", "Guts", "im", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Jezt zieh ich wider die ins Feld,", "tokens": ["Jezt", "zieh", "ich", "wi\u00b7der", "die", "ins", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mir die Liebst ist in der Welt,", "tokens": ["Die", "mir", "die", "Liebst", "ist", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frisch auf!", "tokens": ["Frisch", "auf", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Gott wei\u00df, ich bin bereit,", "tokens": ["Gott", "wei\u00df", ",", "ich", "bin", "be\u00b7reit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit ihr zu leben ohne Streit,", "tokens": ["Mit", "ihr", "zu", "le\u00b7ben", "oh\u00b7ne", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn sie nur selber wollt'.", "tokens": ["Wenn", "sie", "nur", "sel\u00b7ber", "wollt'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was all ihr Gott verliehen hat", "tokens": ["Was", "all", "ihr", "Gott", "ver\u00b7lie\u00b7hen", "hat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIAT", "PPOSAT", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor andern Frau'n aus gro\u00dfer Gnad,", "tokens": ["Vor", "an\u00b7dern", "Frau'n", "aus", "gro\u00b7\u00dfer", "Gnad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frisch auf!", "tokens": ["Frisch", "auf", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Das setzt sie wider mich,", "tokens": ["Das", "setzt", "sie", "wi\u00b7der", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich zu vertilgen eigentlich,", "tokens": ["Mich", "zu", "ver\u00b7til\u00b7gen", "ei\u00b7gent\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der ich doch nichts verschuldt.", "tokens": ["Der", "ich", "doch", "nichts", "ver\u00b7schuldt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr Leib von Gott gar sch\u00f6n bereit", "tokens": ["Ihr", "Leib", "von", "Gott", "gar", "sch\u00f6n", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN", "ADV", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Festung ist, darum ich streit',", "tokens": ["Die", "Fes\u00b7tung", "ist", ",", "da\u00b7rum", "ich", "streit'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frisch auf!", "tokens": ["Frisch", "auf", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Ihr zarte Br\u00fcstelein", "tokens": ["Ihr", "zar\u00b7te", "Br\u00fcs\u00b7tel\u00b7ein"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zwei m\u00e4chtige Basteien sein,", "tokens": ["Zwei", "m\u00e4ch\u00b7ti\u00b7ge", "Bas\u00b7tei\u00b7en", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Worauf sie sich verl\u00e4\u00dft.", "tokens": ["Wo\u00b7rauf", "sie", "sich", "ver\u00b7l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr F\u00e4hnlein ist der Uebermuth,", "tokens": ["Ihr", "F\u00e4hn\u00b7lein", "ist", "der", "Ue\u00b7ber\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Damit sie mich verachten thut.", "tokens": ["Da\u00b7mit", "sie", "mich", "ver\u00b7ach\u00b7ten", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frisch auf!", "tokens": ["Frisch", "auf", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Ihr zarter rother Mund,", "tokens": ["Ihr", "zar\u00b7ter", "ro\u00b7ther", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ist Spie\u00df und Schwerdt, so mich verwundt,", "tokens": ["Ist", "Spie\u00df", "und", "Schwerdt", ",", "so", "mich", "ver\u00b7wundt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$,", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ja \u00f6fters bis in Tod.", "tokens": ["Ja", "\u00f6f\u00b7ters", "bis", "in", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Trabanten, Fu\u00dfknecht, Reiterei", "tokens": ["Tra\u00b7ban\u00b7ten", ",", "Fu\u00df\u00b7knecht", ",", "Rei\u00b7te\u00b7rei"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind Ungnad, Falschheit, Tirannei.", "tokens": ["Sind", "U\u00b7ngnad", ",", "Falschheit", ",", "Ti\u00b7ran\u00b7nei", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Frisch auf!", "tokens": ["Frisch", "auf", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Ihr klare Aeugelein,", "tokens": ["Ihr", "kla\u00b7re", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die sind zwei Feuerk\u00fcgelein,", "tokens": ["Die", "sind", "zwei", "Feu\u00b7er\u00b7k\u00fc\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Damit sie mich verblendt.", "tokens": ["Da\u00b7mit", "sie", "mich", "ver\u00b7blendt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So Gott mir g\u00f6nnet Gl\u00fcck und Preis", "tokens": ["So", "Gott", "mir", "g\u00f6n\u00b7net", "Gl\u00fcck", "und", "Preis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich das F\u00e4hnlein niederrei\u00df,", "tokens": ["Da\u00df", "ich", "das", "F\u00e4hn\u00b7lein", "nie\u00b7der\u00b7rei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frisch auf!", "tokens": ["Frisch", "auf", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Ich hoff' damit zu sieg'n,", "tokens": ["Ich", "hoff'", "da\u00b7mit", "zu", "sieg'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Herzlieb, du mu\u00dft doch unterlieg'n", "tokens": ["Herz\u00b7lieb", ",", "du", "mu\u00dft", "doch", "un\u00b7ter\u00b7lieg'n"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und geben mir den Preis.", "tokens": ["Und", "ge\u00b7ben", "mir", "den", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Waffen sind, womit ich streit,", "tokens": ["Die", "Waf\u00b7fen", "sind", ",", "wo\u00b7mit", "ich", "streit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kunst, Tugend, Ehr und Fr\u00f6mmigkeit,", "tokens": ["Kunst", ",", "Tu\u00b7gend", ",", "Ehr", "und", "Fr\u00f6m\u00b7mig\u00b7keit", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frisch auf!", "tokens": ["Frisch", "auf", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "So soll ihr Spies und Schwerd", "tokens": ["So", "soll", "ihr", "Spies", "und", "Schwerd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So mich vor Zeiten hat versehrt", "tokens": ["So", "mich", "vor", "Zei\u00b7ten", "hat", "ver\u00b7sehrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPR", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Meinen Schaden machen heil.", "tokens": ["Mei\u00b7nen", "Scha\u00b7den", "ma\u00b7chen", "heil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Denn nimmer hast du die Gewalt,", "tokens": ["Denn", "nim\u00b7mer", "hast", "du", "die", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sich dein List gen mir erhalt,", "tokens": ["Da\u00df", "sich", "dein", "List", "gen", "mir", "er\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frisch auf!", "tokens": ["Frisch", "auf", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Geliebt dir Fr\u00f6mmigkeit,", "tokens": ["Ge\u00b7liebt", "dir", "Fr\u00f6m\u00b7mig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kunst, Tugend, Ehr, so wird der Streit", "tokens": ["Kunst", ",", "Tu\u00b7gend", ",", "Ehr", ",", "so", "wird", "der", "Streit"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch mich gewonnen seyn.", "tokens": ["Durch", "mich", "ge\u00b7won\u00b7nen", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.10": {"line.1": {"text": "Wo aber du nach Reichthum freist,", "tokens": ["Wo", "a\u00b7ber", "du", "nach", "Reicht\u00b7hum", "freist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schau, da\u00df du nie den Kauf bereust,", "tokens": ["Schau", ",", "da\u00df", "du", "nie", "den", "Kauf", "be\u00b7reust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Frisch auf!", "tokens": ["Frisch", "auf", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "O Weh! Ein alter Mann", "tokens": ["O", "Weh", "!", "Ein", "al\u00b7ter", "Mann"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hat einen Sack voll Thaler an,", "tokens": ["Hat", "ei\u00b7nen", "Sack", "voll", "Tha\u00b7ler", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der wird dich f\u00fchren hin.", "tokens": ["Der", "wird", "dich", "f\u00fch\u00b7ren", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ein wenig denke nach, mein Schatz,", "tokens": ["Ein", "we\u00b7nig", "den\u00b7ke", "nach", ",", "mein", "Schatz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eh du k\u00f6mmst auf den Musterplatz,", "tokens": ["Eh", "du", "k\u00f6mmst", "auf", "den", "Mus\u00b7ter\u00b7platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "O Weh!", "tokens": ["O", "Weh", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Wenn du mich nun besiegst,", "tokens": ["Wenn", "du", "mich", "nun", "be\u00b7siegst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und dann bei deinem Alten liegst,", "tokens": ["Und", "dann", "bei", "dei\u00b7nem", "Al\u00b7ten", "liegst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie wird dir sein zu Muth!", "tokens": ["Wie", "wird", "dir", "sein", "zu", "Muth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Herzallerliebstes Engellein,", "tokens": ["Her\u00b7zal\u00b7ler\u00b7liebs\u00b7tes", "En\u00b7gel\u00b7lein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bedenk, was dir zu thun mag sein,", "tokens": ["Be\u00b7denk", ",", "was", "dir", "zu", "thun", "mag", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "PTKZU", "VVINF", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Weh!", "tokens": ["O", "Weh", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Wirst du einmal verf\u00fchrt,", "tokens": ["Wirst", "du", "ein\u00b7mal", "ver\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein junger Leib dir nimmer wird,", "tokens": ["Mein", "jun\u00b7ger", "Leib", "dir", "nim\u00b7mer", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du bringst mich auch in Tod.", "tokens": ["Du", "bringst", "mich", "auch", "in", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}