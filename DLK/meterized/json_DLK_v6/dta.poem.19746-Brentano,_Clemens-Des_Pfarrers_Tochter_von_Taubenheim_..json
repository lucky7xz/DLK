{"dta.poem.19746": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Des Pfarrers Tochter von Taubenheim .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da drunten auf der Wiesen               ", "tokens": ["Da", "drun\u00b7ten", "auf", "der", "Wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da ist ein kleiner Platz,", "tokens": ["Da", "ist", "ein", "klei\u00b7ner", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da th\u00e4t ein Wasser flie\u00dfen,", "tokens": ["Da", "th\u00e4t", "ein", "Was\u00b7ser", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da w\u00e4chst kein gr\u00fcnes Gras.", "tokens": ["Da", "w\u00e4chst", "kein", "gr\u00fc\u00b7nes", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da wachsen keine Rosen,", "tokens": ["Da", "wach\u00b7sen", "kei\u00b7ne", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und auch kein Rosmarein,", "tokens": ["Und", "auch", "kein", "Ros\u00b7ma\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hab ich mein Kind erstochen", "tokens": ["Hab", "ich", "mein", "Kind", "er\u00b7sto\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit einem Messerlein.", "tokens": ["Mit", "ei\u00b7nem", "Mes\u00b7ser\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Im k\u00fchlen Wasser flie\u00dfet", "tokens": ["Im", "k\u00fch\u00b7len", "Was\u00b7ser", "flie\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein rosenrothes Blut,", "tokens": ["Sein", "ro\u00b7sen\u00b7ro\u00b7thes", "Blut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das B\u00e4chlein sich ergie\u00dfet", "tokens": ["Das", "B\u00e4ch\u00b7lein", "sich", "er\u00b7gie\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl in die Meeresfluth.", "tokens": ["Wohl", "in", "die", "Mee\u00b7res\u00b7fluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vom hohen Himmel sehen", "tokens": ["Vom", "ho\u00b7hen", "Him\u00b7mel", "se\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zwei blaue Aeugelein,", "tokens": ["Zwei", "blau\u00b7e", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seh ich mein Englein stehen", "tokens": ["Seh", "ich", "mein", "En\u00b7glein", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In einem Sternelein.", "tokens": ["In", "ei\u00b7nem", "Ster\u00b7ne\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dort droben auf dem Berge", "tokens": ["Dort", "dro\u00b7ben", "auf", "dem", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da steht das hohe Rad,", "tokens": ["Da", "steht", "das", "ho\u00b7he", "Rad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Will ich mich drunter legen", "tokens": ["Will", "ich", "mich", "drun\u00b7ter", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "PAV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und trauern fr\u00fch und spat.", "tokens": ["Und", "trau\u00b7ern", "fr\u00fch", "und", "spat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Hast du mich denn verlassen", "tokens": ["Hast", "du", "mich", "denn", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der mich betrogen hat,", "tokens": ["Der", "mich", "be\u00b7tro\u00b7gen", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Will ich die Welt verlassen,", "tokens": ["Will", "ich", "die", "Welt", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bekennen meine That.", "tokens": ["Be\u00b7ken\u00b7nen", "mei\u00b7ne", "That", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Leib der wird begraben,", "tokens": ["Der", "Leib", "der", "wird", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Kopf steht auf dem Rad,", "tokens": ["Der", "Kopf", "steht", "auf", "dem", "Rad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es fressen den die Raben", "tokens": ["Es", "fres\u00b7sen", "den", "die", "Ra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der mich verf\u00fchret hat.", "tokens": ["Der", "mich", "ver\u00b7f\u00fch\u00b7ret", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}