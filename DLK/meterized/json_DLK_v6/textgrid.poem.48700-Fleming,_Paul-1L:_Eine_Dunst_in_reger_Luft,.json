{"textgrid.poem.48700": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eine Dunst in reger Luft,", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Dunst in reger Luft,", "tokens": ["Ei\u00b7ne", "Dunst", "in", "re\u00b7ger", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "ein geschwindes Wetterleuchten;", "tokens": ["ein", "ge\u00b7schwin\u00b7des", "Wet\u00b7ter\u00b7leuch\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00fcsse, so den Grund nicht feuchten;", "tokens": ["G\u00fcs\u00b7se", ",", "so", "den", "Grund", "nicht", "feuch\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ein Gescho\u00df, der bald verpufft,", "tokens": ["ein", "Ge\u00b7scho\u00df", ",", "der", "bald", "ver\u00b7pufft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hall, der durch die T\u00e4ler ruft;", "tokens": ["Hall", ",", "der", "durch", "die", "T\u00e4\u00b7ler", "ruft", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcrme, so uns Nichts sein deuchten;", "tokens": ["St\u00fcr\u00b7me", ",", "so", "uns", "Nichts", "sein", "deuch\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PPER", "PIS", "PPOSAT", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pfeile, die den Zweck erreichten;", "tokens": ["Pfei\u00b7le", ",", "die", "den", "Zweck", "er\u00b7reich\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eis in einer warmen Gruft;", "tokens": ["Eis", "in", "ei\u00b7ner", "war\u00b7men", "Gruft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Alle diese sind zwar r\u00fcchtig,", "tokens": ["Al\u00b7le", "die\u00b7se", "sind", "zwar", "r\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PDS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df sie fl\u00fcchtig sein und nichtig.", "tokens": ["da\u00df", "sie", "fl\u00fcch\u00b7tig", "sein", "und", "nich\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAINF", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch wie Nichts sie alle sein,", "tokens": ["Doch", "wie", "Nichts", "sie", "al\u00b7le", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "PPER", "PIS", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "so ist doch, o Mensch, dein Leben,", "tokens": ["so", "ist", "doch", ",", "o", "Mensch", ",", "dein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "mehr, als sie, der Flucht ergeben.", "tokens": ["mehr", ",", "als", "sie", ",", "der", "Flucht", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "$,", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nichts ist Alles, du sein Schein.", "tokens": ["Nichts", "ist", "Al\u00b7les", ",", "du", "sein", "Schein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PIS", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}