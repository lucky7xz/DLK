{"dta.poem.834": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxI.  Auf den Sontag de\u00df mit vn\u00df k\u00e4mpfen-  \n den Heylands/ oder  Reminiscere.  \n Matth. 18.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hochbetr\u00fcbtes Hertz! Ich Schaw-Platz grauser plag&#7869;/ ", "tokens": ["Ich", "hoch\u00b7be\u00b7tr\u00fcb\u00b7tes", "Hertz", "!", "Ich", "Scha\u00b7wPlatz", "grau\u00b7ser", "plag", "&#7869;", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "XML_entity", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PPER", "NN", "ADJA", "NN", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schrey f\u00fcr vnd f\u00fcr. vmbsonst! der/ auf den je vnd eh", "tokens": ["Schrey", "f\u00fcr", "vnd", "f\u00fcr", ".", "vmbsonst", "!", "der", "/", "auf", "den", "je", "vnd", "eh"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "KON", "APPR", "$.", "ADV", "$.", "ART", "$(", "APPR", "ART", "ADV", "KON", "KOUS"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein hoffen hat gebaw\u2019t/ verh\u00fcll\u2019t sich/ (ach vnd weh!)", "tokens": ["Mein", "hof\u00b7fen", "hat", "ge\u00b7ba\u00b7w't", "/", "ver\u00b7h\u00fcll't", "sich", "/", "(", "ach", "vnd", "weh", "!", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$(", "VVFIN", "PRF", "$(", "$(", "XY", "KON", "PTKVZ", "$.", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In stille grausambkeit! Was helffen meine klagen?", "tokens": ["In", "stil\u00b7le", "grau\u00b7samb\u00b7keit", "!", "Was", "helf\u00b7fen", "mei\u00b7ne", "kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "PWS", "VVFIN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich mu\u00df (wie schwer es auch) de\u00df Teufels schl\u00e4ge tragen?", "tokens": ["Ich", "mu\u00df", "(", "wie", "schwer", "es", "auch", ")", "de\u00df", "Teu\u00b7fels", "schl\u00e4\u00b7ge", "tra\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "PWAV", "ADJD", "PPER", "ADV", "$(", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je mehr ich mich mit ernst zu betten vndersteh", "tokens": ["Je", "mehr", "ich", "mich", "mit", "ernst", "zu", "bet\u00b7ten", "vn\u00b7der\u00b7steh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "PRF", "APPR", "ADJD", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Je frembder stellt sich Gott! hilff/ eh\u2019 ich gantz vergeh/", "tokens": ["Je", "fremb\u00b7der", "stellt", "sich", "Gott", "!", "hilff", "/", "eh'", "ich", "gantz", "ver\u00b7geh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PRF", "NN", "$.", "NN", "$(", "KOUS", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O der du keinen noch hast beystand abgeschlagen/", "tokens": ["O", "der", "du", "kei\u00b7nen", "noch", "hast", "beys\u00b7tand", "ab\u00b7ge\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "PPER", "PIAT", "ADV", "VAFIN", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ob zwar ich schu\u00f6der Hund nicht deiner gaben werth/", "tokens": ["Ob", "zwar", "ich", "schu\u00b7\u00f6\u00b7der", "Hund", "nicht", "dei\u00b7ner", "ga\u00b7ben", "werth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADJA", "NN", "PTKNEG", "PPOSAT", "VVFIN", "ADJD", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hast du den Hunden doch offt Kinder ", "tokens": ["Hast", "du", "den", "Hun\u00b7den", "doch", "offt", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nun wol/ ich werd auch nicht best\u00fcrtzt weg von dir gehen!", "tokens": ["Nun", "wol", "/", "ich", "werd", "auch", "nicht", "be\u00b7st\u00fcrtzt", "weg", "von", "dir", "ge\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Vielleicht wird mir bi\u00dfher ein bissen Brodts versagt", "tokens": ["Viel\u00b7leicht", "wird", "mir", "bi\u00df\u00b7her", "ein", "bis\u00b7sen", "Brodts", "ver\u00b7sagt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Weil du mich/ wenn ich recht in Tr\u00fcbsal abgejagt", "tokens": ["Weil", "du", "mich", "/", "wenn", "ich", "recht", "in", "Tr\u00fcb\u00b7sal", "ab\u00b7ge\u00b7jagt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "$(", "KOUS", "PPER", "ADV", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu deinem Ehren Tisch/ Mein Vater wilt erh\u00f6hen.", "tokens": ["Zu", "dei\u00b7nem", "Eh\u00b7ren", "Tisch", "/", "Mein", "Va\u00b7ter", "wilt", "er\u00b7h\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$(", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}