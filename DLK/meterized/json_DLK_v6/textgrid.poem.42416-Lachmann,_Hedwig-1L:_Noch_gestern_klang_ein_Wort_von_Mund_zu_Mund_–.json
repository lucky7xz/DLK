{"textgrid.poem.42416": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch gestern klang ein Wort von Mund zu Mund \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch gestern klang ein Wort von Mund zu Mund \u2013", "tokens": ["Noch", "ge\u00b7stern", "klang", "ein", "Wort", "von", "Mund", "zu", "Mund", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "APPR", "NN", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Menschheit \u2013 beseeltes B\u00fcndnis aller Zonen!", "tokens": ["Menschheit", "\u2013", "be\u00b7seel\u00b7tes", "B\u00fcnd\u00b7nis", "al\u00b7ler", "Zo\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJA", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Im Fluge kreisend um das Erdenrund,", "tokens": ["Im", "Flu\u00b7ge", "krei\u00b7send", "um", "das", "Er\u00b7den\u00b7rund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Umfing die hingegebne Brust Millionen.", "tokens": ["Um\u00b7fing", "die", "hin\u00b7ge\u00b7geb\u00b7ne", "Brust", "Mil\u00b7lion\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Menschheit! Du Urmacht gleich der Ewigkeit,", "tokens": ["Menschheit", "!", "Du", "Ur\u00b7macht", "gleich", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Von der von jeher Tausende entbrannten,", "tokens": ["Von", "der", "von", "je\u00b7her", "Tau\u00b7sen\u00b7de", "ent\u00b7brann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu frommem Dienst und Opfertod bereit,", "tokens": ["Zu", "from\u00b7mem", "Dienst", "und", "Op\u00b7fer\u00b7tod", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr die verbluteten die Gottgesandten \u2013", "tokens": ["F\u00fcr", "die", "ver\u00b7blu\u00b7te\u00b7ten", "die", "Gott\u00b7ge\u00b7sand\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zernagte dir ein D\u00e4mon das Gebein,", "tokens": ["Zer\u00b7nag\u00b7te", "dir", "ein", "D\u00e4\u00b7mon", "das", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bis aufgerissen deine Weichen klafften,", "tokens": ["Bis", "auf\u00b7ge\u00b7ris\u00b7sen", "dei\u00b7ne", "Wei\u00b7chen", "klaff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Frass in dein Lebensmark ein Frost sich ein,", "tokens": ["Frass", "in", "dein", "Le\u00b7bens\u00b7mark", "ein", "Frost", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Dass du zerf\u00e4llst in wirre V\u00f6lkerschaften?", "tokens": ["Dass", "du", "zer\u00b7f\u00e4llst", "in", "wir\u00b7re", "V\u00f6l\u00b7ker\u00b7schaf\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Oder verfluchtest du den eignen Schoss", "tokens": ["O\u00b7der", "ver\u00b7fluch\u00b7test", "du", "den", "eig\u00b7nen", "Schoss"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und l\u00e4ssest deine Brut elend verk\u00fcmmern,", "tokens": ["Und", "l\u00e4s\u00b7sest", "dei\u00b7ne", "Brut", "e\u00b7lend", "ver\u00b7k\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gibst ihr mit eigner Hand den Todesstoss,", "tokens": ["Gibst", "ihr", "mit", "eig\u00b7ner", "Hand", "den", "To\u00b7dess\u00b7toss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dass sie verende unter Schutt und Tr\u00fcmmern?", "tokens": ["Dass", "sie", "ver\u00b7en\u00b7de", "un\u00b7ter", "Schutt", "und", "Tr\u00fcm\u00b7mern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Aus grauer Vorzeit schwelt es in den Tag", "tokens": ["Aus", "grau\u00b7er", "Vor\u00b7zeit", "schwelt", "es", "in", "den", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Moderdunst und blutgef\u00e4rbten Nebeln;", "tokens": ["Von", "Mo\u00b7der\u00b7dunst", "und", "blut\u00b7ge\u00b7f\u00e4rb\u00b7ten", "Ne\u00b7beln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Bruderst\u00e4mme holen aus zum Schlag,", "tokens": ["Die", "Bru\u00b7der\u00b7st\u00e4m\u00b7me", "ho\u00b7len", "aus", "zum", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Einander zu vernichten und zu knebeln.", "tokens": ["Ein\u00b7an\u00b7der", "zu", "ver\u00b7nich\u00b7ten", "und", "zu", "kne\u00b7beln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Welt ein\u00e4schernd, wie wenn Berge spein,", "tokens": ["Die", "Welt", "ein\u00b7\u00e4sc\u00b7hernd", ",", "wie", "wenn", "Ber\u00b7ge", "spein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "KOKOM", "KOUS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wogt Untergang in allen Himmelsstrichen,", "tokens": ["Wogt", "Un\u00b7ter\u00b7gang", "in", "al\u00b7len", "Him\u00b7mels\u00b7stri\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wie versteinert starrst du \u2013 Menschheit \u2013 drein,", "tokens": ["Und", "wie", "ver\u00b7stei\u00b7nert", "starrst", "du", "\u2013", "Menschheit", "\u2013", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "VVPP", "VVFIN", "PPER", "$(", "NN", "$(", "PTKVZ", "$,"], "meter": "-+-+-+-++", "measure": "unknown.measure.penta"}, "line.8": {"text": "Gleich einer Larve, draus der Geist entwichen.", "tokens": ["Gleich", "ei\u00b7ner", "Lar\u00b7ve", ",", "draus", "der", "Geist", "ent\u00b7wi\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}