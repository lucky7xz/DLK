{"textgrid.poem.62398": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "2. Das Fenster", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Fenster geht nach Morgen,", "tokens": ["Mein", "Fens\u00b7ter", "geht", "nach", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach Morgen geht mein Sinn;", "tokens": ["Nach", "Mor\u00b7gen", "geht", "mein", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ziehen meine Sorgen", "tokens": ["Da", "zie\u00b7hen", "mei\u00b7ne", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und meine Sehnsucht hin.", "tokens": ["Und", "mei\u00b7ne", "Sehn\u00b7sucht", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr Mitternachtsgesichte,", "tokens": ["Ihr", "Mit\u00b7ter\u00b7nachts\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun weichet weit zur\u00fcck;", "tokens": ["Nun", "wei\u00b7chet", "weit", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich gr\u00fc\u00dft vom reinen Lichte", "tokens": ["Mich", "gr\u00fc\u00dft", "vom", "rei\u00b7nen", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der erste fr\u00fche Blick.", "tokens": ["Der", "ers\u00b7te", "fr\u00fc\u00b7he", "Blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Luft um Brust und Locken", "tokens": ["Die", "Luft", "um", "Brust", "und", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir spielet frisch und mild,", "tokens": ["Mir", "spie\u00b7let", "frisch", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohin denn willst du locken,", "tokens": ["Wo\u00b7hin", "denn", "willst", "du", "lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O Lust, so gotterf\u00fcllt?", "tokens": ["O", "Lust", ",", "so", "got\u00b7ter\u00b7f\u00fcllt", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die fernen Kl\u00e4nge dringen", "tokens": ["Die", "fer\u00b7nen", "Kl\u00e4n\u00b7ge", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So r\u00fchrend in mein Ohr,", "tokens": ["So", "r\u00fch\u00b7rend", "in", "mein", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hinauf m\u00f6cht' ich mich schwingen", "tokens": ["Hin\u00b7auf", "m\u00f6cht'", "ich", "mich", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Aufgang hoch empor.", "tokens": ["Zum", "Auf\u00b7gang", "hoch", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das goldne Thor steht offen,", "tokens": ["Das", "gold\u00b7ne", "Thor", "steht", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die liebe Stimme spricht,", "tokens": ["Die", "lie\u00b7be", "Stim\u00b7me", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da weilt mein s\u00fc\u00dfes Hoffen,", "tokens": ["Da", "weilt", "mein", "s\u00fc\u00b7\u00dfes", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da wohnt das ew'ge Licht.", "tokens": ["Da", "wohnt", "das", "ew'\u00b7ge", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}