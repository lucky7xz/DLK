{"textgrid.poem.43702": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wie gut bin ich dir!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Schmetterling, sprich,", "tokens": ["O", "Schmet\u00b7ter\u00b7ling", ",", "sprich", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "$,"], "meter": "-+---", "measure": "dactylic.init"}, "line.2": {"text": "Was fliehest du mich?", "tokens": ["Was", "flie\u00b7hest", "du", "mich", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Warum doch so eilig,", "tokens": ["Wa\u00b7rum", "doch", "so", "ei\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Jetzt fern und dann nah?", "tokens": ["Jetzt", "fern", "und", "dann", "nah", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Jetzt fern und dann nah,", "tokens": ["Jetzt", "fern", "und", "dann", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Jetzt hier und dann da \u2013", "tokens": ["Jetzt", "hier", "und", "dann", "da", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "ADV", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ich will dich nicht haschen,", "tokens": ["Ich", "will", "dich", "nicht", "ha\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ich thu' dir kein Leid.", "tokens": ["Ich", "thu'", "dir", "kein", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Ich thu' dir kein Leid:", "tokens": ["Ich", "thu'", "dir", "kein", "Leid", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "O bleib allezeit!", "tokens": ["O", "bleib", "al\u00b7le\u00b7zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und w\u00e4r' ich ein Bl\u00fcmchen,", "tokens": ["Und", "w\u00e4r'", "ich", "ein", "Bl\u00fcm\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So spr\u00e4ch' ich zu dir,", "tokens": ["So", "spr\u00e4ch'", "ich", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "So spr\u00e4ch' ich zu dir:", "tokens": ["So", "spr\u00e4ch'", "ich", "zu", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Komm, komm doch zu mir!", "tokens": ["Komm", ",", "komm", "doch", "zu", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ich schenk' dir mein Herzchen,", "tokens": ["Ich", "schenk'", "dir", "mein", "Herz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wie gut bin ich dir!", "tokens": ["Wie", "gut", "bin", "ich", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}