{"dta.poem.6128": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Schwere und der Leichte,  \n von der Fluht und Ebbe  \n des Meeres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und wenn die Erde, die sich drehet,", "tokens": ["Und", "wenn", "die", "Er\u00b7de", ",", "die", "sich", "dre\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach einen halben Tag, aufs neu,", "tokens": ["Nach", "ei\u00b7nen", "hal\u00b7ben", "Tag", ",", "aufs", "neu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPRART", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An selben Ort hinkommt, woselbst der Mond dann stehet;", "tokens": ["An", "sel\u00b7ben", "Ort", "hin\u00b7kommt", ",", "wo\u00b7selbst", "der", "Mond", "dann", "ste\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird sie durch die Lufft aufs neue weggedruckt:", "tokens": ["So", "wird", "sie", "durch", "die", "Lufft", "aufs", "neu\u00b7e", "weg\u00b7ge\u00b7druckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wodurch die Fluht den Strand denn abermahl verschluckt.", "tokens": ["Wo\u00b7durch", "die", "Fluht", "den", "Strand", "denn", "a\u00b7ber\u00b7mahl", "ver\u00b7schluckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}