{"textgrid.poem.53562": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Ein untergehendes Land", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was k\u00fcndet dir von ihrem Baum Frau Holle?", "tokens": ["Was", "k\u00fcn\u00b7det", "dir", "von", "ih\u00b7rem", "Baum", "Frau", "Hol\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das reinste Gl\u00fcck h\u00e4ngt an der Heimatscholle.", "tokens": ["Das", "reins\u00b7te", "Gl\u00fcck", "h\u00e4ngt", "an", "der", "Hei\u00b7mat\u00b7schol\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aus diesem Baume sprechen deine Ahnen,", "tokens": ["Aus", "die\u00b7sem", "Bau\u00b7me", "spre\u00b7chen", "dei\u00b7ne", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie wollen dich zum Bleiben hier gemahnen.", "tokens": ["Sie", "wol\u00b7len", "dich", "zum", "Blei\u00b7ben", "hier", "ge\u00b7mah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Vaterhaus, von Holder \u00fcbersponnen,", "tokens": ["Das", "Va\u00b7ter\u00b7haus", ",", "von", "Hol\u00b7der", "\u00fc\u00b7bers\u00b7pon\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird bergen dir den reichsten Liebesbronnen.", "tokens": ["Wird", "ber\u00b7gen", "dir", "den", "reichs\u00b7ten", "Lie\u00b7bes\u00b7bron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dies niedre Dach, verh\u00e4ngt von Bl\u00fctendolden,", "tokens": ["Dies", "nied\u00b7re", "Dach", ",", "ver\u00b7h\u00e4ngt", "von", "Bl\u00fc\u00b7ten\u00b7dol\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PAV", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ger\u00e4t dir wohl zu einer Halle golden.", "tokens": ["Ge\u00b7r\u00e4t", "dir", "wohl", "zu", "ei\u00b7ner", "Hal\u00b7le", "gol\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Denn nicht die Arbeit birgt sich drin von heute,", "tokens": ["Denn", "nicht", "die", "Ar\u00b7beit", "birgt", "sich", "drin", "von", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch des Vergangenen ferne Siegesbeute.", "tokens": ["Auch", "des", "Ver\u00b7gan\u00b7ge\u00b7nen", "fer\u00b7ne", "Sie\u00b7ges\u00b7beu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Es haust ein Ahnherr drin, ein grauer Alter,", "tokens": ["Es", "haust", "ein", "Ahn\u00b7herr", "drin", ",", "ein", "grau\u00b7er", "Al\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es wohnen Geister drin als Hausverwalter.", "tokens": ["Es", "woh\u00b7nen", "Geis\u00b7ter", "drin", "als", "Haus\u00b7ver\u00b7wal\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Was das Geschlecht zusammen sich gewoben,", "tokens": ["Was", "das", "Ge\u00b7schlecht", "zu\u00b7sam\u00b7men", "sich", "ge\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir, ihrem Enkel, ist es aufgehoben.", "tokens": ["Dir", ",", "ih\u00b7rem", "En\u00b7kel", ",", "ist", "es", "auf\u00b7ge\u00b7ho\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Das war einmal, Blonde, und \u00fcber all das hinweg geht eine neue Zeit.", "tokens": ["Das", "war", "ein\u00b7mal", ",", "Blon\u00b7de", ",", "und", "\u00fc\u00b7ber", "all", "das", "hin\u00b7weg", "geht", "ei\u00b7ne", "neu\u00b7e", "Zeit", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,", "NN", "$,", "KON", "APPR", "PIAT", "PDS", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+--+-+-+-+", "measure": "amphibrach.tri.plus"}}}}}