{"textgrid.poem.39818": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "2. Zu Schiffers Braut von Messina", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geduldet wird der S\u00e4nger m\u00fc\u00dfig Volk noch stets,", "tokens": ["Ge\u00b7dul\u00b7det", "wird", "der", "S\u00e4n\u00b7ger", "m\u00fc\u00b7\u00dfig", "Volk", "noch", "stets", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ADJD", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und noch nicht ganz in dieser sorgenvollen Welt", "tokens": ["Und", "noch", "nicht", "ganz", "in", "die\u00b7ser", "sor\u00b7gen\u00b7vol\u00b7len", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verp\u00f6nt und ausgesto\u00dfen ist Sorglosigkeit.", "tokens": ["Ver\u00b7p\u00f6nt", "und", "aus\u00b7ge\u00b7sto\u00b7\u00dfen", "ist", "Sorg\u00b7lo\u00b7sig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer schwer am Reiseb\u00fcndel durch den gr\u00fcnen Wald", "tokens": ["Wer", "schwer", "am", "Rei\u00b7se\u00b7b\u00fcn\u00b7del", "durch", "den", "gr\u00fc\u00b7nen", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "APPRART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Im Schwei\u00dfe tr\u00e4gt, wer seiner G\u00fcter volle Fracht", "tokens": ["Im", "Schwei\u00b7\u00dfe", "tr\u00e4gt", ",", "wer", "sei\u00b7ner", "G\u00fc\u00b7ter", "vol\u00b7le", "Fracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PWS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zw\u00f6lfsp\u00e4nnig in der B\u00e4ume Schatten fortbewegt,", "tokens": ["Zw\u00f6lf\u00b7sp\u00e4n\u00b7nig", "in", "der", "B\u00e4u\u00b7me", "Schat\u00b7ten", "fort\u00b7be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer mit dem Wagen fliegend kaum die Zweige streift,", "tokens": ["Wer", "mit", "dem", "Wa\u00b7gen", "flie\u00b7gend", "kaum", "die", "Zwei\u00b7ge", "streift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVPP", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der St\u00e4dte Strudel seine Sinne zugekehrt: \u2013", "tokens": ["Der", "St\u00e4d\u00b7te", "Stru\u00b7del", "sei\u00b7ne", "Sin\u00b7ne", "zu\u00b7ge\u00b7kehrt", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie alle lauschen, wenn aus luft'gem Bl\u00e4tterdach", "tokens": ["Sie", "al\u00b7le", "lau\u00b7schen", ",", "wenn", "aus", "luft'\u00b7gem", "Bl\u00e4t\u00b7ter\u00b7dach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "VVINF", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Amsel schl\u00e4gt, wenn schmetternd sich die Lerche hebt.", "tokens": ["Die", "Am\u00b7sel", "schl\u00e4gt", ",", "wenn", "schmet\u00b7ternd", "sich", "die", "Ler\u00b7che", "hebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "ADJD", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So wird der Dichter buntes Lied wohl auch behorcht,", "tokens": ["So", "wird", "der", "Dich\u00b7ter", "bun\u00b7tes", "Lied", "wohl", "auch", "be\u00b7horcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und was man selbst sich nicht mehr Zeit zu f\u00fchlen nimmt,", "tokens": ["Und", "was", "man", "selbst", "sich", "nicht", "mehr", "Zeit", "zu", "f\u00fch\u00b7len", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "ADV", "PRF", "PTKNEG", "PIAT", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die erdverge\u00dfne Stimme der Gem\u00fctlichkeit,", "tokens": ["Die", "erd\u00b7ver\u00b7ge\u00df\u00b7ne", "Stim\u00b7me", "der", "Ge\u00b7m\u00fct\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Man h\u00f6rt sie jezuweilen gern aus Andrer Mund.", "tokens": ["Man", "h\u00f6rt", "sie", "je\u00b7zu\u00b7wei\u00b7len", "gern", "aus", "A\u00b7ndrer", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Begn\u00fcge nur der S\u00e4nger, wie der Vogel, sich", "tokens": ["Be\u00b7gn\u00fc\u00b7ge", "nur", "der", "S\u00e4n\u00b7ger", ",", "wie", "der", "Vo\u00b7gel", ",", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "ADV", "ART", "NN", "$,", "PWAV", "ART", "NE", "$,", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit diesem fl\u00fccht'gen Beifall f\u00fcr sein fl\u00fcchtig Lied,", "tokens": ["Mit", "die\u00b7sem", "fl\u00fccht'\u00b7gen", "Bei\u00b7fall", "f\u00fcr", "sein", "fl\u00fcch\u00b7tig", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verlange nicht, da\u00df \u00fcber seinem Sang und Klang", "tokens": ["Ver\u00b7lan\u00b7ge", "nicht", ",", "da\u00df", "\u00fc\u00b7ber", "sei\u00b7nem", "Sang", "und", "Klang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr Tagewerk vergesse die gesch\u00e4ft'ge Zeit,", "tokens": ["Ihr", "Ta\u00b7ge\u00b7werk", "ver\u00b7ges\u00b7se", "die", "ge\u00b7sch\u00e4ft'\u00b7ge", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und schelte nicht den kurzen Dank Undankbarkeit.", "tokens": ["Und", "schel\u00b7te", "nicht", "den", "kur\u00b7zen", "Dank", "Un\u00b7dank\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Undankbar ist die Welt nicht, wenn es Gro\u00dfes gilt;", "tokens": ["Un\u00b7dank\u00b7bar", "ist", "die", "Welt", "nicht", ",", "wenn", "es", "Gro\u00b7\u00dfes", "gilt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "PTKNEG", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.8": {"text": "Zwar kommt der Dank f\u00fcr Herrliches wohl sp\u00e4t genug", "tokens": ["Zwar", "kommt", "der", "Dank", "f\u00fcr", "Herr\u00b7li\u00b7ches", "wohl", "sp\u00e4t", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NE", "ADV", "ADJD", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und \u00fcber'm Grabe bl\u00fchet er den Besten oft.", "tokens": ["Und", "\u00fc\u00b7ber'm", "Gra\u00b7be", "bl\u00fc\u00b7het", "er", "den", "Bes\u00b7ten", "oft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nur gro\u00dfe ", "tokens": ["Nur", "gro\u00b7\u00dfe"], "token_info": ["word", "word"], "pos": ["ADV", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Ihr m\u00e4chtig Wort durchsch\u00fcttert schnell die Gegenwart", "tokens": ["Ihr", "m\u00e4ch\u00b7tig", "Wort", "durch\u00b7sch\u00fct\u00b7tert", "schnell", "die", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "NN", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und t\u00f6nt in alle Zukunft unverhallt hinaus.", "tokens": ["Und", "t\u00f6nt", "in", "al\u00b7le", "Zu\u00b7kunft", "un\u00b7ver\u00b7hallt", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie bald war in dem Hohen, dessen Wiege stolz", "tokens": ["Wie", "bald", "war", "in", "dem", "Ho\u00b7hen", ",", "des\u00b7sen", "Wie\u00b7ge", "stolz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "APPR", "ART", "ADJA", "$,", "PRELAT", "NN", "ADJD"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.14": {"text": "Das Vaterland dem Fremden zeigt, die Wunderkraft", "tokens": ["Das", "Va\u00b7ter\u00b7land", "dem", "Frem\u00b7den", "zeigt", ",", "die", "Wun\u00b7der\u00b7kraft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Erkannt, wie bald vernommen sein gewaltig Lied,", "tokens": ["Er\u00b7kannt", ",", "wie", "bald", "ver\u00b7nom\u00b7men", "sein", "ge\u00b7wal\u00b7tig", "Lied", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADV", "VVPP", "VAINF", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wie bald verklungen jedes schw\u00e4ch're neben ihm!", "tokens": ["Wie", "bald", "ver\u00b7klun\u00b7gen", "je\u00b7des", "schw\u00e4ch'\u00b7re", "ne\u00b7ben", "ihm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PIAT", "ADJA", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Nicht nur dem ", "tokens": ["Nicht", "nur", "dem"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Nein, vor dem ", "tokens": ["Nein", ",", "vor", "dem"], "token_info": ["word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und an dem G\u00f6tterboten sah man scheu empor,", "tokens": ["Und", "an", "dem", "G\u00f6t\u00b7ter\u00b7bo\u00b7ten", "sah", "man", "scheu", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PIS", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Niegeahntes, Unenth\u00fclltes k\u00fcndete.", "tokens": ["Der", "Nie\u00b7geahn\u00b7tes", ",", "Un\u00b7ent\u00b7h\u00fcll\u00b7tes", "k\u00fcn\u00b7de\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Er kam emporgestiegen aus dem dunkeln Schacht", "tokens": ["Er", "kam", "em\u00b7por\u00b7ge\u00b7stie\u00b7gen", "aus", "dem", "dun\u00b7keln", "Schacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des stillen Abgrunds, welcher Menschenseele hei\u00dft,", "tokens": ["Des", "stil\u00b7len", "Ab\u00b7grunds", ",", "wel\u00b7cher", "Men\u00b7schen\u00b7see\u00b7le", "hei\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durchwandert hatt' er die verborgnen Tiefen all,", "tokens": ["Durch\u00b7wan\u00b7dert", "hatt'", "er", "die", "ver\u00b7borg\u00b7nen", "Tie\u00b7fen", "all", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "ADJA", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Leidenschaften unbekannte M\u00fctter dort,", "tokens": ["Der", "Lei\u00b7den\u00b7schaf\u00b7ten", "un\u00b7be\u00b7kann\u00b7te", "M\u00fct\u00b7ter", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Urgef\u00fchle, durchgeforscht, die schlummernden;", "tokens": ["Die", "Ur\u00b7ge\u00b7f\u00fch\u00b7le", ",", "durch\u00b7ge\u00b7forscht", ",", "die", "schlum\u00b7mern\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.10": {"text": "Und was vertragen h\u00e4tte kein gemeiner Blick,", "tokens": ["Und", "was", "ver\u00b7tra\u00b7gen", "h\u00e4t\u00b7te", "kein", "ge\u00b7mei\u00b7ner", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ward ihm in seiner Dichterfackel Schein verkl\u00e4rt,", "tokens": ["Ward", "ihm", "in", "sei\u00b7ner", "Dich\u00b7ter\u00b7fa\u00b7ckel", "Schein", "ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und das Verkl\u00e4rte f\u00fchrt' er in den Tempel ein,", "tokens": ["Und", "das", "Ver\u00b7kl\u00e4r\u00b7te", "f\u00fchrt'", "er", "in", "den", "Tem\u00b7pel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "In dem die Musen solchen Priesters harreten.", "tokens": ["In", "dem", "die", "Mu\u00b7sen", "sol\u00b7chen", "Pries\u00b7ters", "har\u00b7re\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.14": {"text": "Da rang des Erdenlebens innerster Gehalt", "tokens": ["Da", "rang", "des", "Er\u00b7den\u00b7le\u00b7bens", "in\u00b7ners\u00b7ter", "Ge\u00b7halt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+----+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "Empor in m\u00e4chtig k\u00e4mpfender Gestaltung sich;", "tokens": ["Em\u00b7por", "in", "m\u00e4ch\u00b7tig", "k\u00e4mp\u00b7fen\u00b7der", "Ge\u00b7stal\u00b7tung", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJD", "ADJA", "NN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der Sch\u00f6nheit und der Wahrheit Opfer flammte hoch", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "und", "der", "Wahr\u0