{"textgrid.poem.57208": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Licht ist Liebe ... Sonnen-Weben", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Licht ist Liebe ... Sonnen-Weben", "tokens": ["Licht", "ist", "Lie\u00b7be", "...", "Son\u00b7nen\u00b7We\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "VAFIN", "NN", "$(", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebes-Strahlung einer Welt", "tokens": ["Lie\u00b7bes\u00b7Strah\u00b7lung", "ei\u00b7ner", "Welt"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "sch\u00f6pferischer Wesenheiten \u2013", "tokens": ["sch\u00f6p\u00b7fe\u00b7ri\u00b7scher", "We\u00b7sen\u00b7hei\u00b7ten", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "die durch unerh\u00f6rte Zeiten", "tokens": ["die", "durch", "un\u00b7er\u00b7h\u00f6r\u00b7te", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "uns an ihrem Herzen h\u00e4lt,", "tokens": ["uns", "an", "ih\u00b7rem", "Her\u00b7zen", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und die uns zuletzt gegeben", "tokens": ["und", "die", "uns", "zu\u00b7letzt", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "ihren h\u00f6chsten Geist in eines", "tokens": ["ih\u00b7ren", "h\u00f6chs\u00b7ten", "Geist", "in", "ei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Menschen H\u00fclle w\u00e4hrend dreier", "tokens": ["Men\u00b7schen", "H\u00fcl\u00b7le", "w\u00e4h\u00b7rend", "drei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "CARD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jahre: da Er kam in Seines", "tokens": ["Jah\u00b7re", ":", "da", "Er", "kam", "in", "Sei\u00b7nes"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KOUS", "PPER", "VVFIN", "APPR", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Vaters Erbteil \u2013 nun der Erde", "tokens": ["Va\u00b7ters", "Erb\u00b7teil", "\u2013", "nun", "der", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$(", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "innerlichstes Himmelsfeuer:", "tokens": ["in\u00b7ner\u00b7lichs\u00b7tes", "Him\u00b7mels\u00b7feu\u00b7er", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df auch sie einst Sonne werde.", "tokens": ["da\u00df", "auch", "sie", "einst", "Son\u00b7ne", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}