{"dta.poem.19958": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht nach der Waldgegend .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r' ich nie aus euch gegangen", "tokens": ["W\u00e4r'", "ich", "nie", "aus", "euch", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4lder hehr und wunderbar!", "tokens": ["W\u00e4l\u00b7der", "hehr", "und", "wun\u00b7der\u00b7bar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hieltet liebend mich umfangen", "tokens": ["Hiel\u00b7tet", "lie\u00b7bend", "mich", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ADJD", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch so lange lange Jahr! \u2014", "tokens": ["Doch", "so", "lan\u00b7ge", "lan\u00b7ge", "Jahr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ADV", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo in euren D\u00e4mmerungen", "tokens": ["Wo", "in", "eu\u00b7ren", "D\u00e4m\u00b7me\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vogel sang und Silberquell", "tokens": ["Vo\u00b7gel", "sang", "und", "Sil\u00b7ber\u00b7quell"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist auch manches Lied entsprungen", "tokens": ["Ist", "auch", "man\u00b7ches", "Lied", "ent\u00b7sprun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinem Busen, frisch und hell.", "tokens": ["Mei\u00b7nem", "Bu\u00b7sen", ",", "frisch", "und", "hell", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Euer Wogen, eure Halle,", "tokens": ["Eu\u00b7er", "Wo\u00b7gen", ",", "eu\u00b7re", "Hal\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Euer S\u00e4useln nimmer m\u00fcd,", "tokens": ["Eu\u00b7er", "S\u00e4u\u00b7seln", "nim\u00b7mer", "m\u00fcd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eure Melodien alle", "tokens": ["Eu\u00b7re", "Me\u00b7lo\u00b7dien", "al\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "PIAT"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Weckten in der Brust das Lied.", "tokens": ["Weck\u00b7ten", "in", "der", "Brust", "das", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hier in diesen weiten Triften", "tokens": ["Hier", "in", "die\u00b7sen", "wei\u00b7ten", "Trif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist mir alles \u00f6d und stumm,", "tokens": ["Ist", "mir", "al\u00b7les", "\u00f6d", "und", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich schau' in blauen L\u00fcften", "tokens": ["Und", "ich", "schau'", "in", "blau\u00b7en", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich nach Wolkenbildern um.", "tokens": ["Mich", "nach", "Wol\u00b7ken\u00b7bil\u00b7dern", "um", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "In den Busen eingezwinget", "tokens": ["In", "den", "Bu\u00b7sen", "ein\u00b7ge\u00b7zwin\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Regt sich selten nur das Lied.", "tokens": ["Regt", "sich", "sel\u00b7ten", "nur", "das", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie nur halb der Vogel singet", "tokens": ["Wie", "nur", "halb", "der", "Vo\u00b7gel", "sin\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "ART", "NE", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den von Baum und Bach man schied.", "tokens": ["Den", "von", "Baum", "und", "Bach", "man", "schied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "KON", "NE", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}