{"dta.poem.17199": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "Vierdtes Buch  \n Geistreicher Sinn- und  \n Schlu\u00df-Reime.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.71", "af:0.28"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Sein Blutt ists Purpurkleid/ die M\u00f6rder die Tra-", "tokens": ["Sein", "Blutt", "ists", "Pur\u00b7pur\u00b7kleid", "/", "die", "M\u00f6r\u00b7der", "die", "Tra"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "VAFIN", "NN", "$(", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "banten/", "tokens": ["ban\u00b7ten", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Das Hoffgesind ein Schaum von Buben und Scher-", "tokens": ["Das", "Hoff\u00b7ge\u00b7sind", "ein", "Schaum", "von", "Bu\u00b7ben", "und", "Scher"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "ganten:", "tokens": ["gan\u00b7ten", ":"], "token_info": ["word", "punct"], "pos": ["ADJA", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Der Mundtrank bittre Gall/ die Music Hohn und", "tokens": ["Der", "Mund\u00b7trank", "bitt\u00b7re", "Gall", "/", "die", "Mu\u00b7sic", "Hohn", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Spott.", "tokens": ["Spott", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Di\u00df ist die Herrligkeit die hier hat unser GOtt!", "tokens": ["Di\u00df", "ist", "die", "Herr\u00b7lig\u00b7keit", "die", "hier", "hat", "un\u00b7ser", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}