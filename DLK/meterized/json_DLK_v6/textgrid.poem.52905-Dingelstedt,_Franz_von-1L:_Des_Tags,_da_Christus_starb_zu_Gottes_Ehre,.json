{"textgrid.poem.52905": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des Tags, da Christus starb zu Gottes Ehre,", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Tags, da Christus starb zu Gottes Ehre,", "tokens": ["Des", "Tags", ",", "da", "Chris\u00b7tus", "starb", "zu", "Got\u00b7tes", "Eh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "NE", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kniet' ich an der Mich\u00e4lis-Kirche Schwelle,", "tokens": ["Kniet'", "ich", "an", "der", "Mich\u00e4\u00b7lis\u00b7Kir\u00b7che", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Umbraust von stolzer Sang- und Orgel-Welle,", "tokens": ["Um\u00b7braust", "von", "stol\u00b7zer", "Sang", "und", "Or\u00b7gel\u00b7Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Still und zerknirscht: \u2013 ", "tokens": ["Still", "und", "zer\u00b7knirscht", ":", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJD", "KON", "VVPP", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Am Hochaltar erlosch die Kerzenhelle", "tokens": ["Am", "Hoc\u00b7hal\u00b7tar", "er\u00b7losch", "die", "Ker\u00b7zen\u00b7hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Langsam und m\u00e4lig, bis die lautlos-schwere,", "tokens": ["Lang\u00b7sam", "und", "m\u00e4\u00b7lig", ",", "bis", "die", "laut\u00b7los\u00b7schwe\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "KOUS", "ART", "ADJA", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die starre Nacht \u2013 ", "tokens": ["Die", "star\u00b7re", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Rings auf den Betern lag und der Kapelle.", "tokens": ["Rings", "auf", "den", "Be\u00b7tern", "lag", "und", "der", "Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und als ich so sie schwinden sah, die Lichter,", "tokens": ["Und", "als", "ich", "so", "sie", "schwin\u00b7den", "sah", ",", "die", "Lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPER", "ADJD", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eins nach dem andern von der Nacht verschlungen,", "tokens": ["Eins", "nach", "dem", "an\u00b7dern", "von", "der", "Nacht", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schien mir's, als ob's ein Bild des Landes w\u00e4re:", "tokens": ["Schien", "mir's", ",", "als", "ob's", "ein", "Bild", "des", "Lan\u00b7des", "w\u00e4\u00b7re", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KOUS", "NE", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Bald schied ein Denker, bald erstarb ein Dichter,", "tokens": ["Bald", "schied", "ein", "Den\u00b7ker", ",", "bald", "er\u00b7starb", "ein", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Still ward's und \u00f6d' und aus den D\u00e4mmerungen", "tokens": ["Still", "ward's", "und", "\u00f6d'", "und", "aus", "den", "D\u00e4m\u00b7me\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "KON", "ADJD", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Klang's schluchzend auf: \u2013 ", "tokens": ["Klang's", "schluch\u00b7zend", "auf", ":", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}