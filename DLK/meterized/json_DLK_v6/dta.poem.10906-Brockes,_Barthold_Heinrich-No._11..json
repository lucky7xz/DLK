{"dta.poem.10906": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "No.  11.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit ergetzendem Erstaunen, mit bewunderndem Vergn\u00fcgen,", "tokens": ["Mit", "er\u00b7get\u00b7zen\u00b7dem", "Er\u00b7stau\u00b7nen", ",", "mit", "be\u00b7wun\u00b7dern\u00b7dem", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sieht man hier den stolzen Hirsch, gleichsam majest\u00e4tisch, liegen.", "tokens": ["Sieht", "man", "hier", "den", "stol\u00b7zen", "Hirsch", ",", "gleich\u00b7sam", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", ",", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "ADJA", "NN", "$,", "ADJD", "ADJD", "$,", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Ich bewundere, mit Ehrfurcht, den, der solcher Glieder Pracht", "tokens": ["Ich", "be\u00b7wun\u00b7de\u00b7re", ",", "mit", "Ehr\u00b7furcht", ",", "den", ",", "der", "sol\u00b7cher", "Glie\u00b7der", "Pracht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN", "$,", "ART", "$,", "PRELS", "PIAT", "NN", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dieser Creatur geschenkt. Aber, wird nicht auch bedacht,", "tokens": ["Die\u00b7ser", "Crea\u00b7tur", "ge\u00b7schenkt", ".", "A\u00b7ber", ",", "wird", "nicht", "auch", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVPP", "$.", "KON", "$,", "VAFIN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Rief ich, durch die Kunst ger\u00fchrt, wie, durch etwas schwarze", "tokens": ["Rief", "ich", ",", "durch", "die", "Kunst", "ge\u00b7r\u00fchrt", ",", "wie", ",", "durch", "et\u00b7was", "schwar\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ART", "NN", "VVPP", "$,", "PWAV", "$,", "APPR", "PIAT", "ADJA"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Solch ein Thier, als wenn es lebet, lebhaft vorgestellet werde?", "tokens": ["Solch", "ein", "Thier", ",", "als", "wenn", "es", "le\u00b7bet", ",", "leb\u00b7haft", "vor\u00b7ge\u00b7stel\u00b7let", "wer\u00b7de", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,", "ADJD", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Seht, es reget sich der Hals! seht, ihn kr\u00f6net sein Geweih;", "tokens": ["Seht", ",", "es", "re\u00b7get", "sich", "der", "Hals", "!", "seht", ",", "ihn", "kr\u00f6\u00b7net", "sein", "Ge\u00b7weih", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PRF", "ART", "NN", "$.", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Seht, wie jede Muskel, selbst durch die Haut, zusehen sey.", "tokens": ["Seht", ",", "wie", "je\u00b7de", "Mus\u00b7kel", ",", "selbst", "durch", "die", "Haut", ",", "zu\u00b7se\u00b7hen", "sey", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PIAT", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,", "VVPP", "VAFIN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Aus dem Auge guckt der Hirschgeist. Kann man seiner L\u00e4ufte", "tokens": ["Aus", "dem", "Au\u00b7ge", "guckt", "der", "Hirschgeist", ".", "Kann", "man", "sei\u00b7ner", "L\u00e4uf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$.", "VMFIN", "PIS", "PPOSAT", "NN"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "H\u00e4rte, Gl\u00e4tt und Festigkeit deutlicher mit Farben malen?", "tokens": ["H\u00e4r\u00b7te", ",", "Gl\u00e4tt", "und", "Fes\u00b7tig\u00b7keit", "deut\u00b7li\u00b7cher", "mit", "Far\u00b7ben", "ma\u00b7len", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "KON", "NN", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "In noch mehr, man sieht mit Lust, wie die\u00df Kunst - erf\u00fcllte", "tokens": ["In", "noch", "mehr", ",", "man", "sieht", "mit", "Lust", ",", "wie", "die\u00df", "Kunst", "er\u00b7f\u00fcll\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADV", "$,", "PIS", "VVFIN", "APPR", "NN", "$,", "PWAV", "PDS", "NN", "$(", "VVFIN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Noch, vor allen andern Bl\u00e4ttern, was besonders an sich hat.", "tokens": ["Noch", ",", "vor", "al\u00b7len", "an\u00b7dern", "Bl\u00e4t\u00b7tern", ",", "was", "be\u00b7son\u00b7ders", "an", "sich", "hat", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PIAT", "ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "PRF", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Man sieht hier, durch Kunst, die Kunst sich mit der Natur", "tokens": ["Man", "sieht", "hier", ",", "durch", "Kunst", ",", "die", "Kunst", "sich", "mit", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "$,", "APPR", "NN", "$,", "ART", "NN", "PRF", "APPR", "ART", "NN"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "In der B\u00e4ume Perspectiv herrscht die Ordnung, und es scheinen", "tokens": ["In", "der", "B\u00e4u\u00b7me", "Per\u00b7spec\u00b7tiv", "herrscht", "die", "Ord\u00b7nung", ",", "und", "es", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "--+-++-+-+-+-+-", "measure": "anapaest.init"}, "line.15": {"text": "Hier die Bilder keine Bilder, sondern K\u00f6rper, die versteint.", "tokens": ["Hier", "die", "Bil\u00b7der", "kei\u00b7ne", "Bil\u00b7der", ",", "son\u00b7dern", "K\u00f6r\u00b7per", ",", "die", "ver\u00b7steint", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PIAT", "NN", "$,", "KON", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Wie nun alles, was man sieht, nach dem Aug-Punkt sich verkleint:", "tokens": ["Wie", "nun", "al\u00b7les", ",", "was", "man", "sieht", ",", "nach", "dem", "Aug\u00b7Punkt", "sich", "ver\u00b7kleint", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "$,", "PRELS", "PIS", "VVFIN", "$,", "APPR", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+--++--+", "measure": "trochaic.septa.relaxed"}, "line.17": {"text": "So vergr\u00f6ssert sich bey mir die Bewunderung der Kunst,", "tokens": ["So", "ver\u00b7gr\u00f6s\u00b7sert", "sich", "bey", "mir", "die", "Be\u00b7wun\u00b7de\u00b7rung", "der", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.18": {"text": "Und zugleich der Preis des Sch\u00f6pfers, dessen Weisheit, Macht", "tokens": ["Und", "zu\u00b7gleich", "der", "Preis", "des", "Sch\u00f6p\u00b7fers", ",", "des\u00b7sen", "Weis\u00b7heit", ",", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "$,", "PRELAT", "NN", "$,", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.19": {"text": "Wir so manche Wissenschaft, geist-und k\u00f6rperliche Gaben,", "tokens": ["Wir", "so", "man\u00b7che", "Wis\u00b7sen\u00b7schaft", ",", "geist\u00b7\u00b7und", "k\u00f6r\u00b7per\u00b7li\u00b7che", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "So zur Lust, als auch zum Nutzen, blo\u00df allem zu danken haben.", "tokens": ["So", "zur", "Lust", ",", "als", "auch", "zum", "Nut\u00b7zen", ",", "blo\u00df", "al\u00b7lem", "zu", "dan\u00b7ken", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "KOUS", "ADV", "APPRART", "NN", "$,", "ADV", "PIS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}