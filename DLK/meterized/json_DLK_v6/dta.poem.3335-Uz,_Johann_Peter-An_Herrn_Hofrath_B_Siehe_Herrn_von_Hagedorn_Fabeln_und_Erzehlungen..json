{"dta.poem.3335": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Hofrath B   Siehe Herrn von Hagedorn Fabeln und Erzehlungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Wie, wenn des M\u00fcllers brauner Stecken", "tokens": ["Wie", ",", "wenn", "des", "M\u00fcl\u00b7lers", "brau\u00b7ner", "Ste\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Esel, welcher ledig zeucht,", "tokens": ["Dem", "E\u00b7sel", ",", "wel\u00b7cher", "le\u00b7dig", "zeucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von seiner Eselinn vielleicht,", "tokens": ["Von", "sei\u00b7ner", "E\u00b7se\u00b7linn", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vielleicht von distelreichen Hecken", "tokens": ["Viel\u00b7leicht", "von", "dis\u00b7tel\u00b7rei\u00b7chen", "He\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gebietherisch verscheucht;", "tokens": ["Ge\u00b7bie\u00b7the\u00b7risch", "ver\u00b7scheucht", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das tr\u00e4ge Thier alsdann, beschwert mit neuen S\u00e4cken,", "tokens": ["Das", "tr\u00e4\u00b7ge", "Thier", "als\u00b7dann", ",", "be\u00b7schwert", "mit", "neu\u00b7en", "S\u00e4\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Ohren hangen l\u00e4\u00dft, und melancholisch schleicht:", "tokens": ["Die", "Oh\u00b7ren", "han\u00b7gen", "l\u00e4\u00dft", ",", "und", "me\u00b7lan\u00b7cho\u00b7lisch", "schleicht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit gleicher traurigen Geberde", "tokens": ["Mit", "glei\u00b7cher", "trau\u00b7ri\u00b7gen", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Gieng ich im Innersten der Erde,", "tokens": ["Gieng", "ich", "im", "In\u00b7ners\u00b7ten", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Wo durch die unerhellte Nacht", "tokens": ["Wo", "durch", "die", "un\u00b7er\u00b7hell\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mein Alter mich zum Gnomen f\u00fchrte.", "tokens": ["Mein", "Al\u00b7ter", "mich", "zum", "Gno\u00b7men", "f\u00fchr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Er schien mir, wie ich ihn gedacht,", "tokens": ["Er", "schien", "mir", ",", "wie", "ich", "ihn", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Klein, h\u00e4\u00dflich, erdenbleich und stolz auf seinen Schacht.", "tokens": ["Klein", ",", "h\u00e4\u00df\u00b7lich", ",", "er\u00b7den\u00b7bleich", "und", "stolz", "auf", "sei\u00b7nen", "Schacht", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die H\u00f6hle, seine Wohnung, zierte", "tokens": ["Die", "H\u00f6h\u00b7le", ",", "sei\u00b7ne", "Woh\u00b7nung", ",", "zier\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Was Tellus kostbars zeugt, der Geiz mit Angst bewacht,", "tokens": ["Was", "Tel\u00b7lus", "kost\u00b7bars", "zeugt", ",", "der", "Geiz", "mit", "Angst", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "NE", "VVFIN", "$,", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und Narren unertr\u00e4glich macht.", "tokens": ["Und", "Nar\u00b7ren", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ein grosser Affe warf beym Eingang mich mit Kothe:", "tokens": ["Ein", "gros\u00b7ser", "Af\u00b7fe", "warf", "beym", "Ein\u00b7gang", "mich", "mit", "Ko\u00b7the", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "PRF", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich stutzt\u2019 und wich zur\u00fcck; doch als der Gnom\u2019 ihm", "tokens": ["Ich", "stutzt'", "und", "wich", "zu\u00b7r\u00fcck", ";", "doch", "als", "der", "Gnom'", "ihm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$.", "ADV", "KOUS", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "drohte,", "tokens": ["droh\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Dann ihm zween derbe Streiche gab,", "tokens": ["Dann", "ihm", "zween", "der\u00b7be", "Strei\u00b7che", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.21": {"text": "So lie\u00df er zornig von mir ab,", "tokens": ["So", "lie\u00df", "er", "zor\u00b7nig", "von", "mir", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und hatte Lust mich anzuspeyen,", "tokens": ["Und", "hat\u00b7te", "Lust", "mich", "an\u00b7zus\u00b7pe\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "PPER", "VVIZU", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Wandt endlich sich hinweg, und zeigte mir den Steis.", "tokens": ["Wandt", "end\u00b7lich", "sich", "hin\u00b7weg", ",", "und", "zeig\u00b7te", "mir", "den", "Steis", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PRF", "APZR", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mit Lachen sprach der Geist zu seines Lieblings Preis:", "tokens": ["Mit", "La\u00b7chen", "sprach", "der", "Geist", "zu", "sei\u00b7nes", "Lieb\u00b7lings", "Preis", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Es ist mein Hofpoet; man mu\u00df ihm was verzeihen.", "tokens": ["Es", "ist", "mein", "Hof\u00b7poet", ";", "man", "mu\u00df", "ihm", "was", "ver\u00b7zei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$.", "PIS", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.26": {"text": "Er spa\u00dft stets aufgeweckt und fein.", "tokens": ["Er", "spa\u00dft", "stets", "auf\u00b7ge\u00b7weckt", "und", "fein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Ich geb ihm Brod, mit Sch\u00e4ckereyen", "tokens": ["Ich", "geb", "ihm", "Brod", ",", "mit", "Sch\u00e4\u00b7cke\u00b7re\u00b7yen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.28": {"text": "Mich, eh ich schlafe, zu erfreuen:", "tokens": ["Mich", ",", "eh", "ich", "schla\u00b7fe", ",", "zu", "er\u00b7freu\u00b7en", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Denn seine Scherze schl\u00e4fern ein.", "tokens": ["Denn", "sei\u00b7ne", "Scher\u00b7ze", "schl\u00e4\u00b7fern", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Seyd ihr Poeten sonst was n\u00fctze?", "tokens": ["Seyd", "ihr", "Po\u00b7et\u00b7en", "sonst", "was", "n\u00fct\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPOSAT", "NN", "ADV", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Wenn ihr nicht Possen macht, so bleibt bey eurer Pf\u00fctze,", "tokens": ["Wenn", "ihr", "nicht", "Pos\u00b7sen", "macht", ",", "so", "bleibt", "bey", "eu\u00b7rer", "Pf\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Bey Hypokrenen, ohne Wein!", "tokens": ["Bey", "Hy\u00b7po\u00b7kre\u00b7nen", ",", "oh\u00b7ne", "Wein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}