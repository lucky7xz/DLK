{"textgrid.poem.50119": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "7. Auff Artemons Deutsche Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "In jedem Abschnitt h\u00f6rt man klingen,", "tokens": ["In", "je\u00b7dem", "Ab\u00b7schnitt", "h\u00f6rt", "man", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die sich im Unverstand verschantzen,", "tokens": ["Die", "sich", "im", "Un\u00b7ver\u00b7stand", "ver\u00b7schant\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und in geschlossner Reihe tantzen:", "tokens": ["Und", "in", "ge\u00b7schloss\u00b7ner", "Rei\u00b7he", "tant\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zwar les' ich selten sie vom Anfang biss ans End',", "tokens": ["Zwar", "les'", "ich", "sel\u00b7ten", "sie", "vom", "An\u00b7fang", "biss", "ans", "End'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PPER", "APPRART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch klopf' ich lachend in die H\u00e4nd',", "tokens": ["Doch", "klopf'", "ich", "la\u00b7chend", "in", "die", "H\u00e4nd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und denck' es sind nicht schlechte Sachen", "tokens": ["Und", "denck", "es", "sind", "nicht", "schlech\u00b7te", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "VAFIN", "PTKNEG", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Aus ", "tokens": ["Aus"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}}}}