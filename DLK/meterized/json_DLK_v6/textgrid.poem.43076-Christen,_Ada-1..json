{"textgrid.poem.43076": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Luft ist wie verpestet,", "tokens": ["Die", "Luft", "ist", "wie", "ver\u00b7pes\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vergiftet, was ich seh',", "tokens": ["Ver\u00b7gif\u00b7tet", ",", "was", "ich", "seh'", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und alle Blicke sind Dolche", "tokens": ["Und", "al\u00b7le", "Bli\u00b7cke", "sind", "Dol\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und jedes Wort ein Weh.", "tokens": ["Und", "je\u00b7des", "Wort", "ein", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Herzen sind verschlossen,", "tokens": ["Die", "Her\u00b7zen", "sind", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erkennen mich nimmermehr;", "tokens": ["Er\u00b7ken\u00b7nen", "mich", "nim\u00b7mer\u00b7mehr", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von Allen aber, von Allen", "tokens": ["Von", "Al\u00b7len", "a\u00b7ber", ",", "von", "Al\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "ADV", "$,", "APPR", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verkennt mich am meisten ", "tokens": ["Ver\u00b7kennt", "mich", "am", "meis\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKA", "PIS"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Und w\u00fcrd' ich's ihm erz\u00e4hlen,", "tokens": ["Und", "w\u00fcrd'", "ich's", "ihm", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihm Alles sagen \u2013 o Gott!", "tokens": ["Ihm", "Al\u00b7les", "sa\u00b7gen", "\u2013", "o", "Gott", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVINF", "$(", "FM", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er w\u00fcrde auch dann noch lachen", "tokens": ["Er", "w\u00fcr\u00b7de", "auch", "dann", "noch", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und ich \u2013 ich w\u00e4re todt!", "tokens": ["Und", "ich", "\u2013", "ich", "w\u00e4\u00b7re", "todt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}