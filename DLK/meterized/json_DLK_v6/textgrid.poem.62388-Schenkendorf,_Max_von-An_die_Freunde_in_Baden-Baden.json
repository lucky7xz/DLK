{"textgrid.poem.62388": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "An die Freunde in Baden-Baden", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ihr wandelt auf den Matten", "tokens": ["Wenn", "ihr", "wan\u00b7delt", "auf", "den", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An des Oelbachs klarer Flut,", "tokens": ["An", "des", "O\u00b7el\u00b7bachs", "kla\u00b7rer", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ihr in dem Eichenschatten", "tokens": ["Wenn", "ihr", "in", "dem", "Ei\u00b7chen\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An dem Fu\u00df der Berge ruht;", "tokens": ["An", "dem", "Fu\u00df", "der", "Ber\u00b7ge", "ruht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist auch Einer, der den Becher", "tokens": ["Ist", "auch", "Ei\u00b7ner", ",", "der", "den", "Be\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIS", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4gt, und Brod und k\u00fchlen Wein?", "tokens": ["Tr\u00e4gt", ",", "und", "Brod", "und", "k\u00fch\u00b7len", "Wein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Treuer Diener, treuer Zecher", "tokens": ["Treu\u00b7er", "Die\u00b7ner", ",", "treu\u00b7er", "Ze\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit euch trinket, euch schenkt ein?", "tokens": ["Mit", "euch", "trin\u00b7ket", ",", "euch", "schenkt", "ein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00f6rt ihr's fl\u00fcstern in den Zweigen", "tokens": ["H\u00f6rt", "ih\u00b7r's", "fl\u00fcs\u00b7tern", "in", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Z\u00e4rtlich, wehmuthsvoll und mild?", "tokens": ["Z\u00e4rt\u00b7lich", ",", "weh\u00b7muths\u00b7voll", "und", "mild", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht ihr aus den Fluten steigen", "tokens": ["Seht", "ihr", "aus", "den", "Flu\u00b7ten", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Ein bewegtes, dunkles Bild?", "tokens": ["Ein", "be\u00b7weg\u00b7tes", ",", "dunk\u00b7les", "Bild", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Das bin ich, das ist mein Sehnen,", "tokens": ["Das", "bin", "ich", ",", "das", "ist", "mein", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welches immer um euch ist,", "tokens": ["Wel\u00b7ches", "im\u00b7mer", "um", "euch", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Euch begr\u00fc\u00dft in allen T\u00f6nen,", "tokens": ["Euch", "be\u00b7gr\u00fc\u00dft", "in", "al\u00b7len", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euer Haupt im Westwind k\u00fc\u00dft.", "tokens": ["Eu\u00b7er", "Haupt", "im", "West\u00b7wind", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Thal von Baden, zu gesunden", "tokens": ["Thal", "von", "Ba\u00b7den", ",", "zu", "ge\u00b7sun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kam ich hin, ein kranker Mann,", "tokens": ["Kam", "ich", "hin", ",", "ein", "kran\u00b7ker", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich habe mehr gefunden", "tokens": ["Und", "ich", "ha\u00b7be", "mehr", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ich singen und sagen kann.", "tokens": ["Als", "ich", "sin\u00b7gen", "und", "sa\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "KON", "VVINF", "VMFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Gr\u00fc\u00df dich Gott, du Thal von Baden,", "tokens": ["Gr\u00fc\u00df", "dich", "Gott", ",", "du", "Thal", "von", "Ba\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "PPER", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Wunderquelle quoll,", "tokens": ["Wo", "die", "Wun\u00b7der\u00b7quel\u00b7le", "quoll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aller Wonnen, aller Gnaden,", "tokens": ["Al\u00b7ler", "Won\u00b7nen", ",", "al\u00b7ler", "Gna\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Allen Zaubers reich und voll.", "tokens": ["Al\u00b7len", "Zau\u00b7bers", "reich", "und", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Segensmeer herabgeflossen,", "tokens": ["Se\u00b7gens\u00b7meer", "her\u00b7ab\u00b7ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "All' Erinnern festgebannt,", "tokens": ["All'", "E\u00b7rin\u00b7nern", "fest\u00b7ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder Wunsch in dir beschlossen,", "tokens": ["Je\u00b7der", "Wunsch", "in", "dir", "be\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie du selbst von Bergesrand.", "tokens": ["Wie", "du", "selbst", "von", "Ber\u00b7ges\u00b7rand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Gr\u00fc\u00df dich Gott, du Herz der Herzen,", "tokens": ["Gr\u00fc\u00df", "dich", "Gott", ",", "du", "Herz", "der", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ne Frau so still und mild,", "tokens": ["Sch\u00f6\u00b7ne", "Frau", "so", "still", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4gdlein, welche singen und scherzen,", "tokens": ["M\u00e4gd\u00b7lein", ",", "wel\u00b7che", "sin\u00b7gen", "und", "scher\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "KON", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Dich der Demuth frommes Bild.", "tokens": ["Dich", "der", "De\u00b7muth", "from\u00b7mes", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Euch, ihr M\u00e4nner, euch, ihr Frauen,", "tokens": ["Euch", ",", "ihr", "M\u00e4n\u00b7ner", ",", "euch", ",", "ihr", "Frau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mich dulden und verstehn,", "tokens": ["Die", "mich", "dul\u00b7den", "und", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Euch, ihr Bl\u00fcmlein auf den Auen,", "tokens": ["Euch", ",", "ihr", "Bl\u00fcm\u00b7lein", "auf", "den", "Au\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schl\u00f6sser auf den Felsenh\u00f6h'n.", "tokens": ["Schl\u00f6s\u00b7ser", "auf", "den", "Fel\u00b7sen\u00b7h\u00f6h'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["NN", "APPR", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Weit umher auf Str\u00f6men, Wegen,", "tokens": ["Weit", "um\u00b7her", "auf", "Str\u00f6\u00b7men", ",", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "NN", "$,", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Zog ich in dem heil'gen Reich;", "tokens": ["Zog", "ich", "in", "dem", "heil'\u00b7gen", "Reich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mancher Gru\u00df kam mir entgegen,", "tokens": ["Man\u00b7cher", "Gru\u00df", "kam", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch mein Gr\u00fc\u00dfen meint nur Euch.", "tokens": ["Doch", "mein", "Gr\u00fc\u00b7\u00dfen", "meint", "nur", "Euch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Denkt auch mein mit guten Worten,", "tokens": ["Denkt", "auch", "mein", "mit", "gu\u00b7ten", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der euch t\u00e4glich Kr\u00e4nze flicht,", "tokens": ["Der", "euch", "t\u00e4g\u00b7lich", "Kr\u00e4n\u00b7ze", "flicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem sich \u00f6ffnen hundert Pforten,", "tokens": ["Dem", "sich", "\u00f6ff\u00b7nen", "hun\u00b7dert", "Pfor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADV", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber ach! die liebste nicht!", "tokens": ["A\u00b7ber", "ach", "!", "die", "liebs\u00b7te", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "ART", "ADJA", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Der ich irre, der ich wandre", "tokens": ["Der", "ich", "ir\u00b7re", ",", "der", "ich", "wand\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Manche Nacht und manchen Tag,", "tokens": ["Man\u00b7che", "Nacht", "und", "man\u00b7chen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber nimmermehr mir andre", "tokens": ["A\u00b7ber", "nim\u00b7mer\u00b7mehr", "mir", "and\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freud' und Freundschaft suchen mag.", "tokens": ["Freud'", "und", "Freund\u00b7schaft", "su\u00b7chen", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["NN", "KON", "NN", "VVFIN", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}