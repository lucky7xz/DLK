{"dta.poem.1308": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Vier Creutze sind vorbey/ das f\u00fcnffte soll ich schliessen/", "tokens": ["Vier", "Creut\u00b7ze", "sind", "vor\u00b7bey", "/", "das", "f\u00fcnff\u00b7te", "soll", "ich", "schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "$(", "ART", "ADJA", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich mu\u00df nun abermahl auff Rechnung seyn befliessen/", "tokens": ["Ich", "mu\u00df", "nun", "a\u00b7ber\u00b7mahl", "auff", "Rech\u00b7nung", "seyn", "be\u00b7flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "NN", "PPOSAT", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erweg ich den Verlust/ betracht ich den Gewinn/", "tokens": ["Er\u00b7weg", "ich", "den", "Ver\u00b7lust", "/", "be\u00b7tracht", "ich", "den", "Ge\u00b7winn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$(", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So find sich/ da\u00df ich viel/ und nichts gebessert bin.", "tokens": ["So", "find", "sich", "/", "da\u00df", "ich", "viel", "/", "und", "nichts", "ge\u00b7bes\u00b7sert", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$(", "KOUS", "PPER", "ADV", "$(", "KON", "PIS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gott gab mir manche Gunst und Wohlthat zu gen\u00fcssen/", "tokens": ["Gott", "gab", "mir", "man\u00b7che", "Gunst", "und", "Wohlt\u00b7hat", "zu", "ge\u00b7n\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PIAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch mu\u00df ich sonst dabey am Creutzes-Joche ziehn/", "tokens": ["Doch", "mu\u00df", "ich", "sonst", "da\u00b7bey", "am", "Creut\u00b7zes\u00b7Jo\u00b7che", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PAV", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Beschwerden nehmen zu/ die beste Zeit geht hin/", "tokens": ["Be\u00b7schwer\u00b7den", "neh\u00b7men", "zu", "/", "die", "bes\u00b7te", "Zeit", "geht", "hin", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "PTKZU", "$(", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich lebe wie zuvor/ beb\u00fcrde mein Gewissen/", "tokens": ["Ich", "le\u00b7be", "wie", "zu\u00b7vor", "/", "be\u00b7b\u00fcr\u00b7de", "mein", "Ge\u00b7wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADV", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Bund mit GOtt gemacht wird offtermahls zerrissen/", "tokens": ["Der", "Bund", "mit", "Gott", "ge\u00b7macht", "wird", "off\u00b7ter\u00b7mahls", "zer\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich hege/ was ich offt am meisten solte fliehn/", "tokens": ["Ich", "he\u00b7ge", "/", "was", "ich", "offt", "am", "meis\u00b7ten", "sol\u00b7te", "fliehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "PPER", "ADV", "PTKA", "PIS", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Lust/ Eyfer/ Ungedult und eiteles Bem\u00fchn;", "tokens": ["Lust", "/", "Ey\u00b7fer", "/", "Un\u00b7ge\u00b7dult", "und", "ei\u00b7te\u00b7les", "Be\u00b7m\u00fchn", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was kan hieraus als Furcht und Sorg\u2019 und Leyd entsprissen/", "tokens": ["Was", "kan", "hier\u00b7aus", "als", "Furcht", "und", "Sor\u00b7g'", "und", "Leyd", "ent\u00b7spris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PAV", "KOUS", "NN", "KON", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Darvon zu mancher Zeit mein Hertze ward gebissen.", "tokens": ["Dar\u00b7von", "zu", "man\u00b7cher", "Zeit", "mein", "Hert\u00b7ze", "ward", "ge\u00b7bis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PIAT", "NN", "PPOSAT", "VVFIN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Hilff H\u00f6chster/ da\u00df ich dir ins k\u00fcnfftig besser dien/", "tokens": ["Hilff", "H\u00f6chs\u00b7ter", "/", "da\u00df", "ich", "dir", "ins", "k\u00fcnff\u00b7tig", "bes\u00b7ser", "di\u00b7en", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "KOUS", "PPER", "PPER", "APPRART", "ADJD", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Erweich ein Hertz aus Stein/ erneure Geist und Sinn/", "tokens": ["Er\u00b7weich", "ein", "Hertz", "aus", "Stein", "/", "er\u00b7neu\u00b7re", "Geist", "und", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "NN", "$(", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Soll ich im f\u00fcnfften Creutz mein Creutz und Leben schl\u00fcssen/", "tokens": ["Soll", "ich", "im", "f\u00fcnff\u00b7ten", "Creutz", "mein", "Creutz", "und", "Le\u00b7ben", "schl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "ADJA", "NN", "PPOSAT", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "La\u00df deiner Gnaden Thau auff meine Seele fl\u00fcssen.", "tokens": ["La\u00df", "dei\u00b7ner", "Gna\u00b7den", "Thau", "auff", "mei\u00b7ne", "See\u00b7le", "fl\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dein Leyden sey mein Trost/ dein Sterben mein Gewinn/", "tokens": ["Dein", "Ley\u00b7den", "sey", "mein", "Trost", "/", "dein", "Ster\u00b7ben", "mein", "Ge\u00b7winn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Gieb/ da\u00df ich ewiglich dein Kind und bey dir bin.", "tokens": ["Gieb", "/", "da\u00df", "ich", "e\u00b7wig\u00b7lich", "dein", "Kind", "und", "bey", "dir", "bin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "KON", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}