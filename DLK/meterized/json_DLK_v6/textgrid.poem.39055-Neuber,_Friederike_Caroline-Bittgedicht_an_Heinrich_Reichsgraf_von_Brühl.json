{"textgrid.poem.39055": {"metadata": {"author": {"name": "Neuber, Friederike Caroline", "birth": "N.A.", "death": "N.A."}, "title": "Bittgedicht an Heinrich Reichsgraf von Br\u00fchl", "genre": "verse", "period": "N.A.", "pub_year": 1728, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach! Hochgebohrner Br\u00fchl!", "tokens": ["Ach", "!", "Hoch\u00b7ge\u00b7bohr\u00b7ner", "Br\u00fchl", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "hilfft denn kein Bitten nicht?", "tokens": ["hilfft", "denn", "kein", "Bit\u00b7ten", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ist denn gar kein Mensch der vor mein Elend spricht?", "tokens": ["Und", "ist", "denn", "gar", "kein", "Mensch", "der", "vor", "mein", "E\u00b7lend", "spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PIAT", "NN", "ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was soll ich Arme thun? Soll denn des M\u00fcllers wegen", "tokens": ["Was", "soll", "ich", "Ar\u00b7me", "thun", "?", "Soll", "denn", "des", "M\u00fcl\u00b7lers", "we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "NN", "VVINF", "$.", "VMFIN", "ADV", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Recht gekr\u00e4ncket seyn? Kann Dich mein Jammer regen?", "tokens": ["Mein", "Recht", "ge\u00b7kr\u00e4n\u00b7cket", "seyn", "?", "Kann", "Dich", "mein", "Jam\u00b7mer", "re\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAINF", "$.", "VMFIN", "PPER", "PPOSAT", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist denn mein Hab und Guth desswegen nicht mehr mein,", "tokens": ["Ist", "denn", "mein", "Hab", "und", "Guth", "dess\u00b7we\u00b7gen", "nicht", "mehr", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "PAV", "PTKNEG", "ADV", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Weil es der M\u00fcller will? Es jammert einen Stein", "tokens": ["Weil", "es", "der", "M\u00fcl\u00b7ler", "will", "?", "Es", "jam\u00b7mert", "ei\u00b7nen", "Stein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NE", "VMFIN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und gleichwohl kan mein Leid doch keine Hertzen r\u00fchren", "tokens": ["Und", "gleich\u00b7wohl", "kan", "mein", "Leid", "doch", "kei\u00b7ne", "Hert\u00b7zen", "r\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPOSAT", "NN", "ADV", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach lass Dir dissmahl doch mein Recht zu Hertzen f\u00fchren", "tokens": ["Ach", "lass", "Dir", "diss\u00b7mahl", "doch", "mein", "Recht", "zu", "Hert\u00b7zen", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es sieht das gantze Land alleine nur auf Dich", "tokens": ["Es", "sieht", "das", "gant\u00b7ze", "Land", "al\u00b7lei\u00b7ne", "nur", "auf", "Dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mein Recht ist offenbahr, und dennoch soll ich mich", "tokens": ["Mein", "Recht", "ist", "of\u00b7fen\u00b7bahr", ",", "und", "den\u00b7noch", "soll", "ich", "mich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "ADV", "VMFIN", "PPER", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Davon verstossen sehn. Ach! lass Dich doch erbitten", "tokens": ["Da\u00b7von", "ver\u00b7stos\u00b7sen", "sehn", ".", "Ach", "!", "lass", "Dich", "doch", "er\u00b7bit\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVINF", "VVINF", "$.", "ITJ", "$.", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich habe ohne Schuld ja schon genug gelitten,", "tokens": ["Ich", "ha\u00b7be", "oh\u00b7ne", "Schuld", "ja", "schon", "ge\u00b7nug", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den Schaden thut mir ja kein Mensch nicht wieder gut", "tokens": ["Den", "Scha\u00b7den", "thut", "mir", "ja", "kein", "Mensch", "nicht", "wie\u00b7der", "gut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PIAT", "NN", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich zittre, HErr! davor, und diss mein redlich Blut", "tokens": ["Ich", "zitt\u00b7re", ",", "Herr", "!", "da\u00b7vor", ",", "und", "diss", "mein", "red\u00b7lich", "Blut"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$.", "PAV", "$,", "KON", "PDS", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das muss vor Hertzeleid in meinen Adern wallen", "tokens": ["Das", "muss", "vor", "Hert\u00b7ze\u00b7leid", "in", "mei\u00b7nen", "A\u00b7dern", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und durch die Augen gehn. Lass nur diss Wort erschallen", "tokens": ["Und", "durch", "die", "Au\u00b7gen", "gehn", ".", "Lass", "nur", "diss", "Wort", "er\u00b7schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$.", "NE", "ADV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dass ich auf meinen Platz mein Brod behalten kan", "tokens": ["Dass", "ich", "auf", "mei\u00b7nen", "Platz", "mein", "Brod", "be\u00b7hal\u00b7ten", "kan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und gieb dem Rath Befehl, es geht ja sonst nicht an.", "tokens": ["Und", "gieb", "dem", "Rath", "Be\u00b7fehl", ",", "es", "geht", "ja", "sonst", "nicht", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "NN", "$,", "PPER", "VVFIN", "ADV", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Weiss denn mein K\u00f6nig nicht durch mein vielf\u00e4ltges Klagen", "tokens": ["Weiss", "denn", "mein", "K\u00f6\u00b7nig", "nicht", "durch", "mein", "viel\u00b7f\u00e4lt\u00b7ges", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "PPOSAT", "NN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was mir mit Recht geh\u00f6rt? Du kannst es Ihm ja sagen", "tokens": ["Was", "mir", "mit", "Recht", "ge\u00b7h\u00f6rt", "?", "Du", "kannst", "es", "Ihm", "ja", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Warum vorziehst Du denn? Es stehet ja bey Dir", "tokens": ["Wa\u00b7rum", "vor\u00b7ziehst", "Du", "denn", "?", "Es", "ste\u00b7het", "ja", "bey", "Dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Den Vortrag frey zu thun, wir alle sind schon hier", "tokens": ["Den", "Vor\u00b7trag", "frey", "zu", "thun", ",", "wir", "al\u00b7le", "sind", "schon", "hier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$,", "PPER", "PIS", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Viel hundert Thaler hab ich albereits verlohren", "tokens": ["Viel", "hun\u00b7dert", "Tha\u00b7ler", "hab", "ich", "al\u00b7be\u00b7reits", "ver\u00b7loh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "NN", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der M\u00fcller, der nur l\u00fcgt und sich dazu verschworen", "tokens": ["Der", "M\u00fcl\u00b7ler", ",", "der", "nur", "l\u00fcgt", "und", "sich", "da\u00b7zu", "ver\u00b7schwo\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "$,", "PRELS", "ADV", "VVFIN", "KON", "PRF", "PAV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und fest entschlossen hat: mein Untergang allein", "tokens": ["Und", "fest", "ent\u00b7schlos\u00b7sen", "hat", ":", "mein", "Un\u00b7ter\u00b7gang", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "VAFIN", "$.", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Soll seiner Raserey ein FreudenOpfer seyn", "tokens": ["Soll", "sei\u00b7ner", "Ra\u00b7se\u00b7rey", "ein", "Freu\u00b7den", "Op\u00b7fer", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "ART", "NN", "NN", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der hintergehet Dich und will es nur erzwingen", "tokens": ["Der", "hin\u00b7ter\u00b7ge\u00b7het", "Dich", "und", "will", "es", "nur", "er\u00b7zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "KON", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Mich um mein Haab und Guth, doch durch Befehl, zu bringen", "tokens": ["Mich", "um", "mein", "Ha\u00b7ab", "und", "Guth", ",", "doch", "durch", "Be\u00b7fehl", ",", "zu", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "KON", "NN", "$,", "ADV", "APPR", "NN", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.30": {"text": "Denn wenn ich l\u00e4nger nicht zum Spiel Erlaubniss hab", "tokens": ["Denn", "wenn", "ich", "l\u00e4n\u00b7ger", "nicht", "zum", "Spiel", "Er\u00b7laub\u00b7niss", "hab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "PTKNEG", "APPRART", "NN", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So bringt er mich gewiss dadurch an Bettelstab,", "tokens": ["So", "bringt", "er", "mich", "ge\u00b7wiss", "da\u00b7durch", "an", "Bet\u00b7tel\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "PAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ich muss, denn Haab und Guth ist an dem Bau gewendet", "tokens": ["Ich", "muss", ",", "denn", "Ha\u00b7ab", "und", "Guth", "ist", "an", "dem", "Bau", "ge\u00b7wen\u00b7det"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "KON", "NN", "KON", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "Nichts ist in meiner Hand, wenn sich die Noth nicht endet", "tokens": ["Nichts", "ist", "in", "mei\u00b7ner", "Hand", ",", "wenn", "sich", "die", "Noth", "nicht", "en\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "PPOSAT", "NN", "$,", "KOUS", "PRF", "ART", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So wird mein gantzes Werck zerrissen und zerst\u00f6rt", "tokens": ["So", "wird", "mein", "gant\u00b7zes", "Werck", "zer\u00b7ris\u00b7sen", "und", "zer\u00b7st\u00f6rt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wenn Da nicht f\u00fcr mich sprichst, dass mich der K\u00f6nig h\u00f6rt.", "tokens": ["Wenn", "Da", "nicht", "f\u00fcr", "mich", "sprichst", ",", "dass", "mich", "der", "K\u00f6\u00b7nig", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKNEG", "APPR", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ich bitte Dich zuletzt um des Augustus Nahmen,", "tokens": ["Ich", "bit\u00b7te", "Dich", "zu\u00b7letzt", "um", "des", "Au\u00b7gus\u00b7tus", "Nah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.37": {"text": "Um Seinen kalten Leib von dem die Kr\u00e4ffte kahmen", "tokens": ["Um", "Sei\u00b7nen", "kal\u00b7ten", "Leib", "von", "dem", "die", "Kr\u00e4ff\u00b7te", "kah\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "APPR", "PRELS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die Deine Wohlfurth so vollkommen sch\u00f6n gebaut", "tokens": ["Die", "Dei\u00b7ne", "Wohl\u00b7furth", "so", "voll\u00b7kom\u00b7men", "sch\u00f6n", "ge\u00b7baut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADJD", "ADJD", "VVPP"], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.39": {"text": "Dass man Dich itzo noch zu Seinem Ehren schaut.", "tokens": ["Dass", "man", "Dich", "it\u00b7zo", "noch", "zu", "Sei\u00b7nem", "Eh\u00b7ren", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ich bitte dem August der lebet und regieret", "tokens": ["Ich", "bit\u00b7te", "dem", "Au\u00b7gust", "der", "le\u00b7bet", "und", "re\u00b7gie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "Und der an seiner Statt den K\u00f6nigsZepter f\u00fchret", "tokens": ["Und", "der", "an", "sei\u00b7ner", "Statt", "den", "K\u00f6\u00b7nigs", "Zep\u00b7ter", "f\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "PPOSAT", "NN", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Dass Er mir gn\u00e4dig ist, und mir den SchauspielPlatz", "tokens": ["Dass", "Er", "mir", "gn\u00e4\u00b7dig", "ist", ",", "und", "mir", "den", "Schau\u00b7spiel", "Platz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$,", "KON", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Er\u00f6ffnen lassen mag. Das ist der gr\u00f6sste Schatz", "tokens": ["Er\u00b7\u00f6ff\u00b7nen", "las\u00b7sen", "mag.", "Das", "ist", "der", "gr\u00f6ss\u00b7te", "Schatz"], "token_info": ["word", "word", "abbreviation", "word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "VMFIN", "PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.44": {"text": "Den ich erhalten kan, ich will mich nur, mit Ehren,", "tokens": ["Den", "ich", "er\u00b7hal\u00b7ten", "kan", ",", "ich", "will", "mich", "nur", ",", "mit", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "VVINF", "VMFIN", "$,", "PPER", "VMFIN", "PPER", "ADV", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Als wie ein Unterthan zur Nothdurfft drauf ernehren.", "tokens": ["Als", "wie", "ein", "Un\u00b7ter\u00b7than", "zur", "Noth\u00b7durfft", "drauf", "er\u00b7neh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "APPRART", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Augustus Nahmen, den Er mir, als K\u00f6nig, schrieb", "tokens": ["Au\u00b7gus\u00b7tus", "Nah\u00b7men", ",", "den", "Er", "mir", ",", "als", "K\u00f6\u00b7nig", ",", "schrieb"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "PPER", "$,", "KOUS", "NN", "$,", "VVFIN"], "meter": "+--+---+-+-+", "measure": "dactylic.di.plus"}, "line.47": {"text": "Wird, Hochgebohrner Herr, Dir doch gewiss mehr lieb", "tokens": ["Wird", ",", "Hoch\u00b7ge\u00b7bohr\u00b7ner", "Herr", ",", "Dir", "doch", "ge\u00b7wiss", "mehr", "lieb"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "ADJA", "NN", "$,", "PPER", "ADV", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Als wie der M\u00fcller seyn. Vergieb mir meine Klagen", "tokens": ["Als", "wie", "der", "M\u00fcl\u00b7ler", "seyn", ".", "Ver\u00b7gieb", "mir", "mei\u00b7ne", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "NE", "VAINF", "$.", "VVIMP", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Ich weiss Dir warlich nun nichts kl\u00e4glichers zu sagen.", "tokens": ["Ich", "weiss", "Dir", "war\u00b7lich", "nun", "nichts", "kl\u00e4g\u00b7li\u00b7chers", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "PIS", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Verzeihe wenn die Noth Gesetz und Eisen bricht.", "tokens": ["Ver\u00b7zei\u00b7he", "wenn", "die", "Noth", "Ge\u00b7setz", "und", "Ei\u00b7sen", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "ART", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Sind meine Worte schlecht, vorwirff sie darum nicht", "tokens": ["Sind", "mei\u00b7ne", "Wor\u00b7te", "schlecht", ",", "vor\u00b7wirff", "sie", "da\u00b7rum", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PAV", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Ich werde Lebenslang Dich ehrerbiethigst ehren", "tokens": ["Ich", "wer\u00b7de", "Le\u00b7bens\u00b7lang", "Dich", "ehr\u00b7er\u00b7biet\u00b7higst", "eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "PPER", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Und Deinen hohen Ruhm, in Demuth, auch vermehren.", "tokens": ["Und", "Dei\u00b7nen", "ho\u00b7hen", "Ruhm", ",", "in", "De\u00b7muth", ",", "auch", "ver\u00b7meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hochgebohrner Herr!", "tokens": ["Hoch\u00b7ge\u00b7bohr\u00b7ner", "Herr", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ew. Hochgebohrn: Excell:", "tokens": ["Ew.", "Hoch\u00b7ge\u00b7bohrn", ":", "Ex\u00b7cell", ":"], "token_info": ["abbreviation", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$.", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Leipzig", "tokens": ["Leip\u00b7zig"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "d. 17. M\u00e4y", "tokens": ["d.", "17.", "M\u00e4y"], "token_info": ["abbreviation", "ordinal", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "1734.", "tokens": ["."], "token_info": ["punct"], "pos": ["$."]}}, "stanza.4": {"line.1": {"text": "Nimm Hochgebohrner Br\u00fchl!", "tokens": ["Nimm", "Hoch\u00b7ge\u00b7bohr\u00b7ner", "Br\u00fchl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "von mir diss leichte Blat", "tokens": ["von", "mir", "diss", "leich\u00b7te", "Blat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADJD", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das leyder! nichts als schon bekanten Inhalt hat", "tokens": ["Das", "ley\u00b7der", "!", "nichts", "als", "schon", "be\u00b7kan\u00b7ten", "In\u00b7halt", "hat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PIS", "KOKOM", "ADV", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich zittre dass ich Dich so sehr damit muss plagen,", "tokens": ["Ich", "zitt\u00b7re", "dass", "ich", "Dich", "so", "sehr", "da\u00b7mit", "muss", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "PRF", "ADV", "ADV", "PAV", "VMFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Allein was soll ich thun? Ich muss mein Elend klagen.", "tokens": ["Al\u00b7lein", "was", "soll", "ich", "thun", "?", "Ich", "muss", "mein", "E\u00b7lend", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VMFIN", "PPER", "VVINF", "$.", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iamb