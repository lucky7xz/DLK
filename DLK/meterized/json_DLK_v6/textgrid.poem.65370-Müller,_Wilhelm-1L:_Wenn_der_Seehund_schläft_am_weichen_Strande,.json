{"textgrid.poem.65370": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn der Seehund schl\u00e4ft am weichen Strande,", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Seehund schl\u00e4ft am weichen Strande,", "tokens": ["Wenn", "der", "See\u00b7hund", "schl\u00e4ft", "am", "wei\u00b7chen", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "H\u00e4lt bei ihm die treue Mewe Hut,", "tokens": ["H\u00e4lt", "bei", "ihm", "die", "treu\u00b7e", "Me\u00b7we", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "NN", "$,"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Kreist umher und schauet nach dem Lande,", "tokens": ["Kreist", "um\u00b7her", "und", "schau\u00b7et", "nach", "dem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schauet wieder in die hohe Fluth.", "tokens": ["Schau\u00b7et", "wie\u00b7der", "in", "die", "ho\u00b7he", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "H\u00f6rt sie's rascheln in des Ufers B\u00e4umen,", "tokens": ["H\u00f6rt", "sie's", "ra\u00b7scheln", "in", "des", "U\u00b7fers", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Kr\u00e4ht sie hell \u2013 das ist ein J\u00e4gersmann.", "tokens": ["Kr\u00e4ht", "sie", "hell", "\u2013", "das", "ist", "ein", "J\u00e4\u00b7gers\u00b7mann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$(", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sieht sie's auf dem fernen Spiegel sch\u00e4umen,", "tokens": ["Sieht", "sie's", "auf", "dem", "fer\u00b7nen", "Spie\u00b7gel", "sch\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Das sind Boote \u2013 und sie fliegt ihn an.", "tokens": ["Das", "sind", "Boo\u00b7te", "\u2013", "und", "sie", "fliegt", "ihn", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$(", "KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Und der Schl\u00e4fer folgt den Losungszeichen", "tokens": ["Und", "der", "Schl\u00e4\u00b7fer", "folgt", "den", "Lo\u00b7sungs\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seiner immerwachen Warnerin;", "tokens": ["Sei\u00b7ner", "im\u00b7mer\u00b7wa\u00b7chen", "War\u00b7ne\u00b7rin", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Eh' Harpun' und Kugel ihn erreichen,", "tokens": ["Eh'", "Ha\u00b7rpun'", "und", "Ku\u00b7gel", "ihn", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schl\u00fcpft er in das Meer und schwimmt dahin.", "tokens": ["Schl\u00fcpft", "er", "in", "das", "Meer", "und", "schwimmt", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "KON", "VVFIN", "PAV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Lieber, seh' ich dich vom Strande schiffen", "tokens": ["Lie\u00b7ber", ",", "seh'", "ich", "dich", "vom", "Stran\u00b7de", "schif\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "VVFIN", "PPER", "PRF", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In die hohe wilde Fluth hinein,", "tokens": ["In", "die", "ho\u00b7he", "wil\u00b7de", "Fluth", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nach den Wirbeln, B\u00e4nken, Klippen, Riffen \u2013", "tokens": ["Nach", "den", "Wir\u00b7beln", ",", "B\u00e4n\u00b7ken", ",", "Klip\u00b7pen", ",", "Rif\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "M\u00f6cht' ich bei dir wie die Mewe sein.", "tokens": ["M\u00f6cht'", "ich", "bei", "dir", "wie", "die", "Me\u00b7we", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "KOKOM", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Aber ach, wer giebt mir ihre Schwingen?", "tokens": ["A\u00b7ber", "ach", ",", "wer", "giebt", "mir", "ih\u00b7re", "Schwin\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nimm mich zu dir in dein kleines Boot!", "tokens": ["Nimm", "mich", "zu", "dir", "in", "dein", "klei\u00b7nes", "Boot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Mit dir will ich durch die Wogen ringen,", "tokens": ["Mit", "dir", "will", "ich", "durch", "die", "Wo\u00b7gen", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mit dir theilen aller St\u00fcrme Noth.", "tokens": ["Mit", "dir", "thei\u00b7len", "al\u00b7ler", "St\u00fcr\u00b7me", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Sage nicht, ich soll im Hause bleiben,", "tokens": ["Sa\u00b7ge", "nicht", ",", "ich", "soll", "im", "Hau\u00b7se", "blei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPER", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bist du fort, so mu\u00df mein Herz dir nach.", "tokens": ["Bist", "du", "fort", ",", "so", "mu\u00df", "mein", "Herz", "dir", "nach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKVZ", "$,", "ADV", "VMFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Willst du's ohne Steuer lassen treiben", "tokens": ["Willst", "du's", "oh\u00b7ne", "Steu\u00b7er", "las\u00b7sen", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "APPR", "NN", "VVINF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Durch der Fluthen grauses Ungemach?", "tokens": ["Durch", "der", "Flut\u00b7hen", "grau\u00b7ses", "Un\u00b7ge\u00b7mach", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}