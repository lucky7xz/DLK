{"dta.poem.1293": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.71", "sv:0.28"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Vier Str\u00f6me sahe man in Edens Garten fliessen/", "tokens": ["Vier", "Str\u00f6\u00b7me", "sa\u00b7he", "man", "in", "E\u00b7dens", "Gar\u00b7ten", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PIS", "APPR", "NE", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "in Nord/ S\u00fcd/ Ost und West ihr Wasser theilen ein/", "tokens": ["in", "Nord", "/", "S\u00fcd", "/", "Ost", "und", "West", "ihr", "Was\u00b7ser", "thei\u00b7len", "ein", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$(", "NN", "$(", "NN", "KON", "NN", "PPOSAT", "NN", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kan wohl ein besser Quell und edler Spring-Brunn seyn/", "tokens": ["Kan", "wohl", "ein", "bes\u00b7ser", "Quell", "und", "ed\u00b7ler", "Spring\u00b7Brunn", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als den sich Zion sieht in alle Welt ergiessen.", "tokens": ["Als", "den", "sich", "Zi\u00b7on", "sieht", "in", "al\u00b7le", "Welt", "er\u00b7gies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PRF", "NE", "VVFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df ist der Tauffe Flutt; der mu\u00df Amanens Bach/", "tokens": ["Di\u00df", "ist", "der", "Tauf\u00b7fe", "Flutt", ";", "der", "mu\u00df", "A\u00b7ma\u00b7nens", "Bach", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$.", "ART", "VMFIN", "NE", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Damascus Pharphar und der Jordan geben nach.", "tokens": ["Da\u00b7mas\u00b7cus", "Phar\u00b7phar", "und", "der", "Jor\u00b7dan", "ge\u00b7ben", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "ART", "NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Erbschuld Aussatz wird durch dieses Bad geheilet/", "tokens": ["Der", "Erb\u00b7schuld", "Aus\u00b7satz", "wird", "durch", "die\u00b7ses", "Bad", "ge\u00b7hei\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "PDAT", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Bad das seinen Brunn allein im Himmel wei\u00df/", "tokens": ["Ein", "Bad", "das", "sei\u00b7nen", "Brunn", "al\u00b7lein", "im", "Him\u00b7mel", "wei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Brunn der wieder fleust/ und f\u00fchrt ins Paradei\u00df/", "tokens": ["Den", "Brunn", "der", "wie\u00b7der", "fleust", "/", "und", "f\u00fchrt", "ins", "Pa\u00b7ra\u00b7dei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "VVFIN", "$(", "KON", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Di\u00df ward mir weyland auch zum Leben mitgetheilet/", "tokens": ["Di\u00df", "ward", "mir", "wey\u00b7land", "auch", "zum", "Le\u00b7ben", "mit\u00b7ge\u00b7thei\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVFIN", "ADV", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der H\u00f6chste lege mir desselben Nutzen bey/", "tokens": ["Der", "H\u00f6chs\u00b7te", "le\u00b7ge", "mir", "des\u00b7sel\u00b7ben", "Nut\u00b7zen", "bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PDAT", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df seine Gnad und Huld best\u00e4ndig um mich sey!", "tokens": ["Da\u00df", "sei\u00b7ne", "Gnad", "und", "Huld", "be\u00b7st\u00e4n\u00b7dig", "um", "mich", "sey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "ADJD", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}