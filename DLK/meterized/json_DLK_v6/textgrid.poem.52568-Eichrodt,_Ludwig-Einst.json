{"textgrid.poem.52568": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Einst", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An dich allein denk ich so gerne,", "tokens": ["An", "dich", "al\u00b7lein", "denk", "ich", "so", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu dir flieht mein Gedanke hin!", "tokens": ["Zu", "dir", "flieht", "mein", "Ge\u00b7dan\u00b7ke", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die s\u00fc\u00dfen braunen Augensterne", "tokens": ["Die", "s\u00fc\u00b7\u00dfen", "brau\u00b7nen", "Au\u00b7gens\u00b7ter\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie gehn mir nimmer aus dem Sinn.", "tokens": ["Sie", "gehn", "mir", "nim\u00b7mer", "aus", "dem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich mu\u00df mit ihren Feuerblicken", "tokens": ["Ich", "mu\u00df", "mit", "ih\u00b7ren", "Feu\u00b7er\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie \u00fcberall und immer sehn,", "tokens": ["Sie", "\u00fc\u00b7be\u00b7rall", "und", "im\u00b7mer", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie kommen meinen Traum zu schm\u00fccken,", "tokens": ["Sie", "kom\u00b7men", "mei\u00b7nen", "Traum", "zu", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich seh sie Nachts am Himmel stehn.", "tokens": ["Ich", "seh", "sie", "Nachts", "am", "Him\u00b7mel", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Denk ich der Stunden jener trauten,", "tokens": ["Denk", "ich", "der", "Stun\u00b7den", "je\u00b7ner", "trau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PDAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da wir wie Kinder fort und fort", "tokens": ["Da", "wir", "wie", "Kin\u00b7der", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOKOM", "NN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns l\u00e4chelnd in die Augen schauten,", "tokens": ["Uns", "l\u00e4\u00b7chelnd", "in", "die", "Au\u00b7gen", "schau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beseligt ohne Ku\u00df und Wort.", "tokens": ["Be\u00b7se\u00b7ligt", "oh\u00b7ne", "Ku\u00df", "und", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da wir erquickten Geist und Sinne", "tokens": ["Da", "wir", "er\u00b7quick\u00b7ten", "Geist", "und", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An allem Trefflichen der Welt,", "tokens": ["An", "al\u00b7lem", "Treff\u00b7li\u00b7chen", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wir allm\u00e4lig wurden inne,", "tokens": ["Da", "wir", "all\u00b7m\u00e4\u00b7lig", "wur\u00b7den", "in\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was uns befl\u00fcgelt und beseelt. \u2013", "tokens": ["Was", "uns", "be\u00b7fl\u00fc\u00b7gelt", "und", "be\u00b7seelt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und denk ich jenes Augenblickes,", "tokens": ["Und", "denk", "ich", "je\u00b7nes", "Au\u00b7gen\u00b7bli\u00b7ckes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da ich den ersten Ku\u00df gewagt,", "tokens": ["Da", "ich", "den", "ers\u00b7ten", "Ku\u00df", "ge\u00b7wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und denk ich des verrauschten Gl\u00fcckes,", "tokens": ["Und", "denk", "ich", "des", "ver\u00b7rauschten", "Gl\u00fc\u00b7ckes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da du mir keinen Ku\u00df versagt. \u2013", "tokens": ["Da", "du", "mir", "kei\u00b7nen", "Ku\u00df", "ver\u00b7sagt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und denk ich, wie die Monden flossen,", "tokens": ["Und", "denk", "ich", ",", "wie", "die", "Mon\u00b7den", "flos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo dann wir, Brust an Brust gepre\u00dft,", "tokens": ["Wo", "dann", "wir", ",", "Brust", "an", "Brust", "ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "$,", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns in die Arme liebend schlossen,", "tokens": ["Uns", "in", "die", "Ar\u00b7me", "lie\u00b7bend", "schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So innig lang, so hei\u00df, so fest. \u2013", "tokens": ["So", "in\u00b7nig", "lang", ",", "so", "hei\u00df", ",", "so", "fest", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach! wenn ich jener Zeit gedenke,", "tokens": ["Ach", "!", "wenn", "ich", "je\u00b7ner", "Zeit", "ge\u00b7den\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die wie ein goldner Traum verblich,", "tokens": ["Die", "wie", "ein", "gold\u00b7ner", "Traum", "ver\u00b7blich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in den alten Traum mich senke,", "tokens": ["Und", "in", "den", "al\u00b7ten", "Traum", "mich", "sen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wei\u00df ich nur: ich liebte dich!", "tokens": ["So", "wei\u00df", "ich", "nur", ":", "ich", "lieb\u00b7te", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}