{"dta.poem.14863": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die stille Gemeine.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eund dieses Kirchlein hier bewacht,", "tokens": ["\u201e", "und", "die\u00b7ses", "Kirch\u00b7lein", "hier", "be\u00b7wacht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PDAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sollen nicht Messe singen,", "tokens": ["Sie", "sol\u00b7len", "nicht", "Mes\u00b7se", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir rei\u00dfen's nieder \u00fcber Nacht,", "tokens": ["Wir", "rei\u00b7\u00dfen's", "nie\u00b7der", "\u00fc\u00b7ber", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Licht sei, wohin wir dringen!\u201c \u2014", "tokens": ["Licht", "sei", ",", "wo\u00b7hin", "wir", "drin\u00b7gen", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}