{"textgrid.poem.53934": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Aus der Ferne", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Bett steht auf der menschenleeren Insel,", "tokens": ["Mein", "Bett", "steht", "auf", "der", "men\u00b7schen\u00b7lee\u00b7ren", "In\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "und drum herum ein kleines Haus;", "tokens": ["und", "drum", "he\u00b7rum", "ein", "klei\u00b7nes", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "APZR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "bei mir ist Courteline, ein Seifenpinsel,", "tokens": ["bei", "mir", "ist", "Cour\u00b7te\u00b7li\u00b7ne", ",", "ein", "Sei\u00b7fen\u00b7pin\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "$,", "ART", "NN", "$,"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "und nachts zur Unterhaltung eine Maus.", "tokens": ["und", "nachts", "zur", "Un\u00b7ter\u00b7hal\u00b7tung", "ei\u00b7ne", "Maus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hier gibt es keine Wasserleitung,", "tokens": ["Hier", "gibt", "es", "kei\u00b7ne", "Was\u00b7ser\u00b7lei\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "mein Essen kocht ein vegetarscher Greis;", "tokens": ["mein", "Es\u00b7sen", "kocht", "ein", "ve\u00b7ge\u00b7tar\u00b7scher", "Greis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "ich bin seit Wochen ohne Zeitung . . .", "tokens": ["ich", "bin", "seit", "Wo\u00b7chen", "oh\u00b7ne", "Zei\u00b7tung", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "APPR", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sag mir nichts \u2013", "tokens": ["Sag", "mir", "nichts", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "ich wei\u00df.", "tokens": ["ich", "wei\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df, die Volkspartei ber\u00e4t mit Scholzen,", "tokens": ["Ich", "wei\u00df", ",", "die", "Volks\u00b7par\u00b7tei", "be\u00b7r\u00e4t", "mit", "Schol\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ob sie und oder ob sie nicht;", "tokens": ["ob", "sie", "und", "o\u00b7der", "ob", "sie", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "KON", "KOUS", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ich wei\u00df, da\u00df sie in China sich verholzen;", "tokens": ["ich", "wei\u00df", ",", "da\u00df", "sie", "in", "Chi\u00b7na", "sich", "ver\u00b7hol\u00b7zen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NE", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ich wei\u00df, im Blatte prangt manch arm Gesicht.", "tokens": ["ich", "wei\u00df", ",", "im", "Blat\u00b7te", "prangt", "manch", "arm", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Wanderv\u00f6gel wolln nach Flandern wandern;", "tokens": ["Die", "Wan\u00b7der\u00b7v\u00f6\u00b7gel", "wolln", "nach", "Flan\u00b7dern", "wan\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Herr Liedtke kriegt den n\u00e4chsten Sch\u00f6nheitspreis,", "tokens": ["Herr", "Liedt\u00b7ke", "kriegt", "den", "n\u00e4chs\u00b7ten", "Sch\u00f6n\u00b7heits\u00b7preis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und eilt, beim Golf, von einem Loch zum andern...", "tokens": ["und", "eilt", ",", "beim", "Golf", ",", "von", "ei\u00b7nem", "Loch", "zum", "an\u00b7dern", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPRART", "NN", "$,", "APPR", "ART", "NN", "APPRART", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "ich wei\u00df.", "tokens": ["ich", "wei\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Herr Wolfgang Goetz machts immer noch historisch;", "tokens": ["Herr", "Wolf\u00b7gang", "Goetz", "machts", "im\u00b7mer", "noch", "his\u00b7to\u00b7risch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Herr Nobile ist eigentlich eine Frau;", "tokens": ["Herr", "No\u00b7bi\u00b7le", "ist", "ei\u00b7gent\u00b7lich", "ei\u00b7ne", "Frau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "der Leitartikel fordert kategorisch;", "tokens": ["der", "Leit\u00b7ar\u00b7ti\u00b7kel", "for\u00b7dert", "ka\u00b7te\u00b7go\u00b7risch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Herr Thomas Mann bem\u00fcht sich.", "tokens": ["Herr", "Tho\u00b7mas", "Mann", "be\u00b7m\u00fcht", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Herr Strauss tut seine Impotenz vertonen.", "tokens": ["Herr", "Strauss", "tut", "sei\u00b7ne", "Im\u00b7po\u00b7tenz", "ver\u00b7to\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Bitterfeld tagt still und leis", "tokens": ["In", "Bit\u00b7ter\u00b7feld", "tagt", "still", "und", "leis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der Reichsverband vertriebener Embryonen . . .", "tokens": ["der", "Reichs\u00b7ver\u00b7band", "ver\u00b7trie\u00b7be\u00b7ner", "Emb\u00b7ry\u00b7o\u00b7nen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "erz\u00e4hl mir nichts \u2013!", "tokens": ["er\u00b7z\u00e4hl", "mir", "nichts", "\u2013", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}