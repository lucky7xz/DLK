{"textgrid.poem.38217": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Einsiedler", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort droben auf dem H\u00fcgel,", "tokens": ["Dort", "dro\u00b7ben", "auf", "dem", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo die Nachtigall singt,", "tokens": ["Wo", "die", "Nach\u00b7ti\u00b7gall", "singt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da tanzt der Einsiedel,", "tokens": ["Da", "tanzt", "der", "Ein\u00b7sie\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df die Kutt in die H\u00f6h springt.", "tokens": ["Da\u00df", "die", "Kutt", "in", "die", "H\u00f6h", "springt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Ey la\u00dft ihn nur tanzen,", "tokens": ["Ey", "la\u00dft", "ihn", "nur", "tan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Ey la\u00dft ihn nur seyn,", "tokens": ["Ey", "la\u00dft", "ihn", "nur", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "VAINF", "$,"], "meter": "++--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Zu Nacht mu\u00df er beten", "tokens": ["Zu", "Nacht", "mu\u00df", "er", "be\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und schlafen allein.", "tokens": ["Und", "schla\u00b7fen", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Dort dr\u00fcben auf dem H\u00fcgel", "tokens": ["Dort", "dr\u00fc\u00b7ben", "auf", "dem", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wos F\u00fcchsle rum lauft,", "tokens": ["Wos", "F\u00fcchs\u00b7le", "rum", "lauft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da sizt der Einsiedel,", "tokens": ["Da", "sizt", "der", "Ein\u00b7sie\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hat die Kutte verkauft.", "tokens": ["Hat", "die", "Kut\u00b7te", "ver\u00b7kauft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Dort drunten im Thale", "tokens": ["Dort", "drun\u00b7ten", "im", "Tha\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Geht er ins Wirthshaus,", "tokens": ["Geht", "er", "ins", "Wirths\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Geh leih ihm dein Dirnel,", "tokens": ["Geh", "leih", "ihm", "dein", "Dir\u00b7nel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das mein hat ein Rausch.", "tokens": ["Das", "mein", "hat", "ein", "Rausch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "VAFIN", "ART", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Ich geh nit aufs Bergle,", "tokens": ["Ich", "geh", "nit", "aufs", "Berg\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ich geh nit ins Thal,", "tokens": ["Ich", "geh", "nit", "ins", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich leih ihm nits Dirnel,", "tokens": ["Ich", "leih", "ihm", "nits", "Dir\u00b7nel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Weg ist zu schmal.", "tokens": ["Der", "Weg", "ist", "zu", "schmal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}