{"textgrid.poem.39458": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Hoffnung Gr\u00fcn, die Bl\u00fcthe s\u00fc\u00dfer Stunden,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Hoffnung Gr\u00fcn, die Bl\u00fcthe s\u00fc\u00dfer Stunden,", "tokens": ["Der", "Hoff\u00b7nung", "Gr\u00fcn", ",", "die", "Bl\u00fc\u00b7the", "s\u00fc\u00b7\u00dfer", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Unschuld Lilie und der Sch\u00f6nheit Rose,", "tokens": ["Der", "Un\u00b7schuld", "Li\u00b7lie", "und", "der", "Sch\u00f6n\u00b7heit", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann, wie Cypressen, dunkler Schickung Loo\u00dfe,", "tokens": ["Dann", ",", "wie", "Cyp\u00b7res\u00b7sen", ",", "dunk\u00b7ler", "Schi\u00b7ckung", "Loo\u00b7\u00dfe", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NE", "$,", "ADJA", "NN", "NE", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Hast du, Guarini, zart zum Kranz gewunden.", "tokens": ["Hast", "du", ",", "Gu\u00b7a\u00b7ri\u00b7ni", ",", "zart", "zum", "Kranz", "ge\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NE", "$,", "ADJD", "APPRART", "NN", "VAPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Schon sind im Tod die Liebenden verbunden,", "tokens": ["Schon", "sind", "im", "Tod", "die", "Lie\u00b7ben\u00b7den", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da finden sie der Wonne sich im Schoo\u00dfe,", "tokens": ["Da", "fin\u00b7den", "sie", "der", "Won\u00b7ne", "sich", "im", "Schoo\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da l\u00f6s't sich auf in fl\u00fcsterndes Gekose", "tokens": ["Da", "l\u00f6s't", "sich", "auf", "in", "fl\u00fcs\u00b7tern\u00b7des", "Ge\u00b7ko\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Weh der Dornen, die ihr Herz verwunden.", "tokens": ["Das", "Weh", "der", "Dor\u00b7nen", ",", "die", "ihr", "Herz", "ver\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Treu leitet Anmuth deinen treuen Hirten;", "tokens": ["Treu", "lei\u00b7tet", "An\u00b7muth", "dei\u00b7nen", "treu\u00b7en", "Hir\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihm, wenn er lehrt die Wi\u00dfenschaft der K\u00fc\u00dfe,", "tokens": ["Ihm", ",", "wenn", "er", "lehrt", "die", "Wi\u00b7\u00dfen\u00b7schaft", "der", "K\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gl\u00fchn Wangen, zittern Lippen, wallen Busen.", "tokens": ["Gl\u00fchn", "Wan\u00b7gen", ",", "zit\u00b7tern", "Lip\u00b7pen", ",", "wal\u00b7len", "Bu\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein neu Arkadien schatten frische Myrten:", "tokens": ["Ein", "neu", "Ar\u00b7ka\u00b7di\u00b7en", "schat\u00b7ten", "fri\u00b7sche", "Myr\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NE", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Liebe huld'gen Wald, Thal, Berge, Fl\u00fc\u00dfe,", "tokens": ["Der", "Lie\u00b7be", "huld'\u00b7gen", "Wald", ",", "Thal", ",", "Ber\u00b7ge", ",", "Fl\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und tauchend folgt Alpheus Arethusen.", "tokens": ["Und", "tau\u00b7chend", "folgt", "Al\u00b7pheus", "A\u00b7ret\u00b7hu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "NE", "NE", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}