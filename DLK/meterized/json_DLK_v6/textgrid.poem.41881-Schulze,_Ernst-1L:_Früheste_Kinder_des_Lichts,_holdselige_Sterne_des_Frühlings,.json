{"textgrid.poem.41881": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fr\u00fcheste Kinder des Lichts, holdselige Sterne des Fr\u00fchlings,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fcheste Kinder des Lichts, holdselige Sterne des Fr\u00fchlings,", "tokens": ["Fr\u00fc\u00b7hes\u00b7te", "Kin\u00b7der", "des", "Lichts", ",", "hold\u00b7se\u00b7li\u00b7ge", "Ster\u00b7ne", "des", "Fr\u00fch\u00b7lings", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Bl\u00fcmlein, welche zum Strau\u00df selbst mir die Liebste gepfl\u00fcckt,", "tokens": ["Bl\u00fcm\u00b7lein", ",", "wel\u00b7che", "zum", "Strau\u00df", "selbst", "mir", "die", "Liebs\u00b7te", "ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NE", "ADV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Freundliche, wahrlich es ward ein freundliches Loos euch beschieden,", "tokens": ["Freund\u00b7li\u00b7che", ",", "wahr\u00b7lich", "es", "ward", "ein", "freund\u00b7li\u00b7ches", "Loos", "euch", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PPER", "VAFIN", "ART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Fr\u00f6hliches Leben und dann fr\u00fch ein begl\u00fcckender Tod.", "tokens": ["Fr\u00f6h\u00b7li\u00b7ches", "Le\u00b7ben", "und", "dann", "fr\u00fch", "ein", "be\u00b7gl\u00fc\u00b7cken\u00b7der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Denn ihr schautet zuerst mit den leis' aufknospenden Aeuglein", "tokens": ["Denn", "ihr", "schau\u00b7tet", "zu\u00b7erst", "mit", "den", "leis'", "auf\u00b7knos\u00b7pen\u00b7den", "A\u00b7e\u00b7u\u00b7glein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+--+--+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Hold in kindlicher Lust staunend das himmlische Licht,", "tokens": ["Hold", "in", "kind\u00b7li\u00b7cher", "Lust", "stau\u00b7nend", "das", "himm\u00b7li\u00b7sche", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Schm\u00fccktet zuerst mit den Perlen des Thaus die err\u00f6thenden Wangen,", "tokens": ["Schm\u00fcck\u00b7tet", "zu\u00b7erst", "mit", "den", "Per\u00b7len", "des", "Thaus", "die", "er\u00b7r\u00f6\u00b7then\u00b7den", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "F\u00fchltet den laulichen Ku\u00df s\u00e4uselnder L\u00fcfte zuerst.", "tokens": ["F\u00fchl\u00b7tet", "den", "lau\u00b7li\u00b7chen", "Ku\u00df", "s\u00e4u\u00b7seln\u00b7der", "L\u00fcf\u00b7te", "zu\u00b7erst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "ADV", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Und dann nahete sanft wie ein heimw\u00e4rts winkender Engel", "tokens": ["Und", "dann", "na\u00b7he\u00b7te", "sanft", "wie", "ein", "heim\u00b7w\u00e4rts", "win\u00b7ken\u00b7der", "En\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "KOKOM", "ART", "ADJA", "ADJA", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Mit zartschonender Hand meine Geliebte sich euch.", "tokens": ["Mit", "zart\u00b7scho\u00b7nen\u00b7der", "Hand", "mei\u00b7ne", "Ge\u00b7lieb\u00b7te", "sich", "euch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "PRF", "PPER", "$."], "meter": "-++--+---+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Ach, wohl zagtet ihr nicht, als sie liebkosend euch pfl\u00fcckte;", "tokens": ["Ach", ",", "wohl", "zag\u00b7tet", "ihr", "nicht", ",", "als", "sie", "lieb\u00b7ko\u00b7send", "euch", "pfl\u00fcck\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+--++-++--+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Hat doch wehe zu thun nimmer die Milde gelernt.", "tokens": ["Hat", "doch", "we\u00b7he", "zu", "thun", "nim\u00b7mer", "die", "Mil\u00b7de", "ge\u00b7lernt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "PTKZU", "VVINF", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Nein, euch schien's, als schwebe der Lenz vom heiteren Himmel,", "tokens": ["Nein", ",", "euch", "schien's", ",", "als", "schwe\u00b7be", "der", "Lenz", "vom", "hei\u00b7te\u00b7ren", "Him\u00b7mel", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$,", "KOUS", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Lieblich in M\u00e4dchengestalt kleidend den ewigen Reiz,", "tokens": ["Lieb\u00b7lich", "in", "M\u00e4d\u00b7chen\u00b7ge\u00b7stalt", "klei\u00b7dend", "den", "e\u00b7wi\u00b7gen", "Reiz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Freundlich herab, und wolle nun selbst mit den fr\u00fchesten Bl\u00fcmlein,", "tokens": ["Freund\u00b7lich", "her\u00b7ab", ",", "und", "wol\u00b7le", "nun", "selbst", "mit", "den", "fr\u00fc\u00b7hes\u00b7ten", "Bl\u00fcm\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KON", "VMFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.16": {"text": "Mit den geliebtesten, hold schm\u00fccken das heilige Haupt.", "tokens": ["Mit", "den", "ge\u00b7lieb\u00b7tes\u00b7ten", ",", "hold", "schm\u00fc\u00b7cken", "das", "hei\u00b7li\u00b7ge", "Haupt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "++-+---+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Ach, ihr sahet es nicht, wie die andern Schwestern so fr\u00f6hlich", "tokens": ["Ach", ",", "ihr", "sa\u00b7het", "es", "nicht", ",", "wie", "die", "an\u00b7dern", "Schwes\u00b7tern", "so", "fr\u00f6h\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "ART", "ADJA", "NN", "ADV", "ADJD"], "meter": "+-+---+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Bl\u00fchten, inde\u00df ihr selbst welktet im z\u00f6gernden Tod.", "tokens": ["Bl\u00fch\u00b7ten", ",", "in\u00b7de\u00df", "ihr", "selbst", "welk\u00b7tet", "im", "z\u00f6\u00b7gern\u00b7den", "Tod", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PPER", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.19": {"text": "Nimmer verletzte den z\u00e4rtlichen Kelch ein feindlicher Sturmwind,", "tokens": ["Nim\u00b7mer", "ver\u00b7letz\u00b7te", "den", "z\u00e4rt\u00b7li\u00b7chen", "Kelch", "ein", "feind\u00b7li\u00b7cher", "Sturm\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Nicht hat sengende Gluth fr\u00fch euch die Wangen entf\u00e4rbt;", "tokens": ["Nicht", "hat", "sen\u00b7gen\u00b7de", "Gluth", "fr\u00fch", "euch", "die", "Wan\u00b7gen", "ent\u00b7f\u00e4rbt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "ADJA", "NN", "ADJD", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.21": {"text": "Z\u00fcchtig bl\u00fchtet ihr auf, jungfr\u00e4ulich seyd ihr gestorben", "tokens": ["Z\u00fcch\u00b7tig", "bl\u00fch\u00b7tet", "ihr", "auf", ",", "jung\u00b7fr\u00e4u\u00b7lich", "seyd", "ihr", "ge\u00b7stor\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "ADJD", "VAFIN", "PPER", "VVPP"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Auf jungfr\u00e4ulicher Flur, heilig durch heiligen Tod.", "tokens": ["Auf", "jung\u00b7fr\u00e4u\u00b7li\u00b7cher", "Flur", ",", "hei\u00b7lig", "durch", "hei\u00b7li\u00b7gen", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "Seliges Loos! wer im fr\u00fchesten Glanz der entfalteten Sch\u00f6nheit", "tokens": ["Se\u00b7li\u00b7ges", "Loos", "!", "wer", "im", "fr\u00fc\u00b7hes\u00b7ten", "Glanz", "der", "ent\u00b7fal\u00b7te\u00b7ten", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$.", "PWS", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Hinsinkt, Vielen geliebt, Vielen noch lange beweint;", "tokens": ["Hin\u00b7sinkt", ",", "Vie\u00b7len", "ge\u00b7liebt", ",", "Vie\u00b7len", "noch", "lan\u00b7ge", "be\u00b7weint", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "VVPP", "$,", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.25": {"text": "Wer nicht sieht, wie die Blume verwelkt, die ihm lieblich geduftet,", "tokens": ["Wer", "nicht", "sieht", ",", "wie", "die", "Blu\u00b7me", "ver\u00b7welkt", ",", "die", "ihm", "lieb\u00b7lich", "ge\u00b7duf\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVFIN", "$,", "PWAV", "ART", "NN", "VVPP", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.26": {"text": "Nicht, wie das Roth sich entf\u00e4rbt, das ihm den Himmel geschm\u00fcckt.", "tokens": ["Nicht", ",", "wie", "das", "Roth", "sich", "ent\u00b7f\u00e4rbt", ",", "das", "ihm", "den", "Him\u00b7mel", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Ihm nur ward es gew\u00e4hrt, was wir All' uns w\u00fcnschen: der Fr\u00fchling", "tokens": ["Ihm", "nur", "ward", "es", "ge\u00b7w\u00e4hrt", ",", "was", "wir", "All'", "uns", "w\u00fcn\u00b7schen", ":", "der", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "VVPP", "$,", "PRELS", "PPER", "PIS", "PPER", "VVINF", "$.", "ART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.28": {"text": "Schwand ihm nimmer, und nie hat ihn das Sch\u00f6ne get\u00e4uscht.", "tokens": ["Schwand", "ihm", "nim\u00b7mer", ",", "und", "nie", "hat", "ihn", "das", "Sch\u00f6\u00b7ne", "ge\u00b7t\u00e4uscht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "Ruht nun sanft an dem Herzen, ihr Lieblichen, welches wie ihr einst", "tokens": ["Ruht", "nun", "sanft", "an", "dem", "Her\u00b7zen", ",", "ihr", "Lieb\u00b7li\u00b7chen", ",", "wel\u00b7ches", "wie", "ihr", "einst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "PRELS", "KOKOM", "PPOSAT", "NN"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "Bl\u00fchte, doch nicht wie ihr, eh' es verbl\u00fchete, brach,", "tokens": ["Bl\u00fch\u00b7te", ",", "doch", "nicht", "wie", "ihr", ",", "eh'", "es", "ver\u00b7bl\u00fc\u00b7he\u00b7te", ",", "brach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "PTKNEG", "PWAV", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.31": {"text": "Welkt nun sanft und f\u00fchrt mir, noch heilausspendend im Tode,", "tokens": ["Welkt", "nun", "sanft", "und", "f\u00fchrt", "mir", ",", "noch", "heil\u00b7aus\u00b7spen\u00b7dend", "im", "To\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "KON", "VVFIN", "PPER", "$,", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Boten des Lenzes, den Lenz heim in die traurende Brust!", "tokens": ["Bo\u00b7ten", "des", "Len\u00b7zes", ",", "den", "Lenz", "heim", "in", "die", "trau\u00b7ren\u00b7de", "Brust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}}}}