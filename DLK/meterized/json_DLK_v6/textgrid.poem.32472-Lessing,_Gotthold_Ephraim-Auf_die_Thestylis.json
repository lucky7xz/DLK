{"textgrid.poem.32472": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Thestylis", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die schiele Thestylis hat Augen in dem Kopfe,", "tokens": ["Die", "schie\u00b7le", "Thes\u00b7ty\u00b7lis", "hat", "Au\u00b7gen", "in", "dem", "Kop\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "VAFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So hat ein Luchs sie nicht.", "tokens": ["So", "hat", "ein", "Luchs", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Glaubt ihr, sie sieht euch ins Gesicht,", "tokens": ["Glaubt", "ihr", ",", "sie", "sieht", "euch", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sieht sie nach dem Hosenknopfe.", "tokens": ["So", "sieht", "sie", "nach", "dem", "Ho\u00b7sen\u00b7knop\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}