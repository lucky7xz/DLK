{"textgrid.poem.66702": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mag des Lebens stille Welle", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mag des Lebens stille Welle", "tokens": ["Mag", "des", "Le\u00b7bens", "stil\u00b7le", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich getrost ein Weilchen tragen \u2013", "tokens": ["Mich", "ge\u00b7trost", "ein", "Weil\u00b7chen", "tra\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sanfter Windhauch ihr Geselle \u2013", "tokens": ["Sanf\u00b7ter", "Wind\u00b7hauch", "ihr", "Ge\u00b7sel\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In das Land der Lotophagen,", "tokens": ["In", "das", "Land", "der", "Lo\u00b7to\u00b7pha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zu den Fluren der Vergesser", "tokens": ["Zu", "den", "Flu\u00b7ren", "der", "Ver\u00b7ges\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles Kampfes, aller Klagen,", "tokens": ["Al\u00b7les", "Kamp\u00b7fes", ",", "al\u00b7ler", "Kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo die pl\u00e4tschernden Gew\u00e4sser", "tokens": ["Wo", "die", "pl\u00e4t\u00b7schern\u00b7den", "Ge\u00b7w\u00e4s\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.8": {"text": "Tr\u00e4umerisch ans Ufer schlagen.", "tokens": ["Tr\u00e4u\u00b7me\u00b7risch", "ans", "U\u00b7fer", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Meine Seele, ja, gestatte", "tokens": ["Mei\u00b7ne", "See\u00b7le", ",", "ja", ",", "ge\u00b7stat\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "PTKANT", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir, der Welt dich zu verschlie\u00dfen,", "tokens": ["Dir", ",", "der", "Welt", "dich", "zu", "ver\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Darfst nun gleich dem Rosenblatte", "tokens": ["Darfst", "nun", "gleich", "dem", "Ro\u00b7sen\u00b7blat\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Leichtigkeit genie\u00dfen.", "tokens": ["Dei\u00b7ne", "Leich\u00b7tig\u00b7keit", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df sie k\u00e4mpfen, la\u00df sie jagen,", "tokens": ["La\u00df", "sie", "k\u00e4mp\u00b7fen", ",", "la\u00df", "sie", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie sie dich den K\u00e4mpfer hie\u00dfen,", "tokens": ["Wie", "sie", "dich", "den", "K\u00e4mp\u00b7fer", "hie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gleich den G\u00f6ttern goldner Sagen", "tokens": ["Gleich", "den", "G\u00f6t\u00b7tern", "gold\u00b7ner", "Sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Selig so dahinzuflie\u00dfen ...", "tokens": ["Se\u00b7lig", "so", "da\u00b7hin\u00b7zu\u00b7flie\u00b7\u00dfen", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}