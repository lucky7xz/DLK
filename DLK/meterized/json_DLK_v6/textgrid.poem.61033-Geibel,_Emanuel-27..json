{"textgrid.poem.61033": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist der letzte Tag erschienen,", "tokens": ["Nun", "ist", "der", "letz\u00b7te", "Tag", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sonnig blickt er in das Tal.", "tokens": ["Und", "son\u00b7nig", "blickt", "er", "in", "das", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wald scheint tiefer heut zu gr\u00fcnen,", "tokens": ["Der", "Wald", "scheint", "tie\u00b7fer", "heut", "zu", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Blumen duften ohne Zahl,", "tokens": ["Und", "Blu\u00b7men", "duf\u00b7ten", "oh\u00b7ne", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es wogt das Korn in goldnen \u00c4hren,", "tokens": ["Es", "wogt", "das", "Korn", "in", "gold\u00b7nen", "\u00c4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die V\u00f6gel singen wie zum Fest,", "tokens": ["Die", "V\u00f6\u00b7gel", "sin\u00b7gen", "wie", "zum", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Himmel selbst will uns verkl\u00e4ren", "tokens": ["Der", "Him\u00b7mel", "selbst", "will", "uns", "ver\u00b7kl\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der s\u00fc\u00dfen Stunden kurzen Rest.", "tokens": ["Der", "s\u00fc\u00b7\u00dfen", "Stun\u00b7den", "kur\u00b7zen", "Rest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O la\u00df noch heute drum das H\u00e4rmen!", "tokens": ["O", "la\u00df", "noch", "heu\u00b7te", "drum", "das", "H\u00e4r\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch ruh' ich ja an deiner Brust.", "tokens": ["Noch", "ruh'", "ich", "ja", "an", "dei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Jephthas Tochter wolle schw\u00e4rmen", "tokens": ["Wie", "Je\u00b7phthas", "Toch\u00b7ter", "wol\u00b7le", "schw\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Berg und Tal in reiner Lust!", "tokens": ["Durch", "Berg", "und", "Tal", "in", "rei\u00b7ner", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ergib dich selig dem Genusse,", "tokens": ["Er\u00b7gib", "dich", "se\u00b7lig", "dem", "Ge\u00b7nus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bis fern der Sonne Strahl verglimmt", "tokens": ["Bis", "fern", "der", "Son\u00b7ne", "Strahl", "ver\u00b7glimmt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und mit dem letzten Abschiedskusse", "tokens": ["Und", "mit", "dem", "letz\u00b7ten", "Ab\u00b7schieds\u00b7kus\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Kelch uns von den Lippen nimmt.", "tokens": ["Den", "Kelch", "uns", "von", "den", "Lip\u00b7pen", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}