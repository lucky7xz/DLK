{"textgrid.poem.41708": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf gr\u00fcnen Wiesen sah ich L\u00e4mmer weiden \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf gr\u00fcnen Wiesen sah ich L\u00e4mmer weiden \u2013", "tokens": ["Auf", "gr\u00fc\u00b7nen", "Wie\u00b7sen", "sah", "ich", "L\u00e4m\u00b7mer", "wei\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Gl\u00f6ckleinklang als einziges Get\u00f6n", "tokens": ["Ihr", "Gl\u00f6c\u00b7klein\u00b7klang", "als", "ein\u00b7zi\u00b7ges", "Ge\u00b7t\u00f6n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KOUS", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "War zu vernehmen im Vor\u00fcberschreiten \u2013", "tokens": ["War", "zu", "ver\u00b7neh\u00b7men", "im", "Vor\u00b7\u00fc\u00b7bersc\u00b7hrei\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKZU", "VVINF", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sonst Alles still \u2013 so friedlich und so sch\u00f6n.", "tokens": ["Sonst", "Al\u00b7les", "still", "\u2013", "so", "fried\u00b7lich", "und", "so", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADJD", "$(", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bei einer Linde weilte traut beisammen", "tokens": ["Bei", "ei\u00b7ner", "Lin\u00b7de", "weil\u00b7te", "traut", "bei\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "VVFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein jugendfrohes, hochbegl\u00fccktes Paar.", "tokens": ["Ein", "ju\u00b7gend\u00b7fro\u00b7hes", ",", "hoch\u00b7be\u00b7gl\u00fcck\u00b7tes", "Paar", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er lie\u00df sein Auge in das ihre flammen,", "tokens": ["Er", "lie\u00df", "sein", "Au\u00b7ge", "in", "das", "ih\u00b7re", "flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PRELS", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie bot ihm sch\u00fcchtern ihre Wange dar.", "tokens": ["Sie", "bot", "ihm", "sch\u00fcch\u00b7tern", "ih\u00b7re", "Wan\u00b7ge", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dicht gegen\u00fcber wo aus gr\u00fcnen B\u00e4umen", "tokens": ["Dicht", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "wo", "aus", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PWAV", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Gar traulich winkt ein strohgedecktes Dach,", "tokens": ["Gar", "trau\u00b7lich", "winkt", "ein", "stroh\u00b7ge\u00b7deck\u00b7tes", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da mochten sie den eignen Heerd sich tr\u00e4umen,", "tokens": ["Da", "moch\u00b7ten", "sie", "den", "eig\u00b7nen", "Heerd", "sich", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo sich ihr Wunsch der Zukunft Gl\u00fcck versprach.", "tokens": ["Wo", "sich", "ihr", "Wunsch", "der", "Zu\u00b7kunft", "Gl\u00fcck", "ver\u00b7sprach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Des Dorfes Kinder spielten muntre Spiele", "tokens": ["Des", "Dor\u00b7fes", "Kin\u00b7der", "spiel\u00b7ten", "mun\u00b7tre", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als Pferde spannten sie dem Pflug sich vor,", "tokens": ["Als", "Pfer\u00b7de", "spann\u00b7ten", "sie", "dem", "Pflug", "sich", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Knabe lenkte zum bestimmten Ziele", "tokens": ["Ein", "Kna\u00b7be", "lenk\u00b7te", "zum", "be\u00b7stimm\u00b7ten", "Zie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Peitschenknall den muntern Br\u00fcderchor. \u2013", "tokens": ["Mit", "Peit\u00b7schen\u00b7knall", "den", "mun\u00b7tern", "Br\u00fc\u00b7derc\u00b7hor", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das war vor Zeiten \u2013 als ich wiederkommen", "tokens": ["Das", "war", "vor", "Zei\u00b7ten", "\u2013", "als", "ich", "wie\u00b7der\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "NN", "$(", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu diesem stillen, waldumkr\u00e4nzten Thal \u2013", "tokens": ["Zu", "die\u00b7sem", "stil\u00b7len", ",", "wald\u00b7um\u00b7kr\u00e4nz\u00b7ten", "Thal", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hei! wie da aller Friede ist genommen,", "tokens": ["Hei", "!", "wie", "da", "al\u00b7ler", "Frie\u00b7de", "ist", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "ADV", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Hei! wie das Alles anders auf einmal!", "tokens": ["Hei", "!", "wie", "das", "Al\u00b7les", "an\u00b7ders", "auf", "ein\u00b7mal", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "ART", "PIS", "ADV", "APPR", "ADV", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}}, "stanza.6": {"line.1": {"text": "Die neue Macht, die sich die Welt erk\u00fcret", "tokens": ["Die", "neu\u00b7e", "Macht", ",", "die", "sich", "die", "Welt", "er\u00b7k\u00fc\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie hat auch hier jed alten Brauch verdr\u00e4ngt:", "tokens": ["Sie", "hat", "auch", "hier", "jed", "al\u00b7ten", "Brauch", "ver\u00b7dr\u00e4ngt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Seht wie ein Pfad jetzt durch die Berge f\u00fchret", "tokens": ["Seht", "wie", "ein", "Pfad", "jetzt", "durch", "die", "Ber\u00b7ge", "f\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Wagen an dem andern rollend h\u00e4ngt.", "tokens": ["Ein", "Wa\u00b7gen", "an", "dem", "an\u00b7dern", "rol\u00b7lend", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Statt Herdengl\u00f6cklein l\u00e4utende Signale \u2013", "tokens": ["Statt", "Her\u00b7den\u00b7gl\u00f6c\u00b7klein", "l\u00e4u\u00b7ten\u00b7de", "Sig\u00b7na\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es rauscht und zischt und saust mit Ungest\u00fcm", "tokens": ["Es", "rauscht", "und", "zischt", "und", "saust", "mit", "Un\u00b7ge\u00b7st\u00fcm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und r\u00fcttelt alle Tr\u00e4umer auf im Thale", "tokens": ["Und", "r\u00fct\u00b7telt", "al\u00b7le", "Tr\u00e4u\u00b7mer", "auf", "im", "Tha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das m\u00e4chtge feuerspeiende Unget\u00fcm. \u2013", "tokens": ["Das", "m\u00e4cht\u00b7ge", "feu\u00b7er\u00b7spei\u00b7en\u00b7de", "Un\u00b7ge\u00b7t\u00fcm", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Wird lang das Paar noch bei der Linde bleiben?", "tokens": ["Wird", "lang", "das", "Paar", "noch", "bei", "der", "Lin\u00b7de", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Maid steht bleich vor naher Trennungsqual \u2013", "tokens": ["Die", "Maid", "steht", "bleich", "vor", "na\u00b7her", "Tren\u00b7nungs\u00b7qual", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbmich will's hinaus ins rasche Leben treiben!\u00ab", "tokens": ["\u00bb", "mich", "will's", "hin\u00b7aus", "ins", "ra\u00b7sche", "Le\u00b7ben", "trei\u00b7ben", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ruft er, \u00bbleb wohl! schon pfiffs zum drittenmal!\u00ab", "tokens": ["Ruft", "er", ",", "\u00bb", "leb", "wohl", "!", "schon", "pfiffs", "zum", "drit\u00b7ten\u00b7mal", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "$(", "VVIMP", "ADV", "$.", "ADV", "NN", "APPRART", "ADV", "$.", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Sie schaut ihm nach mit sehnsuchtsvollen Blicken,", "tokens": ["Sie", "schaut", "ihm", "nach", "mit", "sehn\u00b7suchts\u00b7vol\u00b7len", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohl ahnt sie drau\u00dfen die bewegte Welt! \u2013", "tokens": ["Wohl", "ahnt", "sie", "drau\u00b7\u00dfen", "die", "be\u00b7weg\u00b7te", "Welt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wird nicht ihr Glanz des Liebsten Herz umstricken?", "tokens": ["Wird", "nicht", "ihr", "Glanz", "des", "Liebs\u00b7ten", "Herz", "um\u00b7stri\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist dies kein Riff an dem ihr Gl\u00fcck zerschellt?\u00ab", "tokens": ["Ist", "dies", "kein", "Riff", "an", "dem", "ihr", "Gl\u00fcck", "zer\u00b7schellt", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PDS", "PIAT", "NN", "APPR", "ART", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wo sind die Knaben, die sich hier erfreuten?", "tokens": ["Wo", "sind", "die", "Kna\u00b7ben", ",", "die", "sich", "hier", "er\u00b7freu\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das alte Pflugspiel ist zu schlecht und kle n", "tokens": ["Das", "al\u00b7te", "Pflug\u00b7spiel", "ist", "zu", "schlecht", "und", "kle", "n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKA", "ADJD", "KON", "VVFIN", "XY"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein bessres Loos denkt Jeder zu erbeuten", "tokens": ["Ein", "bess\u00b7res", "Loos", "denkt", "Je\u00b7der", "zu", "er\u00b7beu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als das nur ein gepeitschtes Pferd zu sein. \u2013", "tokens": ["Als", "das", "nur", "ein", "ge\u00b7peitschtes", "Pferd", "zu", "sein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PDS", "ADV", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Dahin, dahin der einsam stille Frieden,", "tokens": ["Da\u00b7hin", ",", "da\u00b7hin", "der", "ein\u00b7sam", "stil\u00b7le", "Frie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PAV", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dahin, dahin ein jed idyllisch Gl\u00fcck!", "tokens": ["Da\u00b7hin", ",", "da\u00b7hin", "ein", "jed", "i\u00b7dyl\u00b7lisch", "Gl\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PAV", "ART", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Denn alle Ruh ist aus der Welt geschieden \u2013", "tokens": ["Denn", "al\u00b7le", "Ruh", "ist", "aus", "der", "Welt", "ge\u00b7schie\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}}, "stanza.12": {"line.1": {"text": "Ja, Frieden stirb! \u2013 Du stiller Kirchhoffrieden,", "tokens": ["Ja", ",", "Frie\u00b7den", "stirb", "!", "\u2013", "Du", "stil\u00b7ler", "Kirch\u00b7hof\u00b7frie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "VVFIN", "$.", "$(", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du hast f\u00fcrwahr zu lange schon gew\u00e4hrt,", "tokens": ["Du", "hast", "f\u00fcr\u00b7wahr", "zu", "lan\u00b7ge", "schon", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKA", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein ander Gl\u00fcck giebt's noch f\u00fcr uns hinieden,", "tokens": ["Ein", "an\u00b7der", "Gl\u00fcck", "giebt's", "noch"