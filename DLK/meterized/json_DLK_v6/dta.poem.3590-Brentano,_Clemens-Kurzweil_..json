{"dta.poem.3590": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Kurzweil .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, was ich meinem Sch\u00e4tzchen               ", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "ich", "mei\u00b7nem", "Sch\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "verhie\u00df,", "tokens": ["ver\u00b7hie\u00df", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Das sie den Riegel wohl hinter sich stie\u00df;", "tokens": ["Das", "sie", "den", "Rie\u00b7gel", "wohl", "hin\u00b7ter", "sich", "stie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "ADV", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wohl hinter sich in die Ecke,", "tokens": ["Wohl", "hin\u00b7ter", "sich", "in", "die", "E\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dann schlich ich zu ihr ins Bettchen.", "tokens": ["Dann", "schlich", "ich", "zu", "ihr", "ins", "Bett\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die zwei, die liegen die halbe Nacht,", "tokens": ["Die", "zwei", ",", "die", "lie\u00b7gen", "die", "hal\u00b7be", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "$,", "PRELS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bis da\u00df das Gl\u00f6cklein zw\u00f6lfe schlagt:", "tokens": ["Bis", "da\u00df", "das", "Gl\u00f6c\u00b7klein", "zw\u00f6l\u00b7fe", "schlagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "CARD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Steh auf Braunes M\u00e4dchen zum Laden,", "tokens": ["Steh", "auf", "Brau\u00b7nes", "M\u00e4d\u00b7chen", "zum", "La\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Schau ob es noch nicht will tagen.", "tokens": ["Schau", "ob", "es", "noch", "nicht", "will", "ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie gab dem Laden einen Sto\u00df,", "tokens": ["Sie", "gab", "dem", "La\u00b7den", "ei\u00b7nen", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da scheint ihr der helle Mond in den Schoos:", "tokens": ["Da", "scheint", "ihr", "der", "hel\u00b7le", "Mond", "in", "den", "Schoos", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bleibt liegen gut Ritterlein stille,", "tokens": ["Bleibt", "lie\u00b7gen", "gut", "Rit\u00b7ter\u00b7lein", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Es taget nach unserm Willen.", "tokens": ["Es", "ta\u00b7get", "nach", "un\u00b7serm", "Wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Die zwei, die liegen die ganze Nacht,", "tokens": ["Die", "zwei", ",", "die", "lie\u00b7gen", "die", "gan\u00b7ze", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "$,", "PRELS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bis das das Gl\u00f6cklein Sechse schlagt;", "tokens": ["Bis", "das", "das", "Gl\u00f6c\u00b7klein", "Sech\u00b7se", "schlagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Steh auf braunes M\u00e4dchen zum Laden,", "tokens": ["Steh", "auf", "brau\u00b7nes", "M\u00e4d\u00b7chen", "zum", "La\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Schau ob es noch nicht will tagen?", "tokens": ["Schau", "ob", "es", "noch", "nicht", "will", "ta\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie gab dem Laden einen Sto\u00df,", "tokens": ["Sie", "gab", "dem", "La\u00b7den", "ei\u00b7nen", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da scheint ihr die helle Sonn in den Schoos;", "tokens": ["Da", "scheint", "ihr", "die", "hel\u00b7le", "Sonn", "in", "den", "Schoos", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Steh auf gut Ritterlein balde,", "tokens": ["Steh", "auf", "gut", "Rit\u00b7ter\u00b7lein", "bal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Sonn steht \u00fcberm Walde.", "tokens": ["Die", "Sonn", "steht", "\u00fc\u00b7berm", "Wal\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ei scheint die Sonn, und ich bin noch hier,", "tokens": ["Ei", "scheint", "die", "Sonn", ",", "und", "ich", "bin", "noch", "hier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VAFIN", "ADV", "ADV", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "O Gott! wie wirds ergehen mir;", "tokens": ["O", "Gott", "!", "wie", "wirds", "er\u00b7ge\u00b7hen", "mir", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich hab mich gestern Abend vermessen,", "tokens": ["Ich", "hab", "mich", "ge\u00b7stern", "A\u00b7bend", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "NN", "VVPP", "$,"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bin's leztemal bei dir gewesen.", "tokens": ["Bin's", "lez\u00b7te\u00b7mal", "bei", "dir", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPER", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Das M\u00e4dchen war so hurtig und eil,", "tokens": ["Das", "M\u00e4d\u00b7chen", "war", "so", "hur\u00b7tig", "und", "eil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Lie\u00df den Knaben herunter am Seil,", "tokens": ["Lie\u00df", "den", "Kna\u00b7ben", "her\u00b7un\u00b7ter", "am", "Seil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Sie meint, er w\u00e4re schon drunnen,", "tokens": ["Sie", "meint", ",", "er", "w\u00e4\u00b7re", "schon", "drun\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da lag er im k\u00fchlen Brunnen.", "tokens": ["Da", "lag", "er", "im", "k\u00fch\u00b7len", "Brun\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Es stand wohl an drei viertel Jahr,", "tokens": ["Es", "stand", "wohl", "an", "drei", "vier\u00b7tel", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da Brau\u00dfinde ein Kind gebahr;", "tokens": ["Da", "Brau\u00b7\u00dfin\u00b7de", "ein", "Kind", "ge\u00b7bahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir wollen tauffen H\u00e4nschen den Jungen,", "tokens": ["Wir", "wol\u00b7len", "tauf\u00b7fen", "H\u00e4n\u00b7schen", "den", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sein Vater ertrunken im Brunnen.", "tokens": ["Sein", "Va\u00b7ter", "er\u00b7trun\u00b7ken", "im", "Brun\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}