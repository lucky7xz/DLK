{"dta.poem.6694": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Geh\u00f6r.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es reitzt uns durch sich selbst ein Ton; Ein Ton verletzet", "tokens": ["Es", "reitzt", "uns", "durch", "sich", "selbst", "ein", "Ton", ";", "Ein", "Ton", "ver\u00b7let\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PRF", "ADV", "ART", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Uns gleichfalls durch sich selbst. Wir werden offt sogleich", "tokens": ["Uns", "gleich\u00b7falls", "durch", "sich", "selbst", ".", "Wir", "wer\u00b7den", "offt", "sog\u00b7leich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "PRF", "ADV", "$.", "PPER", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch ihn betr\u00fcbt, durch ihn erg\u00f6tzet.", "tokens": ["Durch", "ihn", "be\u00b7tr\u00fcbt", ",", "durch", "ihn", "er\u00b7g\u00f6t\u00b7zet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn aber er das Reich", "tokens": ["Wenn", "a\u00b7ber", "er", "das", "Reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der W\u00f6rter macht, formirt und gr\u00fcndet,", "tokens": ["Der", "W\u00f6r\u00b7ter", "macht", ",", "for\u00b7mirt", "und", "gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und mit demselben sich verbindet;", "tokens": ["Und", "mit", "dem\u00b7sel\u00b7ben", "sich", "ver\u00b7bin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn T\u00f6ne, welchen der Gebrauch", "tokens": ["Wenn", "T\u00f6\u00b7ne", ",", "wel\u00b7chen", "der", "Ge\u00b7brauch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "PWAT", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein festes Zeichen giebet;", "tokens": ["Ein", "fes\u00b7tes", "Zei\u00b7chen", "gie\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wenn unsre Seele hofft, verlanget, ha\u00dft und liebet", "tokens": ["Wenn", "uns\u00b7re", "See\u00b7le", "hofft", ",", "ver\u00b7lan\u00b7get", ",", "ha\u00dft", "und", "lie\u00b7bet"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Durchs Werckzeug des Geh\u00f6rs, auf manche Dinge denckt;", "tokens": ["Durchs", "Wer\u00b7ck\u00b7zeug", "des", "Ge\u00b7h\u00f6rs", ",", "auf", "man\u00b7che", "Din\u00b7ge", "denckt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Sodann entdeckt ein Mensch dem andern, was er denckt.", "tokens": ["So\u00b7dann", "ent\u00b7deckt", "ein", "Mensch", "dem", "an\u00b7dern", ",", "was", "er", "denckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Man sieht, von jenem Geist mit diesem, den Verband;", "tokens": ["Man", "sieht", ",", "von", "je\u00b7nem", "Geist", "mit", "die\u00b7sem", ",", "den", "Ver\u00b7band", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "APPR", "PDAT", "NN", "APPR", "PDAT", "$,", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Und durch ein Wechselweis\u2019 getriebenes Erzehlen", "tokens": ["Und", "durch", "ein", "Wech\u00b7sel\u00b7weis'", "ge\u00b7trie\u00b7be\u00b7nes", "Er\u00b7zeh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.14": {"text": "Er\u00f6ffnen, theilen mit und geben sich die Seelen", "tokens": ["Er\u00b7\u00f6ff\u00b7nen", ",", "thei\u00b7len", "mit", "und", "ge\u00b7ben", "sich", "die", "See\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPR", "KON", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Einander gl\u00fccklich den Verstand.", "tokens": ["Ein\u00b7an\u00b7der", "gl\u00fcck\u00b7lich", "den", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}