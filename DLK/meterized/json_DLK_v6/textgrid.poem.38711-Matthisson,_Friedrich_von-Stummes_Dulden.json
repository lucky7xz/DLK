{"textgrid.poem.38711": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Stummes Dulden", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Feige Sterbliche nur und aberwizige Schw\u00e4rmer", "tokens": ["Fei\u00b7ge", "Sterb\u00b7li\u00b7che", "nur", "und", "a\u00b7ber\u00b7wi\u00b7zi\u00b7ge", "Schw\u00e4r\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "KON", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Schrein von den D\u00e4chern ihr Weh, Mitleid erbettelnd vom Volk.", "tokens": ["Schrein", "von", "den", "D\u00e4\u00b7chern", "ihr", "Weh", ",", "Mit\u00b7leid", "er\u00b7bet\u00b7telnd", "vom", "Volk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Klage geziemt nicht dem Starken; im Kampf mit dem eisernen Schicksal", "tokens": ["Kla\u00b7ge", "ge\u00b7ziemt", "nicht", "dem", "Star\u00b7ken", ";", "im", "Kampf", "mit", "dem", "ei\u00b7ser\u00b7nen", "Schick\u00b7sal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKNEG", "ART", "NN", "$.", "APPRART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+---+", "measure": "dactylic.tetra.plus"}, "line.4": {"text": "Siegt nur die r\u00fcstige That; Worte sind Beute des Sturms.", "tokens": ["Siegt", "nur", "die", "r\u00fcs\u00b7ti\u00b7ge", "That", ";", "Wor\u00b7te", "sind", "Beu\u00b7te", "des", "Sturms", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$.", "NN", "VAFIN", "NN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Schl\u00e4gt ihm ein \u00e4hnliches Herz, so geb' er sich ganz und auf ewig,", "tokens": ["Schl\u00e4gt", "ihm", "ein", "\u00e4hn\u00b7li\u00b7ches", "Herz", ",", "so", "geb'", "er", "sich", "ganz", "und", "auf", "e\u00b7wig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADV", "KON", "APPR", "ADJD", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Bleibt ihm dies Kleinod versagt, werd' er sich selber die Welt.", "tokens": ["Bleibt", "ihm", "dies", "Klei\u00b7nod", "ver\u00b7sagt", ",", "werd'", "er", "sich", "sel\u00b7ber", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "NN", "VVPP", "$,", "VAFIN", "PPER", "PRF", "ADV", "ART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}}}}