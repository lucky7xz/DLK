{"dta.poem.18983": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "10.  \n  Der Sch\u00f6nen wunderliche Kunst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df au\u00df vndanckbarkeit/ oder au\u00df mi\u00dfverstand/", "tokens": ["Da\u00df", "au\u00df", "vn\u00b7dan\u00b7ck\u00b7bar\u00b7keit", "/", "o\u00b7der", "au\u00df", "mi\u00df\u00b7ver\u00b7stand", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$(", "KON", "APPR", "NE", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Sie mein lob/ lieb vnnd layd als einen schimpf", "tokens": ["Sie", "mein", "lob", "/", "lieb", "vnnd", "layd", "als", "ei\u00b7nen", "schimpf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "$(", "ADJD", "KON", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "verlachet/", "tokens": ["ver\u00b7la\u00b7chet", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Ist mir nicht wunderbar; Dieweil des himmels", "tokens": ["Ist", "mir", "nicht", "wun\u00b7der\u00b7bar", ";", "Die\u00b7weil", "des", "him\u00b7mels"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "$.", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "hand", "tokens": ["hand"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Sie also raw als sch\u00f6n zu meiner qual gemachet.", "tokens": ["Sie", "al\u00b7so", "raw", "als", "sch\u00f6n", "zu", "mei\u00b7ner", "qual", "ge\u00b7ma\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "KOKOM", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vnd da\u00df nach dem verlust/ ohn alles gegenpfand/", "tokens": ["Vnd", "da\u00df", "nach", "dem", "ver\u00b7lust", "/", "ohn", "al\u00b7les", "ge\u00b7gen\u00b7pfand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ART", "NN", "$(", "APPR", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von seiner hofnung traum mein gaist noch nicht", "tokens": ["Von", "sei\u00b7ner", "hof\u00b7nung", "traum", "mein", "gaist", "noch", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PPOSAT", "VVFIN", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "aufwachet/", "tokens": ["auf\u00b7wa\u00b7chet", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Ist mir kein wunder auch: Dan meines gaists", "tokens": ["Ist", "mir", "kein", "wun\u00b7der", "auch", ":", "Dan", "mei\u00b7nes", "gaists"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "ADV", "$.", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "bestand", "tokens": ["be\u00b7stand"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Verliebet sich in dem/ was sein laid verursachet.", "tokens": ["Ver\u00b7lie\u00b7bet", "sich", "in", "dem", "/", "was", "sein", "laid", "ver\u00b7ur\u00b7sa\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "$(", "PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--++--+-+--", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Das aber ist mir frembd/ da\u00df jhr so sch\u00f6ner mund", "tokens": ["Das", "a\u00b7ber", "ist", "mir", "frembd", "/", "da\u00df", "jhr", "so", "sch\u00f6\u00b7ner", "mund"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "VAFIN", "PPER", "ADJD", "$(", "KOUS", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die/ deren hertzen Sie durch jhre blick versehret", "tokens": ["Die", "/", "de\u00b7ren", "hert\u00b7zen", "Sie", "durch", "jhre", "blick", "ver\u00b7seh\u00b7ret"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$(", "PRELAT", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "So leichtlich lehren kan der hitzigsten lieb grund.", "tokens": ["So", "leicht\u00b7lich", "leh\u00b7ren", "kan", "der", "hit\u00b7zigs\u00b7ten", "lieb", "grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VMFIN", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}}, "stanza.4": {"line.1": {"text": "Dan alle meine witz mit wunder wirt beschweret/", "tokens": ["Dan", "al\u00b7le", "mei\u00b7ne", "witz", "mit", "wun\u00b7der", "wirt", "be\u00b7schwe\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "PPOSAT", "NN", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wan Sie so schnell vnd wol das was jhr selbs", "tokens": ["Wan", "Sie", "so", "schnell", "vnd", "wol", "das", "was", "jhr", "selbs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "KON", "ADV", "PDS", "PRELS", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "nicht kund/", "tokens": ["nicht", "kund", "/"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "PTKVZ", "$("], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Was Sie nicht lernen will/ die dolleste k\u00f6pff lehret.", "tokens": ["Was", "Sie", "nicht", "ler\u00b7nen", "will", "/", "die", "dol\u00b7les\u00b7te", "k\u00f6pff", "leh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$(", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}