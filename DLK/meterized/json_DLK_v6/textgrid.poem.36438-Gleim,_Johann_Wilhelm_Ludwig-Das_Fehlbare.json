{"textgrid.poem.36438": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das Fehlbare", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir, Weisheit, bin ich gar nicht gut,", "tokens": ["Dir", ",", "Weis\u00b7heit", ",", "bin", "ich", "gar", "nicht", "gut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du l\u00e4\u00dft mirs oft an Freuden fehlen;", "tokens": ["Du", "l\u00e4\u00dft", "mirs", "oft", "an", "Freu\u00b7den", "feh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn das, was man am liebsten thut,", "tokens": ["Denn", "das", ",", "was", "man", "am", "liebs\u00b7ten", "thut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "PIS", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Willst du sogleich zum B\u00f6sen z\u00e4len.", "tokens": ["Willst", "du", "sog\u00b7leich", "zum", "B\u00f6\u00b7sen", "z\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dich, Fr\u00f6mmigkeit, dich lieb ich zwar,", "tokens": ["Dich", ",", "Fr\u00f6m\u00b7mig\u00b7keit", ",", "dich", "lieb", "ich", "zwar", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch! la\u00df mich auch zuweilen fehlen;", "tokens": ["Doch", "!", "la\u00df", "mich", "auch", "zu\u00b7wei\u00b7len", "feh\u00b7len", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "VVIMP", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will mir ia durchs ganze Jahr", "tokens": ["Ich", "will", "mir", "i\u00b7a", "durchs", "gan\u00b7ze", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Den Fr\u00fcling nur dazu erw\u00e4hlen.", "tokens": ["Den", "Fr\u00fc\u00b7ling", "nur", "da\u00b7zu", "er\u00b7w\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dich, liebes Gl\u00fckke, bet' ich an,", "tokens": ["Dich", ",", "lie\u00b7bes", "Gl\u00fck\u00b7ke", ",", "bet'", "ich", "an", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df mirs an keinem Guten fehlen!", "tokens": ["La\u00df", "mirs", "an", "kei\u00b7nem", "Gu\u00b7ten", "feh\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur la\u00df mich, wenn ichs haben kann,", "tokens": ["Nur", "la\u00df", "mich", ",", "wenn", "ichs", "ha\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$,", "KOUS", "PIS", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mehr Neider, als Dukaten z\u00e4len.", "tokens": ["Mehr", "Nei\u00b7der", ",", "als", "Du\u00b7ka\u00b7ten", "z\u00e4\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn du mir, Liebe, g\u00fcnstig bist,", "tokens": ["Wenn", "du", "mir", ",", "Lie\u00b7be", ",", "g\u00fcns\u00b7tig", "bist", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "NN", "$,", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So la\u00df mir nicht Brunetten fehlen;", "tokens": ["So", "la\u00df", "mir", "nicht", "Bru\u00b7net\u00b7ten", "feh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ich Brunetten gnug gek\u00fc\u00dft,", "tokens": ["Wenn", "ich", "Bru\u00b7net\u00b7ten", "gnug", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So will ich denn Blondinen w\u00e4hlen.", "tokens": ["So", "will", "ich", "denn", "Blon\u00b7di\u00b7nen", "w\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es fehlet mir des Ehstands Quaal,", "tokens": ["Es", "feh\u00b7let", "mir", "des", "Eh\u00b7stands", "Qua\u00b7al", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ach m\u00f6chte sie nur immer fehlen;", "tokens": ["Ach", "m\u00f6ch\u00b7te", "sie", "nur", "im\u00b7mer", "feh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So k\u00f6nnt ich einst zur frohen Zahl", "tokens": ["So", "k\u00f6nnt", "ich", "einst", "zur", "fro\u00b7hen", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tage auch die N\u00e4chte z\u00e4len.", "tokens": ["Der", "Ta\u00b7ge", "auch", "die", "N\u00e4ch\u00b7te", "z\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach seeliger Anakreon,", "tokens": ["Ach", "see\u00b7li\u00b7ger", "A\u00b7nak\u00b7re\u00b7on", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach da\u00df uns deine Zeiten fehlen!", "tokens": ["Ach", "da\u00df", "uns", "dei\u00b7ne", "Zei\u00b7ten", "feh\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Vorteil w\u00fcnscht ich mir davon,", "tokens": ["Den", "Vor\u00b7teil", "w\u00fcnscht", "ich", "mir", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du solltest mir ein M\u00e4dchen w\u00e4hlen.", "tokens": ["Du", "soll\u00b7test", "mir", "ein", "M\u00e4d\u00b7chen", "w\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch! wenn gleich dieser Kenner fehlt,", "tokens": ["Doch", "!", "wenn", "gleich", "die\u00b7ser", "Ken\u00b7ner", "fehlt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "KOUS", "ADV", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So soll mir doch die Wahl nicht fehlen.", "tokens": ["So", "soll", "mir", "doch", "die", "Wahl", "nicht", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Freund, der i\u00fcngst f\u00fcr sich gew\u00e4hlt,", "tokens": ["Mein", "Freund", ",", "der", "i\u00b7\u00fcngst", "f\u00fcr", "sich", "ge\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "APPR", "PRF", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Soll auch f\u00fcr mich ein M\u00e4dchen w\u00e4hlen.", "tokens": ["Soll", "auch", "f\u00fcr", "mich", "ein", "M\u00e4d\u00b7chen", "w\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}