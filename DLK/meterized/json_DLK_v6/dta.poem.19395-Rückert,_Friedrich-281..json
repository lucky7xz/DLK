{"dta.poem.19395": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "281.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df mit Unth\u00e4tigkeit ist Ueberdru\u00df verbunden,", "tokens": ["Da\u00df", "mit", "Un\u00b7th\u00e4\u00b7tig\u00b7keit", "ist", "Ue\u00b7berd\u00b7ru\u00df", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und nur in Th\u00e4tigkeit die Ruhe wird empfunden;", "tokens": ["Und", "nur", "in", "Th\u00e4\u00b7tig\u00b7keit", "die", "Ru\u00b7he", "wird", "emp\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dis, was ein Gr\u00e4mlicher h\u00e4lt f\u00fcr der Menschheit Fluch,", "tokens": ["Dis", ",", "was", "ein", "Gr\u00e4m\u00b7li\u00b7cher", "h\u00e4lt", "f\u00fcr", "der", "Menschheit", "Fluch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Erkl\u00e4rt ein Heiterer f\u00fcr weisen G\u00f6tterspruch.", "tokens": ["Er\u00b7kl\u00e4rt", "ein", "Hei\u00b7te\u00b7rer", "f\u00fcr", "wei\u00b7sen", "G\u00f6t\u00b7ter\u00b7spruch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn jener sagt: es ist der alten S\u00fcnd' Ererbnis,", "tokens": ["Wenn", "je\u00b7ner", "sagt", ":", "es", "ist", "der", "al\u00b7ten", "S\u00fcnd'", "Er\u00b7erb\u00b7nis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df unbefriedigt sich der Mensch f\u00fchlt in Verderbnis;", "tokens": ["Da\u00df", "un\u00b7be\u00b7frie\u00b7digt", "sich", "der", "Mensch", "f\u00fchlt", "in", "Ver\u00b7derb\u00b7nis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PRF", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sagt dieser ihm darauf: es ist des Himmels F\u00fcgung,", "tokens": ["Sagt", "die\u00b7ser", "ihm", "da\u00b7rauf", ":", "es", "ist", "des", "Him\u00b7mels", "F\u00fc\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "PPER", "PAV", "$.", "PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ihm zum Wohl der Welt nur Arbeit gibt Vergn\u00fcgung.", "tokens": ["Da\u00df", "ihm", "zum", "Wohl", "der", "Welt", "nur", "Ar\u00b7beit", "gibt", "Ver\u00b7gn\u00fc\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "ADV", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}