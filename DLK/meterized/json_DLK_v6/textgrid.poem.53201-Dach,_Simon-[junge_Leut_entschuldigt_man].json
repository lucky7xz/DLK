{"textgrid.poem.53201": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[junge Leut entschuldigt man]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Junge Leut entschuldigt man,", "tokens": ["Jun\u00b7ge", "Leut", "ent\u00b7schul\u00b7digt", "man", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PIS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieb und Lust steht ihnen an", "tokens": ["Lieb", "und", "Lust", "steht", "ih\u00b7nen", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie dem Gold ein Demantstein,", "tokens": ["Wie", "dem", "Gold", "ein", "De\u00b7mant\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die S\u00fcssigheit dem Wein,", "tokens": ["Wie", "die", "S\u00fcs\u00b7sig\u00b7heit", "dem", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie dem Felde Gras und Kraut,", "tokens": ["Wie", "dem", "Fel\u00b7de", "Gras", "und", "Kraut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie ein sch\u00f6nes Kleid der Braut,", "tokens": ["Wie", "ein", "sch\u00f6\u00b7nes", "Kleid", "der", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie dem Held ein freyer Muht", "tokens": ["Wie", "dem", "Held", "ein", "frey\u00b7er", "Muht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Vnd ein Federpusch dem Hutt.", "tokens": ["Vnd", "ein", "Fe\u00b7der\u00b7pusch", "dem", "Hutt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob die Zeit weint oder lacht,", "tokens": ["Ob", "die", "Zeit", "weint", "o\u00b7der", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wa\u00df Gestirn vnd Himmel macht,", "tokens": ["Wa\u00df", "Ge\u00b7stirn", "vnd", "Him\u00b7mel", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob sich rottet alle Welt,", "tokens": ["Ob", "sich", "rot\u00b7tet", "al\u00b7le", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wa\u00df das Korn im Lande gelt,", "tokens": ["Wa\u00df", "das", "Korn", "im", "Lan\u00b7de", "gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wa\u00df der Alten Vrtheil spricht,", "tokens": ["Wa\u00df", "der", "Al\u00b7ten", "Vrtheil", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Darnach fragt die Jugend nicht,", "tokens": ["Dar\u00b7nach", "fragt", "die", "Ju\u00b7gend", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sondern liebt vnd frewt sich satt,", "tokens": ["Son\u00b7dern", "liebt", "vnd", "frewt", "sich", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn sie Fug vnd Mittel hat.", "tokens": ["Wenn", "sie", "Fug", "vnd", "Mit\u00b7tel", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schilt sie wer in diesem St\u00fcck,", "tokens": ["Schilt", "sie", "wer", "in", "die\u00b7sem", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PWS", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der gedencke doch zur\u00fcck,", "tokens": ["Der", "ge\u00b7den\u00b7cke", "doch", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob er jung ein faules Bley", "tokens": ["Ob", "er", "jung", "ein", "fau\u00b7les", "Bley"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder Klotz gewesen sey,", "tokens": ["O\u00b7der", "Klotz", "ge\u00b7we\u00b7sen", "sey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer ihr Schertz vnd Liebe wehrt", "tokens": ["Wer", "ihr", "Schertz", "vnd", "Lie\u00b7be", "wehrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist in der Natur verkehrt,", "tokens": ["Ist", "in", "der", "Na\u00b7tur", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Welche steiff in aller Welt", "tokens": ["Wel\u00b7che", "steiff", "in", "al\u00b7ler", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Vber solcher Satzung h\u00e4lt.", "tokens": ["Vber", "sol\u00b7cher", "Sat\u00b7zung", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Weicht sie au\u00df der Vnschuld nicht", "tokens": ["Weicht", "sie", "au\u00df", "der", "Vn\u00b7schuld", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnd der Zucht giebt ihre Pflicht,", "tokens": ["Vnd", "der", "Zucht", "giebt", "ih\u00b7re", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat man ihr es zu gestehn,", "tokens": ["Hat", "man", "ihr", "es", "zu", "ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPER", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kan sie auff dem Kopff auch gehn.", "tokens": ["Kan", "sie", "auff", "dem", "Kopff", "auch", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wa\u00df die\u00df kurtze Leben ziehrt,", "tokens": ["Wa\u00df", "die\u00df", "kurt\u00b7ze", "Le\u00b7ben", "ziehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sorg vnd Furcht von hinnen f\u00fchrt,", "tokens": ["Sorg", "vnd", "Furcht", "von", "hin\u00b7nen", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seiten, Tantz, Gelach vnd Wein,", "tokens": ["Sei\u00b7ten", ",", "Tantz", ",", "Ge\u00b7lach", "vnd", "Wein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Scheint ihr Eigenthumb zu seyn.", "tokens": ["Scheint", "ihr", "Ei\u00b7gen\u00b7thumb", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}