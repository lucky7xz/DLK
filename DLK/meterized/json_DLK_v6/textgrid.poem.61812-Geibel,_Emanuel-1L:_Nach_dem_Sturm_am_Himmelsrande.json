{"textgrid.poem.61812": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nach dem Sturm am Himmelsrande", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach dem Sturm am Himmelsrande", "tokens": ["Nach", "dem", "Sturm", "am", "Him\u00b7mels\u00b7ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwebt der Mond um Mitternacht;", "tokens": ["Schwebt", "der", "Mond", "um", "Mit\u00b7ter\u00b7nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Langsam, schimmernd her zum Strande", "tokens": ["Lang\u00b7sam", ",", "schim\u00b7mernd", "her", "zum", "Stran\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rollt die Flut und brandet sacht.", "tokens": ["Rollt", "die", "Flut", "und", "bran\u00b7det", "sacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihre dumpfen Schl\u00e4ge mahnen", "tokens": ["Ih\u00b7re", "dum\u00b7pfen", "Schl\u00e4\u00b7ge", "mah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An ein Herz, das m\u00fcde pocht;", "tokens": ["An", "ein", "Herz", ",", "das", "m\u00fc\u00b7de", "pocht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine Spur mehr l\u00e4\u00dft dich ahnen,", "tokens": ["Kei\u00b7ne", "Spur", "mehr", "l\u00e4\u00dft", "dich", "ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welch ein Chaos hier gekocht.", "tokens": ["Welch", "ein", "Chaos", "hier", "ge\u00b7kocht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "ADV", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Sagt, wohin dies wilde Schwellen", "tokens": ["Sagt", ",", "wo\u00b7hin", "dies", "wil\u00b7de", "Schwel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "PDS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jauchzender Titanenlust? \u2013", "tokens": ["Jauch\u00b7zen\u00b7der", "Ti\u00b7ta\u00b7nen\u00b7lust", "?", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer begreift euch, Meereswellen?", "tokens": ["Wer", "be\u00b7greift", "euch", ",", "Mee\u00b7res\u00b7wel\u00b7len", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer begreift dich, Menschenbrust?", "tokens": ["Wer", "be\u00b7greift", "dich", ",", "Men\u00b7schen\u00b7brust", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}