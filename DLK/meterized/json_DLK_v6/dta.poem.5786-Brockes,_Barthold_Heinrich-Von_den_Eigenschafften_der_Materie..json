{"dta.poem.5786": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein, sprach Epieur; wo soll das Wesen hin,", "tokens": ["Al\u00b7lein", ",", "sprach", "E\u00b7pie\u00b7ur", ";", "wo", "soll", "das", "We\u00b7sen", "hin", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "NN", "$.", "PWAV", "VMFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das euer Fisch zertheilt und dr\u00fcckt?", "tokens": ["Das", "eu\u00b7er", "Fisch", "zer\u00b7theilt", "und", "dr\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist es durch einen Leib zu dringen wol geschickt,", "tokens": ["Ist", "es", "durch", "ei\u00b7nen", "Leib", "zu", "drin\u00b7gen", "wol", "ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "PTKZU", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn ihm blos durch das Leere", "tokens": ["Wenn", "ihm", "blos", "durch", "das", "Lee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Der Durchgang nicht gestattet w\u00e4re?", "tokens": ["Der", "Durch\u00b7gang", "nicht", "ge\u00b7stat\u00b7tet", "w\u00e4\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}