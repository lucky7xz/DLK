{"textgrid.poem.46463": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Weltrad saust,", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.85", "et:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Weltrad saust,", "tokens": ["Das", "Welt\u00b7rad", "saust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ich sause mit!", "tokens": ["Ich", "sau\u00b7se", "mit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es sch\u00fcttert, schleudert, rast, braust", "tokens": ["Es", "sch\u00fct\u00b7tert", ",", "schleu\u00b7dert", ",", "rast", ",", "braust"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Pfeifendschrill \u2013", "tokens": ["Pfei\u00b7fend\u00b7schrill", "\u2013"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ich schleudere, rase, brause mit", "tokens": ["Ich", "schleu\u00b7de\u00b7re", ",", "ra\u00b7se", ",", "brau\u00b7se", "mit"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "APPR"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Weil ich will! weil ich will!", "tokens": ["Weil", "ich", "will", "!", "weil", "ich", "will", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "$.", "KOUS", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich geh t\u00e4glich meine m\u00fchsamen Schritte,", "tokens": ["Ich", "geh", "t\u00e4g\u00b7lich", "mei\u00b7ne", "m\u00fch\u00b7sa\u00b7men", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Doch \u2013 zu wirbelndem Fluge", "tokens": ["Doch", "\u2013", "zu", "wir\u00b7beln\u00b7dem", "Flu\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$(", "APPR", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Im Zeit-Zuge", "tokens": ["Im", "Zeit\u00b7Zu\u00b7ge"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Rei\u00dft mich des Weltrades Kraftmitte", "tokens": ["Rei\u00dft", "mich", "des", "Welt\u00b7ra\u00b7des", "Kraft\u00b7mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN"], "meter": "+--+--++-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Vorw\u00e4rts!", "tokens": ["Vor\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Das Weltradsausen singt,", "tokens": ["Das", "Welt\u00b7rads\u00b7au\u00b7sen", "singt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der unaufh\u00f6rlich gro\u00dfe Ton bezwingt", "tokens": ["Der", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "gro\u00b7\u00dfe", "Ton", "be\u00b7zwingt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mich in den Rasekreis:", "tokens": ["Mich", "in", "den", "Ra\u00b7se\u00b7kreis", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das ist mein Schicksalsbeschlu\u00df,", "tokens": ["Das", "ist", "mein", "Schick\u00b7sals\u00b7be\u00b7schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das ist alles, was ich wei\u00df:", "tokens": ["Das", "ist", "al\u00b7les", ",", "was", "ich", "wei\u00df", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ich mitsausen,", "tokens": ["Da\u00df", "ich", "mit\u00b7sau\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Da\u00df ich mitbrausen", "tokens": ["Da\u00df", "ich", "mit\u00b7brau\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Mu\u00df!", "tokens": ["Mu\u00df", "!"], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "+", "measure": "single.up"}}}}}