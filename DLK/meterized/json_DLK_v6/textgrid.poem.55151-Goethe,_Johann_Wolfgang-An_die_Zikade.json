{"textgrid.poem.55151": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "An die Zikade", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selig bist du, liebe Kleine,", "tokens": ["Se\u00b7lig", "bist", "du", ",", "lie\u00b7be", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "VVFIN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die du auf der B\u00e4ume Zweigen,", "tokens": ["Die", "du", "auf", "der", "B\u00e4u\u00b7me", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von geringem Trank begeistert,", "tokens": ["Von", "ge\u00b7rin\u00b7gem", "Trank", "be\u00b7geis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Singend, wie ein K\u00f6nig lebest!", "tokens": ["Sin\u00b7gend", ",", "wie", "ein", "K\u00f6\u00b7nig", "le\u00b7best", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dir geh\u00f6ret eigen alles,", "tokens": ["Dir", "ge\u00b7h\u00f6\u00b7ret", "ei\u00b7gen", "al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was du auf den Feldern siehest,", "tokens": ["Was", "du", "auf", "den", "Fel\u00b7dern", "sie\u00b7hest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Alles, was die Stunden bringen;", "tokens": ["Al\u00b7les", ",", "was", "die", "Stun\u00b7den", "brin\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lebest unter Ackersleuten,", "tokens": ["Le\u00b7best", "un\u00b7ter", "A\u00b7ckers\u00b7leu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ihre Freundin, unbesch\u00e4digt,", "tokens": ["Ih\u00b7re", "Freun\u00b7din", ",", "un\u00b7be\u00b7sch\u00e4\u00b7digt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Du den Sterblichen Verehrte,", "tokens": ["Du", "den", "Sterb\u00b7li\u00b7chen", "Ver\u00b7ehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "S\u00fc\u00dfen Fr\u00fchlings s\u00fc\u00dfer Bote!", "tokens": ["S\u00fc\u00b7\u00dfen", "Fr\u00fch\u00b7lings", "s\u00fc\u00b7\u00dfer", "Bo\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Ja, dich lieben alle Musen,", "tokens": ["Ja", ",", "dich", "lie\u00b7ben", "al\u00b7le", "Mu\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Ph\u00f6bus selber mu\u00df dich lieben,", "tokens": ["Ph\u00f6\u00b7bus", "sel\u00b7ber", "mu\u00df", "dich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Gaben dir die Silberstimme,", "tokens": ["Ga\u00b7ben", "dir", "die", "Sil\u00b7ber\u00b7stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Dich ergreifet nie das Alter,", "tokens": ["Dich", "er\u00b7grei\u00b7fet", "nie", "das", "Al\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Weise, Zarte, Dichterfreundin,", "tokens": ["Wei\u00b7se", ",", "Zar\u00b7te", ",", "Dich\u00b7ter\u00b7freun\u00b7din", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Ohne Fleisch und Blut Geborne,", "tokens": ["Oh\u00b7ne", "Fleisch", "und", "Blut", "Ge\u00b7bor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Leidenlose Erdentochter,", "tokens": ["Lei\u00b7den\u00b7lo\u00b7se", "Er\u00b7den\u00b7toch\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Fast den G\u00f6ttern zu vergleichen.", "tokens": ["Fast", "den", "G\u00f6t\u00b7tern", "zu", "ver\u00b7glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}