{"textgrid.poem.31508": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach du aller Wunder Wunder / und des H\u00f6chsten h\u00f6chste That!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach du aller Wunder Wunder / und des H\u00f6chsten h\u00f6chste That!", "tokens": ["Ach", "du", "al\u00b7ler", "Wun\u00b7der", "Wun\u00b7der", "/", "und", "des", "H\u00f6chs\u00b7ten", "h\u00f6chs\u00b7te", "That", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "PIAT", "NN", "NN", "$(", "KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "G\u00fctigkeit / von der sich selbst dieser l\u00e4sset \u00fcberwinden /", "tokens": ["G\u00fc\u00b7tig\u00b7keit", "/", "von", "der", "sich", "selbst", "die\u00b7ser", "l\u00e4s\u00b7set", "\u00fc\u00b7berw\u00b7in\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "APPR", "PRELS", "PRF", "ADV", "PDS", "VVFIN", "VVINF", "$("], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "der Vn\u00fcberwindlich sonst. S\u00fcnd / und Noht und Tod verschwinden /", "tokens": ["der", "Vn\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "sonst", ".", "S\u00fcnd", "/", "und", "Noht", "und", "Tod", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$.", "NN", "$(", "KON", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "wann mit Herzen-s\u00fcssem fliessen / du erweichest Gottes Raht.", "tokens": ["wann", "mit", "Her\u00b7zen\u00b7s\u00fcs\u00b7sem", "flies\u00b7sen", "/", "du", "er\u00b7wei\u00b7chest", "Got\u00b7tes", "Raht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVINF", "$(", "PPER", "VVFIN", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Du hast st\u00e4ts in Gottes Herze die Allherrschend' Oberstatt.", "tokens": ["Du", "hast", "st\u00e4ts", "in", "Got\u00b7tes", "Her\u00b7ze", "die", "All\u00b7herr\u00b7schend'", "O\u00b7bers\u00b7tatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "VVFIN", "ART", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Nichts ist in der G\u00f6ttlichkeit / das dich \u00fcbertrifft / zufinden.", "tokens": ["Nichts", "ist", "in", "der", "G\u00f6tt\u00b7lich\u00b7keit", "/", "das", "dich", "\u00fc\u00b7bert\u00b7rifft", "/", "zu\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$(", "ADJD", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Du kanst Gottes Allmacht-Hand / schutzend st\u00e4rken / straffend binden.", "tokens": ["Du", "kanst", "Got\u00b7tes", "All\u00b7macht\u00b7Hand", "/", "schut\u00b7zend", "st\u00e4r\u00b7ken", "/", "straf\u00b7fend", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "NN", "$(", "ADJD", "VVINF", "$(", "ADJD", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Die gesamte Menschen-Wolfart aus dir ihren Vrsprung hat.", "tokens": ["Die", "ge\u00b7sam\u00b7te", "Men\u00b7schen\u00b7Wolf\u00b7art", "aus", "dir", "ih\u00b7ren", "Vr\u00b7sprung", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Du vermenschest Gott / O G\u00fcte! da\u00df der Mensch unsterblich wird:", "tokens": ["Du", "ver\u00b7men\u00b7schest", "Gott", "/", "O", "G\u00fc\u00b7te", "!", "da\u00df", "der", "Mensch", "uns\u00b7terb\u00b7lich", "wird", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$(", "NE", "NN", "$.", "KOUS", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "ja willst selbst / dein Gegentheil / Grausamkeit / an Gott ver\u00fcben;", "tokens": ["ja", "willst", "selbst", "/", "dein", "Ge\u00b7gen\u00b7theil", "/", "Grau\u00b7sam\u00b7keit", "/", "an", "Gott", "ver\u00b7\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "$(", "PPOSAT", "NN", "$(", "NN", "$(", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.11": {"text": "da\u00df du nur an uns erf\u00fcllest deine s\u00fcsse Lieb-Begird.", "tokens": ["da\u00df", "du", "nur", "an", "uns", "er\u00b7f\u00fcl\u00b7lest", "dei\u00b7ne", "s\u00fcs\u00b7se", "Lie\u00b7bBe\u00b7gird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Alles / ja Gott selbst / du gibst: uns nur Ewig Treu zu lieben.", "tokens": ["Al\u00b7les", "/", "ja", "Gott", "selbst", "/", "du", "gibst", ":", "uns", "nur", "E\u00b7wig", "Treu", "zu", "lie\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$(", "ADV", "NN", "ADV", "$(", "PPER", "VVFIN", "$.", "PPER", "ADV", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.13": {"text": "Guter Gott und Gottes G\u00fcte! meine Schrifft erreicht dich nicht:", "tokens": ["Gu\u00b7ter", "Gott", "und", "Got\u00b7tes", "G\u00fc\u00b7te", "!", "mei\u00b7ne", "Schrifft", "er\u00b7reicht", "dich", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "NN", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "mit von Lieb verzuckten schweigen deinen Ruhm man mehr ausspricht.", "tokens": ["mit", "von", "Lieb", "ver\u00b7zuck\u00b7ten", "schwei\u00b7gen", "dei\u00b7nen", "Ruhm", "man", "mehr", "aus\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "VVFIN", "VVFIN", "PPOSAT", "NN", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}