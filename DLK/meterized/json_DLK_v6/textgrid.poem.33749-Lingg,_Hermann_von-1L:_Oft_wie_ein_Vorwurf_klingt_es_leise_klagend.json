{"textgrid.poem.33749": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oft wie ein Vorwurf klingt es leise klagend", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft wie ein Vorwurf klingt es leise klagend", "tokens": ["Oft", "wie", "ein", "Vor\u00b7wurf", "klingt", "es", "lei\u00b7se", "kla\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir in die Seele tief von deinem Munde;", "tokens": ["Mir", "in", "die", "See\u00b7le", "tief", "von", "dei\u00b7nem", "Mun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich wei\u00df es ja, ich h\u00e4tte meine Wunde", "tokens": ["Ich", "wei\u00df", "es", "ja", ",", "ich", "h\u00e4t\u00b7te", "mei\u00b7ne", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verschlie\u00dfen sollen, standhaft dir entsagend.", "tokens": ["Ver\u00b7schlie\u00b7\u00dfen", "sol\u00b7len", ",", "stand\u00b7haft", "dir", "ent\u00b7sa\u00b7gend", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich durfte, statt so k\u00fchne Tr\u00e4ume wagend,", "tokens": ["Ich", "durf\u00b7te", ",", "statt", "so", "k\u00fch\u00b7ne", "Tr\u00e4u\u00b7me", "wa\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUI", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich stumm nur lieben, segnen nur die Stunde,", "tokens": ["Dich", "stumm", "nur", "lie\u00b7ben", ",", "seg\u00b7nen", "nur", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VVINF", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da ich dich fand, doch nie zum inn'gen Bunde", "tokens": ["Da", "ich", "dich", "fand", ",", "doch", "nie", "zum", "inn'\u00b7gen", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Hand dir reichen, streng mein Leid ertragend.", "tokens": ["Die", "Hand", "dir", "rei\u00b7chen", ",", "streng", "mein", "Leid", "er\u00b7tra\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,", "VVFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O wirf sie, wenn dich meine Kr\u00e4nze dr\u00fccken,", "tokens": ["O", "wirf", "sie", ",", "wenn", "dich", "mei\u00b7ne", "Kr\u00e4n\u00b7ze", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wirf sie von dir, verbiet mir, untersage,", "tokens": ["Wirf", "sie", "von", "dir", ",", "ver\u00b7biet", "mir", ",", "un\u00b7ter\u00b7sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "$,", "VVFIN", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn dich es schmerzt, mein frevelhaft Entz\u00fccken!", "tokens": ["Wenn", "dich", "es", "schmerzt", ",", "mein", "fre\u00b7vel\u00b7haft", "Ent\u00b7z\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Anstatt auf Fl\u00fcgeln dich emporzutragen,", "tokens": ["An\u00b7statt", "auf", "Fl\u00fc\u00b7geln", "dich", "em\u00b7por\u00b7zu\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Will ich mit einem Trauerflor dich schm\u00fccken,", "tokens": ["Will", "ich", "mit", "ei\u00b7nem", "Trau\u00b7er\u00b7flor", "dich", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn nur nicht deine Blicke mich verklagen.", "tokens": ["Wenn", "nur", "nicht", "dei\u00b7ne", "Bli\u00b7cke", "mich", "ver\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKNEG", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}