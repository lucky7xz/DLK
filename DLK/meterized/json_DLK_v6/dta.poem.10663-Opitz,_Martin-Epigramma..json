{"dta.poem.10663": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist dein sch\u00f6ner Leib/ du schn\u00f6de blinde Jugendt/", "tokens": ["Was", "ist", "dein", "sch\u00f6\u00b7ner", "Leib", "/", "du", "schn\u00f6\u00b7de", "blin\u00b7de", "Ju\u00b7gendt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "ADJA", "NN", "$(", "PPER", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Wann er nit ist begabt mit Zier der Zucht vnd Tugendt:", "tokens": ["Wann", "er", "nit", "ist", "be\u00b7gabt", "mit", "Zier", "der", "Zucht", "vnd", "Tu\u00b7gendt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "VAFIN", "ADJD", "APPR", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Die hellen Aeugelein/ ein finster b\u00f6ser lust/", "tokens": ["Die", "hel\u00b7len", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", "/", "ein", "fins\u00b7ter", "b\u00f6\u00b7ser", "lust", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Der Leib ist eine Kist erf\u00fcllt mit Koth vnd Wust.", "tokens": ["Der", "Leib", "ist", "ei\u00b7ne", "Kist", "er\u00b7f\u00fcllt", "mit", "Koth", "vnd", "Wust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Mund/ ein Thor/ darau\u00df sich alle Laster finden/", "tokens": ["Der", "Mund", "/", "ein", "Thor", "/", "dar\u00b7au\u00df", "sich", "al\u00b7le", "Las\u00b7ter", "fin\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "PAV", "PRF", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der zarten Br\u00fcste quell/ ein Bronn der Schand vnd S\u00fcnden:", "tokens": ["Der", "zar\u00b7ten", "Br\u00fcs\u00b7te", "quell", "/", "ein", "Bronn", "der", "Schand", "vnd", "S\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$(", "ART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Freuden port/ die scho\u00df/ ein Grab der Vppigkeit/", "tokens": ["Der", "Freu\u00b7den", "port", "/", "die", "scho\u00df", "/", "ein", "Grab", "der", "Vp\u00b7pig\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "ART", "VVFIN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vnd Wahlstatt da die Ehr ist blieben in dem Streit.", "tokens": ["Vnd", "Wahl\u00b7statt", "da", "die", "Ehr", "ist", "blie\u00b7ben", "in", "dem", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ART", "NN", "VAFIN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wo aber Tugend sich bey der Zierligkeit erzeiget/", "tokens": ["Wo", "a\u00b7ber", "Tu\u00b7gend", "sich", "bey", "der", "Zier\u00b7lig\u00b7keit", "er\u00b7zei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Ist wie wenn eine Blum durchs Wasser sich ere\u00fcget:", "tokens": ["Ist", "wie", "wenn", "ei\u00b7ne", "Blum", "durchs", "Was\u00b7ser", "sich", "er\u00b7e\u00fc\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "KOUS", "ART", "NE", "APPRART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da stehet alles wol/ da sich es lustig au\u00df/", "tokens": ["Da", "ste\u00b7het", "al\u00b7les", "wol", "/", "da", "sich", "es", "lus\u00b7tig", "au\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$(", "KOUS", "PRF", "PPER", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da ist ein sch\u00f6ner Wirt/ da ist ein sch\u00f6nes Hau\u00df.", "tokens": ["Da", "ist", "ein", "sch\u00f6\u00b7ner", "Wirt", "/", "da", "ist", "ein", "sch\u00f6\u00b7nes", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$(", "ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}