{"dta.poem.23776": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Gehorsam.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Hundchen ist ein gutes Thier,", "tokens": ["Mein", "Hund\u00b7chen", "ist", "ein", "gu\u00b7tes", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So bald ich rufe, folgt er mir,", "tokens": ["So", "bald", "ich", "ru\u00b7fe", ",", "folgt", "er", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch k\u00f6mmt er nicht, wenn ichs ihm sage,", "tokens": ["Doch", "k\u00f6mmt", "er", "nicht", ",", "wenn", "ichs", "ihm", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ist er werth, da\u00df ich ihn schlage.", "tokens": ["So", "ist", "er", "werth", ",", "da\u00df", "ich", "ihn", "schla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bestrafet mich mein Vater nun,", "tokens": ["Be\u00b7stra\u00b7fet", "mich", "mein", "Va\u00b7ter", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will ich nicht seinen Willen thun,", "tokens": ["Will", "ich", "nicht", "sei\u00b7nen", "Wil\u00b7len", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Darf ich es denn so \u00fcbel nehmen? \u2014", "tokens": ["Darf", "ich", "es", "denn", "so", "\u00fc\u00b7bel", "neh\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich w\u00fcrde ja mein Hund besch\u00e4men.", "tokens": ["Mich", "w\u00fcr\u00b7de", "ja", "mein", "Hund", "be\u00b7sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}