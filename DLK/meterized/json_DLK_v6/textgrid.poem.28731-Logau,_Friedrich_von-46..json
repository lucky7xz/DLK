{"textgrid.poem.28731": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt, die gerne lag;", "tokens": ["Hier", "liegt", ",", "die", "ger\u00b7ne", "lag", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hat immer Nacht f\u00fcr Tag,", "tokens": ["Hat", "im\u00b7mer", "Nacht", "f\u00fcr", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil als der Tag die Nacht", "tokens": ["Weil", "als", "der", "Tag", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ihr mehr Belieben bracht.", "tokens": ["Ihr", "mehr", "Be\u00b7lie\u00b7ben", "bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nur di\u00df ist ihr Beschwer:", "tokens": ["Nur", "di\u00df", "ist", "ihr", "Be\u00b7schwer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Die Armen sind ihr leer.", "tokens": ["Die", "Ar\u00b7men", "sind", "ihr", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Tod liegt ihr am Arm", "tokens": ["Der", "Tod", "liegt", "ihr", "am", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und macht ihr doch nicht warm;", "tokens": ["Und", "macht", "ihr", "doch", "nicht", "warm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Die so geliebte Scho\u00df", "tokens": ["Die", "so", "ge\u00b7lieb\u00b7te", "Scho\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Deckt ietzt ein Erdenklo\u00df.", "tokens": ["Deckt", "ietzt", "ein", "Er\u00b7den\u00b7klo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}