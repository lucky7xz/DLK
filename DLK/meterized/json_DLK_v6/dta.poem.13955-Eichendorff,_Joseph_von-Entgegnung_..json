{"dta.poem.13955": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Entgegnung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Oft durch des Theaters Ritzen", "tokens": ["Oft", "durch", "des", "The\u00b7a\u00b7ters", "Rit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bricht's mit wunderbarem Lichte,", "tokens": ["Bricht's", "mit", "wun\u00b7der\u00b7ba\u00b7rem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn der Herr in feur'gen Blitzen", "tokens": ["Wenn", "der", "Herr", "in", "feur'\u00b7gen", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dichtend schreibt die Weltgeschichte,", "tokens": ["Dich\u00b7tend", "schreibt", "die", "Welt\u00b7ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und das ist der Klang der Wehmuth,", "tokens": ["Und", "das", "ist", "der", "Klang", "der", "Weh\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der durch alle Dichter-Geister", "tokens": ["Der", "durch", "al\u00b7le", "Dich\u00b7ter\u00b7Geis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schauernd geht, wenn sie in Demuth", "tokens": ["Schau\u00b7ernd", "geht", ",", "wenn", "sie", "in", "De\u00b7muth"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Ueber sich erkannt den Meister.", "tokens": ["Ue\u00b7ber", "sich", "er\u00b7kannt", "den", "Meis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVPP", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}