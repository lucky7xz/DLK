{"textgrid.poem.41575": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am vierundzwanzigsten Sonntage", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gebt Gott sein Recht und gebt's dem Kaiser auch!", "tokens": ["Gebt", "Gott", "sein", "Recht", "und", "gebt's", "dem", "Kai\u00b7ser", "auch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PPOSAT", "NN", "KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein Odem ist's, der um den Obern schwebet;", "tokens": ["Sein", "O\u00b7dem", "ist's", ",", "der", "um", "den", "O\u00b7bern", "schwe\u00b7bet", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus Hochmut nicht, in Eigenwillen hebet", "tokens": ["Aus", "Hoch\u00b7mut", "nicht", ",", "in", "Ei\u00b7gen\u00b7wil\u00b7len", "he\u00b7bet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "PTKNEG", "$,", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht eure Rechte gen geweihten Brauch.", "tokens": ["Nicht", "eu\u00b7re", "Rech\u00b7te", "gen", "ge\u00b7weih\u00b7ten", "Brauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch Gott und Welt im Streit: da Br\u00fcder gebet", "tokens": ["Doch", "Gott", "und", "Welt", "im", "Streit", ":", "da", "Br\u00fc\u00b7der", "ge\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "APPRART", "NN", "$.", "KOUS", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nicht mehr auf Kaiserwort als Dunst und Rauch.", "tokens": ["Nicht", "mehr", "auf", "Kai\u00b7ser\u00b7wort", "als", "Dunst", "und", "Rauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er ist der Oberste, dem alle Macht", "tokens": ["Er", "ist", "der", "O\u00b7bers\u00b7te", ",", "dem", "al\u00b7le", "Macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Zusammenbricht, wie d\u00fcrres Reisig kracht.", "tokens": ["Zu\u00b7sam\u00b7men\u00b7bricht", ",", "wie", "d\u00fcr\u00b7res", "Rei\u00b7sig", "kracht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Den Eltern gib, und gib auch Gott sein Recht!", "tokens": ["Den", "El\u00b7tern", "gib", ",", "und", "gib", "auch", "Gott", "sein", "Recht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "$,", "KON", "VVIMP", "ADV", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O weh des tief Gesunknen, dem verloren", "tokens": ["O", "weh", "des", "tief", "Ge\u00b7sun\u00b7knen", ",", "dem", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "ADV", "ART", "ADJD", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der frommste Trieb, jedwedem angeboren,", "tokens": ["Der", "fromms\u00b7te", "Trieb", ",", "jed\u00b7we\u00b7dem", "an\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Freisten stempelnd zum begl\u00fcckten Knecht.", "tokens": ["Den", "Freis\u00b7ten", "stem\u00b7pelnd", "zum", "be\u00b7gl\u00fcck\u00b7ten", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch stell den W\u00e4chter an der Ehrfurcht Toren", "tokens": ["Doch", "stell", "den", "W\u00e4ch\u00b7ter", "an", "der", "Ehr\u00b7furcht", "To\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NE", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und halte das Gewissen rein und echt;", "tokens": ["Und", "hal\u00b7te", "das", "Ge\u00b7wis\u00b7sen", "rein", "und", "echt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er ist der Vater, dem du Seel' und Leib", "tokens": ["Er", "ist", "der", "Va\u00b7ter", ",", "dem", "du", "Seel'", "und", "Leib"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Verschuldest mehr als irgend Mann und Weib.", "tokens": ["Ver\u00b7schul\u00b7dest", "mehr", "als", "ir\u00b7gend", "Mann", "und", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Den Gatten lieb und denk an Gott dabei!", "tokens": ["Den", "Gat\u00b7ten", "lieb", "und", "denk", "an", "Gott", "da\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVFIN", "APPR", "NN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er gab den Segen dir, als am Altare", "tokens": ["Er", "gab", "den", "Se\u00b7gen", "dir", ",", "als", "am", "Al\u00b7ta\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den Eid du sprachst, gewaltig bis zur Bahre", "tokens": ["Den", "Eid", "du", "sprachst", ",", "ge\u00b7wal\u00b7tig", "bis", "zur", "Bah\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "ADJD", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Fesseln legend deine Lieb und Treu.", "tokens": ["In", "Fes\u00b7seln", "le\u00b7gend", "dei\u00b7ne", "Lieb", "und", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch wird die Liebe Torheit, o dann wahre,", "tokens": ["Doch", "wird", "die", "Lie\u00b7be", "Tor\u00b7heit", ",", "o", "dann", "wah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "$,", "FM", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "O halte deine tiefsten Gluten frei!", "tokens": ["O", "hal\u00b7te", "dei\u00b7ne", "tiefs\u00b7ten", "Glu\u00b7ten", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er ist es, dem du einer Flamme Zoll", "tokens": ["Er", "ist", "es", ",", "dem", "du", "ei\u00b7ner", "Flam\u00b7me", "Zoll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mu\u00dft zahlen, die kein Mensch begehren soll.", "tokens": ["Mu\u00dft", "zah\u00b7len", ",", "die", "kein", "Mensch", "be\u00b7geh\u00b7ren", "soll", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "PRELS", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "An deine Kinder h\u00e4nge nur dein Herz,", "tokens": ["An", "dei\u00b7ne", "Kin\u00b7der", "h\u00e4n\u00b7ge", "nur", "dein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In deren Adern rollt dein eignes Leben;", "tokens": ["In", "de\u00b7ren", "A\u00b7dern", "rollt", "dein", "eig\u00b7nes", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Gottesbild, in deine Hand gegeben,", "tokens": ["Das", "Got\u00b7tes\u00b7bild", ",", "in", "dei\u00b7ne", "Hand", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es nicht zu lieben, w\u00e4re herber Schmerz.", "tokens": ["Es", "nicht", "zu", "lie\u00b7ben", ",", "w\u00e4\u00b7re", "her\u00b7ber", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "PTKZU", "VVINF", "$,", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch siehst du zwischen Gl\u00fcck und Schuld es schweben,", "tokens": ["Doch", "siehst", "du", "zwi\u00b7schen", "Gl\u00fcck", "und", "Schuld", "es", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wend deine Augen, sto\u00df es niederw\u00e4rts.", "tokens": ["Wend", "dei\u00b7ne", "Au\u00b7gen", ",", "sto\u00df", "es", "nie\u00b7der\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Er, \u00fcber tausend Kinder lieb und hehr,", "tokens": ["Er", ",", "\u00fc\u00b7ber", "tau\u00b7send", "Kin\u00b7der", "lieb", "und", "hehr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "CARD", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Er sieht dir nach, ist deine Seele schwer.", "tokens": ["Er", "sieht", "dir", "nach", ",", "ist", "dei\u00b7ne", "See\u00b7le", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und auch dem Freunde halte Treue fest,", "tokens": ["Und", "auch", "dem", "Freun\u00b7de", "hal\u00b7te", "Treu\u00b7e", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit der die Ehre innig sich verbunden;", "tokens": ["Mit", "der", "die", "Eh\u00b7re", "in\u00b7nig", "sich", "ver\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein irdisch Gut, was Gnade doch gefunden,", "tokens": ["Ein", "ir\u00b7disch", "Gut", ",", "was", "Gna\u00b7de", "doch", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWS", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Solang es nicht die Hand der Tugend l\u00e4\u00dft,", "tokens": ["So\u00b7lang", "es", "nicht", "die", "Hand", "der", "Tu\u00b7gend", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch nahen gl\u00e4nzender Versuchung Stunden,", "tokens": ["Doch", "na\u00b7hen", "gl\u00e4n\u00b7zen\u00b7der", "Ver\u00b7su\u00b7chung", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dann aller Erdenr\u00fccksicht gib den Rest", "tokens": ["Dann", "al\u00b7ler", "Er\u00b7den\u00b7r\u00fcck\u00b7sicht", "gib", "den", "Rest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VVIMP", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und klammre an den einen dich, der dann", "tokens": ["Und", "klamm\u00b7re", "an", "den", "ei\u00b7nen", "dich", ",", "der", "dann"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ART", "PPER", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dir mehr als Freund und Ehre geben kann.", "tokens": ["Dir", "mehr", "als", "Freund", "und", "Eh\u00b7re", "ge\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KOUS", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So biete jedem, was sein Recht begehrt,", "tokens": ["So", "bie\u00b7te", "je\u00b7dem", ",", "was", "sein", "Recht", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und nimm von jedem, was du darfst empfangen;", "tokens": ["Und", "nimm", "von", "je\u00b7dem", ",", "was", "du", "darfst", "emp\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PIS", "$,", "PWS", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Herz es mag an zarten Banden hangen,", "tokens": ["Dein", "Herz", "es", "mag", "an", "zar\u00b7ten", "Ban\u00b7den", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Gottes Huld so gnadenvoll gew\u00e4hrt;", "tokens": ["Die", "Got\u00b7tes", "Huld", "so", "gna\u00b7den\u00b7voll", "ge\u00b7w\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch dr\u00fcber, wie ein Glutstern, das Verlangen", "tokens": ["Doch", "dr\u00fc\u00b7ber", ",", "wie", "ein", "Gluts\u00b7tern", ",", "das", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PAV", "$,", "PWAV", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nach einem leuchte, irdisch unversehrt,", "tokens": ["Nach", "ei\u00b7nem", "leuch\u00b7te", ",", "ir\u00b7disch", "un\u00b7ver\u00b7sehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nach einem, ohne den dein Herz, so warm,", "tokens": ["Nach", "ei\u00b7nem", ",", "oh\u00b7ne", "den", "dein", "Herz", ",", "so", "warm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "KOUI", "ART", "PPOSAT", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ewig verlassen bliebe doch und arm!", "tokens": ["E\u00b7wig", "ver\u00b7las\u00b7sen", "blie\u00b7be", "doch", "und", "arm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "ADV", "KON", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}