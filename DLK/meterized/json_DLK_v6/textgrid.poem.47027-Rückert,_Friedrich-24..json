{"textgrid.poem.47027": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O da\u00df doch eine Fee den bunten Flitter", "tokens": ["O", "da\u00df", "doch", "ei\u00b7ne", "Fee", "den", "bun\u00b7ten", "Flit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu tausendlei Verwandlungen mir b\u00f6te,", "tokens": ["Zu", "tau\u00b7send\u00b7lei", "Ver\u00b7wand\u00b7lun\u00b7gen", "mir", "b\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Da\u00df mich als blanken Hirt die Morgenr\u00f6te,", "tokens": ["Da\u00df", "mich", "als", "blan\u00b7ken", "Hirt", "die", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Abendrot mich s\u00e4h' als braunen Schnitter;", "tokens": ["Das", "A\u00b7ben\u00b7drot", "mich", "s\u00e4h'", "als", "brau\u00b7nen", "Schnit\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da\u00df ich als Spielmann heute mit der Zither", "tokens": ["Da\u00df", "ich", "als", "Spiel\u00b7mann", "heu\u00b7te", "mit", "der", "Zi\u00b7ther"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOUS", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Land durchz\u00f6g' und morgen mit der Fl\u00f6te,", "tokens": ["Das", "Land", "durch\u00b7z\u00f6g'", "und", "mor\u00b7gen", "mit", "der", "Fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als Weidmann heute meinen Speer erh\u00f6hte", "tokens": ["Als", "Weid\u00b7mann", "heu\u00b7te", "mei\u00b7nen", "Speer", "er\u00b7h\u00f6h\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und morgen mich erh\u00f6ht' aufs Ro\u00df als Ritter.", "tokens": ["Und", "mor\u00b7gen", "mich", "er\u00b7h\u00f6ht'", "aufs", "Ro\u00df", "als", "Rit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "APPRART", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich wollte so mit wechselndem Gepr\u00e4nge", "tokens": ["Ich", "woll\u00b7te", "so", "mit", "wech\u00b7seln\u00b7dem", "Ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Darstellen deinem Blicke mich und ringen", "tokens": ["Dar\u00b7stel\u00b7len", "dei\u00b7nem", "Bli\u00b7cke", "mich", "und", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um deine Gunst in so viel Lustgestalten,", "tokens": ["Um", "dei\u00b7ne", "Gunst", "in", "so", "viel", "Lust\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da\u00df es in einer endlich mir gel\u00e4nge;", "tokens": ["Da\u00df", "es", "in", "ei\u00b7ner", "end\u00b7lich", "mir", "ge\u00b7l\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und welcher es gelang, dich zu bezwingen,", "tokens": ["Und", "wel\u00b7cher", "es", "ge\u00b7lang", ",", "dich", "zu", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die hielt' ich fest; und w\u00fcrd' ich fest dich halten?", "tokens": ["Die", "hielt'", "ich", "fest", ";", "und", "w\u00fcrd'", "ich", "fest", "dich", "hal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$.", "KON", "VAFIN", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}