{"textgrid.poem.57608": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Fischerdorf Wiek", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Graugr\u00fcne, blumenlose Matten", "tokens": ["Grau\u00b7gr\u00fc\u00b7ne", ",", "blu\u00b7men\u00b7lo\u00b7se", "Mat\u00b7ten"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wellenloses, blaues Meer,", "tokens": ["Und", "wel\u00b7len\u00b7lo\u00b7ses", ",", "blau\u00b7es", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAT", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Masten werfen d\u00fcnne Schatten,", "tokens": ["Die", "Mas\u00b7ten", "wer\u00b7fen", "d\u00fcn\u00b7ne", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es riecht die Luft nach Rauch und Teer.", "tokens": ["Es", "riecht", "die", "Luft", "nach", "Rauch", "und", "Teer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zerhau'ne Backen, bunte M\u00fctzen,", "tokens": ["Zer\u00b7hau'\u00b7ne", "Ba\u00b7cken", ",", "bun\u00b7te", "M\u00fct\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Karbol- und Jodoformgeruch,", "tokens": ["Ka\u00b7rbol", "und", "Jo\u00b7do\u00b7form\u00b7ge\u00b7ruch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die kecken Burschenaugen blitzen", "tokens": ["Die", "ke\u00b7cken", "Bur\u00b7schen\u00b7au\u00b7gen", "blit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ungeduldig klopft der Krug.", "tokens": ["Und", "un\u00b7ge\u00b7dul\u00b7dig", "klopft", "der", "Krug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Herr Professor redet weise,", "tokens": ["Der", "Herr", "Pro\u00b7fes\u00b7sor", "re\u00b7det", "wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ernst h\u00f6rt der Theologe zu,", "tokens": ["Ernst", "h\u00f6rt", "der", "Theo\u00b7lo\u00b7ge", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Professors T\u00f6chterlein g\u00e4hnt leise,", "tokens": ["Pro\u00b7fes\u00b7sors", "T\u00f6ch\u00b7ter\u00b7lein", "g\u00e4hnt", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Sande malt ihr schmaler Schuh.", "tokens": ["Im", "San\u00b7de", "malt", "ihr", "schma\u00b7ler", "Schuh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Vom Schankhaus schrilles Fiedelt\u00f6nen,", "tokens": ["Vom", "Schank\u00b7haus", "schril\u00b7les", "Fie\u00b7del\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hamburger Polka scheint's zu sein;", "tokens": ["Ham\u00b7bur\u00b7ger", "Pol\u00b7ka", "scheint's", "zu", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "PTKZU", "VAINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Dort tanzt der Bursch mit seiner Sch\u00f6nen \u2013", "tokens": ["Dort", "tanzt", "der", "Bursch", "mit", "sei\u00b7ner", "Sch\u00f6\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es seufzt Professors T\u00f6chterlein.", "tokens": ["Es", "seufzt", "Pro\u00b7fes\u00b7sors", "T\u00f6ch\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}