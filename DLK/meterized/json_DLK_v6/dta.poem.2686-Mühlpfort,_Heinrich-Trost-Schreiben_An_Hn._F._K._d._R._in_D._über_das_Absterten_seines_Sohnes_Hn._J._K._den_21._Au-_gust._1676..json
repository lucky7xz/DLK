{"dta.poem.2686": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trost-Schreiben/  \n  An Hn. F. K. d. R. in D. \u00fcber das Absterten  \n seines Sohnes Hn. J. K. den 21. Au-  \n gust. 1676.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje soll/ Woledler Herr/ mein Blat ihn nicht er-\nschrecken/", "tokens": ["Wie", "soll", "/", "Wo\u00b7led\u00b7ler", "Herr", "/", "mein", "Blat", "ihn", "nicht", "er", "schre\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "$(", "NE", "NN", "$(", "PPOSAT", "NN", "PPER", "PTKNEG", "TRUNC", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das diese Botschafft bringt: der ", "tokens": ["Das", "die\u00b7se", "Bot\u00b7schafft", "bringt", ":", "der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "PDAT", "NN", "VVFIN", "$.", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jedwede Zeile wird nach Gall und Wermuth schmecken/", "tokens": ["Jed\u00b7we\u00b7de", "Zei\u00b7le", "wird", "nach", "Gall", "und", "Wer\u00b7muth", "schme\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "NE", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und meine gantze Schrifft erf\u00fcllt nur Ach und Noth", "tokens": ["Und", "mei\u00b7ne", "gant\u00b7ze", "Schrifft", "er\u00b7f\u00fcllt", "nur", "Ach", "und", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So schl\u00e4gt der Donner nicht mit ungeheuren Krachen", "tokens": ["So", "schl\u00e4gt", "der", "Don\u00b7ner", "nicht", "mit", "un\u00b7ge\u00b7heu\u00b7ren", "Kra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und Schwefel-lichtem Blitz bey Vieh und Menschen ein/", "tokens": ["Und", "Schwe\u00b7fel\u00b7lich\u00b7tem", "Blitz", "bey", "Vieh", "und", "Men\u00b7schen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "KON", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Als diese Post sein Hertz itzt wird zu nichte machen/", "tokens": ["Als", "die\u00b7se", "Post", "sein", "Hertz", "itzt", "wird", "zu", "nich\u00b7te", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPOSAT", "NN", "ADV", "VAFIN", "PTKA", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ihn fast ohne Geist und Seele heissen seyn.", "tokens": ["Und", "ihn", "fast", "oh\u00b7ne", "Geist", "und", "See\u00b7le", "heis\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So wirfft ein Augenblick die gantze Hoffnung nieder/", "tokens": ["So", "wirfft", "ein", "Au\u00b7gen\u00b7blick", "die", "gant\u00b7ze", "Hoff\u00b7nung", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und aller W\u00fcnsche Zweck verkehrt des Todes Macht.", "tokens": ["Und", "al\u00b7ler", "W\u00fcn\u00b7sche", "Zweck", "ver\u00b7kehrt", "des", "To\u00b7des", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So sieht er seinen Trost der Jahre nicht mehr wieder/", "tokens": ["So", "sieht", "er", "sei\u00b7nen", "Trost", "der", "Jah\u00b7re", "nicht", "mehr", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "PTKNEG", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und de\u00df Geschlechtes Stern sinckt in des ", "tokens": ["Und", "de\u00df", "Ge\u00b7schlech\u00b7tes", "Stern", "sinckt", "in", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Welch Stoicus kan hier die heissen Thr\u00e4nen hemmen/", "tokens": ["Welch", "Stoi\u00b7cus", "kan", "hier", "die", "heis\u00b7sen", "Thr\u00e4\u00b7nen", "hem\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Das Zoll Geld der Natur bey unsrer Sterbligkeit/", "tokens": ["Das", "Zoll", "Geld", "der", "Na\u00b7tur", "bey", "uns\u00b7rer", "Ster\u00b7blig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und wenn sie Str\u00f6men gleich die Augen \u00fcberschwemmen/", "tokens": ["Und", "wenn", "sie", "Str\u00f6\u00b7men", "gleich", "die", "Au\u00b7gen", "\u00fc\u00b7bersc\u00b7hwem\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So bilden sie nicht ab das innre Seelen-Leid.", "tokens": ["So", "bil\u00b7den", "sie", "nicht", "ab", "das", "inn\u00b7re", "See\u00b7len\u00b7Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Timanthes mu\u00df verh\u00fcllt den Agamemnon mahlen/", "tokens": ["Ti\u00b7man\u00b7thes", "mu\u00df", "ver\u00b7h\u00fcllt", "den", "A\u00b7ga\u00b7mem\u00b7non", "mah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "+-+--+--++-+-", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Wenn seine Tochter wird zum Opffer abgeschlacht.", "tokens": ["Wenn", "sei\u00b7ne", "Toch\u00b7ter", "wird", "zum", "Opf\u00b7fer", "ab\u00b7ge\u00b7schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Kein Pinsel gibt an Tag der heissen Liebe Strahlen", "tokens": ["Kein", "Pin\u00b7sel", "gibt", "an", "Tag", "der", "heis\u00b7sen", "Lie\u00b7be", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die in der Eltern Hertz vor ihre Kinder wacht.", "tokens": ["Die", "in", "der", "El\u00b7tern", "Hertz", "vor", "ih\u00b7re", "Kin\u00b7der", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die G\u00fcter des Gel\u00fccks sind schmertzlich zu verlieren", "tokens": ["Die", "G\u00fc\u00b7ter", "des", "Ge\u00b7l\u00fccks", "sind", "schmertz\u00b7lich", "zu", "ver\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Stand/ Hoheit/ Ehre/ Ruhm sch\u00e4tzt man dem Leben gleich:", "tokens": ["Stand", "/", "Ho\u00b7heit", "/", "Eh\u00b7re", "/", "Ruhm", "sch\u00e4tzt", "man", "dem", "Le\u00b7ben", "gleich", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "$(", "NN", "VVFIN", "PIS", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Doch seynd die Regungen weit hefftiger zu sp\u00fcren/", "tokens": ["Doch", "seynd", "die", "Re\u00b7gun\u00b7gen", "weit", "heff\u00b7ti\u00b7ger", "zu", "sp\u00fc\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJD", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.24": {"text": "Wenn unser Fleisch und Blut zertrennt de\u00df Todes Streich.", "tokens": ["Wenn", "un\u00b7ser", "Fleisch", "und", "Blut", "zer\u00b7trennt", "de\u00df", "To\u00b7des", "Streich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Auf die man hat gebaut/ da\u00df sie de\u00df Stammes Erben/", "tokens": ["Auf", "die", "man", "hat", "ge\u00b7baut", "/", "da\u00df", "sie", "de\u00df", "Stam\u00b7mes", "Er\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "VAFIN", "VVPP", "$(", "KOUS", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "De\u00df Namens Ewigkeit Fortpflantzer solten seyn/", "tokens": ["De\u00df", "Na\u00b7mens", "E\u00b7wig\u00b7keit", "Fort\u00b7pflant\u00b7zer", "sol\u00b7ten", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Seh\u2019n in der ersten Bl\u00fcth/ im Lentz der Jahre sterben/", "tokens": ["Seh'n", "in", "der", "ers\u00b7ten", "Bl\u00fcth", "/", "im", "Lentz", "der", "Jah\u00b7re", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$(", "APPRART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Bringt nur gedoppelt Leid und ungemeine Pein.", "tokens": ["Bringt", "nur", "ge\u00b7dop\u00b7pelt", "Leid", "und", "un\u00b7ge\u00b7mei\u00b7ne", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und zwar/ Wol-Edler Herr/ was mehr den Schmertz ergrimmet/", "tokens": ["Und", "zwar", "/", "Wol\u00b7Ed\u00b7ler", "Herr", "/", "was", "mehr", "den", "Schmertz", "er\u00b7grim\u00b7met", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "NE", "NN", "$(", "PWS", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.30": {"text": "Da\u00df itzt in frembdem Sand sein Sohn erblassen mu\u00df/", "tokens": ["Da\u00df", "itzt", "in", "fremb\u00b7dem", "Sand", "sein", "Sohn", "er\u00b7blas\u00b7sen", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da\u00df er abwesende den herben Abschied nimmet/", "tokens": ["Da\u00df", "er", "ab\u00b7we\u00b7sen\u00b7de", "den", "her\u00b7ben", "Ab\u00b7schied", "nim\u00b7met", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.32": {"text": "Und nicht au\u00df Liebes-Pflicht ertheilt den letzten Ku\u00df:", "tokens": ["Und", "nicht", "au\u00df", "Lie\u00b7bes\u00b7Pflicht", "er\u00b7theilt", "den", "letz\u00b7ten", "Ku\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Da\u00df nicht die Vater-Hand die Augen kan zudr\u00fccken/", "tokens": ["Da\u00df", "nicht", "die", "Va\u00b7ter\u00b7Hand", "die", "Au\u00b7gen", "kan", "zu\u00b7dr\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und bi\u00df ins schwartze Grab nachruffen: ", "tokens": ["Und", "bi\u00df", "ins", "schwart\u00b7ze", "Grab", "nach\u00b7ruf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Gehab dich ewig wol/ mein Hoffen und Erquicken!", "tokens": ["Ge\u00b7hab", "dich", "e\u00b7wig", "wol", "/", "mein", "Hof\u00b7fen", "und", "Er\u00b7qui\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ADV", "$(", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ach da\u00df ich/ ", "tokens": ["Ach", "da\u00df", "ich", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.37": {"text": "Denn tritt auch die Vernunfft dem Schmertzen an die Seite/", "tokens": ["Denn", "tritt", "auch", "die", "Ver\u00b7nunfft", "dem", "Schmert\u00b7zen", "an", "die", "Sei\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Stellt seine Gr\u00f6sse f\u00fcr und zeiget den Verlust/", "tokens": ["Stellt", "sei\u00b7ne", "Gr\u00f6s\u00b7se", "f\u00fcr", "und", "zei\u00b7get", "den", "Ver\u00b7lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Bem\u00fcht sich/ wie sie siegt bey solchem Zweiffel-Streite/", "tokens": ["Be\u00b7m\u00fcht", "sich", "/", "wie", "sie", "siegt", "bey", "sol\u00b7chem", "Zweif\u00b7fel\u00b7Strei\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$(", "PWAV", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und ob sie Rath und Trost kan reissen au\u00df der Brust.", "tokens": ["Und", "ob", "sie", "Rath", "und", "Trost", "kan", "reis\u00b7sen", "au\u00df", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "KON", "NN", "VMFIN", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Bevor wenn sie die Lieb als Beystand ihr erwehlet", "tokens": ["Be\u00b7vor", "wenn", "sie", "die", "Lieb", "als", "Beys\u00b7tand", "ihr", "er\u00b7weh\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN", "KOKOM", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und de\u00df verbla\u00dften Bild tieff in das Hertze dr\u00fcckt.", "tokens": ["Und", "de\u00df", "ver\u00b7bla\u00df\u00b7ten", "Bild", "tieff", "in", "das", "Hert\u00b7ze", "dr