{"dta.poem.11409": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aria wider die wanckelm\u00fcthigkeit  \n seiner liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Flatternde liebe!", "tokens": ["Flat\u00b7tern\u00b7de", "lie\u00b7be", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wo eilest du hin?", "tokens": ["Wo", "ei\u00b7lest", "du", "hin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ein gesetzter sinn", "tokens": ["Ein", "ge\u00b7setz\u00b7ter", "sinn"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Folget einem edlern triebe,", "tokens": ["Fol\u00b7get", "ei\u00b7nem", "ed\u00b7lern", "trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Den kein neben-licht,", "tokens": ["Den", "kein", "ne\u00b7ben\u00b7licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Ja der donner selbst, nicht unterbricht.", "tokens": ["Ja", "der", "don\u00b7ner", "selbst", ",", "nicht", "un\u00b7ter\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "ADV", "$,", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Jrrende sterne", "tokens": ["Jr\u00b7ren\u00b7de", "ster\u00b7ne"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Bedeuten nicht viel:", "tokens": ["Be\u00b7deu\u00b7ten", "nicht", "viel", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ein best\u00e4ndig ziel", "tokens": ["Ein", "be\u00b7st\u00e4n\u00b7dig", "ziel"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Jagt die unruh in die ferne.", "tokens": ["Jagt", "die", "un\u00b7ruh", "in", "die", "fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Taugt auch ein magnet,", "tokens": ["Taugt", "auch", "ein", "mag\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der nicht stets nach einem pole steht?", "tokens": ["Der", "nicht", "stets", "nach", "ei\u00b7nem", "po\u00b7le", "steht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Fl\u00fcchtige flammen", "tokens": ["Fl\u00fcch\u00b7ti\u00b7ge", "flam\u00b7men"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Verzehren sich bald:", "tokens": ["Ver\u00b7zeh\u00b7ren", "sich", "bald", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Einen pappel-wald", "tokens": ["Ei\u00b7nen", "pap\u00b7pel\u00b7wald"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Schl\u00e4gt ein schwacher sturm vonsammen;", "tokens": ["Schl\u00e4gt", "ein", "schwa\u00b7cher", "sturm", "von\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wahre liebe bleibt,", "tokens": ["Wah\u00b7re", "lie\u00b7be", "bleibt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Wie ein ceder-baum, der recht bekleibt.", "tokens": ["Wie", "ein", "ce\u00b7der\u00b7baum", ",", "der", "recht", "be\u00b7kleibt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Macht eine sonne", "tokens": ["Macht", "ei\u00b7ne", "son\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Den monden vergn\u00fcgt,", "tokens": ["Den", "mon\u00b7den", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der nicht eckel kriegt", "tokens": ["Der", "nicht", "ec\u00b7kel", "kriegt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "ADJD", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Vor so alter kost und wonne;", "tokens": ["Vor", "so", "al\u00b7ter", "kost", "und", "won\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "KON", "PWAV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Warum findest du", "tokens": ["Wa\u00b7rum", "fin\u00b7dest", "du"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Nicht in einem sterne lust und ruh?", "tokens": ["Nicht", "in", "ei\u00b7nem", "ster\u00b7ne", "lust", "und", "ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Darum, o liebe!", "tokens": ["Da\u00b7rum", ",", "o", "lie\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "FM", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Vollende den flug!", "tokens": ["Voll\u00b7en\u00b7de", "den", "flug", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Handle treu und klug,", "tokens": ["Hand\u00b7le", "treu", "und", "klug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und gehorche doch dem triebe,", "tokens": ["Und", "ge\u00b7hor\u00b7che", "doch", "dem", "trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der nach leib und geist", "tokens": ["Der", "nach", "leib", "und", "geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Dich Ambretten stets umfassen heist!", "tokens": ["Dich", "Am\u00b7bret\u00b7ten", "stets", "um\u00b7fas\u00b7sen", "heist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}