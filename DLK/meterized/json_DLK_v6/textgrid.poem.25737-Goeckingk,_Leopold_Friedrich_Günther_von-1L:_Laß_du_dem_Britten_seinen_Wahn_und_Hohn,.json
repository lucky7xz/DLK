{"textgrid.poem.25737": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00df du dem Britten seinen Wahn und Hohn,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df du dem Britten seinen Wahn und Hohn,", "tokens": ["La\u00df", "du", "dem", "Brit\u00b7ten", "sei\u00b7nen", "Wahn", "und", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df er der erstgeborne Sohn", "tokens": ["Da\u00df", "er", "der", "erst\u00b7ge\u00b7bor\u00b7ne", "Sohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Freiheit sey, wir aber Sklaven!", "tokens": ["Der", "Frei\u00b7heit", "sey", ",", "wir", "a\u00b7ber", "Skla\u00b7ven", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was k\u00fcmmert sich der Weis' um Fasees und um Thron?", "tokens": ["Was", "k\u00fcm\u00b7mert", "sich", "der", "Weis'", "um", "Fa\u00b7sees", "und", "um", "Thron", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.5": {"text": "F\u00fcr ihn hat kein Gesetz noch Strafen.", "tokens": ["F\u00fcr", "ihn", "hat", "kein", "Ge\u00b7setz", "noch", "Stra\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIAT", "NN", "ADV", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "So lang des Landes Satzung ihm gef\u00e4llt,", "tokens": ["So", "lang", "des", "Lan\u00b7des", "Sat\u00b7zung", "ihm", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ist er der erste, der sie h\u00e4lt;", "tokens": ["Ist", "er", "der", "ers\u00b7te", ",", "der", "sie", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was hat er noch zu f\u00fcrchten? Gl\u00fccklich leben", "tokens": ["Was", "hat", "er", "noch", "zu", "f\u00fcrch\u00b7ten", "?", "Gl\u00fcck\u00b7lich", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$.", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Kannst du, ob \u00fcber dich den Zepter Einer h\u00e4lt,", "tokens": ["Kannst", "du", ",", "ob", "\u00fc\u00b7ber", "dich", "den", "Zep\u00b7ter", "Ei\u00b7ner", "h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "APPR", "PRF", "ART", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ob Hundert dir Gesetze geben.", "tokens": ["Ob", "Hun\u00b7dert", "dir", "Ge\u00b7set\u00b7ze", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Doch, kann der Stab der Consuln nicht so schwer,", "tokens": ["Doch", ",", "kann", "der", "Stab", "der", "Con\u00b7suln", "nicht", "so", "schwer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VMFIN", "ART", "NN", "ART", "NN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Als eines w\u00fcthenden Tiber", "tokens": ["Als", "ei\u00b7nes", "w\u00fct\u00b7hen\u00b7den", "Ti\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Metallner Zepter, auf dich fallen?", "tokens": ["Me\u00b7tall\u00b7ner", "Zep\u00b7ter", ",", "auf", "dich", "fal\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Was ist dir \u00fcbrig, Freund, als, selbst an G\u00fctern leer,", "tokens": ["Was", "ist", "dir", "\u00fcb\u00b7rig", ",", "Freund", ",", "als", ",", "selbst", "an", "G\u00fc\u00b7tern", "leer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKVZ", "$,", "NN", "$,", "KOUS", "$,", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Zu einem freiern Volk' zu wallen?", "tokens": ["Zu", "ei\u00b7nem", "frei\u00b7ern", "Vol\u00b7k'", "zu", "wal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Denn, ", "tokens": ["Denn", ","], "token_info": ["word", "punct"], "pos": ["KON", "$,"], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Als alle G\u00fcter dieser Welt,", "tokens": ["Als", "al\u00b7le", "G\u00fc\u00b7ter", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ja selbst als dieses gute Pilgerleben:", "tokens": ["Ja", "selbst", "als", "die\u00b7ses", "gu\u00b7te", "Pil\u00b7ger\u00b7le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "KOUS", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der ist ein Sklav, wenn gleich Tartaren ihm ein Zelt", "tokens": ["Der", "ist", "ein", "Sklav", ",", "wenn", "gleich", "Tar\u00b7ta\u00b7ren", "ihm", "ein", "Zelt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "KOUS", "ADV", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "In ihrer freien Horde geben.", "tokens": ["In", "ih\u00b7rer", "frei\u00b7en", "Hor\u00b7de", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Zu edlen Thaten f\u00fchlt er nimmer Muth;", "tokens": ["Zu", "ed\u00b7len", "Tha\u00b7ten", "f\u00fchlt", "er", "nim\u00b7mer", "Muth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Ihm ist sein Leben oder Gut", "tokens": ["Ihm", "ist", "sein", "Le\u00b7ben", "o\u00b7der", "Gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Der h\u00f6chste Trieb zum Thun und Unterlassen.", "tokens": ["Der", "h\u00f6chs\u00b7te", "Trieb", "zum", "Thun", "und", "Un\u00b7ter\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "So billigt denn der Sklav das, was ein Nero thut,", "tokens": ["So", "bil\u00b7ligt", "denn", "der", "Sklav", "das", ",", "was", "ein", "Ne\u00b7ro", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PDS", "$,", "PRELS", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und mu\u00df August mit Nero hassen.", "tokens": ["Und", "mu\u00df", "Au\u00b7gust", "mit", "Ne\u00b7ro", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "APPR", "NE", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Den P\u00f6bel blendet jedes falsche Licht;", "tokens": ["Den", "P\u00f6\u00b7bel", "blen\u00b7det", "je\u00b7des", "fal\u00b7sche", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Der Freiheit Name hat Gewicht", "tokens": ["Der", "Frei\u00b7heit", "Na\u00b7me", "hat", "Ge\u00b7wicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "F\u00fcr ihn; die Freiheit selbst, f\u00fcr Weise.", "tokens": ["F\u00fcr", "ihn", ";", "die", "Frei\u00b7heit", "selbst", ",", "f\u00fcr", "Wei\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "ART", "NN", "ADV", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Der Schweizer, der so laut zu Hause prahlet, spricht", "tokens": ["Der", "Schwei\u00b7zer", ",", "der", "so", "laut", "zu", "Hau\u00b7se", "prah\u00b7let", ",", "spricht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Mit seinen Consuln, ach! so leise!", "tokens": ["Mit", "sei\u00b7nen", "Con\u00b7suln", ",", "ach", "!", "so", "lei\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ITJ", "$.", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Er beugt vor ihnen dreimal tiefer sich,", "tokens": ["Er", "beugt", "vor", "ih\u00b7nen", "drei\u00b7mal", "tie\u00b7fer", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "ADJD", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Als ich vor einem F\u00fcrsten mich,", "tokens": ["Als", "ich", "vor", "ei\u00b7nem", "F\u00fcrs\u00b7ten", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Und was er ist, das mu\u00df er ewig bleiben.", "tokens": ["Und", "was", "er", "ist", ",", "das", "mu\u00df", "er", "e\u00b7wig", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VAFIN", "$,", "PDS", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Wenn du ein ", "tokens": ["Wenn", "du", "ein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.35": {"text": "Nicht auf der Rathsherrn Rolle schreiben.", "tokens": ["Nicht", "auf", "der", "Raths\u00b7herrn", "Rol\u00b7le", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Den Prunk mit Sina's Vasen, mit Kristall", "tokens": ["Den", "Prunk", "mit", "Si\u00b7na's", "Va\u00b7sen", ",", "mit", "Kris\u00b7tall"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "NE", "$,", "APPR", "NN"], "meter": "-+-+-+--++", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Aus B\u00f6hmen, und den ganzen Schwall", "tokens": ["Aus", "B\u00f6h\u00b7men", ",", "und", "den", "gan\u00b7zen", "Schwall"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Von Frankreichs Moden, mag der Staat mir wehren.", "tokens": ["Von", "Fran\u00b7kreichs", "Mo\u00b7den", ",", "mag", "der", "Staat", "mir", "weh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "St\u00f6rt das des Weisen Gl\u00fcck: so mu\u00df sich \u00fcberall", "tokens": ["St\u00f6rt", "das", "des", "Wei\u00b7sen", "Gl\u00fcck", ":", "so", "mu\u00df", "sich", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ART", "NN", "NN", "$.", "ADV", "VMFIN", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Sein Freiheits-Sinn in ihm emp\u00f6ren.", "tokens": ["Sein", "Frei\u00b7heits\u00b7Sinn", "in", "ihm", "em\u00b7p\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "La\u00df auch den Staat der Russen Kaviar,", "tokens": ["La\u00df", "auch", "den", "Staat", "der", "Rus\u00b7sen", "Ka\u00b7vi\u00b7ar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Und Holsteins Austern, ja sogar", "tokens": ["Und", "Hol\u00b7steins", "Aus\u00b7tern", ",", "ja", "so\u00b7gar"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "NN", "$,", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Den edlen Most vom Rhingau selbst, vertheuren;", "tokens": ["Den", "ed\u00b7len", "Most", "vom", "Rhin\u00b7gau", "selbst", ",", "ver\u00b7theu\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ADV", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Der Freundschaft Feste werd' ich dennoch, Freund, ob zwar", "tokens": ["Der", "Freund\u00b7schaft", "Fes\u00b7te", "werd'", "ich", "den\u00b7noch", ",", "Freund", ",", "ob", "zwar"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "ADV", "$,", "NN", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Mit etwas kleinern Bechern feiren.", "tokens": ["Mit", "et\u00b7was", "klei\u00b7nern", "Be\u00b7chern", "fei\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Doch wenn er auf der Bahn der Ehre, Stein", "tokens": ["Doch", "wenn", "er", "auf", "der", "Bahn", "der", "Eh\u00b7re", ",", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Auf Steine h\u00e4ufte, aus dem Hain'", "tokens": ["Auf", "Stei\u00b7ne", "h\u00e4uf\u00b7te", ",", "aus", "dem", "Hain'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Der Wahrheit, uns zum Nichtstuhl' z\u00f6ge,", "tokens": ["Der", "Wahr\u00b7heit", ",", "uns", "zum", "Nicht\u00b7stuhl'", "z\u00f6\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "APPRART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Und Wahrheit strafte: dann w\u00fcrd' ich der erste seyn,", "tokens": ["Und", "Wahr\u00b7heit", "straf\u00b7te", ":", "dann", "w\u00fcrd'", "ich", "der", "ers\u00b7te", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "ART", "ADJA", "VAINF", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.50": {"text": "Der nackt nach ", "tokens": ["Der", "nackt", "nach"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "APPR"], "meter": "+--", "measure": "dactylic.init"}}}}}