{"dta.poem.5860": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie bildeten sich ein,", "tokens": ["Sie", "bil\u00b7de\u00b7ten", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es w\u00fcrd ihr Einflu\u00df und ihr Schein,", "tokens": ["Es", "w\u00fcrd", "ihr", "Ein\u00b7flu\u00df", "und", "ihr", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Mit mehrer Fruchtbarkeit herab gef\u00fchret,", "tokens": ["Mit", "meh\u00b7rer", "Frucht\u00b7bar\u00b7keit", "her\u00b7ab", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn sie vor dem Altar, auf ihren Knien,", "tokens": ["Wenn", "sie", "vor", "dem", "Al\u00b7tar", ",", "auf", "ih\u00b7ren", "Kni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von ihnen w\u00fcrden angeschrien.", "tokens": ["Von", "ih\u00b7nen", "w\u00fcr\u00b7den", "an\u00b7ge\u00b7schri\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}