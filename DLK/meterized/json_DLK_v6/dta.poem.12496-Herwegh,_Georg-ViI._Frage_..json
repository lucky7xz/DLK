{"dta.poem.12496": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n  Frage   .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sage mir, Freund: wann erscheint sie, die", "tokens": ["Sa\u00b7ge", "mir", ",", "Freund", ":", "wann", "er\u00b7scheint", "sie", ",", "die"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "$,", "NN", "$.", "PWAV", "VVFIN", "PPER", "$,", "PRELS"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Prachtausgabe von Deutschland?", "tokens": ["Prach\u00b7taus\u00b7ga\u00b7be", "von", "Deutschland", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Subskribirten doch schon unsere V\u00e4ter", "tokens": ["Sub\u00b7skri\u00b7bir\u00b7ten", "doch", "schon", "un\u00b7se\u00b7re", "V\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "darauf;", "tokens": ["da\u00b7rauf", ";"], "token_info": ["word", "punct"], "pos": ["PAV", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "L\u00e4ngst ist's unter der Press' im Noten-", "tokens": ["L\u00e4ngst", "ist's", "un\u00b7ter", "der", "Press'", "im", "No\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "APPRART", "TRUNC"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "verlage zu Frankfurt:", "tokens": ["ver\u00b7la\u00b7ge", "zu", "Frank\u00b7furt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aber ich w\u00fcnschte, die Herrn g\u00e4ben es", "tokens": ["A\u00b7ber", "ich", "w\u00fcnschte", ",", "die", "Herrn", "g\u00e4\u00b7ben", "es"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "endlich heraus!", "tokens": ["end\u00b7lich", "he\u00b7raus", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}