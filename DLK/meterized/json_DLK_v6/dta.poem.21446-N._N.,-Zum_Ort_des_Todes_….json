{"dta.poem.21446": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Zum Ort des Todes \u2026", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Zum Ort des Todes lenk\u2019 ich oft den Gang,", "tokens": ["Zum", "Ort", "des", "To\u00b7des", "lenk'", "ich", "oft", "den", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dort wandl\u2019 ich still der Gr\u00e4ber Reih\u2019n entlang.", "tokens": ["Dort", "wandl'", "ich", "still", "der", "Gr\u00e4\u00b7ber", "Reih'n", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zuweilen les\u2019 ich, was auf schlichtem Stein", "tokens": ["Zu\u00b7wei\u00b7len", "les'", "ich", ",", "was", "auf", "schlich\u00b7tem", "Stein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die treue Liebe schrieb so r\u00fchrend ein.", "tokens": ["Die", "treu\u00b7e", "Lie\u00b7be", "schrieb", "so", "r\u00fch\u00b7rend", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Gro\u00dfstadt L\u00e4rm nur traumhaft t\u00f6nt an\u2019s Ohr,", "tokens": ["Der", "Gro\u00df\u00b7stadt", "L\u00e4rm", "nur", "traum\u00b7haft", "t\u00f6nt", "an's", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mich d\u00fcnkt: in selige Au\u2019n ich mich verlor.", "tokens": ["Mich", "d\u00fcnkt", ":", "in", "se\u00b7li\u00b7ge", "Au'n", "ich", "mich", "ver\u00b7lor", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "APPR", "ADJA", "NN", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Die Seele stirbt: es stirbt der ewige Schmerz,", "tokens": ["Die", "See\u00b7le", "stirbt", ":", "es", "stirbt", "der", "e\u00b7wi\u00b7ge", "Schmerz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Tiefsinnige Tr\u00e4ume ziehen himmelw\u00e4rts.", "tokens": ["Tief\u00b7sin\u00b7ni\u00b7ge", "Tr\u00e4u\u00b7me", "zie\u00b7hen", "him\u00b7mel\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+--+-+-+--", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ich bin der Falter, der zum Kelche strebt,", "tokens": ["Ich", "bin", "der", "Fal\u00b7ter", ",", "der", "zum", "Kel\u00b7che", "strebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ich bin das St\u00e4ubchen, das im Lichte webt.", "tokens": ["Ich", "bin", "das", "St\u00e4ub\u00b7chen", ",", "das", "im", "Lich\u00b7te", "webt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ich lebe und bin todt vieltausend Jahr,", "tokens": ["Ich", "le\u00b7be", "und", "bin", "todt", "viel\u00b7tau\u00b7send", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "ADJD", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ich wei\u00df, da\u00df ich einst war und doch nicht war.", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "ich", "einst", "war", "und", "doch", "nicht", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VAFIN", "KON", "ADV", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "So d\u00e4mmer\u2019 ich schrankenlos in Zeit und Raum,", "tokens": ["So", "d\u00e4m\u00b7mer'", "ich", "schran\u00b7ken\u00b7los", "in", "Zeit", "und", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Wie sich ein welkes Blatt losl\u00f6st vom Baum.", "tokens": ["Wie", "sich", "ein", "wel\u00b7kes", "Blatt", "los\u00b7l\u00f6st", "vom", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}