{"textgrid.poem.49662": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Zwanglos", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schillerfeier ignorierten", "tokens": ["Die", "Schil\u00b7ler\u00b7fei\u00b7er", "ig\u00b7no\u00b7rier\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6chst Ihre Majest\u00e4t, obgleichen", "tokens": ["H\u00f6chst", "Ih\u00b7re", "Ma\u00b7jes\u00b7t\u00e4t", ",", "ob\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KOUS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie andre Feste h\u00e4ufig zierten", "tokens": ["Sie", "and\u00b7re", "Fes\u00b7te", "h\u00e4u\u00b7fig", "zier\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch des Besuches Gnadenzeichen.", "tokens": ["Durch", "des", "Be\u00b7su\u00b7ches", "Gna\u00b7den\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Der Deutsche fragt mit wunder Seele,", "tokens": ["Der", "Deut\u00b7sche", "fragt", "mit", "wun\u00b7der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Warum? Warum in diesen Tagen", "tokens": ["Wa\u00b7rum", "?", "Wa\u00b7rum", "in", "die\u00b7sen", "Ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "PWAV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die sch\u00f6nste Zier dem Feste fehle?", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Zier", "dem", "Fes\u00b7te", "feh\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was hei\u00dft das? h\u00f6rt man viele fragen.", "tokens": ["Was", "hei\u00dft", "das", "?", "h\u00f6rt", "man", "vie\u00b7le", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDS", "$.", "VVFIN", "PIS", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich braucht' das Maul nicht aufzusperren.", "tokens": ["Ich", "braucht'", "das", "Maul", "nicht", "auf\u00b7zu\u00b7sper\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sache ist zwar ungew\u00f6hnlich,", "tokens": ["Die", "Sa\u00b7che", "ist", "zwar", "un\u00b7ge\u00b7w\u00f6hn\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch gebt mir zu, verehrte Herren,", "tokens": ["Doch", "gebt", "mir", "zu", ",", "ver\u00b7ehr\u00b7te", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geschmack ist einmal ganz pers\u00f6nlich.", "tokens": ["Ge\u00b7schmack", "ist", "ein\u00b7mal", "ganz", "per\u00b7s\u00f6n\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nicht allen leuchten gleiche Sterne,", "tokens": ["Nicht", "al\u00b7len", "leuch\u00b7ten", "glei\u00b7che", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Schillerfest ist uns gegl\u00fcckt,", "tokens": ["Das", "Schil\u00b7ler\u00b7fest", "ist", "uns", "ge\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Majest\u00e4t hat f\u00fcr Jules Verne", "tokens": ["Und", "Ma\u00b7jes\u00b7t\u00e4t", "hat", "f\u00fcr", "Ju\u00b7les", "Ver\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "APPR", "NE", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sein Wohlgefallen ausgedr\u00fcckt.", "tokens": ["Sein", "Wohl\u00b7ge\u00b7fal\u00b7len", "aus\u00b7ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}