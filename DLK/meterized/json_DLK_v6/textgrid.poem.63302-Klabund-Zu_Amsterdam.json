{"textgrid.poem.63302": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Zu Amsterdam", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Amsterdam bin ich geboren,", "tokens": ["Zu", "A\u00b7mster\u00b7dam", "bin", "ich", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Meine Mutter war ein M\u00e4dchen ums Geld.", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "war", "ein", "M\u00e4d\u00b7chen", "ums", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Mein Vater hat ihr die Ehe geschworen,", "tokens": ["Mein", "Va\u00b7ter", "hat", "ihr", "die", "E\u00b7he", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "War aber weit gefehlt.", "tokens": ["War", "a\u00b7ber", "weit", "ge\u00b7fehlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In einer dunklen Gasse,", "tokens": ["In", "ei\u00b7ner", "dunk\u00b7len", "Gas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sah ich zum erstenmal das Sonnenlicht.", "tokens": ["Sah", "ich", "zum", "ers\u00b7ten\u00b7mal", "das", "Son\u00b7nen\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich wollte es mit meinen H\u00e4nden fassen,", "tokens": ["Ich", "woll\u00b7te", "es", "mit", "mei\u00b7nen", "H\u00e4n\u00b7den", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und konnt' es aber nicht.", "tokens": ["Und", "konnt'", "es", "a\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein junger Mann kam eines Tages,", "tokens": ["Ein", "jun\u00b7ger", "Mann", "kam", "ei\u00b7nes", "Ta\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und k\u00fc\u00dfte mich und rief mich seinen Schatz.", "tokens": ["Und", "k\u00fc\u00df\u00b7te", "mich", "und", "rief", "mich", "sei\u00b7nen", "Schatz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie legten bald ihn in den Schragen,", "tokens": ["Sie", "leg\u00b7ten", "bald", "ihn", "in", "den", "Schra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein anderer nahm seinen Platz.", "tokens": ["Ein", "an\u00b7de\u00b7rer", "nahm", "sei\u00b7nen", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir sind im Fr\u00fchling durch den Wald gegangen", "tokens": ["Wir", "sind", "im", "Fr\u00fch\u00b7ling", "durch", "den", "Wald", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sahen Hirsch und Reh.", "tokens": ["Und", "sa\u00b7hen", "Hirsch", "und", "Reh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die B\u00e4ume bl\u00fchten und die V\u00f6gel sangen,", "tokens": ["Die", "B\u00e4u\u00b7me", "bl\u00fch\u00b7ten", "und", "die", "V\u00f6\u00b7gel", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vierbl\u00e4ttrig stand der Klee.", "tokens": ["Vier\u00b7bl\u00e4tt\u00b7rig", "stand", "der", "Klee", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein jeder hat mir Treu' in Ewigkeit geschworen,", "tokens": ["Ein", "je\u00b7der", "hat", "mir", "Treu'", "in", "E\u00b7wig\u00b7keit", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "PPER", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "War aber weit gefehlt.", "tokens": ["War", "a\u00b7ber", "weit", "ge\u00b7fehlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu Amsterdam hab' ich mein' Ehr' verloren,", "tokens": ["Zu", "A\u00b7mster\u00b7dam", "hab'", "ich", "mein'", "Ehr'", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich bin ein M\u00e4dchen um's Geld.", "tokens": ["Ich", "bin", "ein", "M\u00e4d\u00b7chen", "um's", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}