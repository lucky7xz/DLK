{"dta.poem.14583": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonntag .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich dacht': ihr kleinen Br\u00e4ute,", "tokens": ["Ich", "dacht'", ":", "ihr", "klei\u00b7nen", "Br\u00e4u\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was schm\u00fcckt ihr euch so sehr? \u2014", "tokens": ["Was", "schm\u00fcckt", "ihr", "euch", "so", "sehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da blickt' die eine her:", "tokens": ["Da", "blickt'", "die", "ei\u00b7ne", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u201estill, still, 's ist Sonntag heute.\u201c", "tokens": ["\u201e", "still", ",", "still", ",", "'s", "ist", "Sonn\u00b7tag", "heu\u00b7te", ".", "\u201c"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "ADJD", "$,", "PPER", "VAFIN", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}