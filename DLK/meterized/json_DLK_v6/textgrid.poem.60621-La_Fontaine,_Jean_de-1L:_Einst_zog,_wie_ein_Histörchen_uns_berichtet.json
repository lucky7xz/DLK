{"textgrid.poem.60621": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst zog, wie ein Hist\u00f6rchen uns berichtet", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst zog, wie ein Hist\u00f6rchen uns berichtet", "tokens": ["Einst", "zog", ",", "wie", "ein", "His\u00b7t\u00f6r\u00b7chen", "uns", "be\u00b7rich\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "(und ist's nicht wahr, so ist's doch gut erdichtet),", "tokens": ["(", "und", "ist's", "nicht", "wahr", ",", "so", "ist's", "doch", "gut", "er\u00b7dich\u00b7tet", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "VAFIN", "PTKNEG", "ADJD", "$,", "ADV", "VAFIN", "ADV", "ADJD", "VVPP", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Sultans Abgesandter, der beim Kaiser war,", "tokens": ["Des", "Sul\u00b7tans", "Ab\u00b7ge\u00b7sand\u00b7ter", ",", "der", "beim", "Kai\u00b7ser", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Sultans Macht der kaiserlichen vor.", "tokens": ["Des", "Sul\u00b7tans", "Macht", "der", "kai\u00b7ser\u00b7li\u00b7chen", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Worte trafen eines Deutschen Ohr,", "tokens": ["Die", "Wor\u00b7te", "tra\u00b7fen", "ei\u00b7nes", "Deut\u00b7schen", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der fand des T\u00fcrken Meinung anfechtbar.", "tokens": ["Der", "fand", "des", "T\u00fcr\u00b7ken", "Mei\u00b7nung", "an\u00b7fecht\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er sagte: \u00bbUnser Herrscher hat Vasallen,", "tokens": ["Er", "sag\u00b7te", ":", "\u00bb", "Un\u00b7ser", "Herr\u00b7scher", "hat", "Va\u00b7sal\u00b7len", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die reich und m\u00e4chtig sind wie er;", "tokens": ["Die", "reich", "und", "m\u00e4ch\u00b7tig", "sind", "wie", "er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VAFIN", "KOKOM", "PPER", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Jeder von ihnen k\u00f6nnte nach Gefallen", "tokens": ["Je\u00b7der", "von", "ih\u00b7nen", "k\u00f6nn\u00b7te", "nach", "Ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "PPER", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Besolden ein gewaltig Heer.\u00ab", "tokens": ["Be\u00b7sol\u00b7den", "ein", "ge\u00b7wal\u00b7tig", "Heer", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "ADJD", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Des Sultans Untertan, ein kluger Mann,", "tokens": ["Des", "Sul\u00b7tans", "Un\u00b7ter\u00b7tan", ",", "ein", "klu\u00b7ger", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Entgegnete: \u00bbGewi\u00df, wohl h\u00f6rte ich,", "tokens": ["Ent\u00b7geg\u00b7ne\u00b7te", ":", "\u00bb", "Ge\u00b7wi\u00df", ",", "wohl", "h\u00f6r\u00b7te", "ich", ","], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "$(", "PTKANT", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Da\u00df jeder Kurf\u00fcrst Truppen r\u00fcsten kann.", "tokens": ["Da\u00df", "je\u00b7der", "Kur\u00b7f\u00fcrst", "Trup\u00b7pen", "r\u00fcs\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "An einen Traum gemahnte dieses mich.", "tokens": ["An", "ei\u00b7nen", "Traum", "ge\u00b7mahn\u00b7te", "die\u00b7ses", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PDAT", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ich war an sicherm Ort, als jenseits hoher Hecken", "tokens": ["Ich", "war", "an", "si\u00b7cherm", "Ort", ",", "als", "jen\u00b7seits", "ho\u00b7her", "He\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die hundert K\u00f6pfe einer Hydra drohten.", "tokens": ["Die", "hun\u00b7dert", "K\u00f6p\u00b7fe", "ei\u00b7ner", "Hyd\u00b7ra", "droh\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Mich \u00fcberfiel ein kalter Schrecken,", "tokens": ["Mich", "\u00fc\u00b7berf\u00b7iel", "ein", "kal\u00b7ter", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ich z\u00e4hlte mich schon zu den Toten,", "tokens": ["Ich", "z\u00e4hl\u00b7te", "mich", "schon", "zu", "den", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Doch kam ich mit dem Schrecken fort;", "tokens": ["Doch", "kam", "ich", "mit", "dem", "Schre\u00b7cken", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Der Drache konnte keine \u00d6ffnung finden,", "tokens": ["Der", "Dra\u00b7che", "konn\u00b7te", "kei\u00b7ne", "\u00d6ff\u00b7nung", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Um durch die hohe Hecke dort", "tokens": ["Um", "durch", "die", "ho\u00b7he", "He\u00b7cke", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Die vielen K\u00f6pfe glatt hindurchzuwinden.", "tokens": ["Die", "vie\u00b7len", "K\u00f6p\u00b7fe", "glatt", "hin\u00b7durch\u00b7zu\u00b7win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Noch sann ich nach dem Abenteuer,", "tokens": ["Noch", "sann", "ich", "nach", "dem", "A\u00b7bent\u00b7eu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Da zeigte sich am selben Ort", "tokens": ["Da", "zeig\u00b7te", "sich", "am", "sel\u00b7ben", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Ein andres Ungeheuer;", "tokens": ["Ein", "and\u00b7res", "Un\u00b7ge\u00b7heu\u00b7er", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Das hatte nur ein einzig Haupt,", "tokens": ["Das", "hat\u00b7te", "nur", "ein", "ein\u00b7zig", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Doch mehr als einen Schwanz am Leibe.", "tokens": ["Doch", "mehr", "als", "ei\u00b7nen", "Schwanz", "am", "Lei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Entsetzlich kam es angeschnaubt \u2013", "tokens": ["Ent\u00b7setz\u00b7lich", "kam", "es", "an\u00b7ge\u00b7schnaubt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Unm\u00f6glich, da\u00df ich's Euch beschreibe.", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", ",", "da\u00df", "ich's", "Euch", "be\u00b7schrei\u00b7be", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Der Kopf schl\u00fcpft m\u00fchlos durch mit Brust und Bauch,", "tokens": ["Der", "Kopf", "schl\u00fcpft", "m\u00fch\u00b7los", "durch", "mit", "Brust", "und", "Bauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Und hinterher die Schw\u00e4nze alle auch;", "tokens": ["Und", "hin\u00b7ter\u00b7her", "die", "Schw\u00e4n\u00b7ze", "al\u00b7le", "auch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Nichts hindert sie, leicht ist's vollbracht,", "tokens": ["Nichts", "hin\u00b7dert", "sie", ",", "leicht", "ist's", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Da eins dem andern Platz gemacht.", "tokens": ["Da", "eins", "dem", "an\u00b7dern", "Platz", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Und seht \u2013 dies ist der Sache Kern \u2013", "tokens": ["Und", "seht", "\u2013", "dies", "ist", "der", "Sa\u00b7che", "Kern", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PDS", "VAFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "So steht's mit Eurem, so mit unserm Herrn.\u00ab", "tokens": ["So", "steht's", "mit", "Eu\u00b7rem", ",", "so", "mit", "un\u00b7serm", "Herrn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "$,", "ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}