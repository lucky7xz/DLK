{"textgrid.poem.39637": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr kennt des Eulenspiegels Streiche,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr kennt des Eulenspiegels Streiche,", "tokens": ["Ihr", "kennt", "des", "Eu\u00b7len\u00b7spie\u00b7gels", "Strei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gar weit ber\u00fchmt im r\u00f6m'schen Reiche.", "tokens": ["Gar", "weit", "be\u00b7r\u00fchmt", "im", "r\u00f6m'\u00b7schen", "Rei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "APPRART", "ADJA", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Fast jedes Handwerk gieng er durch,", "tokens": ["Fast", "je\u00b7des", "Hand\u00b7werk", "gieng", "er", "durch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nahm Kriegsdienst auch in einer Burg;", "tokens": ["Nahm", "Kriegs\u00b7dienst", "auch", "in", "ei\u00b7ner", "Burg", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da ward er auf den Thurm gestellt,", "tokens": ["Da", "ward", "er", "auf", "den", "Thurm", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo weit zu schauen war das Feld,", "tokens": ["Wo", "weit", "zu", "schau\u00b7en", "war", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PTKZU", "VVINF", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df er in die Trompete stie\u00dfe,", "tokens": ["Da\u00df", "er", "in", "die", "Trom\u00b7pe\u00b7te", "stie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Wenn Feindes Schaar sich blicken lie\u00dfe.", "tokens": ["Wenn", "Fein\u00b7des", "Schaar", "sich", "bli\u00b7cken", "lie\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sie sprachen: \u00bbSiehst du F\u00e4hnlein nah'n,", "tokens": ["Sie", "spra\u00b7chen", ":", "\u00bb", "Siehst", "du", "F\u00e4hn\u00b7lein", "nah'n", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "VVFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So blase nur sie flugs heran\u00ab. \u2013", "tokens": ["So", "bla\u00b7se", "nur", "sie", "flugs", "he\u00b7ran", "\u00ab", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ADV", "PTKVZ", "$(", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das war so eine Redensart.", "tokens": ["Das", "war", "so", "ei\u00b7ne", "Re\u00b7den\u00b7sart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Dabei bedrohten sie ihn hart,", "tokens": ["Da\u00b7bei", "be\u00b7droh\u00b7ten", "sie", "ihn", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wenn er entwiche von dem Posten,", "tokens": ["Wenn", "er", "ent\u00b7wi\u00b7che", "von", "dem", "Pos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "So w\u00fcrd' es seinen Sold ihm kosten.", "tokens": ["So", "w\u00fcrd'", "es", "sei\u00b7nen", "Sold", "ihm", "kos\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Nun kam die Zeit zum Mittage\u00dfen,", "tokens": ["Nun", "kam", "die", "Zeit", "zum", "Mit\u00b7ta\u00b7ge\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und Eulenspiegel ward verge\u00dfen,", "tokens": ["Und", "Eu\u00b7len\u00b7spie\u00b7gel", "ward", "ver\u00b7ge\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Durch keinen guten Trunk getr\u00f6st't,", "tokens": ["Durch", "kei\u00b7nen", "gu\u00b7ten", "Trunk", "ge\u00b7tr\u00f6st't", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und auch von niemand abgel\u00f6s't.", "tokens": ["Und", "auch", "von", "nie\u00b7mand", "ab\u00b7ge\u00b7l\u00f6s'", "t."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ADV", "APPR", "PIS", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Da griff er zur Trompete fein,", "tokens": ["Da", "griff", "er", "zur", "Trom\u00b7pe\u00b7te", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.20": {"text": "Und blies aus Leibeskr\u00e4ften drein.", "tokens": ["Und", "blies", "aus", "Lei\u00b7bes\u00b7kr\u00e4f\u00b7ten", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die Knechte waffnen sich zu Haufen;", "tokens": ["Die", "Knech\u00b7te", "waff\u00b7nen", "sich", "zu", "Hau\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Viel' kamen auf den Thurm gelaufen.", "tokens": ["Viel'", "ka\u00b7men", "auf", "den", "Thurm", "ge\u00b7lau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "\u00bbwo ist der Feind? Sag', Eulenspiegel!", "tokens": ["\u00bb", "wo", "ist", "der", "Feind", "?", "Sag'", ",", "Eu\u00b7len\u00b7spie\u00b7gel", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "ART", "NN", "$.", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Man sieht ja nichts um Wald und H\u00fcgel.\u00ab \u2013", "tokens": ["Man", "sieht", "ja", "nichts", "um", "Wald", "und", "H\u00fc\u00b7gel", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PIS", "APPR", "NN", "KON", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Er sprach: \u00bbIch kann ihn auch nicht sehn,", "tokens": ["Er", "sprach", ":", "\u00bb", "Ich", "kann", "ihn", "auch", "nicht", "sehn", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Doch solches ist mit Flei\u00df geschehn.", "tokens": ["Doch", "sol\u00b7ches", "ist", "mit", "Flei\u00df", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Ich sah, da\u00df keine Feinde kamen:", "tokens": ["Ich", "sah", ",", "da\u00df", "kei\u00b7ne", "Fein\u00b7de", "ka\u00b7men", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Da hab' ich denn, in Gottes Namen,", "tokens": ["Da", "hab'", "ich", "denn", ",", "in", "Got\u00b7tes", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Heranzublasen sie versucht.\u00ab \u2013", "tokens": ["Her\u00b7an\u00b7zu\u00b7bla\u00b7sen", "sie", "ver\u00b7sucht", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Sie sprachen: \u00bbSchalksnarr, sei verflucht!", "tokens": ["Sie", "spra\u00b7chen", ":", "\u00bb", "Schalks\u00b7narr", ",", "sei", "ver\u00b7flucht", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NE", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Du hast die Mahlzeit uns verst\u00f6rt,", "tokens": ["Du", "hast", "die", "Mahl\u00b7zeit", "uns", "ver\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Da wir den Zeterl\u00e4rm geh\u00f6rt.\u00ab", "tokens": ["Da", "wir", "den", "Ze\u00b7ter\u00b7l\u00e4rm", "ge\u00b7h\u00f6rt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So schrie j\u00fcngst Zeter, Zeter, Zeter,", "tokens": ["So", "schrie", "j\u00fcngst", "Ze\u00b7ter", ",", "Ze\u00b7ter", ",", "Ze\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein lang verschollener Trompeter.", "tokens": ["Ein", "lang", "ver\u00b7schol\u00b7le\u00b7ner", "Trom\u00b7pe\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Tragt ihm das E\u00dfen flei\u00dfig zu,", "tokens": ["Tragt", "ihm", "das", "E\u00b7\u00dfen", "flei\u00b7\u00dfig", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf da\u00df er k\u00fcnftig schweigen thu'.", "tokens": ["Auf", "da\u00df", "er", "k\u00fcnf\u00b7tig", "schwei\u00b7gen", "thu'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}