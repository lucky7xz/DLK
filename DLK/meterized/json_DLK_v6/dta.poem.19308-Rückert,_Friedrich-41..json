{"dta.poem.19308": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was nennst du gro\u00df und klein? du nennest gr\u00f6\u00dfer, was", "tokens": ["Was", "nennst", "du", "gro\u00df", "und", "klein", "?", "du", "nen\u00b7nest", "gr\u00f6\u00b7\u00dfer", ",", "was"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$.", "PPER", "VVFIN", "ADJD", "$,", "PWS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist \u00fcber, kleiner wol, was unter deinem Ma\u00df.", "tokens": ["Ist", "\u00fc\u00b7ber", ",", "klei\u00b7ner", "wol", ",", "was", "un\u00b7ter", "dei\u00b7nem", "Ma\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "$,", "ADJD", "ADV", "$,", "PRELS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Selbst gegen Kleineres kommst du dir gr\u00f6\u00dfer vor,", "tokens": ["Selbst", "ge\u00b7gen", "Klei\u00b7ne\u00b7res", "kommst", "du", "dir", "gr\u00f6\u00b7\u00dfer", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und gegen Gro\u00dfes klein, so schwankest du o Rohr.", "tokens": ["Und", "ge\u00b7gen", "Gro\u00b7\u00dfes", "klein", ",", "so", "schwan\u00b7kest", "du", "o", "Rohr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "$,", "ADV", "VVFIN", "PPER", "FM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bist du das Ma\u00df der Welt? hast du an dir das Ma\u00df?", "tokens": ["Bist", "du", "das", "Ma\u00df", "der", "Welt", "?", "hast", "du", "an", "dir", "das", "Ma\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "$.", "VAFIN", "PPER", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprich, ist an deinem Leib, in deinem Geiste das?", "tokens": ["Sprich", ",", "ist", "an", "dei\u00b7nem", "Leib", ",", "in", "dei\u00b7nem", "Geis\u00b7te", "das", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VAFIN", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "PDS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nicht klein nenn' oder gro\u00df, was gro\u00df ist oder klein", "tokens": ["Nicht", "klein", "nenn'", "o\u00b7der", "gro\u00df", ",", "was", "gro\u00df", "ist", "o\u00b7der", "klein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VVFIN", "KON", "ADJD", "$,", "PRELS", "ADJD", "VAFIN", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach deinem Leibe blo\u00df, nach deinem Sinn allein.", "tokens": ["Nach", "dei\u00b7nem", "Lei\u00b7be", "blo\u00df", ",", "nach", "dei\u00b7nem", "Sinn", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Gro\u00df ist kein Sonnenball, und klein kein Sonnenst\u00e4ubchen;", "tokens": ["Gro\u00df", "ist", "kein", "Son\u00b7nen\u00b7ball", ",", "und", "klein", "kein", "Son\u00b7nen\u00b7st\u00e4ub\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PIAT", "NN", "$,", "KON", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gro\u00df ist der Sch\u00f6pfung Baum, doch klein daran kein L\u00e4ubchen.", "tokens": ["Gro\u00df", "ist", "der", "Sch\u00f6p\u00b7fung", "Baum", ",", "doch", "klein", "da\u00b7ran", "kein", "L\u00e4ub\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NE", "$,", "ADV", "ADJD", "PAV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Nur der Gedank' ist gro\u00df, da\u00df nichts so gro\u00df, so klein,", "tokens": ["Nur", "der", "Ge\u00b7dank'", "ist", "gro\u00df", ",", "da\u00df", "nichts", "so", "gro\u00df", ",", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADJD", "$,", "KOUS", "PIS", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als der Gedanken ist, der alles ist allein.", "tokens": ["Als", "der", "Ge\u00b7dan\u00b7ken", "ist", ",", "der", "al\u00b7les", "ist", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "$,", "PRELS", "PIS", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}