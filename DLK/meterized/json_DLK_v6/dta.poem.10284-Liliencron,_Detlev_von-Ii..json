{"dta.poem.10284": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.85", "nl:0.14"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Im Nebelnorden, an der Ostseek\u00fcste,", "tokens": ["Im", "Ne\u00b7bel\u00b7nor\u00b7den", ",", "an", "der", "Ost\u00b7see\u00b7k\u00fcs\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Abseits der St\u00e4dte und der gro\u00dfen Stra\u00dfen,", "tokens": ["Ab\u00b7seits", "der", "St\u00e4d\u00b7te", "und", "der", "gro\u00b7\u00dfen", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Schl\u00e4ft einsam und vergessen, halb verweht", "tokens": ["Schl\u00e4ft", "ein\u00b7sam", "und", "ver\u00b7ges\u00b7sen", ",", "halb", "ver\u00b7weht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "VVPP", "$,", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Schnee von harten St\u00fcrmen oft gezaust,", "tokens": ["Im", "Schnee", "von", "har\u00b7ten", "St\u00fcr\u00b7men", "oft", "ge\u00b7zaust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein kleines Gut. Zwei ungeschlachte Riesen,", "tokens": ["Ein", "klei\u00b7nes", "Gut", ".", "Zwei", "un\u00b7ge\u00b7schlach\u00b7te", "Rie\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Uralte Tannen, strecken ihre Arme", "tokens": ["Ur\u00b7al\u00b7te", "Tan\u00b7nen", ",", "stre\u00b7cken", "ih\u00b7re", "Ar\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie Speere vor zum Schutz des Herrenhauses.", "tokens": ["Wie", "Spee\u00b7re", "vor", "zum", "Schutz", "des", "Her\u00b7ren\u00b7hau\u00b7ses", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Unh\u00f6rbar, drinnen auf dem Smyrnateppich,", "tokens": ["Un\u00b7h\u00f6r\u00b7bar", ",", "drin\u00b7nen", "auf", "dem", "Smyr\u00b7na\u00b7tep\u00b7pich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Geht eine junge Dame auf und nieder.", "tokens": ["Geht", "ei\u00b7ne", "jun\u00b7ge", "Da\u00b7me", "auf", "und", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Bisweilen bleibt sie stehn, schraubt an der Lampe,", "tokens": ["Bis\u00b7wei\u00b7len", "bleibt", "sie", "stehn", ",", "schraubt", "an", "der", "Lam\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Schiebt auf dem Bechstein an das Notenpult", "tokens": ["Schiebt", "auf", "dem", "Bech\u00b7stein", "an", "das", "No\u00b7ten\u00b7pult"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die schweren Bronzecandelaber n\u00e4her,", "tokens": ["Die", "schwe\u00b7ren", "Bron\u00b7ze\u00b7can\u00b7de\u00b7la\u00b7ber", "n\u00e4\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Zupft im Vor\u00fcbergehen an der Decke", "tokens": ["Zupft", "im", "Vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen", "an", "der", "De\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Des Sophatisches, horcht, und wandert, horcht,", "tokens": ["Des", "So\u00b7pha\u00b7ti\u00b7sches", ",", "horcht", ",", "und", "wan\u00b7dert", ",", "horcht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,", "KON", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Die grauen Augen auf die Th\u00fcr gerichtet.", "tokens": ["Die", "grau\u00b7en", "Au\u00b7gen", "auf", "die", "Th\u00fcr", "ge\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Bis endlich ihre schwere Stirn ein Schwarm", "tokens": ["Bis", "end\u00b7lich", "ih\u00b7re", "schwe\u00b7re", "Stirn", "ein", "Schwarm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PPOSAT", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Von Sommerv\u00f6geln lustig \u00fcberflattert.", "tokens": ["Von", "Som\u00b7mer\u00b7v\u00f6\u00b7geln", "lus\u00b7tig", "\u00fc\u00b7berf\u00b7lat\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Nun schreitet langsam auf dem warmen Teppich", "tokens": ["Nun", "schrei\u00b7tet", "lang\u00b7sam", "auf", "dem", "war\u00b7men", "Tep\u00b7pich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Ein P\u00e4rchen, angeschmiedet, auf und nieder.", "tokens": ["Ein", "P\u00e4r\u00b7chen", ",", "an\u00b7ge\u00b7schmie\u00b7det", ",", "auf", "und", "nie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,", "APPR", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Behaglichkeit, das K\u00e4tzchen, schnurrt im Zimmer,", "tokens": ["Be\u00b7hag\u00b7lich\u00b7keit", ",", "das", "K\u00e4tz\u00b7chen", ",", "schnurrt", "im", "Zim\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Indessen drau\u00dfen in der Winternacht,", "tokens": ["In\u00b7des\u00b7sen", "drau\u00b7\u00dfen", "in", "der", "Win\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Ein Abglanz von den Schilden Schlachterschlagner,", "tokens": ["Ein", "Ab\u00b7glanz", "von", "den", "Schil\u00b7den", "Schlach\u00b7ter\u00b7schlag\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Die flei\u00dfig in Walhall den Humpen schwingen,", "tokens": ["Die", "flei\u00b7\u00dfig", "in", "Wal\u00b7hall", "den", "Hum\u00b7pen", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.24": {"text": "Die blassen Strahlenb\u00fcndel eines Nordlichts", "tokens": ["Die", "blas\u00b7sen", "Strah\u00b7len\u00b7b\u00fcn\u00b7del", "ei\u00b7nes", "Nord\u00b7lichts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Am strengen Himmel Odins sich ergie\u00dfen.", "tokens": ["Am", "stren\u00b7gen", "Him\u00b7mel", "O\u00b7dins", "sich", "er\u00b7gie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NE", "PRF", "VVINF", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.26": {"text": "Und auf der toten Heide bellt der Fuchs.", "tokens": ["Und", "auf", "der", "to\u00b7ten", "Hei\u00b7de", "bellt", "der", "Fuchs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}