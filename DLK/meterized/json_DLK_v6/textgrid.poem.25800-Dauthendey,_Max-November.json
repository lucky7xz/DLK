{"textgrid.poem.25800": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "November", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grau verwirrt der leere Wald.", "tokens": ["Grau", "ver\u00b7wirrt", "der", "lee\u00b7re", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit tausend blaugl\u00fchenden \u00c4theraugen,", "tokens": ["Mit", "tau\u00b7send", "blau\u00b7gl\u00fc\u00b7hen\u00b7den", "\u00c4\u00b7ther\u00b7au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hoch durch schwarzen Fichtenbehang,", "tokens": ["Hoch", "durch", "schwar\u00b7zen", "Fich\u00b7ten\u00b7be\u00b7hang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Irren Heere blauer gigantischer Bl\u00fcten.", "tokens": ["Ir\u00b7ren", "Hee\u00b7re", "blau\u00b7er", "gi\u00b7gan\u00b7ti\u00b7scher", "Bl\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Von fremden Dolden,", "tokens": ["Von", "frem\u00b7den", "Dol\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Niemand hat je sie belauscht,", "tokens": ["Nie\u00b7mand", "hat", "je", "sie", "be\u00b7lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bl\u00fcht jeder Morgen im Grase", "tokens": ["Bl\u00fcht", "je\u00b7der", "Mor\u00b7gen", "im", "Gra\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Eisigen Samen.", "tokens": ["Ei\u00b7si\u00b7gen", "Sa\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Graue Frauen,", "tokens": ["Grau\u00b7e", "Frau\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Die lautlos im Reigen kamen,", "tokens": ["Die", "laut\u00b7los", "im", "Rei\u00b7gen", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sind lautlos gegangen.", "tokens": ["Sind", "laut\u00b7los", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Bleichen Juwelen", "tokens": ["Der", "Blei\u00b7chen", "Ju\u00b7we\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Strahlende F\u00e4den", "tokens": ["Strah\u00b7len\u00b7de", "F\u00e4\u00b7den"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Irisgr\u00fcn, irisgolden,", "tokens": ["I\u00b7ris\u00b7gr\u00fcn", ",", "i\u00b7ris\u00b7gol\u00b7den", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Hangen an allen Zweigen.", "tokens": ["Han\u00b7gen", "an", "al\u00b7len", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "In nackten Kronen singen", "tokens": ["In", "nack\u00b7ten", "Kro\u00b7nen", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wachszarte Str\u00f6me der Sonne.", "tokens": ["Wachs\u00b7zar\u00b7te", "Str\u00f6\u00b7me", "der", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Um blo\u00dfe S\u00e4ulen,", "tokens": ["Um", "blo\u00b7\u00dfe", "S\u00e4u\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Auf wei\u00dfen Schwingen kreist", "tokens": ["Auf", "wei\u00b7\u00dfen", "Schwin\u00b7gen", "kreist"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein\u00e4ugig ein Aar,", "tokens": ["Ein\u00b7\u00e4u\u00b7gig", "ein", "Aar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Das Schweigen.", "tokens": ["Das", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}