{"textgrid.poem.52062": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der alte Vater wil in Welschland Kriege f\u00fchrn,", "tokens": ["Der", "al\u00b7te", "Va\u00b7ter", "wil", "in", "Wel\u00b7schland", "Krie\u00b7ge", "f\u00fchrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und l\u00e4st voll Geistlichkeit vor Glocken Drommeln r\u00fchrn:", "tokens": ["Und", "l\u00e4st", "voll", "Geist\u00b7lich\u00b7keit", "vor", "Glo\u00b7cken", "Drom\u00b7meln", "r\u00fchrn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der friedsam hat gelebt, l\u00e4st itzt die Bienen schw\u00e4rmen,", "tokens": ["Der", "fried\u00b7sam", "hat", "ge\u00b7lebt", ",", "l\u00e4st", "itzt", "die", "Bie\u00b7nen", "schw\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "VVPP", "$,", "VVFIN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer ihren Stachel f\u00fchlt, wird sich von Stund an h\u00e4rmen.", "tokens": ["Wer", "ih\u00b7ren", "Sta\u00b7chel", "f\u00fchlt", ",", "wird", "sich", "von", "Stund", "an", "h\u00e4r\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "VAFIN", "PRF", "APPR", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sey wach, o gro\u00dfes Rom, der Finne fragt nach dir,", "tokens": ["Sey", "wach", ",", "o", "gro\u00b7\u00dfes", "Rom", ",", "der", "Fin\u00b7ne", "fragt", "nach", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "FM", "ADJA", "NE", "$,", "ART", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der alten Gothen Geist waltzt sich vom Arctos f\u00fcr:", "tokens": ["Der", "al\u00b7ten", "Go\u00b7then", "Geist", "waltzt", "sich", "vom", "Arc\u00b7tos", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "PRF", "APPRART", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr Herrn, ihr werdet noch der Barbarn ihre Diener,", "tokens": ["Ihr", "Herrn", ",", "ihr", "wer\u00b7det", "noch", "der", "Bar\u00b7barn", "ih\u00b7re", "Die\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Barbarn, oder ja zu vor der Barberiner.", "tokens": ["Der", "Bar\u00b7barn", ",", "o\u00b7der", "ja", "zu", "vor", "der", "Bar\u00b7be\u00b7ri\u00b7ner", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ADV", "PTKZU", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}