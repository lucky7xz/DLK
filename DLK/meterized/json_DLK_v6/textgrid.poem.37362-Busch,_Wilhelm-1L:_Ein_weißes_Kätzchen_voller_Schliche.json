{"textgrid.poem.37362": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein wei\u00dfes K\u00e4tzchen voller Schliche", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein wei\u00dfes K\u00e4tzchen voller Schliche", "tokens": ["Ein", "wei\u00b7\u00dfes", "K\u00e4tz\u00b7chen", "vol\u00b7ler", "Schli\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ging heimlich, weil es gerne schleckt,", "tokens": ["Ging", "heim\u00b7lich", ",", "weil", "es", "ger\u00b7ne", "schleckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Abends in die Nachbark\u00fcche,", "tokens": ["Des", "A\u00b7bends", "in", "die", "Nach\u00b7bar\u00b7k\u00fc\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo man es leider bald entdeckt.", "tokens": ["Wo", "man", "es", "lei\u00b7der", "bald", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit Besen und mit Feuerzangen", "tokens": ["Mit", "Be\u00b7sen", "und", "mit", "Feu\u00b7er\u00b7zan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gejagt in alle Ecken ward's.", "tokens": ["Ge\u00b7jagt", "in", "al\u00b7le", "E\u00b7cken", "ward'", "s."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["VVPP", "APPR", "PIAT", "NN", "VAFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es fuhr zuletzt voll Todesbangen", "tokens": ["Es", "fuhr", "zu\u00b7letzt", "voll", "To\u00b7des\u00b7ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Schlot hinaus und wurde schwarz.", "tokens": ["Zum", "Schlot", "hin\u00b7aus", "und", "wur\u00b7de", "schwarz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, siehst du wohl, mein liebes Herze?", "tokens": ["Ja", ",", "siehst", "du", "wohl", ",", "mein", "lie\u00b7bes", "Her\u00b7ze", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer schlecken will, was ihm gef\u00e4llt,", "tokens": ["Wer", "schle\u00b7cken", "will", ",", "was", "ihm", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VMFIN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der kommt nicht ohne Schmutz und Schw\u00e4rze", "tokens": ["Der", "kommt", "nicht", "oh\u00b7ne", "Schmutz", "und", "Schw\u00e4r\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PTKNEG", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hinaus aus dieser b\u00f6sen Welt.", "tokens": ["Hin\u00b7aus", "aus", "die\u00b7ser", "b\u00f6\u00b7sen", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}