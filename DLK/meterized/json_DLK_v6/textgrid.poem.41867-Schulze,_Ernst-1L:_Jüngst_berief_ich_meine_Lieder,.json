{"textgrid.poem.41867": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: J\u00fcngst berief ich meine Lieder,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst berief ich meine Lieder,", "tokens": ["J\u00fcngst", "be\u00b7rief", "ich", "mei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie flatterten herbey,", "tokens": ["Und", "sie", "flat\u00b7ter\u00b7ten", "her\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Schwebten singend auf und nieder,", "tokens": ["Schweb\u00b7ten", "sin\u00b7gend", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spielten, flogen hin und wieder,", "tokens": ["Spiel\u00b7ten", ",", "flo\u00b7gen", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "KON", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie der Bienenschwarm im May.", "tokens": ["Wie", "der", "Bie\u00b7nen\u00b7schwarm", "im", "May", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ich sagte: Fliegt und nistet,", "tokens": ["Und", "ich", "sag\u00b7te", ":", "Fliegt", "und", "nis\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Singt und t\u00e4ndelt, wo's euch l\u00fcstet,", "tokens": ["Singt", "und", "t\u00e4n\u00b7delt", ",", "wo's", "euch", "l\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lieder, geht, ich geb' euch frey.", "tokens": ["Lie\u00b7der", ",", "geht", ",", "ich", "geb'", "euch", "frey", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lange hab' ich euch gehalten,", "tokens": ["Lan\u00b7ge", "hab'", "ich", "euch", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Liebste zu erfreun;", "tokens": ["Mei\u00b7ne", "Liebs\u00b7te", "zu", "er\u00b7freun", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ihr werdet von der Kalten", "tokens": ["Doch", "ihr", "wer\u00b7det", "von", "der", "Kal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmermehr den Dank erhalten,", "tokens": ["Nim\u00b7mer\u00b7mehr", "den", "Dank", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimmer frey und fr\u00f6hlich seyn.", "tokens": ["Nim\u00b7mer", "frey", "und", "fr\u00f6h\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nun so flieht und flattert weiter;", "tokens": ["Nun", "so", "flieht", "und", "flat\u00b7tert", "wei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ewig hell und ewig heiter", "tokens": ["E\u00b7wig", "hell", "und", "e\u00b7wig", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist der duft'ge Musenhain.", "tokens": ["Ist", "der", "duft'\u00b7ge", "Mu\u00b7sen\u00b7hain", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch sie schienen still zu klagen,", "tokens": ["Doch", "sie", "schie\u00b7nen", "still", "zu", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchlten weder Lust noch Dank,", "tokens": ["F\u00fchl\u00b7ten", "we\u00b7der", "Lust", "noch", "Dank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vor Wehmuth und vor Zagen", "tokens": ["Und", "vor", "Weh\u00b7muth", "und", "vor", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "APPR", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Konnte keins ein W\u00f6rtchen sagen,", "tokens": ["Konn\u00b7te", "keins", "ein", "W\u00f6rt\u00b7chen", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jedes seufzte leis' und bang.", "tokens": ["Je\u00b7des", "seufz\u00b7te", "leis'", "und", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sie neigten ihr Gefieder,", "tokens": ["Und", "sie", "neig\u00b7ten", "ihr", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Senkten still das K\u00f6pfchen nieder,", "tokens": ["Senk\u00b7ten", "still", "das", "K\u00f6pf\u00b7chen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ohne Sang und ohne Klang.", "tokens": ["Oh\u00b7ne", "Sang", "und", "oh\u00b7ne", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und nur eines spannte dreister", "tokens": ["Und", "nur", "ei\u00b7nes", "spann\u00b7te", "dreis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bittend seine Fl\u00fcgel aus:", "tokens": ["Bit\u00b7tend", "sei\u00b7ne", "Fl\u00fc\u00b7gel", "aus", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df doch, sang es, lieber Meister,", "tokens": ["La\u00df", "doch", ",", "sang", "es", ",", "lie\u00b7ber", "Meis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "VVFIN", "PPER", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht die armen kleinen Geister", "tokens": ["Nicht", "die", "ar\u00b7men", "klei\u00b7nen", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Irren ohne Pfleg' und Haus!", "tokens": ["Ir\u00b7ren", "oh\u00b7ne", "Pfleg'", "und", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Treib' uns doch von unsern Rosen", "tokens": ["Treib'", "uns", "doch", "von", "un\u00b7sern", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nimmer in den bl\u00e4tterlosen,", "tokens": ["Nim\u00b7mer", "in", "den", "bl\u00e4t\u00b7ter\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In den wilden Hain hinaus!", "tokens": ["In", "den", "wil\u00b7den", "Hain", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Kannst du Sch\u00f6nes wohl uns zeigen,", "tokens": ["Kannst", "du", "Sch\u00f6\u00b7nes", "wohl", "uns", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was die Liebe nicht erzieht?", "tokens": ["Was", "die", "Lie\u00b7be", "nicht", "er\u00b7zieht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, wo ihre L\u00fcfte schweigen,", "tokens": ["Ach", ",", "wo", "ih\u00b7re", "L\u00fcf\u00b7te", "schwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00e4llt das Laub von allen Zweigen,", "tokens": ["F\u00e4llt", "das", "Laub", "von", "al\u00b7len", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Blumenkelch verbl\u00fcht.", "tokens": ["Und", "der", "Blu\u00b7men\u00b7kelch", "ver\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Traurig stehn die gr\u00fcnen Hallen,", "tokens": ["Trau\u00b7rig", "stehn", "die", "gr\u00fc\u00b7nen", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und es fliehn die Nachtigallen,", "tokens": ["Und", "es", "fliehn", "die", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn der frische Lenz entflieht.", "tokens": ["Wenn", "der", "fri\u00b7sche", "Lenz", "ent\u00b7flieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schw\u00e4rmen auch in bl\u00fch'nden Hainen", "tokens": ["Schw\u00e4r\u00b7men", "auch", "in", "bl\u00fch'n\u00b7den", "Hai\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Br\u00fcder gro\u00df und klein,", "tokens": ["Uns\u00b7re", "Br\u00fc\u00b7der", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6ner wird es uns erscheinen,", "tokens": ["Sch\u00f6\u00b7ner", "wird", "es", "uns", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bey der Sch\u00f6nen, bey der Reinen,", "tokens": ["Bey", "der", "Sch\u00f6\u00b7nen", ",", "bey", "der", "Rei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bey der Freundlichen zu seyn.", "tokens": ["Bey", "der", "Freund\u00b7li\u00b7chen", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Blickt die Lieb' auch streng und tr\u00fcbe,", "tokens": ["Blickt", "die", "Lieb'", "auch", "streng", "und", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lieb' ist Leben, Leben Liebe,", "tokens": ["Lieb'", "ist", "Le\u00b7ben", ",", "Le\u00b7ben", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der Freye wohnt allein.", "tokens": ["Und", "der", "Frey\u00b7e", "wohnt", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Schwindet nicht der Morgenschimmer,", "tokens": ["Schwin\u00b7det", "nicht", "der", "Mor\u00b7gen\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweigt das laue S\u00e4useln nicht?", "tokens": ["Schweigt", "das", "lau\u00b7e", "S\u00e4u\u00b7seln", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Augen leuchten immer,", "tokens": ["Ih\u00b7re", "Au\u00b7gen", "leuch\u00b7ten", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Gluth und Milde scheiden nimmer", "tokens": ["Gluth", "und", "Mil\u00b7de", "schei\u00b7den", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus dem keuschen Angesicht.", "tokens": ["Aus", "dem", "keu\u00b7schen", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mag sie nie den Dank uns geben,", "tokens": ["Mag", "sie", "nie", "den", "Dank", "uns", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00df uns spielen, la\u00df uns leben", "tokens": ["La\u00df", "uns", "spie\u00b7len", ",", "la\u00df", "uns", "le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In dem warmen Sonnenlicht!", "tokens": ["In", "dem", "war\u00b7men", "Son\u00b7nen\u00b7licht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nun so flattert hin und wieder", "tokens": ["Nun", "so", "flat\u00b7tert", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PTKVZ", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die sch\u00f6ne Zauberin;", "tokens": ["Um", "die", "sch\u00f6\u00b7ne", "Zau\u00b7be\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald versengt ihr, arme Lieder,", "tokens": ["Bald", "ver\u00b7sengt", "ihr", ",", "ar\u00b7me", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch das lustige Gefieder,", "tokens": ["Euch", "das", "lus\u00b7ti\u00b7ge", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und verklungen sinkt ihr hin.", "tokens": ["Und", "ver\u00b7klun\u00b7gen", "sinkt", "ihr", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "S\u00fc\u00dfen Tod sollt ihr erwerben;", "tokens": ["S\u00fc\u00b7\u00dfen", "Tod", "sollt", "ihr", "er\u00b7wer\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fcr der Liebsten Lust zu sterben,", "tokens": ["F\u00fcr", "der", "Liebs\u00b7ten", "Lust", "zu", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist der freundlichste Gewinn.", "tokens": ["Ist", "der", "freund\u00b7lichs\u00b7te", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}