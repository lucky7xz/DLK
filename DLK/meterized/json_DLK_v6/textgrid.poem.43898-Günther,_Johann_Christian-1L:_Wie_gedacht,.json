{"textgrid.poem.43898": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie gedacht,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie gedacht,", "tokens": ["Wie", "ge\u00b7dacht", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Vor geliebt, jezt ausgelacht.", "tokens": ["Vor", "ge\u00b7liebt", ",", "jezt", "aus\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "VVPP", "$,", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gestern in die Schoos geri\u00dfen,", "tokens": ["Ge\u00b7stern", "in", "die", "Schoos", "ge\u00b7ri\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Heute von der Brust geschmi\u00dfen,", "tokens": ["Heu\u00b7te", "von", "der", "Brust", "ge\u00b7schmi\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Morgen in die Gruft gebracht.", "tokens": ["Mor\u00b7gen", "in", "die", "Gruft", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie gedacht,", "tokens": ["Wie", "ge\u00b7dacht", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Vor geliebt, jezt ausgelacht.", "tokens": ["Vor", "ge\u00b7liebt", ",", "jezt", "aus\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "VVPP", "$,", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dieses ist", "tokens": ["Die\u00b7ses", "ist"], "token_info": ["word", "word"], "pos": ["PDS", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Aller Jungfern Hinterlist:", "tokens": ["Al\u00b7ler", "Jung\u00b7fern", "Hin\u00b7ter\u00b7list", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Viel versprechen, wenig halten;", "tokens": ["Viel", "ver\u00b7spre\u00b7chen", ",", "we\u00b7nig", "hal\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie entz\u00fcnden und erkalten", "tokens": ["Sie", "ent\u00b7z\u00fcn\u00b7den", "und", "er\u00b7kal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00d6fters, eh ein Tag verfliest.", "tokens": ["\u00d6f\u00b7ters", ",", "eh", "ein", "Tag", "ver\u00b7fliest", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieses ist", "tokens": ["Die\u00b7ses", "ist"], "token_info": ["word", "word"], "pos": ["PDS", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Aller Jungfern Hinterlist.", "tokens": ["Al\u00b7ler", "Jung\u00b7fern", "Hin\u00b7ter\u00b7list", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dein Betrug,", "tokens": ["Dein", "Be\u00b7trug", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Falsche Seele, macht mich klug;", "tokens": ["Fal\u00b7sche", "See\u00b7le", ",", "macht", "mich", "klug", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine soll mich mehr umfa\u00dfen,", "tokens": ["Kei\u00b7ne", "soll", "mich", "mehr", "um\u00b7fa\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine soll mich mehr verla\u00dfen,", "tokens": ["Kei\u00b7ne", "soll", "mich", "mehr", "ver\u00b7la\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einmahl ist vorwahr genug.", "tokens": ["Ein\u00b7mahl", "ist", "vor\u00b7wahr", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dein Betrug,", "tokens": ["Dein", "Be\u00b7trug", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Falsche Seele, macht mich klug.", "tokens": ["Fal\u00b7sche", "See\u00b7le", ",", "macht", "mich", "klug", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dencke nur,", "tokens": ["Den\u00b7cke", "nur", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Ungetreue Creatur,", "tokens": ["Un\u00b7ge\u00b7treu\u00b7e", "Crea\u00b7tur", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dencke, sag ich, nur zur\u00fccke", "tokens": ["Den\u00b7cke", ",", "sag", "ich", ",", "nur", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und betrachte deine T\u00fccke", "tokens": ["Und", "be\u00b7trach\u00b7te", "dei\u00b7ne", "T\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und erwege deinen Schwur.", "tokens": ["Und", "er\u00b7we\u00b7ge", "dei\u00b7nen", "Schwur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dencke nur,", "tokens": ["Den\u00b7cke", "nur", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Ungetreue Creatur!", "tokens": ["Un\u00b7ge\u00b7treu\u00b7e", "Crea\u00b7tur", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Hastu nicht", "tokens": ["Has\u00b7tu", "nicht"], "token_info": ["word", "word"], "pos": ["NE", "PTKNEG"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Ein Gewi\u00dfen, das dich sticht,", "tokens": ["Ein", "Ge\u00b7wi\u00b7\u00dfen", ",", "das", "dich", "sticht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Treue meines Herzens,", "tokens": ["Wenn", "die", "Treu\u00b7e", "mei\u00b7nes", "Her\u00b7zens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die Gr\u00f6\u00dfe meines Schmerzens", "tokens": ["Wenn", "die", "Gr\u00f6\u00b7\u00dfe", "mei\u00b7nes", "Schmer\u00b7zens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deinem Wechsel widerspricht?", "tokens": ["Dei\u00b7nem", "Wech\u00b7sel", "wi\u00b7der\u00b7spricht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hastu nicht", "tokens": ["Has\u00b7tu", "nicht"], "token_info": ["word", "word"], "pos": ["NE", "PTKNEG"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Ein Gewi\u00dfen, das dich sticht?", "tokens": ["Ein", "Ge\u00b7wi\u00b7\u00dfen", ",", "das", "dich", "sticht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Bringt mein Ku\u00df", "tokens": ["Bringt", "mein", "Ku\u00df"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Dir so eilends \u00dcberdru\u00df,", "tokens": ["Dir", "so", "ei\u00b7lends", "\u00dc\u00b7berd\u00b7ru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ey so geh und k\u00fc\u00dfe diesen,", "tokens": ["Ey", "so", "geh", "und", "k\u00fc\u00b7\u00dfe", "die\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "KON", "VVFIN", "PDAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welcher dir sein Geld gewiesen,", "tokens": ["Wel\u00b7cher", "dir", "sein", "Geld", "ge\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das dich warlich blenden mu\u00df,", "tokens": ["Das", "dich", "war\u00b7lich", "blen\u00b7den", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bringt mein Ku\u00df", "tokens": ["Bringt", "mein", "Ku\u00df"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Dir so eilends \u00dcberdru\u00df.", "tokens": ["Dir", "so", "ei\u00b7lends", "\u00dc\u00b7berd\u00b7ru\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Bin ich arm,", "tokens": ["Bin", "ich", "arm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Dieses macht mir wenig Harm;", "tokens": ["Die\u00b7ses", "macht", "mir", "we\u00b7nig", "Harm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tugend steckt nicht in dem Beuthel,", "tokens": ["Tu\u00b7gend", "steckt", "nicht", "in", "dem", "Beut\u00b7hel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Gold und Schmuck macht nur die Scheitel,", "tokens": ["Gold", "und", "Schmuck", "macht", "nur", "die", "Schei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber nicht die Liebe warm.", "tokens": ["A\u00b7ber", "nicht", "die", "Lie\u00b7be", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bin ich arm,", "tokens": ["Bin", "ich", "arm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Dieses macht mir wenig Harm.", "tokens": ["Die\u00b7ses", "macht", "mir", "we\u00b7nig", "Harm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und wie bald", "tokens": ["Und", "wie", "bald"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Mi\u00dft die Sch\u00f6nheit die Gestalt!", "tokens": ["Mi\u00dft", "die", "Sch\u00f6n\u00b7heit", "die", "Ge\u00b7stalt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "R\u00fchmstu gleich von deiner Farbe,", "tokens": ["R\u00fchm\u00b7stu", "gleich", "von", "dei\u00b7ner", "Far\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie ihres gleichen darbe,", "tokens": ["Da\u00df", "sie", "ih\u00b7res", "glei\u00b7chen", "dar\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch die Rosen werden alt.", "tokens": ["Auch", "die", "Ro\u00b7sen", "wer\u00b7den", "alt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und wie bald", "tokens": ["Und", "wie", "bald"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Mi\u00dft die Sch\u00f6nheit die Gestalt!", "tokens": ["Mi\u00dft", "die", "Sch\u00f6n\u00b7heit", "die", "Ge\u00b7stalt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Weg mit dir,", "tokens": ["Weg", "mit", "dir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Falsches Herze, weg von mir!", "tokens": ["Fal\u00b7sches", "Her\u00b7ze", ",", "weg", "von", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "$,", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich zerrei\u00dfe deine Kette,", "tokens": ["Ich", "zer\u00b7rei\u00b7\u00dfe", "dei\u00b7ne", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn die kluge Henriette", "tokens": ["Denn", "die", "klu\u00b7ge", "Hen\u00b7riet\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Stellet mir was Be\u00dfers f\u00fcr.", "tokens": ["Stel\u00b7let", "mir", "was", "Be\u00b7\u00dfers", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "NN", "APPR", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Weg mit dir,", "tokens": ["Weg", "mit", "dir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Falsches Herze, weg von mir!", "tokens": ["Fal\u00b7sches", "Her\u00b7ze", ",", "weg", "von", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "$,", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}