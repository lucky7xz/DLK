{"textgrid.poem.48051": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Maiblumen pfl\u00fcckt ich mir einen Strau\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Maiblumen pfl\u00fcckt ich mir einen Strau\u00df", "tokens": ["Maib\u00b7lu\u00b7men", "pfl\u00fcckt", "ich", "mir", "ei\u00b7nen", "Strau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPER", "ART", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und brachte ihn abends mit nach Haus", "tokens": ["und", "brach\u00b7te", "ihn", "a\u00b7bends", "mit", "nach", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "und stellte ihn in ein Wasserglas", "tokens": ["und", "stell\u00b7te", "ihn", "in", "ein", "Was\u00b7ser\u00b7glas"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "auf den Schreibtisch neben mein Tintenfa\u00df \u2013", "tokens": ["auf", "den", "Schreib\u00b7tisch", "ne\u00b7ben", "mein", "Tin\u00b7ten\u00b7fa\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "und schlief und tr\u00e4umte von Blumenbl\u00fchn,", "tokens": ["und", "schlief", "und", "tr\u00e4um\u00b7te", "von", "Blu\u00b7men\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "von Wogenrauschen und Waldesgr\u00fcn,", "tokens": ["von", "Wo\u00b7gen\u00b7rau\u00b7schen", "und", "Wal\u00b7des\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "und als die Sonne ins Zimmer sah,", "tokens": ["und", "als", "die", "Son\u00b7ne", "ins", "Zim\u00b7mer", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "welch lieblich Wunder beschien sie da:", "tokens": ["welch", "lieb\u00b7lich", "Wun\u00b7der", "be\u00b7schien", "sie", "da", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJD", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "ein Spinnlein, das ich vergangene Nacht,", "tokens": ["ein", "Spinn\u00b7lein", ",", "das", "ich", "ver\u00b7gan\u00b7ge\u00b7ne", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "im Strau\u00df verborgen, mit heimgebracht,", "tokens": ["im", "Strau\u00df", "ver\u00b7bor\u00b7gen", ",", "mit", "heim\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "APPR", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "war seiner duftigen Haft entronnen", "tokens": ["war", "sei\u00b7ner", "duf\u00b7ti\u00b7gen", "Haft", "ent\u00b7ron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und hatte ein schimmerndes Netz gesponnen;", "tokens": ["und", "hat\u00b7te", "ein", "schim\u00b7mern\u00b7des", "Netz", "ge\u00b7spon\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "das schwankte nun zwischen dem Blumenglas", "tokens": ["das", "schwank\u00b7te", "nun", "zwi\u00b7schen", "dem", "Blu\u00b7men\u00b7glas"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und dem Liederbuch \u00fcber dem Tintenfa\u00df.", "tokens": ["und", "dem", "Lie\u00b7der\u00b7buch", "\u00fc\u00b7ber", "dem", "Tin\u00b7ten\u00b7fa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.8": {"line.1": {"text": "Da lacht ich: du willst eine Dichterin sein \u2013", "tokens": ["Da", "lacht", "ich", ":", "du", "willst", "ei\u00b7ne", "Dich\u00b7te\u00b7rin", "sein", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPER", "VMFIN", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "und die Spinnen spinnen dein Tintfa\u00df ein?", "tokens": ["und", "die", "Spin\u00b7nen", "spin\u00b7nen", "dein", "Tint\u00b7fa\u00df", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "So la\u00df es gelten als freundliches Bild:", "tokens": ["So", "la\u00df", "es", "gel\u00b7ten", "als", "freund\u00b7li\u00b7ches", "Bild", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVINF", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "das Lied, das dir frisch aus der Seele quillt,", "tokens": ["das", "Lied", ",", "das", "dir", "frisch", "aus", "der", "See\u00b7le", "quillt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.10": {"line.1": {"text": "schreib es nicht nieder mit Stift und Stahl, \u2013", "tokens": ["schreib", "es", "nicht", "nie\u00b7der", "mit", "Stift", "und", "Stahl", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKVZ", "APPR", "NN", "KON", "NN", "$,", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "gib es dem leuchtenden Sonnenstrahl", "tokens": ["gib", "es", "dem", "leuch\u00b7ten\u00b7den", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "und sing es hinaus in die bl\u00fchende Welt . . .", "tokens": ["und", "sing", "es", "hin\u00b7aus", "in", "die", "bl\u00fc\u00b7hen\u00b7de", "Welt", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Nachsingen mag es, wem es gef\u00e4llt!", "tokens": ["Nach\u00b7sin\u00b7gen", "mag", "es", ",", "wem", "es", "ge\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}