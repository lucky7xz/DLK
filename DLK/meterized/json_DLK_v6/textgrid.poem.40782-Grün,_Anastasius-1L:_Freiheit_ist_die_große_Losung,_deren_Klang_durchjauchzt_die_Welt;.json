{"textgrid.poem.40782": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freiheit ist die gro\u00dfe Losung, deren Klang durchjauchzt die Welt;", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freiheit ist die gro\u00dfe Losung, deren Klang durchjauchzt die Welt;", "tokens": ["Frei\u00b7heit", "ist", "die", "gro\u00b7\u00dfe", "Lo\u00b7sung", ",", "de\u00b7ren", "Klang", "durch\u00b7jauchzt", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Traun, es wird euch wenig frommen, da\u00df fortan ihr taub euch stellt!", "tokens": ["Traun", ",", "es", "wird", "euch", "we\u00b7nig", "from\u00b7men", ",", "da\u00df", "for\u00b7tan", "ihr", "taub", "euch", "stellt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "PIAT", "ADJA", "$,", "KOUS", "ADV", "PPER", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Mild und bittend sprach sie einstens; eure Taubheit zwang sie jetzt,", "tokens": ["Mild", "und", "bit\u00b7tend", "sprach", "sie", "eins\u00b7tens", ";", "eu\u00b7re", "Taub\u00b7heit", "zwang", "sie", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "PPER", "ADV", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Da\u00df sie in Kanonendonner nun ihr Wort euch \u00fcbersetzt.", "tokens": ["Da\u00df", "sie", "in", "Ka\u00b7no\u00b7nen\u00b7don\u00b7ner", "nun", "ihr", "Wort", "euch", "\u00fc\u00b7bers\u00b7etzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Freiheit, die erkorne Jungfrau, schwingt das Banner unsrer Zeit;", "tokens": ["Frei\u00b7heit", ",", "die", "er\u00b7kor\u00b7ne", "Jung\u00b7frau", ",", "schwingt", "das", "Ban\u00b7ner", "uns\u00b7rer", "Zeit", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df fortan ihr blind euch stellet, o f\u00fcrwahr, es hilft nicht weit", "tokens": ["Da\u00df", "for\u00b7tan", "ihr", "blind", "euch", "stel\u00b7let", ",", "o", "f\u00fcr\u00b7wahr", ",", "es", "hilft", "nicht", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "VVFIN", "PPER", "VVFIN", "$,", "FM", "FM", "$,", "PPER", "VVFIN", "PTKNEG", "ADJD"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Da ihr nicht gesehn das Banner, als es wei\u00df und rein und hell,", "tokens": ["Da", "ihr", "nicht", "ge\u00b7sehn", "das", "Ban\u00b7ner", ",", "als", "es", "wei\u00df", "und", "rein", "und", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVPP", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ei was Wunder, wenn mit Blute sie's gef\u00e4rbt nun roth und grell!", "tokens": ["Ei", "was", "Wun\u00b7der", ",", "wenn", "mit", "Blu\u00b7te", "sie's", "ge\u00b7f\u00e4rbt", "nun", "roth", "und", "grell", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "NN", "$,", "KOUS", "APPR", "NN", "NE", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Ihr nur habt die sch\u00f6ne Jungfrau mit dem Kriegesgott gepaart.", "tokens": ["Ihr", "nur", "habt", "die", "sch\u00f6\u00b7ne", "Jung\u00b7frau", "mit", "dem", "Krie\u00b7ges\u00b7gott", "ge\u00b7paart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Waffenspiel und Blutgew\u00e4nder sind wohl sonst nicht ihre Art;", "tokens": ["Waf\u00b7fen\u00b7spiel", "und", "Blut\u00b7ge\u00b7w\u00e4n\u00b7der", "sind", "wohl", "sonst", "nicht", "ih\u00b7re", "Art", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADV", "ADV", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Aber siegen mu\u00df sie immer! die\u00df bleibt ihre Art und Macht,", "tokens": ["A\u00b7ber", "sie\u00b7gen", "mu\u00df", "sie", "im\u00b7mer", "!", "die\u00df", "bleibt", "ih\u00b7re", "Art", "und", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "ADV", "$.", "PDS", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ueber Herzen in dem Hause, \u00fcber Speere in der Schlacht!", "tokens": ["Ue\u00b7ber", "Her\u00b7zen", "in", "dem", "Hau\u00b7se", ",", "\u00fc\u00b7ber", "Spee\u00b7re", "in", "der", "Schlacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Wenn mit Rocken nicht und Spindel, und mit Wort' und Blicken s\u00fc\u00df,", "tokens": ["Wenn", "mit", "Ro\u00b7cken", "nicht", "und", "Spin\u00b7del", ",", "und", "mit", "Wort'", "und", "Bli\u00b7cken", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PTKNEG", "KON", "NN", "$,", "KON", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "So als erzgeschuppte Pallas mit dem Schwert und Schild gewi\u00df", "tokens": ["So", "als", "erz\u00b7ge\u00b7schupp\u00b7te", "Pal\u00b7las", "mit", "dem", "Schwert", "und", "Schild", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "VVFIN", "NE", "APPR", "ART", "NN", "KON", "NN", "ADV"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und bei uns auch wird sie siegen, ja ich k\u00fcnd' es laut und frei", "tokens": ["Und", "bei", "uns", "auch", "wird", "sie", "sie\u00b7gen", ",", "ja", "ich", "k\u00fcnd'", "es", "laut", "und", "frei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADV", "VAFIN", "PPER", "VVINF", "$,", "ADV", "PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wunsch und Hoffnung meines Herzens riefen gern den Sieg herbei.", "tokens": ["Wunsch", "und", "Hoff\u00b7nung", "mei\u00b7nes", "Her\u00b7zens", "rie\u00b7fen", "gern", "den", "Sieg", "her\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Dort auf dem vulkan'schen Boden mu\u00df wohl ein Vesuv es sein,", "tokens": ["Dort", "auf", "dem", "vul\u00b7kan'\u00b7schen", "Bo\u00b7den", "mu\u00df", "wohl", "ein", "Ve\u00b7suv", "es", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VMFIN", "ADV", "ART", "NN", "PPER", "VAINF", "$,"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Der die Luft mit Flammenruthen wieder fege hell und rein!", "tokens": ["Der", "die", "Luft", "mit", "Flam\u00b7men\u00b7rut\u00b7hen", "wie\u00b7der", "fe\u00b7ge", "hell", "und", "rein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "ADV", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Dort auf st\u00fcrmereichem Meere tobt sich erst das Wetter aus,", "tokens": ["Dort", "auf", "st\u00fcr\u00b7me\u00b7rei\u00b7chem", "Mee\u00b7re", "tobt", "sich", "erst", "das", "Wet\u00b7ter", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "PRF", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Eh' erhellt, gereint, gel\u00e4utert prangt des Aethers blaues Haus!", "tokens": ["Eh'", "er\u00b7hellt", ",", "ge\u00b7reint", ",", "ge\u00b7l\u00e4u\u00b7tert", "prangt", "des", "A\u00b7e\u00b7thers", "blau\u00b7es", "Haus", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVPP", "$,", "VVPP", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "Doch in unsrem Rebenlande, hier in milder Bl\u00fcthenau,", "tokens": ["Doch", "in", "uns\u00b7rem", "Re\u00b7ben\u00b7lan\u00b7de", ",", "hier", "in", "mil\u00b7der", "Bl\u00fc\u00b7the\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Gn\u00fcgt ein lauer Fr\u00fchlingsregen, frische Luft und Morgenthau!", "tokens": ["Gn\u00fcgt", "ein", "lau\u00b7er", "Fr\u00fch\u00b7lings\u00b7re\u00b7gen", ",", "fri\u00b7sche", "Luft", "und", "Mor\u00b7gent\u00b7hau", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "F\u00fcrchtet nicht die edle G\u00e4hrung; g\u00e4hrt ja doch auch unser Wein,", "tokens": ["F\u00fcrch\u00b7tet", "nicht", "die", "ed\u00b7le", "G\u00e4h\u00b7rung", ";", "g\u00e4hrt", "ja", "doch", "auch", "un\u00b7ser", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$.", "VVFIN", "ADV", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Da\u00df er zwiefach dann erquicke, doppelt golden, s\u00fc\u00df und rein!", "tokens": ["Da\u00df", "er", "zwie\u00b7fach", "dann", "er\u00b7qui\u00b7cke", ",", "dop\u00b7pelt", "gol\u00b7den", ",", "s\u00fc\u00df", "und", "rein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "VVFIN", "$,", "ADJD", "ADJA", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Nicht das Schwert sei unsre Waffe, nein, das Wort, Licht und Gesetz!", "tokens": ["Nicht", "das", "Schwert", "sei", "uns\u00b7re", "Waf\u00b7fe", ",", "nein", ",", "das", "Wort", ",", "Licht", "und", "Ge\u00b7setz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PTKANT", "$,", "ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-++--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.2": {"text": "Denn der fr\u00f6hlich heitre Sieger ist der sch\u00f6nste Sieger stets!", "tokens": ["Denn", "der", "fr\u00f6h\u00b7lich", "heit\u00b7re", "Sie\u00b7ger", "ist", "der", "sch\u00f6ns\u00b7te", "Sie\u00b7ger", "stets", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Seht den Lenz, den Freiheitshelden, lernt von ihm es, wie man siegt,", "tokens": ["Seht", "den", "Lenz", ",", "den", "Frei\u00b7heits\u00b7hel\u00b7den", ",", "lernt", "von", "ihm", "es", ",", "wie", "man", "siegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "APPR", "PPER", "PPER", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wenn mit dem Tyrannen Winter er im harten Kampfe liegt!", "tokens": ["Wenn", "mit", "dem", "Ty\u00b7ran\u00b7nen", "Win\u00b7ter", "er", "im", "har\u00b7ten", "Kamp\u00b7fe", "liegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.8": {"line.1": {"text": "Winter ist ein Erzdespote, ein gar arger Obscurant,", "tokens": ["Win\u00b7ter", "ist", "ein", "Erz\u00b7des\u00b7po\u00b7te", ",", "ein", "gar", "ar\u00b7ger", "Ob\u00b7scu\u00b7rant", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Denn in seine langen N\u00e4chte h\u00fcllt' er ewig gern das Land;", "tokens": ["Denn", "in", "sei\u00b7ne", "lan\u00b7gen", "N\u00e4ch\u00b7te", "h\u00fcllt'", "er", "e\u00b7wig", "gern", "das", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Winter ist ein arger Zwingherr; in den eis'gen Fesseln fest", "tokens": ["Win\u00b7ter", "ist", "ein", "ar\u00b7ger", "Zwing\u00b7herr", ";", "in", "den", "eis'\u00b7gen", "Fes\u00b7seln", "fest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$.", "APPR", "ART", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+---+-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "H\u00e4lt des Lebens freiheitlust'ge frische Quellen er gepre\u00dft.", "tokens": ["H\u00e4lt", "des", "Le\u00b7bens", "frei\u00b7heit\u00b7lust'\u00b7ge", "fri\u00b7sche", "Quel\u00b7len", "er", "ge\u00b7pre\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Sieh, im Lager \u00fcberrumpelt hat den tr\u00e4gen Alten schnell", "tokens": ["Sieh", ",", "im", "La\u00b7ger", "\u00fc\u00b7berr\u00b7um\u00b7pelt", "hat", "den", "tr\u00e4\u00b7gen", "Al\u00b7ten", "schnell"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPRART", "NN", "VVPP", "VAFIN", "ART", "ADJA", "NN", "ADJD"], "meter": "+-+-+--++-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Jetzt mit seinem ganzen Heere Lenz, der fr\u00f6hliche Rebell!", "tokens": ["Jetzt", "mit", "sei\u00b7nem", "gan\u00b7zen", "Hee\u00b7re", "Lenz", ",", "der", "fr\u00f6h\u00b7li\u00b7che", "Re\u00b7bell", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Sonnenstrahlen seine Schwerter, gr\u00fcne Halme seine Speer'!", "tokens": ["Son\u00b7nen\u00b7strah\u00b7len", "sei\u00b7ne", "Schwer\u00b7ter", ",", "gr\u00fc\u00b7ne", "Hal\u00b7me", "sei\u00b7ne", "Speer'", "