{"textgrid.poem.25847": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[graue Engel gehen um mich]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Graue Engel gehen um mich,", "tokens": ["Grau\u00b7e", "En\u00b7gel", "ge\u00b7hen", "um", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sehen trauernd auf dich, meine Seele,", "tokens": ["Se\u00b7hen", "trau\u00b7ernd", "auf", "dich", ",", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sie stehen mit lahmen Fl\u00fcgeln", "tokens": ["Sie", "ste\u00b7hen", "mit", "lah\u00b7men", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "An Aschenh\u00fcgeln und sinnen;", "tokens": ["An", "A\u00b7schen\u00b7h\u00fc\u00b7geln", "und", "sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Drau\u00dfen und drinnen ist es Abend, meine Seele.", "tokens": ["Drau\u00b7\u00dfen", "und", "drin\u00b7nen", "ist", "es", "A\u00b7bend", ",", "mei\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VAFIN", "PPER", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}