{"textgrid.poem.35156": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Die Ehe", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Betrachte dich, und werde, was du bist!", "tokens": ["Be\u00b7trach\u00b7te", "dich", ",", "und", "wer\u00b7de", ",", "was", "du", "bist", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VAFIN", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Mann bist du, und hasts doch erst zu werden.", "tokens": ["Ein", "Mann", "bist", "du", ",", "und", "hasts", "doch", "erst", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "KON", "VAFIN", "ADV", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wei\u00dft du vielleicht, was an dir m\u00e4nnlich ist?", "tokens": ["Wei\u00dft", "du", "viel\u00b7leicht", ",", "was", "an", "dir", "m\u00e4nn\u00b7lich", "ist", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PRELS", "APPR", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der K\u00f6rper, die Bewegung, die Geberden.", "tokens": ["Der", "K\u00f6r\u00b7per", ",", "die", "Be\u00b7we\u00b7gung", ",", "die", "Ge\u00b7ber\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du bist so ernst, energisch, alles Das,", "tokens": ["Du", "bist", "so", "ernst", ",", "e\u00b7ner\u00b7gisch", ",", "al\u00b7les", "Das", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADJD", "$,", "PIS", "PDS", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Was man am Manne lobt, wenn man es findet,", "tokens": ["Was", "man", "am", "Man\u00b7ne", "lobt", ",", "wenn", "man", "es", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPRART", "NN", "VVFIN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wei\u00dft auch leicht zu \u00fcberwinden, was", "tokens": ["Und", "wei\u00dft", "auch", "leicht", "zu", "\u00fc\u00b7berw\u00b7in\u00b7den", ",", "was"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$,", "PWS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein Anderer nur m\u00fchsam \u00fcberwindet.", "tokens": ["Ein", "An\u00b7de\u00b7rer", "nur", "m\u00fch\u00b7sam", "\u00fc\u00b7berw\u00b7in\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und doch, und doch \u2013 \u2013 o s\u00e4hest du es ein! \u2013 \u2013", "tokens": ["Und", "doch", ",", "und", "doch", "\u2013", "\u2013", "o", "s\u00e4\u00b7hest", "du", "es", "ein", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ADV", "$,", "KON", "ADV", "$(", "$(", "FM", "VVFIN", "PPER", "PPER", "PTKVZ", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Bist du noch weit entfernt, ein Mann zu sein.", "tokens": ["Bist", "du", "noch", "weit", "ent\u00b7fernt", ",", "ein", "Mann", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Als Mann ererbtest du die heilge Pflicht,", "tokens": ["Als", "Mann", "er\u00b7erb\u00b7test", "du", "die", "heil\u00b7ge", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu suchen, was der erste Mann verloren,", "tokens": ["Zu", "su\u00b7chen", ",", "was", "der", "ers\u00b7te", "Mann", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Paradies, und findest du es nicht,", "tokens": ["Das", "Pa\u00b7ra\u00b7dies", ",", "und", "fin\u00b7dest", "du", "es", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "PPER", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wurdest du f\u00fcr hier umsonst geboren.", "tokens": ["So", "wur\u00b7dest", "du", "f\u00fcr", "hier", "um\u00b7sonst", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denk dich als Den, der aus dem Eden ging,", "tokens": ["Denk", "dich", "als", "Den", ",", "der", "aus", "dem", "E\u00b7den", "ging", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KOUS", "NE", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und sinne nach, so wirst du dich erinnern.", "tokens": ["Und", "sin\u00b7ne", "nach", ",", "so", "wirst", "du", "dich", "e\u00b7rin\u00b7nern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Such nach der Heimath, die dich einst umfing;", "tokens": ["Such", "nach", "der", "Hei\u00b7math", ",", "die", "dich", "einst", "um\u00b7fing", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den Schl\u00fcssel tr\u00e4gst du stets in deinem Innern.", "tokens": ["Den", "Schl\u00fcs\u00b7sel", "tr\u00e4gst", "du", "stets", "in", "dei\u00b7nem", "In\u00b7nern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Liebst du dein Weib, so f\u00fchrs dort wieder ein;", "tokens": ["Liebst", "du", "dein", "Weib", ",", "so", "f\u00fchrs", "dort", "wie\u00b7der", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Dem wahren Manne wird es m\u00f6glich sein.", "tokens": ["Dem", "wah\u00b7ren", "Man\u00b7ne", "wird", "es", "m\u00f6g\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Betrachte dich, und werde, was du bist!", "tokens": ["Be\u00b7trach\u00b7te", "dich", ",", "und", "wer\u00b7de", ",", "was", "du", "bist", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VAFIN", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Weib bist du, und hasts doch erst zu werden.", "tokens": ["Ein", "Weib", "bist", "du", ",", "und", "hasts", "doch", "erst", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "KON", "VAFIN", "ADV", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wei\u00dft du vielleicht, was an dir weiblich ist?", "tokens": ["Wei\u00dft", "du", "viel\u00b7leicht", ",", "was", "an", "dir", "weib\u00b7lich", "ist", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PRELS", "APPR", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der K\u00f6rper, die Bewegung, die Geberden,", "tokens": ["Der", "K\u00f6r\u00b7per", ",", "die", "Be\u00b7we\u00b7gung", ",", "die", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du bist so fromm, vers\u00f6hnlich, mild und zart;", "tokens": ["Du", "bist", "so", "fromm", ",", "ver\u00b7s\u00f6hn\u00b7lich", ",", "mild", "und", "zart", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du liebst die Deinen, wie nur Frauen lieben.", "tokens": ["Du", "liebst", "die", "Dei\u00b7nen", ",", "wie", "nur", "Frau\u00b7en", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du warst als Kind von guter Kinder Art", "tokens": ["Du", "warst", "als", "Kind", "von", "gu\u00b7ter", "Kin\u00b7der", "Art"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOUS", "NN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und bist so herzig, wie du warst, geblieben.", "tokens": ["Und", "bist", "so", "her\u00b7zig", ",", "wie", "du", "warst", ",", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "$,", "PWAV", "PPER", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und doch und doch \u2013 \u2013 o s\u00e4hest du es ein! \u2013 \u2013", "tokens": ["Und", "doch", "und", "doch", "\u2013", "\u2013", "o", "s\u00e4\u00b7hest", "du", "es", "ein", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "$(", "$(", "FM", "VVFIN", "PPER", "PPER", "PTKVZ", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Bist du noch weit entfernt, ein Weib zu sein.", "tokens": ["Bist", "du", "noch", "weit", "ent\u00b7fernt", ",", "ein", "Weib", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Als Weib ererbtest du die heilge Pflicht,", "tokens": ["Als", "Weib", "er\u00b7erb\u00b7test", "du", "die", "heil\u00b7ge", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu suchen, was das erste Weib verloren.", "tokens": ["Zu", "su\u00b7chen", ",", "was", "das", "ers\u00b7te", "Weib", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Paradies, und findest du es nicht,", "tokens": ["Das", "Pa\u00b7ra\u00b7dies", ",", "und", "fin\u00b7dest", "du", "es", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "PPER", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So bist und hast du hier umsonst geboren.", "tokens": ["So", "bist", "und", "hast", "du", "hier", "um\u00b7sonst", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "KON", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denk, du seist Die, die einst der Herr verstie\u00df,", "tokens": ["Denk", ",", "du", "seist", "Die", ",", "die", "einst", "der", "Herr", "ver\u00b7stie\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ART", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Weil sie die Himmelsliebe nicht verstanden.", "tokens": ["Weil", "sie", "die", "Him\u00b7mels\u00b7lie\u00b7be", "nicht", "ver\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Such nach der Heimath, nach dem Paradies;", "tokens": ["Such", "nach", "der", "Hei\u00b7math", ",", "nach", "dem", "Pa\u00b7ra\u00b7dies", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Es bleibt der Liebe ewig zugestanden.", "tokens": ["Es", "bleibt", "der", "Lie\u00b7be", "e\u00b7wig", "zu\u00b7ge\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Den Mann, das Kind, f\u00fchr sie dort mit dir ein;", "tokens": ["Den", "Mann", ",", "das", "Kind", ",", "f\u00fchr", "sie", "dort", "mit", "dir", "ein", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Dem wahren Weibe wird es m\u00f6glich sein.", "tokens": ["Dem", "wah\u00b7ren", "Wei\u00b7be", "wird", "es", "m\u00f6g\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Betrachtet euch, und werdet, was ihr seid!", "tokens": ["Be\u00b7trach\u00b7tet", "euch", ",", "und", "wer\u00b7det", ",", "was", "ihr", "seid", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VAFIN", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ja, ihr seid Mann und Weib; ich h\u00f6rs euch sagen.", "tokens": ["Ja", ",", "ihr", "seid", "Mann", "und", "Weib", ";", "ich", "h\u00f6rs", "euch", "sa\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "NN", "KON", "NN", "$.", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das hei\u00dft, ihr seids geworden f\u00fcr die Zeit,", "tokens": ["Das", "hei\u00dft", ",", "ihr", "seids", "ge\u00b7wor\u00b7den", "f\u00fcr", "die", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VAFIN", "VAPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In welcher euch die Erdenstunden schlagen.", "tokens": ["In", "wel\u00b7cher", "euch", "die", "Er\u00b7dens\u00b7tun\u00b7den", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wer als Christ sich zeigen will, der spricht:", "tokens": ["Und", "wer", "als", "Christ", "sich", "zei\u00b7gen", "will", ",", "der", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "KOUS", "NN", "PRF", "VVINF", "VMFIN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den Bund der Herzen trennen selbst die Schauer", "tokens": ["Den", "Bund", "der", "Her\u00b7zen", "tren\u00b7nen", "selbst", "die", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Des Todes und des offnen Grabes nicht;", "tokens": ["Des", "To\u00b7des", "und", "des", "off\u00b7nen", "Gra\u00b7bes", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Er ward geweiht und ist von ewger Dauer.", "tokens": ["Er", "ward", "ge\u00b7weiht", "und", "ist", "von", "ew\u00b7ger", "Dau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und doch, und doch \u2013 \u2013 o s\u00e4het ihr es ein! \u2013 \u2013", "tokens": ["Und", "doch", ",", "und", "doch", "\u2013", "\u2013", "o", "s\u00e4\u00b7het", "ihr", "es", "ein", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "w