{"dta.poem.17093": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "194.  Die Wei\u00dfheit ist das beste Weib.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Begehrestu ein Weib/ die pr\u00e4chtig reich und fein:", "tokens": ["Be\u00b7geh\u00b7res\u00b7tu", "ein", "Weib", "/", "die", "pr\u00e4ch\u00b7tig", "reich", "und", "fein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$(", "ART", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So nimb die Wei\u00dfheit nur sie wird dir alles seyn.", "tokens": ["So", "nimb", "die", "Wei\u00df\u00b7heit", "nur", "sie", "wird", "dir", "al\u00b7les", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PPER", "VAFIN", "PPER", "PIS", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}