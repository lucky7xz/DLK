{"textgrid.poem.67010": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dem edlen Geist! Er kam, in Ha\u00dfgewalten", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem edlen Geist! Er kam, in Ha\u00dfgewalten", "tokens": ["Dem", "ed\u00b7len", "Geist", "!", "Er", "kam", ",", "in", "Ha\u00df\u00b7ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Lebens Heiligt\u00fcmer zu erhalten.", "tokens": ["Des", "Le\u00b7bens", "Hei\u00b7lig\u00b7t\u00fc\u00b7mer", "zu", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er mu\u00dfte gehn vor seines Werkes Zeit,", "tokens": ["Er", "mu\u00df\u00b7te", "gehn", "vor", "sei\u00b7nes", "Wer\u00b7kes", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wir bleiben treu dem tiefen Ruf geweiht:", "tokens": ["Wir", "blei\u00b7ben", "treu", "dem", "tie\u00b7fen", "Ruf", "ge\u00b7weiht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des Lebens Pole, die sich feindlich trennen,", "tokens": ["Des", "Le\u00b7bens", "Po\u00b7le", ",", "die", "sich", "feind\u00b7lich", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu einigen in liebendem Erkennen.", "tokens": ["Zu", "ei\u00b7ni\u00b7gen", "in", "lie\u00b7ben\u00b7dem", "Er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}}}}