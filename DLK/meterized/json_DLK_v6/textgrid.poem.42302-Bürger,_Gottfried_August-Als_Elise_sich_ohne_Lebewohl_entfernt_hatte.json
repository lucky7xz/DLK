{"textgrid.poem.42302": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Als Elise sich ohne Lebewohl entfernt hatte", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frisch, B\u00fcrger, frisch zusammen dich genommen,", "tokens": ["Frisch", ",", "B\u00fcr\u00b7ger", ",", "frisch", "zu\u00b7sam\u00b7men", "dich", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADJD", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und r\u00fcstig vorw\u00e4rts stets von hier", "tokens": ["Und", "r\u00fcs\u00b7tig", "vor\u00b7w\u00e4rts", "stets", "von", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "ADV", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Ozean der Zeiten fortgeschwommen! \u2013", "tokens": ["Im", "O\u00b7ze\u00b7an", "der", "Zei\u00b7ten", "fort\u00b7ge\u00b7schwom\u00b7men", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie ist nicht fort, das glaube mir! \u2013", "tokens": ["Sie", "ist", "nicht", "fort", ",", "das", "glau\u00b7be", "mir", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PTKVZ", "$,", "PDS", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Steh nicht so d\u00fcster, so beklommen,", "tokens": ["Steh", "nicht", "so", "d\u00fcs\u00b7ter", ",", "so", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht so an Hoffnung, Mut und Lebenskraft verglommen!", "tokens": ["Nicht", "so", "an", "Hoff\u00b7nung", ",", "Mut", "und", "Le\u00b7bens\u00b7kraft", "ver\u00b7glom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie wird gewi\u00df noch irgendwo zu dir,", "tokens": ["Sie", "wird", "ge\u00b7wi\u00df", "noch", "ir\u00b7gend\u00b7wo", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Du wirst gewi\u00df noch irgendwo zu Ihr,", "tokens": ["Du", "wirst", "ge\u00b7wi\u00df", "noch", "ir\u00b7gend\u00b7wo", "zu", "Ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Auf einem Freudenfest der Edlen und der Frommen,", "tokens": ["Auf", "ei\u00b7nem", "Freu\u00b7den\u00b7fest", "der", "Ed\u00b7len", "und", "der", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wer wei\u00df an welcher Quelle, kommen.", "tokens": ["Wer", "wei\u00df", "an", "wel\u00b7cher", "Quel\u00b7le", ",", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PWAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Im Engelston gebot Sie dir:", "tokens": ["Im", "En\u00b7gel\u00b7ston", "ge\u00b7bot", "Sie", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "\u00bbsteh nicht so d\u00fcster, so beklommen!\u00ab \u2013", "tokens": ["\u00bb", "steh", "nicht", "so", "d\u00fcs\u00b7ter", ",", "so", "be\u00b7klom\u00b7men", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "ADV", "ADJD", "$,", "ADV", "ADJD", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sie ist nicht fort, das glaube mir!", "tokens": ["Sie", "ist", "nicht", "fort", ",", "das", "glau\u00b7be", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PTKVZ", "$,", "PDS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Denn \u2013 Abschied hat sie nicht genommen.", "tokens": ["Denn", "\u2013", "Ab\u00b7schied", "hat", "sie", "nicht", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}