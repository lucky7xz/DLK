{"dta.poem.21825": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Vi.  \n  Uber der Liebsten Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Ofnet euch/ ihr Augen-g\u00fcsse/", "tokens": ["Of\u00b7net", "euch", "/", "ihr", "Au\u00b7gen\u00b7g\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "tr\u00e4hnet Blut-gef\u00e4rbte Fl\u00fcsse/", "tokens": ["tr\u00e4h\u00b7net", "Blut\u00b7ge\u00b7f\u00e4rb\u00b7te", "Fl\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "klagt/ beweinet/ was ich misse!", "tokens": ["klagt", "/", "be\u00b7wei\u00b7net", "/", "was", "ich", "mis\u00b7se", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "$(", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Freud\u2019 ach! ist verblichen.", "tokens": ["Mei\u00b7ne", "Freud'", "ach", "!", "ist", "ver\u00b7bli\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ITJ", "$.", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Helfft/ ihr G\u00f6tter meiner Noht!", "tokens": ["Helfft", "/", "ihr", "G\u00f6t\u00b7ter", "mei\u00b7ner", "Noht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sch\u00f6nheit/ Tugend/ Zucht ist tod", "tokens": ["Sch\u00f6n\u00b7heit", "/", "Tu\u00b7gend", "/", "Zucht", "ist", "tod"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "NN", "$(", "NN", "VAFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "und nach Leten hingewichen.", "tokens": ["und", "nach", "Le\u00b7ten", "hin\u00b7ge\u00b7wi\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Rauffet euch/ Jhr Nymfen-schaaren/", "tokens": ["Rauf\u00b7fet", "euch", "/", "Ihr", "Nym\u00b7fen\u00b7schaa\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ungemenschet in den Haaren/", "tokens": ["un\u00b7ge\u00b7men\u00b7schet", "in", "den", "Haa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "heulet bey der Leichen-Bahren/", "tokens": ["heu\u00b7let", "bey", "der", "Lei\u00b7chen\u00b7Bah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "h\u00fcllet euer Angesichte", "tokens": ["h\u00fcl\u00b7let", "eu\u00b7er", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine G\u00f6ttin ist zu nichte.", "tokens": ["Mei\u00b7ne", "G\u00f6t\u00b7tin", "ist", "zu", "nich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKA", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Amor/ lesche deine Flammen/", "tokens": ["A\u00b7mor", "/", "le\u00b7sche", "dei\u00b7ne", "Flam\u00b7men", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "tritt mit Zyprien zusammen", "tokens": ["tritt", "mit", "Zyp\u00b7rien", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NE", "PTKVZ"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "alle Wollust zu verdammen/", "tokens": ["al\u00b7le", "Wol\u00b7lust", "zu", "ver\u00b7dam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "weil das Bild der Treffligkeiten", "tokens": ["weil", "das", "Bild", "der", "Tref\u00b7flig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "deiner Fakkel wehrter Zwekk", "tokens": ["dei\u00b7ner", "Fak\u00b7kel", "wehr\u00b7ter", "Zwekk"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "nu ist au\u00df dem Leben weg", "tokens": ["nu", "ist", "au\u00df", "dem", "Le\u00b7ben", "weg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "und mit ihr der Trost der Zeiten.", "tokens": ["und", "mit", "ihr", "der", "Trost", "der", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Pfl\u00fckkt/ ihr Musen/ um Permessen", "tokens": ["Pfl\u00fckkt", "/", "ihr", "Mu\u00b7sen", "/", "um", "Per\u00b7mes\u00b7sen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$(", "PPOSAT", "NN", "$(", "APPR", "NN"], "meter": "+-+-+++-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Amaranten und Zypressen/", "tokens": ["A\u00b7ma\u00b7ran\u00b7ten", "und", "Zyp\u00b7res\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$("], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "die Melposens Z\u00e4hren n\u00e4ssen/", "tokens": ["die", "Mel\u00b7po\u00b7sens", "Z\u00e4h\u00b7ren", "n\u00e4s\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wimmert um Asopus Wellen", "tokens": ["wim\u00b7mert", "um", "A\u00b7so\u00b7pus", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NE", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "einen kl\u00e4glichen Gesang/", "tokens": ["ei\u00b7nen", "kl\u00e4g\u00b7li\u00b7chen", "Ge\u00b7sang", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df der raue Jammer-klang", "tokens": ["da\u00df", "der", "rau\u00b7e", "Jam\u00b7mer\u00b7klang"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "m\u00f6g\u2019 an Teben wieder gellen.", "tokens": ["m\u00f6g'", "an", "Te\u00b7ben", "wie\u00b7der", "gel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Brecht/ ihr Wolken/ donnert/ sch\u00fczzet/", "tokens": ["Brecht", "/", "ihr", "Wol\u00b7ken", "/", "don\u00b7nert", "/", "sch\u00fcz\u00b7zet", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "PPOSAT", "NN", "$(", "VVFIN", "$(", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "weil mein Nord-stern ist verblizzet.", "tokens": ["weil", "mein", "Nord\u00b7stern", "ist", "ver\u00b7bliz\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du verg\u00f6ldtes Radt der Sonnen/", "tokens": ["Du", "ver\u00b7g\u00f6ld\u00b7tes", "Radt", "der", "Son\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dunkle deiner Reise Bahn/", "tokens": ["dunk\u00b7le", "dei\u00b7ner", "Rei\u00b7se", "Bahn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "ziehe schwarze Kleider an/", "tokens": ["zie\u00b7he", "schwar\u00b7ze", "Klei\u00b7der", "an", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lune/ weil mein Licht verbronnen.", "tokens": ["Lu\u00b7ne", "/", "weil", "mein", "Licht", "ver\u00b7bron\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Pfeifft erb\u00e4rmlich/ Lufft und Winde/", "tokens": ["Pfeifft", "er\u00b7b\u00e4rm\u00b7lich", "/", "Lufft", "und", "Win\u00b7de", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$(", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Echo \u00e4chz es in die Gr\u00fcnde/", "tokens": ["E\u00b7cho", "\u00e4chz", "es", "in", "die", "Gr\u00fcn\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wo ich mich verzweifelt finde", "tokens": ["wo", "ich", "mich", "ver\u00b7zwei\u00b7felt", "fin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "diese Faust ist schon ger\u00fcstet", "tokens": ["die\u00b7se", "Faust", "ist", "schon", "ge\u00b7r\u00fcs\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "mir zutuhn den lezten Sto\u00df.", "tokens": ["mir", "zu\u00b7tuhn", "den", "lez\u00b7ten", "Sto\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine Marter ist zu gro\u00df/", "tokens": ["Mei\u00b7ne", "Mar\u00b7ter", "ist", "zu", "gro\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "da\u00df mich nicht zuleben l\u00fcstet.", "tokens": ["da\u00df", "mich", "nicht", "zu\u00b7le\u00b7ben", "l\u00fcs\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Hohlt mich ab/ ihr junge Hirten/", "tokens": ["Hohlt", "mich", "ab", "/", "ihr", "jun\u00b7ge", "Hir\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "beyde soll ein Grab bewirten.", "tokens": ["bey\u00b7de", "soll", "ein", "Grab", "be\u00b7wir\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leget uns in gr\u00fcne Myrten.", "tokens": ["Le\u00b7get", "uns", "in", "gr\u00fc\u00b7ne", "Myr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die das Leben nie geschieden/", "tokens": ["die", "das", "Le\u00b7ben", "nie", "ge\u00b7schie\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "trennet auch die lezte Pflicht", "tokens": ["tren\u00b7net", "auch", "die", "lez\u00b7te", "Pflicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "und der Ri\u00df der Parzen nicht/", "tokens": ["und", "der", "Ri\u00df", "der", "Par\u00b7zen", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "der sonst alles kan zerglieden.", "tokens": ["der", "sonst", "al\u00b7les", "kan", "zer\u00b7glie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Brechet au\u00df den Marmor-steinen", "tokens": ["Bre\u00b7chet", "au\u00df", "den", "Mar\u00b7mor\u00b7stei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "von den allerreinsten einen/", "tokens": ["von", "den", "al\u00b7ler\u00b7reins\u00b7ten", "ei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ART", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "drauff soll diese Schrifft erscheinen", "tokens": ["drauff", "soll", "die\u00b7se", "Schrifft", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PDAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die im Leben treu verharret", "tokens": ["Die", "im", "Le\u00b7ben", "treu", "ver\u00b7har\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach", "tokens": ["Ach"], "token_info": ["word"], "pos": ["ITJ"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Amor selber eingescharret.", "tokens": ["A\u00b7mor", "sel\u00b7ber", "ein\u00b7ge\u00b7schar\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}