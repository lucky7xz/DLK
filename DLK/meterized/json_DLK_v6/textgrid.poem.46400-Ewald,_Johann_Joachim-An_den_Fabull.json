{"textgrid.poem.46400": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "An den Fabull", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du spottest \u00fcber den Voltair", "tokens": ["Du", "spot\u00b7test", "\u00fc\u00b7ber", "den", "Vol\u00b7tair"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bist von aller Einsicht leer?", "tokens": ["Und", "bist", "von", "al\u00b7ler", "Ein\u00b7sicht", "leer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ein Voltair dein Lob entbehrt,", "tokens": ["Da\u00df", "ein", "Vol\u00b7tair", "dein", "Lob", "ent\u00b7behrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Fabull! ist er vollkommen werth.", "tokens": ["Fa\u00b7bull", "!", "ist", "er", "voll\u00b7kom\u00b7men", "werth", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VAFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}