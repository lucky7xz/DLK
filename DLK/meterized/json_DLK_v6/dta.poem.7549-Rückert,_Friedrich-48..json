{"dta.poem.7549": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wol sein Erkenntniskreis ist jedem Geist bestimmt,", "tokens": ["Wol", "sein", "Er\u00b7kennt\u00b7nis\u00b7kreis", "ist", "je\u00b7dem", "Geist", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Doch unbestimmt, wieviel er in den Kreis aufnimmt.", "tokens": ["Doch", "un\u00b7be\u00b7stimmt", ",", "wie\u00b7viel", "er", "in", "den", "Kreis", "auf\u00b7nimmt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du kannst das Zimmer nicht, in dem du wohnst, erweitern,", "tokens": ["Du", "kannst", "das", "Zim\u00b7mer", "nicht", ",", "in", "dem", "du", "wohnst", ",", "er\u00b7wei\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "$,", "APPR", "PRELS", "PPER", "ADV", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch es nach deiner Lust ausschm\u00fccken und erheitern.", "tokens": ["Doch", "es", "nach", "dei\u00b7ner", "Lust", "aus\u00b7schm\u00fc\u00b7cken", "und", "er\u00b7hei\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich blicke stets umher, was ich noch ohne Schaden", "tokens": ["Ich", "bli\u00b7cke", "stets", "um\u00b7her", ",", "was", "ich", "noch", "oh\u00b7ne", "Scha\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "PWS", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der innern Einheit kann in meine Kreise laden.", "tokens": ["Der", "in\u00b7nern", "Ein\u00b7heit", "kann", "in", "mei\u00b7ne", "Krei\u00b7se", "la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der engste Raum ist weit dem was kein Raum umkreist,", "tokens": ["Der", "engs\u00b7te", "Raum", "ist", "weit", "dem", "was", "kein", "Raum", "um\u00b7kreist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "ART", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alle Geister sind zu Gaste gern beim Geist.", "tokens": ["Und", "al\u00b7le", "Geis\u00b7ter", "sind", "zu", "Gas\u00b7te", "gern", "beim", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "APPR", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}