{"textgrid.poem.55641": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es geht eins nach dem andern hin,", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es geht eins nach dem andern hin,", "tokens": ["Es", "geht", "eins", "nach", "dem", "an\u00b7dern", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und auch wohl vor dem andern;", "tokens": ["Und", "auch", "wohl", "vor", "dem", "an\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ART", "ADJA", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Drum la\u00dft uns rasch und brav und k\u00fchn", "tokens": ["Drum", "la\u00dft", "uns", "rasch", "und", "brav", "und", "k\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lebenswege wandern.", "tokens": ["Die", "Le\u00b7bens\u00b7we\u00b7ge", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es h\u00e4lt dich auf, mit Seitenblick,", "tokens": ["Es", "h\u00e4lt", "dich", "auf", ",", "mit", "Sei\u00b7ten\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Blumen viel zu lesen;", "tokens": ["Der", "Blu\u00b7men", "viel", "zu", "le\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch h\u00e4lt nichts grimmiger zur\u00fcck,", "tokens": ["Doch", "h\u00e4lt", "nichts", "grim\u00b7mi\u00b7ger", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als wenn du falsch gewesen.", "tokens": ["Als", "wenn", "du", "falsch", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}