{"textgrid.poem.67561": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Klopstock's lyrische Poesie", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stets die bl\u00fchende, zarte Blume,", "tokens": ["Stets", "die", "bl\u00fc\u00b7hen\u00b7de", ",", "zar\u00b7te", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wo sie bl\u00fche! die m\u00e4cht' gen Schauer Gottes", "tokens": ["Wo", "sie", "bl\u00fc\u00b7he", "!", "die", "m\u00e4cht'", "gen", "Schau\u00b7er", "Got\u00b7tes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "ART", "NN", "APPR", "NE", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Tragend, weckend im Hauch der Allerf\u00fcllung Gottes", "tokens": ["Tra\u00b7gend", ",", "we\u00b7ckend", "im", "Hauch", "der", "Al\u00b7ler\u00b7f\u00fcl\u00b7lung", "Got\u00b7tes"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "APPRART", "NN", "ART", "NN", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Ihren lispelnden Goldklang,", "tokens": ["Ih\u00b7ren", "lis\u00b7peln\u00b7den", "Gold\u00b7klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Oder wilder im Bardenhaine", "tokens": ["O\u00b7der", "wil\u00b7der", "im", "Bar\u00b7den\u00b7hai\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "APPRART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Rauschend, Tochter der edlen Mutter Hertha,", "tokens": ["Rau\u00b7schend", ",", "Toch\u00b7ter", "der", "ed\u00b7len", "Mut\u00b7ter", "Her\u00b7tha", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "NN", "ART", "ADJA", "NN", "NE", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Stolze Tochter, im Sturm des fernen V\u00e4terhochsangs", "tokens": ["Stol\u00b7ze", "Toch\u00b7ter", ",", "im", "Sturm", "des", "fer\u00b7nen", "V\u00e4\u00b7ter\u00b7hoch\u00b7sangs"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Roth und stolzer erzogen;", "tokens": ["Roth", "und", "stol\u00b7zer", "er\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Lieblich immer. O aber Blume,", "tokens": ["Lieb\u00b7lich", "im\u00b7mer", ".", "O", "a\u00b7ber", "Blu\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "NE", "KON", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Du mir lieblicher, mir, der Morgensonne", "tokens": ["Du", "mir", "lieb\u00b7li\u00b7cher", ",", "mir", ",", "der", "Mor\u00b7gen\u00b7son\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "PPER", "ADJD", "$,", "PPER", "$,", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Strahlenharfe, den Himmel t\u00f6nend, wenn an Deines", "tokens": ["Strah\u00b7len\u00b7har\u00b7fe", ",", "den", "Him\u00b7mel", "t\u00f6\u00b7nend", ",", "wenn", "an", "Dei\u00b7nes"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "VVPP", "$,", "KOUS", "APPR", "PPOSAT"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "M\u00e4dchens Krone Du bl\u00fchest,", "tokens": ["M\u00e4d\u00b7chens", "Kro\u00b7ne", "Du", "bl\u00fc\u00b7hest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Ihr am Busen, o Engel, Engel!", "tokens": ["Ihr", "am", "Bu\u00b7sen", ",", "o", "En\u00b7gel", ",", "En\u00b7gel", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "FM", "NE", "$,", "NE", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Welche Seele! Die zartsten, unbewegtsten", "tokens": ["Wel\u00b7che", "See\u00b7le", "!", "Die", "zarts\u00b7ten", ",", "un\u00b7be\u00b7wegt\u00b7sten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAT", "NN", "$.", "ART", "ADJA", "$,", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Saiten klingen! die Kelche duften! Hundert Farben", "tokens": ["Sai\u00b7ten", "klin\u00b7gen", "!", "die", "Kel\u00b7che", "duf\u00b7ten", "!", "Hun\u00b7dert", "Far\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "$.", "ART", "NN", "VVFIN", "$.", "CARD", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Kleiden sch\u00f6ner die Braut an,", "tokens": ["Klei\u00b7den", "sch\u00f6\u00b7ner", "die", "Braut", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}}, "stanza.5": {"line.1": {"text": "Edens Wohnerin, Blumenseele.", "tokens": ["E\u00b7dens", "Woh\u00b7ne\u00b7rin", ",", "Blu\u00b7men\u00b7see\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und sie starb, der Du bl\u00fchetest! da sankest", "tokens": ["Und", "sie", "starb", ",", "der", "Du", "bl\u00fc\u00b7he\u00b7test", "!", "da", "san\u00b7kest"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$.", "ADV", "VVFIN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Du auch nieder, ert\u00f6ntest Todeslaut, bis wieder", "tokens": ["Du", "auch", "nie\u00b7der", ",", "er\u00b7t\u00f6n\u00b7test", "To\u00b7des\u00b7laut", ",", "bis", "wie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "PTKVZ", "$,", "VVFIN", "NN", "$,", "KOUS", "ADV"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Dir \u2013 welch anderer Trost kam!", "tokens": ["Dir", "\u2013", "welch", "an\u00b7de\u00b7rer", "Trost", "kam", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PWAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "T\u00e4uscht Dich noch, und die Bardensprache t\u00f6nt Dir", "tokens": ["T\u00e4uscht", "Dich", "noch", ",", "und", "die", "Bar\u00b7den\u00b7spra\u00b7che", "t\u00f6nt", "Dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Bardesang! und erfindest ihm Stimmen! Hermann's Barden,", "tokens": ["Bar\u00b7de\u00b7sang", "!", "und", "er\u00b7fin\u00b7dest", "ihm", "Stim\u00b7men", "!", "Her\u00b7mann's", "Bar\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "VVFIN", "PPER", "NN", "$.", "NE", "NN", "$,"], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wie sie edel Dir singen!", "tokens": ["Wie", "sie", "e\u00b7del", "Dir", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.7": {"line.1": {"text": "Singe, t\u00e4usche Dich lang', entschlummre", "tokens": ["Sin\u00b7ge", ",", "t\u00e4u\u00b7sche", "Dich", "lang'", ",", "ent\u00b7schlumm\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "$,", "VVFIN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Nie dem zweiten geliebten Wahnesbilde,", "tokens": ["Nie", "dem", "zwei\u00b7ten", "ge\u00b7lieb\u00b7ten", "Wah\u00b7nes\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sie Dir alle denn, Cidli?", "tokens": ["Sie", "Dir", "al\u00b7le", "denn", ",", "Cid\u00b7li", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PPER", "PIS", "ADV", "$,", "XY", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}