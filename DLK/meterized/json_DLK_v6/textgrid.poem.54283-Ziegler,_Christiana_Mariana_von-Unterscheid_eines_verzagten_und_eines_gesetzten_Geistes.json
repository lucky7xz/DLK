{"textgrid.poem.54283": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "Unterscheid eines verzagten und eines gesetzten Geistes", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein rauschend Blat vermag ein feiges Hertz zu schrecken,", "tokens": ["Ein", "rau\u00b7schend", "Blat", "ver\u00b7mag", "ein", "fei\u00b7ges", "Hertz", "zu", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ein gesetzter Geist sich nicht sucht zu verstecken,", "tokens": ["Da", "ein", "ge\u00b7setz\u00b7ter", "Geist", "sich", "nicht", "sucht", "zu", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PRF", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er wei\u00df, wann Donner, Blitz und Strahl vor\u00fcber seyn,", "tokens": ["Er", "wei\u00df", ",", "wann", "Don\u00b7ner", ",", "Blitz", "und", "Strahl", "vor\u00b7\u00fc\u00b7ber", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "$,", "NN", "KON", "NN", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So folget auch hernach ohnfehlbahr Sonnen-Schein.", "tokens": ["So", "fol\u00b7get", "auch", "her\u00b7nach", "ohn\u00b7fehl\u00b7bahr", "Son\u00b7nen\u00b7Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}