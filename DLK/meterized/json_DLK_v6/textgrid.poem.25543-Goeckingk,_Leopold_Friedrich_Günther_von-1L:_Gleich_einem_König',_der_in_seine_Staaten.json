{"textgrid.poem.25543": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gleich einem K\u00f6nig', der in seine Staaten", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleich einem K\u00f6nig', der in seine Staaten", "tokens": ["Gleich", "ei\u00b7nem", "K\u00f6\u00b7nig'", ",", "der", "in", "sei\u00b7ne", "Staa\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur\u00fcck als Sieger kehrt, empf\u00e4ngt ein Jubel dich!", "tokens": ["Zu\u00b7r\u00fcck", "als", "Sie\u00b7ger", "kehrt", ",", "emp\u00b7f\u00e4ngt", "ein", "Ju\u00b7bel", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Knabe balgt um deine Pflocken sich,", "tokens": ["Der", "Kna\u00b7be", "balgt", "um", "dei\u00b7ne", "Pflo\u00b7cken", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie bei der Kr\u00f6nung um Dukaten.", "tokens": ["Wie", "bei", "der", "Kr\u00f6\u00b7nung", "um", "Du\u00b7ka\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Selbst mir, obschon ein M\u00e4dchen, und der Ruthe", "tokens": ["Selbst", "mir", ",", "ob\u00b7schon", "ein", "M\u00e4d\u00b7chen", ",", "und", "der", "Ru\u00b7the"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "KOUS", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Lang' nicht mehr unterthan, bist du ein lieber Gast;", "tokens": ["Lang'", "nicht", "mehr", "un\u00b7ter\u00b7than", ",", "bist", "du", "ein", "lie\u00b7ber", "Gast", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "ADJD", "$,", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn siehst du nicht, seit du die Erde hast", "tokens": ["Denn", "siehst", "du", "nicht", ",", "seit", "du", "die", "Er\u00b7de", "hast"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So weich belegt, wie ich mich spute?", "tokens": ["So", "weich", "be\u00b7legt", ",", "wie", "ich", "mich", "spu\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Zu fahren, ohne Segel, ohne R\u00e4der,", "tokens": ["Zu", "fah\u00b7ren", ",", "oh\u00b7ne", "Se\u00b7gel", ",", "oh\u00b7ne", "R\u00e4\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUI", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Auf einer Muschel, hin durch deinen wei\u00dfen Flor,", "tokens": ["Auf", "ei\u00b7ner", "Mu\u00b7schel", ",", "hin", "durch", "dei\u00b7nen", "wei\u00b7\u00dfen", "Flor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So sanft, und doch so leicht, so schnell, wie vor", "tokens": ["So", "sanft", ",", "und", "doch", "so", "leicht", ",", "so", "schnell", ",", "wie", "vor"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KON", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "PWAV", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Dem Westwind' eine Pflaumenfeder.", "tokens": ["Dem", "West\u00b7wind'", "ei\u00b7ne", "Pflau\u00b7men\u00b7fe\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Aus allen Fenstern, und allen Th\u00fcren,", "tokens": ["Aus", "al\u00b7len", "Fens\u00b7tern", ",", "und", "al\u00b7len", "Th\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "KON", "PIAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Sieht mir der bleiche Neid aus hohlen Augen nach,", "tokens": ["Sieht", "mir", "der", "blei\u00b7che", "Neid", "aus", "hoh\u00b7len", "Au\u00b7gen", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Selbst die Matrone wird ein leises Ach!", "tokens": ["Selbst", "die", "Mat\u00b7ro\u00b7ne", "wird", "ein", "lei\u00b7ses", "Ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und einen Wunsch um mich verlieren.", "tokens": ["Und", "ei\u00b7nen", "Wunsch", "um", "mich", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Denn der, um den wir M\u00e4dchen oft uns stritten,", "tokens": ["Denn", "der", ",", "um", "den", "wir", "M\u00e4d\u00b7chen", "oft", "uns", "strit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "KOUI", "ART", "PPER", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Wird hinter mir, so schlank wie eine Tanne, stehn,", "tokens": ["Wird", "hin\u00b7ter", "mir", ",", "so", "schlank", "wie", "ei\u00b7ne", "Tan\u00b7ne", ",", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "$,", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und sonst auf nichts mit seinen Augen sehn,", "tokens": ["Und", "sonst", "auf", "nichts", "mit", "sei\u00b7nen", "Au\u00b7gen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIS", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Als auf das M\u00e4dchen in dem Schlitten.", "tokens": ["Als", "auf", "das", "M\u00e4d\u00b7chen", "in", "dem", "Schlit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}