{"textgrid.poem.56428": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Lager Tore fassen nicht die Menge.", "tokens": ["Der", "La\u00b7ger", "To\u00b7re", "fas\u00b7sen", "nicht", "die", "Men\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Fliehnden pressen sich an Tores Schwellen.", "tokens": ["Die", "Fliehn\u00b7den", "pres\u00b7sen", "sich", "an", "To\u00b7res", "Schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Leiber t\u00fcrmen auf sich zu den W\u00e4llen,", "tokens": ["Die", "Lei\u00b7ber", "t\u00fcr\u00b7men", "auf", "sich", "zu", "den", "W\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie eine Woge brausend durch die Enge.", "tokens": ["Wie", "ei\u00b7ne", "Wo\u00b7ge", "brau\u00b7send", "durch", "die", "En\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auf den Erdr\u00fcckten, die zu tausend fallen,", "tokens": ["Auf", "den", "Er\u00b7dr\u00fcck\u00b7ten", ",", "die", "zu", "tau\u00b7send", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "CARD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "St\u00fcrzt nach die Flucht, hin durch die Lagergassen.", "tokens": ["St\u00fcrzt", "nach", "die", "Flucht", ",", "hin", "durch", "die", "La\u00b7ger\u00b7gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Zelte st\u00fcrzen nieder vor den Massen,", "tokens": ["Die", "Zel\u00b7te", "st\u00fcr\u00b7zen", "nie\u00b7der", "vor", "den", "Mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die wie Lawinen wachsend hin sich ballen.", "tokens": ["Die", "wie", "La\u00b7wi\u00b7nen", "wach\u00b7send", "hin", "sich", "bal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NN", "ADJD", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Kranken treten tot sie in den Betten.", "tokens": ["Die", "Kran\u00b7ken", "tre\u00b7ten", "tot", "sie", "in", "den", "Bet\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Fraun und Kinder st\u00f6\u00dft sie in den Kot.", "tokens": ["Die", "Fraun", "und", "Kin\u00b7der", "st\u00f6\u00dft", "sie", "in", "den", "Kot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur ein Gedanke: sich zum Schiff zu retten.", "tokens": ["Nur", "ein", "Ge\u00b7dan\u00b7ke", ":", "sich", "zum", "Schiff", "zu", "ret\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Menge w\u00e4lzt herab sich zu dem Strande,", "tokens": ["Die", "Men\u00b7ge", "w\u00e4lzt", "her\u00b7ab", "sich", "zu", "dem", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was in den Weg ihr kommt, das tritt sie tot.", "tokens": ["Was", "in", "den", "Weg", "ihr", "kommt", ",", "das", "tritt", "sie", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ins Wasser st\u00fcrzt sie, wie ein Strom vom Lande.", "tokens": ["Ins", "Was\u00b7ser", "st\u00fcrzt", "sie", ",", "wie", "ein", "Strom", "vom", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}