{"textgrid.poem.52037": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Mundst\u00fcck an der Braut, sprichstu, ist trefflich gutt,", "tokens": ["Das", "Mund\u00b7st\u00fcck", "an", "der", "Braut", ",", "sprichs\u00b7tu", ",", "ist", "treff\u00b7lich", "gutt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "VVFIN", "$,", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Trompeter, wie da\u00df du den Ansatz nicht kanst sehen:", "tokens": ["Trom\u00b7pe\u00b7ter", ",", "wie", "da\u00df", "du", "den", "An\u00b7satz", "nicht", "kanst", "se\u00b7hen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOKOM", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du bl\u00e4sest doch, obgleich es an dir mangeln thut,", "tokens": ["Du", "bl\u00e4\u00b7sest", "doch", ",", "ob\u00b7gleich", "es", "an", "dir", "man\u00b7geln", "thut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "APPR", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch h\u00f6rt man manchesmal auch einen Kapphahn kr\u00e4hen.", "tokens": ["Doch", "h\u00f6rt", "man", "man\u00b7ches\u00b7mal", "auch", "ei\u00b7nen", "Kap\u00b7phahn", "kr\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word",