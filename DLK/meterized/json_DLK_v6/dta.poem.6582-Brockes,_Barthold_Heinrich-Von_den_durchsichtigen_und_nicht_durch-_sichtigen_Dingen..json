{"dta.poem.6582": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den durchsichtigen und nicht durch-  \n sichtigen Dingen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man seh' demnach verschiedne C\u00f6rper an,", "tokens": ["Man", "seh'", "dem\u00b7nach", "ver\u00b7schied\u00b7ne", "C\u00f6r\u00b7per", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PAV", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Wasser, Glas, die Lufft, so man durchsichtig nennet", "tokens": ["Das", "Was\u00b7ser", ",", "Glas", ",", "die", "Lufft", ",", "so", "man", "durch\u00b7sich\u00b7tig", "nen\u00b7net"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "ART", "NN", "$,", "ADV", "PIS", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wodurch man alles das, was sichtbar sehen kan;", "tokens": ["Wo\u00b7durch", "man", "al\u00b7les", "das", ",", "was", "sicht\u00b7bar", "se\u00b7hen", "kan", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PIS", "PDS", "$,", "PRELS", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als durch und durch zerl\u00f6chert und zertrennet,", "tokens": ["Als", "durch", "und", "durch", "zer\u00b7l\u00f6\u00b7chert", "und", "zer\u00b7tren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "KON", "APPR", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit Oeffnungen die unempfindlich klein:", "tokens": ["Mit", "Oeff\u00b7nun\u00b7gen", "die", "un\u00b7emp\u00b7find\u00b7lich", "klein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df diese Oeffnungen jedoch gerade seyn,", "tokens": ["Da\u00df", "die\u00b7se", "Oeff\u00b7nun\u00b7gen", "je\u00b7doch", "ge\u00b7ra\u00b7de", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADV", "ADV", "VAINF", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "So, da\u00df des Lichtes Strahl und Schein", "tokens": ["So", ",", "da\u00df", "des", "Lich\u00b7tes", "Strahl", "und", "Schein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie in geraden Strich durchdringt;", "tokens": ["Sie", "in", "ge\u00b7ra\u00b7den", "Strich", "durch\u00b7dringt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wodurch die W\u00fcrckung denn gelingt,", "tokens": ["Wo\u00b7durch", "die", "W\u00fcr\u00b7ckung", "denn", "ge\u00b7lingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df durch die Lufft, das Wasser, die Crystallen,", "tokens": ["Da\u00df", "durch", "die", "Lufft", ",", "das", "Was\u00b7ser", ",", "die", "Crys\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die Vorw\u00fcrff\u2019 all\u2019 uns in die Augen fallen.", "tokens": ["Die", "Vor\u00b7w\u00fcrff'", "all'", "uns", "in", "die", "Au\u00b7gen", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}