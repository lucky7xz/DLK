{"textgrid.poem.56189": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe, bevor der Morgen", "tokens": ["Ich", "ha\u00b7be", ",", "be\u00b7vor", "der", "Mor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Im Osten noch gegraut,", "tokens": ["Im", "Os\u00b7ten", "noch", "ge\u00b7graut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Fenster zitternd geharret", "tokens": ["Am", "Fens\u00b7ter", "zit\u00b7ternd", "ge\u00b7har\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und dort hinaus geschaut.", "tokens": ["Und", "dort", "hin\u00b7aus", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APZR", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und in der Mittagsstunde,", "tokens": ["Und", "in", "der", "Mit\u00b7tags\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da hab ich bitter geweint,", "tokens": ["Da", "hab", "ich", "bit\u00b7ter", "ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und habe doch im Herzen:", "tokens": ["Und", "ha\u00b7be", "doch", "im", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er kommt wohl noch, gemeint.", "tokens": ["Er", "kommt", "wohl", "noch", ",", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Nacht, die Nacht ist kommen,", "tokens": ["Die", "Nacht", ",", "die", "Nacht", "ist", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor der ich mich gescheut;", "tokens": ["Vor", "der", "ich", "mich", "ge\u00b7scheut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun ist der Tag verloren,", "tokens": ["Nun", "ist", "der", "Tag", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf den ich mich gefreut.", "tokens": ["Auf", "den", "ich", "mich", "ge\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}