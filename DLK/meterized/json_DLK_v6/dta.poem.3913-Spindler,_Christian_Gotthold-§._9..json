{"dta.poem.3913": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "\u00a7. 9.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Wird der Verliebte nun, mit br\u00fcnstigen Ver-", "tokens": ["Wird", "der", "Ver\u00b7lieb\u00b7te", "nun", ",", "mit", "br\u00fcns\u00b7ti\u00b7gen", "Ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADV", "$,", "APPR", "ADJA", "TRUNC"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "langen,", "tokens": ["lan\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und ausgestrecktem Arm umschlossen und umfangen", "tokens": ["Und", "aus\u00b7ge\u00b7streck\u00b7tem", "Arm", "um\u00b7schlos\u00b7sen", "und", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und noch darzu gek\u00fc\u00dft, so ists nicht ungeschickt,", "tokens": ["Und", "noch", "dar\u00b7zu", "ge\u00b7k\u00fc\u00dft", ",", "so", "ists", "nicht", "un\u00b7ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "VVPP", "$,", "ADV", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wen\u0303 er ihr gleich daf\u00fcr ein Spanisch Creutzge\u0303 dr\u00fckt.", "tokens": ["We\u00f1", "er", "ihr", "gleich", "da\u00b7f\u00fcr", "ein", "Spa\u00b7nisch", "Creutz\u00b7g\u1ebd", "dr\u00fckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PAV", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}