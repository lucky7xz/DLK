"text": "Dir so auch nichts erwieder,", "tokens": ["Dir", "so", "auch", "nichts", "er\u00b7wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So wei\u00df ich, da\u00df du keine Gab", "tokens": ["So", "wei\u00df", "ich", ",", "da\u00df", "du", "kei\u00b7ne", "Gab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht weiter forderst, als ich hab.", "tokens": ["Nicht", "wei\u00b7ter", "for\u00b7derst", ",", "als", "ich", "hab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "$,", "KOUS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Nimm, was ich habe, mich selbst, mir", "tokens": ["Nimm", ",", "was", "ich", "ha\u00b7be", ",", "mich", "selbst", ",", "mir"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "PWS", "PPER", "VAFIN", "$,", "PPER", "ADV", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lege mich, mein Heyl, zu dir", "tokens": ["Und", "le\u00b7ge", "mich", ",", "mein", "Heyl", ",", "zu", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In deiner lieben Wiegen;", "tokens": ["In", "dei\u00b7ner", "lie\u00b7ben", "Wie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hie z\u00fcnd mich an durch deine Glut,", "tokens": ["Hie", "z\u00fcnd", "mich", "an", "durch", "dei\u00b7ne", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hie will ich, wie der Ph\u00f6nix thut,", "tokens": ["Hie", "will", "ich", ",", "wie", "der", "Ph\u00f6\u00b7nix", "thut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Herr, meinem Tod' obsiegen", "tokens": ["Herr", ",", "mei\u00b7nem", "Tod'", "ob\u00b7sie\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und dir gleich unter deinem Schein", "tokens": ["Und", "dir", "gleich", "un\u00b7ter", "dei\u00b7nem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von neuen gantz geboren seyn.", "tokens": ["Von", "neu\u00b7en", "gantz", "ge\u00b7bo\u00b7ren", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}