{"textgrid.poem.49127": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Von der Geburt Jesu Christi", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, k\u00f6mmstu endlich in die Welt,", "tokens": ["Ach", ",", "k\u00f6mms\u00b7tu", "end\u00b7lich", "in", "die", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du l\u00e4ngst versprochner Sternen-Held,", "tokens": ["Du", "l\u00e4ngst", "ver\u00b7sproch\u00b7ner", "Ster\u00b7nen\u00b7Held", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit deinen Himmels-Gaben?", "tokens": ["Mit", "dei\u00b7nen", "Him\u00b7mels\u00b7Ga\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie selig sind wir doch f\u00fcr die,", "tokens": ["Wie", "se\u00b7lig", "sind", "wir", "doch", "f\u00fcr", "die", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "APPR", "ART", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So dich mit gro\u00dfer Angst und M\u00fch", "tokens": ["So", "dich", "mit", "gro\u00b7\u00dfer", "Angst", "und", "M\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vorher gew\u00fcnschet haben;", "tokens": ["Vor\u00b7her", "ge\u00b7w\u00fcn\u00b7schet", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Uns sp\u00e4ten S\u00fcndern scheint das Licht,", "tokens": ["Uns", "sp\u00e4\u00b7ten", "S\u00fcn\u00b7dern", "scheint", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das auch den Heiligsten ward nicht.", "tokens": ["Das", "auch", "den", "Hei\u00b7ligs\u00b7ten", "ward", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VAFIN", "PTKNEG", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Belobter Heyland, sey gegr\u00fcst,", "tokens": ["Be\u00b7lob\u00b7ter", "Hey\u00b7land", ",", "sey", "ge\u00b7gr\u00fcst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Quell, au\u00df welcher einig fliest", "tokens": ["Du", "Quell", ",", "au\u00df", "wel\u00b7cher", "ei\u00b7nig", "fliest"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "APPR", "PRELS", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wolfahrt meiner Seelen;", "tokens": ["Die", "Wol\u00b7fahrt", "mei\u00b7ner", "See\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was sag' ich dir f\u00fcr Dank, mein Gott,", "tokens": ["Was", "sag'", "ich", "dir", "f\u00fcr", "Dank", ",", "mein", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "APPR", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df du mit meinem S\u00fcnden-Koth", "tokens": ["Da\u00df", "du", "mit", "mei\u00b7nem", "S\u00fcn\u00b7den\u00b7Koth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wilst deinen Glantz verm\u00e4hlen?", "tokens": ["Wilst", "dei\u00b7nen", "Glantz", "ver\u00b7m\u00e4h\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und da\u00df der Herr der Herrlichkeit", "tokens": ["Und", "da\u00df", "der", "Herr", "der", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich in des Knechts Gestalt verkleidt.", "tokens": ["Sich", "in", "des", "Knechts", "Ge\u00b7stalt", "ver\u00b7kleidt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O gro\u00dfes Wunder, gro\u00dfe Gnad!", "tokens": ["O", "gro\u00b7\u00dfes", "Wun\u00b7der", ",", "gro\u00b7\u00dfe", "Gnad", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der alle Ding' erschaffen hat,", "tokens": ["Der", "al\u00b7le", "Ding'", "er\u00b7schaf\u00b7fen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "L\u00e4st sich itzunder zeugen;", "tokens": ["L\u00e4st", "sich", "it\u00b7zun\u00b7der", "zeu\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Das Wort des Vaters lallet kaum,", "tokens": ["Das", "Wort", "des", "Va\u00b7ters", "lal\u00b7let", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der F\u00fcrst des Lebens findt nicht Raum", "tokens": ["Der", "F\u00fcrst", "des", "Le\u00b7bens", "findt", "nicht", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PTKNEG", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und k\u00f6mmt doch in sein eigen;", "tokens": ["Und", "k\u00f6mmt", "doch", "in", "sein", "ei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Sonn' und Mond in H\u00e4nden tr\u00e4gt,", "tokens": ["Der", "Sonn'", "und", "Mond", "in", "H\u00e4n\u00b7den", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wird hin auff Streu und Stroh gelegt.", "tokens": ["Wird", "hin", "auff", "Streu", "und", "Stroh", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie find' ich dich, mein Jesu, hier?", "tokens": ["Wie", "find'", "ich", "dich", ",", "mein", "Je\u00b7su", ",", "hier", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$,", "PPOSAT", "NE", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist doch ein unvern\u00fcnfftigs Thier", "tokens": ["Ist", "doch", "ein", "un\u00b7ver\u00b7n\u00fcnff\u00b7tigs", "Thier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weit kl\u00fcger, als wir Alle,", "tokens": ["Weit", "kl\u00fc\u00b7ger", ",", "als", "wir", "Al\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "KOUS", "PPER", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das r\u00e4umt dir seine Krippen ein,", "tokens": ["Das", "r\u00e4umt", "dir", "sei\u00b7ne", "Krip\u00b7pen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da du nicht kanst im Hause seyn,", "tokens": ["Da", "du", "nicht", "kanst", "im", "Hau\u00b7se", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "APPRART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das rufft dich an im Stalle,", "tokens": ["Das", "rufft", "dich", "an", "im", "Stal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da Keiner an dich von uns denkt", "tokens": ["Da", "Kei\u00b7ner", "an", "dich", "von", "uns", "denkt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PRF", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und dir ein frohes Lob-Lied schenkt.", "tokens": ["Und", "dir", "ein", "fro\u00b7hes", "Lob\u00b7Lied", "schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gleich in der allerl\u00e4ngsten Nacht", "tokens": ["Gleich", "in", "der", "al\u00b7ler\u00b7l\u00e4ngs\u00b7ten", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wirst du, o Licht, ans Licht gebracht,", "tokens": ["Wirst", "du", ",", "o", "Licht", ",", "ans", "Licht", "ge\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "FM", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich da der Krey\u00df der Erden", "tokens": ["Gleich", "da", "der", "Krey\u00df", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Ey\u00df und Schnee gantz liegt verstellt,", "tokens": ["In", "Ey\u00df", "und", "Schnee", "gantz", "liegt", "ver\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So mustu, Leben deiner Welt,", "tokens": ["So", "mus\u00b7tu", ",", "Le\u00b7ben", "dei\u00b7ner", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr neu geboren werden;", "tokens": ["Ihr", "neu", "ge\u00b7bo\u00b7ren", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da Alles todt und abgethan,", "tokens": ["Da", "Al\u00b7les", "todt", "und", "ab\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So stellstu dein Geburts-Fest an.", "tokens": ["So", "stell\u00b7stu", "dein", "Ge\u00b7burts\u00b7Fest", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dies, ach, dies zeugt von unsrer Noth;", "tokens": ["Dies", ",", "ach", ",", "dies", "zeugt", "von", "uns\u00b7rer", "Noth", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ITJ", "$,", "PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir lagen gantz in S\u00fcnden todt,", "tokens": ["Wir", "la\u00b7gen", "gantz", "in", "S\u00fcn\u00b7den", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns hielt die Nacht gefangen,", "tokens": ["Uns", "hielt", "die", "Nacht", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die l\u00e4ngst verdiente H\u00f6llen-Nacht;", "tokens": ["Die", "l\u00e4ngst", "ver\u00b7dien\u00b7te", "H\u00f6l\u00b7len\u00b7Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Seelen erster Schmuck und Pracht", "tokens": ["Der", "See\u00b7len", "ers\u00b7ter", "Schmuck", "und", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "War mehr als gantz vergangen", "tokens": ["War", "mehr", "als", "gantz", "ver\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "KOKOM", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und nichts, als lauter Frost und K\u00e4lt", "tokens": ["Und", "nichts", ",", "als", "lau\u00b7ter", "Frost", "und", "K\u00e4lt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "$,", "KOUS", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "War um die Hertzen hergestellt.", "tokens": ["War", "um", "die", "Hert\u00b7zen", "her\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Itzt merk' ich erst, o Vater, recht,", "tokens": ["Itzt", "merk'", "ich", "erst", ",", "o", "Va\u00b7ter", ",", "recht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie viel der Mensch, der S\u00fcnden-Knecht,", "tokens": ["Wie", "viel", "der", "Mensch", ",", "der", "S\u00fcn\u00b7den\u00b7Knecht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gehabt hat zu verb\u00fcssen,", "tokens": ["Ge\u00b7habt", "hat", "zu", "ver\u00b7b\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df, wo du soltst vers\u00fchnet seyn,", "tokens": ["Da\u00df", ",", "wo", "du", "soltst", "ver\u00b7s\u00fch\u00b7net", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "PPER", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein einger Sohn mit grosser Pein", "tokens": ["Dein", "ein\u00b7ger", "Sohn", "mit", "gros\u00b7ser", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Daf\u00fcr hat zahlen m\u00fcssen.", "tokens": ["Da\u00b7f\u00fcr", "hat", "zah\u00b7len", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Was niemand kan, als Gott allein,", "tokens": ["Was", "nie\u00b7mand", "kan", ",", "als", "Gott", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VMFIN", "$,", "KOUS", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das mu\u00df ja nichts Geringes seyn.", "tokens": ["Das", "mu\u00df", "ja", "ni