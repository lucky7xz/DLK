{"textgrid.poem.50930": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "6", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah zwei Gr\u00e4ber auf der Heide,", "tokens": ["Ich", "sah", "zwei", "Gr\u00e4\u00b7ber", "auf", "der", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Immortellen ganz bedeckt;", "tokens": ["Von", "Im\u00b7mor\u00b7tel\u00b7len", "ganz", "be\u00b7deckt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein sch\u00f6nes Weib mit schwerem Leide", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Weib", "mit", "schwe\u00b7rem", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lag auf dem einen hingestreckt.", "tokens": ["Lag", "auf", "dem", "ei\u00b7nen", "hin\u00b7ge\u00b7streckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ART", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das andre hielt in hei\u00dfen Tr\u00e4nen", "tokens": ["Das", "and\u00b7re", "hielt", "in", "hei\u00b7\u00dfen", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein gramerf\u00fcllter Mann bewacht,", "tokens": ["Ein", "gra\u00b7mer\u00b7f\u00fcll\u00b7ter", "Mann", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und beide sahn voll Liebessehnen", "tokens": ["Und", "bei\u00b7de", "sahn", "voll", "Lie\u00b7bes\u00b7seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auf in die klare Sternennacht.", "tokens": ["Auf", "in", "die", "kla\u00b7re", "Ster\u00b7nen\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbin jenen selig heitren Fernen", "tokens": ["\u00bb", "in", "je\u00b7nen", "se\u00b7lig", "hei\u00b7tren", "Fer\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PDAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Harrt nun die liebste Seele mein,", "tokens": ["Harrt", "nun", "die", "liebs\u00b7te", "See\u00b7le", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald werd ich unter goldnen Sternen", "tokens": ["Bald", "werd", "ich", "un\u00b7ter", "gold\u00b7nen", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf ewig, ewig bei ihm sein!", "tokens": ["Auf", "e\u00b7wig", ",", "e\u00b7wig", "bei", "ihm", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "ADJD", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als einen Hauch und Seufzer z\u00e4hle", "tokens": ["Als", "ei\u00b7nen", "Hauch", "und", "Seuf\u00b7zer", "z\u00e4h\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich noch die Spanne schn\u00f6der Zeit;", "tokens": ["Ich", "noch", "die", "Span\u00b7ne", "schn\u00f6\u00b7der", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann aber sind so Lieb wie Seele", "tokens": ["Dann", "a\u00b7ber", "sind", "so", "Lieb", "wie", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "NN", "KOKOM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ganz der Unsterblichkeit geweiht!\u00ab \u2013", "tokens": ["Ganz", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "ge\u00b7weiht", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$.", "$(", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "\u00bbo kreiset rascher, tr\u00e4ge Sonnen,", "tokens": ["\u00bb", "o", "krei\u00b7set", "ra\u00b7scher", ",", "tr\u00e4\u00b7ge", "Son\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und l\u00f6set dieses Leibes Bann,", "tokens": ["Und", "l\u00f6\u00b7set", "die\u00b7ses", "Lei\u00b7bes", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich auf euch in neuen Wonnen", "tokens": ["Da\u00df", "ich", "auf", "euch", "in", "neu\u00b7en", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein selig Liebchen finden kann!", "tokens": ["Mein", "se\u00b7lig", "Lieb\u00b7chen", "fin\u00b7den", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Heil mir! ich will sie wiedersehen!", "tokens": ["Heil", "mir", "!", "ich", "will", "sie", "wie\u00b7der\u00b7se\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und wenn auch Stern um Stern zerbricht:", "tokens": ["Und", "wenn", "auch", "Stern", "um", "Stern", "zer\u00b7bricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In Ewigkeit wird nie vergehen", "tokens": ["In", "E\u00b7wig\u00b7keit", "wird", "nie", "ver\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zwei treuer Seelen Bund und Licht!\u00ab", "tokens": ["Zwei", "treu\u00b7er", "See\u00b7len", "Bund", "und", "Licht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["CARD", "ADJA", "NN", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So riefen Weib und Mann, so beide,", "tokens": ["So", "rie\u00b7fen", "Weib", "und", "Mann", ",", "so", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$,", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ganz in den eignen Gram gebannt;", "tokens": ["Ganz", "in", "den", "eig\u00b7nen", "Gram", "ge\u00b7bannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sahn sich nicht auf dunkler Heide,", "tokens": ["Sie", "sahn", "sich", "nicht", "auf", "dunk\u00b7ler", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Blicke sternenw\u00e4rts gewandt.", "tokens": ["Die", "Bli\u00b7cke", "ster\u00b7nen\u00b7w\u00e4rts", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie trauerten, bis da\u00df der Morgen", "tokens": ["Sie", "trau\u00b7er\u00b7ten", ",", "bis", "da\u00df", "der", "Mor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "KOUS", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Erbleichen lie\u00df der Sterne Schar,", "tokens": ["Er\u00b7blei\u00b7chen", "lie\u00df", "der", "Ster\u00b7ne", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der H\u00f6he Blau das Gold verborgen", "tokens": ["Der", "H\u00f6\u00b7he", "Blau", "das", "Gold", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und es auf Erden heiter war.", "tokens": ["Und", "es", "auf", "Er\u00b7den", "hei\u00b7ter", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da rafften sie sich auf und gingen", "tokens": ["Da", "raff\u00b7ten", "sie", "sich", "auf", "und", "gin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entlang das schimmernde Gefild,", "tokens": ["Ent\u00b7lang", "das", "schim\u00b7mern\u00b7de", "Ge\u00b7fild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis pl\u00f6tzlich ihre Augen hingen", "tokens": ["Bis", "pl\u00f6tz\u00b7lich", "ih\u00b7re", "Au\u00b7gen", "hin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Eins an des andern sch\u00f6nem Bild.", "tokens": ["Eins", "an", "des", "an\u00b7dern", "sch\u00f6\u00b7nem", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und eh der junge Tag, der warme,", "tokens": ["Und", "eh", "der", "jun\u00b7ge", "Tag", ",", "der", "war\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die letzten Tr\u00e4nen weggek\u00fc\u00dft,", "tokens": ["Die", "letz\u00b7ten", "Tr\u00e4\u00b7nen", "weg\u00b7ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schon fielen l\u00e4chelnd in die Arme", "tokens": ["Schon", "fie\u00b7len", "l\u00e4\u00b7chelnd", "in", "die", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sich beide, Lust in Lust geb\u00fc\u00dft.", "tokens": ["Sich", "bei\u00b7de", ",", "Lust", "in", "Lust", "ge\u00b7b\u00fc\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIS", "$,", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Enkel Trupp mit festen H\u00e4nden,", "tokens": ["Der", "En\u00b7kel", "Trupp", "mit", "fes\u00b7ten", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf selber Heid im Sonnenschein,", "tokens": ["Auf", "sel\u00b7ber", "Heid", "im", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sah pfl\u00fcgen ich und singend wenden", "tokens": ["Sah", "pfl\u00fc\u00b7gen", "ich", "und", "sin\u00b7gend", "wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein l\u00e4ngst verschollenes Gebein.", "tokens": ["Ein", "l\u00e4ngst", "ver\u00b7schol\u00b7le\u00b7nes", "Ge\u00b7bein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie deckten rasch, was sie gefunden,", "tokens": ["Sie", "deck\u00b7ten", "rasch", ",", "was", "sie", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit jungen Saaten, im Gem\u00fct", "tokens": ["Mit", "jun\u00b7gen", "Saa\u00b7ten", ",", "im", "Ge\u00b7m\u00fct"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Leis ahnend, da\u00df die eignen Stunden", "tokens": ["Leis", "ah\u00b7nend", ",", "da\u00df", "die", "eig\u00b7nen", "Stun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Aus diesem Tode nur erbl\u00fcht.", "tokens": ["Aus", "die\u00b7sem", "To\u00b7de", "nur", "er\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}