{"textgrid.poem.59171": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "390. Man findet's bei Jesus", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, gib mir Kreuz! Die Gabe gro\u00df", "tokens": ["Herr", ",", "gib", "mir", "Kreuz", "!", "Die", "Ga\u00b7be", "gro\u00df"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "PPER", "NN", "$.", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird jedem, der dir folgt, gegeben.", "tokens": ["Wird", "je\u00b7dem", ",", "der", "dir", "folgt", ",", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mach mich von mir und allem blo\u00df,", "tokens": ["Mach", "mich", "von", "mir", "und", "al\u00b7lem", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "KON", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df mich allein mit dir bekleidet leben!", "tokens": ["La\u00df", "mich", "al\u00b7lein", "mit", "dir", "be\u00b7klei\u00b7det", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}