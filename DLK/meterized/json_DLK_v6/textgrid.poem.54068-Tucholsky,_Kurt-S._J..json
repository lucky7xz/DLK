{"textgrid.poem.54068": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "S. J.", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4rst du noch da!", "tokens": ["W\u00e4rst", "du", "noch", "da", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Soviel wartet auf dich.", "tokens": ["So\u00b7viel", "war\u00b7tet", "auf", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Alles wartet vergebens.", "tokens": ["Al\u00b7les", "war\u00b7tet", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Du t\u00e4test dein Werk so s\u00e4uberlich", "tokens": ["Du", "t\u00e4\u00b7test", "dein", "Werk", "so", "s\u00e4u\u00b7ber\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "wie im Laufe deines Lebens.", "tokens": ["wie", "im", "Lau\u00b7fe", "dei\u00b7nes", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich seh dich am Tisch. Und die tr\u00fcbe Zeit", "tokens": ["Ich", "seh", "dich", "am", "Tisch", ".", "Und", "die", "tr\u00fc\u00b7be", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$.", "KON", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "w\u00e4re hell \u2013 denn du bist heiter.", "tokens": ["w\u00e4\u00b7re", "hell", "\u2013", "denn", "du", "bist", "hei\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$(", "KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Du pfiffst auf die h\u00e4rteste Schwierigkeit:", "tokens": ["Du", "pfiffst", "auf", "die", "h\u00e4r\u00b7tes\u00b7te", "Schwie\u00b7rig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "du lachst und arbeitest weiter.", "tokens": ["du", "lachst", "und", "ar\u00b7bei\u00b7test", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Du kanntest das Blatt und seinen Ort", "tokens": ["Du", "kann\u00b7test", "das", "Blatt", "und", "sei\u00b7nen", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "im Strudel der tausend Parteien.", "tokens": ["im", "Stru\u00b7del", "der", "tau\u00b7send", "Par\u00b7tei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "CARD", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Leise schobst du die Bonzen fort", "tokens": ["Lei\u00b7se", "schobst", "du", "die", "Bon\u00b7zen", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "und lie\u00dfest die Schreier schreien.", "tokens": ["und", "lie\u00b7\u00dfest", "die", "Schrei\u00b7er", "schrei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Du warst dem, der schreiten und folgen kann,", "tokens": ["Du", "warst", "dem", ",", "der", "schrei\u00b7ten", "und", "fol\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "$,", "ART", "ADJA", "KON", "VVINF", "VMFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "der treuste Begleiter.", "tokens": ["der", "treus\u00b7te", "Be\u00b7glei\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Pfiff der Wind recht laut: wir sahn dich nur an \u2013", "tokens": ["Pfiff", "der", "Wind", "recht", "laut", ":", "wir", "sahn", "dich", "nur", "an", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADJD", "$.", "PPER", "VVFIN", "PPER", "ADV", "APPR", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "du lachst und arbeitest weiter.", "tokens": ["du", "lachst", "und", "ar\u00b7bei\u00b7test", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Aber nun bist du untergetaucht.", "tokens": ["A\u00b7ber", "nun", "bist", "du", "un\u00b7ter\u00b7ge\u00b7taucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wir sehn noch nach deinen Zielen.", "tokens": ["Wir", "sehn", "noch", "nach", "dei\u00b7nen", "Zie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Jeder hat mal einen Vater gebraucht . . .", "tokens": ["Je\u00b7der", "hat", "mal", "ei\u00b7nen", "Va\u00b7ter", "ge\u00b7braucht", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ART", "NN", "VVPP", "$.", "$.", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "du warst der Vater von vielen . . .", "tokens": ["du", "warst", "der", "Va\u00b7ter", "von", "vie\u00b7len", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PIAT", "$.", "$.", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ich h\u00f6r deine Stimme: \u00bbWer schwach ist, flennt.", "tokens": ["Ich", "h\u00f6r", "dei\u00b7ne", "Stim\u00b7me", ":", "\u00bb", "Wer", "schwach", "ist", ",", "flennt", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "$(", "PWS", "ADJD", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Arbeiten ist gescheiter.\u00ab", "tokens": ["Ar\u00b7bei\u00b7ten", "ist", "ge\u00b7schei\u00b7ter", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wenn der ganze Schnee verbrennt:", "tokens": ["Und", "wenn", "der", "gan\u00b7ze", "Schnee", "ver\u00b7brennt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "wir lachen und arbeiten weiter.", "tokens": ["wir", "la\u00b7chen", "und", "ar\u00b7bei\u00b7ten", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}