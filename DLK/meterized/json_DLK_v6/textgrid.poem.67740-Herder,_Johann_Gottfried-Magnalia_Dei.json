{"textgrid.poem.67740": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Magnalia Dei", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich singe Gott! Jehovens Rath und That!", "tokens": ["Ich", "sin\u00b7ge", "Gott", "!", "Je\u00b7ho\u00b7vens", "Rath", "und", "That", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$.", "NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Euch Himmeln, Erde, Dir erz\u00e4hl' ich Gottes Ehre.", "tokens": ["Euch", "Him\u00b7meln", ",", "Er\u00b7de", ",", "Dir", "er\u00b7z\u00e4hl'", "ich", "Got\u00b7tes", "Eh\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "$,", "PPER", "VVFIN", "PPER", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Singt, Sph\u00e4ren! singt mir vor! Du h\u00f6rtest, h\u00f6chste Sph\u00e4re,", "tokens": ["Singt", ",", "Sph\u00e4\u00b7ren", "!", "singt", "mir", "vor", "!", "Du", "h\u00f6r\u00b7test", ",", "h\u00f6chs\u00b7te", "Sph\u00e4\u00b7re", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch nur von fern des Ew'gen Rath!", "tokens": ["Doch", "nur", "von", "fern", "des", "Ew'\u00b7gen", "Rath", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und singst. \u2013 Und, Erde, steh! und ruh \u2013 und h\u00f6re,", "tokens": ["Und", "singst", ".", "\u2013", "Und", ",", "Er\u00b7de", ",", "steh", "!", "und", "ruh", "\u2013", "und", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "KON", "$,", "NN", "$,", "VVFIN", "$.", "KON", "NN", "$(", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was Gott f\u00fcr Dich beschlo\u00df und that! \u2013", "tokens": ["Was", "Gott", "f\u00fcr", "Dich", "be\u00b7schlo\u00df", "und", "that", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "NN", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er singt \u2013 sie schweigt \u2013 noch brausen H\u00f6llenmeere,", "tokens": ["Er", "singt", "\u2013", "sie", "schweigt", "\u2013", "noch", "brau\u00b7sen", "H\u00f6l\u00b7len\u00b7mee\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "$(", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verstummt vor Gott. \u2013 Erzittre, H\u00f6ll', und bebend h\u00f6re,", "tokens": ["Ver\u00b7stummt", "vor", "Gott", ".", "\u2013", "Er\u00b7zitt\u00b7re", ",", "H\u00f6ll'", ",", "und", "be\u00b7bend", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$.", "$(", "NN", "$,", "NN", "$,", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was wider Dich Jehovah sprach und that!", "tokens": ["Was", "wi\u00b7der", "Dich", "Je\u00b7ho\u00b7vah", "sprach", "und", "that", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "NE", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Es wird still! \u2013 Ich singe! \u2013 Ich?", "tokens": ["Es", "wird", "still", "!", "\u2013", "Ich", "sin\u00b7ge", "!", "\u2013", "Ich", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "$(", "PPER", "VVFIN", "$.", "$(", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Bin ich Engel, der von Gottes Rath", "tokens": ["Bin", "ich", "En\u00b7gel", ",", "der", "von", "Got\u00b7tes", "Rath"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "NE", "$,", "PRELS", "APPR", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Den tiefsten Widerhall nur rauschen h\u00f6rte \u2013 Ich?", "tokens": ["Den", "tiefs\u00b7ten", "Wi\u00b7der\u00b7hall", "nur", "rau\u00b7schen", "h\u00f6r\u00b7te", "\u2013", "Ich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "VVFIN", "$(", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Halb Nichts, halb Staubkorn \u2013 ja, ich singe \u2013 und ganz Erde?", "tokens": ["Halb", "Nichts", ",", "halb", "Staub\u00b7korn", "\u2013", "ja", ",", "ich", "sin\u00b7ge", "\u2013", "und", "ganz", "Er\u00b7de", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PIS", "$,", "ADJD", "NN", "$(", "PTKANT", "$,", "PPER", "VVFIN", "$(", "KON", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mit Feuer aus der H\u00f6he taufe mich!", "tokens": ["Mit", "Feu\u00b7er", "aus", "der", "H\u00f6\u00b7he", "tau\u00b7fe", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Dein Gesch\u00f6pf, Dein Kind,", "tokens": ["Dein", "Ge\u00b7sch\u00f6pf", ",", "Dein", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Ja Erde ganz! \u2013 doch Dein Gesch\u00f6pf, Dein Christ, Dein Kind,", "tokens": ["Ja", "Er\u00b7de", "ganz", "!", "\u2013", "doch", "Dein", "Ge\u00b7sch\u00f6pf", ",", "Dein", "Christ", ",", "Dein", "Kind", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "NN", "ADV", "$.", "$(", "KON", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "O Sch\u00f6pfer, Vater, Mittler, mich! \u2013 Dann werde", "tokens": ["O", "Sch\u00f6p\u00b7fer", ",", "Va\u00b7ter", ",", "Mitt\u00b7ler", ",", "mich", "!", "\u2013", "Dann", "wer\u00b7de"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "$,", "NN", "$,", "PPER", "$.", "$(", "ADV", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Ich hoch zu Dir entz\u00fcckt und singe Dich!", "tokens": ["Ich", "hoch", "zu", "Dir", "ent\u00b7z\u00fcckt", "und", "sin\u00b7ge", "Dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPER", "VVPP", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gott war! Fleuch, mein Gesang, empor! \u2013", "tokens": ["Gott", "war", "!", "Fleuch", ",", "mein", "Ge\u00b7sang", ",", "em\u00b7por", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "$.", "NN", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Gott war! um ihn nur Endliches, Nichts! \u2013", "tokens": ["Gott", "war", "!", "um", "ihn", "nur", "End\u00b7li\u00b7ches", ",", "Nichts", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "$.", "APPR", "PPER", "ADV", "NN", "$,", "PIS", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Allgegenw\u00e4rtig Nichts! wer kann es malen!", "tokens": ["All\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "Nichts", "!", "wer", "kann", "es", "ma\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIS", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Wer blickt in diesen Tod des Lichts?", "tokens": ["Wer", "blickt", "in", "die\u00b7sen", "Tod", "des", "Lichts", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer herrscht? Gott herrscht auch \u00fcbers All des Nichts. \u2013", "tokens": ["Wer", "herrscht", "?", "Gott", "herrscht", "auch", "\u00fc\u00b7bers", "All", "des", "Nichts", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "$.", "NN", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Er spricht! \u2013 sein Sch\u00f6pfershauch bebt durch die W\u00fcsten", "tokens": ["Er", "spricht", "!", "\u2013", "sein", "Sch\u00f6p\u00b7fers\u00b7hauch", "bebt", "durch", "die", "W\u00fcs\u00b7ten"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Des Undings \u2013 ruft das Sein hervor!", "tokens": ["Des", "Un\u00b7dings", "\u2013", "ruft", "das", "Sein", "her\u00b7vor", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Chaos Nacht blinzt schon von seinen Strahlen \u2013", "tokens": ["Des", "Chaos", "Nacht", "blinzt", "schon", "von", "sei\u00b7nen", "Strah\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Sie blinzt und sieht zum Licht! \u2013", "tokens": ["Sie", "blinzt", "und", "sieht", "zum", "Licht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Noch schafft sein Licht! \u2013 doch einst! \u2013 Jehovah spricht \u2013", "tokens": ["Noch", "schafft", "sein", "Licht", "!", "\u2013", "doch", "einst", "!", "\u2013", "Je\u00b7ho\u00b7vah", "spricht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "$(", "ADV", "ADV", "$.", "$(", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Sie blinzt \u2013 es zittern ihre feinsten Strahlen \u2013", "tokens": ["Sie", "blinzt", "\u2013", "es", "zit\u00b7tern", "ih\u00b7re", "feins\u00b7ten", "Strah\u00b7len", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und sterben! Sie ist Nacht \u2013 Gott ruft das Sein ins Nichts!", "tokens": ["Und", "ster\u00b7ben", "!", "Sie", "ist", "Nacht", "\u2013", "Gott", "ruft", "das", "Sein", "ins", "Nichts", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "PPER", "VAFIN", "NN", "$(", "NN", "VVFIN", "ART", "NN", "APPRART", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und Gottes Allmachtshauch bebt durch die ew'gen W\u00fcsten", "tokens": ["Und", "Got\u00b7tes", "All\u00b7machts\u00b7hauch", "bebt", "durch", "die", "ew'\u00b7gen", "W\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Des neugeschaffnen Nichts!", "tokens": ["Des", "neu\u00b7ge\u00b7schaff\u00b7nen", "Nichts", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Du schufst's \u2013 o Gott! \u2013 Du, Dir selbst ewig g'nug!", "tokens": ["Du", "schuf\u00b7st's", "\u2013", "o", "Gott", "!", "\u2013", "Du", ",", "Dir", "selbst", "e\u00b7wig", "g'\u00b7nug", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "FM", "NN", "$.", "$(", "PPER", "$,", "PPER", "ADV", "ADJD", "ADV", "$."], "meter": "-++-+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.16": {"text": "Warum schufst Du? \u2013 Mensch, neige Dich zur Erde!", "tokens": ["Wa\u00b7rum", "schufst", "Du", "?", "\u2013", "Mensch", ",", "nei\u00b7ge", "Dich", "zur", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$.", "$(", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Gott spricht: \u00bbEs sei der Mensch, er werde", "tokens": ["Gott", "spricht", ":", "\u00bb", "Es", "sei", "der", "Mensch", ",", "er", "wer\u00b7de"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$.", "$(", "PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ein Bild, uns gleich!\u00ab", "tokens": ["Ein", "Bild", ",", "uns", "gleich", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PPER", "ADV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "F\u00fcr mich, ein denkend Nichts, schufst Du des Segens Reich?", "tokens": ["F\u00fcr", "mich", ",", "ein", "den\u00b7kend", "Nichts", ",", "schufst", "Du", "des", "Se\u00b7gens", "Reich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "ADJD", "PIS", "$,", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "Der Sch\u00f6pfung Plan, wer kann ihn \u00fcbersehn? O nein!", "tokens": ["Der", "Sch\u00f6p\u00b7fung", "Plan", ",", "wer", "kann", "ihn", "\u00fc\u00b7ber\u00b7sehn", "?", "O", "nein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$.", "NE", "PTKANT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ein Punkt des Ganzen! Aus dem Mittelpunkt", "tokens": ["Ein", "Punkt", "des", "Gan\u00b7zen", "!", "Aus", "dem", "Mit\u00b7tel\u00b7punkt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Sieht auch der Punkt sich selbst; das All zu \u00fcbersehn,", "tokens": ["Sieht", "auch", "der", "Punkt", "sich", "selbst", ";", "das", "All", "zu", "\u00fc\u00b7ber\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PRF", "ADV", "$.", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Mu\u00df ich kein Theil des Alls \u2013 selbst Sch\u00f6pfer sein!", "tokens": ["Mu\u00df", "ich", "kein", "Theil", "des", "Alls", "\u2013", "selbst", "Sch\u00f6p\u00b7fer", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "ART", "NN", "$(", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Es sah und sieht Dein Gottesblick, wie Myriaden", "tokens": ["Es", "sah", "und", "sieht", "Dein", "Got\u00b7tes\u00b7blick", ",