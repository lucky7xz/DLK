{"textgrid.poem.63811": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Vision", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch mein Fenster, wenn ich kaum die Augen", "tokens": ["Durch", "mein", "Fens\u00b7ter", ",", "wenn", "ich", "kaum", "die", "Au\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aufgeschlagen, von der K\u00fcste dr\u00fcben", "tokens": ["Auf\u00b7ge\u00b7schla\u00b7gen", ",", "von", "der", "K\u00fcs\u00b7te", "dr\u00fc\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Gr\u00fc\u00dft mich schon die junge Morgenr\u00f6te,", "tokens": ["Gr\u00fc\u00dft", "mich", "schon", "die", "jun\u00b7ge", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Droht mir l\u00e4chelnd mit dem Rosenfinger,", "tokens": ["Droht", "mir", "l\u00e4\u00b7chelnd", "mit", "dem", "Ro\u00b7sen\u00b7fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Da\u00df ich faul mich noch im Bette dehne,", "tokens": ["Da\u00df", "ich", "faul", "mich", "noch", "im", "Bet\u00b7te", "deh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Da sie selbst so p\u00fcnktlich aufgestanden.", "tokens": ["Da", "sie", "selbst", "so", "p\u00fcnkt\u00b7lich", "auf\u00b7ge\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Holde G\u00f6ttin! ruf' ich, hier an deinem", "tokens": ["Hol\u00b7de", "G\u00f6t\u00b7tin", "!", "ruf'", "ich", ",", "hier", "an", "dei\u00b7nem"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "VVFIN", "PPER", "$,", "ADV", "APPR", "PPOSAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Sch\u00f6nen See in Ferienlaune weil' ich,", "tokens": ["Sch\u00f6\u00b7nen", "See", "in", "Fe\u00b7ri\u00b7en\u00b7lau\u00b7ne", "weil'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NE", "VVFIN", "PPER", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.9": {"text": "Aller Arbeit ledig, als nur etwa", "tokens": ["Al\u00b7ler", "Ar\u00b7beit", "le\u00b7dig", ",", "als", "nur", "et\u00b7wa"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJD", "$,", "KOUS", "ADV", "ADV"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Einen Brief zu schreiben in die Heimat,", "tokens": ["Ei\u00b7nen", "Brief", "zu", "schrei\u00b7ben", "in", "die", "Hei\u00b7mat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Oder lieber noch ein Cartolinchen.", "tokens": ["O\u00b7der", "lie\u00b7ber", "noch", "ein", "Car\u00b7to\u00b7lin\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "G\u00f6nne mir's, auf meinem Lotterbettlein", "tokens": ["G\u00f6n\u00b7ne", "mir's", ",", "auf", "mei\u00b7nem", "Lot\u00b7ter\u00b7bet\u00b7tlein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Noch ein Weilchen d\u00e4mmernd hinzutr\u00e4umen,", "tokens": ["Noch", "ein", "Weil\u00b7chen", "d\u00e4m\u00b7mernd", "hin\u00b7zu\u00b7tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Bis die strenge Herrin dir, die Sonne,", "tokens": ["Bis", "die", "stren\u00b7ge", "Her\u00b7rin", "dir", ",", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Folgt, die abhold allem M\u00fc\u00dfiggange.", "tokens": ["Folgt", ",", "die", "ab\u00b7hold", "al\u00b7lem", "M\u00fc\u00b7\u00dfig\u00b7gan\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ADJD", "PIS", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Zeit dann wird es sein, mich aufzuraffen.", "tokens": ["Zeit", "dann", "wird", "es", "sein", ",", "mich", "auf\u00b7zu\u00b7raf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PPER", "VAINF", "$,", "PPER", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und die G\u00f6ttin h\u00f6rt's und scheint's zufrieden.", "tokens": ["Und", "die", "G\u00f6t\u00b7tin", "h\u00f6rt's", "und", "scheint's", "zu\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Aber heut, da wieder mit verstohlnem", "tokens": ["A\u00b7ber", "heut", ",", "da", "wie\u00b7der", "mit", "ver\u00b7stohl\u00b7nem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "ADV", "APPR", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ku\u00df auf meine Augen sie mich, weckte,", "tokens": ["Ku\u00df", "auf", "mei\u00b7ne", "Au\u00b7gen", "sie", "mich", ",", "weck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PPER", "PPER", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wen erblick' ich ihr zur Seite schwebend?", "tokens": ["Wen", "er\u00b7blick'", "ich", "ihr", "zur", "Sei\u00b7te", "schwe\u00b7bend", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ist's ein Nebelspuk, dem See entstiegen?", "tokens": ["Ist's", "ein", "Ne\u00b7bels\u00b7puk", ",", "dem", "See", "ent\u00b7stie\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Nein, ein Frauenbild, in duft'ge Schleier", "tokens": ["Nein", ",", "ein", "Frau\u00b7en\u00b7bild", ",", "in", "duft'\u00b7ge", "Schlei\u00b7er"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Eingeh\u00fcllt; und jetzt empor sich schwingend,", "tokens": ["Ein\u00b7ge\u00b7h\u00fcllt", ";", "und", "jetzt", "em\u00b7por", "sich", "schwin\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "KON", "ADV", "PTKVZ", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Langsam, wie ein Schwan zum \u00c4ther aufstrebt,", "tokens": ["Lang\u00b7sam", ",", "wie", "ein", "Schwan", "zum", "\u00c4\u00b7ther", "auf\u00b7strebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Dann sich wieder senkend, steten Fluges", "tokens": ["Dann", "sich", "wie\u00b7der", "sen\u00b7kend", ",", "ste\u00b7ten", "Flu\u00b7ges"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PRF", "ADV", "VVPP", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Mir entgegen strebt sie. Ja, nun kenn' ich", "tokens": ["Mir", "ent\u00b7ge\u00b7gen", "strebt", "sie", ".", "Ja", ",", "nun", "kenn'", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "VVFIN", "PPER", "$.", "PTKANT", "$,", "ADV", "VVFIN", "PPER"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Dich, geliebtes Antlitz meiner Muse,", "tokens": ["Dich", ",", "ge\u00b7lieb\u00b7tes", "Ant\u00b7litz", "mei\u00b7ner", "Mu\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Die geheimnisvollen G\u00f6tteraugen,", "tokens": ["Die", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7len", "G\u00f6t\u00b7ter\u00b7au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Die mit ernster Frage jetzt mich angl\u00fchn.", "tokens": ["Die", "mit", "erns\u00b7ter", "Fra\u00b7ge", "jetzt", "mich", "an\u00b7gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+---", "measure": "unknown.measure.tetra"}}, "stanza.4": {"line.1": {"text": "Ist's bedauernd, ist's mit leisem Vorwurf?", "tokens": ["Ist's", "be\u00b7dau\u00b7ernd", ",", "ist's", "mit", "lei\u00b7sem", "Vor\u00b7wurf", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "$,", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Doch alsbald mit beiden Lilienarmen", "tokens": ["Doch", "als\u00b7bald", "mit", "bei\u00b7den", "Li\u00b7li\u00b7en\u00b7ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Winkend, wie man einen S\u00e4umigen antreibt,", "tokens": ["Win\u00b7kend", ",", "wie", "man", "ei\u00b7nen", "S\u00e4u\u00b7mi\u00b7gen", "an\u00b7treibt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Nachzufolgen, in die obern L\u00fcfte", "tokens": ["Nach\u00b7zu\u00b7fol\u00b7gen", ",", "in", "die", "o\u00b7bern", "L\u00fcf\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Schie\u00dft sie wie ein Sternbild und verschwindet.", "tokens": ["Schie\u00dft", "sie", "wie", "ein", "Stern\u00b7bild", "und", "ver\u00b7schwin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Und ich starr' ihr nach. O hehre G\u00f6ttin,", "tokens": ["Und", "ich", "starr'", "ihr", "nach", ".", "O", "heh\u00b7re", "G\u00f6t\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ruf' ich, Freundin du der Morgenr\u00f6te,", "tokens": ["Ruf'", "ich", ",", "Freun\u00b7din", "du", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dank dir, da\u00df du kamst, mich zu besch\u00e4men!", "tokens": ["Dank", "dir", ",", "da\u00df", "du", "kamst", ",", "mich", "zu", "be\u00b7sch\u00e4\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und nun will ich r\u00fcstig mich ermannen,", "tokens": ["Und", "nun", "will", "ich", "r\u00fcs\u00b7tig", "mich", "er\u00b7man\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Deiner Spur zu folgen. Aber wirst du,", "tokens": ["Dei\u00b7ner", "Spur", "zu", "fol\u00b7gen", ".", "A\u00b7ber", "wirst", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$.", "KON", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Wenn ich schweifend im Olivenw\u00e4ldchen,", "tokens": ["Wenn", "ich", "schwei\u00b7fend", "im", "O\u00b7li\u00b7ven\u00b7w\u00e4ld\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Oder dort am Seegestad dich suche,", "tokens": ["O\u00b7der", "dort", "am", "See\u00b7ge\u00b7stad", "dich", "su\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wie ein Liebender der Liebsten nachgeht,", "tokens": ["Wie", "ein", "Lie\u00b7ben\u00b7der", "der", "Liebs\u00b7ten", "nach\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Gern dich finden lassen, deinem Treuen", "tokens": ["Gern", "dich", "fin\u00b7den", "las\u00b7sen", ",", "dei\u00b7nem", "Treu\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "VVINF", "VVINF", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Helle Lieder in die Seele hauchend,", "tokens": ["Hel\u00b7le", "Lie\u00b7der", "in", "die", "See\u00b7le", "hau\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "S\u00fc\u00dfmelodische, wie du in den Tagen", "tokens": ["S\u00fc\u00df\u00b7me\u00b7lo\u00b7di\u00b7sche", ",", "wie", "du", "in", "den", "Ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.12": {"text": "Unsrer jungen Liebe sie mir g\u00f6nntest?", "tokens": ["Uns\u00b7rer", "jun\u00b7gen", "Lie\u00b7be", "sie", "mir", "g\u00f6nn\u00b7test", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}