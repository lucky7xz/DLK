{"textgrid.poem.25790": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Vollmond", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gelbes Eis", "tokens": ["Gel\u00b7bes", "Eis"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Und gr\u00fcne Nebel.", "tokens": ["Und", "gr\u00fc\u00b7ne", "Ne\u00b7bel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Kranke Kallabl\u00fcten leuchten.", "tokens": ["Kran\u00b7ke", "Kal\u00b7la\u00b7bl\u00fc\u00b7ten", "leuch\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von den bleichen Bechern rinnet", "tokens": ["Von", "den", "blei\u00b7chen", "Be\u00b7chern", "rin\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Goldnes \u00d6l in sanften Str\u00f6men.", "tokens": ["Gold\u00b7nes", "\u00d6l", "in", "sanf\u00b7ten", "Str\u00f6\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Warmer Moder,", "tokens": ["War\u00b7mer", "Mo\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Nackte Sch\u00e4del.", "tokens": ["Nack\u00b7te", "Sch\u00e4\u00b7del", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "\u00dcber wei\u00dfe Marmorw\u00fcsten", "tokens": ["\u00dc\u00b7ber", "wei\u00b7\u00dfe", "Mar\u00b7mor\u00b7w\u00fcs\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fliehen lautlos", "tokens": ["Flie\u00b7hen", "laut\u00b7los"], "token_info": ["word", "word"], "pos": ["NN", "ADJD"], "meter": "+---", "measure": "dactylic.init"}, "line.5": {"text": "Schwarze Schw\u00e4ne.", "tokens": ["Schwar\u00b7ze", "Schw\u00e4\u00b7ne", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}