measure": "iambic.tetra"}, "line.2": {"text": "Jedoch gar bald besann sie sich:", "tokens": ["Je\u00b7doch", "gar", "bald", "be\u00b7sann", "sie", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sang: Dich lieb ich als mein Leben,", "tokens": ["Sie", "sang", ":", "Dich", "lieb", "ich", "als", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mehr als mein Leben lieb ich dich!", "tokens": ["Mehr", "als", "mein", "Le\u00b7ben", "lieb", "ich", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PPOSAT", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}