{"textgrid.poem.56188": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist's, o Vater, was ich verbrach?", "tokens": ["Was", "ist's", ",", "o", "Va\u00b7ter", ",", "was", "ich", "ver\u00b7brach", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "FM", "NN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du brichst mir das Herz, und fragst nicht darnach.", "tokens": ["Du", "brichst", "mir", "das", "Herz", ",", "und", "fragst", "nicht", "dar\u00b7nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "PTKNEG", "PAV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich hab ihm entsagt, nach deinem Befehl,", "tokens": ["Ich", "hab", "ihm", "ent\u00b7sagt", ",", "nach", "dei\u00b7nem", "Be\u00b7fehl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch nicht ihn vergessen, ich hab es nicht Hehl.", "tokens": ["Doch", "nicht", "ihn", "ver\u00b7ges\u00b7sen", ",", "ich", "hab", "es", "nicht", "Hehl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PPER", "VVPP", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "NN", "$."], "meter": "----+--+--+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Noch lebt er in mir, ich selbst bin tot,", "tokens": ["Noch", "lebt", "er", "in", "mir", ",", "ich", "selbst", "bin", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$,", "PPER", "ADV", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und \u00fcber mich schaltet dein strenges Gebot.", "tokens": ["Und", "\u00fc\u00b7ber", "mich", "schal\u00b7tet", "dein", "stren\u00b7ges", "Ge\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Wann Herz und Wille gebrochen sind,", "tokens": ["Wann", "Herz", "und", "Wil\u00b7le", "ge\u00b7bro\u00b7chen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bittet um eins noch dein armes Kind.", "tokens": ["Bit\u00b7tet", "um", "eins", "noch", "dein", "ar\u00b7mes", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIS", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-----+-+", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Wann bald mein m\u00fcdes Auge sich schlie\u00dft,", "tokens": ["Wann", "bald", "mein", "m\u00fc\u00b7des", "Au\u00b7ge", "sich", "schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und Tr\u00e4nen vielleicht das deine vergie\u00dft;", "tokens": ["Und", "Tr\u00e4\u00b7nen", "viel\u00b7leicht", "das", "dei\u00b7ne", "ver\u00b7gie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ART", "PPOSAT", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "An der Kirchwand dort, beim Holunderstrauch,", "tokens": ["An", "der", "Kirch\u00b7wand", "dort", ",", "beim", "Ho\u00b7lun\u00b7der\u00b7strauch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$,", "APPRART", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo die Mutter liegt, da lege mich auch.", "tokens": ["Wo", "die", "Mut\u00b7ter", "liegt", ",", "da", "le\u00b7ge", "mich", "auch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}}}}}