{"textgrid.poem.61192": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "L\u00fcbecks Bedr\u00e4ngnis", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun reich', o Muse, den Pokal,", "tokens": ["Nun", "reich'", ",", "o", "Mu\u00b7se", ",", "den", "Po\u00b7kal", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch la\u00df von hellem Zorn ihn sch\u00e4umen!", "tokens": ["Doch", "la\u00df", "von", "hel\u00b7lem", "Zorn", "ihn", "sch\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Lied gib, das wie Blitzesstrahl", "tokens": ["Ein", "Lied", "gib", ",", "das", "wie", "Blit\u00b7zes\u00b7strahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVIMP", "$,", "PRELS", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schl\u00e4fer schreck' aus ihren Tr\u00e4umen!", "tokens": ["Die", "Schl\u00e4\u00b7fer", "schreck'", "aus", "ih\u00b7ren", "Tr\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Ruf der Glocke zur Gefahr", "tokens": ["Wie", "Ruf", "der", "Glo\u00b7cke", "zur", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erschall' es weit im deutschen Lande;", "tokens": ["Er\u00b7schall'", "es", "weit", "im", "deut\u00b7schen", "Lan\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es gilt der Stadt, die mich gebar,", "tokens": ["Es", "gilt", "der", "Stadt", ",", "die", "mich", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Mutter, die man schl\u00e4gt in Bande!", "tokens": ["Der", "Mut\u00b7ter", ",", "die", "man", "schl\u00e4gt", "in", "Ban\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie steigst, o L\u00fcbeck, du herauf", "tokens": ["Wie", "steigst", ",", "o", "L\u00fc\u00b7beck", ",", "du", "her\u00b7auf"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "$,", "FM", "NE", "$,", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In alter Pracht vor meinen Sinnen", "tokens": ["In", "al\u00b7ter", "Pracht", "vor", "mei\u00b7nen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "An des beflaggten Stromes Lauf,", "tokens": ["An", "des", "be\u00b7flagg\u00b7ten", "Stro\u00b7mes", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit stolzen T\u00fcrmen, schart'gen Zinnen!", "tokens": ["Mit", "stol\u00b7zen", "T\u00fcr\u00b7men", ",", "schart'\u00b7gen", "Zin\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dort war's, wo deiner Erker Zahl", "tokens": ["Dort", "wa\u00b7r's", ",", "wo", "dei\u00b7ner", "Er\u00b7ker", "Zahl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "PWAV", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Der Hansa Boten wartend z\u00e4hlten,", "tokens": ["Der", "Han\u00b7sa", "Bo\u00b7ten", "war\u00b7tend", "z\u00e4hl\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dort, wo die V\u00e4ter hoch im Saal", "tokens": ["Dort", ",", "wo", "die", "V\u00e4\u00b7ter", "hoch", "im", "Saal"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Haupt f\u00fcr leere Kronen w\u00e4hlten.", "tokens": ["Ein", "Haupt", "f\u00fcr", "lee\u00b7re", "Kro\u00b7nen", "w\u00e4hl\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn eine F\u00fcrstin standest du,", "tokens": ["Denn", "ei\u00b7ne", "F\u00fcrs\u00b7tin", "stan\u00b7dest", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Markt war dein, und dein die Wege,", "tokens": ["Der", "Markt", "war", "dein", ",", "und", "dein", "die", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "$,", "KON", "PPOSAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du f\u00fchrtest reich dem S\u00fcden zu,", "tokens": ["Du", "f\u00fchr\u00b7test", "reich", "dem", "S\u00fc\u00b7den", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was nur gedieh in Nordens Pflege.", "tokens": ["Was", "nur", "ge\u00b7dieh", "in", "Nor\u00b7dens", "Pfle\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es bot dir Norweg seinen Zoll,", "tokens": ["Es", "bot", "dir", "Nor\u00b7weg", "sei\u00b7nen", "Zoll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Schwede bog sein Haupt, der D\u00e4ne,", "tokens": ["Der", "Schwe\u00b7de", "bog", "sein", "Haupt", ",", "der", "D\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn deine Schiffe segelvoll", "tokens": ["Wenn", "dei\u00b7ne", "Schif\u00b7fe", "se\u00b7gel\u00b7voll"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vor\u00fcberflohn, des Meeres Schw\u00e4ne.", "tokens": ["Vor\u00b7\u00fc\u00b7berf\u00b7lohn", ",", "des", "Mee\u00b7res", "Schw\u00e4\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und jetzt? - Verh\u00fcll' ihn nicht im Lied,", "tokens": ["Und", "jetzt", "?", "Ver\u00b7h\u00fcll'", "ihn", "nicht", "im", "Lied", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "$(", "VVIMP", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Schmerz, da\u00df solcher Glanz zerronnen;", "tokens": ["Den", "Schmerz", ",", "da\u00df", "sol\u00b7cher", "Glanz", "zer\u00b7ron\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur leis um deine Stirn noch zieht", "tokens": ["Nur", "leis", "um", "dei\u00b7ne", "Stirn", "noch", "zieht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Glorie der versunknen Sonnen.", "tokens": ["Die", "Glo\u00b7rie", "der", "ver\u00b7sun\u00b7knen", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl beugt sich still, wen eh'rnen Schritts", "tokens": ["Wohl", "beugt", "sich", "still", ",", "wen", "eh'r\u00b7nen", "Schritts"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PTKVZ", "$,", "PWS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein gro\u00df Geschick im Gang versehret,", "tokens": ["Ein", "gro\u00df", "Ge\u00b7schick", "im", "Gang", "ver\u00b7seh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch das emp\u00f6rt, wenn Menschenwitz", "tokens": ["Doch", "das", "em\u00b7p\u00f6rt", ",", "wenn", "Men\u00b7schen\u00b7witz"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "An alter Gr\u00f6\u00dfe h\u00e4misch zehret.", "tokens": ["An", "al\u00b7ter", "Gr\u00f6\u00b7\u00dfe", "h\u00e4\u00b7misch", "zeh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Jetzt tr\u00e4gst du ", "tokens": ["Jetzt", "tr\u00e4gst", "du"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Zerpfl\u00fcckt man deinem Aar mit Hadern,", "tokens": ["Zer\u00b7pfl\u00fcckt", "man", "dei\u00b7nem", "Aar", "mit", "Ha\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durchschneidet kleinen Ingrimms dir", "tokens": ["Durch\u00b7schnei\u00b7det", "klei\u00b7nen", "In\u00b7grimms", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Stra\u00dfen, deines Lebens Adern.", "tokens": ["Die", "Stra\u00b7\u00dfen", ",", "dei\u00b7nes", "Le\u00b7bens", "A\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O Schmach und Scham! Das Land hindurch", "tokens": ["O", "Schmach", "und", "Scham", "!", "Das", "Land", "hin\u00b7durch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "$.", "ART", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist tiefer Fried' in S\u00fcd und Norden,", "tokens": ["Ist", "tie\u00b7fer", "Fried'", "in", "S\u00fcd", "und", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du aber bist wie eine Burg,", "tokens": ["Du", "a\u00b7ber", "bist", "wie", "ei\u00b7ne", "Burg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die man umlagert h\u00e4lt, geworden!", "tokens": ["Die", "man", "um\u00b7la\u00b7gert", "h\u00e4lt", ",", "ge\u00b7wor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "VVPP", "VVFIN", "$,", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du zahlst es sp\u00e4t uns heim f\u00fcrwahr,", "tokens": ["Du", "zahlst", "es", "sp\u00e4t", "uns", "heim", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PPER", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O D\u00e4nemark, mit bittrem Leide,", "tokens": ["O", "D\u00e4\u00b7ne\u00b7mark", ",", "mit", "bitt\u00b7rem", "Lei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df einst vor uns dein Waldemar", "tokens": ["Da\u00df", "einst", "vor", "uns", "dein", "Wal\u00b7de\u00b7mar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erzittert' auf Bornh\u00f6veds Heide:", "tokens": ["Er\u00b7zit\u00b7tert'", "auf", "Born\u00b7h\u00f6\u00b7veds", "Hei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "NE", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Da\u00df er, der kaum noch trunknen Muts", "tokens": ["Da\u00df", "er", ",", "der", "kaum", "noch", "trunk\u00b7nen", "Muts"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PRELS", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geprunkt im Schwarm der Bogenspanner,", "tokens": ["Ge\u00b7prunkt", "im", "Schwarm", "der", "Bo\u00b7gen\u00b7span\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auf fl\u00fccht'gem Renner, wund, voll Bluts", "tokens": ["Auf", "fl\u00fccht'\u00b7gem", "Ren\u00b7ner", ",", "wund", ",", "voll", "Bluts"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "$,", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Heimsprengte nach verlornem Banner.", "tokens": ["Heim\u00b7spreng\u00b7te", "nach", "ver\u00b7lor\u00b7nem", "Ban\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch sei's. Du warst uns ewig feind;", "tokens": ["Doch", "sei'", "s.", "Du", "warst", "uns", "e\u00b7wig", "feind", ";"], "token_info": ["word", "word", "abbreviation", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVIMP", "PPER", "VAFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und magst du B\u00fcndner auch dich w\u00e4hnen:", "tokens": ["Und", "magst", "du", "B\u00fcnd\u00b7ner", "auch", "dich", "w\u00e4h\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von Herzen hast du's nie gemeint,", "tokens": ["Von", "Her\u00b7zen", "hast", "du's", "nie", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es taugt der Deutsche nicht zum D\u00e4nen.", "tokens": ["Es", "taugt", "der", "Deut\u00b7sche", "nicht", "zum", "D\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wir sahn uns bei der D\u00f6rfer Brand", "tokens": ["Wir", "sahn", "uns", "bei", "der", "D\u00f6r\u00b7fer", "Brand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "m