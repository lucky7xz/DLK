{"textgrid.poem.53863": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Confessio", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir M\u00e4nner aus Berlin und Neuk\u00f6lln,", "tokens": ["Wir", "M\u00e4n\u00b7ner", "aus", "Ber\u00b7lin", "und", "Neu\u00b7k\u00f6lln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NE", "KON", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "wir wissen leider nicht, was wir w\u00f6lln.", "tokens": ["wir", "wis\u00b7sen", "lei\u00b7der", "nicht", ",", "was", "wir", "w\u00f6lln", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mal . . .", "tokens": ["Mal", ".", ".", "."], "token_info": ["word", "punct", "punct", "punct"], "pos": ["NN", "$.", "$.", "$."], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Mal konzentrieren wir uns auf die eine,", "tokens": ["Mal", "kon\u00b7zent\u00b7rie\u00b7ren", "wir", "uns", "auf", "die", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "APPR", "ART", "ART", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "spielen mit ihr: die oder keine,", "tokens": ["spie\u00b7len", "mit", "ihr", ":", "die", "o\u00b7der", "kei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "$.", "ART", "KON", "PIAT", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "legen uns fest, ohne Bedenken,", "tokens": ["le\u00b7gen", "uns", "fest", ",", "oh\u00b7ne", "Be\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "wollen auch einem S\u00f6hnlein das Leben schenken,", "tokens": ["wol\u00b7len", "auch", "ei\u00b7nem", "S\u00f6hn\u00b7lein", "das", "Le\u00b7ben", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "verlegen den Sitz der Seele, als Gatte,", "tokens": ["ver\u00b7le\u00b7gen", "den", "Sitz", "der", "See\u00b7le", ",", "als", "Gat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,", "KOUS", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "oberhalb des Tisches Platte \u2013", "tokens": ["o\u00b7ber\u00b7halb", "des", "Ti\u00b7sches", "Plat\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und sind \u00fcberhaupt sehr monogam.", "tokens": ["Und", "sind", "\u00fc\u00b7ber\u00b7haupt", "sehr", "mo\u00b7no\u00b7gam", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Wie das so kam . . .", "tokens": ["Wie", "das", "so", "kam", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "PDS", "ADV", "VVFIN", "$.", "$.", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Da lockten die andern. Ihrer sind viele.", "tokens": ["Da", "lock\u00b7ten", "die", "an\u00b7dern", ".", "Ih\u00b7rer", "sind", "vie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$.", "PPOSAT", "VAFIN", "PIS", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie lockten zu kindlichem Zimmerspiele", "tokens": ["Sie", "lock\u00b7ten", "zu", "kind\u00b7li\u00b7chem", "Zim\u00b7mer\u00b7spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "\u2013 Bewegung l\u00e4cherlich, Preis bedeutend \u2013", "tokens": ["\u2013", "Be\u00b7we\u00b7gung", "l\u00e4\u00b7cher\u00b7lich", ",", "Preis", "be\u00b7deu\u00b7tend", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NN", "ADJD", "$,", "NN", "VVPP", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Immer nur eine Glocke l\u00e4utend?", "tokens": ["Im\u00b7mer", "nur", "ei\u00b7ne", "Glo\u00b7cke", "l\u00e4u\u00b7tend", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Immer an eine Frau gebunden?", "tokens": ["Im\u00b7mer", "an", "ei\u00b7ne", "Frau", "ge\u00b7bun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "So sollen uns alle Lebensstunden", "tokens": ["So", "sol\u00b7len", "uns", "al\u00b7le", "Le\u00b7bens\u00b7stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "verrinnen? Ohne boshafte Feste?", "tokens": ["ver\u00b7rin\u00b7nen", "?", "Oh\u00b7ne", "bos\u00b7haf\u00b7te", "Fes\u00b7te", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVINF", "$.", "APPR", "ADJA", "NN", "$."], "meter": "-+-+----+-", "measure": "unknown.measure.tri"}, "line.8": {"text": "Liegt nicht drau\u00dfen das Allerbeste?", "tokens": ["Liegt", "nicht", "drau\u00b7\u00dfen", "das", "Al\u00b7ler\u00b7bes\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "M\u00e4dchen? Freiheit? Frauen nach Wahl \u2013?", "tokens": ["M\u00e4d\u00b7chen", "?", "Frei\u00b7heit", "?", "Frau\u00b7en", "nach", "Wahl", "\u2013", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "APPR", "NN", "$(", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Gesagt, getan.", "tokens": ["Ge\u00b7sagt", ",", "ge\u00b7tan", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mal . . .", "tokens": ["Mal", ".", ".", "."], "token_info": ["word", "punct", "punct", "punct"], "pos": ["NN", "$.", "$.", "$."], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Mal trudeln wir durch bl\u00e4uliche Stunden,", "tokens": ["Mal", "tru\u00b7deln", "wir", "durch", "bl\u00e4u\u00b7li\u00b7che", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "tun scheinbar an fr\u00f6hlichem Wechsel gesunden;", "tokens": ["tun", "schein\u00b7bar", "an", "fr\u00f6h\u00b7li\u00b7chem", "Wech\u00b7sel", "ge\u00b7sun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "k\u00f6nnen es manchmal gar nicht fassen,", "tokens": ["k\u00f6n\u00b7nen", "es", "manch\u00b7mal", "gar", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "welch feine Damen bei uns arbeiten lassen.", "tokens": ["welch", "fei\u00b7ne", "Da\u00b7men", "bei", "uns", "ar\u00b7bei\u00b7ten", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "APPR", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und jede Seele, die eine hatte,", "tokens": ["Und", "je\u00b7de", "See\u00b7le", ",", "die", "ei\u00b7ne", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PIS", "VAFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "liegt unterhalb des Tisches Platte.", "tokens": ["liegt", "un\u00b7ter\u00b7halb", "des", "Ti\u00b7sches", "Plat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und sind \u00fcberhaupt sehr polygam.", "tokens": ["Und", "sind", "\u00fc\u00b7ber\u00b7haupt", "sehr", "po\u00b7ly\u00b7gam", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Wie das so kam . . .", "tokens": ["Wie", "das", "so", "kam", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "PDS", "ADV", "VVFIN", "$.", "$.", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "So herumwirtschaften? Lebensl\u00e4nglich?", "tokens": ["So", "her\u00b7um\u00b7wirt\u00b7schaf\u00b7ten", "?", "Le\u00b7bens\u00b7l\u00e4ng\u00b7lich", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "ADJD", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Pl\u00f6tzlich werden wir recht bedenklich.", "tokens": ["Pl\u00f6tz\u00b7lich", "wer\u00b7den", "wir", "recht", "be\u00b7denk\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Sehnen uns beinah fiebrig zur\u00fcck", "tokens": ["Seh\u00b7nen", "uns", "bei\u00b7nah", "fieb\u00b7rig", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKVZ"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "nach Einsamkeit und Familiengl\u00fcck.", "tokens": ["nach", "Ein\u00b7sam\u00b7keit", "und", "Fa\u00b7mi\u00b7li\u00b7en\u00b7gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und fangen als ein ganzer Mann", "tokens": ["Und", "fan\u00b7gen", "als", "ein", "gan\u00b7zer", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die Geschichte wieder von vorne an.", "tokens": ["die", "Ge\u00b7schich\u00b7te", "wie\u00b7der", "von", "vor\u00b7ne", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADV", "PTKVZ", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Wir M\u00e4nner aus Berlin und Neuk\u00f6lln,", "tokens": ["Wir", "M\u00e4n\u00b7ner", "aus", "Ber\u00b7lin", "und", "Neu\u00b7k\u00f6lln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NE", "KON", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "wir wissen leider nicht, was wir w\u00f6lln.", "tokens": ["wir", "wis\u00b7sen", "lei\u00b7der", "nicht", ",", "was", "wir", "w\u00f6lln", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wir piesacken uns und unsre Fraun;", "tokens": ["Wir", "pie\u00b7sa\u00b7cken", "uns", "und", "uns\u00b7re", "Fraun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "uns sollten sie mal den Hintern aushaun.", "tokens": ["uns", "soll\u00b7ten", "sie", "mal", "den", "Hin\u00b7tern", "aus\u00b7haun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ART", "NN", "VVIZU", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Bileams Esel, ich und du.", "tokens": ["Bi\u00b7leams", "E\u00b7sel", ",", "ich", "und", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "KON", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gott schenke uns allen die ewige Ruh.", "tokens": ["Gott", "schen\u00b7ke", "uns", "al\u00b7len", "die", "e\u00b7wi\u00b7ge", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PIS", "ART", "ADJA", "NN", "$."], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Amen.", "tokens": ["A\u00b7men", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}