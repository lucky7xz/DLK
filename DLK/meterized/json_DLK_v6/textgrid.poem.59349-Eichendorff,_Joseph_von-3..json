{"textgrid.poem.59349": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon kehren die V\u00f6gel wieder ein,", "tokens": ["Schon", "keh\u00b7ren", "die", "V\u00f6\u00b7gel", "wie\u00b7der", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es schallen die alten Lieder,", "tokens": ["Es", "schal\u00b7len", "die", "al\u00b7ten", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ach, die fr\u00f6hliche Jugend mein", "tokens": ["Ach", ",", "die", "fr\u00f6h\u00b7li\u00b7che", "Ju\u00b7gend", "mein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "PPOSAT"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Kommt sie wohl auch noch wieder?", "tokens": ["Kommt", "sie", "wohl", "auch", "noch", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df nicht, was ich so t\u00f6richt bin!", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "ich", "so", "t\u00f6\u00b7richt", "bin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wolken im Herbstwind jagen,", "tokens": ["Wol\u00b7ken", "im", "Herbst\u00b7wind", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die V\u00f6gel ziehn \u00fcber die W\u00e4lder hin,", "tokens": ["Die", "V\u00f6\u00b7gel", "ziehn", "\u00fc\u00b7ber", "die", "W\u00e4l\u00b7der", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das klang wie in Fr\u00fchlingstagen.", "tokens": ["Das", "klang", "wie", "in", "Fr\u00fch\u00b7lings\u00b7ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Dort auf dem Berge da steht ein Baum,", "tokens": ["Dort", "auf", "dem", "Ber\u00b7ge", "da", "steht", "ein", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Drin jubeln die Wanderg\u00e4ste,", "tokens": ["Drin", "ju\u00b7beln", "die", "Wan\u00b7der\u00b7g\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er aber, m\u00fcde, r\u00fchrt wie im Traum", "tokens": ["Er", "a\u00b7ber", ",", "m\u00fc\u00b7de", ",", "r\u00fchrt", "wie", "im", "Traum"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "ADJD", "$,", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Noch einmal Wipfel und \u00c4ste.", "tokens": ["Noch", "ein\u00b7mal", "Wip\u00b7fel", "und", "\u00c4s\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}