{"textgrid.poem.25542": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo ist der Freund, der Freud' und Leid so gleich", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ist der Freund, der Freud' und Leid so gleich", "tokens": ["Wo", "ist", "der", "Freund", ",", "der", "Freud'", "und", "Leid", "so", "gleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Freunden theilt, wie du?", "tokens": ["Mit", "Freun\u00b7den", "theilt", ",", "wie", "du", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo ist ein Mensch, an Scherz, wie du, so reich?", "tokens": ["Wo", "ist", "ein", "Mensch", ",", "an", "Scherz", ",", "wie", "du", ",", "so", "reich", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "APPR", "NN", "$,", "PWAV", "PPER", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wer h\u00f6rte Klagen je theilnehmender wohl zu?", "tokens": ["Wer", "h\u00f6r\u00b7te", "Kla\u00b7gen", "je", "theil\u00b7neh\u00b7men\u00b7der", "wohl", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du scherzest nicht, wenn unterm Trauerflor'", "tokens": ["Du", "scher\u00b7zest", "nicht", ",", "wenn", "un\u00b7term", "Trau\u00b7e\u00b7rflor'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mir eine Thr\u00e4n' entschl\u00fcpft,", "tokens": ["Mir", "ei\u00b7ne", "Thr\u00e4n'", "ent\u00b7schl\u00fcpft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du seufzest nicht mir Dissonanzen vor,", "tokens": ["Du", "seuf\u00b7zest", "nicht", "mir", "Dis\u00b7so\u00b7nan\u00b7zen", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wenn jugendlich das Blut in meinen Adern h\u00fcpft.", "tokens": ["Wenn", "ju\u00b7gend\u00b7lich", "das", "Blut", "in", "mei\u00b7nen", "A\u00b7dern", "h\u00fcpft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich bin mit dir, wenn Amarant mir fehlt,", "tokens": ["Ich", "bin", "mit", "dir", ",", "wenn", "A\u00b7ma\u00b7rant", "mir", "fehlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Noch gl\u00fccklich; aber ach!", "tokens": ["Noch", "gl\u00fcck\u00b7lich", ";", "a\u00b7ber", "ach", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Schickt einst wie du, er, den mein Herz erw\u00e4hlt,", "tokens": ["Schickt", "einst", "wie", "du", ",", "er", ",", "den", "mein", "Herz", "er\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOKOM", "PPER", "$,", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "In meine Launen sich, so bin ich's tausendfach.", "tokens": ["In", "mei\u00b7ne", "Lau\u00b7nen", "sich", ",", "so", "bin", "ich's", "tau\u00b7send\u00b7fach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "$,", "ADV", "VAFIN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}