{"dta.poem.12640": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Abbildung der Liebe.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der liebe rosen-blat hat d\u00f6rner zu gefehrten,", "tokens": ["Der", "lie\u00b7be", "ro\u00b7sen\u00b7blat", "hat", "d\u00f6r\u00b7ner", "zu", "ge\u00b7fehr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus welchen nach der lust der unlust fr\u00fcchte bl\u00fchn;", "tokens": ["Aus", "wel\u00b7chen", "nach", "der", "lust", "der", "un\u00b7lust", "fr\u00fcch\u00b7te", "bl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "APPR", "ART", "NN", "ART", "NN", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie hebt ihr haupt empor, als wie auf zauber-gerten,", "tokens": ["Sie", "hebt", "ihr", "haupt", "em\u00b7por", ",", "als", "wie", "auf", "zau\u00b7ber\u00b7ger\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "KOUS", "KOKOM", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und kan durch einen blick uns ins geh\u00e4ge ziehn.", "tokens": ["Und", "kan", "durch", "ei\u00b7nen", "blick", "uns", "ins", "ge\u00b7h\u00e4\u00b7ge", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ART", "NN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dann st\u00f6st der freyheit schiff an ungeheure klippen,", "tokens": ["Dann", "st\u00f6st", "der", "frey\u00b7heit", "schiff", "an", "un\u00b7ge\u00b7heu\u00b7re", "klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es bleibt, eh wirs vermeynt, auf einer sand-banck stehn,", "tokens": ["Es", "bleibt", ",", "eh", "wirs", "ver\u00b7meynt", ",", "auf", "ei\u00b7ner", "san\u00b7dbanck", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "VVPP", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und lacht kein trost uns an von rosen-lichten lippen;", "tokens": ["Und", "lacht", "kein", "trost", "uns", "an", "von", "ro\u00b7sen\u00b7lich\u00b7ten", "lip\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So heists: O himmel hilff! wir m\u00fcssen hier vergehn.", "tokens": ["So", "heists", ":", "O", "him\u00b7mel", "hilff", "!", "wir", "m\u00fcs\u00b7sen", "hier", "ver\u00b7gehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "NE", "NN", "NN", "$.", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da stimmt das hertze an: Verlasse mich, o liebe!", "tokens": ["Da", "stimmt", "das", "hert\u00b7ze", "an", ":", "Ver\u00b7las\u00b7se", "mich", ",", "o", "lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$.", "NN", "PPER", "$,", "FM", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dann heists: Entfernet euch, die ihr ans lieben denckt,", "tokens": ["Dann", "heists", ":", "Ent\u00b7fer\u00b7net", "euch", ",", "die", "ihr", "ans", "lie\u00b7ben", "denckt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "VVFIN", "PPER", "$,", "PRELS", "PPER", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Durch lieben wird uns nur der wohlfahrts-himmel tr\u00fcbe,", "tokens": ["Durch", "lie\u00b7ben", "wird", "uns", "nur", "der", "wohl\u00b7fahrts\u00b7him\u00b7mel", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VAFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nichts ist, was unsre brust mehr als die liebe kr\u00e4nckt.", "tokens": ["Nichts", "ist", ",", "was", "uns\u00b7re", "brust", "mehr", "als", "die", "lie\u00b7be", "kr\u00e4nckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "PIAT", "KOKOM", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch, sind die dornen weg, so greifft man nach den rosen,", "tokens": ["Doch", ",", "sind", "die", "dor\u00b7nen", "weg", ",", "so", "greifft", "man", "nach", "den", "ro\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "ART", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "PIS", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es giebt die be\u00dfre zeit uns andre sinnen ein,", "tokens": ["Es", "giebt", "die", "be\u00df\u00b7re", "zeit", "uns", "and\u00b7re", "sin\u00b7nen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dann k\u00f6nnen wir vergn\u00fcgt in den gedancken losen,", "tokens": ["Dann", "k\u00f6n\u00b7nen", "wir", "ver\u00b7gn\u00fcgt", "in", "den", "ge\u00b7dan\u00b7cken", "lo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Auf welcher seite wiram liebsten wollen seyn.", "tokens": ["Auf", "wel\u00b7cher", "sei\u00b7te", "wi\u00b7ram", "liebs\u00b7ten", "wol\u00b7len", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ADJA", "NN", "VVFIN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und so verlieren wir die kurtzen lebens-zeiten,", "tokens": ["Und", "so", "ver\u00b7lie\u00b7ren", "wir", "die", "kurt\u00b7zen", "le\u00b7bens\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Das schiff des lebens laufft dem hafen n\u00e4her zu,", "tokens": ["Das", "schiff", "des", "le\u00b7bens", "laufft", "dem", "ha\u00b7fen", "n\u00e4\u00b7her", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Bi\u00df uns der winter pflegt in so ein land zu leiten,", "tokens": ["Bi\u00df", "uns", "der", "win\u00b7ter", "pflegt", "in", "so", "ein", "land", "zu", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NE", "VVFIN", "APPR", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wo man der liebe baum mit erde decket zu.", "tokens": ["Wo", "man", "der", "lie\u00b7be", "baum", "mit", "er\u00b7de", "de\u00b7cket", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}