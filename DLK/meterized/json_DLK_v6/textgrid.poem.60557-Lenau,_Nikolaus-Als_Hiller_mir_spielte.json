{"textgrid.poem.60557": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Als Hiller mir spielte", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn seine Sonne hat das Licht,", "tokens": ["Wenn", "sei\u00b7ne", "Son\u00b7ne", "hat", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus der ein Meer von Strahlen bricht,", "tokens": ["Aus", "der", "ein", "Meer", "von", "Strah\u00b7len", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ist die Sonne f\u00fcr den Klang,", "tokens": ["Wo", "ist", "die", "Son\u00b7ne", "f\u00fcr", "den", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dies Meer ausstr\u00f6mend von Gesang?", "tokens": ["Dies", "Meer", "aus\u00b7str\u00f6\u00b7mend", "von", "Ge\u00b7sang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}