{"dta.poem.9133": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "X.  \n Das schlaffende gl\u00fccke.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Mein gl\u00fccke schl\u00e4fft nun lange zeit/", "tokens": ["Mein", "gl\u00fc\u00b7cke", "schl\u00e4fft", "nun", "lan\u00b7ge", "zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und will noch nicht erwachen/", "tokens": ["Und", "will", "noch", "nicht", "er\u00b7wa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und meine furchtsamkeit", "tokens": ["Und", "mei\u00b7ne", "furcht\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Kan sich gar wenig hoffnung machen/", "tokens": ["Kan", "sich", "gar", "we\u00b7nig", "hoff\u00b7nung", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dann wann es helffen soll/", "tokens": ["Dann", "wann", "es", "helf\u00b7fen", "soll", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So schnarcht es mir die ohren voll.", "tokens": ["So", "schnarcht", "es", "mir", "die", "oh\u00b7ren", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Der gantze leib ist ausgestreckt/", "tokens": ["Der", "gant\u00b7ze", "leib", "ist", "aus\u00b7ge\u00b7streckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der mund ist zugeschlossen/", "tokens": ["Der", "mund", "ist", "zu\u00b7ge\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die augen sind versteckt/", "tokens": ["Die", "au\u00b7gen", "sind", "ver\u00b7steckt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die krafft der sinnen ist zerflossen/", "tokens": ["Die", "krafft", "der", "sin\u00b7nen", "ist", "zer\u00b7flos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nur in der blossen brust", "tokens": ["Nur", "in", "der", "blos\u00b7sen", "brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Treibt noch der athem seine lust.", "tokens": ["Treibt", "noch", "der", "at\u00b7hem", "sei\u00b7ne", "lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Ich sto\u00df/ und brauche was ich kan", "tokens": ["Ich", "sto\u00df", "/", "und", "brau\u00b7che", "was", "ich", "kan"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KON", "VVFIN", "PWS", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die kr\u00e4ffte meines lebens/", "tokens": ["Die", "kr\u00e4ff\u00b7te", "mei\u00b7nes", "le\u00b7bens", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich schrey die ohren an/", "tokens": ["Ich", "schrey", "die", "oh\u00b7ren", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch m\u00fch und arbeit ist vergebens:", "tokens": ["Doch", "m\u00fch", "und", "ar\u00b7beit", "ist", "ver\u00b7ge\u00b7bens", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dann mein verh\u00e4ngni\u00df spricht/", "tokens": ["Dann", "mein", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "spricht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dein gl\u00fccke schl\u00e4fft und h\u00f6rt dich nicht.", "tokens": ["Dein", "gl\u00fc\u00b7cke", "schl\u00e4fft", "und", "h\u00f6rt", "dich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Wolan mein gl\u00fccke schlaff nur fort/", "tokens": ["Wo\u00b7lan", "mein", "gl\u00fc\u00b7cke", "schlaff", "nur", "fort", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich werde zwar indessen", "tokens": ["Ich", "wer\u00b7de", "zwar", "in\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Noch manchen lieben ort", "tokens": ["Noch", "man\u00b7chen", "lie\u00b7ben", "ort"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und manche lustigkeit vergessen/", "tokens": ["Und", "man\u00b7che", "lus\u00b7tig\u00b7keit", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich werde noch der pein", "tokens": ["Ich", "wer\u00b7de", "noch", "der", "pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der ungewi\u00dfheit dienstbar seyn.", "tokens": ["Der", "un\u00b7ge\u00b7wi\u00df\u00b7heit", "dienst\u00b7bar", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Doch wer zu lange schlaffen mu\u00df/", "tokens": ["Doch", "wer", "zu", "lan\u00b7ge", "schlaf\u00b7fen", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der kan auch lange wachen/", "tokens": ["Der", "kan", "auch", "lan\u00b7ge", "wa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und kan den \u00fcberflu\u00df", "tokens": ["Und", "kan", "den", "\u00fc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der sp\u00e4ten nacht zum tage machen/", "tokens": ["Der", "sp\u00e4\u00b7ten", "nacht", "zum", "ta\u00b7ge", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn er die lager-statt/", "tokens": ["Wenn", "er", "die", "la\u00b7ger\u00b7statt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nur lang genug gedr\u00fccket hat.", "tokens": ["Nur", "lang", "ge\u00b7nug", "ge\u00b7dr\u00fc\u00b7cket", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Darum mein gl\u00fccke schlaff nur aus/", "tokens": ["Da\u00b7rum", "mein", "gl\u00fc\u00b7cke", "schlaff", "nur", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bringe mit den segen", "tokens": ["Und", "brin\u00b7ge", "mit", "den", "se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hernachmals in das haus/", "tokens": ["Her\u00b7nach\u00b7mals", "in", "das", "haus", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Jetzt will ich dich nicht mehr bewegen/", "tokens": ["Jetzt", "will", "ich", "dich", "nicht", "mehr", "be\u00b7we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die noth kan nicht bestehn/", "tokens": ["Die", "noth", "kan", "nicht", "be\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sie mu\u00df doch auch zu bette gehn.", "tokens": ["Sie", "mu\u00df", "doch", "auch", "zu", "bet\u00b7te", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PTKZU", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Wo sie mich in der jugend l\u00e4st", "tokens": ["Wo", "sie", "mich", "in", "der", "ju\u00b7gend", "l\u00e4st"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey der gewohnheit bleiben/", "tokens": ["Bey", "der", "ge\u00b7wohn\u00b7heit", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "So werd ich wol den rest", "tokens": ["So", "werd", "ich", "wol", "den", "rest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Des alters mehr begl\u00fcckt vertreiben/", "tokens": ["Des", "al\u00b7ters", "mehr", "be\u00b7gl\u00fcckt", "ver\u00b7trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVPP", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Inzwischen wird die schuld", "tokens": ["I\u00b7nzwi\u00b7schen", "wird", "die", "schuld"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Des gl\u00fcckes leichter durch gedult.", "tokens": ["Des", "gl\u00fc\u00b7ckes", "leich\u00b7ter", "durch", "ge\u00b7dult", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}