{"dta.poem.2887": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "88.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du gef\u00e4llst der Welt, wird dir die Welt gefallen;", "tokens": ["Wenn", "du", "ge\u00b7f\u00e4llst", "der", "Welt", ",", "wird", "dir", "die", "Welt", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$,", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wer sich selbst gef\u00e4llt, das ist ein Gl\u00fcck vor allen:", "tokens": ["Doch", "wer", "sich", "selbst", "ge\u00b7f\u00e4llt", ",", "das", "ist", "ein", "Gl\u00fcck", "vor", "al\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "ADV", "VVPP", "$,", "PDS", "VAFIN", "ART", "NN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sich zu gefallen, nicht wie sich ein Thor gef\u00e4llt,", "tokens": ["Sich", "zu", "ge\u00b7fal\u00b7len", ",", "nicht", "wie", "sich", "ein", "Thor", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "$,", "PTKNEG", "PWAV", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Eitler, der allein sich d\u00fcnkt die ganze Welt.", "tokens": ["Ein", "Eit\u00b7ler", ",", "der", "al\u00b7lein", "sich", "d\u00fcnkt", "die", "gan\u00b7ze", "Welt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PRF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der schwache Wahn geht wie ein Glas vom Sto\u00df entzwei,", "tokens": ["Der", "schwa\u00b7che", "Wahn", "geht", "wie", "ein", "Glas", "vom", "Sto\u00df", "ent\u00b7zwei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KOKOM", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und merkt, indem er bricht, da\u00df au\u00dfer ihm was sei.", "tokens": ["Und", "merkt", ",", "in\u00b7dem", "er", "bricht", ",", "da\u00df", "au\u00b7\u00dfer", "ihm", "was", "sei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "APPR", "PPER", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch du gefalle dir, weil dir die Welt gef\u00e4llt,", "tokens": ["Doch", "du", "ge\u00b7fal\u00b7le", "dir", ",", "weil", "dir", "die", "Welt", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil du die Welt in dir und dich f\u00fchlst in der Welt.", "tokens": ["Weil", "du", "die", "Welt", "in", "dir", "und", "dich", "f\u00fchlst", "in", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPER", "KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}