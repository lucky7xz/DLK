{"textgrid.poem.65679": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df das Wehgeschrei erschalle von dem Bosporus zum Belt!", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df das Wehgeschrei erschalle von dem Bosporus zum Belt!", "tokens": ["Da\u00df", "das", "Weh\u00b7ge\u00b7schrei", "er\u00b7schal\u00b7le", "von", "dem", "Bos\u00b7po\u00b7rus", "zum", "Belt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "L\u00f6scht den ", "tokens": ["L\u00f6scht", "den"], "token_info": ["word", "word"], "pos": ["NN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Endlich siegt die Zahl; die Waffen taugen nicht mehr zum Gefecht,", "tokens": ["End\u00b7lich", "siegt", "die", "Zahl", ";", "die", "Waf\u00b7fen", "tau\u00b7gen", "nicht", "mehr", "zum", "Ge\u00b7fecht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und der Helden kleines H\u00e4uflein hat's und Schmerz geschw\u00e4cht!", "tokens": ["Und", "der", "Hel\u00b7den", "klei\u00b7nes", "H\u00e4uf\u00b7lein", "hat's", "und", "Schmerz", "ge\u00b7schw\u00e4cht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VAFIN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Schmerz, da\u00df Alles sie verlassen, da\u00df kein Arm sich h\u00fclfreich hebt,", "tokens": ["Schmerz", ",", "da\u00df", "Al\u00b7les", "sie", "ver\u00b7las\u00b7sen", ",", "da\u00df", "kein", "Arm", "sich", "h\u00fclf\u00b7reich", "hebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PIS", "PPER", "VVINF", "$,", "KOUS", "PIAT", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Da\u00df selbst in den n\u00e4chsten Br\u00fcdern nicht der alte Geist mehr lebt!", "tokens": ["Da\u00df", "selbst", "in", "den", "n\u00e4chs\u00b7ten", "Br\u00fc\u00b7dern", "nicht", "der", "al\u00b7te", "Geist", "mehr", "lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "ADJA", "NN", "PTKNEG", "ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "T\u00f6ne laut durch alle L\u00e4nder und durch alle Zeiten fort!", "tokens": ["T\u00f6\u00b7ne", "laut", "durch", "al\u00b7le", "L\u00e4n\u00b7der", "und", "durch", "al\u00b7le", "Zei\u00b7ten", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PIAT", "NN", "KON", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Ach, zweihundert Millionen ", "tokens": ["Ach", ",", "zwei\u00b7hun\u00b7dert", "Mil\u00b7lion\u00b7en"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$,", "CARD", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Ihre Heere, ihre Flotten herrschen \u00fcber Land und Meer!", "tokens": ["Ih\u00b7re", "Hee\u00b7re", ",", "ih\u00b7re", "Flot\u00b7ten", "herr\u00b7schen", "\u00fc\u00b7ber", "Land", "und", "Meer", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Und sie brennen, doch vergebens, ihren Br\u00fcdern beizustehn,", "tokens": ["Und", "sie", "bren\u00b7nen", ",", "doch", "ver\u00b7ge\u00b7bens", ",", "ih\u00b7ren", "Br\u00fc\u00b7dern", "bei\u00b7zu\u00b7stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$,", "ADV", "ADV", "$,", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Weil die Herrscher im Zerst\u00f6rer Scio's nur den \u2013 Herrscher sehn!", "tokens": ["Weil", "die", "Herr\u00b7scher", "im", "Zer\u00b7st\u00f6\u00b7rer", "Scio's", "nur", "den", "\u2013", "Herr\u00b7scher", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "NE", "ADV", "ART", "$(", "NN", "VVINF", "$."], "meter": "--+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Es bedarf nur ", "tokens": ["Es", "be\u00b7darf", "nur"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Und ein edles, hartgedr\u00fccktes ", "tokens": ["Und", "ein", "ed\u00b7les", ",", "hart\u00b7ge\u00b7dr\u00fcck\u00b7tes"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Ach, dies ", "tokens": ["Ach", ",", "dies"], "token_info": ["word", "punct", "word"], "pos": ["ITJ", "$,", "PDS"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Und mit ", "tokens": ["Und", "mit"], "token_info": ["word", "word"], "pos": ["KON", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "In der alten ", "tokens": ["In", "der", "al\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "--+-", "measure": "anapaest.init"}, "line.17": {"text": "Grinsend sieht er diese K\u00f6pfe, die er fast noch f\u00fcrchtet, an;", "tokens": ["Grin\u00b7send", "sieht", "er", "die\u00b7se", "K\u00f6p\u00b7fe", ",", "die", "er", "fast", "noch", "f\u00fcrch\u00b7tet", ",", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PDAT", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "\u00bbseht,\u00ab ruft er, \u00bbdie ", "tokens": ["\u00bb", "seht", ",", "\u00ab", "ruft", "er", ",", "\u00bb", "die"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word"], "pos": ["$(", "VVFIN", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Und der andern Christen Herrscher buhlen doch um meine Gunst!\u00ab", "tokens": ["Und", "der", "an\u00b7dern", "Chris\u00b7ten", "Herr\u00b7scher", "buh\u00b7len", "doch", "um", "mei\u00b7ne", "Gunst", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.21": {"text": "Tag der Rache, Tag der ", "tokens": ["Tag", "der", "Ra\u00b7che", ",", "Tag", "der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.22": {"text": "Da\u00df des Greises m\u00fcdes Auge sich in Frieden schlie\u00dfen kann!", "tokens": ["Da\u00df", "des", "Grei\u00b7ses", "m\u00fc\u00b7des", "Au\u00b7ge", "sich", "in", "Frie\u00b7den", "schlie\u00b7\u00dfen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "PRF", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}