{"textgrid.poem.65114": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da G\u00f6tter dich zu ihrem Dienste weihten,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da G\u00f6tter dich zu ihrem Dienste weihten,", "tokens": ["Da", "G\u00f6t\u00b7ter", "dich", "zu", "ih\u00b7rem", "Diens\u00b7te", "weih\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Folg' ihrem Rufe und zertritt mein Gl\u00fcck!", "tokens": ["Fol\u00b7g'", "ih\u00b7rem", "Ru\u00b7fe", "und", "zer\u00b7tritt", "mein", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Mit dir f\u00fchl' ich das Leben mir entgleiten,", "tokens": ["Mit", "dir", "f\u00fchl'", "ich", "das", "Le\u00b7ben", "mir", "ent\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Doch meine Hand, sie h\u00e4lt dich nicht zur\u00fcck.", "tokens": ["Doch", "mei\u00b7ne", "Hand", ",", "sie", "h\u00e4lt", "dich", "nicht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ob dunkle Wolken rings den Himmel schw\u00e4rzen,", "tokens": ["Ob", "dunk\u00b7le", "Wol\u00b7ken", "rings", "den", "Him\u00b7mel", "schw\u00e4r\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Lust entflieht, ein scheidend Abendroth,", "tokens": ["Die", "Lust", "ent\u00b7flieht", ",", "ein", "schei\u00b7dend", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es hebt mich \u00fcber mich und meine Schmerzen", "tokens": ["Es", "hebt", "mich", "\u00fc\u00b7ber", "mich", "und", "mei\u00b7ne", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Liebe, die da st\u00e4rker als der Tod.", "tokens": ["Die", "Lie\u00b7be", ",", "die", "da", "st\u00e4r\u00b7ker", "als", "der", "Tod", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Uns einte und verkn\u00fcpfte nicht f\u00fcr Stunden", "tokens": ["Uns", "ein\u00b7te", "und", "ver\u00b7kn\u00fcpf\u00b7te", "nicht", "f\u00fcr", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Sinne Gluth, der Laune fl\u00fcchtig Spiel;", "tokens": ["Der", "Sin\u00b7ne", "Gluth", ",", "der", "Lau\u00b7ne", "fl\u00fcch\u00b7tig", "Spiel", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was unaufl\u00f6slich Geist mit Geist verbunden,", "tokens": ["Was", "un\u00b7auf\u00b7l\u00f6s\u00b7lich", "Geist", "mit", "Geist", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein gleich Erkennen war's, ein gleiches Ziel.", "tokens": ["Ein", "gleich", "Er\u00b7ken\u00b7nen", "wa\u00b7r's", ",", "ein", "glei\u00b7ches", "Ziel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VAFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "O, fort mit jeder schw\u00e4chlich feigen Klage!", "tokens": ["O", ",", "fort", "mit", "je\u00b7der", "schw\u00e4ch\u00b7lich", "fei\u00b7gen", "Kla\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "APPR", "PIS", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bleibt nicht mein Loos dem deinen zugesellt", "tokens": ["Bleibt", "nicht", "mein", "Loos", "dem", "dei\u00b7nen", "zu\u00b7ge\u00b7sellt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "ART", "PPOSAT", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "F\u00fcr immer, da in deiner Thaten Wage", "tokens": ["F\u00fcr", "im\u00b7mer", ",", "da", "in", "dei\u00b7ner", "Tha\u00b7ten", "Wa\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "$,", "KOUS", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nun das Gewicht auch meiner Leiden f\u00e4llt?", "tokens": ["Nun", "das", "Ge\u00b7wicht", "auch", "mei\u00b7ner", "Lei\u00b7den", "f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So bauen treu wir an demselben Werke,", "tokens": ["So", "bau\u00b7en", "treu", "wir", "an", "dem\u00b7sel\u00b7ben", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem unser tiefstes Hoffen zugekehrt:", "tokens": ["Dem", "un\u00b7ser", "tiefs\u00b7tes", "Hof\u00b7fen", "zu\u00b7ge\u00b7kehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du schaffst daran mit deines Geistes St\u00e4rke,", "tokens": ["Du", "schaffst", "da\u00b7ran", "mit", "dei\u00b7nes", "Geis\u00b7tes", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich mit dem Grame, der mich still verzehrt,", "tokens": ["Ich", "mit", "dem", "Gra\u00b7me", ",", "der", "mich", "still", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Dich wird der Ruhm mit seinem Licht umkleiden,", "tokens": ["Dich", "wird", "der", "Ruhm", "mit", "sei\u00b7nem", "Licht", "um\u00b7klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn kein Ged\u00e4chtni\u00df meinen Namen tr\u00e4gt;", "tokens": ["Wenn", "kein", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "mei\u00b7nen", "Na\u00b7men", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch Gottes Richterspruch wird dann entscheiden,", "tokens": ["Doch", "Got\u00b7tes", "Rich\u00b7ter\u00b7spruch", "wird", "dann", "ent\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wer sich der Opfer gr\u00f6\u00df'res auferlegt! \u2013", "tokens": ["Wer", "sich", "der", "Op\u00b7fer", "gr\u00f6\u00df'\u00b7res", "auf\u00b7er\u00b7legt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PRF", "ART", "NN", "ADJA", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbwie willst du leben?\u00ab t\u00f6nt's von deinem Munde,", "tokens": ["\u00bb", "wie", "willst", "du", "le\u00b7ben", "?", "\u00ab", "t\u00f6nt's", "von", "dei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "VVINF", "$.", "$(", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbwie willst du leben, wenn ich fern von hier?\u00ab", "tokens": ["\u00bb", "wie", "willst", "du", "le\u00b7ben", ",", "wenn", "ich", "fern", "von", "hier", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "ADJD", "APPR", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O! anders nicht wie bis zu dieser Stunde:", "tokens": ["O", "!", "an\u00b7ders", "nicht", "wie", "bis", "zu", "die\u00b7ser", "Stun\u00b7de", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "PTKNEG", "KOKOM", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch dich, mein Freund, mein Bruder! und in dir!", "tokens": ["Durch", "dich", ",", "mein", "Freund", ",", "mein", "Bru\u00b7der", "!", "und", "in", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "KON", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An jedem Tage will ich neu besiegeln", "tokens": ["An", "je\u00b7dem", "Ta\u00b7ge", "will", "ich", "neu", "be\u00b7sie\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit M\u00e4rtyrwonne meinen Liebesschwur,", "tokens": ["Mit", "M\u00e4r\u00b7tyr\u00b7won\u00b7ne", "mei\u00b7nen", "Lie\u00b7bes\u00b7schwur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und nichts soll sich in meiner Seele spiegeln,", "tokens": ["Und", "nichts", "soll", "sich", "in", "mei\u00b7ner", "See\u00b7le", "spie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als ein Erinnern und ein Hoffen nur!", "tokens": ["Als", "ein", "E\u00b7rin\u00b7nern", "und", "ein", "Hof\u00b7fen", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nicht nur Entz\u00fcckungen, es gibt auch Leiden", "tokens": ["Nicht", "nur", "Ent\u00b7z\u00fc\u00b7ckun\u00b7gen", ",", "es", "gibt", "auch", "Lei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "NN", "$,", "PPER", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In deren Strome sich das Herz erfrischt.", "tokens": ["In", "de\u00b7ren", "Stro\u00b7me", "sich", "das", "Herz", "er\u00b7frischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem unermess'nen Weh, von dir zu scheiden,", "tokens": ["Dem", "un\u00b7er\u00b7mess'\u00b7nen", "Weh", ",", "von", "dir", "zu", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist eine s\u00fc\u00dfe Tr\u00f6stung beigemischt!", "tokens": ["Ist", "ei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Tr\u00f6s\u00b7tung", "bei\u00b7ge\u00b7mischt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Sonnenstrahl durch finst'rer Nebel Decke", "tokens": ["Ein", "Son\u00b7nen\u00b7strahl", "durch", "finst'\u00b7rer", "Ne\u00b7bel", "De\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ringt leuchtend der Gedanke sich empor,", "tokens": ["Ringt", "leuch\u00b7tend", "der", "Ge\u00b7dan\u00b7ke", "sich", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df ich dich nur um ewig heil'ge Zwecke,", "tokens": ["Da\u00df", "ich", "dich", "nur", "um", "e\u00b7wig", "heil'\u00b7ge", "Zwe\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df nur an G\u00f6ttliches ich dich verlor!", "tokens": ["Da\u00df", "nur", "an", "G\u00f6tt\u00b7li\u00b7ches", "ich", "dich", "ver\u00b7lor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}