{"textgrid.poem.42794": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Marschierende Krieger", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor mir her schritt Infanterie,", "tokens": ["Vor", "mir", "her", "schritt", "In\u00b7fan\u00b7te\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APZR", "VVFIN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Eine ganze Kompanie", "tokens": ["Ei\u00b7ne", "gan\u00b7ze", "Kom\u00b7pa\u00b7nie"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kr\u00e4ftiger Soldaten.", "tokens": ["Kr\u00e4f\u00b7ti\u00b7ger", "Sol\u00b7da\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Stramm im Takte traten", "tokens": ["Stramm", "im", "Tak\u00b7te", "tra\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sie den Sand,", "tokens": ["Sie", "den", "Sand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Schritten achtlos \u00fcber einen", "tokens": ["Schrit\u00b7ten", "acht\u00b7los", "\u00fc\u00b7ber", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kleinen K\u00e4fer, den ich fand.", "tokens": ["Klei\u00b7nen", "K\u00e4\u00b7fer", ",", "den", "ich", "fand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich blieb stehen,", "tokens": ["Ich", "blieb", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Um ihn zu besehen,", "tokens": ["Um", "ihn", "zu", "be\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und weil's hinter jenem Milit\u00e4r", "tokens": ["Und", "weil's", "hin\u00b7ter", "je\u00b7nem", "Mi\u00b7li\u00b7t\u00e4r"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Stark nach Schwei\u00df und Leder roch.", "tokens": ["Stark", "nach", "Schwei\u00df", "und", "Le\u00b7der", "roch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da: \u2013 Der K\u00e4fer kroch", "tokens": ["Da", ":", "\u2013", "Der", "K\u00e4\u00b7fer", "kroch"], "token_info": ["word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "$(", "ART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Pl\u00f6tzlich fort, als ob er lebend w\u00e4r.", "tokens": ["Pl\u00f6tz\u00b7lich", "fort", ",", "als", "ob", "er", "le\u00b7bend", "w\u00e4r", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "KOKOM", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Doch ich konstatierte noch:", "tokens": ["Doch", "ich", "kons\u00b7ta\u00b7tier\u00b7te", "noch", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur zwei Steinchen an zwei Seiten retteten \u2013", "tokens": ["Nur", "zwei", "Stein\u00b7chen", "an", "zwei", "Sei\u00b7ten", "ret\u00b7te\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "APPR", "CARD", "NN", "VVINF", "$("], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.9": {"text": "Gleichsam wie als Felsenw\u00e4nde \u2013 diesen \u2013", "tokens": ["Gleich\u00b7sam", "wie", "als", "Fel\u00b7sen\u00b7w\u00e4n\u00b7de", "\u2013", "die\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "KOKOM", "KOUS", "NN", "$(", "PDAT", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Gleichsam zwischen ihnen eingebetteten \u2013", "tokens": ["Gleich\u00b7sam", "zwi\u00b7schen", "ih\u00b7nen", "ein\u00b7ge\u00b7bet\u00b7te\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.11": {"text": "K\u00e4fer vorm Zertrampeltwerden durch die Riesen.", "tokens": ["K\u00e4\u00b7fer", "vorm", "Zer\u00b7tram\u00b7pelt\u00b7wer\u00b7den", "durch", "die", "Rie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Gro\u00dfe Riesen \u2013 kleine Tiere \u2013", "tokens": ["Gro\u00b7\u00dfe", "Rie\u00b7sen", "\u2013", "klei\u00b7ne", "Tie\u00b7re", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich lief, die Wandersohlen,", "tokens": ["Und", "ich", "lief", ",", "die", "Wan\u00b7der\u00b7soh\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die so stanken, einzuholen,", "tokens": ["Die", "so", "stan\u00b7ken", ",", "ein\u00b7zu\u00b7ho\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil ich gar zu gern im Takt marschiere.", "tokens": ["Weil", "ich", "gar", "zu", "gern", "im", "Takt", "mar\u00b7schie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKA", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und ich hustete und spuckte", "tokens": ["Und", "ich", "hus\u00b7te\u00b7te", "und", "spuck\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Staub und mu\u00dfte viermal niesen.", "tokens": ["Staub", "und", "mu\u00df\u00b7te", "vier\u00b7mal", "nie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich schluckte. Und ich duckte", "tokens": ["Und", "ich", "schluck\u00b7te", ".", "Und", "ich", "duck\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich vor Felsenw\u00e4nden und vor Riesen.", "tokens": ["Mich", "vor", "Fel\u00b7sen\u00b7w\u00e4n\u00b7den", "und", "vor", "Rie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}