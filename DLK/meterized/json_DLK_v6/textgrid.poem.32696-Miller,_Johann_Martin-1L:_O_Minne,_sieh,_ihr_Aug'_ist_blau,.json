{"textgrid.poem.32696": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Minne, sieh, ihr Aug' ist blau,", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Minne, sieh, ihr Aug' ist blau,", "tokens": ["O", "Min\u00b7ne", ",", "sieh", ",", "ihr", "Aug'", "ist", "blau", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie der Himmel offen!", "tokens": ["Und", "wie", "der", "Him\u00b7mel", "of\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Je l\u00e4nger ich mich drinnen schau,", "tokens": ["Je", "l\u00e4n\u00b7ger", "ich", "mich", "drin\u00b7nen", "schau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Je mehr l\u00e4\u00dft mich die gute Frau", "tokens": ["Je", "mehr", "l\u00e4\u00dft", "mich", "die", "gu\u00b7te", "Frau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Auf tausend Freuden hoffen.", "tokens": ["Auf", "tau\u00b7send", "Freu\u00b7den", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es l\u00e4chelt, sitz' ich manches Mal", "tokens": ["Es", "l\u00e4\u00b7chelt", ",", "sitz'", "ich", "man\u00b7ches", "Mal"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr abends gegen\u00fcber,", "tokens": ["Ihr", "a\u00b7bends", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bald heller als der Sonnenstrahl,", "tokens": ["Bald", "hel\u00b7ler", "als", "der", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald wird es wieder auf einmal", "tokens": ["Bald", "wird", "es", "wie\u00b7der", "auf", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von Thr\u00e4nenwolken tr\u00fcber.", "tokens": ["Von", "Thr\u00e4\u00b7nen\u00b7wol\u00b7ken", "tr\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dies, liebe Minne, dank' ich dir", "tokens": ["Dies", ",", "lie\u00b7be", "Min\u00b7ne", ",", "dank'", "ich", "dir"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tief aus des Herzens Grunde;", "tokens": ["Tief", "aus", "des", "Her\u00b7zens", "Grun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn, weinst du jetzo gleich aus ihr,", "tokens": ["Denn", ",", "weinst", "du", "jet\u00b7zo", "gleich", "aus", "ihr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wei\u00df ich doch, es l\u00e4chelt mir", "tokens": ["So", "wei\u00df", "ich", "doch", ",", "es", "l\u00e4\u00b7chelt", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dereinst die sch\u00f6nste Stunde.", "tokens": ["De\u00b7reinst", "die", "sch\u00f6ns\u00b7te", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}