{"textgrid.poem.26226": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Keiner stirbt, der f\u00fcr das Leben f\u00e4llt", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle gro\u00dfen Berge wurden klein,", "tokens": ["Al\u00b7le", "gro\u00b7\u00dfen", "Ber\u00b7ge", "wur\u00b7den", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Nirgends ist ein Berg so schwer aus Stein", "tokens": ["Nir\u00b7gends", "ist", "ein", "Berg", "so", "schwer", "aus", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Als der Berg der Schmerzen und der Klagen,", "tokens": ["Als", "der", "Berg", "der", "Schmer\u00b7zen", "und", "der", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Den die Menschen in der Kriegsnot tragen.", "tokens": ["Den", "die", "Men\u00b7schen", "in", "der", "Kriegs\u00b7not", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "N\u00e4chte werden wilde Ewigkeit.", "tokens": ["N\u00e4ch\u00b7te", "wer\u00b7den", "wil\u00b7de", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Nie war je so krasse Winterszeit.", "tokens": ["Nie", "war", "je", "so", "kras\u00b7se", "Win\u00b7ter\u00b7szeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Viel Verzweifelte ins Dunkel stieren,", "tokens": ["Viel", "Ver\u00b7zwei\u00b7fel\u00b7te", "ins", "Dun\u00b7kel", "stie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Viele tausend Tote machen frieren.", "tokens": ["Vie\u00b7le", "tau\u00b7send", "To\u00b7te", "ma\u00b7chen", "frie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "CARD", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Auch der Frieden br\u00e4cht' nicht Frieden her.", "tokens": ["Auch", "der", "Frie\u00b7den", "br\u00e4cht'", "nicht", "Frie\u00b7den", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKNEG", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Siege wecken Tote nimmermehr.", "tokens": ["Sie\u00b7ge", "we\u00b7cken", "To\u00b7te", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nur ein Tor spricht mir von frohen Siegen,", "tokens": ["Nur", "ein", "Tor", "spricht", "mir", "von", "fro\u00b7hen", "Sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nur ein Narr kann froh bei Gr\u00e4bern liegen.", "tokens": ["Nur", "ein", "Narr", "kann", "froh", "bei", "Gr\u00e4\u00b7bern", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Grollend dacht' ich's, und der Regen fiel.", "tokens": ["Grol\u00b7lend", "dacht'", "ich's", ",", "und", "der", "Re\u00b7gen", "fiel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und der Krieg trieb fort sein w\u00fctend Spiel.", "tokens": ["Und", "der", "Krieg", "trieb", "fort", "sein", "w\u00fc\u00b7tend", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "PPOSAT", "ADJD", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Suchend mu\u00dft' ich in die Wolken schauen,", "tokens": ["Su\u00b7chend", "mu\u00dft'", "ich", "in", "die", "Wol\u00b7ken", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo der Himmel weinte wie die Frauen.", "tokens": ["Wo", "der", "Him\u00b7mel", "wein\u00b7te", "wie", "die", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Aber lebte nicht ein ewig Blau,", "tokens": ["A\u00b7ber", "leb\u00b7te", "nicht", "ein", "e\u00b7wig", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ewig Sonnenlicht dort hinterm Grau?", "tokens": ["E\u00b7wig", "Son\u00b7nen\u00b7licht", "dort", "hin\u00b7term", "Grau", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dieses kannte keine armen Toten,", "tokens": ["Die\u00b7ses", "kann\u00b7te", "kei\u00b7ne", "ar\u00b7men", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Helle Helden ihren Gru\u00df mir boten.", "tokens": ["Hel\u00b7le", "Hel\u00b7den", "ih\u00b7ren", "Gru\u00df", "mir", "bo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "All die Tapfern sprachen auf mich ein:", "tokens": ["All", "die", "Tap\u00b7fern", "spra\u00b7chen", "auf", "mich", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "\u00bbsollen wir umsonst verblutet sein?", "tokens": ["\u00bb", "sol\u00b7len", "wir", "um\u00b7sonst", "ver\u00b7blu\u00b7tet", "sein", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Deine Klagen wollen uns entwerten,", "tokens": ["Dei\u00b7ne", "Kla\u00b7gen", "wol\u00b7len", "uns", "ent\u00b7wer\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Uns, die wir den Gott der Tat verehrten.", "tokens": ["Uns", ",", "die", "wir", "den", "Gott", "der", "Tat", "ver\u00b7ehr\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Keiner stirbt, der f\u00fcr das Leben f\u00e4llt,", "tokens": ["Kei\u00b7ner", "stirbt", ",", "der", "f\u00fcr", "das", "Le\u00b7ben", "f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Keiner, der gek\u00e4mpft f\u00fcr seine Welt.", "tokens": ["Kei\u00b7ner", ",", "der", "ge\u00b7k\u00e4mpft", "f\u00fcr", "sei\u00b7ne", "Welt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und ihr sollt um uns nicht Klage tragen,", "tokens": ["Und", "ihr", "sollt", "um", "uns", "nicht", "Kla\u00b7ge", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "PPER", "PTKNEG", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Um Verkl\u00e4rte nicht aus gr\u00f6\u00dften Tagen.", "tokens": ["Um", "Ver\u00b7kl\u00e4r\u00b7te", "nicht", "aus", "gr\u00f6\u00df\u00b7ten", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Gr\u00f6\u00dfer als die Sorge ist die Kraft,", "tokens": ["Gr\u00f6\u00b7\u00dfer", "als", "die", "Sor\u00b7ge", "ist", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Die aus Totem Berge Leben schafft.\u00ab", "tokens": ["Die", "aus", "To\u00b7tem", "Ber\u00b7ge", "Le\u00b7ben", "schafft", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Danach sah ich sie, die hingegangen,", "tokens": ["Da\u00b7nach", "sah", "ich", "sie", ",", "die", "hin\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "$,", "PRELS", "VVPP", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "H\u00f6her als den Berg der Kriegsnot prangen.", "tokens": ["H\u00f6\u00b7her", "als", "den", "Berg", "der", "Kriegs\u00b7not", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}