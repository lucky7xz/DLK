{"textgrid.poem.67641": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Bleicher hatt' ein weites, gro\u00dfes Haus.", "tokens": ["Ein", "Blei\u00b7cher", "hatt'", "ein", "wei\u00b7tes", ",", "gro\u00b7\u00dfes", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbwas soll das leer denn stehn? Hier mach' ich Geld mir draus;", "tokens": ["\u00bb", "was", "soll", "das", "leer", "denn", "stehn", "?", "Hier", "mach'", "ich", "Geld", "mir", "draus", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "ART", "ADJA", "KON", "VVINF", "$.", "ADV", "VVFIN", "PPER", "NN", "PPER", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Vetter K\u00f6hler soll hier wohnen.\u00ab", "tokens": ["Mein", "Vet\u00b7ter", "K\u00f6h\u00b7ler", "soll", "hier", "woh\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "NE", "VMFIN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Vetter K\u00f6hler th\u00e4t ihn lohnen.", "tokens": ["Der", "Vet\u00b7ter", "K\u00f6h\u00b7ler", "th\u00e4t", "ihn", "loh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Bleicher machte wei\u00df; der K\u00f6hler macht's voll Graus", "tokens": ["Der", "Blei\u00b7cher", "mach\u00b7te", "wei\u00df", ";", "der", "K\u00f6h\u00b7ler", "macht's", "voll", "Graus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$.", "ART", "NN", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit seinem Kohlendampf; der K\u00f6hler mu\u00dft' hinaus.", "tokens": ["Mit", "sei\u00b7nem", "Koh\u00b7len\u00b7dampf", ";", "der", "K\u00f6h\u00b7ler", "mu\u00dft'", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ART", "NN", "VMFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Christus und Belial,", "tokens": ["Chris\u00b7tus", "und", "Be\u00b7li\u00b7al", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was sollen sie in diesem gro\u00dfen Saal?", "tokens": ["Was", "sol\u00b7len", "sie", "in", "die\u00b7sem", "gro\u00b7\u00dfen", "Saal", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Freund, Deine Kohlendampfphilosophie", "tokens": ["Freund", ",", "Dei\u00b7ne", "Koh\u00b7len\u00b7dampf\u00b7phi\u00b7lo\u00b7so\u00b7phie"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Hier am Altar \u2013 o Freund, was soll sie hie?", "tokens": ["Hier", "am", "Al\u00b7tar", "\u2013", "o", "Freund", ",", "was", "soll", "sie", "hie", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$(", "FM", "NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}