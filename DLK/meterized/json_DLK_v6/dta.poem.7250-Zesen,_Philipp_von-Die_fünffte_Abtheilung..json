{"dta.poem.7250": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Die f\u00fcnffte Abtheilung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1640", "urn": "urn:nbn:de:kobv:b4-20685-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kom Adelheit Sophie/ die Sonne k\u00f6mpt geg angen/", "tokens": ["Kom", "A\u00b7del\u00b7heit", "So\u00b7phie", "/", "die", "Son\u00b7ne", "k\u00f6mpt", "geg", "an\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$(", "ART", "NN", "VVFIN", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr rosen-farbes kind die Tochter geth voran/", "tokens": ["Ihr", "ro\u00b7sen\u00b7fa\u00b7rbes", "kind", "die", "Toch\u00b7ter", "ge\u00b7th", "vo\u00b7ran", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der bald die Mutter folgt mit gantz verg\u00fcldten wan-", "tokens": ["Der", "bald", "die", "Mut\u00b7ter", "folgt", "mit", "gantz", "ver\u00b7g\u00fcld\u00b7ten", "wan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "APPR", "ADV", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "gen/", "tokens": ["gen", "/"], "token_info": ["word", "punct"], "pos": ["APPR", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Es fehlet nur an dir/ hier ist die g\u00fcldene bahn/", "tokens": ["Es", "feh\u00b7let", "nur", "an", "dir", "/", "hier", "ist", "die", "g\u00fcl\u00b7de\u00b7ne", "bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$(", "ADV", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Tritt vor mein edles bild vnd f\u00fchre bey den h\u00e4nden", "tokens": ["Tritt", "vor", "mein", "ed\u00b7les", "bild", "vnd", "f\u00fch\u00b7re", "bey", "den", "h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "KON", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mich deinen Liebsten fort: wenn du nicht bey", "tokens": ["Mich", "dei\u00b7nen", "Liebs\u00b7ten", "fort", ":", "wenn", "du", "nicht", "bey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "$.", "KOUS", "PPER", "PTKNEG", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "mir bist/", "tokens": ["mir", "bist", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VAFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Du meine Sonn\u2019 vnd zier/ mu\u00df ich zu r\u00fccke wen-", "tokens": ["Du", "mei\u00b7ne", "Sonn'", "vnd", "zier", "/", "mu\u00df", "ich", "zu", "r\u00fc\u00b7cke", "wen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "KON", "NN", "$(", "VMFIN", "PPER", "PTKZU", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "den/", "tokens": ["den", "/"], "token_info": ["word", "punct"], "pos": ["ART", "$("], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Mein ziel in schneller eyl/ weil sonst es dunckel", "tokens": ["Mein", "ziel", "in", "schnel\u00b7ler", "eyl", "/", "weil", "sonst", "es", "dun\u00b7ckel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$(", "KOUS", "ADV", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "ist. &amp;c.", "tokens": ["ist", ".", "&amp;", "c."], "token_info": ["word", "punct", "XML_entity", "abbreviation"], "pos": ["VAFIN", "$.", "NE", "NE"], "meter": "-", "measure": "single.down"}}}}}