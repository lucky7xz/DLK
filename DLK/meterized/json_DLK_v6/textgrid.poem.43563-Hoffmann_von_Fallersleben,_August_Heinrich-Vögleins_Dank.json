{"textgrid.poem.43563": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "V\u00f6gleins Dank", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sitzt ein Vogel in dem Ried,", "tokens": ["Es", "sitzt", "ein", "Vo\u00b7gel", "in", "dem", "Ried", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der singt ein traurig, traurig Lied:", "tokens": ["Der", "singt", "ein", "trau\u00b7rig", ",", "trau\u00b7rig", "Lied", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJD", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weh mir! weh mir! Der grimme West,", "tokens": ["Weh", "mir", "!", "weh", "mir", "!", "Der", "grim\u00b7me", "West", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ADV", "PPER", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er hat zerst\u00f6rt mein liebes Nest;", "tokens": ["Er", "hat", "zer\u00b7st\u00f6rt", "mein", "lie\u00b7bes", "Nest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Halme liegen alle nieder \u2013", "tokens": ["Die", "Hal\u00b7me", "lie\u00b7gen", "al\u00b7le", "nie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PTKVZ", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wer hilft mir wieder?", "tokens": ["Wer", "hilft", "mir", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der Fr\u00fchling h\u00f6rt's, erbarmt sich sein,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "h\u00f6rt's", ",", "er\u00b7barmt", "sich", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "PRF", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er spendet seinen Sonnenschein,", "tokens": ["Er", "spen\u00b7det", "sei\u00b7nen", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und richtet auf noch fr\u00fch am Tag", "tokens": ["Und", "rich\u00b7tet", "auf", "noch", "fr\u00fch", "am", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Ried, was ganz danieder lag:", "tokens": ["Das", "Ried", ",", "was", "ganz", "da\u00b7nie\u00b7der", "lag", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbnun bau', und singe deine Lieder", "tokens": ["\u00bb", "nun", "bau'", ",", "und", "sin\u00b7ge", "dei\u00b7ne", "Lie\u00b7der"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "$,", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nur fr\u00f6hlich wieder!\u00ab \u2013", "tokens": ["Nur", "fr\u00f6h\u00b7lich", "wie\u00b7der", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADJD", "ADV", "$.", "$(", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Der Fr\u00fchling hat erbarmt sich mein,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "hat", "er\u00b7barmt", "sich", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVFIN", "PRF", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun kann ich wieder fr\u00f6hlich sein.", "tokens": ["Nun", "kann", "ich", "wie\u00b7der", "fr\u00f6h\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was ich zum Dank dir bringen kann,", "tokens": ["Was", "ich", "zum", "Dank", "dir", "brin\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nimm, lieber Fr\u00fchling, freundlich an:", "tokens": ["Nimm", ",", "lie\u00b7ber", "Fr\u00fch\u00b7ling", ",", "freund\u00b7lich", "an", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "NN", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich singe dir aus meinem Neste", "tokens": ["Ich", "sin\u00b7ge", "dir", "aus", "mei\u00b7nem", "Nes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Liebst' und Beste.", "tokens": ["Das", "Liebst'", "und", "Bes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}