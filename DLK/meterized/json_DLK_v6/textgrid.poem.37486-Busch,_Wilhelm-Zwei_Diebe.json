{"textgrid.poem.37486": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Zwei Diebe", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ganz heimlich fl\u00fcstern diese zwei,", "tokens": ["Ganz", "heim\u00b7lich", "fl\u00fcs\u00b7tern", "die\u00b7se", "zwei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PDAT", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nat\u00fcrlich nur von Lumperei.", "tokens": ["Na\u00b7t\u00fcr\u00b7lich", "nur", "von", "Lum\u00b7pe\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da gehen sie in tiefem Schweigen,", "tokens": ["Da", "ge\u00b7hen", "sie", "in", "tie\u00b7fem", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohin? das wird sich sp\u00e4ter zeigen.", "tokens": ["Wo\u00b7hin", "?", "das", "wird", "sich", "sp\u00e4\u00b7ter", "zei\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PDS", "VAFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Fenster, welches nicht verschlossen,", "tokens": ["Ein", "Fens\u00b7ter", ",", "wel\u00b7ches", "nicht", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erklimmen sie auf Leitersprossen.", "tokens": ["Er\u00b7klim\u00b7men", "sie", "auf", "Lei\u00b7ter\u00b7spros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hier schl\u00e4ft ein reicher Privatier", "tokens": ["Hier", "schl\u00e4ft", "ein", "rei\u00b7cher", "Pri\u00b7va\u00b7tier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei seinem Gelde in der N\u00e4h'!", "tokens": ["Bei", "sei\u00b7nem", "Gel\u00b7de", "in", "der", "N\u00e4h'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und als der Privatier erwacht,", "tokens": ["Und", "als", "der", "Pri\u00b7va\u00b7tier", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Messer ihm entgegenlacht.", "tokens": ["Ein", "Mes\u00b7ser", "ihm", "ent\u00b7ge\u00b7gen\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Schnell will er die Pistole kriegen,", "tokens": ["Schnell", "will", "er", "die", "Pis\u00b7to\u00b7le", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Dieb mi\u00dfg\u00f6nnt ihm das Vergn\u00fcgen.", "tokens": ["Der", "Dieb", "mi\u00df\u00b7g\u00f6nnt", "ihm", "das", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Seht nur! wie die Pistole kracht,", "tokens": ["Seht", "nur", "!", "wie", "die", "Pis\u00b7to\u00b7le", "kracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Dem Lumpen hat es nichts gemacht.", "tokens": ["Dem", "Lum\u00b7pen", "hat", "es", "nichts", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Privatier, ganz zornentbrannt,", "tokens": ["Der", "Pri\u00b7va\u00b7tier", ",", "ganz", "zor\u00b7nent\u00b7brannt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Haut mit dem S\u00e4bel umeinand.", "tokens": ["Haut", "mit", "dem", "S\u00e4\u00b7bel", "um\u00b7ei\u00b7nand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.9": {"line.1": {"text": "Und jeder haut und jeder sticht,", "tokens": ["Und", "je\u00b7der", "haut", "und", "je\u00b7der", "sticht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und keiner trifft den andern nicht.", "tokens": ["Und", "kei\u00b7ner", "trifft", "den", "an\u00b7dern", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJA", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Hier knebeln sie den dicken Mann,", "tokens": ["Hier", "kne\u00b7beln", "sie", "den", "di\u00b7cken", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er nicht schrein und laufen kann.", "tokens": ["Da\u00df", "er", "nicht", "schrein", "und", "lau\u00b7fen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "KON", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und h\u00e4ngen ihn, o S\u00fcnd' und Schand',", "tokens": ["Und", "h\u00e4n\u00b7gen", "ihn", ",", "o", "S\u00fcnd'", "und", "Schand'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "FM", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An einen Nagel an die Wand.", "tokens": ["An", "ei\u00b7nen", "Na\u00b7gel", "an", "die", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Da kommt, vom lauten Knack erwacht,", "tokens": ["Da", "kommt", ",", "vom", "lau\u00b7ten", "Knack", "er\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die K\u00f6chin im Gewand der Nacht.", "tokens": ["Die", "K\u00f6\u00b7chin", "im", "Ge\u00b7wand", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Und ruft mit bangem Wehgeschrei", "tokens": ["Und", "ruft", "mit", "ban\u00b7gem", "Weh\u00b7ge\u00b7schrei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchs Fenster nach der Polizei.", "tokens": ["Durchs", "Fens\u00b7ter", "nach", "der", "Po\u00b7li\u00b7zei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Da fa\u00dft der Dieb sie bei der Jacke", "tokens": ["Da", "fa\u00dft", "der", "Dieb", "sie", "bei", "der", "Ja\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00fcberzieht sie mit dem Sacke.", "tokens": ["Und", "\u00fc\u00b7berz\u00b7ieht", "sie", "mit", "dem", "Sa\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Da liegt sie nun. Was hilft ihr Schrein?", "tokens": ["Da", "liegt", "sie", "nun", ".", "Was", "hilft", "ihr", "Schrein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sack h\u00fcllt ihre Klagen ein.", "tokens": ["Der", "Sack", "h\u00fcllt", "ih\u00b7re", "Kla\u00b7gen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Doch seht! Die brave Polizei", "tokens": ["Doch", "seht", "!", "Die", "bra\u00b7ve", "Po\u00b7li\u00b7zei"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kommt, wie gew\u00f6hnlich, schnell herbei.", "tokens": ["Kommt", ",", "wie", "ge\u00b7w\u00f6hn\u00b7lich", ",", "schnell", "her\u00b7bei", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADJD", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Die Diebe sind im Schrank versteckt,", "tokens": ["Die", "Die\u00b7be", "sind", "im", "Schrank", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Polizei hat's gleich entdeckt.", "tokens": ["Die", "Po\u00b7li\u00b7zei", "hat's", "gleich", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Die Diebe sausen ins Gemach", "tokens": ["Die", "Die\u00b7be", "sau\u00b7sen", "ins", "Ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit aufgespanntem Regendach.", "tokens": ["Mit", "auf\u00b7ge\u00b7spann\u00b7tem", "Re\u00b7gen\u00b7dach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Am R\u00fccken liegt die Polizei,", "tokens": ["Am", "R\u00fc\u00b7cken", "liegt", "die", "Po\u00b7li\u00b7zei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Diebe st\u00fcrmen schnell vorbei.", "tokens": ["Die", "Die\u00b7be", "st\u00fcr\u00b7men", "schnell", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Da sieht man beide lustig fliegen,", "tokens": ["Da", "sieht", "man", "bei\u00b7de", "lus\u00b7tig", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die b\u00f6se Sache scheint zu siegen.", "tokens": ["Die", "b\u00f6\u00b7se", "Sa\u00b7che", "scheint", "zu", "sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.21": {"line.1": {"text": "Doch still: die Strafe fehlet nie!", "tokens": ["Doch", "still", ":", "die", "Stra\u00b7fe", "feh\u00b7let", "nie", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gesegnet sei das Parapl\u00fc!", "tokens": ["Ge\u00b7seg\u00b7net", "sei", "das", "Pa\u00b7ra\u00b7pl\u00fc", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}