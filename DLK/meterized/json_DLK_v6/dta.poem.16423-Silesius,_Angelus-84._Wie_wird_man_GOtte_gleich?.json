{"dta.poem.16423": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "84.  Wie wird man GOtte gleich?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wer GOtt wil gleiche seyn/ mu\u00df allem ungleich werden.", "tokens": ["Wer", "Gott", "wil", "glei\u00b7che", "seyn", "/", "mu\u00df", "al\u00b7lem", "un\u00b7gleich", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VMFIN", "ADJA", "VAINF", "$(", "VMFIN", "PIS", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mu\u00df ledig seiner selbst/ und lo\u00df seyn von beschwerden.", "tokens": ["Mu\u00df", "le\u00b7dig", "sei\u00b7ner", "selbst", "/", "und", "lo\u00df", "seyn", "von", "be\u00b7schwer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPOSAT", "ADV", "$(", "KON", "ADJD", "VAINF", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}