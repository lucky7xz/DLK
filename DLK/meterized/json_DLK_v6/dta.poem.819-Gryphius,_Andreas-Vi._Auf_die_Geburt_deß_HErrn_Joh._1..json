{"dta.poem.819": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Vi.  Auf die Geburt de\u00df HErrn Joh. 1.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Das wesentliche Wort/ das eh' al\u00df Ewigkeitten ", "tokens": ["Das", "we\u00b7sent\u00b7li\u00b7che", "Wort", "/", "das", "eh'", "al\u00df", "E\u00b7wig\u00b7keit\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich vnd die zeit gek\u00e4nn\u2019t/ Gott ist/ vnd Gott ge-", "tokens": ["Sich", "vnd", "die", "zeit", "ge\u00b7k\u00e4nn't", "/", "Gott", "ist", "/", "vnd", "Gott", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "KON", "ART", "NN", "VVPP", "$(", "NN", "VAFIN", "$(", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "schaw't ", "tokens": ["scha\u00b7w't"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Das wort durch das Gott hat der Erden Hau\u00df gebaw\u2019t", "tokens": ["Das", "wort", "durch", "das", "Gott", "hat", "der", "Er\u00b7den", "Hau\u00df", "ge\u00b7ba\u00b7w't"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "NN", "VVPP"], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Durch das der Himmel stund/ das Licht das vns wird leitten", "tokens": ["Durch", "das", "der", "Him\u00b7mel", "stund", "/", "das", "Licht", "das", "vns", "wird", "leit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "VVFIN", "$(", "ART", "NN", "ART", "PPER", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "(das mehr denn lichte Licht!) wenn H\u00e4nd\u2019 v\u00f1 F\u00fcsse gleitt\u1ebd", "tokens": ["(", "das", "mehr", "denn", "lich\u00b7te", "Licht", "!", ")", "wenn", "H\u00e4nd'", "v\u00f1", "F\u00fcs\u00b7se", "gleit\u00b7t\u1ebd"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADV", "ADV", "ADJA", "NN", "$.", "$(", "KOUS", "NN", "APPR", "NN", "NE"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Vor dem nichts finster ist/ Vor dem der H\u00f6llen graw\u2019t", "tokens": ["Vor", "dem", "nichts", "fins\u00b7ter", "ist", "/", "Vor", "dem", "der", "H\u00f6l\u00b7len", "gra\u00b7w't"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIS", "ADJD", "VAFIN", "$(", "APPR", "ART", "ART", "NN", "VVFIN"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vnd was mehr dunckel heist/ hat sich der Welt vertraw\u2019t", "tokens": ["Vnd", "was", "mehr", "dun\u00b7ckel", "heist", "/", "hat", "sich", "der", "Welt", "ver\u00b7tra\u00b7w't"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "ADJD", "VAFIN", "$(", "VAFIN", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd nimbt an vnser Fleisch vnd schwere last der zeitten.", "tokens": ["Vnd", "nimbt", "an", "vn\u00b7ser", "Fleisch", "vnd", "schwe\u00b7re", "last", "der", "zeit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "ADJA", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es ist vom Ehrenthron ins threnen Thal ankommen/", "tokens": ["Es", "ist", "vom", "Eh\u00b7rent\u00b7hron", "ins", "thre\u00b7nen", "Thal", "an\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "APPRART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd hat dis Leibes Zelt zur wohnung angenommen", "tokens": ["Vnd", "hat", "dis", "Lei\u00b7bes", "Zelt", "zur", "woh\u00b7nung", "an\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PDS", "NN", "NN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wiewol sein eigenthumb sich stets jhm widersetzt.", "tokens": ["Wie\u00b7wol", "sein", "ei\u00b7gen\u00b7thumb", "sich", "stets", "jhm", "wi\u00b7der\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADV", "PRF", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wer diesen gast aufnim\u2019t wird augenblicks erkennen", "tokens": ["Wer", "die\u00b7sen", "gast", "auf\u00b7nim't", "wird", "au\u00b7gen\u00b7blicks", "er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PDAT", "NN", "VVPP", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie herrlich seine gunst/ Er wird in lieb\u2019 entbrennen", "tokens": ["Wie", "herr\u00b7lich", "sei\u00b7ne", "gunst", "/", "Er", "wird", "in", "lieb'", "ent\u00b7bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "$(", "PPER", "VAFIN", "APPR", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In liebe/ die mit lust/ vnd f\u00fcr vnd f\u00fcr ergetz\u2019t.", "tokens": ["In", "lie\u00b7be", "/", "die", "mit", "lust", "/", "vnd", "f\u00fcr", "vnd", "f\u00fcr", "er\u00b7getz'", "t."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "VVFIN", "$(", "ART", "APPR", "NN", "$(", "KON", "APPR", "KON", "APPR", "NE", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}