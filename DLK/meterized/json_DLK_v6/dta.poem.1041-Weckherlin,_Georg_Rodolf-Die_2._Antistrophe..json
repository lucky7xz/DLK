{"dta.poem.1041": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die 2. Antistrophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sje ist allein/ nicht zuvergleichen/", "tokens": ["Sje", "ist", "al\u00b7lein", "/", "nicht", "zu\u00b7ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "$(", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein einige volkommenheit:", "tokens": ["Ein", "ei\u00b7ni\u00b7ge", "vol\u00b7kom\u00b7men\u00b7heit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sonnen gewalt vnd klarheit", "tokens": ["Der", "Son\u00b7nen", "ge\u00b7walt", "vnd", "klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mu\u00df jhrer augen klarheit weichen:", "tokens": ["Mu\u00df", "jhrer", "au\u00b7gen", "klar\u00b7heit", "wei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd wan jhr englische gestalt", "tokens": ["Vnd", "wan", "jhr", "eng\u00b7li\u00b7sche", "ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die G\u00f6tter vnd menschen versehret;", "tokens": ["Die", "G\u00f6t\u00b7ter", "vnd", "men\u00b7schen", "ver\u00b7seh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als dan jhre Tugent verzehret", "tokens": ["Als", "dan", "jhre", "Tu\u00b7gent", "ver\u00b7zeh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Der b\u00f6sen gedancken gewalt.", "tokens": ["Der", "b\u00f6\u00b7sen", "ge\u00b7dan\u00b7cken", "ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Kurtz/ ob wol jhren Leib zusehen", "tokens": ["Kurtz", "/", "ob", "wol", "jhren", "Leib", "zu\u00b7se\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$(", "KOUS", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Cypris selbs jhr nach m\u00fcste gehen/", "tokens": ["Cyp\u00b7ris", "selbs", "jhr", "nach", "m\u00fcs\u00b7te", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "APPR", "VMFIN", "VVINF", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Ist Sie doch Pallas in der stirn/", "tokens": ["Ist", "Sie", "doch", "Pal\u00b7las", "in", "der", "stirn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Kommend au\u00df eines K\u00f6nigs hirn/", "tokens": ["Kom\u00b7mend", "au\u00df", "ei\u00b7nes", "K\u00f6\u00b7nigs", "hirn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Welcher auff diser gantzen erden", "tokens": ["Wel\u00b7cher", "auff", "di\u00b7ser", "gant\u00b7zen", "er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "APPR", "PDAT", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.14": {"text": "An verstand vnd gerechtigkeit", "tokens": ["An", "ver\u00b7stand", "vnd", "ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "Mu\u00df billich der erst gez\u00f6hlt werden/", "tokens": ["Mu\u00df", "bil\u00b7lich", "der", "erst", "ge\u00b7z\u00f6hlt", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ART", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Der wahre Ph", "tokens": ["Der", "wah\u00b7re", "Ph"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}}}}}