{"textgrid.poem.37717": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Muthwille", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wi\u00dft ihr, wie ich m\u00f6chte ziehen", "tokens": ["Wi\u00dft", "ihr", ",", "wie", "ich", "m\u00f6ch\u00b7te", "zie\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Stadt, wo Liebchen wohnt,", "tokens": ["In", "die", "Stadt", ",", "wo", "Lieb\u00b7chen", "wohnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, wohin ich oft geschrieen", "tokens": ["Ach", ",", "wo\u00b7hin", "ich", "oft", "ge\u00b7schri\u00b7een"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Wind, ganz unbelohnt:", "tokens": ["In", "den", "Wind", ",", "ganz", "un\u00b7be\u00b7lohnt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie weit ist die Zeit!", "tokens": ["Wie", "weit", "ist", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Nicht als Pilger, nicht als S\u00e4nger,", "tokens": ["Nicht", "als", "Pil\u00b7ger", ",", "nicht", "als", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "NN", "$,", "PTKNEG", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht wie Geister unsichtbar,", "tokens": ["Nicht", "wie", "Geis\u00b7ter", "un\u00b7sicht\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht wie V\u00f6gel, nein viel l\u00e4nger", "tokens": ["Nicht", "wie", "V\u00f6\u00b7gel", ",", "nein", "viel", "l\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "KOKOM", "NN", "$,", "PTKANT", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blieb' ich da, weit \u00fcber's Jahr:", "tokens": ["Blieb'", "ich", "da", ",", "weit", "\u00fc\u00b7ber's", "Jahr", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Wie weit ist die Zeit!", "tokens": ["Wie", "weit", "ist", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Nicht mit Blumen, nicht mit B\u00e4ndern", "tokens": ["Nicht", "mit", "Blu\u00b7men", ",", "nicht", "mit", "B\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "$,", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlich ich hin vor Liebchens Th\u00fcr,", "tokens": ["Schlich", "ich", "hin", "vor", "Lieb\u00b7chens", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ADV", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sehnsucht opfern alle L\u00e4nder,", "tokens": ["Sehn\u00b7sucht", "op\u00b7fern", "al\u00b7le", "L\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Singen bl\u00fchend hin zu ihr:", "tokens": ["Sin\u00b7gen", "bl\u00fc\u00b7hend", "hin", "zu", "ihr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ADV", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie weit ist die Zeit!", "tokens": ["Wie", "weit", "ist", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Nein, als Sieger m\u00f6cht' ich ziehen,", "tokens": ["Nein", ",", "als", "Sie\u00b7ger", "m\u00f6cht'", "ich", "zie\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprengen auf die Th\u00fcr zum Scherz,", "tokens": ["Spren\u00b7gen", "auf", "die", "Th\u00fcr", "zum", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Bombe springend gl\u00fchen,", "tokens": ["Wie", "die", "Bom\u00b7be", "sprin\u00b7gend", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch den Mund ihr in das Herz:", "tokens": ["Durch", "den", "Mund", "ihr", "in", "das", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie weit ist die Zeit!", "tokens": ["Wie", "weit", "ist", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Um von unten auf zu dienen,", "tokens": ["Um", "von", "un\u00b7ten", "auf", "zu", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ADV", "APPR", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meint ihr, w\u00e4r' ich nun zu alt,", "tokens": ["Meint", "ihr", ",", "w\u00e4r'", "ich", "nun", "zu", "alt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Obenein m\u00f6cht' ich verdienen", "tokens": ["O\u00b7be\u00b7nein", "m\u00f6cht'", "ich", "ver\u00b7die\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So die himmlische Gestalt:", "tokens": ["So", "die", "himm\u00b7li\u00b7sche", "Ge\u00b7stalt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie weit ist die Zeit!", "tokens": ["Wie", "weit", "ist", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Wenn ich mit dem S\u00e4bel klopfe,", "tokens": ["Wenn", "ich", "mit", "dem", "S\u00e4\u00b7bel", "klop\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Liebchen macht die Th\u00fcre auf,", "tokens": ["Lieb\u00b7chen", "macht", "die", "Th\u00fc\u00b7re", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie kniet, hat Angst im Kopfe,", "tokens": ["Und", "sie", "kniet", ",", "hat", "Angst", "im", "Kop\u00b7fe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "VAFIN", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie als Krone h\u00f6b' ich auf:", "tokens": ["Sie", "als", "Kro\u00b7ne", "h\u00f6b'", "ich", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie weit ist die Zeit!", "tokens": ["Wie", "weit", "ist", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Setzte sie auf meinen Scheitel,", "tokens": ["Setz\u00b7te", "sie", "auf", "mei\u00b7nen", "Schei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00fcg' wie Atlas meine Welt,", "tokens": ["Tr\u00fcg'", "wie", "At\u00b7las", "mei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NE", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Welt schien mir dann eitel,", "tokens": ["Al\u00b7le", "Welt", "schien", "mir", "dann", "ei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich w\u00e4r' der einz'ge Held:", "tokens": ["Und", "ich", "w\u00e4r'", "der", "einz'\u00b7ge", "Held", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie weit ist die Zeit!", "tokens": ["Wie", "weit", "ist", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}