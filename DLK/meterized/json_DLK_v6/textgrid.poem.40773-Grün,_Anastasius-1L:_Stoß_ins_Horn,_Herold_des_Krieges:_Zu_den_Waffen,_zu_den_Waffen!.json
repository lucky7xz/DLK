{"textgrid.poem.40773": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sto\u00df ins Horn, Herold des Krieges: Zu den Waffen, zu den Waffen!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sto\u00df ins Horn, Herold des Krieges: Zu den Waffen, zu den Waffen!", "tokens": ["Sto\u00df", "ins", "Horn", ",", "He\u00b7rold", "des", "Krie\u00b7ges", ":", "Zu", "den", "Waf\u00b7fen", ",", "zu", "den", "Waf\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "NN", "ART", "NN", "$.", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Kampf und Krieg der argen Horde heuchlerischer dummer Pfaffen!", "tokens": ["Kampf", "und", "Krieg", "der", "ar\u00b7gen", "Hor\u00b7de", "heuch\u00b7le\u00b7ri\u00b7scher", "dum\u00b7mer", "Pfaf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "ADJA", "NN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Aber Friede, Gottesfriede, mit der frommen Priesterschaar,", "tokens": ["A\u00b7ber", "Frie\u00b7de", ",", "Got\u00b7tes\u00b7frie\u00b7de", ",", "mit", "der", "from\u00b7men", "Pries\u00b7ter\u00b7schaar", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Frieden ihrem Segensamte, Ehrfurcht ihrem Weihaltar!", "tokens": ["Frie\u00b7den", "ih\u00b7rem", "Se\u00b7gen\u00b7sam\u00b7te", ",", "Ehr\u00b7furcht", "ih\u00b7rem", "Wei\u00b7hal\u00b7tar", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Priester sind's, die's bittre Sterben uns mit Wundertrost vers\u00fc\u00dfen,", "tokens": ["Pries\u00b7ter", "sin\u00b7d's", ",", "die's", "bitt\u00b7re", "Ster\u00b7ben", "uns", "mit", "Wun\u00b7der\u00b7trost", "ver\u00b7s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PDAT", "ADJA", "NN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Pfaffen sind's, die's s\u00fc\u00dfe Leben bitter uns zu machen wissen;", "tokens": ["Pfaf\u00b7fen", "sin\u00b7d's", ",", "die's", "s\u00fc\u00b7\u00dfe", "Le\u00b7ben", "bit\u00b7ter", "uns", "zu", "ma\u00b7chen", "wis\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PDAT", "ADJA", "NN", "ADJD", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Priesterherz, o See voll Klarheit, der den Himmel spiegelnd h\u00e4lt,", "tokens": ["Pries\u00b7ter\u00b7herz", ",", "o", "See", "voll", "Klar\u00b7heit", ",", "der", "den", "Him\u00b7mel", "spie\u00b7gelnd", "h\u00e4lt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "ADJD", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Pfaffenseele, ekle Pf\u00fctze, f\u00fcllend dich vom Koth der Welt!", "tokens": ["Pfaf\u00b7fen\u00b7see\u00b7le", ",", "ek\u00b7le", "Pf\u00fct\u00b7ze", ",", "f\u00fcl\u00b7lend", "dich", "vom", "Koth", "der", "Welt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "VVPP", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Priester gleicht der treuen Dogge, die uns Haus und Hof besch\u00fctzte,", "tokens": ["Pries\u00b7ter", "gleicht", "der", "treu\u00b7en", "Dog\u00b7ge", ",", "die", "uns", "Haus", "und", "Hof", "be\u00b7sch\u00fctz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Pfaff' ist Fuchs, der Nachts die H\u00fchner aus dem Stall uns wegstibitzte;", "tokens": ["Pfaff'", "ist", "Fuchs", ",", "der", "Nachts", "die", "H\u00fch\u00b7ner", "aus", "dem", "Stall", "uns", "weg\u00b7sti\u00b7bitz\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NE", "$,", "PRELS", "ADV", "ART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Priester ist ein Markusl\u00f6we, der das Evangelium wahrt,", "tokens": ["Pries\u00b7ter", "ist", "ein", "Mar\u00b7kus\u00b7l\u00f6\u00b7we", ",", "der", "das", "E\u00b7van\u00b7ge\u00b7li\u00b7um", "wahrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NE", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+--+-+--+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Pfaff' ist eine Tigerkatze, jener Gattung schlecht're Art. \u2013", "tokens": ["Pfaff'", "ist", "ei\u00b7ne", "Ti\u00b7ger\u00b7kat\u00b7ze", ",", "je\u00b7ner", "Gat\u00b7tung", "schlecht'\u00b7re", "Art", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "PDAT", "NN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Priester! \u2013 hui, du kr\u00e4ft'ge Ceder, frei das Haupt zum Himmel kehrend!", "tokens": ["Pries\u00b7ter", "!", "\u2013", "hui", ",", "du", "kr\u00e4ft'\u00b7ge", "Ce\u00b7der", ",", "frei", "das", "Haupt", "zum", "Him\u00b7mel", "keh\u00b7rend", "!"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "FM.la", "$,", "PPER", "ADJA", "NN", "$,", "ADJD", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Pfaffe! pfui, du \u00fcppig Schlingkraut, frech von fremdem Marke zehrend!", "tokens": ["Pfaf\u00b7fe", "!", "pfui", ",", "du", "\u00fcp\u00b7pig", "Schling\u00b7kraut", ",", "frech", "von", "frem\u00b7dem", "Mar\u00b7ke", "zeh\u00b7rend", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ITJ", "$,", "PPER", "ADJD", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Religion! \u2013 der Priester huldigt weihevoll dem G\u00f6tterweib!", "tokens": ["Re\u00b7li\u00b7gi\u00b7on", "!", "\u2013", "der", "Pries\u00b7ter", "hul\u00b7digt", "wei\u00b7he\u00b7voll", "dem", "G\u00f6t\u00b7ter\u00b7weib", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "ART", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Doch der Pfaff' umschlingt im Taumel einer Gassendirne Leib!", "tokens": ["Doch", "der", "Pfaff'", "um\u00b7schlingt", "im", "Tau\u00b7mel", "ei\u00b7ner", "Gas\u00b7sen\u00b7dir\u00b7ne", "Leib", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Einst von Gott erbaten Priester wohl die Sonne f\u00fcr die Erde,", "tokens": ["Einst", "von", "Gott", "er\u00b7ba\u00b7ten", "Pries\u00b7ter", "wohl", "die", "Son\u00b7ne", "f\u00fcr", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJA", "NN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df der Tag, der sch\u00f6ne helle, sch\u00f6ner noch und heller werde;", "tokens": ["Da\u00df", "der", "Tag", ",", "der", "sch\u00f6\u00b7ne", "hel\u00b7le", ",", "sch\u00f6\u00b7ner", "noch", "und", "hel\u00b7ler", "wer\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "ADJA", "ADJA", "$,", "ADJA", "ADV", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Doch des Monds, der Stern' Erl\u00f6schen flehten Pfaffen stets herbei,", "tokens": ["Doch", "des", "Monds", ",", "der", "Stern'", "Er\u00b7l\u00f6\u00b7schen", "fleh\u00b7ten", "Pfaf\u00b7fen", "stets", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "NN", "ADJA", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Da\u00df die Nacht, die schwarze finstre, schw\u00e4rzer noch und finstrer sei!", "tokens": ["Da\u00df", "die", "Nacht", ",", "die", "schwar\u00b7ze", "finst\u00b7re", ",", "schw\u00e4r\u00b7zer", "noch", "und", "finst\u00b7rer", "sei", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "ADJA", "VVFIN", "$,", "ADJA", "ADV", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Disteln wuchern auch in Oestreich, wie ein jedes Land sie br\u00fctet,", "tokens": ["Dis\u00b7teln", "wu\u00b7chern", "auch", "in", "O\u00b7e\u00b7streich", ",", "wie", "ein", "je\u00b7des", "Land", "sie", "br\u00fc\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "NE", "$,", "PWAV", "ART", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Reben bl\u00fch'n und gl\u00fch'n in Oestreich, wie nicht jedes Land sie bietet;", "tokens": ["Re\u00b7ben", "bl\u00fch'n", "und", "gl\u00fch'n", "in", "O\u00b7e\u00b7streich", ",", "wie", "nicht", "je\u00b7des", "Land", "sie", "bie\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "APPR", "NE", "$,", "PWAV", "PTKNEG", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Bombardirt mit Distelk\u00f6pfen frisch die Pfaffen aus dem Land!", "tokens": ["Bom\u00b7bar\u00b7dirt", "mit", "Dis\u00b7tel\u00b7k\u00f6p\u00b7fen", "frisch", "die", "Pfaf\u00b7fen", "aus", "dem", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Nehmt ein Glas des besten Weines auf der Priester Wohl zur Hand!", "tokens": ["Nehmt", "ein", "Glas", "des", "bes\u00b7ten", "Wei\u00b7nes", "auf", "der", "Pries\u00b7ter", "Wohl", "zur", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}