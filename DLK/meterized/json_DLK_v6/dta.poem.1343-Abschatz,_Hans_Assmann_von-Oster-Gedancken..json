{"dta.poem.1343": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Oster-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.85", "fr:0.14"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Last uns mit den frommen Frauen/", "tokens": ["Last", "uns", "mit", "den", "from\u00b7men", "Frau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun der fr\u00fche Tag anbricht/", "tokens": ["Nun", "der", "fr\u00fc\u00b7he", "Tag", "an\u00b7bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr erwachtem Sonnen-Licht", "tokens": ["F\u00fcr", "er\u00b7wach\u00b7tem", "Son\u00b7nen\u00b7Licht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu des HErren Grabe schauen.", "tokens": ["Zu", "des", "Her\u00b7ren", "Gra\u00b7be", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Last uns Salb\u2019 und Specerey/", "tokens": ["Last", "uns", "Salb'", "und", "Spe\u00b7ce\u00b7rey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Seinem C\u00f6rper bringen bey.", "tokens": ["Sei\u00b7nem", "C\u00f6r\u00b7per", "brin\u00b7gen", "bey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seht Aurorens R\u00f6th auffsteigen/", "tokens": ["Seht", "Au\u00b7ro\u00b7rens", "R\u00f6th", "auffs\u00b7tei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der helle Morgen-Stern", "tokens": ["Und", "der", "hel\u00b7le", "Mor\u00b7gen\u00b7S\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Wird uns selbst den Weg zum HErrn", "tokens": ["Wird", "uns", "selbst", "den", "Weg", "zum", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch den k\u00fchlen Thau anzeigen.", "tokens": ["Durch", "den", "k\u00fch\u00b7len", "Thau", "an\u00b7zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber ach! der schwere Stein", "tokens": ["A\u00b7ber", "ach", "!", "der", "schwe\u00b7re", "Stein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00f6mmt mir unterwegens ein.", "tokens": ["K\u00f6mmt", "mir", "un\u00b7ter\u00b7we\u00b7gens", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kan ich mit dem Stein der S\u00fcnden/", "tokens": ["Kan", "ich", "mit", "dem", "Stein", "der", "S\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der mir auff dem R\u00fccken liegt/", "tokens": ["Der", "mir", "auff", "dem", "R\u00fc\u00b7cken", "liegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tausend Centner \u00fcberwiegt/", "tokens": ["Tau\u00b7send", "Cent\u00b7ner", "\u00fc\u00b7berw\u00b7iegt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich zur heilgen St\u00e4tte finden?", "tokens": ["Mich", "zur", "heil\u00b7gen", "St\u00e4t\u00b7te", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo treff ich den Simson an", "tokens": ["Wo", "treff", "ich", "den", "Sim\u00b7son", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NE", "PTKVZ"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der den Stein abweltzen kan?", "tokens": ["Der", "den", "Stein", "ab\u00b7welt\u00b7zen", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Unverzagt! dir ist gerathen/", "tokens": ["Un\u00b7ver\u00b7zagt", "!", "dir", "ist", "ge\u00b7ra\u00b7then", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PPER", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der/ den du besuchen wilt/", "tokens": ["Der", "/", "den", "du", "be\u00b7su\u00b7chen", "wilt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ART", "PPER", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat den Kummer schon gestillt:", "tokens": ["Hat", "den", "Kum\u00b7mer", "schon", "ge\u00b7stillt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Treu k\u00f6mmt dir zu statten/", "tokens": ["Sei\u00b7ne", "Treu", "k\u00f6mmt", "dir", "zu", "stat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKZU", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hebt den Stein f\u00fcr sich und dich/", "tokens": ["Hebt", "den", "Stein", "f\u00fcr", "sich", "und", "dich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PRF", "KON", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und nimmt deine Last auff sich.", "tokens": ["Und", "nimmt", "dei\u00b7ne", "Last", "auff", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PRF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Mag ihn S\u00fcnd und Tod nicht zwingen/", "tokens": ["Mag", "ihn", "S\u00fcnd", "und", "Tod", "nicht", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "KON", "NN", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4lt ihn nicht der H\u00f6llen Klufft/", "tokens": ["H\u00e4lt", "ihn", "nicht", "der", "H\u00f6l\u00b7len", "Klufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kan er sich durch Stein und Grufft", "tokens": ["Kan", "er", "sich", "durch", "Stein", "und", "Grufft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebend in die H\u00f6he schwingen/", "tokens": ["Le\u00b7bend", "in", "die", "H\u00f6\u00b7he", "schwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "So wird auch kein S\u00fcnden-Stein", "tokens": ["So", "wird", "auch", "kein", "S\u00fcn\u00b7den\u00b7Stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ihm bey dir zu m\u00e4chtig seyn.", "tokens": ["Ihm", "bey", "dir", "zu", "m\u00e4ch\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "PTKA", "ADJD", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Schau/ das leere Grab ist offen/", "tokens": ["Schau", "/", "das", "lee\u00b7re", "Grab", "ist", "of\u00b7fen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo dein liebster Heyland lag/", "tokens": ["Wo", "dein", "liebs\u00b7ter", "Hey\u00b7land", "lag", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun hast du den Oster-Tag", "tokens": ["Nun", "hast", "du", "den", "Os\u00b7ter\u00b7Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Froher Seligkeit zu hoffen/", "tokens": ["Fro\u00b7her", "Se\u00b7lig\u00b7keit", "zu", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und durchs k\u00fchle Schlaff-Gemach", "tokens": ["Und", "durchs", "k\u00fch\u00b7le", "Schlaff\u00b7Ge\u00b7mach"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}