{"dta.poem.18813": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt durchaus ist lieblich anzuschauen,", "tokens": ["Die", "Welt", "durc\u00b7haus", "ist", "lieb\u00b7lich", "an\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vorz\u00fcglich aber sch\u00f6n die Welt der Dichter,", "tokens": ["Vor\u00b7z\u00fcg\u00b7lich", "a\u00b7ber", "sch\u00f6n", "die", "Welt", "der", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf bunten, hellen oder silbergrauen", "tokens": ["Auf", "bun\u00b7ten", ",", "hel\u00b7len", "o\u00b7der", "sil\u00b7ber\u00b7grau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gefilden, Tag und Nacht, ergl\u00e4nzen Lichter.", "tokens": ["Ge\u00b7fil\u00b7den", ",", "Tag", "und", "Nacht", ",", "er\u00b7gl\u00e4n\u00b7zen", "Lich\u00b7ter", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Heut ist mir alles herrlich, wenn\u2019s nur bliebe,", "tokens": ["Heut", "ist", "mir", "al\u00b7les", "herr\u00b7lich", ",", "wenn's", "nur", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADJD", "$,", "KOUS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich sehe heut durchs Augenglas der Liebe.", "tokens": ["Ich", "se\u00b7he", "heut", "durchs", "Au\u00b7gen\u00b7glas", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}