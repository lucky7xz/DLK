{"dta.poem.21330": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput VII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich gab ihm zur Antwort lachenden Muths:", "tokens": ["Ich", "gab", "ihm", "zur", "Ant\u00b7wort", "la\u00b7chen\u00b7den", "Muths", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vergebens ist deine Bem\u00fchung!", "tokens": ["Ver\u00b7ge\u00b7bens", "ist", "dei\u00b7ne", "Be\u00b7m\u00fc\u00b7hung", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich sehe, da\u00df du der Vergangenheit", "tokens": ["Ich", "se\u00b7he", ",", "da\u00df", "du", "der", "Ver\u00b7gan\u00b7gen\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Geh\u00f6rst in jeder Beziehung.", "tokens": ["Ge\u00b7h\u00f6rst", "in", "je\u00b7der", "Be\u00b7zie\u00b7hung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Fort! fort von hier! im tiefen Grab,", "tokens": ["Fort", "!", "fort", "von", "hier", "!", "im", "tie\u00b7fen", "Grab", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PTKVZ", "APPR", "ADV", "$.", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist Eure nat\u00fcrliche Stelle.", "tokens": ["Ist", "Eu\u00b7re", "na\u00b7t\u00fcr\u00b7li\u00b7che", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Das Leben nimmt jetzt in Beschlag", "tokens": ["Das", "Le\u00b7ben", "nimmt", "jetzt", "in", "Be\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sch\u00e4tze dieser Kapelle.", "tokens": ["Die", "Sch\u00e4t\u00b7ze", "die\u00b7ser", "Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Zukunft fr\u00f6hliche Cavallerie", "tokens": ["Der", "Zu\u00b7kunft", "fr\u00f6h\u00b7li\u00b7che", "Ca\u00b7val\u00b7le\u00b7rie"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Soll hier im Dome hausen.", "tokens": ["Soll", "hier", "im", "Do\u00b7me", "hau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und weicht Ihr nicht willig, so brauch ich Gewalt,", "tokens": ["Und", "weicht", "Ihr", "nicht", "wil\u00b7lig", ",", "so", "brauch", "ich", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADJD", "$,", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und la\u00df\u2019 Euch mit Kolben lausen!", "tokens": ["Und", "la\u00df'", "Euch", "mit", "Kol\u00b7ben", "lau\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "So sprach ich und ich drehte mich um,", "tokens": ["So", "sprach", "ich", "und", "ich", "dreh\u00b7te", "mich", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da sah ich furchtbar blinken", "tokens": ["Da", "sah", "ich", "furcht\u00b7bar", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des stummen Begleiters furchtbares Beil \u2013", "tokens": ["Des", "stum\u00b7men", "Be\u00b7glei\u00b7ters", "furcht\u00b7ba\u00b7res", "Beil", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und er verstand mein Winken.", "tokens": ["Und", "er", "ver\u00b7stand", "mein", "Win\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}