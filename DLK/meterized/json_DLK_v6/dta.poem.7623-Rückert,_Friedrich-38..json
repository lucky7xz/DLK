{"dta.poem.7623": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob Tugend Reinigung, ob Reinheit selber sei?", "tokens": ["Ob", "Tu\u00b7gend", "Rei\u00b7ni\u00b7gung", ",", "ob", "Rein\u00b7heit", "sel\u00b7ber", "sei", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "$,", "KOUS", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob Streben H\u00f6chstem zu, ob H\u00f6chstes strebenfrei?", "tokens": ["Ob", "Stre\u00b7ben", "H\u00f6chs\u00b7tem", "zu", ",", "ob", "H\u00f6chs\u00b7tes", "stre\u00b7ben\u00b7frei", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "PTKVZ", "$,", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nach H\u00f6chstem streben ist das H\u00f6chste freilich nicht,", "tokens": ["Nach", "H\u00f6chs\u00b7tem", "stre\u00b7ben", "ist", "das", "H\u00f6chs\u00b7te", "frei\u00b7lich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "VAFIN", "ART", "ADJA", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein H\u00f6chstes ist es doch, wo H\u00f6heres gebricht.", "tokens": ["Ein", "H\u00f6chs\u00b7tes", "ist", "es", "doch", ",", "wo", "H\u00f6\u00b7he\u00b7res", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "ADV", "$,", "PWAV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und so ist Reinheit auch nicht deine Reinigung;", "tokens": ["Und", "so", "ist", "Rein\u00b7heit", "auch", "nicht", "dei\u00b7ne", "Rei\u00b7ni\u00b7gung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "ADV", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Menschentugend thut sich selber nie genung.", "tokens": ["Und", "Men\u00b7schen\u00b7tu\u00b7gend", "thut", "sich", "sel\u00b7ber", "nie", "ge\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PRF", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}