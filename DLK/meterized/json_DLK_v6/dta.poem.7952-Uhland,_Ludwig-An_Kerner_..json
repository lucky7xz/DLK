{"dta.poem.7952": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An Kerner .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Es war in traurigen Novembertagen,", "tokens": ["Es", "war", "in", "trau\u00b7ri\u00b7gen", "No\u00b7vem\u00b7ber\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich war gewallt zum stillen Tannenhaine", "tokens": ["Ich", "war", "ge\u00b7wallt", "zum", "stil\u00b7len", "Tan\u00b7nen\u00b7hai\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und stand gelehnet an der h\u00f6chsten eine,", "tokens": ["Und", "stand", "ge\u00b7leh\u00b7net", "an", "der", "h\u00f6chs\u00b7ten", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "APPR", "ART", "ADJA", "ART", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da hielt ich deine Lieder aufgeschlagen.", "tokens": ["Da", "hielt", "ich", "dei\u00b7ne", "Lie\u00b7der", "auf\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Versunken war ich in die frommen Sagen:", "tokens": ["Ver\u00b7sun\u00b7ken", "war", "ich", "in", "die", "from\u00b7men", "Sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald kniet\u2019 ich vor Sankt Albans Wundersteine,", "tokens": ["Bald", "kniet'", "ich", "vor", "Sankt", "Al\u00b7bans", "Wun\u00b7der\u00b7stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "VVFIN", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bald schaut\u2019 ich Regiswind im Rosenscheine,", "tokens": ["Bald", "schaut'", "ich", "Re\u00b7gis\u00b7wind", "im", "Ro\u00b7sen\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald sah ich Helicena\u2019s M\u00fcnster ragen.", "tokens": ["Bald", "sah", "ich", "He\u00b7li\u00b7ce\u00b7na's", "M\u00fcns\u00b7ter", "ra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Welch lieblich Wunder wirkten deine Lieder!", "tokens": ["Welch", "lieb\u00b7lich", "Wun\u00b7der", "wirk\u00b7ten", "dei\u00b7ne", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die H\u00f6h\u2019 erschien in goldnem Maienstrale", "tokens": ["Die", "H\u00f6h'", "er\u00b7schien", "in", "gold\u00b7nem", "Mai\u00b7en\u00b7stra\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Fr\u00fchlingsruf ert\u00f6nte durch die Wipfel.", "tokens": ["Und", "Fr\u00fch\u00b7lings\u00b7ruf", "er\u00b7t\u00f6n\u00b7te", "durch", "die", "Wip\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch bald verschwand der Wunderfr\u00fchling wieder,", "tokens": ["Doch", "bald", "ver\u00b7schwand", "der", "Wun\u00b7der\u00b7fr\u00fch\u00b7ling", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er durfte nicht sich senken in die Thale,", "tokens": ["Er", "durf\u00b7te", "nicht", "sich", "sen\u00b7ken", "in", "die", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Fluge streift\u2019 er nur der Erde Gipfel.", "tokens": ["Im", "Flu\u00b7ge", "streift'", "er", "nur", "der", "Er\u00b7de", "Gip\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}