{"dta.poem.1244": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Giebt das Verh\u00e4ngnis uns denn keine Zeit zu letzen?", "tokens": ["Giebt", "das", "Ver\u00b7h\u00e4ng\u00b7nis", "uns", "denn", "kei\u00b7ne", "Zeit", "zu", "let\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geht also schleunig fort der Reise fester Schlu\u00df/", "tokens": ["Geht", "al\u00b7so", "schleu\u00b7nig", "fort", "der", "Rei\u00b7se", "fes\u00b7ter", "Schlu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PTKVZ", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df meinem Munde kaum verlaubt den lezten Ku\u00df", "tokens": ["Da\u00df", "mei\u00b7nem", "Mun\u00b7de", "kaum", "ver\u00b7laubt", "den", "lez\u00b7ten", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In das Corallne Paar der Lippen einzu\u00e4tzen?", "tokens": ["In", "das", "Co\u00b7rall\u00b7ne", "Paar", "der", "Lip\u00b7pen", "ein\u00b7zu\u00b7\u00e4t\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "O Wort/ wie Diamant und harter Stahl zu sch\u00e4tzen/", "tokens": ["O", "Wort", "/", "wie", "Di\u00b7a\u00b7mant", "und", "har\u00b7ter", "Stahl", "zu", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "KOKOM", "NN", "KON", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das Hoffnung und Gedult allein erweichen mu\u00df!", "tokens": ["Das", "Hoff\u00b7nung", "und", "Ge\u00b7dult", "al\u00b7lein", "er\u00b7wei\u00b7chen", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch bringt das Scheiden izt dem Hertzen viel Verdru\u00df/", "tokens": ["Doch", "bringt", "das", "Schei\u00b7den", "izt", "dem", "Hert\u00b7zen", "viel", "Ver\u00b7dru\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wird das Wiedersehn uns desto mehr erg\u00f6tzen.", "tokens": ["So", "wird", "das", "Wie\u00b7der\u00b7sehn", "uns", "des\u00b7to", "mehr", "er\u00b7g\u00f6t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Indessen lebet wohl/ ihr treu-geliebten Sinnen!", "tokens": ["In\u00b7des\u00b7sen", "le\u00b7bet", "wohl", "/", "ihr", "treu\u00b7ge\u00b7lieb\u00b7ten", "Sin\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$(", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es m\u00fcsse Gl\u00fcck und Zeit zu euren Diensten stehn/", "tokens": ["Es", "m\u00fcs\u00b7se", "Gl\u00fcck", "und", "Zeit", "zu", "eu\u00b7ren", "Diens\u00b7ten", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es m\u00fc\u00df euch zu der Hand Lufft/ Erd und Himmel gehn/", "tokens": ["Es", "m\u00fc\u00df", "euch", "zu", "der", "Hand", "Lufft", "/", "Erd", "und", "Him\u00b7mel", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "ART", "NN", "NN", "$(", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bi\u00df wir uns wiederum mit Freud umfassen k\u00fcnnen.", "tokens": ["Bi\u00df", "wir", "uns", "wie\u00b7de\u00b7rum", "mit", "Freud", "um\u00b7fas\u00b7sen", "k\u00fcn\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Schliest eurem Hertzen ein/ wie ich/ ein F\u00fcncklein Liebe/", "tokens": ["Schliest", "eu\u00b7rem", "Hert\u00b7zen", "ein", "/", "wie", "ich", "/", "ein", "F\u00fcnc\u00b7klein", "Lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "$(", "PWAV", "PPER", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So bleibet unsre Glutt verwahrt f\u00fcr Zeit und Diebe.", "tokens": ["So", "blei\u00b7bet", "uns\u00b7re", "Glutt", "ver\u00b7wahrt", "f\u00fcr", "Zeit", "und", "Die\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}