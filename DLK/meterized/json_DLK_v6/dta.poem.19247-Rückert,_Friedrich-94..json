{"dta.poem.19247": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "94.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie mangelhaft und falsch kann eines Menschen Wissen", "tokens": ["Wie", "man\u00b7gel\u00b7haft", "und", "falsch", "kann", "ei\u00b7nes", "Men\u00b7schen", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Himmelsl\u00e4ufen seyn, Mondsonnenfinsternissen!", "tokens": ["Von", "Him\u00b7mels\u00b7l\u00e4u\u00b7fen", "seyn", ",", "Mond\u00b7son\u00b7nen\u00b7fins\u00b7ter\u00b7nis\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VAINF", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Sterne werden durch sein Irren irr nicht werden,", "tokens": ["Die", "Ster\u00b7ne", "wer\u00b7den", "durch", "sein", "Ir\u00b7ren", "irr", "nicht", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wei\u00df er nur selber was er hat zu thun auf Erden.", "tokens": ["Wei\u00df", "er", "nur", "sel\u00b7ber", "was", "er", "hat", "zu", "thun", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PWS", "PPER", "VAFIN", "PTKZU", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wenn er das nicht wei\u00df; was hilft da\u00df er die Bahn", "tokens": ["Und", "wenn", "er", "das", "nicht", "wei\u00df", ";", "was", "hilft", "da\u00df", "er", "die", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PDS", "PTKNEG", "VVFIN", "$.", "PWS", "VVFIN", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Himmels kenne, die er doch nicht wandeln kan!", "tokens": ["Des", "Him\u00b7mels", "ken\u00b7ne", ",", "die", "er", "doch", "nicht", "wan\u00b7deln", "kan", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}