{"dta.poem.18969": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Corydon.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Schaw/ Cloris/ wie der Flu\u00df", "tokens": ["Schaw", "/", "Clo\u00b7ris", "/", "wie", "der", "Flu\u00df"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "NE", "$(", "KOKOM", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Durch meiner Zeher gu\u00df", "tokens": ["Durch", "mei\u00b7ner", "Ze\u00b7her", "gu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zunemend/ dir bekennet", "tokens": ["Zu\u00b7ne\u00b7mend", "/", "dir", "be\u00b7ken\u00b7net"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$(", "PPER", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Das wider dich er brennet", "tokens": ["Das", "wi\u00b7der", "dich", "er", "bren\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd rauschet von verdru\u00df:", "tokens": ["Vnd", "rau\u00b7schet", "von", "ver\u00b7dru\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Schaw wie sein starckes rauschen", "tokens": ["Schaw", "wie", "sein", "star\u00b7ckes", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Versuchend meine brunst", "tokens": ["Ver\u00b7su\u00b7chend", "mei\u00b7ne", "brunst"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verfluchend deine kunst", "tokens": ["Ver\u00b7flu\u00b7chend", "dei\u00b7ne", "kunst"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wolt gern sein layd vertauschen.", "tokens": ["Wolt", "gern", "sein", "layd", "ver\u00b7tau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}