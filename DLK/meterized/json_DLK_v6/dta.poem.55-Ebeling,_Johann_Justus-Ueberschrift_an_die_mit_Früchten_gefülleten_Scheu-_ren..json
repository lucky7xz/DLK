{"dta.poem.55": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Ueberschrift  \n an die mit Fr\u00fcchten gef\u00fclleten Scheu-  \n ren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dies ist des Landmans Vorraths-Kammer,", "tokens": ["Dies", "ist", "des", "Land\u00b7mans", "Vor\u00b7raths\u00b7Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "das Schazhaus, wo sein Reichthum liegt,", "tokens": ["das", "Schaz\u00b7haus", ",", "wo", "sein", "Reicht\u00b7hum", "liegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So lang dieselbe angef\u00fcllet; so lange le-", "tokens": ["So", "lang", "die\u00b7sel\u00b7be", "an\u00b7ge\u00b7f\u00fcl\u00b7let", ";", "so", "lan\u00b7ge", "le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "PDAT", "VVPP", "$.", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Hier liegt ein esbar Gold verwahrt; hier", "tokens": ["Hier", "liegt", "ein", "es\u00b7bar", "Gold", "ver\u00b7wahrt", ";", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "VVPP", "$.", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die besser als des Tagus Sand; hier sind verg\u00fcld-", "tokens": ["Die", "bes\u00b7ser", "als", "des", "Ta\u00b7gus", "Sand", ";", "hier", "sind", "ver\u00b7g\u00fcld"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "KOKOM", "ART", "NN", "NN", "$.", "ADV", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die g\u00fcldne M\u00fcnzen \u00fcbertreffen. Man i\u00dft und trin-", "tokens": ["Die", "g\u00fcld\u00b7ne", "M\u00fcn\u00b7zen", "\u00fc\u00b7bert\u00b7ref\u00b7fen", ".", "Man", "i\u00dft", "und", "trin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "$.", "PIS", "VVFIN", "KON", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Und k\u00f6nnen, wie man diese brauchet, auch dazu", "tokens": ["Und", "k\u00f6n\u00b7nen", ",", "wie", "man", "die\u00b7se", "brau\u00b7chet", ",", "auch", "da\u00b7zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VMFIN", "$,", "PWAV", "PIS", "PDS", "VVFIN", "$,", "ADV", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Man kan den reichen Ueberflus an andre wiederum", "tokens": ["Man", "kan", "den", "rei\u00b7chen", "Ue\u00b7ber\u00b7flus", "an", "and\u00b7re", "wie\u00b7de\u00b7rum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "ADJA", "NN", "APPR", "PIS", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Und dadurch dieses Seegenskorn in Gold und Sil-", "tokens": ["Und", "da\u00b7durch", "die\u00b7ses", "See\u00b7gens\u00b7korn", "in", "Gold", "und", "Sil"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "PDAT", "NN", "APPR", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Proviant-Schaz-Zeuge-H\u00e4user, die in den gros-", "tokens": ["Die", "Pro\u00b7vi\u00b7a\u00b7nt\u00b7Scha\u00b7zZeu\u00b7ge\u00b7H\u00e4u\u00b7ser", ",", "die", "in", "den", "gros"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "TRUNC"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Die kan man auf dem platten Lande, in einer Scheur", "tokens": ["Die", "kan", "man", "auf", "dem", "plat\u00b7ten", "Lan\u00b7de", ",", "in", "ei\u00b7ner", "Scheur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Hier haben Kleidung, Brodt und Geld, und was", "tokens": ["Hier", "ha\u00b7ben", "Klei\u00b7dung", ",", "Brodt", "und", "Geld", ",", "und", "was"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "$,", "NN", "KON", "NN", "$,", "KON", "PWS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die Leute die den Akker baun, vom zugeme\u00dfnen", "tokens": ["Die", "Leu\u00b7te", "die", "den", "Ak\u00b7ker", "baun", ",", "vom", "zu\u00b7ge\u00b7me\u00df\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "ART", "NN", "VVINF", "$,", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Hier nehmen sie auch was sie brauchen, als der", "tokens": ["Hier", "neh\u00b7men", "sie", "auch", "was", "sie", "brau\u00b7chen", ",", "als", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PWS", "PPER", "VVFIN", "$,", "KOUS", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Drum kan man jede Scheure nennen, der H\u00f6ch-", "tokens": ["Drum", "kan", "man", "je\u00b7de", "Scheu\u00b7re", "nen\u00b7nen", ",", "der", "H\u00f6ch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VMFIN", "PIS", "PIAT", "NN", "VVINF", "$,", "ART", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "Drum mu\u00df an ihren Eingang stehn: ", "tokens": ["Drum", "mu\u00df", "an", "ih\u00b7ren", "Ein\u00b7gang", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}