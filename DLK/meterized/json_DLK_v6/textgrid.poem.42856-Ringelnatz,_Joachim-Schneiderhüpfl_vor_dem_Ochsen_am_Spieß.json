{"textgrid.poem.42856": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Schneiderh\u00fcpfl vor dem Ochsen am Spie\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Ma\u00df Bier und zwei Ma\u00df Bier", "tokens": ["Ein", "Ma\u00df", "Bier", "und", "zwei", "Ma\u00df", "Bier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "CARD", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und hundert Ma\u00df Bier und tausend Ma\u00df Bier.", "tokens": ["Und", "hun\u00b7dert", "Ma\u00df", "Bier", "und", "tau\u00b7send", "Ma\u00df", "Bier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "NN", "KON", "CARD", "NN", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So leben wir, so leben wir", "tokens": ["So", "le\u00b7ben", "wir", ",", "so", "le\u00b7ben", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An der Isar.", "tokens": ["An", "der", "I\u00b7sar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und Kalbshaxn und Kalbshaxn.", "tokens": ["Und", "Kalbs\u00b7haxn", "und", "Kalbs\u00b7haxn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wir sind keine Preu\u00dfen, wir sind keine Sachsen.", "tokens": ["Wir", "sind", "kei\u00b7ne", "Preu\u00b7\u00dfen", ",", "wir", "sind", "kei\u00b7ne", "Sach\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Wir sind keine Spie\u00dfer.", "tokens": ["Wir", "sind", "kei\u00b7ne", "Spie\u00b7\u00dfer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Wir sind Genie\u00dfer.", "tokens": ["Wir", "sind", "Ge\u00b7nie\u00b7\u00dfer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Oktoberfest im Mai, im August,", "tokens": ["Ok\u00b7to\u00b7ber\u00b7fest", "im", "Mai", ",", "im", "Au\u00b7gust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Oktober zu jeder Zeit.", "tokens": ["Ok\u00b7to\u00b7ber", "zu", "je\u00b7der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir sind uns unserer selbst bewu\u00dft", "tokens": ["Wir", "sind", "uns", "un\u00b7se\u00b7rer", "selbst", "be\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PPOSAT", "ADV", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und jodeln aus herziger Brust:", "tokens": ["Und", "jo\u00b7deln", "aus", "her\u00b7zi\u00b7ger", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "\u00bbimmer kampfbereit!\u00ab", "tokens": ["\u00bb", "im\u00b7mer", "kampf\u00b7be\u00b7reit", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wir sind urw\u00fcchsig und frei.", "tokens": ["Wir", "sind", "ur\u00b7w\u00fcch\u00b7sig", "und", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wir sind international gesinnt.", "tokens": ["Wir", "sind", "in\u00b7ter\u00b7na\u00b7ti\u00b7o\u00b7nal", "ge\u00b7sinnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Un, zwo, trois, gsuffa!", "tokens": ["Un", ",", "zwo", ",", "trois", ",", "gsuf\u00b7fa", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "CARD", "$,", "FM.fr", "$,", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Es lebe unsere Polizei!", "tokens": ["Es", "le\u00b7be", "un\u00b7se\u00b7re", "Po\u00b7li\u00b7zei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wer unsere Beh\u00f6rden nicht liebt,", "tokens": ["Wer", "un\u00b7se\u00b7re", "Be\u00b7h\u00f6r\u00b7den", "nicht", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Der spinnt.", "tokens": ["Der", "spinnt", "."], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Wir sind tolerant.", "tokens": ["Wir", "sind", "to\u00b7le\u00b7rant", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Die preu\u00dfischen Sauerein", "tokens": ["Die", "preu\u00b7\u00dfi\u00b7schen", "Sau\u00b7er\u00b7ein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Sind uns bekannt.", "tokens": ["Sind", "uns", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Kommt zum Oktoberfest!", "tokens": ["Kommt", "zum", "Ok\u00b7to\u00b7ber\u00b7fest", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Unterst\u00fctzt unsere Brauerein!", "tokens": ["Un\u00b7ter\u00b7st\u00fctzt", "un\u00b7se\u00b7re", "Brau\u00b7er\u00b7ein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.12": {"text": "Himmel Herrgott Sakrament!", "tokens": ["Him\u00b7mel", "Herr\u00b7gott", "Sak\u00b7ra\u00b7ment", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}