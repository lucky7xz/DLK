{"dta.poem.3584": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Schweizerisch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.85", "no:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sisch no nit lang da\u00df gregnet h\u00e4tt,               ", "tokens": ["Sisch", "no", "nit", "lang", "da\u00df", "greg\u00b7net", "h\u00e4tt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKNEG", "ADJD", "KOUS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die L\u00e4ubli tr\u00f6pfle no,", "tokens": ["Die", "L\u00e4ub\u00b7li", "tr\u00f6pf\u00b7le", "no", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "I hab e mohl e Schazli gh\u00e4tt,", "tokens": ["I", "hab", "e", "mohl", "e", "Schaz\u00b7li", "gh\u00e4tt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "I wott, i h\u00e4tt es no.", "tokens": ["I", "wott", ",", "i", "h\u00e4tt", "es", "no", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "NE", "VAFIN", "PPER", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Jez isch er gange go wandere,", "tokens": ["Jez", "isch", "er", "gan\u00b7ge", "go", "wan\u00b7de\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "I w\u00fcnsch em L\u00f6cher in d'Schuh,", "tokens": ["I", "w\u00fcnsch", "em", "L\u00f6\u00b7cher", "in", "d'\u00b7Schuh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Jez hab i wieder en andere,", "tokens": ["Jez", "hab", "i", "wie\u00b7der", "en", "an\u00b7de\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "ADV", "VVFIN", "PIS", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Gott g\u00e4b mer Gl\u00fcck dazu.", "tokens": ["Gott", "g\u00e4b", "mer", "Gl\u00fcck", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "S'isch no nit lang, da\u00df er g'heirat h\u00e4tt,", "tokens": ["S'\u00b7isch", "no", "nit", "lang", ",", "da\u00df", "er", "g'\u00b7hei\u00b7rat", "h\u00e4tt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKNEG", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "VAFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "S'isch gar e kurzi Zyt;", "tokens": ["S'\u00b7isch", "gar", "e", "kur\u00b7zi", "Zyt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "NE", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Si R\u00f6ckli ist em loderich,", "tokens": ["Si", "R\u00f6ck\u00b7li", "ist", "em", "lo\u00b7de\u00b7rich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Si Str\u00fcmpfli sin em z'wyt.", "tokens": ["Si", "Str\u00fcm\u00b7pf\u00b7li", "sin", "em", "z'\u00b7wyt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}}}}