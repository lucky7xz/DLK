{"textgrid.poem.37789": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbich stund auf an eim Morgen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich stund auf an eim Morgen,", "tokens": ["\u00bb", "ich", "stund", "auf", "an", "eim", "Mor\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wollt gen M\u00fcnchen gehn,", "tokens": ["Und", "wollt", "gen", "M\u00fcn\u00b7chen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und war in gro\u00dfen Sorgen,", "tokens": ["Und", "war", "in", "gro\u00b7\u00dfen", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach Gott w\u00e4r ich davon,", "tokens": ["Ach", "Gott", "w\u00e4r", "ich", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VAFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Meim Wirth, dem war ich schuldig viel,", "tokens": ["Meim", "Wirth", ",", "dem", "war", "ich", "schul\u00b7dig", "viel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PDS", "VAFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich wollt ihn gern bezahlen,", "tokens": ["Ich", "wollt", "ihn", "gern", "be\u00b7zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch auf ein ander Ziel.", "tokens": ["Doch", "auf", "ein", "an\u00b7der", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Herr Gast ich hab vernommen,", "tokens": ["Herr", "Gast", "ich", "hab", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du w\u00f6llest von hinnen schier,", "tokens": ["Du", "w\u00f6l\u00b7lest", "von", "hin\u00b7nen", "schier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich la\u00df dich nicht weg kommen,", "tokens": ["Ich", "la\u00df", "dich", "nicht", "weg", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Zehrung zahl vor mir,", "tokens": ["Die", "Zeh\u00b7rung", "zahl", "vor", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Oder setz mir den Mantel ein,", "tokens": ["O\u00b7der", "setz", "mir", "den", "Man\u00b7tel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Demnach will ich gern warten,", "tokens": ["Dem\u00b7nach", "will", "ich", "gern", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auf die Bezahlung dein.", "tokens": ["Auf", "die", "Be\u00b7zah\u00b7lung", "dein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Die Red ging mir zu Herzen,", "tokens": ["Die", "Red", "ging", "mir", "zu", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Betr\u00fcbt ward mir mein Muth,", "tokens": ["Be\u00b7tr\u00fcbt", "ward", "mir", "mein", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich dacht, da hilft kein Scherzen,", "tokens": ["Ich", "dacht", ",", "da", "hilft", "kein", "Scher\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sollt ich mein Mantel gut", "tokens": ["Sollt", "ich", "mein", "Man\u00b7tel", "gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu Augsburg lassen auf der Gant", "tokens": ["Zu", "Augs\u00b7burg", "las\u00b7sen", "auf", "der", "Gant"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und blos von hinnen ziehen,", "tokens": ["Und", "blos", "von", "hin\u00b7nen", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ist allen Singern ein Schand.", "tokens": ["Ist", "al\u00b7len", "Sin\u00b7gern", "ein", "Schand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Ach Wirth nun hab Gedulte", "tokens": ["Ach", "Wirth", "nun", "hab", "Ge\u00b7dul\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "ADV", "VAFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit mir ein kleine Zeit,", "tokens": ["Mit", "mir", "ein", "klei\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist nicht gros die Schulde,", "tokens": ["Es", "ist", "nicht", "gros", "die", "Schul\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vielleicht sich bald begeit,", "tokens": ["Viel\u00b7leicht", "sich", "bald", "be\u00b7geit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ich dich zahl mit baarem Geld,", "tokens": ["Da\u00df", "ich", "dich", "zahl", "mit", "baa\u00b7rem", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drum lasse mich von hinnen,", "tokens": ["Drum", "las\u00b7se", "mich", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich zieh nicht aus der Welt.", "tokens": ["Ich", "zieh", "nicht", "aus", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O Gast! das geschieht mit nichten,", "tokens": ["O", "Gast", "!", "das", "ge\u00b7schieht", "mit", "nich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PDS", "VVFIN", "APPR", "PIS", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da\u00df ich dir borg die\u00dfmal,", "tokens": ["Da\u00df", "ich", "dir", "borg", "die\u00df\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dich hilft kein Ausred-Dichten,", "tokens": ["Dich", "hilft", "kein", "Aus\u00b7re\u00b7dDich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tag Nacht wollst du seyn voll,", "tokens": ["Tag", "Nacht", "wollst", "du", "seyn", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "PPER", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich trug dir auf den besten Wein,", "tokens": ["Ich", "trug", "dir", "auf", "den", "bes\u00b7ten", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drum mach dich nur nicht m\u00fc\u00dfig,", "tokens": ["Drum", "mach", "dich", "nur", "nicht", "m\u00fc\u00b7\u00dfig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich will bezahlet seyn.", "tokens": ["Ich", "will", "be\u00b7zah\u00b7let", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Wirth, der sah ganz krumme,", "tokens": ["Der", "Wirth", ",", "der", "sah", "ganz", "krum\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was ich sang oder sagt,", "tokens": ["Was", "ich", "sang", "o\u00b7der", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "So gab er nichts darumme,", "tokens": ["So", "gab", "er", "nichts", "da\u00b7rum\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erst macht er mich verzagt,", "tokens": ["Erst", "macht", "er", "mich", "ver\u00b7zagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein Geld wu\u00dft ich in solcher Noth,", "tokens": ["Kein", "Geld", "wu\u00dft", "ich", "in", "sol\u00b7cher", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo nicht der fromm Herr Fuker", "tokens": ["Wo", "nicht", "der", "fromm", "Herr", "Fu\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mir hilft mit seinem Rath.", "tokens": ["Mir", "hilft", "mit", "sei\u00b7nem", "Rath", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Herr Fuker la\u00dft Euch erbarmen", "tokens": ["Herr", "Fu\u00b7ker", "la\u00dft", "Euch", "er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "PPER", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mein Klag und gro\u00dfe Pein", "tokens": ["Mein", "Klag", "und", "gro\u00b7\u00dfe", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und kommt zu H\u00fclf mir Armen,", "tokens": ["Und", "kommt", "zu", "H\u00fclf", "mir", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es will bezahlet seyn", "tokens": ["Es", "will", "be\u00b7zah\u00b7let", "seyn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Wirth von mir auf diesen Tag,", "tokens": ["Mein", "Wirth", "von", "mir", "auf", "die\u00b7sen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Mantel thut ihm gefallen,", "tokens": ["Mein", "Man\u00b7tel", "thut", "ihm", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Mich hilft kein Bitt noch Klag.", "tokens": ["Mich", "hilft", "kein", "Bitt", "noch", "Klag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Den Wirth th\u00e4t bald bezahlen", "tokens": ["Den", "Wirth", "th\u00e4t", "bald", "be\u00b7zah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der edel Fuker gut,", "tokens": ["Der", "e\u00b7del", "Fu\u00b7ker", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Schuld ganz \u00fcber alle,", "tokens": ["Mein", "Schuld", "ganz", "\u00fc\u00b7ber", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das macht mir leichten Muth,", "tokens": ["Das", "macht", "mir", "leich\u00b7ten", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich schwang mich zu dem Thor hinaus,", "tokens": ["Ich", "schwang", "mich", "zu", "dem", "Thor", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Adie du kreidiger Wirthe,", "tokens": ["A\u00b7die", "du", "krei\u00b7di\u00b7ger", "Wirt\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Ich komm dir nimmer ins Haus.\u00ab", "tokens": ["Ich", "komm", "dir", "nim\u00b7mer", "ins", "Haus", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}