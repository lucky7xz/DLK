}, "line.3": {"text": "Mit Fahn' und Kreuz und Z\u00fcgen frommer Schaaren;", "tokens": ["Mit", "Fahn'", "und", "Kreuz", "und", "Z\u00fc\u00b7gen", "from\u00b7mer", "Schaa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Etwas vom Segen mu\u00df auf mich auch fallen!", "tokens": ["Et\u00b7was", "vom", "Se\u00b7gen", "mu\u00df", "auf", "mich", "auch", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VMFIN", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.19": {"line.1": {"text": "Wenn hoch in meiner Hand nach Landessitten", "tokens": ["Wenn", "hoch", "in", "mei\u00b7ner", "Hand", "nach", "Lan\u00b7des\u00b7sit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir \u00fcber'm Haupt die Maccaronen schweben,", "tokens": ["Mir", "\u00fc\u00b7ber'm", "Haupt", "die", "Mac\u00b7ca\u00b7ro\u00b7nen", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Freund, da mu\u00df empor sich unbestritten", "tokens": ["Mein", "Freund", ",", "da", "mu\u00df", "em\u00b7por", "sich", "un\u00b7be\u00b7strit\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VMFIN", "PPER", "PRF", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Auge selber auch zum Himmel heben!", "tokens": ["Das", "Au\