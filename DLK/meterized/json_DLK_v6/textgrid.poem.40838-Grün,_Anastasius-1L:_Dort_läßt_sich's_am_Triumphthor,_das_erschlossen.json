-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hinf\u00fchro selber doch zu mir gegangen!\u00ab", "tokens": ["Hin\u00b7f\u00fch\u00b7ro", "sel\u00b7ber", "doch", "zu", "mir", "ge\u00b7gan\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.24": {"line.1": {"text": "So kn\u00fcpft der dunkle Pfad in Enkeltagen", "tokens": ["So", "kn\u00fcpft", "der", "dunk\u00b7le", "Pfad", "in", "En\u00b7kel\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich an des Ahnherrn Gleis, das glanzerhellte,", "tokens": ["Sich", "an", "des", "Ahn\u00b7herrn", "Gleis", ",", "das", "glan\u00b7zer\u00b7hell\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Sklaven gleich, der sich am Siegeswagen", "tokens": ["Dem", "Skla\u00b7ven", "gleich", ",", "der", "sich", "am", "Sie\u00b7ges\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Einst hinter Roms Triumphatoren stellte.", "tokens": ["Einst", "hin\u00b7ter", "Roms", "Tri\u00b7um\u00b7pha\u00b7to\u00b7ren", "stell\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.25": {"line.1": {"text": "Mit einer Kron' in Gold und Demantschimmer", "tokens": ["Mit",