{"textgrid.poem.40838": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort l\u00e4\u00dft sich's am Triumphthor, das erschlossen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort l\u00e4\u00dft sich's am Triumphthor, das erschlossen", "tokens": ["Dort", "l\u00e4\u00dft", "sich's", "am", "Tri\u00b7umph\u00b7thor", ",", "das", "er\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "NN", "$,", "PDS", "VVINF"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Pompeji's Forum einst den Siegeswagen,", "tokens": ["Pom\u00b7pe\u00b7ji's", "Fo\u00b7rum", "einst", "den", "Sie\u00b7ges\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein brauner Lazarone, hingegossen,", "tokens": ["Ein", "brau\u00b7ner", "La\u00b7za\u00b7ro\u00b7ne", ",", "hin\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie die Philosophei im Staub, behagen!", "tokens": ["Wie", "die", "Phi\u00b7lo\u00b7so\u00b7phei", "im", "Staub", ",", "be\u00b7ha\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Am Marmorblock, drauf mocht' ein Gott einst gl\u00e4nzen,", "tokens": ["Am", "Mar\u00b7mor\u00b7block", ",", "drauf", "mocht'", "ein", "Gott", "einst", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PAV", "VVFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "St\u00fctzt er sein Haupt, traun, ein' selt'ne Vase!", "tokens": ["St\u00fctzt", "er", "sein", "Haupt", ",", "traun", ",", "ein'", "selt'\u00b7ne", "Va\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "VVINF", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ein Lorberbaum umweht's mit Schattenkr\u00e4nzen", "tokens": ["Ein", "Lor\u00b7ber\u00b7baum", "um\u00b7weht's", "mit", "Schat\u00b7ten\u00b7kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und streut ihm seine Bl\u00e4tter auf die Nase.", "tokens": ["Und", "streut", "ihm", "sei\u00b7ne", "Bl\u00e4t\u00b7ter", "auf", "die", "Na\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Tag ist lang, und so geschieht's zu Zeiten,", "tokens": ["Der", "Tag", "ist", "lang", ",", "und", "so", "ge\u00b7schieht's", "zu", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ihn beschleichen mancherlei Gedanken,", "tokens": ["Da\u00df", "ihn", "be\u00b7schlei\u00b7chen", "man\u00b7cher\u00b7lei", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die um den alten Stein wie Moos sich breiten,", "tokens": ["Die", "um", "den", "al\u00b7ten", "Stein", "wie", "Moos", "sich", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "KOKOM", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hinan des Lorbers Schaft wie Epheu ranken:", "tokens": ["Hi\u00b7nan", "des", "Lor\u00b7bers", "Schaft", "wie", "E\u00b7pheu", "ran\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbich seh' im Lavapflaster dieser Stra\u00dfe", "tokens": ["\u00bb", "ich", "seh'", "im", "La\u00b7vapf\u00b7las\u00b7ter", "die\u00b7ser", "Stra\u00b7\u00dfe"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "APPRART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Gleis noch von des Triumphators Wagen,", "tokens": ["Das", "Gleis", "noch", "von", "des", "Tri\u00b7um\u00b7pha\u00b7tors", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So frisch, als sei er noch nicht fern die Gasse;", "tokens": ["So", "frisch", ",", "als", "sei", "er", "noch", "nicht", "fern", "die", "Gas\u00b7se", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOKOM", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vielleicht gelingt mir's noch, ihn zu erjagen!", "tokens": ["Viel\u00b7leicht", "ge\u00b7lingt", "mir's", "noch", ",", "ihn", "zu", "er\u00b7ja\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein W\u00f6rtlein, das ich ihm zu sagen h\u00e4tte,", "tokens": ["Ein", "W\u00f6rt\u00b7lein", ",", "das", "ich", "ihm", "zu", "sa\u00b7gen", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Treibt mich ihm nach! Doch nein! Wozu soll's frommen?", "tokens": ["Treibt", "mich", "ihm", "nach", "!", "Doch", "nein", "!", "Wo\u00b7zu", "soll's", "from\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$.", "KON", "PTKANT", "$.", "PWAV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wozu aufstehn von so bequemem Bette?", "tokens": ["Wo\u00b7zu", "auf\u00b7stehn", "von", "so", "be\u00b7que\u00b7mem", "Bet\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-----+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Will er's just wissen, mag er selber kommen!", "tokens": ["Will", "er's", "just", "wis\u00b7sen", ",", "mag", "er", "sel\u00b7ber", "kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VVINF", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich spr\u00e4che: Freund, wozu dein gro\u00dfes Wagen?", "tokens": ["Ich", "spr\u00e4\u00b7che", ":", "Freund", ",", "wo\u00b7zu", "dein", "gro\u00b7\u00dfes", "Wa\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf da\u00df ein Siegeslied dir sei gesungen!", "tokens": ["Auf", "da\u00df", "ein", "Sie\u00b7ges\u00b7lied", "dir", "sei", "ge\u00b7sun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie schad', die sch\u00f6nen Ross' in Schwei\u00df zu jagen,", "tokens": ["Wie", "schad'", ",", "die", "sch\u00f6\u00b7nen", "Ross'", "in", "Schwei\u00df", "zu", "ja\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "ART", "ADJA", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie schade um des Volkes gute Lungen!", "tokens": ["Wie", "scha\u00b7de", "um", "des", "Vol\u00b7kes", "gu\u00b7te", "Lun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wozu so viele Weg' im Weltenraume?", "tokens": ["Wo\u00b7zu", "so", "vie\u00b7le", "Weg'", "im", "Wel\u00b7ten\u00b7rau\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df dir den Lorber reichen deine Br\u00fcder?", "tokens": ["Da\u00df", "dir", "den", "Lor\u00b7ber", "rei\u00b7chen", "dei\u00b7ne", "Br\u00fc\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sieh, Freund, freiwillig senkt in diesem Baume", "tokens": ["Sieh", ",", "Freund", ",", "frei\u00b7wil\u00b7lig", "senkt", "in", "die\u00b7sem", "Bau\u00b7me"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "ADJD", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Himmel selbst den Lorber auf mich nieder!", "tokens": ["Der", "Him\u00b7mel", "selbst", "den", "Lor\u00b7ber", "auf", "mich", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wozu dein Krieg, da's Keinem eingefallen", "tokens": ["Wo\u00b7zu", "dein", "Krieg", ",", "da's", "Kei\u00b7nem", "ein\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "KOUS", "PIS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu stehlen uns die\u00df blaue Meer, die Reben,", "tokens": ["Zu", "steh\u00b7len", "uns", "die\u00df", "blau\u00b7e", "Meer", ",", "die", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "PDS", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den sch\u00f6nen Himmel, Rosen, Nachtigallen?", "tokens": ["Den", "sch\u00f6\u00b7nen", "Him\u00b7mel", ",", "Ro\u00b7sen", ",", "Nach\u00b7ti\u00b7gal\u00b7len", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was sonst ist werth, drum Schwert und Schild zu heben?", "tokens": ["Was", "sonst", "ist", "werth", ",", "drum", "Schwert", "und", "Schild", "zu", "he\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "ADJD", "$,", "PAV", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Der Vesten Fall, die Siege deiner Heere", "tokens": ["Der", "Ves\u00b7ten", "Fall", ",", "die", "Sie\u00b7ge", "dei\u00b7ner", "Hee\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beb\u00fcrden dich mit Pflicht zu neuen Siegen;", "tokens": ["Be\u00b7b\u00fcr\u00b7den", "dich", "mit", "Pflicht", "zu", "neu\u00b7en", "Sie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir gibt die Last, die fr\u00fch ich trug zum Meere,", "tokens": ["Mir", "gibt", "die", "Last", ",", "die", "fr\u00fch", "ich", "trug", "zum", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Tag\u00fcber frei im Sonnenglanz zu liegen!", "tokens": ["Ta\u00b7g\u00fc\u00b7ber", "frei", "im", "Son\u00b7nen\u00b7glanz", "zu", "lie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wozu dein Prunkpalast? Was ist's vonn\u00f6then,", "tokens": ["Wo\u00b7zu", "dein", "Prunk\u00b7pa\u00b7last", "?", "Was", "ist's", "von\u00b7n\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$.", "PWS", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich zu vermauern diesen sch\u00f6nen Himmel!", "tokens": ["Sich", "zu", "ver\u00b7mau\u00b7ern", "die\u00b7sen", "sch\u00f6\u00b7nen", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lustwandeln gehn hei\u00dft nur dem Herrn zertreten", "tokens": ["Lust\u00b7wan\u00b7deln", "gehn", "hei\u00dft", "nur", "dem", "Herrn", "zer\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "VVFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Rasen und der Blumen bunt Gewimmel.", "tokens": ["Den", "Ra\u00b7sen", "und", "der", "Blu\u00b7men", "bunt", "Ge\u00b7wim\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Wozu auf der Orangen B\u00e4ume klettern?", "tokens": ["Wo\u00b7zu", "auf", "der", "O\u00b7ran\u00b7gen", "B\u00e4u\u00b7me", "klet\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sie werden reif selbst in den Schoo\u00df dir fallen!", "tokens": ["Sie", "wer\u00b7den", "reif", "selbst", "in", "den", "Schoo\u00df", "dir", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was soll im Rosendorn die Nase bl\u00e4ttern?", "tokens": ["Was", "soll", "im", "Ro\u00b7sen\u00b7dorn", "die", "Na\u00b7se", "bl\u00e4t\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Duft liegt selber dran, zu ihr zu wallen!", "tokens": ["Dem", "Duft", "liegt", "sel\u00b7ber", "dran", ",", "zu", "ihr", "zu", "wal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PAV", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Der Stein und ich sind Freunde und Verm\u00e4hlte,", "tokens": ["Der", "Stein", "und", "ich", "sind", "Freun\u00b7de", "und", "Ver\u00b7m\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPER", "VAFIN", "NN", "KON", "NN",