{"textgrid.poem.67727": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Empfindungen der Freundschaft", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, um des ersten Othems, der Dich nannte,", "tokens": ["Du", ",", "um", "des", "ers\u00b7ten", "O\u00b7thems", ",", "der", "Dich", "nann\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUI", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Um jenes sanften Feuerblicks,", "tokens": ["Um", "je\u00b7nes", "sanf\u00b7ten", "Feu\u00b7er\u00b7blicks", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Dich mit Gottes Finger in mich brannte,", "tokens": ["Der", "Dich", "mit", "Got\u00b7tes", "Fin\u00b7ger", "in", "mich", "brann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um alles Freundschaftsgl\u00fccks,", "tokens": ["Um", "al\u00b7les", "Freund\u00b7schafts\u00b7gl\u00fccks", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das wir \u2013 doch f\u00fchltest Du's, wenn diese Thr\u00e4ne,", "tokens": ["Das", "wir", "\u2013", "doch", "f\u00fchl\u00b7test", "Du's", ",", "wenn", "die\u00b7se", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$(", "ADV", "VVFIN", "NE", "$,", "KOUS", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Seele Blut, sich dr\u00e4ngte hin", "tokens": ["Der", "See\u00b7le", "Blut", ",", "sich", "dr\u00e4ng\u00b7te", "hin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRF", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Freundesbusen? O dring, blut'ge Thr\u00e4ne,", "tokens": ["Zum", "Freun\u00b7des\u00b7bu\u00b7sen", "?", "O", "dring", ",", "blut'\u00b7ge", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "NE", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ans Menschenherz denn hin,", "tokens": ["Ans", "Men\u00b7schen\u00b7herz", "denn", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Durch Stein und Busen! Wie des Blutes Stimmen", "tokens": ["Durch", "Stein", "und", "Bu\u00b7sen", "!", "Wie", "des", "Blu\u00b7tes", "Stim\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$.", "PWAV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum M\u00f6rder rufen: ", "tokens": ["Zum", "M\u00f6r\u00b7der", "ru\u00b7fen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "So t\u00f6dt ihn Du, da\u00df, wenn in ihm verglimmen", "tokens": ["So", "t\u00f6dt", "ihn", "Du", ",", "da\u00df", ",", "wenn", "in", "ihm", "ver\u00b7glim\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "KOUS", "$,", "KOUS", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Tr\u00e4ume wilder Wuth,", "tokens": ["Die", "Tr\u00e4u\u00b7me", "wil\u00b7der", "Wuth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihn k\u00fc\u00dfte, sprach, las und empfand,", "tokens": ["Ihn", "k\u00fc\u00df\u00b7te", ",", "sprach", ",", "las", "und", "emp\u00b7fand", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Empfand Dich, Gluth! \u2013 O, bist Du nicht versto\u00dfen", "tokens": ["Emp\u00b7fand", "Dich", ",", "Gluth", "!", "\u2013", "O", ",", "bist", "Du", "nicht", "ver\u00b7sto\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "$(", "NE", "$,", "VAFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ins H\u00f6llenmarterland", "tokens": ["Ins", "H\u00f6l\u00b7len\u00b7mar\u00b7ter\u00b7land"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Eskimos und rasender Oreste,", "tokens": ["Der", "Es\u00b7ki\u00b7mos", "und", "ra\u00b7sen\u00b7der", "O\u00b7res\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "So h\u00f6r! und h\u00f6rst Du nicht, gespie\u00dft", "tokens": ["So", "h\u00f6r", "!", "und", "h\u00f6rst", "Du", "nicht", ",", "ge\u00b7spie\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$.", "KON", "VVFIN", "PPER", "PTKNEG", "$,", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Eskimos und rasend als Oreste,", "tokens": ["Wie", "Es\u00b7ki\u00b7mos", "und", "ra\u00b7send", "als", "O\u00b7res\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "ADJD", "KOKOM", "NE", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Fluch' ich dann Deiner List,", "tokens": ["Fluch'", "ich", "dann", "Dei\u00b7ner", "List", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Rei\u00df' aus mein Herz, Dir, Unding, falsch gepr\u00e4get,", "tokens": ["Rei\u00df'", "aus", "mein", "Herz", ",", "Dir", ",", "Un\u00b7ding", ",", "falsch", "ge\u00b7pr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "PPER", "$,", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zertret' in seinem Blut Dein Bild", "tokens": ["Zer\u00b7tret'", "in", "sei\u00b7nem", "Blut", "Dein", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jauchze Freundesrache! Doch \u2013 wie es schl\u00e4get,", "tokens": ["Und", "jauch\u00b7ze", "Freun\u00b7des\u00b7ra\u00b7che", "!", "Doch", "\u2013", "wie", "es", "schl\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "KON", "$(", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Weil doch noch Freundschaft quillt!", "tokens": ["Weil", "doch", "noch", "Freund\u00b7schaft", "quillt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}