{"textgrid.poem.59442": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der K\u00fchne", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wo noch kein Wandrer gegangen,", "tokens": ["Und", "wo", "noch", "kein", "Wand\u00b7rer", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Hoch \u00fcber J\u00e4ger und Ro\u00df", "tokens": ["Hoch", "\u00fc\u00b7ber", "J\u00e4\u00b7ger", "und", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Felsen im Abendrot hangen", "tokens": ["Die", "Fel\u00b7sen", "im", "A\u00b7ben\u00b7drot", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Als wie ein Wolkenschlo\u00df.", "tokens": ["Als", "wie", "ein", "Wol\u00b7ken\u00b7schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dort zwischen den Zinnen und Spitzen", "tokens": ["Dort", "zwi\u00b7schen", "den", "Zin\u00b7nen", "und", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Von wilden Nelken umbl\u00fcht,", "tokens": ["Von", "wil\u00b7den", "Nel\u00b7ken", "um\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die sch\u00f6nen Waldfrauen sitzen", "tokens": ["Die", "sch\u00f6\u00b7nen", "Wald\u00b7frau\u00b7en", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Und singen im Wind ihr Lied.", "tokens": ["Und", "sin\u00b7gen", "im", "Wind", "ihr", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der J\u00e4ger schaut nach dem Schlosse:", "tokens": ["Der", "J\u00e4\u00b7ger", "schaut", "nach", "dem", "Schlos\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die droben das ist mein Lieb! \u2013", "tokens": ["Die", "dro\u00b7ben", "das", "ist", "mein", "Lieb", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "PDS", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er sprang vom scheuenden Rosse,", "tokens": ["Er", "sprang", "vom", "scheu\u00b7en\u00b7den", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wei\u00df keiner, wo er blieb.", "tokens": ["Wei\u00df", "kei\u00b7ner", ",", "wo", "er", "blieb", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}