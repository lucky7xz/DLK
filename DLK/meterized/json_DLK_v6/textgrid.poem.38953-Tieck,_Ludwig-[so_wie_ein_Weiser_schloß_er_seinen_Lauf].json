{"textgrid.poem.38953": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[so wie ein Weiser schlo\u00df er seinen Lauf]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wie ein Weiser schlo\u00df er seinen Lauf,", "tokens": ["So", "wie", "ein", "Wei\u00b7ser", "schlo\u00df", "er", "sei\u00b7nen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wohlth\u00e4t'g war er, und th\u00e4tig wohl zum Guten,", "tokens": ["Wohlt\u00b7h\u00e4t'g", "war", "er", ",", "und", "th\u00e4\u00b7tig", "wohl", "zum", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "KON", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem freien Sinn konnt alles man zumuthen", "tokens": ["Dem", "frei\u00b7en", "Sinn", "konnt", "al\u00b7les", "man", "zu\u00b7mut\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PIS", "PIS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gebildet war er und gekl\u00e4ret auf.", "tokens": ["Ge\u00b7bil\u00b7det", "war", "er", "und", "ge\u00b7kl\u00e4\u00b7ret", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Jeglichem Streben war er oben drauf,", "tokens": ["Jeg\u00b7li\u00b7chem", "Stre\u00b7ben", "war", "er", "o\u00b7ben", "drauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nie ruhig wu\u00dft er sich also zu sputen", "tokens": ["Nie", "ru\u00b7hig", "wu\u00dft", "er", "sich", "al\u00b7so", "zu", "spu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PRF", "ADV", "PTKZU", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Da\u00df selbst die Meister gegen ihn Rekruten,", "tokens": ["Da\u00df", "selbst", "die", "Meis\u00b7ter", "ge\u00b7gen", "ihn", "Rek\u00b7ru\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So exercirt er Tag, Nacht, ohn Verschnauf.", "tokens": ["So", "ex\u00b7er\u00b7cirt", "er", "Tag", ",", "Nacht", ",", "ohn", "Ver\u00b7schnauf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "NN", "$,", "KOUI", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Moral, Choral, Frugal und Ideale,", "tokens": ["Mo\u00b7ral", ",", "Cho\u00b7ral", ",", "Fru\u00b7gal", "und", "I\u00b7dea\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Real, Sentimental, die Ale alle", "tokens": ["Re\u00b7al", ",", "Sen\u00b7ti\u00b7men\u00b7tal", ",", "die", "A\u00b7le", "al\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "ART", "NN", "PIAT"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wu\u00dft' er an seinen Pfoten abzuz\u00e4hlen.", "tokens": ["Wu\u00dft'", "er", "an", "sei\u00b7nen", "Pfo\u00b7ten", "ab\u00b7zu\u00b7z\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie mu\u00df der Zeit doch dieser Edle fehlen!", "tokens": ["Wie", "mu\u00df", "der", "Zeit", "doch", "die\u00b7ser", "Ed\u00b7le", "feh\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ADV", "PDAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Bildung all sank in des Orkus Halle,", "tokens": ["Die", "Bil\u00b7dung", "all", "sank", "in", "des", "Or\u00b7kus", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "VVFIN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir weinen an der Urn' im stillen Thale.", "tokens": ["Wir", "wei\u00b7nen", "an", "der", "Urn'", "im", "stil\u00b7len", "Tha\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}