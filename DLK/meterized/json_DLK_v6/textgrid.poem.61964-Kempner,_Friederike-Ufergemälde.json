{"textgrid.poem.61964": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Ufergem\u00e4lde", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es heulet der Sturm,", "tokens": ["Es", "heu\u00b7let", "der", "Sturm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Es tobet die See,", "tokens": ["Es", "to\u00b7bet", "die", "See", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es peitschen die Wellen", "tokens": ["Es", "peit\u00b7schen", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die See in die H\u00f6h'.", "tokens": ["Die", "See", "in", "die", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Es steuert ein Fahrzeug", "tokens": ["Es", "steu\u00b7ert", "ein", "Fahr\u00b7zeug"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Am seegr\u00fcnen Strand,", "tokens": ["Am", "see\u00b7gr\u00fc\u00b7nen", "Strand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es steiget die Mannschaft", "tokens": ["Es", "stei\u00b7get", "die", "Mann\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mit Beben an's Land.", "tokens": ["Mit", "Be\u00b7ben", "an's", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Ein Weib ist dazwischen,", "tokens": ["Ein", "Weib", "ist", "da\u00b7zwi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Das Kind auf dem Arm,", "tokens": ["Das", "Kind", "auf", "dem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dr\u00fcckt's fester und flehet:", "tokens": ["Dr\u00fcckt's", "fes\u00b7ter", "und", "fle\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Da\u00df Gott sich erbarm'!", "tokens": ["Da\u00df", "Gott", "sich", "er\u00b7barm'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Gerettet, bewahret", "tokens": ["Ge\u00b7ret\u00b7tet", ",", "be\u00b7wah\u00b7ret"], "token_info": ["word", "punct", "word"], "pos": ["VVPP", "$,", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Von g\u00f6ttlicher Hand,", "tokens": ["Von", "g\u00f6tt\u00b7li\u00b7cher", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Bewahrt vor dem Abgrund,", "tokens": ["Be\u00b7wahrt", "vor", "dem", "Ab\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Tiefe Gestrand.", "tokens": ["Der", "Tie\u00b7fe", "Ge\u00b7strand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Am Ufer ich bete,", "tokens": ["Am", "U\u00b7fer", "ich", "be\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mit Blumen geschm\u00fcckt,", "tokens": ["Mit", "Blu\u00b7men", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mein Kind, es ist eisig,", "tokens": ["Mein", "Kind", ",", "es", "ist", "ei\u00b7sig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mein Haupt ist geb\u00fcckt.", "tokens": ["Mein", "Haupt", "ist", "ge\u00b7b\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Tot! Tot \u2013 sie es sagen,", "tokens": ["Tot", "!", "Tot", "\u2013", "sie", "es", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$(", "PPER", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "O Vater, o nein,", "tokens": ["O", "Va\u00b7ter", ",", "o", "nein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "PTKANT", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Du l\u00e4ssest nicht halb nur", "tokens": ["Du", "l\u00e4s\u00b7sest", "nicht", "halb", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gerettet uns sein!", "tokens": ["Ge\u00b7ret\u00b7tet", "uns", "sein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VAINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Es schlo\u00df in den Fluten", "tokens": ["Es", "schlo\u00df", "in", "den", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Aeugelein zu,", "tokens": ["Die", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", "zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O rettender Gott,", "tokens": ["O", "ret\u00b7ten\u00b7der", "Gott", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Gelobet seist Du!", "tokens": ["Ge\u00b7lo\u00b7bet", "seist", "Du", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Belebe mein Kindlein,", "tokens": ["Be\u00b7le\u00b7be", "mein", "Kin\u00b7dlein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mein Herz und mein Blut,", "tokens": ["Mein", "Herz", "und", "mein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sonst wollte ich lieber", "tokens": ["Sonst", "woll\u00b7te", "ich", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Hinab in die Flut;", "tokens": ["Hin\u00b7ab", "in", "die", "Flut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Zur\u00fcck in die Tiefe,", "tokens": ["Zu\u00b7r\u00fcck", "in", "die", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In Wassers Gewalt,", "tokens": ["In", "Was\u00b7sers", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wo unser Notschu\u00df", "tokens": ["Wo", "un\u00b7ser", "Not\u00b7schu\u00df"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "In Kl\u00fcften verhallt.", "tokens": ["In", "Kl\u00fcf\u00b7ten", "ver\u00b7hallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "Das Auge sie hebet", "tokens": ["Das", "Au\u00b7ge", "sie", "he\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zum Himmel empor,", "tokens": ["Zum", "Him\u00b7mel", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da schlaget, horch pl\u00f6tzlich", "tokens": ["Da", "schla\u00b7get", ",", "horch", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "VVIMP", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein Schrei an ihr Ohr.", "tokens": ["Ein", "Schrei", "an", "ihr", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.11": {"line.1": {"text": "Ei, sieh da, das Kindlein,", "tokens": ["Ei", ",", "sieh", "da", ",", "das", "Kin\u00b7dlein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ADV", "$,", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Das Kind ist erwacht,", "tokens": ["Das", "Kind", "ist", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sein Mund hat geschrieen,", "tokens": ["Sein", "Mund", "hat", "ge\u00b7schri\u00b7een", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sein Aug' hat gelacht!", "tokens": ["Sein", "Aug'", "hat", "ge\u00b7lacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.12": {"line.1": {"text": "Es sinkt in die Kniee", "tokens": ["Es", "sinkt", "in", "die", "Kni\u00b7ee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Mutter am Strand,", "tokens": ["Die", "Mut\u00b7ter", "am", "Strand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und rufet ganz trunken:", "tokens": ["Und", "ru\u00b7fet", "ganz", "trun\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "O sehet doch Gottes Hand!", "tokens": ["O", "se\u00b7het", "doch", "Got\u00b7tes", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Die M\u00e4nner, sie wenden", "tokens": ["Die", "M\u00e4n\u00b7ner", ",", "sie", "wen\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Verwundert sich um", "tokens": ["Ver\u00b7wun\u00b7dert", "sich", "um"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und geben das Kindlein", "tokens": ["Und", "ge\u00b7ben", "das", "Kin\u00b7dlein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Runde herum.", "tokens": ["Die", "Run\u00b7de", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.14": {"line.1": {"text": "Sie hei\u00dfen es Jeder", "tokens": ["Sie", "hei\u00b7\u00dfen", "es", "Je\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIS"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Willkommen am Land!", "tokens": ["Will\u00b7kom\u00b7men", "am", "Land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und murmeln dazwischen:", "tokens": ["Und", "mur\u00b7meln", "da\u00b7zwi\u00b7schen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "O sehet doch Gottes Hand!", "tokens": ["O", "se\u00b7het", "doch", "Got\u00b7tes", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}