{"dta.poem.4208": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Wunsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier geh ich, HErr! du wei\u00dft, wie lange,", "tokens": ["Hier", "geh", "ich", ",", "Herr", "!", "du", "wei\u00dft", ",", "wie", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$.", "PPER", "VVFIN", "$,", "PWAV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und auch, wie kurz nur ich hier geh.", "tokens": ["Und", "auch", ",", "wie", "kurz", "nur", "ich", "hier", "geh."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ADV", "$,", "PWAV", "ADJD", "ADV", "PPER", "ADV", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Wie lang\u2019 und kurz es nun gescheh\u2019;", "tokens": ["Wie", "lang'", "und", "kurz", "es", "nun", "ge\u00b7scheh'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So gieb, da\u00df in Vergn\u00fcglichkeit,", "tokens": ["So", "gieb", ",", "da\u00df", "in", "Ver\u00b7gn\u00fcg\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "$,", "KOUS", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In dieser ungewissen Zeit,", "tokens": ["In", "die\u00b7ser", "un\u00b7ge\u00b7wis\u00b7sen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich das, was sch\u00f6n, vern\u00fcnftig seh\u2019!", "tokens": ["Ich", "das", ",", "was", "sch\u00f6n", ",", "ver\u00b7n\u00fcnf\u00b7tig", "seh'", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PDS", "$,", "PRELS", "ADJD", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht minder, da\u00df ich, Dir zur Ehre,", "tokens": ["Nicht", "min\u00b7der", ",", "da\u00df", "ich", ",", "Dir", "zur", "Eh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "PPER", "$,", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was hier so sch\u00f6n zu h\u00f6ren, h\u00f6re!", "tokens": ["Was", "hier", "so", "sch\u00f6n", "zu", "h\u00f6\u00b7ren", ",", "h\u00f6\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Auch, wenn ich rieche, f\u00fchl\u2019 und schmecke,", "tokens": ["Auch", ",", "wenn", "ich", "rie\u00b7che", ",", "f\u00fchl'", "und", "schme\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df ich in allen Dich entdecke!", "tokens": ["Da\u00df", "ich", "in", "al\u00b7len", "Dich", "ent\u00b7de\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}