{"textgrid.poem.58075": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "113. Nimm dich's nicht an, was wird getan!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn man dich ha\u00dft, wenn man dich liebt,", "tokens": ["Wenn", "man", "dich", "ha\u00dft", ",", "wenn", "man", "dich", "liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn man dir nimmt, wenn man dir gibt,", "tokens": ["Wenn", "man", "dir", "nimmt", ",", "wenn", "man", "dir", "gibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn man dich schilt, wenn man dich ehrt,", "tokens": ["Wenn", "man", "dich", "schilt", ",", "wenn", "man", "dich", "ehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und was dir sonst auch widerf\u00e4hrt:", "tokens": ["Und", "was", "dir", "sonst", "auch", "wi\u00b7der\u00b7f\u00e4hrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bleib ungest\u00f6rt und abgeschieden", "tokens": ["Bleib", "un\u00b7ge\u00b7st\u00f6rt", "und", "ab\u00b7ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In deinem Grund mit Gott zufrieden!", "tokens": ["In", "dei\u00b7nem", "Grund", "mit", "Gott", "zu\u00b7frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}