{"textgrid.poem.28391": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da von Abrahams Stamm das Scepter ward verloren,", "tokens": ["Da", "von", "Ab\u00b7ra\u00b7hams", "Stamm", "das", "Scep\u00b7ter", "ward", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ist k\u00fcrtzlich Christus drauff in diese Welt geboren.", "tokens": ["Ist", "k\u00fcrtz\u00b7lich", "Chris\u00b7tus", "drauff", "in", "die\u00b7se", "Welt", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "NE", "PAV", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun aber Christus Wort uns Christen wird genummen,", "tokens": ["Nun", "a\u00b7ber", "Chris\u00b7tus", "Wort", "uns", "Chris\u00b7ten", "wird", "ge\u00b7num\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "NN", "PPER", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "D\u00fcrfft auch nicht Christus drauff wol ehstes wieder kummen?", "tokens": ["D\u00fcrfft", "auch", "nicht", "Chris\u00b7tus", "drauff", "wol", "ehs\u00b7tes", "wie\u00b7der", "kum\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "NE", "PAV", "ADV", "ADJA", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iam