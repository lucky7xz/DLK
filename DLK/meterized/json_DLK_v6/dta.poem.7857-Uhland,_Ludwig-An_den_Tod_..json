{"dta.poem.7857": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An den Tod .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Der du still im Abendlichte", "tokens": ["Der", "du", "still", "im", "A\u00b7bend\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandelst durch der Erde Beek,", "tokens": ["Wan\u00b7delst", "durch", "der", "Er\u00b7de", "Beek", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Klare Blumen, goldne Fr\u00fcchte", "tokens": ["Kla\u00b7re", "Blu\u00b7men", ",", "gold\u00b7ne", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sammelst, die dir Gott ges\u00e4t:", "tokens": ["Sam\u00b7melst", ",", "die", "dir", "Gott", "ge\u00b7s\u00e4t", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schon\u2019, o Tod, was, sanft entz\u00fccket,", "tokens": ["Schon'", ",", "o", "Tod", ",", "was", ",", "sanft", "ent\u00b7z\u00fc\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "PRELS", "$,", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An des Lebens Brust sich schmiegt,", "tokens": ["An", "des", "Le\u00b7bens", "Brust", "sich", "schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich zum s\u00fc\u00dfen Liede wiegt", "tokens": ["Sich", "zum", "s\u00fc\u00b7\u00dfen", "Lie\u00b7de", "wiegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und zum Mutterauge blicket!", "tokens": ["Und", "zum", "Mut\u00b7ter\u00b7au\u00b7ge", "bli\u00b7cket", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df der Erde ihre S\u00f6hne,", "tokens": ["La\u00df", "der", "Er\u00b7de", "ih\u00b7re", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deren Kraft im Sturme fleugt,", "tokens": ["De\u00b7ren", "Kraft", "im", "Stur\u00b7me", "fleugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ein freudiges Get\u00f6ne", "tokens": ["Da\u00df", "ein", "freu\u00b7di\u00b7ges", "Ge\u00b7t\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schnell aus todten W\u00e4ldern steigt!", "tokens": ["Schnell", "aus", "tod\u00b7ten", "W\u00e4l\u00b7dern", "steigt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00f6sche nicht den Geist des Weisen,", "tokens": ["L\u00f6\u00b7sche", "nicht", "den", "Geist", "des", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dessen heil\u2019gen Sonnenglanz,", "tokens": ["Des\u00b7sen", "heil'\u00b7gen", "Son\u00b7nen\u00b7glanz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sch\u00f6n verwebt in sichrem Tanz,", "tokens": ["Sch\u00f6n", "ver\u00b7webt", "in", "sich\u00b7rem", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Jugendliche Mond\u2019 umkreisen.", "tokens": ["Ju\u00b7gend\u00b7li\u00b7che", "Mond'", "um\u00b7krei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf der Silberwolke fahre", "tokens": ["Auf", "der", "Sil\u00b7ber\u00b7wol\u00b7ke", "fah\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Still dahin zur Sternezeit,", "tokens": ["Still", "da\u00b7hin", "zur", "Ster\u00b7ne\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ein Greis am Hausaltare", "tokens": ["Wo", "ein", "Greis", "am", "Haus\u00b7al\u00b7ta\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedem Abend Thr\u00e4nen weiht;", "tokens": ["Je\u00b7dem", "A\u00b7bend", "Thr\u00e4\u00b7nen", "weiht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sprich die Namen seiner Lieben,", "tokens": ["Sprich", "die", "Na\u00b7men", "sei\u00b7ner", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fchr ihn auf in ihren Kranz,", "tokens": ["F\u00fchr", "ihn", "auf", "in", "ih\u00b7ren", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo des Auges ew\u2019gen Glanz", "tokens": ["Wo", "des", "Au\u00b7ges", "ew'\u00b7gen", "Glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Keiner Trennung Z\u00e4hren tr\u00fcben!", "tokens": ["Kei\u00b7ner", "Tren\u00b7nung", "Z\u00e4h\u00b7ren", "tr\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und den J\u00fcngling, dem die Liebe", "tokens": ["Und", "den", "J\u00fcng\u00b7ling", ",", "dem", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heisses Sehnen aufgeweckt,", "tokens": ["Heis\u00b7ses", "Seh\u00b7nen", "auf\u00b7ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der in ungestilltem Triebe", "tokens": ["Der", "in", "un\u00b7ge\u00b7still\u00b7tem", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Offne Arme ausgestreckt,", "tokens": ["Off\u00b7ne", "Ar\u00b7me", "aus\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dann zur Blumenflur der Sterne", "tokens": ["Dann", "zur", "Blu\u00b7men\u00b7flur", "der", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aufgeschauet liebewarm:", "tokens": ["Auf\u00b7ge\u00b7schau\u00b7et", "lie\u00b7be\u00b7warm", ":"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fa\u00df ihn freundlich Arm in Arm,", "tokens": ["Fa\u00df", "ihn", "freund\u00b7lich", "Arm", "in", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Trag ihn in die blaue Ferne!", "tokens": ["Trag", "ihn", "in", "die", "blau\u00b7e", "Fer\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo es br\u00e4utlich gl\u00e4nzt und hallet,", "tokens": ["Wo", "es", "br\u00e4ut\u00b7lich", "gl\u00e4nzt", "und", "hal\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebeathmend ihn umschlie\u00dft,", "tokens": ["Lie\u00b7be\u00b7ath\u00b7mend", "ihn", "um\u00b7schlie\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ihn geistig einst umwallet", "tokens": ["Was", "ihn", "geis\u00b7tig", "einst", "um\u00b7wal\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADJD", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit leisem Gru\u00df gegr\u00fc\u00dft;", "tokens": ["Und", "mit", "lei\u00b7sem", "Gru\u00df", "ge\u00b7gr\u00fc\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo es in der Seele maiet,", "tokens": ["Wo", "es", "in", "der", "See\u00b7le", "mai\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die, von neuem Leben jung,", "tokens": ["Die", ",", "von", "neu\u00b7em", "Le\u00b7ben", "jung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ewiger Begeisterung,", "tokens": ["E\u00b7wi\u00b7ger", "Be\u00b7geis\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ewigen Gesangs sich freuet,", "tokens": ["E\u00b7wi\u00b7gen", "Ge\u00b7sangs", "sich", "freu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}}}}}