{"textgrid.poem.37722": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die Wahrsagerin", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vater von allen,", "tokens": ["Va\u00b7ter", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Was soll ich flehen?", "tokens": ["Was", "soll", "ich", "fle\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Klagen erschallen,", "tokens": ["Kla\u00b7gen", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "M\u00fcde Seufzer ergehen;", "tokens": ["M\u00fc\u00b7de", "Seuf\u00b7zer", "er\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Woher der Wind mag wehen,", "tokens": ["Wo\u00b7her", "der", "Wind", "mag", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wohin die Fahnen stehen,", "tokens": ["Wo\u00b7hin", "die", "Fah\u00b7nen", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcrchten und Hoffen,", "tokens": ["F\u00fcrch\u00b7ten", "und", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Donnernd steht der Himmel offen.", "tokens": ["Don\u00b7nernd", "steht", "der", "Him\u00b7mel", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was soll ich beten,", "tokens": ["Was", "soll", "ich", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Bittend abwenden?", "tokens": ["Bit\u00b7tend", "ab\u00b7wen\u00b7den", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "\u00dcber dem Beten", "tokens": ["\u00dc\u00b7ber", "dem", "Be\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Schon mit ", "tokens": ["Schon", "mit"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Woher die Boten kommen,", "tokens": ["Wo\u00b7her", "die", "Bo\u00b7ten", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sind alle schon beklommen,", "tokens": ["Sind", "al\u00b7le", "schon", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcrchten nicht Hoffen", "tokens": ["F\u00fcrch\u00b7ten", "nicht", "Hof\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PTKNEG", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Lebet in der Erd' verschlossen.", "tokens": ["Le\u00b7bet", "in", "der", "Erd'", "ver\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "L\u00f6se entbinde", "tokens": ["L\u00f6\u00b7se", "ent\u00b7bin\u00b7de"], "token_info": ["word", "word"], "pos": ["NN", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Meine Geschwinden", "tokens": ["Mei\u00b7ne", "Ge\u00b7schwin\u00b7den"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "\u00dcber dem Winde,", "tokens": ["\u00dc\u00b7ber", "dem", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ahndend Schicksal zu k\u00fcnden!", "tokens": ["Ahn\u00b7dend", "Schick\u00b7sal", "zu", "k\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Bin meines Schicksals m\u00fcde,", "tokens": ["Bin", "mei\u00b7nes", "Schick\u00b7sals", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Zauberkessel siede,", "tokens": ["Der", "Zau\u00b7ber\u00b7kes\u00b7sel", "sie\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcrchten und Hoffen,", "tokens": ["F\u00fcrch\u00b7ten", "und", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Brause zu der Erde offen.", "tokens": ["Brau\u00b7se", "zu", "der", "Er\u00b7de", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lasse die Karten", "tokens": ["Las\u00b7se", "die", "Kar\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Friedlicher fallen,", "tokens": ["Fried\u00b7li\u00b7cher", "fal\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Menschlich wir warten,", "tokens": ["Menschlich", "wir", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Halten's z\u00fcrnend mit allen;", "tokens": ["Hal\u00b7ten's", "z\u00fcr\u00b7nend", "mit", "al\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "PIAT", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Wer kann Vernichten k\u00fcnden,", "tokens": ["Wer", "kann", "Ver\u00b7nich\u00b7ten", "k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es weht in allen Winden,", "tokens": ["Es", "weht", "in", "al\u00b7len", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "L\u00e4\u00dft sich nicht bannen,", "tokens": ["L\u00e4\u00dft", "sich", "nicht", "ban\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Thr\u00e4nen von dem Himmel rannen.", "tokens": ["Thr\u00e4\u00b7nen", "von", "dem", "Him\u00b7mel", "ran\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Blaugr\u00fcne Reihen", "tokens": ["Blau\u00b7gr\u00fc\u00b7ne", "Rei\u00b7hen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dr\u00e4ngen wie Wellen,", "tokens": ["Dr\u00e4n\u00b7gen", "wie", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "F\u00fcrchterlich dr\u00e4uen,", "tokens": ["F\u00fcrch\u00b7ter\u00b7lich", "dr\u00e4u\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "An der K\u00fcste zerschellen!", "tokens": ["An", "der", "K\u00fcs\u00b7te", "zer\u00b7schel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ich kann nicht Sieger finden!", "tokens": ["Ich", "kann", "nicht", "Sie\u00b7ger", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Geist will sich verk\u00fcnden!", "tokens": ["Ein", "Geist", "will", "sich", "ver\u00b7k\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Alle betroffen!", "tokens": ["Al\u00b7le", "be\u00b7trof\u00b7fen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Schrecken scheidet Streit und Hoffen!", "tokens": ["Schre\u00b7cken", "schei\u00b7det", "Streit", "und", "Hof\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Keiner geht weiter,", "tokens": ["Kei\u00b7ner", "geht", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Alle beklommen;", "tokens": ["Al\u00b7le", "be\u00b7klom\u00b7men", ";"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wer ist der Streiter,", "tokens": ["Wer", "ist", "der", "Strei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der f\u00fcr uns kommen?", "tokens": ["Der", "f\u00fcr", "uns", "kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Um den wir all' noch weinen,", "tokens": ["Um", "den", "wir", "all'", "noch", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PPER", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er f\u00fchret an die Reinen,", "tokens": ["Er", "f\u00fch\u00b7ret", "an", "die", "Rei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u00dcber den H\u00f6hen", "tokens": ["\u00dc\u00b7ber", "den", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "K\u00f6nnt ihr ihn als Stern schon sehen.", "tokens": ["K\u00f6nnt", "ihr", "ihn", "als", "Stern", "schon", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "KOUS", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Rei\u00dfen im Eise", "tokens": ["Rei\u00b7\u00dfen", "im", "Ei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Drohende Spalten,", "tokens": ["Dro\u00b7hen\u00b7de", "Spal\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Endet die Reise,", "tokens": ["En\u00b7det", "die", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "La\u00dft gewarnet euch halten!", "tokens": ["La\u00dft", "ge\u00b7war\u00b7net", "euch", "hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Es st\u00fcrzt ein Sturm die W\u00e4lder", "tokens": ["Es", "st\u00fcrzt", "ein", "Sturm", "die", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und trennt die blut'gen Felder;", "tokens": ["Und", "trennt", "die", "blut'\u00b7gen", "Fel\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nein ich vergehe,", "tokens": ["Nein", "ich", "ver\u00b7ge\u00b7he", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "\u00dcber'm Sehn vergeht das Sehen.", "tokens": ["\u00dc\u00b7ber'm", "Sehn", "ver\u00b7geht", "das", "Se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Vater von allen,", "tokens": ["Va\u00b7ter", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Was soll ich flehen?", "tokens": ["Was", "soll", "ich", "fle\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Klagen erschallen,", "tokens": ["Kla\u00b7gen", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Schmerzensseufzer ergehen,", "tokens": ["Schmer\u00b7zens\u00b7seuf\u00b7zer", "er\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Woher der Wind mag wehen,", "tokens": ["Wo\u00b7her", "der", "Wind", "mag", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wohin die Fahnen stehen,", "tokens": ["Wo\u00b7hin", "die", "Fah\u00b7nen", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcrchten und Hoffen,", "tokens": ["F\u00fcrch\u00b7ten", "und", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Donnernd steht der Himmel offen.", "tokens": ["Don\u00b7nernd", "steht", "der", "Him\u00b7mel", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "N\u00e4chtliche Feuer,", "tokens": ["N\u00e4cht\u00b7li\u00b7che", "Feu\u00b7er", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Feindeget\u00fcmmel,", "tokens": ["Fein\u00b7de\u00b7ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "St\u00f6ren die Feier,", "tokens": ["St\u00f6\u00b7ren", "die", "Fei\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "St\u00fcrmten gern den Himmel.", "tokens": ["St\u00fcrm\u00b7ten", "gern", "den", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Er l\u00e4\u00dft sich nicht erst\u00fcrmen,", "tokens": ["Er", "l\u00e4\u00dft", "sich", "nicht", "er\u00b7st\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Fahnen stehn auf Th\u00fcrmen.", "tokens": ["Die", "Fah\u00b7nen", "stehn", "auf", "Th\u00fcr\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gott wird sie halten,", "tokens": ["Gott", "wird", "sie", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Wird der Feinde Wuth erkalten.", "tokens": ["Wird", "der", "Fein\u00b7de", "Wuth", "er\u00b7kal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "L\u00f6se in Ruhe", "tokens": ["L\u00f6\u00b7se", "in", "Ru\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Menschlich Gesch\u00e4fte,", "tokens": ["Menschlich", "Ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Winter umthue", "tokens": ["Win\u00b7ter", "umt\u00b7hue"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "All' s\u00fcnd'gende Kr\u00e4fte,", "tokens": ["All'", "s\u00fcn\u00b7d'\u00b7gen\u00b7de", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+----+-", "measure": "dactylic.init"}, "line.5": {"text": "Mit kalter wei\u00dfer Decke,", "tokens": ["Mit", "kal\u00b7ter", "wei\u00b7\u00dfer", "De\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df Ungl\u00fcck sich verstecke,", "tokens": ["Da\u00df", "Un\u00b7gl\u00fcck", "sich", "ver\u00b7ste\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Augen voll Thr\u00e4nen", "tokens": ["Au\u00b7gen", "voll", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "M\u00f6gen sie in Unschuld w\u00e4hnen.", "tokens": ["M\u00f6\u00b7gen", "sie", "in", "Un\u00b7schuld", "w\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Fliehen die Feinde,", "tokens": ["Flie\u00b7hen", "die", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Brennen mit Lachen", "tokens": ["Bren\u00b7nen", "mit", "La\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Tobende Freunde", "tokens": ["To\u00b7ben\u00b7de", "Freun\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Hirtenh\u00e4user zum Wachen;", "tokens": ["Hir\u00b7ten\u00b7h\u00e4u\u00b7ser", "zum", "Wa\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Vom Heerd ist nichts geblieben,", "tokens": ["Vom", "Heerd", "ist", "nichts", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Heerden sind vertrieben.", "tokens": ["Die", "Heer\u00b7den", "sind", "ver\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Lieber Gott! schreiend,", "tokens": ["Lie\u00b7ber", "Gott", "!", "schrei\u00b7end", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJD", "NN", "$.", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Flieht der Hirt dem Himmel dr\u00e4uend.", "tokens": ["Flieht", "der", "Hirt", "dem", "Him\u00b7mel", "dr\u00e4u\u00b7end", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.12": {"line.1": {"text": "L\u00e4mmlein, von allen", "tokens": ["L\u00e4mm\u00b7lein", ",", "von", "al\u00b7len"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "APPR", "PIAT"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Einzig ihm blieben,", "tokens": ["Ein\u00b7zig", "ihm", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "W\u00f6lfe anfallen,", "tokens": ["W\u00f6l\u00b7fe", "an\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Kommend witternd von dr\u00fcben,", "tokens": ["Kom\u00b7mend", "wit\u00b7ternd", "von", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ADV", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Sie sind vom Wind geladen,", "tokens": ["Sie", "sind", "vom", "Wind", "ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Todten zu begraben,", "tokens": ["Die", "Tod\u00b7ten", "zu", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Heldenlied schweiget,", "tokens": ["Hel\u00b7den\u00b7lied", "schwei\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Heulend sie der Wolf umreiget.", "tokens": ["Heu\u00b7lend", "sie", "der", "Wolf", "um\u00b7rei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "ART", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Schon ist vergessen", "tokens": ["Schon", "ist", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Eigenes Leiden,", "tokens": ["Ei\u00b7ge\u00b7nes", "Lei\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Nimmer vermessen", "tokens": ["Nim\u00b7mer", "ver\u00b7mes\u00b7sen"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "St\u00f6r' ich ahndend die Freuden.", "tokens": ["St\u00f6r'", "ich", "ahn\u00b7dend", "die", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Und sollt' es auch geschehen,", "tokens": ["Und", "sollt'", "es", "auch", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich will's voraus nicht sehen,", "tokens": ["Ich", "will's", "vo\u00b7raus", "nicht", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcrchten und Hoffen,", "tokens": ["F\u00fcrch\u00b7ten", "und", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Wie mir steht die Zukunft offen.", "tokens": ["Wie", "mir", "steht", "die", "Zu\u00b7kunft", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Wisset es gl\u00fchet", "tokens": ["Wis\u00b7set", "es", "gl\u00fc\u00b7het"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Heilig im Sitze,", "tokens": ["Hei\u00b7lig", "im", "Sit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "APPRART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "\u00dcber mir ziehet", "tokens": ["\u00dc\u00b7ber", "mir", "zie\u