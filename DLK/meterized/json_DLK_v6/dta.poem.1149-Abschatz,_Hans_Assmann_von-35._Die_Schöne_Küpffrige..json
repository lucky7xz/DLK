{"dta.poem.1149": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "35. Die Sch\u00f6ne K\u00fcpffrige.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Kind/ es fleckt dich nicht die R\u00f6the/ die der Wein ", "tokens": ["Mein", "Kind", "/", "es", "fleckt", "dich", "nicht", "die", "R\u00f6\u00b7the", "/", "die", "der", "Wein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$(", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem lieblichen Crystall der Wangen angestrichen.", "tokens": ["Dem", "lieb\u00b7li\u00b7chen", "Crys\u00b7tall", "der", "Wan\u00b7gen", "an\u00b7ge\u00b7stri\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Di\u00df Blutt zeigt die Natur bey dir noch nicht verblichen/", "tokens": ["Di\u00df", "Blutt", "zeigt", "die", "Na\u00b7tur", "bey", "dir", "noch", "nicht", "ver\u00b7bli\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "ART", "NN", "APPR", "PPER", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und kan das beste Ra\u00df bey Amors Speise seyn.", "tokens": ["Und", "kan", "das", "bes\u00b7te", "Ra\u00df", "bey", "A\u00b7mors", "Spei\u00b7se", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "APPR", "NE", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Ehren Hofe-Farb\u2019 hat dich gekleidet ein.", "tokens": ["Der", "Eh\u00b7ren", "Ho\u00b7fe\u00b7Fa\u00b7rb'", "hat", "dich", "ge\u00b7klei\u00b7det", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "PPER", "VVPP", "PTKVZ", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Offt k\u00f6mmt so ", "tokens": ["Offt", "k\u00f6mmt", "so"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV"], "meter": "+--", "measure": "dactylic.init"}, "line.7": {"text": "Wenn Zynthius der Welt mit gleichem Pracht entwichen.", "tokens": ["Wenn", "Zynt\u00b7hi\u00b7us", "der", "Welt", "mit", "glei\u00b7chem", "Pracht", "ent\u00b7wi\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von keiner Schmincke darffst du borgen fremden Schein.", "tokens": ["Von", "kei\u00b7ner", "Schmin\u00b7cke", "darffst", "du", "bor\u00b7gen", "frem\u00b7den", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hastu es von Geburt/ wenn deine Mutter mich", "tokens": ["Has\u00b7tu", "es", "von", "Ge\u00b7burt", "/", "wenn", "dei\u00b7ne", "Mut\u00b7ter", "mich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN", "$(", "KOUS", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zur selben Zeit genau betracht/ so liesse sich", "tokens": ["Zur", "sel\u00b7ben", "Zeit", "ge\u00b7nau", "be\u00b7tracht", "/", "so", "lies\u00b7se", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "VVPP", "$(", "ADV", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Flamme/ die ich f\u00fchl\u2019/ aus deinen Wangen sp\u00fcren.", "tokens": ["Die", "Flam\u00b7me", "/", "die", "ich", "f\u00fchl'", "/", "aus", "dei\u00b7nen", "Wan\u00b7gen", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$(", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Allein das Widerspiel ereignet sich bey mir/", "tokens": ["Al\u00b7lein", "das", "Wi\u00b7der\u00b7spiel", "er\u00b7eig\u00b7net", "sich", "bey", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PRF", "APPR", "PPER", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Mein bleiches Angesicht erweiset nichts von dir/", "tokens": ["Mein", "blei\u00b7ches", "An\u00b7ge\u00b7sicht", "er\u00b7wei\u00b7set", "nichts", "von", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PIS", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dein unverl\u00f6schlich Bild mu\u00df ich im Hertzen f\u00fchren.", "tokens": ["Dein", "un\u00b7ver\u00b7l\u00f6\u00b7schlich", "Bild", "mu\u00df", "ich", "im", "Hert\u00b7zen", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}