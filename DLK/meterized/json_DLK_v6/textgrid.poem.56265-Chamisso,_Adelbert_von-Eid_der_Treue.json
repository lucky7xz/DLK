{"textgrid.poem.56265": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Eid der Treue", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mi\u00dftrauest, Liebchen, du der fl\u00fccht'gen Stunde,", "tokens": ["Mi\u00df\u00b7trau\u00b7est", ",", "Lieb\u00b7chen", ",", "du", "der", "fl\u00fccht'\u00b7gen", "Stun\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Augenblickes Lust?", "tokens": ["Des", "Au\u00b7gen\u00b7bli\u00b7ckes", "Lust", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bist Brust an Brust du nicht, und Mund an Munde,", "tokens": ["Bist", "Brust", "an", "Brust", "du", "nicht", ",", "und", "Mund", "an", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "NN", "PPER", "PTKNEG", "$,", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Ewigkeit bewu\u00dft?", "tokens": ["Der", "E\u00b7wig\u00b7keit", "be\u00b7wu\u00dft", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich soll nur dir, und ewig dir geh\u00f6ren;", "tokens": ["Ich", "soll", "nur", "dir", ",", "und", "e\u00b7wig", "dir", "ge\u00b7h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "$,", "KON", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du willst darauf ein Pfand:", "tokens": ["Du", "willst", "da\u00b7rauf", "ein", "Pfand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohlan! ich will's mit kr\u00e4ft'gem Eid beschw\u00f6ren,", "tokens": ["Wo\u00b7hlan", "!", "ich", "will's", "mit", "kr\u00e4ft'\u00b7gem", "Eid", "be\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich hebe meine Hand:", "tokens": ["Ich", "he\u00b7be", "mei\u00b7ne", "Hand", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich schw\u00f6r's, elftausend heilige Jungfrauen,", "tokens": ["Ich", "schw\u00f6r's", ",", "elf\u00b7tau\u00b7send", "hei\u00b7li\u00b7ge", "Jung\u00b7frau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bei eurem keuschen Bart;", "tokens": ["Bei", "eu\u00b7rem", "keu\u00b7schen", "Bart", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bei Jakobs Leitersprosse, die zu schauen", "tokens": ["Bei", "Ja\u00b7kobs", "Lei\u00b7ter\u00b7spros\u00b7se", ",", "die", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "$,", "PRELS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Mailand wird bewahrt;", "tokens": ["In", "Mai\u00b7land", "wird", "be\u00b7wahrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich schw\u00f6r es noch, zu mehrerem Gewichte \u2013", "tokens": ["Ich", "schw\u00f6r", "es", "noch", ",", "zu", "meh\u00b7re\u00b7rem", "Ge\u00b7wich\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein unerh\u00f6rter Schwur! \u2013", "tokens": ["Ein", "un\u00b7er\u00b7h\u00f6r\u00b7ter", "Schwur", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Beim Vorwort zu des Kaisers Karl Geschichte,", "tokens": ["Beim", "Vor\u00b7wort", "zu", "des", "Kai\u00b7sers", "Karl", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und bei des Windes Spur;", "tokens": ["Und", "bei", "des", "Win\u00b7des", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Beim Schnee, der auf dem Libanon gefallen", "tokens": ["Beim", "Schnee", ",", "der", "auf", "dem", "Li\u00b7ba\u00b7non", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im letzt vergangnen Jahr;", "tokens": ["Im", "letzt", "ver\u00b7gang\u00b7nen", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bei Nihil, Nemo, und dem andern allen,", "tokens": ["Bei", "Ni\u00b7hil", ",", "Ne\u00b7mo", ",", "und", "dem", "an\u00b7dern", "al\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NE", "$,", "KON", "ART", "ADJA", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was nie sein wird noch war.", "tokens": ["Was", "nie", "sein", "wird", "noch", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAINF", "VAFIN", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und falls ich dennoch jemals untreu w\u00fcrde,", "tokens": ["Und", "falls", "ich", "den\u00b7noch", "je\u00b7mals", "un\u00b7treu", "w\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verg\u00e4\u00dfe jemals dein,", "tokens": ["Ver\u00b7g\u00e4\u00b7\u00dfe", "je\u00b7mals", "dein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So soll mein Eid verbleiben ohne W\u00fcrde,", "tokens": ["So", "soll", "mein", "Eid", "ver\u00b7blei\u00b7ben", "oh\u00b7ne", "W\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ganz unb\u00fcndig sein.", "tokens": ["Und", "ganz", "un\u00b7b\u00fcn\u00b7dig", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}