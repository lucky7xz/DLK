{"textgrid.poem.55062": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Generalbeichte", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lasset heut im edeln Kreis", "tokens": ["Las\u00b7set", "heut", "im", "e\u00b7deln", "Kreis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Warnung gelten!", "tokens": ["Mei\u00b7ne", "War\u00b7nung", "gel\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nehmt die ernste Stimmung wahr,", "tokens": ["Nehmt", "die", "erns\u00b7te", "Stim\u00b7mung", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn sie kommt so selten.", "tokens": ["Denn", "sie", "kommt", "so", "sel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Manches habt ihr vorgenommen,", "tokens": ["Man\u00b7ches", "habt", "ihr", "vor\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Manches ist euch schlecht bekommen,", "tokens": ["Man\u00b7ches", "ist", "euch", "schlecht", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ich mu\u00df euch schelten.", "tokens": ["Und", "ich", "mu\u00df", "euch", "schel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "VVFIN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Reue soll man doch einmal", "tokens": ["Reu\u00b7e", "soll", "man", "doch", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PIS", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Welt empfinden!", "tokens": ["In", "der", "Welt", "emp\u00b7fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "So bekennt, vertraut und fromm,", "tokens": ["So", "be\u00b7kennt", ",", "ver\u00b7traut", "und", "fromm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eure gr\u00f6\u00dften S\u00fcnden!", "tokens": ["Eu\u00b7re", "gr\u00f6\u00df\u00b7ten", "S\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Aus des Irrtums falschen Weiten", "tokens": ["Aus", "des", "Irr\u00b7tums", "fal\u00b7schen", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sammelt euch und sucht beizeiten", "tokens": ["Sam\u00b7melt", "euch", "und", "sucht", "bei\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Euch zurechtzufinden.", "tokens": ["Euch", "zu\u00b7recht\u00b7zu\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ja, wir haben, sei's bekannt,", "tokens": ["Ja", ",", "wir", "ha\u00b7ben", ",", "sei's", "be\u00b7kannt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "$,", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wachend oft getr\u00e4umet,", "tokens": ["Wa\u00b7chend", "oft", "ge\u00b7tr\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nicht geleert das frische Glas,", "tokens": ["Nicht", "ge\u00b7leert", "das", "fri\u00b7sche", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der Wein gesch\u00e4umet;", "tokens": ["Wenn", "der", "Wein", "ge\u00b7sch\u00e4u\u00b7met", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Manche rasche Sch\u00e4ferstunde,", "tokens": ["Man\u00b7che", "ra\u00b7sche", "Sch\u00e4\u00b7fer\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fl\u00fccht'gen Ku\u00df vom lieben Munde", "tokens": ["Fl\u00fccht'\u00b7gen", "Ku\u00df", "vom", "lie\u00b7ben", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Haben wir vers\u00e4umet.", "tokens": ["Ha\u00b7ben", "wir", "ver\u00b7s\u00e4u\u00b7met", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Still und maulfaul sa\u00dfen wir,", "tokens": ["Still", "und", "maul\u00b7faul", "sa\u00b7\u00dfen", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn Philister schw\u00e4tzten,", "tokens": ["Wenn", "Phi\u00b7lis\u00b7ter", "schw\u00e4tz\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "\u00dcber g\u00f6ttlichen Gesang", "tokens": ["\u00dc\u00b7ber", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr Geklatsche sch\u00e4tzten,", "tokens": ["Ihr", "Ge\u00b7klat\u00b7sche", "sch\u00e4tz\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wegen gl\u00fccklicher Momente,", "tokens": ["We\u00b7gen", "gl\u00fcck\u00b7li\u00b7cher", "Mo\u00b7men\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deren man sich r\u00fchmen k\u00f6nnte,", "tokens": ["De\u00b7ren", "man", "sich", "r\u00fch\u00b7men", "k\u00f6nn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "PRF", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Uns zur Rede setzten.", "tokens": ["Uns", "zur", "Re\u00b7de", "setz\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Willst du Absolution", "tokens": ["Willst", "du", "Ab\u00b7so\u00b7lu\u00b7ti\u00b7on"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinen Treuen geben,", "tokens": ["Dei\u00b7nen", "Treu\u00b7en", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wollen wir nach deinem Wink", "tokens": ["Wol\u00b7len", "wir", "nach", "dei\u00b7nem", "Wink"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unabl\u00e4\u00dflich streben,", "tokens": ["Un\u00b7ab\u00b7l\u00e4\u00df\u00b7lich", "stre\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Uns vom Halben zu entw\u00f6hnen", "tokens": ["Uns", "vom", "Hal\u00b7ben", "zu", "ent\u00b7w\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und im Ganzen, Guten, Sch\u00f6nen", "tokens": ["Und", "im", "Gan\u00b7zen", ",", "Gu\u00b7ten", ",", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "APPRART", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Resolut zu leben,", "tokens": ["Re\u00b7so\u00b7lut", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Den Philistern allzumal", "tokens": ["Den", "Phi\u00b7lis\u00b7tern", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohlgemut zu schnippen,", "tokens": ["Wohl\u00b7ge\u00b7mut", "zu", "schnip\u00b7pen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jenen Perlenschaum des Weins", "tokens": ["Je\u00b7nen", "Per\u00b7len\u00b7schaum", "des", "Weins"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht nur flach zu nippen,", "tokens": ["Nicht", "nur", "flach", "zu", "nip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Nicht zu liebeln leis mit Augen,", "tokens": ["Nicht", "zu", "lie\u00b7beln", "leis", "mit", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sondern fest uns anzusaugen", "tokens": ["Son\u00b7dern", "fest", "uns", "an\u00b7zu\u00b7sau\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "An geliebte Lippen.", "tokens": ["An", "ge\u00b7lieb\u00b7te", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}