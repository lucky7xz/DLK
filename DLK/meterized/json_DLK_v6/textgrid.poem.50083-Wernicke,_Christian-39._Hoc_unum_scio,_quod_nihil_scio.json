{"textgrid.poem.50083": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "39. Hoc unum scio, quod nihil scio", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du denckst dass jederman aus deinen eitlen Schl\u00fcssen,", "tokens": ["Du", "denckst", "dass", "je\u00b7der\u00b7man", "aus", "dei\u00b7nen", "eit\u00b7len", "Schl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PIS", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und falscher Wissenschaft sehr grosse Klugheit schleusst;", "tokens": ["Und", "fal\u00b7scher", "Wis\u00b7sen\u00b7schaft", "sehr", "gros\u00b7se", "Klug\u00b7heit", "schleusst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was n\u00fctzt es, ", "tokens": ["Was", "n\u00fctzt", "es", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}}}}}