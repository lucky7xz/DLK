{"dta.poem.21624": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4rchenglaube.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Lach\u2019 nicht des Kindes M\u00e4rchenglauben,", "tokens": ["Lach'", "nicht", "des", "Kin\u00b7des", "M\u00e4r\u00b7chen\u00b7glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was ist\u2019s denn, was dein Geist erfand?", "tokens": ["Was", "ist's", "denn", ",", "was", "dein", "Geist", "er\u00b7fand", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was sind die Bibeln, die Systeme", "tokens": ["Was", "sind", "die", "Bi\u00b7beln", ",", "die", "Sys\u00b7te\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Denn anderes als M\u00e4rchentand?", "tokens": ["Denn", "an\u00b7de\u00b7res", "als", "M\u00e4r\u00b7chen\u00b7tand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Jeder dichtet seinen Himmel", "tokens": ["Ein", "Je\u00b7der", "dich\u00b7tet", "sei\u00b7nen", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie\u2019s ihm behagt in\u2019s Blau hinein,", "tokens": ["Wie's", "ihm", "be\u00b7hagt", "in's", "Blau", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcber einem M\u00e4rchen br\u00fctend", "tokens": ["Und", "\u00fc\u00b7ber", "ei\u00b7nem", "M\u00e4r\u00b7chen", "br\u00fc\u00b7tend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schl\u00e4ft endlich er erm\u00fcdet ein.", "tokens": ["Schl\u00e4ft", "end\u00b7lich", "er", "er\u00b7m\u00fc\u00b7det", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}