{"textgrid.poem.37459": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Vertraut", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie liegt die Welt so frisch und tauig", "tokens": ["Wie", "liegt", "die", "Welt", "so", "frisch", "und", "tau\u00b7ig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor mir im Morgensonnenschein.", "tokens": ["Vor", "mir", "im", "Mor\u00b7gen\u00b7son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entz\u00fcckt vom hohen H\u00fcgel schau ich", "tokens": ["Ent\u00b7z\u00fcckt", "vom", "ho\u00b7hen", "H\u00fc\u00b7gel", "schau", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ins fr\u00fchlingsgr\u00fcne Tal hinein.", "tokens": ["Ins", "fr\u00fch\u00b7lings\u00b7gr\u00fc\u00b7ne", "Tal", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit allen Kreaturen bin ich", "tokens": ["Mit", "al\u00b7len", "Kre\u00b7a\u00b7tu\u00b7ren", "bin", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In sch\u00f6nster Seelenharmonie.", "tokens": ["In", "sch\u00f6ns\u00b7ter", "See\u00b7len\u00b7har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sind verwandt, ich f\u00fchl es innig,", "tokens": ["Wir", "sind", "ver\u00b7wandt", ",", "ich", "f\u00fchl", "es", "in\u00b7nig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und eben darum lieb ich sie.", "tokens": ["Und", "e\u00b7ben", "da\u00b7rum", "lieb", "ich", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "ADJD", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wird auch mal der Himmel grauer;", "tokens": ["Und", "wird", "auch", "mal", "der", "Him\u00b7mel", "grau\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer voll Vertraun die Welt besieht,", "tokens": ["Wer", "voll", "Ver\u00b7traun", "die", "Welt", "be\u00b7sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den freut es, wenn ein Regenschauer", "tokens": ["Den", "freut", "es", ",", "wenn", "ein", "Re\u00b7gen\u00b7schau\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Sturm und Blitz vor\u00fcberzieht.", "tokens": ["Mit", "Sturm", "und", "Blitz", "vor\u00b7\u00fc\u00b7berz\u00b7ieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}