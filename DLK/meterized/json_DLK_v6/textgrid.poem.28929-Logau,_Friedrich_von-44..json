{"textgrid.poem.28929": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ey, wie schad ists um die Zeit, die mit Reimen ich verspiele!", "tokens": ["Ey", ",", "wie", "schad", "ists", "um", "die", "Zeit", ",", "die", "mit", "Rei\u00b7men", "ich", "ver\u00b7spie\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "VAFIN", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "+-+---+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "\u00dcbler w\u00fcrde reimen sichs, wann mit nichts thun sie verfiele.", "tokens": ["\u00dcb\u00b7ler", "w\u00fcr\u00b7de", "rei\u00b7men", "sichs", ",", "wann", "mit", "nichts", "thun", "sie", "ver\u00b7fie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVFIN", "PIS", "$,", "PWAV", "APPR", "PIS", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Eine Ruh f\u00fcr Leib und Sinn ist gelassen iedem zu;", "tokens": ["Ei\u00b7ne", "Ruh", "f\u00fcr", "Leib", "und", "Sinn", "ist", "ge\u00b7las\u00b7sen", "ie\u00b7dem", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VAFIN", "VVPP", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Ieder ruhe wie er wil; ich beruh in dieser Ruh.", "tokens": ["Ie\u00b7der", "ru\u00b7he", "wie", "er", "wil", ";", "ich", "be\u00b7ruh", "in", "die\u00b7ser", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KOKOM", "PPER", "VMFIN", "$.", "PPER", "ADJD", "APPR", "PDAT", "NN", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}}}}}