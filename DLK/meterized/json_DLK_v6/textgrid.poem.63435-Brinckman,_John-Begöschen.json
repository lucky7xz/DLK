{"textgrid.poem.63435": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Beg\u00f6schen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wist mi man beg\u00f6schen", "tokens": ["Du", "wist", "mi", "man", "be\u00b7g\u00f6\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "un meenst dat doch nich so;", "tokens": ["un", "me\u00b7enst", "dat", "doch", "nich", "so", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADV", "PTKNEG", "ADV", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "as Immen bi de R\u00f6schen", "tokens": ["as", "Im\u00b7men", "bi", "de", "R\u00f6\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "stellst du un singst dorto.", "tokens": ["stellst", "du", "un", "singst", "dor\u00b7to", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "FM", "FM", "FM", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Min blanke Kron de lat mi", "tokens": ["Min", "blan\u00b7ke", "Kron", "de", "lat", "mi"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NE", "NE", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "un grip nich nah min'n Kranz", "tokens": ["un", "grip", "nich", "nah", "min'n", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "PTKNEG", "ADJD", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "du s\u00f6chst man f\u00f6r de Strat mi,", "tokens": ["du", "s\u00f6chst", "man", "f\u00f6r", "de", "Strat", "mi", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "NE", "NE", "NE", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "nich f\u00f6r din Hus mi, Hans!", "tokens": ["nich", "f\u00f6r", "din", "Hus", "mi", ",", "Hans", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du gierst man nah din L\u00fcsten", "tokens": ["Du", "gierst", "man", "nah", "din", "L\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "un hest mi gor nich leef;", "tokens": ["un", "hest", "mi", "gor", "nich", "leef", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "PTKNEG", "VVFIN", "$."], "meter": "+-----", "measure": "dactylic.init"}, "line.3": {"text": "du s\u00f6chst mi man in D\u00fcstern,", "tokens": ["du", "s\u00f6chst", "mi", "man", "in", "D\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "un dat don all de Deew.", "tokens": ["un", "dat", "don", "all", "de", "Deew", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Din Hart, din Seel verspreckst du", "tokens": ["Din", "Hart", ",", "din", "Seel", "ver\u00b7spreckst", "du"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "un snackst mi gor to sch\u00f6n \u2013", "tokens": ["un", "snackst", "mi", "gor", "to", "sch\u00f6n", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "ADJD", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "man mit din Ogen steckst du", "tokens": ["man", "mit", "din", "O\u00b7gen", "steckst", "du"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "as mit'n Metz nah een'n.", "tokens": ["as", "mit'n", "Metz", "nah", "e\u00b7en'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Du wist mi man beg\u00f6schen", "tokens": ["Du", "wist", "mi", "man", "be\u00b7g\u00f6\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "un meenst dat doch nich so;", "tokens": ["un", "me\u00b7enst", "dat", "doch", "nich", "so", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADV", "PTKNEG", "ADV", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "as Immen bi de R\u00f6schen", "tokens": ["as", "Im\u00b7men", "bi", "de", "R\u00f6\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "stellst du un singst dorto.", "tokens": ["stellst", "du", "un", "singst", "dor\u0