{"textgrid.poem.26284": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Es hockte im Morgen der Hirte am Bach", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hockte im Morgen der Hirte am Bach,", "tokens": ["Es", "hock\u00b7te", "im", "Mor\u00b7gen", "der", "Hir\u00b7te", "am", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Ziegen gingen den Kr\u00e4utern nach,", "tokens": ["Die", "Zie\u00b7gen", "gin\u00b7gen", "den", "Kr\u00e4u\u00b7tern", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er hielt ins Wasser die Fingerspitzen", "tokens": ["Er", "hielt", "ins", "Was\u00b7ser", "die", "Fin\u00b7ger\u00b7spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und lie\u00df sich von kreiselnder Welle bespritzen.", "tokens": ["Und", "lie\u00df", "sich", "von", "krei\u00b7seln\u00b7der", "Wel\u00b7le", "be\u00b7sprit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Es grasten am Uferrain Stier und Kuh,", "tokens": ["Es", "gras\u00b7ten", "am", "Uf\u00b7er\u00b7rain", "Stier", "und", "Kuh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Holzglocken pochten leis ab und zu.", "tokens": ["Die", "Holz\u00b7glo\u00b7cken", "poch\u00b7ten", "leis", "ab", "und", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Das Bachwasser rauschte frisch ohne Ermatten", "tokens": ["Das", "Bach\u00b7was\u00b7ser", "rauschte", "frisch", "oh\u00b7ne", "Er\u00b7mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und rannte durch blaue und gr\u00fcne Schatten.", "tokens": ["Und", "rann\u00b7te", "durch", "blau\u00b7e", "und", "gr\u00fc\u00b7ne", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Es sprach zum Herzen des Grases Duft,", "tokens": ["Es", "sprach", "zum", "Her\u00b7zen", "des", "Gra\u00b7ses", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Wolken T\u00fcrme, gebaut aus Luft.", "tokens": ["Der", "Wol\u00b7ken", "T\u00fcr\u00b7me", ",", "ge\u00b7baut", "aus", "Luft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "VVPP", "APPR", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wunschlos betrat ich die schwindelnden Gassen, \u2013", "tokens": ["Und", "wun\u00b7schlos", "be\u00b7trat", "ich", "die", "schwin\u00b7deln\u00b7den", "Gas\u00b7sen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Einen Augenblick ward ich bei Gott eingelassen.", "tokens": ["Ei\u00b7nen", "Au\u00b7gen\u00b7blick", "ward", "ich", "bei", "Gott", "ein\u00b7ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}