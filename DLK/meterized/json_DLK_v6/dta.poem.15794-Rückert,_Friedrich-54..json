{"dta.poem.15794": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Pfui dem Geschlechte, dem der Zorn ins Angesicht", "tokens": ["Pfui", "dem", "Ge\u00b7schlech\u00b7te", ",", "dem", "der", "Zorn", "ins", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "ART", "NN", "APPRART", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Nicht steigt, wenn kleiner Sinn Hohn gro\u00dfen Todten spricht.", "tokens": ["Nicht", "steigt", ",", "wenn", "klei\u00b7ner", "Sinn", "Hohn", "gro\u00b7\u00dfen", "Tod\u00b7ten", "spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "KOUS", "ADJA", "NN", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "An Manen glaubt ihr nicht, sonst w\u00fcrden sie euch mahnen;", "tokens": ["An", "Ma\u00b7nen", "glaubt", "ihr", "nicht", ",", "sonst", "w\u00fcr\u00b7den", "sie", "euch", "mah\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Ahnen ehrt ihr nicht, sonst w\u00fcrdet ihr dies ahnen.", "tokens": ["Und", "Ah\u00b7nen", "ehrt", "ihr", "nicht", ",", "sonst", "w\u00fcr\u00b7det", "ihr", "dies", "ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VAFIN", "PPER", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Geist, der unter euch viel Geister hat gezeugt,", "tokens": ["Der", "Geist", ",", "der", "un\u00b7ter", "euch", "viel", "Geis\u00b7ter", "hat", "ge\u00b7zeugt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Geist, der euern Geist laut vor der Welt bezeugt;", "tokens": ["Der", "Geist", ",", "der", "eu\u00b7ern", "Geist", "laut", "vor", "der", "Welt", "be\u00b7zeugt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Von jedem Hudler la\u00dft ihr dessen Namen hudeln,", "tokens": ["Von", "je\u00b7dem", "Hud\u00b7ler", "la\u00dft", "ihr", "des\u00b7sen", "Na\u00b7men", "hu\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "PDS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von jedem Sudler frech sein Ehrenmaal besudeln.", "tokens": ["Von", "je\u00b7dem", "Sud\u00b7ler", "frech", "sein", "Eh\u00b7ren\u00b7maal", "be\u00b7su\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Den Namen, den ich nie ohn' Ehrfurcht nenne, G\u00f6the,", "tokens": ["Den", "Na\u00b7men", ",", "den", "ich", "nie", "ohn'", "Ehr\u00b7furcht", "nen\u00b7ne", ",", "G\u00f6\u00b7the", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,", "NE", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Beschmitzt, und Niemand wehrts, mit edlem Gift die Kr\u00f6te,", "tokens": ["Be\u00b7schmitzt", ",", "und", "Nie\u00b7mand", "wehrts", ",", "mit", "ed\u00b7lem", "Gift", "die", "Kr\u00f6\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "PIS", "ADV", "$,", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die sich noch r\u00fchmt, weil sie den Lebenden beschmitzt", "tokens": ["Die", "sich", "noch", "r\u00fchmt", ",", "weil", "sie", "den", "Le\u00b7ben\u00b7den", "be\u00b7schmitzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schon habe, d\u00fcrfe sie's auch thun am Todten itzt.", "tokens": ["Schon", "ha\u00b7be", ",", "d\u00fcr\u00b7fe", "sie's", "auch", "thun", "am", "Tod\u00b7ten", "itzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "VMFIN", "PIS", "ADV", "VVINF", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}