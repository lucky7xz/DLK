{"textgrid.poem.46794": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[es war eine Th\u00fcr, kein Schl\u00fcssel dran]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war eine Th\u00fcr, kein Schl\u00fcssel dran,", "tokens": ["Es", "war", "ei\u00b7ne", "Th\u00fcr", ",", "kein", "Schl\u00fcs\u00b7sel", "dran", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Th\u00fcr ward niemals aufgethan,", "tokens": ["Die", "Th\u00fcr", "ward", "nie\u00b7mals", "auf\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir brauchten sie gar nicht aufzuthun,", "tokens": ["Wir", "brauch\u00b7ten", "sie", "gar", "nicht", "auf\u00b7zu\u00b7thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir wu\u00dften nicht, wozu sie war.", "tokens": ["Wir", "wu\u00df\u00b7ten", "nicht", ",", "wo\u00b7zu", "sie", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir wissen's nun,", "tokens": ["Wir", "wis\u00b7sen's", "nun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Es ward uns klar.", "tokens": ["Es", "ward", "uns", "klar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Vom Schlosser ward sie aufgeschlagen,", "tokens": ["Vom", "Schlos\u00b7ser", "ward", "sie", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Um ein Todtes hinauszutragen;", "tokens": ["Um", "ein", "Tod\u00b7tes", "hin\u00b7aus\u00b7zu\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Nicht merkten's, die krank daneben lagen.", "tokens": ["Nicht", "merk\u00b7ten's", ",", "die", "krank", "da\u00b7ne\u00b7ben", "la\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "$,", "PRELS", "ADJD", "PAV", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Nun, eh' man sie wieder verschlo\u00df, o Graus,", "tokens": ["Nun", ",", "eh'", "man", "sie", "wie\u00b7der", "ver\u00b7schlo\u00df", ",", "o", "Graus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$,", "FM", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Tr\u00e4gt man das zweite Todte hinaus;", "tokens": ["Tr\u00e4gt", "man", "das", "zwei\u00b7te", "Tod\u00b7te", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Das ist die Ungl\u00fccksth\u00fcr im Haus.", "tokens": ["Das", "ist", "die", "Un\u00b7gl\u00fcckst\u00b7h\u00fcr", "im", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Nun sei sie mit Gott geschlossen", "tokens": ["Nun", "sei", "sie", "mit", "Gott", "ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Den \u00fcbrigen Hausgenossen,", "tokens": ["Den", "\u00fcb\u00b7ri\u00b7gen", "Haus\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Ihr Schl\u00fcssel aus Erz gegossen,", "tokens": ["Ihr", "Schl\u00fcs\u00b7sel", "aus", "Erz", "ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Sei abgedreht und versenket,", "tokens": ["Sei", "ab\u00b7ge\u00b7dreht", "und", "ver\u00b7sen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Wo niemand sein gedenket!", "tokens": ["Wo", "nie\u00b7mand", "sein", "ge\u00b7den\u00b7ket", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}