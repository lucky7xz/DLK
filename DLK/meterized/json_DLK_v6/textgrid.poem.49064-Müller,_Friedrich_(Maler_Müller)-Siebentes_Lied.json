easure": "amphibrach.tri.plus"}, "line.22": {"text": "Verzweiflung presst sein Herz: o, ruft er aus, der bauet", "tokens": ["Ver\u00b7zwei\u00b7flung", "presst", "sein", "Herz", ":", "o", ",", "ruft", "er", "aus", ",", "der", "bau\u00b7et"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$.", "FM", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Im Fluch auf tr\u00fcg'rischen Sand, der in der Liebe warm", "tokens": ["Im", "Fluch", "auf", "tr\u00fcg'\u00b7ri\u00b7schen", "Sand", ",", "der", "in", "der", "Lie\u00b7be", "warm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Sein Herz dem Schimmer leichter Hoffnung anvertrauet.", "tokens": ["Sein", "Herz", "dem", "Schim\u00b7mer", "leich\u00b7ter", "Hoff\u00b7nung", "an\u00b7ver\u00b7trau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "O m\u00f6cht' verl\u00f6schen v\u00f6llig aus meinem Ged\u00e4chtni\u00df dahin,", "tokens": ["O", "m\u00f6cht'", "ver\u00b7l\u00f6\u00b7schen", "v\u00f6l\u00b7lig", "aus", "mei\u00b7nem", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "VVINF", "ADJD", "APPR", "PPOSAT", "NN", "PAV", "$,"], "meter": "+--+-+--+--+--+", "measure": "iambic.hexa.invert"}, "line.26": {"text": "M\u00f6cht' aus dem Zeitenlaufe auf immerdar verschwinden", "toke