{"textgrid.poem.49064": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "Siebentes Lied", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leise eilte Luitberta aus ihrer Kammer hervor,", "tokens": ["Lei\u00b7se", "eil\u00b7te", "Luit\u00b7ber\u00b7ta", "aus", "ih\u00b7rer", "Kam\u00b7mer", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "Um die Stunde da sie versprochen, unter den Linden,", "tokens": ["Um", "die", "Stun\u00b7de", "da", "sie", "ver\u00b7spro\u00b7chen", ",", "un\u00b7ter", "den", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "KOUS", "PPER", "VVINF", "$,", "APPR", "ART", "NE", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Bey dem Brunnen mit ihrem Gemahle sich einzufinden;", "tokens": ["Bey", "dem", "Brun\u00b7nen", "mit", "ih\u00b7rem", "Ge\u00b7mah\u00b7le", "sich", "ein\u00b7zu\u00b7fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "PRF", "VVIZU", "$."], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Doch vermochte sie nicht der Amme wachsames Ohr", "tokens": ["Doch", "ver\u00b7moch\u00b7te", "sie", "nicht", "der", "Am\u00b7me", "wach\u00b7sa\u00b7mes", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "ADJA", "NN"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Zu hintergehen.", "tokens": ["Zu", "hin\u00b7ter\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ihr Auge hatte bis dahin dem Schlummer", "tokens": ["Ihr", "Au\u00b7ge", "hat\u00b7te", "bis", "da\u00b7hin", "dem", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nicht zu besiegeln erlaubt der herbe dr\u00fcckende Kummer.", "tokens": ["Nicht", "zu", "be\u00b7sie\u00b7geln", "er\u00b7laubt", "der", "her\u00b7be", "dr\u00fc\u00b7cken\u00b7de", "Kum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Da schleichen sie jezt h\u00f6rt, springet in Eil' sie voran,", "tokens": ["Da", "schlei\u00b7chen", "sie", "jezt", "h\u00f6rt", ",", "sprin\u00b7get", "in", "Eil'", "sie", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "APPR", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Fassend das Fr\u00e4ulein gar \u00e4ngstlich am Arme:", "tokens": ["Fas\u00b7send", "das", "Fr\u00e4u\u00b7lein", "gar", "\u00e4ngst\u00b7lich", "am", "Ar\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.10": {"text": "Was beginnst du, o Trauteste, sag', welcher Wahn", "tokens": ["Was", "be\u00b7ginnst", "du", ",", "o", "Trau\u00b7tes\u00b7te", ",", "sag'", ",", "wel\u00b7cher", "Wahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "VVFIN", "$,", "PWAT", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Treibet dich fort, eine neue Unheils-Bahn", "tokens": ["Trei\u00b7bet", "dich", "fort", ",", "ei\u00b7ne", "neu\u00b7e", "Un\u00b7heils\u00b7Bahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "Zu beschreiten.", "tokens": ["Zu", "be\u00b7schrei\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Um diese Stunde jezt, zu gr\u00f6\u00dferem Harme?", "tokens": ["Um", "die\u00b7se", "Stun\u00b7de", "jezt", ",", "zu", "gr\u00f6\u00b7\u00dfe\u00b7rem", "Har\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "ADV", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Halte ruhig dich hier bis der Morgen anbricht,", "tokens": ["Hal\u00b7te", "ru\u00b7hig", "dich", "hier", "bis", "der", "Mor\u00b7gen", "an\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.15": {"text": "Dann m\u00f6gen die Freunde uns weiteren Rath ertheilen.", "tokens": ["Dann", "m\u00f6\u00b7gen", "die", "Freun\u00b7de", "uns", "wei\u00b7te\u00b7ren", "Rath", "er\u00b7thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.16": {"text": "O lass' mich! seufzet die Holde, halte mich nicht!", "tokens": ["O", "lass'", "mich", "!", "seuf\u00b7zet", "die", "Hol\u00b7de", ",", "hal\u00b7te", "mich", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Zu dem ungl\u00fccklichen Gemahl mu\u00df ich nun eilen.", "tokens": ["Zu", "dem", "un\u00b7gl\u00fcck\u00b7li\u00b7chen", "Ge\u00b7mahl", "mu\u00df", "ich", "nun", "ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VMFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ihn sehen mu\u00df ich, ach! den Geliebtesten! noch einmal sehn,", "tokens": ["Ihn", "se\u00b7hen", "mu\u00df", "ich", ",", "ach", "!", "den", "Ge\u00b7lieb\u00b7tes\u00b7ten", "!", "noch", "ein\u00b7mal", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "PPER", "$,", "ITJ", "$.", "ART", "NN", "$.", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.19": {"text": "Bevor er auf immer vielleicht geschieden \u2013 ich bitte,", "tokens": ["Be\u00b7vor", "er", "auf", "im\u00b7mer", "viel\u00b7leicht", "ge\u00b7schie\u00b7den", "\u2013", "ich", "bit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "ADV", "VVPP", "$(", "PPER", "ADV", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Lass' mich! hier hilft kein Ausschub, hilft kein Fleh'n,", "tokens": ["Lass'", "mich", "!", "hier", "hilft", "kein", "Aus\u00b7schub", ",", "hilft", "kein", "Fleh'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "ADV", "VVFIN", "PIAT", "NN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Voll Jammer mu\u00df ich verfolgen meine Schritte;", "tokens": ["Voll", "Jam\u00b7mer", "mu\u00df", "ich", "ver\u00b7fol\u00b7gen", "mei\u00b7ne", "Schrit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Ihn gr\u00fc\u00dfen mu\u00df ich, sollt' ich auch Fluth und Flammen durchgeh'n,", "tokens": ["Ihn", "gr\u00fc\u00b7\u00dfen", "mu\u00df", "ich", ",", "sollt'", "ich", "auch", "Fluth", "und", "Flam\u00b7men", "durch\u00b7geh'n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "PPER", "$,", "VMFIN", "PPER", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Und m\u00fcsste beschreiten ich des Todtenthales Mitte.", "tokens": ["Und", "m\u00fcss\u00b7te", "be\u00b7schrei\u00b7ten", "ich", "des", "Tod\u00b7tent\u00b7ha\u00b7les", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Halt' fest mich nicht l\u00e4nger! \u2013 Mit Gewalt rei\u00dft sie sich los,", "tokens": ["Halt'", "fest", "mich", "nicht", "l\u00e4n\u00b7ger", "!", "\u2013", "Mit", "Ge\u00b7walt", "rei\u00dft", "sie", "sich", "los", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "ADJD", "$.", "$(", "APPR", "NN", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.25": {"text": "Und eilet davon;", "tokens": ["Und", "ei\u00b7let", "da\u00b7von", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "Erschrocken sinkt die Amme auf die Marmorstiege zur\u00fccke;", "tokens": ["Er\u00b7schro\u00b7cken", "sinkt", "die", "Am\u00b7me", "auf", "die", "Mar\u00b7mor\u00b7stie\u00b7ge", "zu\u00b7r\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.27": {"text": "Schluchzend sizt sie, die Arme traurig gest\u00fcz auf den Schoo\u00df", "tokens": ["Schluch\u00b7zend", "sizt", "sie", ",", "die", "Ar\u00b7me", "trau\u00b7rig", "ge\u00b7st\u00fcz", "auf", "den", "Schoo\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "ART", "NN", "ADJD", "ADJD", "APPR", "ART", "NN"], "meter": "+-+--+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.28": {"text": "Laut beseufzend,", "tokens": ["Laut", "be\u00b7seuf\u00b7zend", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.29": {"text": "Des Schicksal grausame an ihr ver\u00fcbte T\u00fccke:", "tokens": ["Des", "Schick\u00b7sal", "grau\u00b7sa\u00b7me", "an", "ihr", "ver\u00b7\u00fcb\u00b7te", "T\u00fc\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Bis gestern hielt th\u00f6richt ich beneidenswerth mein Loos,", "tokens": ["Bis", "ge\u00b7stern", "hielt", "th\u00f6\u00b7richt", "ich", "be\u00b7nei\u00b7dens\u00b7werth", "mein", "Loos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.31": {"text": "Gesichert von dem Gl\u00fccke vor k\u00fcnftigem Harme und Wehen;", "tokens": ["Ge\u00b7si\u00b7chert", "von", "dem", "Gl\u00fc\u00b7cke", "vor", "k\u00fcnf\u00b7ti\u00b7gem", "Har\u00b7me", "und", "We\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.32": {"text": "O blinder Wahn! gest\u00fcrzt f\u00fchl' ich heute mich schon", "tokens": ["O", "blin\u00b7der", "Wahn", "!", "ge\u00b7st\u00fcrzt", "f\u00fchl'", "ich", "heu\u00b7te", "mich", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "VVPP", "VVFIN", "PPER", "ADV", "PPER", "ADV"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "In den Abgrund des Elends mit Schrecken und Hohn,", "tokens": ["In", "den", "Ab\u00b7grund", "des", "E\u00b7lends", "mit", "Schre\u00b7cken", "und", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.34": {"text": "Ach, Himmel! wie wird es mir nun noch ergehen? \u2013", "tokens": ["Ach", ",", "Him\u00b7mel", "!", "wie", "wird", "es", "mir", "nun", "noch", "er\u00b7ge\u00b7hen", "?", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "NN", "$.", "PWAV", "VAFIN", "PPER", "PPER", "ADV", "ADV", "VVINF", "$.", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Indem sie noch jammert so f\u00fcr sich allein,", "tokens": ["In\u00b7dem", "sie", "noch", "jam\u00b7mert", "so", "f\u00fcr", "sich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ADV", "APPR", "PRF", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.36": {"text": "Erblicket sie von Lichter- und Fackel-Schein", "tokens": ["Er\u00b7bli\u00b7cket", "sie", "von", "Lich\u00b7ter", "und", "Fa\u00b7ckel\u00b7Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "TRUNC", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Erleuchtet des Schlosses hohe G\u00e4nge;", "tokens": ["Er\u00b7leuch\u00b7tet", "des", "Schlos\u00b7ses", "ho\u00b7he", "G\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.38": {"text": "Der K\u00f6nig erscheint", "tokens": ["Der", "K\u00f6\u00b7nig", "er\u00b7scheint"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.39": {"text": "Gepanzert und hinter ihm beweget sich einher", "tokens": ["Ge\u00b7pan\u00b7zert", "und", "hin\u00b7ter", "ihm", "be\u00b7we\u00b7get", "sich", "ein\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "APPR", "PPER", "VVFIN", "PRF", "PTKVZ"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Bewaffnet gleichfalls mit Schilde und Speer", "tokens": ["Be\u00b7waff\u00b7net", "gleich\u00b7falls", "mit", "Schil\u00b7de", "und", "Speer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.41": {"text": "Des Reiches erste M\u00e4chte, in furchtbar'm Gedr\u00e4nge.", "tokens": ["Des", "Rei\u00b7ches", "ers\u00b7te", "M\u00e4ch\u00b7te", ",", "in", "furcht\u00b7ba\u00b7r'm", "Ge\u00b7dr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.42": {"text": "Durch den Bericht der Zofe hatte Geltar bereits alles entdeckt,", "tokens": ["Durch", "den", "Be\u00b7richt", "der", "Zo\u00b7fe", "hat\u00b7te", "Gel\u00b7tar", "be\u00b7reits", "al\u00b7les", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VAFIN", "NN", "ADV", "PIS", "VVPP", "$,"], "meter": "+--+-+-+-+--+---+", "measure": "iambic.septa.invert"}, "line.43": {"text": "Wie Rhin, zu dessen Flucht man alle Mittel anwende,", "tokens": ["Wie", "Rhin", ",", "zu", "des\u00b7sen", "Flucht", "man", "al\u00b7le", "Mit\u00b7tel", "an\u00b7wen\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "APPR", "PRELAT", "NN", "PIS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line