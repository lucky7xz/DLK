eich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.32": {"text": "Wobey mein trotz'ger Muth verirret sich leider so weit,", "tokens": ["Wo\u00b7bey", "mein", "trotz'\u00b7ger", "Muth", "ver\u00b7ir\u00b7ret", "sich", "lei\u00b7der", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "Zu w\u00e4gen nicht achtsam genug die hohe G\u00fcltigkeit", "tokens": ["Zu", "w\u00e4\u00b7gen", "nicht", "acht\u00b7sam", "ge\u00b7nug", "die", "ho\u00b7he", "G\u00fcl\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "PTKNEG", "ADJD", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.34": {"text": "Geheiligter Vorrechte; seitdem ist, gleich dem Funken", "tokens": ["Ge\u00b7hei\u00b7lig\u00b7ter", "Vor\u00b7rech\u00b7te", ";", "seit\u00b7dem", "ist", ",", "gleich", "dem", "Fun\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PAV", "VAFIN", "$,", "ADV", "ART", "NN"], "meter": "-+---+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Der H\u00f6lle, verzehrend Mark und Bein, der Fluch auf mich gesunken.", "tokens": ["Der", "H\u00f6l\u00b7le", ",", "ver\u00b7zeh\u00b7rend", "Mark", "und", "Bein", ",", "der", "Fluch", "auf", "mich", "ge\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "NN", "KON", "NN", "$,", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+-+-+-+-+-