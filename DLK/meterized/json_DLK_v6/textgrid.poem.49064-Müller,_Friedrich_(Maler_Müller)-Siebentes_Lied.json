", ",", "schluch\u00b7zet", "die\u00b7se", "laut", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPOSAT", "NN", "$.", "$(", "NN", "$,", "VVFIN", "PDAT", "ADJD", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.54": {"text": "Keine Auskunft wei\u00df ich auf eure Frage zu geben,", "tokens": ["Kei\u00b7ne", "Aus\u00b7kunft", "wei\u00df", "ich", "auf", "eu\u00b7re", "Fra\u00b7ge", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.55": {"text": "Wie, noch wo das Fr\u00e4ulein sich zu dieser Stund'", "tokens": ["Wie", ",", "noch", "wo", "das", "Fr\u00e4u\u00b7lein", "sich", "zu", "die\u00b7ser", "Stund'"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "KON", "PWAV", "ART", "NN", "PRF", "APPR", "PDAT", "NN"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.56": {"text": "Befinde, wie es stehet um ihr zartes Leben.", "tokens": ["Be\u00b7fin\u00b7de", ",", "wie", "es", "ste\u00b7het", "um", "ihr", "zar\u00b7tes", "Le\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Ganz au\u00dfer sich, sinnlos, an Leib und Seele wund,", "tokens": ["Ganz", "au\u00b7\u00dfer", "sich", ",", "sinn\u00b7los", ",", "an", "Leib", "und", "See\u00b7le", "wund", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "