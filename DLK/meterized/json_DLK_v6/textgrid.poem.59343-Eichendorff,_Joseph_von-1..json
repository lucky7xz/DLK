{"textgrid.poem.59343": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denk ich, du Stille, an dein ruhig Walten,", "tokens": ["Denk", "ich", ",", "du", "Stil\u00b7le", ",", "an", "dein", "ru\u00b7hig", "Wal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPER", "NN", "$,", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An jenes letzten Abends rote K\u00fchle,", "tokens": ["An", "je\u00b7nes", "letz\u00b7ten", "A\u00b7bends", "ro\u00b7te", "K\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo ich die teure Hand noch durfte halten:", "tokens": ["Wo", "ich", "die", "teu\u00b7re", "Hand", "noch", "durf\u00b7te", "hal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Steh ich oft sinnend stille im Gew\u00fchle,", "tokens": ["Steh", "ich", "oft", "sin\u00b7nend", "stil\u00b7le", "im", "Ge\u00b7w\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und, wie den Schweizer heim'sche Alphornslieder", "tokens": ["Und", ",", "wie", "den", "Schwei\u00b7zer", "heim'\u00b7sche", "Al\u00b7phorns\u00b7lie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auf fremden Bergen, fern den Freunden allen,", "tokens": ["Auf", "frem\u00b7den", "Ber\u00b7gen", ",", "fern", "den", "Freun\u00b7den", "al\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "ART", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Oft unverhofft befallen,", "tokens": ["Oft", "un\u00b7ver\u00b7hofft", "be\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kommt tiefe Sehnsucht pl\u00f6tzlich auf mich nieder.", "tokens": ["Kommt", "tie\u00b7fe", "Sehn\u00b7sucht", "pl\u00f6tz\u00b7lich", "auf", "mich", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich hab es oft in deiner Brust gelesen:", "tokens": ["Ich", "hab", "es", "oft", "in", "dei\u00b7ner", "Brust", "ge\u00b7le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nie hast du recht mich in mir selbst gefunden,", "tokens": ["Nie", "hast", "du", "recht", "mich", "in", "mir", "selbst", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PRF", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fremd blieb, zu keck und treibend dir mein Wesen,", "tokens": ["Fremd", "blieb", ",", "zu", "keck", "und", "trei\u00b7bend", "dir", "mein", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PTKA", "ADJD", "KON", "ADJD", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und so bin ich im Strome dir verschwunden.", "tokens": ["Und", "so", "bin", "ich", "im", "Stro\u00b7me", "dir", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O nenn drum nicht die sch\u00f6ne Jugend wilde,", "tokens": ["O", "nenn", "drum", "nicht", "die", "sch\u00f6\u00b7ne", "Ju\u00b7gend", "wil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PAV", "PTKNEG", "ART", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die mit dem Leben und mit seinen Schmerzen", "tokens": ["Die", "mit", "dem", "Le\u00b7ben", "und", "mit", "sei\u00b7nen", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mag unbek\u00fcmmert scherzen,", "tokens": ["Mag", "un\u00b7be\u00b7k\u00fcm\u00b7mert", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Weil sie die Brust reich f\u00fchlt und ernst und milde!", "tokens": ["Weil", "sie", "die", "Brust", "reich", "f\u00fchlt", "und", "ernst", "und", "mil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVFIN", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Getrennt ist l\u00e4ngst schon unsres Lebens Reise,", "tokens": ["Ge\u00b7trennt", "ist", "l\u00e4ngst", "schon", "uns\u00b7res", "Le\u00b7bens", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es trieb mein Herz durch licht' und dunkle Stunden.", "tokens": ["Es", "trieb", "mein", "Herz", "durch", "licht'", "und", "dunk\u00b7le", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem festern Blick erweitern sich die Kreise,", "tokens": ["Dem", "fes\u00b7tern", "Blick", "er\u00b7wei\u00b7tern", "sich", "die", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Duft ist jenes erste Reich verschwunden \u2013", "tokens": ["In", "Duft", "ist", "je\u00b7nes", "ers\u00b7te", "Reich", "ver\u00b7schwun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PDAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch, wie die Pfade einsam sich verwildern,", "tokens": ["Doch", ",", "wie", "die", "Pfa\u00b7de", "ein\u00b7sam", "sich", "ver\u00b7wil\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was ich seitdem, von Lust und Leid bezwungen,", "tokens": ["Was", "ich", "seit\u00b7dem", ",", "von", "Lust", "und", "Leid", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PAV", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Geliebt, geirrt, gesungen:", "tokens": ["Ge\u00b7liebt", ",", "ge\u00b7irrt", ",", "ge\u00b7sun\u00b7gen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich knie vor dir in all den tausend Bildern.", "tokens": ["Ich", "knie", "vor", "dir", "in", "all", "den", "tau\u00b7send", "Bil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "PIAT", "ART", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}