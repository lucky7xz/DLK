{"textgrid.poem.56892": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sorglosen L\u00e4chelns", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sorglosen L\u00e4chelns", "tokens": ["Sorg\u00b7lo\u00b7sen", "L\u00e4\u00b7chelns"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "die Lippen gesch\u00fcrzt,", "tokens": ["die", "Lip\u00b7pen", "ge\u00b7sch\u00fcrzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "fr\u00f6hlich die bl\u00fchenden", "tokens": ["fr\u00f6h\u00b7lich", "die", "bl\u00fc\u00b7hen\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ART", "ADJA"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wangen ger\u00f6tet,", "tokens": ["Wan\u00b7gen", "ge\u00b7r\u00f6\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "tanzen wir Kinder des Gl\u00fccks", "tokens": ["tan\u00b7zen", "wir", "Kin\u00b7der", "des", "Gl\u00fccks"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "unsre sonnigen Pfade dahin.", "tokens": ["uns\u00b7re", "son\u00b7ni\u00b7gen", "Pfa\u00b7de", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PAV", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Rosenkr\u00e4nze", "tokens": ["Ro\u00b7sen\u00b7kr\u00e4n\u00b7ze"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "und schimmernde B\u00e4lle", "tokens": ["und", "schim\u00b7mern\u00b7de", "B\u00e4l\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "werfen wir uns", "tokens": ["wer\u00b7fen", "wir", "uns"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "und den Fremdlingen zu.", "tokens": ["und", "den", "Fremd\u00b7lin\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Wer uns begegnet,", "tokens": ["Wer", "uns", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "dem huscht es wie Gold", "tokens": ["dem", "huscht", "es", "wie", "Gold"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "KOKOM", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "\u00fcber das sinnende", "tokens": ["\u00fc\u00b7ber", "das", "sin\u00b7nen\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.4": {"text": "Antlitz.", "tokens": ["Ant\u00b7litz", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Auf weichen Armen", "tokens": ["Auf", "wei\u00b7chen", "Ar\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "tr\u00e4gt uns das Weib,", "tokens": ["tr\u00e4gt", "uns", "das", "Weib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "s\u00fc\u00df von K\u00fcssen", "tokens": ["s\u00fc\u00df", "von", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "duftet die Luft.", "tokens": ["duf\u00b7tet", "die", "Luft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Unser Wort", "tokens": ["Un\u00b7ser", "Wort"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "ist Gesang", "tokens": ["ist", "Ge\u00b7sang"], "token_info": ["word", "word"], "pos": ["VAFIN", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "und Gesang", "tokens": ["und", "Ge\u00b7sang"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "unsre Antwort.", "tokens": ["uns\u00b7re", "Ant\u00b7wort", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Fallen uns Feinde an,", "tokens": ["Fal\u00b7len", "uns", "Fein\u00b7de", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "sch\u00fctten wir lachend", "tokens": ["sch\u00fct\u00b7ten", "wir", "la\u00b7chend"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "klingende Blitze", "tokens": ["klin\u00b7gen\u00b7de", "Blit\u00b7ze"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "\u00fcber sie aus.", "tokens": ["\u00fc\u00b7ber", "sie", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Aber dem Urfeind", "tokens": ["A\u00b7ber", "dem", "Ur\u00b7feind"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--++", "measure": "adoneus"}, "line.2": {"text": "kommen am liebsten wir", "tokens": ["kom\u00b7men", "am", "liebs\u00b7ten", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PTKA", "ADJD", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "raschen Entschlusses", "tokens": ["ra\u00b7schen", "Ent\u00b7schlus\u00b7ses"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "selber zuvor.", "tokens": ["sel\u00b7ber", "zu\u00b7vor", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Wir sind der Welt", "tokens": ["Wir", "sind", "der", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "unschuldigster Sinn,", "tokens": ["un\u00b7schul\u00b7digs\u00b7ter", "Sinn", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "wir sind die Erntenden", "tokens": ["wir", "sind", "die", "Ern\u00b7ten\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "m\u00fchsamer Saaten.", "tokens": ["m\u00fch\u00b7sa\u00b7mer", "Saa\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Sorglosen L\u00e4chelns", "toke