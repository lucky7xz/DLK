{"textgrid.poem.62752": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heil, Heil'dem Weib, das seine Zierde", "genre": "verse", "period": "N.A.", "pub_year": 1768, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heil, Heil'dem Weib, das seine Zierde", "tokens": ["Heil", ",", "Heil'\u00b7dem", "Weib", ",", "das", "sei\u00b7ne", "Zier\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht fremden Zonen stiehlt,", "tokens": ["Nicht", "frem\u00b7den", "Zo\u00b7nen", "stiehlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, wie Cornelia, die W\u00fcrde", "tokens": ["Und", ",", "wie", "Cor\u00b7ne\u00b7lia", ",", "die", "W\u00fcr\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "PWAV", "NE", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Des Muttertitels f\u00fchlt!", "tokens": ["Des", "Mut\u00b7ter\u00b7ti\u00b7tels", "f\u00fchlt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Einst gab ein fremdes Frauenzimmer", "tokens": ["Einst", "gab", "ein", "frem\u00b7des", "Frau\u00b7en\u00b7zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr einen Staatsbesuch;", "tokens": ["Ihr", "ei\u00b7nen", "Staats\u00b7be\u00b7such", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihr ganzer Leib war lauter Schimmer", "tokens": ["Ihr", "gan\u00b7zer", "Leib", "war", "lau\u00b7ter", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und lauter Wohlgeruch.", "tokens": ["Und", "lau\u00b7ter", "Wohl\u00b7ge\u00b7ruch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Die Nymphe schwatzt von Putz und Kleide,", "tokens": ["Die", "Nym\u00b7phe", "schwatzt", "von", "Putz", "und", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So pflegt es noch zu gehn;", "tokens": ["So", "pflegt", "es", "noch", "zu", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und endlich w\u00fcnscht sie das Geschmeide", "tokens": ["Und", "end\u00b7lich", "w\u00fcnscht", "sie", "das", "Ge\u00b7schmei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der R\u00f6merin zu sehn.", "tokens": ["Der", "R\u00f6\u00b7me\u00b7rin", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Cornelia winkt ihren S\u00f6hnen,", "tokens": ["Cor\u00b7ne\u00b7lia", "winkt", "ih\u00b7ren", "S\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und als sie sich genaht,", "tokens": ["Und", "als", "sie", "sich", "ge\u00b7naht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "So sprach sie zu der eiteln Sch\u00f6nen:", "tokens": ["So", "sprach", "sie", "zu", "der", "ei\u00b7teln", "Sch\u00f6\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Hier diese sind mein Staat.", "tokens": ["Hier", "die\u00b7se", "sind", "mein", "Staat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Wie manche Dame wird hier lachen,", "tokens": ["Wie", "man\u00b7che", "Da\u00b7me", "wird", "hier", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Auch du, Geliebte? \u2013 Nein,", "tokens": ["Auch", "du", ",", "Ge\u00b7lieb\u00b7te", "?", "\u2013", "Nein", ","], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "PPER", "$,", "NN", "$.", "$(", "PTKANT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Nein! die Geb\u00e4hrerin der Gracchen", "tokens": ["Nein", "!", "die", "Ge\u00b7b\u00e4h\u00b7re\u00b7rin", "der", "Grac\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Wird stets dein Vorbild seyn.", "tokens": ["Wird", "stets", "dein", "Vor\u00b7bild", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}