{"textgrid.poem.53463": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Gro\u00dfstadt \u2013 Weihnachten", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun senkt sich wieder auf die heim'schen Fluren", "tokens": ["Nun", "senkt", "sich", "wie\u00b7der", "auf", "die", "heim'\u00b7schen", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die Weihenacht! die Weihenacht!", "tokens": ["die", "Wei\u00b7he\u00b7nacht", "!", "die", "Wei\u00b7he\u00b7nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was die Mamas bepackt nach Hause fuhren,", "tokens": ["Was", "die", "Ma\u00b7mas", "be\u00b7packt", "nach", "Hau\u00b7se", "fuh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wir kriegens jetzo freundlich dargebracht.", "tokens": ["wir", "krie\u00b7gens", "jet\u00b7zo", "freund\u00b7lich", "dar\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Asphalt glitscht. Kann Emil das gebrauchen?", "tokens": ["Der", "As\u00b7phalt", "glitscht", ".", "Kann", "E\u00b7mil", "das", "ge\u00b7brau\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "VMFIN", "NE", "PDS", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Braut kramt sch\u00e4mig in dem Portemonnaie.", "tokens": ["Die", "Braut", "kramt", "sch\u00e4\u00b7mig", "in", "dem", "Por\u00b7te\u00b7mon\u00b7nai\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie schenkt ihm, teils zum Schmuck und teils zum Rauchen,", "tokens": ["Sie", "schenkt", "ihm", ",", "teils", "zum", "Schmuck", "und", "teils", "zum", "Rau\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "APPRART", "NN", "KON", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "den Aschenbecher aus Emalch glas\u00e9.", "tokens": ["den", "A\u00b7schen\u00b7be\u00b7cher", "aus", "E\u00b7malch", "glas\u00e9", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das Christkind kommt! Wir jungen Leute lauschen", "tokens": ["Das", "Christ\u00b7kind", "kommt", "!", "Wir", "jun\u00b7gen", "Leu\u00b7te", "lau\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "auf einen stillen heiligen Grammophon.", "tokens": ["auf", "ei\u00b7nen", "stil\u00b7len", "hei\u00b7li\u00b7gen", "Gram\u00b7mo\u00b7phon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Christkind kommt und ist bereit zu tauschen", "tokens": ["Das", "Christ\u00b7kind", "kommt", "und", "ist", "be\u00b7reit", "zu", "tau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VAFIN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "den Schlips, die Puppe und das Lexikohn,", "tokens": ["den", "Schlips", ",", "die", "Pup\u00b7pe", "und", "das", "Le\u00b7xi\u00b7kohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und sitzt der wackre B\u00fcrger bei den Seinen,", "tokens": ["Und", "sitzt", "der", "wack\u00b7re", "B\u00fcr\u00b7ger", "bei", "den", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "PPOSS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "voll Karpfen, still im Stuhl, um halber zehn,", "tokens": ["voll", "Karp\u00b7fen", ",", "still", "im", "Stuhl", ",", "um", "hal\u00b7ber", "zehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJD", "APPRART", "NN", "$,", "KOUI", "APPO", "CARD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "dann ist er mit sich selbst zufrieden und im reinen:", "tokens": ["dann", "ist", "er", "mit", "sich", "selbst", "zu\u00b7frie\u00b7den", "und", "im", "rei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PRF", "ADV", "ADJD", "KON", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u00bbach ja, son Christfest is doch ooch janz scheen!\u00ab", "tokens": ["\u00bb", "ach", "ja", ",", "son", "Christ\u00b7fest", "is", "doch", "o\u00b7och", "janz", "scheen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "ADV", "$,", "XY", "XY", "FM", "ADV", "ADV", "ADV", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Und frohgelaunt spricht er vom \u203aWeihnachtswetter\u2039,", "tokens": ["Und", "froh\u00b7ge\u00b7launt", "spricht", "er", "vom", "\u203a", "Weih\u00b7nachts\u00b7wet\u00b7ter", "\u2039", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "mag es nun regnen oder mag es schnein,", "tokens": ["mag", "es", "nun", "reg\u00b7nen", "o\u00b7der", "mag", "es", "schnein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "KON", "VMFIN", "PPER", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Jovial und schmauchend liest er seine Morgenbl\u00e4tter,", "tokens": ["Jo\u00b7vi\u00b7al", "und", "schmau\u00b7chend", "liest", "er", "sei\u00b7ne", "Mor\u00b7gen\u00b7bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "die tr\u00e4chtig sind von s\u00fc\u00dfen Plauderein.", "tokens": ["die", "tr\u00e4ch\u00b7tig", "sind", "von", "s\u00fc\u00b7\u00dfen", "Plau\u00b7de\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So trifft denn nur auf eitel Gl\u00fcck hienieden", "tokens": ["So", "trifft", "denn", "nur", "auf", "ei\u00b7tel", "Gl\u00fcck", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "APPR", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in dieser Residenz Christkindleins Flug?", "tokens": ["in", "die\u00b7ser", "Re\u00b7si\u00b7denz", "Christ\u00b7kin\u00b7dleins", "Flug", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Gott, sie mimen eben Weihnachtsfrieden . . .", "tokens": ["Mein", "Gott", ",", "sie", "mi\u00b7men", "e\u00b7ben", "Weih\u00b7nachts\u00b7frie\u00b7den", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADV", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbwir spielen alle. Wer es wei\u00df, ist klug.\u00ab", "tokens": ["\u00bb", "wir", "spie\u00b7len", "al\u00b7le", ".", "Wer", "es", "wei\u00df", ",", "ist", "klug", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIS", "$.", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}