{"dta.poem.13848": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Waffenstillstand der Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Windsgleich kommt der wilde Krieg geritten,", "tokens": ["Winds\u00b7gleich", "kommt", "der", "wil\u00b7de", "Krieg", "ge\u00b7rit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Durch das Gr\u00fcn der Tod ihm nachgeschritten,", "tokens": ["Durch", "das", "Gr\u00fcn", "der", "Tod", "ihm", "nach\u00b7ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Manch Gespenst steht sinnend auf dem Feld,", "tokens": ["Manch", "Ge\u00b7spenst", "steht", "sin\u00b7nend", "auf", "dem", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und der Sommer sch\u00fcttelt sich vor Grausen,", "tokens": ["Und", "der", "Som\u00b7mer", "sch\u00fct\u00b7telt", "sich", "vor", "Grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "L\u00e4\u00dft die Bl\u00e4tter, schlie\u00dft die gr\u00fcnen Klausen,", "tokens": ["L\u00e4\u00dft", "die", "Bl\u00e4t\u00b7ter", ",", "schlie\u00dft", "die", "gr\u00fc\u00b7nen", "Klau\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ab sich wendend von der blut'gen Welt.", "tokens": ["Ab", "sich", "wen\u00b7dend", "von", "der", "blut'\u00b7gen", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}