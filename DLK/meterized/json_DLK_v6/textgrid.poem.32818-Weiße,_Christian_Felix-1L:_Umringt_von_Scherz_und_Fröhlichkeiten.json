{"textgrid.poem.32818": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Umringt von Scherz und Fr\u00f6hlichkeiten", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Umringt von Scherz und Fr\u00f6hlichkeiten", "tokens": ["Um\u00b7ringt", "von", "Scherz", "und", "Fr\u00f6h\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Versammelt uns die Freundschaft hier:", "tokens": ["Ver\u00b7sam\u00b7melt", "uns", "die", "Freund\u00b7schaft", "hier", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entweicht ihr Klagen b\u00f6ser Zeiten", "tokens": ["Ent\u00b7weicht", "ihr", "Kla\u00b7gen", "b\u00f6\u00b7ser", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Gott der Freuden feyern wir.", "tokens": ["Dem", "Gott", "der", "Freu\u00b7den", "fey\u00b7ern", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch Liebe, du, la\u00df uns alleine:", "tokens": ["Auch", "Lie\u00b7be", ",", "du", ",", "la\u00df", "uns", "al\u00b7lei\u00b7ne", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PPER", "$,", "VVIMP", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wir feyern jetzt dem besten Weine.", "tokens": ["Wir", "fey\u00b7ern", "jetzt", "dem", "bes\u00b7ten", "Wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar deine Freuden sind auch s\u00fc\u00dfe,", "tokens": ["Zwar", "dei\u00b7ne", "Freu\u00b7den", "sind", "auch", "s\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und durch sie wird kein Herz entehrt:", "tokens": ["Und", "durch", "sie", "wird", "kein", "Herz", "ent\u00b7ehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn junger Sch\u00f6nen sanfte K\u00fc\u00dfe", "tokens": ["Denn", "jun\u00b7ger", "Sch\u00f6\u00b7nen", "sanf\u00b7te", "K\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind unsrer besten Stunden werth:", "tokens": ["Sind", "uns\u00b7rer", "bes\u00b7ten", "Stun\u00b7den", "werth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch iezt m\u00f6chtst du Ver\u00e4chter finden,", "tokens": ["Doch", "iezt", "m\u00f6chtst", "du", "Ver\u00b7\u00e4ch\u00b7ter", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und wir, wir f\u00fcrchten uns der S\u00fcnden.", "tokens": ["Und", "wir", ",", "wir", "f\u00fcrch\u00b7ten", "uns", "der", "S\u00fcn\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Komm, holde Freyheit, la\u00df dich nieder!", "tokens": ["Komm", ",", "hol\u00b7de", "Frey\u00b7heit", ",", "la\u00df", "dich", "nie\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist die Freundinn von dem Wein:", "tokens": ["Du", "bist", "die", "Freun\u00b7dinn", "von", "dem", "Wein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ert\u00f6nt ihr Ch\u00f6re froher Lieder,", "tokens": ["Er\u00b7t\u00f6nt", "ihr", "Ch\u00f6\u00b7re", "fro\u00b7her", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr muntern Scherze mischt euch ein!", "tokens": ["Ihr", "mun\u00b7tern", "Scher\u00b7ze", "mischt", "euch", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier trinkt, hier scherzt man fern vom Neide:", "tokens": ["Hier", "trinkt", ",", "hier", "scherzt", "man", "fern", "vom", "Nei\u00b7de", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "PIS", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo Bacchus wohnt, da wohnt die Freude.", "tokens": ["Wo", "Bac\u00b7chus", "wohnt", ",", "da", "wohnt", "die", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}