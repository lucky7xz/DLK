{"textgrid.poem.56804": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tief im Walde, tief im Walde", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief im Walde, tief im Walde", "tokens": ["Tief", "im", "Wal\u00b7de", ",", "tief", "im", "Wal\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "$,", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "bildet, fern der Wege Reich,", "tokens": ["bil\u00b7det", ",", "fern", "der", "We\u00b7ge", "Reich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "eines Bruchs verla\u00dfne Halde", "tokens": ["ei\u00b7nes", "Bruchs", "ver\u00b7la\u00df\u00b7ne", "Hal\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "einen kleinen, stillen Teich.", "tokens": ["ei\u00b7nen", "klei\u00b7nen", ",", "stil\u00b7len", "Teich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Moosbewachsne Bl\u00f6cke ragen", "tokens": ["Moos\u00b7be\u00b7wachs\u00b7ne", "Bl\u00f6\u00b7cke", "ra\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "aus der seichten Regenflut,", "tokens": ["aus", "der", "seich\u00b7ten", "Re\u00b7gen\u00b7flut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Falter und Libellen jagen", "tokens": ["Fal\u00b7ter", "und", "Li\u00b7bel\u00b7len", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "\u00fcber bunter Lurche Brut.", "tokens": ["\u00fc\u00b7ber", "bun\u00b7ter", "Lur\u00b7che", "Brut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber wenn im Abendbrande", "tokens": ["A\u00b7ber", "wenn", "im", "A\u00b7bend\u00b7bran\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "hinterm Wald die Glut verraucht,", "tokens": ["hin\u00b7term", "Wald", "die", "Glut", "ver\u00b7raucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "st\u00f6\u00dft und rudert es vom Rande,", "tokens": ["st\u00f6\u00dft", "und", "ru\u00b7dert", "es", "vom", "Ran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "kriecht und klettert, plumpst und taucht.", "tokens": ["kriecht", "und", "klet\u00b7tert", ",", "plumpst", "und", "taucht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und der Unken Urgro\u00dfahne", "tokens": ["Und", "der", "Un\u00b7ken", "Ur\u00b7gro\u00df\u00b7ah\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u2013 niemand wei\u00df, wann Gott ihn schuf \u2013", "tokens": ["\u2013", "nie\u00b7mand", "wei\u00df", ",", "wann", "Gott", "ihn", "schuf", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "$,", "PWAV", "NN", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ruft, da\u00df er sein Weibchen mahne,", "tokens": ["ruft", ",", "da\u00df", "er", "sein", "Weib\u00b7chen", "mah\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "seinen dunklen Werberuf.", "tokens": ["sei\u00b7nen", "dunk\u00b7len", "Wer\u00b7be\u00b7ruf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Da\u00df das Froschgeschlecht nicht sterbe,", "tokens": ["Da\u00df", "das", "Froschge\u00b7schlecht", "nicht", "ster\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "bleibt zuletzt nicht Einer still:", "tokens": ["bleibt", "zu\u00b7letzt", "nicht", "Ei\u00b7ner", "still", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der T\u00fcmpel ist ein Erbe,", "tokens": ["Denn", "der", "T\u00fcm\u00b7pel", "ist", "ein", "Er\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "das getreu gewahrt sein will.", "tokens": ["das", "ge\u00b7treu", "ge\u00b7wahrt", "sein", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "VAINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Liebeskranke Grunzer fliehen", "tokens": ["Lie\u00b7bes\u00b7kran\u00b7ke", "Grun\u00b7zer", "flie\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "der bewegten Weibchen Schlund;", "tokens": ["der", "be\u00b7weg\u00b7ten", "Weib\u00b7chen", "Schlund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "immer k\u00fchnre Harmonien", "tokens": ["im\u00b7mer", "k\u00fchn\u00b7re", "Har\u00b7mo\u00b7ni\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "f\u00fcllen den d\u00e4mmertrauten Grund.", "tokens": ["f\u00fcl\u00b7len", "den", "d\u00e4m\u00b7mer\u00b7trau\u00b7ten", "Grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Bis des Mondes Goldhorn endlich", "tokens": ["Bis", "des", "Mon\u00b7des", "Gold\u00b7horn", "end\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "neuen Schimmers alles speist:", "tokens": ["neu\u00b7en", "Schim\u00b7mers", "al\u00b7les", "speist", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun erwahrt sich unabwendlich", "tokens": ["Nun", "er\u00b7wahrt", "sich", "un\u00b7ab\u00b7wend\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJD"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "trunkner N\u00e4chstenliebe Geist ...", "tokens": ["trunk\u00b7ner", "N\u00e4chs\u00b7ten\u00b7lie\u00b7be", "Geist", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Tief im Walde, tief im Walde", "tokens": ["Tief", "im", "Wal\u00b7de", ",", "tief", "im", "Wal\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "$,", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "schw\u00e4rmt Froschbr\u00e4utigam und Braut", "tokens": ["schw\u00e4rmt", "Froschbr\u00e4u\u00b7ti\u00b7gam", "und", "Braut"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NE", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "in versteckter Steinbruchhalde,", "tokens": ["in", "ver\u00b7steck\u00b7ter", "Stein\u00b7bruch\u00b7hal\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "bis der letzte Stern ergraut.", "tokens": ["bis", "der", "letz\u00b7te", "Stern", "er\u00b7graut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}