{"dta.poem.7309": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "1.  \n Mit Laura's Bilde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Um einen Myrthenzweig sich zu ersingen", "tokens": ["Um", "ei\u00b7nen", "Myr\u00b7then\u00b7zweig", "sich", "zu", "er\u00b7sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schickt seinen Schwan Petrarka Laure'n nach,", "tokens": ["Schickt", "sei\u00b7nen", "Schwan", "Pe\u00b7trar\u00b7ka", "Lau\u00b7re'n", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NE", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Lorbeerreisern f\u00fcllt er das Gemach,", "tokens": ["Mit", "Lor\u00b7beer\u00b7rei\u00b7sern", "f\u00fcllt", "er", "das", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch kann er in den Myrthenhain nicht dringen.", "tokens": ["Doch", "kann", "er", "in", "den", "Myr\u00b7then\u00b7hain", "nicht", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da zieht er durch die Welt mit hellem Klingen,", "tokens": ["Da", "zieht", "er", "durch", "die", "Welt", "mit", "hel\u00b7lem", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schl\u00e4gt mit den Fl\u00fcgeln an das theure Haus,", "tokens": ["Schl\u00e4gt", "mit", "den", "Fl\u00fc\u00b7geln", "an", "das", "theu\u00b7re", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Man reicht ihm den Cypressenkranz hinaus,", "tokens": ["Man", "reicht", "ihm", "den", "Cyp\u00b7res\u00b7sen\u00b7kranz", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "APZR", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Allein die Myrthe kann er nicht erringen.", "tokens": ["Al\u00b7lein", "die", "Myr\u00b7the", "kann", "er", "nicht", "er\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mein Freund, wohl ist der Lorbeer uns versagt,", "tokens": ["Mein", "Freund", ",", "wohl", "ist", "der", "Lor\u00b7beer", "uns", "ver\u00b7sagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch la\u00df uns um den schn\u00f6den Preis nicht klagen,", "tokens": ["Doch", "la\u00df", "uns", "um", "den", "schn\u00f6\u00b7den", "Preis", "nicht", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Dornen und Cypressen rings umragt.", "tokens": ["Von", "Dor\u00b7nen", "und", "Cyp\u00b7res\u00b7sen", "rings", "um\u00b7ragt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Will es in einer Laura Blick mir tagen,", "tokens": ["Will", "es", "in", "ei\u00b7ner", "Lau\u00b7ra", "Blick", "mir", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "NN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann hab' ich gern dem schweren Kranz entsagt,", "tokens": ["Dann", "hab'", "ich", "gern", "dem", "schwe\u00b7ren", "Kranz", "ent\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die kleine Myrthe l\u00e4\u00dft sich leichter tragen.", "tokens": ["Die", "klei\u00b7ne", "Myr\u00b7the", "l\u00e4\u00dft", "sich", "leich\u00b7ter", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}