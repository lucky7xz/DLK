{"textgrid.poem.55356": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Ins Weite", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das geht so fr\u00f6hlich", "tokens": ["Das", "geht", "so", "fr\u00f6h\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ins Allgemeine!", "tokens": ["Ins", "All\u00b7ge\u00b7mei\u00b7ne", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ist leicht und selig,", "tokens": ["Ist", "leicht", "und", "se\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Als w\u00e4r's auch reine.", "tokens": ["Als", "w\u00e4r's", "auch", "rei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ADV", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Sie wissen gar nichts", "tokens": ["Sie", "wis\u00b7sen", "gar", "nichts"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIS"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Von stillen Riffen;", "tokens": ["Von", "stil\u00b7len", "Rif\u00b7fen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und wie sie schiffen,", "tokens": ["Und", "wie", "sie", "schif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Die lieben Heitern,", "tokens": ["Die", "lie\u00b7ben", "Hei\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Sie werden wie gar nichts", "tokens": ["Sie", "wer\u00b7den", "wie", "gar", "nichts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "ADV", "PIS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Zusammen scheitern.", "tokens": ["Zu\u00b7sam\u00b7men", "schei\u00b7tern", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}