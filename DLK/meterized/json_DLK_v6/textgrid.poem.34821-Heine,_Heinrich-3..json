{"textgrid.poem.34821": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hatte einst ein sch\u00f6nes Vaterland.", "tokens": ["Ich", "hat\u00b7te", "einst", "ein", "sch\u00f6\u00b7nes", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Eichenbaum", "tokens": ["Der", "Ei\u00b7chen\u00b7baum"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wuchs dort so hoch, die Veilchen nickten sanft.", "tokens": ["Wuchs", "dort", "so", "hoch", ",", "die", "Veil\u00b7chen", "nick\u00b7ten", "sanft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADJD", "$,", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es war ein Traum.", "tokens": ["Es", "war", "ein", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Das k\u00fc\u00dfte mich auf deutsch, und sprach auf deutsch", "tokens": ["Das", "k\u00fc\u00df\u00b7te", "mich", "auf", "deutsch", ",", "und", "sprach", "auf", "deutsch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ADJD", "$,", "KON", "VVFIN", "APPR", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "(man glaubt es kaum,", "tokens": ["(", "man", "glaubt", "es", "kaum", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie gut es klang) das Wort: \u00bbIch liebe dich!\u00ab", "tokens": ["Wie", "gut", "es", "klang", ")", "das", "Wort", ":", "\u00bb", "Ich", "lie\u00b7be", "dich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "$(", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es war ein Traum.", "tokens": ["Es", "war", "ein", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}