{"textgrid.poem.25903": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Heut abend", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Droben am Berglein im Kirschenland", "tokens": ["Dro\u00b7ben", "am", "Berg\u00b7lein", "im", "Kir\u00b7schen\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "APPRART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Heut abend ich mit meinem Vielliebchen stand,", "tokens": ["Heut", "a\u00b7bend", "ich", "mit", "mei\u00b7nem", "Viel\u00b7lieb\u00b7chen", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo sie manch Schl\u00fcsselbl\u00fcmlein fand.", "tokens": ["Wo", "sie", "manch", "Schl\u00fcs\u00b7sel\u00b7bl\u00fcm\u00b7lein", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie winkte an des Bergleins Rand", "tokens": ["Sie", "wink\u00b7te", "an", "des", "Berg\u00b7leins", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Wolken zu mit gl\u00fccklicher Hand.", "tokens": ["Den", "Wol\u00b7ken", "zu", "mit", "gl\u00fcck\u00b7li\u00b7cher", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Frau Venus trat aus der Himmelswand", "tokens": ["Frau", "Ve\u00b7nus", "trat", "aus", "der", "Him\u00b7mels\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Aufleuchtend, weil sie zwei Selige fand.", "tokens": ["Auf\u00b7leuch\u00b7tend", ",", "weil", "sie", "zwei", "Se\u00b7li\u00b7ge", "fand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}}}}}