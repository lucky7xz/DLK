{"textgrid.poem.37964": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die gef\u00e4hrliche Manschettenblume", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es stand ein Baum im Schweizerland,", "tokens": ["Es", "stand", "ein", "Baum", "im", "Schwei\u00b7zer\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der trug Manschettenblumen,", "tokens": ["Der", "trug", "Man\u00b7schet\u00b7ten\u00b7blu\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die erste Blume die er trug,", "tokens": ["Die", "ers\u00b7te", "Blu\u00b7me", "die", "er", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die war des K\u00f6nigs Tochter.", "tokens": ["Die", "war", "des", "K\u00f6\u00b7nigs", "Toch\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des Bauers Sohn darunter war,", "tokens": ["Des", "Bau\u00b7ers", "Sohn", "da\u00b7run\u00b7ter", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PAV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der th\u00e4te um sie freyen,", "tokens": ["Der", "th\u00e4\u00b7te", "um", "sie", "frey\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er freyte l\u00e4nger als sieben Jahr,", "tokens": ["Er", "frey\u00b7te", "l\u00e4n\u00b7ger", "als", "sie\u00b7ben", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KOKOM", "CARD", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er konnte sie nicht erfreyen.", "tokens": ["Er", "konn\u00b7te", "sie", "nicht", "er\u00b7fre\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Der Bauernsohn steigt auf das Nest,", "tokens": ["Der", "Bau\u00b7ern\u00b7sohn", "steigt", "auf", "das", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da oben auf dem Baume,", "tokens": ["Da", "o\u00b7ben", "auf", "dem", "Bau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der K\u00f6nig h\u00e4lt ihn am Mantel fest:", "tokens": ["Der", "K\u00f6\u00b7nig", "h\u00e4lt", "ihn", "am", "Man\u00b7tel", "fest", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbwas willst mit meiner Tochter?", "tokens": ["\u00bb", "was", "willst", "mit", "mei\u00b7ner", "Toch\u00b7ter", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie ist viel h\u00f6her geboren als du,", "tokens": ["Sie", "ist", "viel", "h\u00f6\u00b7her", "ge\u00b7bo\u00b7ren", "als", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "KOUS", "PPER", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von Vater und von Mutter.\u00ab", "tokens": ["Von", "Va\u00b7ter", "und", "von", "Mut\u00b7ter", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbist sie viel h\u00f6her geboren als ich,", "tokens": ["\u00bb", "ist", "sie", "viel", "h\u00f6\u00b7her", "ge\u00b7bo\u00b7ren", "als", "ich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "KOKOM", "PPER", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "So bin ich viel h\u00f6her gestiegen.\u00ab", "tokens": ["So", "bin", "ich", "viel", "h\u00f6\u00b7her", "ge\u00b7stie\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$.", "$("], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbund wenn du auch mein Rath schon bist,", "tokens": ["\u00bb", "und", "wenn", "du", "auch", "mein", "Rath", "schon", "bist", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist doch nicht vom Blute.\u00ab", "tokens": ["Du", "bist", "doch", "nicht", "vom", "Blu\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbey K\u00f6nig was du jetzo bist,", "tokens": ["\u00bb", "ey", "K\u00f6\u00b7nig", "was", "du", "jet\u00b7zo", "bist", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "PWS", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das dankest du meinem Blute!\u00ab", "tokens": ["Das", "dan\u00b7kest", "du", "mei\u00b7nem", "Blu\u00b7te", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbich dank dir mein Schlo\u00df in Oesterreich,", "tokens": ["\u00bb", "ich", "dank", "dir", "mein", "Schlo\u00df", "in", "O\u00b7es\u00b7ter\u00b7reich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "APPR", "PPER", "PPOSAT", "NN", "APPR", "NE", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da sollst du K\u00f6nig werden,", "tokens": ["Da", "sollst", "du", "K\u00f6\u00b7nig", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich schlag dich zum Ritter mit d\u00fcrrem Zweig,", "tokens": ["Ich", "schlag", "dich", "zum", "Rit\u00b7ter", "mit", "d\u00fcr\u00b7rem", "Zweig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das Kettlein soll dir auch werden.", "tokens": ["Das", "Ket\u00b7tlein", "soll", "dir", "auch", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}}, "stanza.7": {"line.1": {"text": "Und \u00fcber dem Schlo\u00df noch h\u00f6her hinaus,", "tokens": ["Und", "\u00fc\u00b7ber", "dem", "Schlo\u00df", "noch", "h\u00f6\u00b7her", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie sollen hinauf dich ziehen,", "tokens": ["Sie", "sol\u00b7len", "hin\u00b7auf", "dich", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da hast du \u00fcber den Wolken ein Haus,", "tokens": ["Da", "hast", "du", "\u00fc\u00b7ber", "den", "Wol\u00b7ken", "ein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gewitter unter dir ziehen.\u00ab", "tokens": ["Ge\u00b7wit\u00b7ter", "un\u00b7ter", "dir", "zie\u00b7hen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbund h\u00e4tt es des K\u00f6nigs Tochter gethan,", "tokens": ["\u00bb", "und", "h\u00e4tt", "es", "des", "K\u00f6\u00b7nigs", "Toch\u00b7ter", "ge\u00b7than", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kein K\u00f6nig ich w\u00fcrd \u00fcber alle,", "tokens": ["Kein", "K\u00f6\u00b7nig", "ich", "w\u00fcrd", "\u00fc\u00b7ber", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VAFIN", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So gehts wer gerne freyen th\u00e4t,", "tokens": ["So", "gehts", "wer", "ger\u00b7ne", "frey\u00b7en", "th\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PWS", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kann doch keiner gefallen.\u00ab", "tokens": ["Und", "kann", "doch", "kei\u00b7ner", "ge\u00b7fal\u00b7len", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VMFIN", "ADV", "PIS", "VVPP", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}