{"textgrid.poem.44173": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich will schweigen. Mag's doch seyn,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will schweigen. Mag's doch seyn,", "tokens": ["Ich", "will", "schwei\u00b7gen", ".", "Mag's", "doch", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$.", "NE", "ADV", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mag's doch biegen oder brechen,", "tokens": ["Mag's", "doch", "bie\u00b7gen", "o\u00b7der", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mitleid oder Tyranney,", "tokens": ["Mit\u00b7leid", "o\u00b7der", "Ty\u00b7ran\u00b7ney", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Beides gilt mir einerley;", "tokens": ["Bei\u00b7des", "gilt", "mir", "ei\u00b7ner\u00b7ley", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Las die L\u00e4sterzungen stechen,", "tokens": ["Las", "die", "L\u00e4s\u00b7ter\u00b7zun\u00b7gen", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Las die Misgunst Zeter schreyn,", "tokens": ["Las", "die", "Mis\u00b7gunst", "Ze\u00b7ter", "schreyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich will schweigen, mag's doch seyn.", "tokens": ["Ich", "will", "schwei\u00b7gen", ",", "mag's", "doch", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VMFIN", "ADV", "VAINF", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Ich will schweigen. Immerhin,", "tokens": ["Ich", "will", "schwei\u00b7gen", ".", "Im\u00b7mer\u00b7hin", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$.", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Immerhin, ihr falschen Freunde,", "tokens": ["Im\u00b7mer\u00b7hin", ",", "ihr", "fal\u00b7schen", "Freun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Last mich stecken, flieht und lacht,", "tokens": ["Last", "mich", "ste\u00b7cken", ",", "flieht", "und", "lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Geht geheim, verst\u00e4rckt die Macht", "tokens": ["Geht", "ge\u00b7heim", ",", "ver\u00b7st\u00e4rckt", "die", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Meiner abgesagten Feinde,", "tokens": ["Mei\u00b7ner", "ab\u00b7ge\u00b7sag\u00b7ten", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eure Flucht ist mein Gewinn.", "tokens": ["Eu\u00b7re", "Flucht", "ist", "mein", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich will schweigen, immerhin.", "tokens": ["Ich", "will", "schwei\u00b7gen", ",", "im\u00b7mer\u00b7hin", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich will schweigen. Rast nur fort,", "tokens": ["Ich", "will", "schwei\u00b7gen", ".", "Rast", "nur", "fort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$.", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Rast nur fort, ihr groben Sp\u00f6tter,", "tokens": ["Rast", "nur", "fort", ",", "ihr", "gro\u00b7ben", "Sp\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Helft dem Gl\u00fccke, das mich dr\u00fcckt,", "tokens": ["Helft", "dem", "Gl\u00fc\u00b7cke", ",", "das", "mich", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dr\u00e4ngt die Unschuld, die sich b\u00fcckt,", "tokens": ["Dr\u00e4ngt", "die", "Un\u00b7schuld", ",", "die", "sich", "b\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und erregt noch gr\u00f6\u00dfre Wetter,", "tokens": ["Und", "er\u00b7regt", "noch", "gr\u00f6\u00df\u00b7re", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Manchmahl bringt ein Sturm an Port.", "tokens": ["Manch\u00b7mahl", "bringt", "ein", "Sturm", "an", "Port", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich will schweigen, rast nur fort.", "tokens": ["Ich", "will", "schwei\u00b7gen", ",", "rast", "nur", "fort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich will hofen. Hofnung siegt.", "tokens": ["Ich", "will", "ho\u00b7fen", ".", "Hof\u00b7nung", "siegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$.", "NN", "VVFIN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Die Gedult ist meine St\u00e4rcke,", "tokens": ["Die", "Ge\u00b7dult", "ist", "mei\u00b7ne", "St\u00e4r\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Gela\u00dfenheit mein Schwerd;", "tokens": ["Die", "Ge\u00b7la\u00b7\u00dfen\u00b7heit", "mein", "Schwerd", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer sich mit Verachtung wehrt,", "tokens": ["Wer", "sich", "mit", "Ver\u00b7ach\u00b7tung", "wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Thut im Streiten Wunderwercke,", "tokens": ["Thut", "im", "Strei\u00b7ten", "Wun\u00b7der\u00b7wer\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis Gewalt und Bo\u00dfheit liegt.", "tokens": ["Bis", "Ge\u00b7walt", "und", "Bo\u00df\u00b7heit", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich will hofen. Hofnung siegt.", "tokens": ["Ich", "will", "ho\u00b7fen", ".", "Hof\u00b7nung", "siegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$.", "NN", "VVFIN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Ich will hofen als ein Mann.", "tokens": ["Ich", "will", "ho\u00b7fen", "als", "ein", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob ich mich auch jezo schmiege,", "tokens": ["Ob", "ich", "mich", "auch", "je\u00b7zo", "schmie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob gleich niemand nach mir fragt", "tokens": ["Ob", "gleich", "nie\u00b7mand", "nach", "mir", "fragt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIS", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder mich nur treu beklagt,", "tokens": ["O\u00b7der", "mich", "nur", "treu", "be\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O wer weis, wie bald sich's f\u00fcge,", "tokens": ["O", "wer", "weis", ",", "wie", "bald", "sich's", "f\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "PTKVZ", "$,", "PWAV", "ADV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ich andre retten kan.", "tokens": ["Da\u00df", "ich", "and\u00b7re", "ret\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich will hofen als ein Mann.", "tokens": ["Ich", "will", "ho\u00b7fen", "als", "ein", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich will hofen unverzagt.", "tokens": ["Ich", "will", "ho\u00b7fen", "un\u00b7ver\u00b7zagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mischt das Gl\u00fccke gleich die Charten", "tokens": ["Mischt", "das", "Gl\u00fc\u00b7cke", "gleich", "die", "Char\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jezo ziemlich schlimm vor mich,", "tokens": ["Je\u00b7zo", "ziem\u00b7lich", "schlimm", "vor", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur Gedult, der beste Stich", "tokens": ["Nur", "Ge\u00b7dult", ",", "der", "bes\u00b7te", "Stich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kommt auf stille seyn und warthen.", "tokens": ["Kommt", "auf", "stil\u00b7le", "seyn", "und", "wart\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "VAINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Falschheit, Gl\u00fcck und Feinde schlagt!", "tokens": ["Falschheit", ",", "Gl\u00fcck", "und", "Fein\u00b7de", "schlagt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich will hofen unverzagt.", "tokens": ["Ich", "will", "ho\u00b7fen", "un\u00b7ver\u00b7zagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}