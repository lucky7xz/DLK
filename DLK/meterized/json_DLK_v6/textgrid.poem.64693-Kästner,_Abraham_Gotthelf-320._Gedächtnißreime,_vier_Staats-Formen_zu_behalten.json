{"textgrid.poem.64693": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "320. Ged\u00e4chtni\u00dfreime, vier Staats-Formen zu behalten", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie schelten auf Aristokraten,", "tokens": ["Sie", "schel\u00b7ten", "auf", "A\u00b7ris\u00b7to\u00b7kra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schw\u00f6ren Ha\u00df der Monarchie,", "tokens": ["Und", "schw\u00f6\u00b7ren", "Ha\u00df", "der", "Mon\u00b7ar\u00b7chie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch f\u00fchlten sie, die Demokraten,", "tokens": ["Auch", "f\u00fchl\u00b7ten", "sie", ",", "die", "De\u00b7mo\u00b7kra\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schon ", "tokens": ["Schon"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}}}}}