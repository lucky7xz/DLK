{"textgrid.poem.56887": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Urpl\u00f6tzlich \u2013", "tokens": ["Ur\u00b7pl\u00f6tz\u00b7lich", "\u2013"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "durch Vorhangspalten \u2013", "tokens": ["durch", "Vor\u00b7hangs\u00b7pal\u00b7ten", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "der Mond ...", "tokens": ["der", "Mond", "..."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Drunter,", "tokens": ["Drun\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["PAV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "schneehell,", "tokens": ["schne\u00b7e\u00b7hell", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "der See ...", "tokens": ["der", "See", "..."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Dazwischen", "tokens": ["Da\u00b7zwi\u00b7schen"], "token_info": ["word"], "pos": ["PAV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "schwarzblaue Kluft ...", "tokens": ["schwarz\u00b7blau\u00b7e", "Kluft", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Hinaus,", "tokens": ["Hin\u00b7aus", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "in den Nichtraum-Rau