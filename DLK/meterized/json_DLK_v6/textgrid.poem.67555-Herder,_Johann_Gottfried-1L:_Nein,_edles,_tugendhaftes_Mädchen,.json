{"textgrid.poem.67555": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nein, edles, tugendhaftes M\u00e4dchen,", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein, edles, tugendhaftes M\u00e4dchen,", "tokens": ["Nein", ",", "ed\u00b7les", ",", "tu\u00b7gend\u00b7haf\u00b7tes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nein, dieses warme Busenklopfen", "tokens": ["Nein", ",", "die\u00b7ses", "war\u00b7me", "Bu\u00b7sen\u00b7klop\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und diese schweren Odemz\u00fcge", "tokens": ["Und", "die\u00b7se", "schwe\u00b7ren", "O\u00b7dem\u00b7z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Macht nicht Dein helles, blaues Auge,", "tokens": ["Macht", "nicht", "Dein", "hel\u00b7les", ",", "blau\u00b7es", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht Deine wei\u00dfe, runde Wange,", "tokens": ["Nicht", "Dei\u00b7ne", "wei\u00b7\u00dfe", ",", "run\u00b7de", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht, nicht Dein sanfter, kleiner Busen.", "tokens": ["Nicht", ",", "nicht", "Dein", "sanf\u00b7ter", ",", "klei\u00b7ner", "Bu\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PTKNEG", "PPOSAT", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nein, edles, tugendhaftes M\u00e4dchen,", "tokens": ["Nein", ",", "ed\u00b7les", ",", "tu\u00b7gend\u00b7haf\u00b7tes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df ich Dich warm und z\u00e4rtlich liebe,", "tokens": ["Da\u00df", "ich", "Dich", "warm", "und", "z\u00e4rt\u00b7lich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ist nicht der Sinne wildes Feuer,", "tokens": ["Ist", "nicht", "der", "Sin\u00b7ne", "wil\u00b7des", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Liegt tiefer, tiefer in dem Herzen.", "tokens": ["Liegt", "tie\u00b7fer", ",", "tie\u00b7fer", "in", "dem", "Her\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df Du den Wahn aus dem Verstande,", "tokens": ["Da\u00df", "Du", "den", "Wahn", "aus", "dem", "Ver\u00b7stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Das Laster aus dem Herzen scheuchest", "tokens": ["Das", "Las\u00b7ter", "aus", "dem", "Her\u00b7zen", "scheu\u00b7chest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und tugendhaft und edel denkest", "tokens": ["Und", "tu\u00b7gend\u00b7haft", "und", "e\u00b7del", "den\u00b7kest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und tugendhaft und edel handelst,", "tokens": ["Und", "tu\u00b7gend\u00b7haft", "und", "e\u00b7del", "han\u00b7delst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Das macht warm klopfen meinen Busen,", "tokens": ["Das", "macht", "warm", "klop\u00b7fen", "mei\u00b7nen", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Das macht schwer steigen meinen Odem.", "tokens": ["Das", "macht", "schwer", "stei\u00b7gen", "mei\u00b7nen", "O\u00b7dem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Nun, edles, tugendhaftes M\u00e4dchen,", "tokens": ["Nun", ",", "ed\u00b7les", ",", "tu\u00b7gend\u00b7haf\u00b7tes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Glaubst Du, da\u00df ich aufrichtig liebe?", "tokens": ["Glaubst", "Du", ",", "da\u00df", "ich", "auf\u00b7rich\u00b7tig", "lie\u00b7be", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}