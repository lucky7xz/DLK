{"textgrid.poem.63655": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nein, das kannst du doch nicht im Ernste meinen,", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein, das kannst du doch nicht im Ernste meinen,", "tokens": ["Nein", ",", "das", "kannst", "du", "doch", "nicht", "im", "Erns\u00b7te", "mei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VMFIN", "PPER", "ADV", "PTKNEG", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df in Freundschaft die Liebe sei verwandelt,", "tokens": ["Da\u00df", "in", "Freund\u00b7schaft", "die", "Lie\u00b7be", "sei", "ver\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da ich l\u00e4ngst mich des guten Brauchs entw\u00f6hnte,", "tokens": ["Da", "ich", "l\u00e4ngst", "mich", "des", "gu\u00b7ten", "Brauchs", "ent\u00b7w\u00f6hn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich in z\u00e4rtlichen Liedern anzusingen.", "tokens": ["Dich", "in", "z\u00e4rt\u00b7li\u00b7chen", "Lie\u00b7dern", "an\u00b7zu\u00b7sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVIZU", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Pflegt doch selber die Nachtigall, sobald ihr", "tokens": ["Pflegt", "doch", "sel\u00b7ber", "die", "Nach\u00b7ti\u00b7gall", ",", "so\u00b7bald", "ihr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "ADV", "ADV", "ART", "NN", "$,", "KOUS", "PPER"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Nest sie baute, die lyrischen Erg\u00fcsse", "tokens": ["Nest", "sie", "bau\u00b7te", ",", "die", "ly\u00b7ri\u00b7schen", "Er\u00b7g\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "+-+---++-+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Ihrer schmachtenden Brautzeit einzustellen.", "tokens": ["Ih\u00b7rer", "schmach\u00b7ten\u00b7den", "Braut\u00b7zeit", "ein\u00b7zu\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVIZU", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Denn, sein Weibchen zu loben, ist mit Recht wohl", "tokens": ["Denn", ",", "sein", "Weib\u00b7chen", "zu", "lo\u00b7ben", ",", "ist", "mit", "Recht", "wohl"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "VAFIN", "APPR", "NN", "ADV"], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Unter M\u00e4nnern verp\u00f6nt als h\u00f6chst geschmacklos,", "tokens": ["Un\u00b7ter", "M\u00e4n\u00b7nern", "ver\u00b7p\u00f6nt", "als", "h\u00f6chst", "ge\u00b7schmack\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KOKOM", "ADV", "ADJD", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Da man stets in der Frau sich selber lobe.", "tokens": ["Da", "man", "stets", "in", "der", "Frau", "sich", "sel\u00b7ber", "lo\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "ART", "NN", "PRF", "ADV", "VVFIN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Ich nun vollends \u2013 verdient' ich deine Klage,", "tokens": ["Ich", "nun", "vol\u00b7lends", "\u2013", "ver\u00b7dient'", "ich", "dei\u00b7ne", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$(", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Da, gesteh es nur ein, auf all mein Dichten", "tokens": ["Da", ",", "ge\u00b7steh", "es", "nur", "ein", ",", "auf", "all", "mein", "Dich\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "So bedenklichen Einflu\u00df du ge\u00fcbt hast?", "tokens": ["So", "be\u00b7denk\u00b7li\u00b7chen", "Ein\u00b7flu\u00df", "du", "ge\u00b7\u00fcbt", "hast", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Ach, ich h\u00f6re von meinen hochwohlweisen", "tokens": ["Ach", ",", "ich", "h\u00f6\u00b7re", "von", "mei\u00b7nen", "hoch\u00b7wohl\u00b7wei\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Rezensenten noch heut den ew'gen Vorwurf,", "tokens": ["Re\u00b7zen\u00b7sen\u00b7ten", "noch", "heut", "den", "ew'\u00b7gen", "Vor\u00b7wurf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Da\u00df, so alt ich geworden, ich noch immer", "tokens": ["Da\u00df", ",", "so", "alt", "ich", "ge\u00b7wor\u00b7den", ",", "ich", "noch", "im\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "ADV", "ADJD", "PPER", "VAPP", "$,", "PPER", "ADV", "ADV"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.17": {"text": "Sch\u00f6nheitstrunken den schn\u00f6den Schattenseiten", "tokens": ["Sch\u00f6n\u00b7heits\u00b7trun\u00b7ken", "den", "schn\u00f6\u00b7den", "Schat\u00b7ten\u00b7sei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "Dieser heutigen Welt den R\u00fccken wende,", "tokens": ["Die\u00b7ser", "heu\u00b7ti\u00b7gen", "Welt", "den", "R\u00fc\u00b7cken", "wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Da\u00df insonderheit meine Fraungestalten", "tokens": ["Da\u00df", "in\u00b7son\u00b7der\u00b7heit", "mei\u00b7ne", "Fraun\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Stets von adligem Glanz umflossen schienen.", "tokens": ["Stets", "von", "ad\u00b7li\u00b7gem", "Glanz", "um\u00b7flos\u00b7sen", "schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Doch, Geliebteste, wenn in meiner Dichtung", "tokens": ["Doch", ",", "Ge\u00b7lieb\u00b7tes\u00b7te", ",", "wenn", "in", "mei\u00b7ner", "Dich\u00b7tung"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Holde weibliche Wesen dir begegnen,", "tokens": ["Hol\u00b7de", "weib\u00b7li\u00b7che", "We\u00b7sen", "dir", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "Liebevollen Gem\u00fcts, von feiner Klugheit,", "tokens": ["Lie\u00b7be\u00b7vol\u00b7len", "Ge\u00b7m\u00fcts", ",", "von", "fei\u00b7ner", "Klug\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.24": {"text": "Auf sich selber beruhend und so weiter \u2013", "tokens": ["Auf", "sich", "sel\u00b7ber", "be\u00b7ru\u00b7hend", "und", "so", "wei\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "VVPP", "KON", "ADV", "ADV", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.25": {"text": "Wer, du Zweiflerin, hat die gro\u00dfe S\u00fcnde", "tokens": ["Wer", ",", "du", "Zweif\u00b7le\u00b7rin", ",", "hat", "die", "gro\u00b7\u00dfe", "S\u00fcn\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "$,", "PPER", "NN", "$,", "VAFIN", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Meines Idealistentums verschuldet,", "tokens": ["Mei\u00b7nes", "I\u00b7dea\u00b7lis\u00b7ten\u00b7tums", "ver\u00b7schul\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Als die Eine, die mir an Durchschnittsweibern", "tokens": ["Als", "die", "Ei\u00b7ne", ",", "die", "mir", "an", "Durch\u00b7schnitts\u00b7wei\u00b7bern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.28": {"text": "Ein f\u00fcr allemal den Geschmack verdorben?", "tokens": ["Ein", "f\u00fcr", "al\u00b7le\u00b7mal", "den", "Ge\u00b7schmack", "ver\u00b7dor\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}}}}