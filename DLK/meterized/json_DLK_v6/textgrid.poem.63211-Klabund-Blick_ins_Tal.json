{"textgrid.poem.63211": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Blick ins Tal", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lass, o lass mich niedersinken", "tokens": ["Lass", ",", "o", "lass", "mich", "nie\u00b7der\u00b7sin\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "FM", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein Tropfen Tau im Hain.", "tokens": ["Wie", "ein", "Trop\u00b7fen", "Tau", "im", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Berge bl\u00fchen, Wipfel winken,", "tokens": ["Ber\u00b7ge", "bl\u00fc\u00b7hen", ",", "Wip\u00b7fel", "win\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich bin nicht mehr allein.", "tokens": ["Und", "ich", "bin", "nicht", "mehr", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Spukt im Mond, ihr halben Helden!", "tokens": ["Spukt", "im", "Mond", ",", "ihr", "hal\u00b7ben", "Hel\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wind und Wolke l\u00fcgen nicht.", "tokens": ["Wind", "und", "Wol\u00b7ke", "l\u00fc\u00b7gen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine Glockenstrophen melden,", "tokens": ["Kei\u00b7ne", "Glo\u00b7ckens\u00b7tro\u00b7phen", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn ein Enzianauge bricht.", "tokens": ["Wenn", "ein", "En\u00b7zi\u00b7a\u00b7nau\u00b7ge", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Menschen hatten zarte Seelen,", "tokens": ["Men\u00b7schen", "hat\u00b7ten", "zar\u00b7te", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon ein Nadelstich traf Blut ...", "tokens": ["Schon", "ein", "Na\u00b7del\u00b7stich", "traf", "Blut", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Am Gestein sollst du dich st\u00e4hlen,", "tokens": ["Am", "Ge\u00b7stein", "sollst", "du", "dich", "st\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und im Felsen werde gut!", "tokens": ["Und", "im", "Fel\u00b7sen", "wer\u00b7de", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Steinschlag soll das Tal entmannen,", "tokens": ["Stein\u00b7schlag", "soll", "das", "Tal", "ent\u00b7man\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Lau es \u00fcberwehn \u2013", "tokens": ["Und", "die", "Lau", "es", "\u00fc\u00b7berw\u00b7ehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In Narzissen und in Tannen", "tokens": ["In", "Nar\u00b7zis\u00b7sen", "und", "in", "Tan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird es himmlisch auferstehn.", "tokens": ["Wird", "es", "himm\u00b7lisch", "auf\u00b7er\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}