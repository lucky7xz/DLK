{"textgrid.poem.46816": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wie sch\u00f6n die Blumen bl\u00fchn]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sch\u00f6n die Blumen bl\u00fchn", "tokens": ["Wie", "sch\u00f6n", "die", "Blu\u00b7men", "bl\u00fchn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Im Garten frisch und gr\u00fcn,", "tokens": ["Im", "Gar\u00b7ten", "frisch", "und", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sch\u00f6ner kein' als die Rose,", "tokens": ["Sch\u00f6\u00b7ner", "kein'", "als", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "KOKOM", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Die sich kr\u00e4nzet mit Mose.", "tokens": ["Die", "sich", "kr\u00e4n\u00b7zet", "mit", "Mo\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "APPR", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Wie schnell die Blumen bl\u00fchn,", "tokens": ["Wie", "schnell", "die", "Blu\u00b7men", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie hell die Sonnen gl\u00fchn,", "tokens": ["Wie", "hell", "die", "Son\u00b7nen", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Blumen welken, die Rose", "tokens": ["Die", "Blu\u00b7men", "wel\u00b7ken", ",", "die", "Ro\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Welkt mitten im k\u00fchlen Mose.", "tokens": ["Welkt", "mit\u00b7ten", "im", "k\u00fch\u00b7len", "Mo\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie schnell die Blumen verbl\u00fchn,", "tokens": ["Wie", "schnell", "die", "Blu\u00b7men", "ver\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Die Regentropfen spr\u00fchn,", "tokens": ["Die", "Re\u00b7gen\u00b7trop\u00b7fen", "spr\u00fchn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie weinen um die Rose,", "tokens": ["Sie", "wei\u00b7nen", "um", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die liegt auf dem feuchten Mose.", "tokens": ["Die", "liegt", "auf", "dem", "feuch\u00b7ten", "Mo\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie die Blumen verbl\u00fchn!", "tokens": ["Wie", "die", "Blu\u00b7men", "ver\u00b7bl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Umsonst sind Liebesm\u00fchn,", "tokens": ["Um\u00b7sonst", "sind", "Lie\u00b7bes\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Unseres Lebens Rose", "tokens": ["Un\u00b7se\u00b7res", "Le\u00b7bens", "Ro\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Liegt unter dem feuchten Mose.", "tokens": ["Liegt", "un\u00b7ter", "dem", "feuch\u00b7ten", "Mo\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Wie die Blumen erbl\u00fchn,", "tokens": ["Wie", "die", "Blu\u00b7men", "er\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der Garten ist immer gr\u00fcn,", "tokens": ["Der", "Gar\u00b7ten", "ist", "im\u00b7mer", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Unserer Liebe Rose", "tokens": ["Un\u00b7se\u00b7rer", "Lie\u00b7be", "Ro\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NE"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Bl\u00fcht uns neu aus dem Mose.", "tokens": ["Bl\u00fcht", "uns", "neu", "aus", "dem", "Mo\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}