",", "ich", "schlie\u00df", "es", "bald", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Buch, es hei\u00dft: Erinnerung!", "tokens": ["Das", "Buch", ",", "es", "hei\u00dft", ":", "E\u00b7rin\u00b7ne\u00b7rung", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "$.", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Mit T\u00e4uschungen bemalt.", "tokens": ["Mit", "T\u00e4u\u00b7schun\u00b7gen", "be\u00b7malt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}