{"textgrid.poem.55400": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Aus Byrons Manfred", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Mond ist auf der Welle,", "tokens": ["Wenn", "der", "Mond", "ist", "auf", "der", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Gl\u00fchwurm ist im Gras", "tokens": ["Wenn", "der", "Gl\u00fch\u00b7wurm", "ist", "im", "Gras"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Scheinlicht auf dem Grabe,", "tokens": ["Und", "ein", "Schein\u00b7licht", "auf", "dem", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Irres Licht auf dem Morast,", "tokens": ["Ir\u00b7res", "Licht", "auf", "dem", "Mo\u00b7rast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die Sterne fallend schie\u00dfen,", "tokens": ["Wenn", "die", "Ster\u00b7ne", "fal\u00b7lend", "schie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eule der Eul erwidernd heult", "tokens": ["Eu\u00b7le", "der", "Eul", "er\u00b7wi\u00b7dernd", "heult"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "VVPP", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und die Bl\u00e4tter schweigend ruhen", "tokens": ["Und", "die", "Bl\u00e4t\u00b7ter", "schwei\u00b7gend", "ru\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "An des dunkeln H\u00fcgels Wand,", "tokens": ["An", "des", "dun\u00b7keln", "H\u00fc\u00b7gels", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Meine Seel sei auf der deinen", "tokens": ["Mei\u00b7ne", "Seel", "sei", "auf", "der", "dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mit Gewalt und Zeichenwink!", "tokens": ["Mit", "Ge\u00b7walt", "und", "Zei\u00b7chen\u00b7wink", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist dein Schlummer noch so tief,", "tokens": ["Ist", "dein", "Schlum\u00b7mer", "noch", "so", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt dein Geist doch nie zum Schlaf.", "tokens": ["Kommt", "dein", "Geist", "doch", "nie", "zum", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da sind Schatten, die nicht schwinden,", "tokens": ["Da", "sind", "Schat\u00b7ten", ",", "die", "nicht", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "PRELS", "PTKNEG", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Da Gedanken, die nicht bannest.", "tokens": ["Da", "Ge\u00b7dan\u00b7ken", ",", "die", "nicht", "ban\u00b7nest", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Gewalt, die du nicht kennest,", "tokens": ["Die", "Ge\u00b7walt", ",", "die", "du", "nicht", "ken\u00b7nest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00e4\u00dft dich nimmermehr allein.", "tokens": ["L\u00e4\u00dft", "dich", "nim\u00b7mer\u00b7mehr", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bist ins Leichentuch gewindelt,", "tokens": ["Bist", "ins", "Lei\u00b7chen\u00b7tuch", "ge\u00b7win\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eingeh\u00fcllt in einer Wolke,", "tokens": ["Ein\u00b7ge\u00b7h\u00fcllt", "in", "ei\u00b7ner", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und f\u00fcr immer, immer wohnst du", "tokens": ["Und", "f\u00fcr", "im\u00b7mer", ",", "im\u00b7mer", "wohnst", "du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "$,", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "In dem Geiste dieses Spruchs.", "tokens": ["In", "dem", "Geis\u00b7te", "die\u00b7ses", "Spruchs", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Siehst mich nicht vor\u00fcbergehen,", "tokens": ["Siehst", "mich", "nicht", "vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchlst mich doch in deinem Auge", "tokens": ["F\u00fchlst", "mich", "doch", "in", "dei\u00b7nem", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ein Ding, das ungesehen", "tokens": ["Als", "ein", "Ding", ",", "das", "un\u00b7ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nah dir sein mu\u00df, wie es war;", "tokens": ["Nah", "dir", "sein", "mu\u00df", ",", "wie", "es", "war", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VAINF", "VMFIN", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wenn du, geheim durchschaudert,", "tokens": ["Und", "wenn", "du", ",", "ge\u00b7heim", "durch\u00b7schau\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,", "ADJD", "VVPP", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.6": {"text": "Deinen Kopf umwendend blickest,", "tokens": ["Dei\u00b7nen", "Kopf", "um\u00b7wen\u00b7dend", "bli\u00b7ckest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sollst dich wundern, da\u00df nicht etwa", "tokens": ["Sollst", "dich", "wun\u00b7dern", ",", "da\u00df", "nicht", "et\u00b7wa"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PRF", "VVINF", "$,", "KOUS", "PTKNEG", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Wie ein Schatten bin zur Stelle;", "tokens": ["Wie", "ein", "Schat\u00b7ten", "bin", "zur", "Stel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nein! die Kraft, die du empfunden,", "tokens": ["Nein", "!", "die", "Kraft", ",", "die", "du", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ist, was sich in dir verbirgt.", "tokens": ["Ist", ",", "was", "sich", "in", "dir", "ver\u00b7birgt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und ein Zauberwort und Lied", "tokens": ["Und", "ein", "Zau\u00b7ber\u00b7wort", "und", "Lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Taufte dich mit einem Fluch,", "tokens": ["Tauf\u00b7te", "dich", "mit", "ei\u00b7nem", "Fluch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und schon hat ein Geist der Luft", "tokens": ["Und", "schon", "hat", "ein", "Geist", "der", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich umgarnt mit einer Schlinge.", "tokens": ["Dich", "um\u00b7garnt", "mit", "ei\u00b7ner", "Schlin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In dem Wind ist eine Stimme,", "tokens": ["In", "dem", "Wind", "ist", "ei\u00b7ne", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die verbeut dir, dich zu freuen.", "tokens": ["Die", "ver\u00b7beut", "dir", ",", "dich", "zu", "freu\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wenn dir die Nacht versagt", "tokens": ["Und", "wenn", "dir", "die", "Nacht", "ver\u00b7sagt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVPP"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Ihres reinen Himmels Ruhe,", "tokens": ["Ih\u00b7res", "rei\u00b7nen", "Him\u00b7mels", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Bringt der Tag eine Sonn herauf,", "tokens": ["Bringt", "der", "Tag", "ei\u00b7ne", "Sonn", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "W\u00e4r sie nieder! w\u00fcnschest du.", "tokens": ["W\u00e4r", "sie", "nie\u00b7der", "!", "w\u00fcn\u00b7schest", "du", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKVZ", "$.", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Deinen falschen Tr\u00e4nen zog ich", "tokens": ["Dei\u00b7nen", "fal\u00b7schen", "Tr\u00e4\u00b7nen", "zog", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00f6dlichste Essenzen aus,", "tokens": ["T\u00f6d\u00b7lichs\u00b7te", "Es\u00b7sen\u00b7zen", "aus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Deinem eignen Herzen sog ich", "tokens": ["Dei\u00b7nem", "eig\u00b7nen", "Her\u00b7zen", "sog", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Blut, das schw\u00e4rzeste, vom Quell,", "tokens": ["Blut", ",", "das", "schw\u00e4r\u00b7zes\u00b7te", ",", "vom", "Quell", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deinem L\u00e4cheln lockt ich Schlangen,", "tokens": ["Dei\u00b7nem", "L\u00e4\u00b7cheln", "lockt", "ich", "Schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dort geheim geringelt, ab,", "tokens": ["Dort", "ge\u00b7heim", "ge\u00b7rin\u00b7gelt", ",", "ab", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deinem Lippenpaar entsaugt ich", "tokens": ["Dei\u00b7nem", "Lip\u00b7pen\u00b7paar", "ent\u00b7saugt", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Allerschlimmstes aller Gifte.", "tokens": ["Al\u00b7ler\u00b7schlimms\u00b7tes", "al\u00b7ler", "Gif\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Jedem Gift, das ich erprobet,", "tokens": ["Je\u00b7dem", "Gift", ",", "das", "ich", "er\u00b7pro\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Schlimmer ist dein eignes doch.", "tokens": ["Schlim\u00b7mer", "ist", "dein", "eig\u00b7nes", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Bei deiner kalten Brust, dem Schlangenl\u00e4cheln,", "tokens": ["Bei", "dei\u00b7ner", "kal\u00b7ten", "Brust", ",", "dem", "Schlan\u00b7gen\u00b7l\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Arglist unergr\u00fcndlichem Schlund,", "tokens": ["Der", "Arg\u00b7list", "un\u00b7er\u00b7gr\u00fcnd\u00b7li\u00b7chem", "Schlund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Bei dem so tugendsam scheinenden Auge,", "tokens": ["Bei", "dem", "so", "tu\u00b7gend\u00b7sam", "schei\u00b7nen\u00b7den", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bei der verschlossenen Seele Trug,", "tokens": ["Bei", "der", "ver\u00b7schlos\u00b7se\u00b7nen", "See\u00b7le", "Trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Bei der Vollendung deiner K\u00fcnste,", "tokens": ["Bei", "der", "Vol\u00b7len\u00b7dung", "dei\u00b7ner", "K\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Dem Wahn, du tragest ein menschliches Herz,", "tokens": ["Dem", "Wahn", ",", "du", "tra\u00b7gest", "ein", "menschli\u00b7ches", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Bei deinem Gefallen an anderer Pein,", "tokens": ["Bei", "dei\u00b7nem", "Ge\u00b7fal\u00b7len", "an", "an\u00b7de\u00b7rer", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Bei deiner Kains-Bruderschaft", "tokens": ["Bei", "dei\u00b7ner", "Kains\u00b7Bru\u00b7der\u00b7schaft"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Beschw\u00f6re ich dich und n\u00f6tige", "tokens": ["Be\u00b7schw\u00f6\u00b7re", "ich", "dich", "und", "n\u00f6\u00b7ti\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "KON", "ADJA"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Dich, selbst dir eigne H\u00f6lle zu sein!", "tokens": ["Dich", ",", "selbst", "dir", "eig\u00b7ne", "H\u00f6l\u00b7le", "zu", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "PPER", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Auf dein Haupt gie\u00df ich die Schale,", "tokens": ["Auf", "dein", "Haupt", "gie\u00df", "ich", "die", "Scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die dich solchem Urteil widmet:", "tokens": ["Die", "dich", "sol\u00b7chem", "Ur\u00b7teil", "wid\u00b7met", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht zu schlafen, nicht zu sterben", "tokens": ["Nicht", "zu", "schla\u00b7fen", ",", "nicht", "zu", "ster\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "PTKZU", "VVINF", "$,", "PTKNEG", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei dein dauernd Mi\u00dfgeschick;", "tokens": ["Sei", "dein", "dau\u00b7ernd", "Mi\u00df\u00b7ge\u00b7schick", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Scheinbar soll der Tod sich nahen", "tokens": ["Schein\u00b7bar", "soll", "der", "Tod", "sich", "na\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "ART", "NN", "PRF", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deinem Wunsch, doch nur als Grauen.", "tokens": ["Dei\u00b7nem", "Wunsch", ",", "doch", "nur", "als", "Grau\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADV", "KOUS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schau! der Zauber wirkt umher dir,", "tokens": ["Schau", "!", "der", "Zau\u00b7ber", "wirkt", "um\u00b7her", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "PTKVZ", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dich geklirrlos fesselt Kette;", "tokens": ["Dich", "ge\u00b7klirr\u00b7los", "fes\u00b7selt", "Ket\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "\u00dcber Herz und Hirn zusammen", "tokens": ["\u00dc\u00b7ber", "Herz", "und", "Hirn", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ist der Spruch ergangen \u2013 schwinde!", "tokens": ["Ist", "der", "Spruch", "er\u00b7gan\u00b7gen", "\u2013", "schwin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$(", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}