{"textgrid.poem.55472": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "53", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lustrum ist ein fremdes Wort!", "tokens": ["Lust\u00b7rum", "ist", "ein", "frem\u00b7des", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber wenn wir sagen:", "tokens": ["A\u00b7ber", "wenn", "wir", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Lustra haben wir am Ort", "tokens": ["Lus\u00b7tra", "ha\u00b7ben", "wir", "am", "Ort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Acht bis neun ertragen", "tokens": ["Acht", "bis", "neun", "er\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "APPR", "CARD", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und genossen und gelebt", "tokens": ["Und", "ge\u00b7nos\u00b7sen", "und", "ge\u00b7lebt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVPP", "KON", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und geliebt bisweilen,", "tokens": ["Und", "ge\u00b7liebt", "bis\u00b7wei\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVPP", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wird, wer nach dem Gleichen strebt,", "tokens": ["Wird", ",", "wer", "nach", "dem", "Glei\u00b7chen", "strebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Heute mit uns teilen,", "tokens": ["Heu\u00b7te", "mit", "uns", "tei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn wir sagen: Das ist viel!", "tokens": ["Wenn", "wir", "sa\u00b7gen", ":", "Das", "ist", "viel", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "$.", "PDS", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn das Leben streuet", "tokens": ["Denn", "das", "Le\u00b7ben", "streu\u00b7et"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Blum und Dorne! \u2013 Ziel ist Ziel!", "tokens": ["Blum", "und", "Dor\u00b7ne", "!", "\u2013", "Ziel", "ist", "Ziel", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$.", "$(", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Das uns heute freuet.", "tokens": ["Das", "uns", "heu\u00b7te", "freu\u00b7et", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}