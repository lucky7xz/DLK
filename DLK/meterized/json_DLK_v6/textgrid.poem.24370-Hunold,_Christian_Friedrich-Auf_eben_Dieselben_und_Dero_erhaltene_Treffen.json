{"textgrid.poem.24370": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf eben Dieselben und Dero erhaltene Treffen", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Muth beklagte sich: das Gl\u00fcck sey ihm entgegen.", "tokens": ["Der", "Muth", "be\u00b7klag\u00b7te", "sich", ":", "das", "Gl\u00fcck", "sey", "ihm", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$.", "ART", "NN", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In meine Fessel kan ich es nicht immer legen/", "tokens": ["In", "mei\u00b7ne", "Fes\u00b7sel", "kan", "ich", "es", "nicht", "im\u00b7mer", "le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "PPER", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gab Kingheit auch darzu; und die Erfahrung sprach:", "tokens": ["Gab", "King\u00b7heit", "auch", "dar\u00b7zu", ";", "und", "die", "Er\u00b7fah\u00b7rung", "sprach", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "PAV", "$.", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Gl\u00fcck verschaffet wir den meisten Ungemach.", "tokens": ["Das", "Gl\u00fcck", "ver\u00b7schaf\u00b7fet", "wir", "den", "meis\u00b7ten", "Un\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bey diesen dreyen lie\u00df hierauf das Gl\u00fcck sich melden/", "tokens": ["Bey", "die\u00b7sen", "drey\u00b7en", "lie\u00df", "hier\u00b7auf", "das", "Gl\u00fcck", "sich", "mel\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "CARD", "VVFIN", "PAV", "ART", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sprach: so gebet mir denn einen solchen Helden/", "tokens": ["Und", "sprach", ":", "so", "ge\u00b7bet", "mir", "denn", "ei\u00b7nen", "sol\u00b7chen", "Hel\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ADV", "ART", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In dem ihr alle drey vollkommen seyd zu sehn/", "tokens": ["In", "dem", "ihr", "al\u00b7le", "drey", "voll\u00b7kom\u00b7men", "seyd", "zu", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIAT", "CARD", "ADJD", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bey dessen Degen soll alsdenn mein Scepter stehn.", "tokens": ["Bey", "des\u00b7sen", "De\u00b7gen", "soll", "als\u00b7denn", "mein", "Scep\u00b7ter", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So sehe/ sagten Sie/ ", "tokens": ["So", "se\u00b7he", "/", "sag\u00b7ten", "Sie", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "VVFIN", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das Gl\u00fccke sah' ihn an/ es fand ihn ungemein/", "tokens": ["Das", "Gl\u00fc\u00b7cke", "sah'", "ihn", "an", "/", "es", "fand", "ihn", "un\u00b7ge\u00b7mein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$(", "PPER", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sprach in ihn verliebt: der tapfre Printz soll siegen/", "tokens": ["Und", "sprach", "in", "ihn", "ver\u00b7liebt", ":", "der", "tapf\u00b7re", "Printz", "soll", "sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "VVPP", "$.", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und durch uns viere nun un\u00fcberwindlich seyn.", "tokens": ["Und", "durch", "uns", "vie\u00b7re", "nun", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PIS", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}