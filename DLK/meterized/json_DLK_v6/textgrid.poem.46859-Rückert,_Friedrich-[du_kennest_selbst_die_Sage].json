{"textgrid.poem.46859": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[du kennest selbst die Sage]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kennest selbst die Sage", "tokens": ["Du", "ken\u00b7nest", "selbst", "die", "Sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom vielbeweinten Kind,", "tokens": ["Vom", "viel\u00b7be\u00b7wein\u00b7ten", "Kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von der an diesem Tage", "tokens": ["Von", "der", "an", "die\u00b7sem", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir auch gemeinet sind.", "tokens": ["Wir", "auch", "ge\u00b7mei\u00b7net", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ums Kind die Mutter weinet", "tokens": ["Ums", "Kind", "die", "Mut\u00b7ter", "wei\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So sehr ohn' Unterla\u00df,", "tokens": ["So", "sehr", "ohn'", "Un\u00b7ter\u00b7la\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis ihr das Kind erscheinet", "tokens": ["Bis", "ihr", "das", "Kind", "er\u00b7schei\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Todtenhemdlein na\u00df.", "tokens": ["Im", "Tod\u00b7ten\u00b7hemd\u00b7lein", "na\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "La\u00df deine Thr\u00e4nen stocken,", "tokens": ["La\u00df", "dei\u00b7ne", "Thr\u00e4\u00b7nen", "sto\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Mutter, wisch' sie ab!", "tokens": ["O", "Mut\u00b7ter", ",", "wisch'", "sie", "ab", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sonst wird das Hemd nie trocken", "tokens": ["Sonst", "wird", "das", "Hemd", "nie", "tro\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Deinem Kindlein im Grab.", "tokens": ["Dei\u00b7nem", "Kin\u00b7dlein", "im", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}