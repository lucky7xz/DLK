{"textgrid.poem.66844": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Scheu vom Nachtwind flackert der Lampe Schein,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Scheu vom Nachtwind flackert der Lampe Schein,", "tokens": ["Scheu", "vom", "Nacht\u00b7wind", "fla\u00b7ckert", "der", "Lam\u00b7pe", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "M\u00fcde schwankt das rote L\u00f6schblatt nieder.", "tokens": ["M\u00fc\u00b7de", "schwankt", "das", "ro\u00b7te", "L\u00f6schblatt", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Meines Buches tote Ziffernreihn", "tokens": ["Mei\u00b7nes", "Bu\u00b7ches", "to\u00b7te", "Zif\u00b7fern\u00b7reihn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Knicken wunderlich die hagern Glieder;", "tokens": ["Kni\u00b7cken", "wun\u00b7der\u00b7lich", "die", "ha\u00b7gern", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Riesenmassen sch\u00fctteln Fleisch und Bein,", "tokens": ["Rie\u00b7sen\u00b7mas\u00b7sen", "sch\u00fct\u00b7teln", "Fleisch", "und", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Millionen Zahlen zuckend schrein:", "tokens": ["Mil\u00b7lion\u00b7en", "Zah\u00b7len", "zu\u00b7ckend", "schrein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dichter, weckt dein warmes Blut uns wieder?", "tokens": ["Dich\u00b7ter", ",", "weckt", "dein", "war\u00b7mes", "Blut", "uns", "wie\u00b7der", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "F\u00fchle, f\u00fchle deiner Zahlen Pein,", "tokens": ["F\u00fch\u00b7le", ",", "f\u00fch\u00b7le", "dei\u00b7ner", "Zah\u00b7len", "Pein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Unsrer Qualen hochgesummte Summen!", "tokens": ["Uns\u00b7rer", "Qua\u00b7len", "hoch\u00b7ge\u00b7summ\u00b7te", "Sum\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "W\u00fchle, w\u00fchle sie zum Hirn hinein,", "tokens": ["W\u00fch\u00b7le", ",", "w\u00fch\u00b7le", "sie", "zum", "Hirn", "hin\u00b7ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Da\u00df wir nimmer, nimmermehr verstummen!", "tokens": ["Da\u00df", "wir", "nim\u00b7mer", ",", "nim\u00b7mer\u00b7mehr", "ver\u00b7stum\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sieh die Linie, wie sie Zickzack steigt,", "tokens": ["Sieh", "die", "Li\u00b7nie", ",", "wie", "sie", "Zick\u00b7zack", "steigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PWAV", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Hunger, Wahnsinn und Verbrechen zeigt!", "tokens": ["Hun\u00b7ger", ",", "Wahn\u00b7sinn", "und", "Ver\u00b7bre\u00b7chen", "zeigt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wandle sie, die dunklen Spuren!", "tokens": ["Wand\u00b7le", "sie", ",", "die", "dunk\u00b7len", "Spu\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei dem Geisterlumpentro\u00df,", "tokens": ["Sei", "dem", "Geis\u00b7ter\u00b7lum\u00b7pen\u00b7tro\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieben, M\u00f6rdern, S\u00e4ufern, Huren,", "tokens": ["Die\u00b7ben", ",", "M\u00f6r\u00b7dern", ",", "S\u00e4u\u00b7fern", ",", "Hu\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDAT", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Spie\u00dfgesell und Mordgeno\u00df!", "tokens": ["Spie\u00df\u00b7ge\u00b7sell", "und", "Mord\u00b7ge\u00b7no\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo der Fleischtopf \u00fcppig dampft,", "tokens": ["Wo", "der", "Fleischtopf", "\u00fcp\u00b7pig", "dampft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Reibt die \u00bbTugend\u00ab sich den Wanst,", "tokens": ["Reibt", "die", "\u00bb", "Tu\u00b7gend", "\u00ab", "sich", "den", "Wanst", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "$(", "NN", "$(", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "M\u00e4gen, die der Hunger krampft,", "tokens": ["M\u00e4\u00b7gen", ",", "die", "der", "Hun\u00b7ger", "krampft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "H\u00e4lt der Teufel schlau umschanzt.", "tokens": ["H\u00e4lt", "der", "Teu\u00b7fel", "schlau", "um\u00b7schanzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wie sie grinsen, meine Zahlen,", "tokens": ["Wie", "sie", "grin\u00b7sen", ",", "mei\u00b7ne", "Zah\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Nackt und spindeld\u00fcrre hupfen,", "tokens": ["Nackt", "und", "spin\u00b7del\u00b7d\u00fcr\u00b7re", "hup\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Aus zerschossnen Idealen", "tokens": ["Aus", "zer\u00b7schoss\u00b7nen", "I\u00b7dea\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.14": {"text": "Federn \u00fcber Federn rupfen!", "tokens": ["Fe\u00b7dern", "\u00fc\u00b7ber", "Fe\u00b7dern", "rup\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Sieh, nun reihen sich zwei Lager,", "tokens": ["Sieh", ",", "nun", "rei\u00b7hen", "sich", "zwei", "La\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PRF", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Hier die \u00bbGuten\u00ab, dort die \u00bbSchlechten\u00ab,", "tokens": ["Hier", "die", "\u00bb", "Gu\u00b7ten", "\u00ab", ",", "dort", "die", "\u00bb", "Schlech\u00b7ten", "\u00ab", ","], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "ART", "$(", "NN", "$(", "$,", "ADV", "ART", "$(", "NN", "$(", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.17": {"text": "Meistens sind die \u00bbS\u00fcnder\u00ab mager,", "tokens": ["Meis\u00b7tens", "sind", "die", "\u00bb", "S\u00fcn\u00b7der", "\u00ab", "ma\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "$(", "NN", "$(", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Fett sind meistens die \u00bbGerechten\u00ab.", "tokens": ["Fett", "sind", "meis\u00b7tens", "die", "\u00bb", "Ge\u00b7rech\u00b7ten", "\u00ab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "$(", "NN", "$(", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Habe nie den Gott ergr\u00fcndet,", "tokens": ["Ha\u00b7be", "nie", "den", "Gott", "er\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Der von \u00bbSchuld\u00ab und \u00bbUnschuld\u00ab wei\u00df,", "tokens": ["Der", "von", "\u00bb", "Schuld", "\u00ab", "und", "\u00bb", "Un\u00b7schuld", "\u00ab", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "APPR", "$(", "NN", "$(", "KON", "$(", "NN", "$(", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Besser scheint mir schon verk\u00fcndet:", "tokens": ["Bes\u00b7ser", "scheint", "mir", "schon", "ver\u00b7k\u00fcn\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "\u00bbgott und der Getreidepreis.\u00ab", "tokens": ["\u00bb", "gott", "und", "der", "Ge\u00b7trei\u00b7de\u00b7preis", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "KON", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Und in toll und tollerm Ritte", "tokens": ["Und", "in", "toll", "und", "tol\u00b7lerm", "Rit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJD", "KON", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.24": {"text": "\u00dcberschlagen sich die Laster,", "tokens": ["\u00dc\u00b7bersc\u00b7hla\u00b7gen", "sich", "die", "Las\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Wuchert Reichtum, welkt die Sitte,", "tokens": ["Wu\u00b7chert", "Reich\u00b7tum", ",", "welkt", "die", "Sit\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Hungers\u00fcnden heilt kein Paster.", "tokens": ["Hun\u00b7ger\u00b7s\u00fcn\u00b7den", "heilt", "kein", "Pas\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Wie die Branntweinfluten schwellen!", "tokens": ["Wie", "die", "Brannt\u00b7wein\u00b7flu\u00b7ten", "schwel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Wie die Brenner Satan gr\u00fc\u00dfen!", "tokens": ["Wie", "die", "Bren\u00b7ner", "Sa\u00b7tan", "gr\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Kahlgeschorene Gesellen", "tokens": ["Kahl\u00b7ge\u00b7scho\u00b7re\u00b7ne", "Ge\u00b7sel\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.30": {"text": "M\u00fcssen f\u00fcr uns alle b\u00fc\u00dfen.", "tokens": ["M\u00fcs\u00b7sen", "f\u00fcr", "uns", "al\u00b7le", "b\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Kindesunschuld, w\u00fcst gesch\u00e4ndet,", "tokens": ["Kin\u00b7des\u00b7un\u00b7schuld", ",", "w\u00fcst", "ge\u00b7sch\u00e4n\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Wirbelt in der H\u00f6lle Strudel,", "tokens": ["Wir\u00b7belt", "in", "der", "H\u00f6l\u00b7le", "Stru\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.33": {"text": "B\u00fcrgerbauch stolziert verblendet", "tokens": ["B\u00fcr\u00b7ger\u00b7bauch", "stol\u00b7ziert", "ver\u00b7blen\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Wie ein wohl dressierter Pudel.", "tokens": ["Wie", "ein", "wohl", "dres\u00b7sier\u00b7ter", "Pu\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADV", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.35": {"text": "Ach, der gute, der honette Rentner,", "tokens": ["Ach", ",", "der", "gu\u00b7te", ",", "der", "ho\u00b7net\u00b7te", "Rent\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.36": {"text": "Unbescholten, strahlt er wei\u00df wie Schnee,", "tokens": ["Un\u00b7be\u00b7schol\u00b7ten", ",", "strahlt", "er", "wei\u00df", "wie", "Schnee", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "VVFIN", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.37": {"text": "Trostlos schleppt der Strolch den Schicksalszentner,", "tokens": ["Trost\u00b7los", "schleppt", "der", "Strolch", "den", "Schick\u00b7sals\u00b7zent\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.38": {"text": "Ehrlos fri\u00dft er in sich Wut und Weh.", "tokens": ["Ehr\u00b7los", "fri\u00dft", "er", "in", "sich", "Wut", "und", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PRF", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.39": {"text": "\u00bbarbeit! Arbeit!\u00ab Seine Faust, sie zittert,", "tokens": ["\u00bb", "ar\u00b7beit", "!", "Ar\u00b7beit", "!", "\u00ab", "Sei\u00b7ne", "Faust", ",", "sie", "zit\u00b7tert", ","], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NN", "$.", "NN", "$.", "$(", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.40": {"text": "Klirrend schmettert sie durchs Ladenfenster.", "tokens": ["Klir\u00b7rend", "schmet\u00b7tert", "sie", "durchs", "La\u00b7den\u00b7fens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.41": {"text": "Gott Gesellschaft h\u00e4lt ihn gut vergittert,", "tokens": ["Gott", "Ge\u00b7sell\u00b7schaft", "h\u00e4lt", "ihn", "gut", "ver\u00b7git\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.42": {"text": "Gott Gesellschaft kettet die Gespenster.", "tokens": ["Gott", "Ge\u00b7sell\u00b7schaft", "ket\u00b7tet", "die", "Ge\u00b7spens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", 