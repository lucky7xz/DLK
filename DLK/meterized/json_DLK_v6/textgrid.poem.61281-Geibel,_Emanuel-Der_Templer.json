{"textgrid.poem.61281": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Der Templer", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durchs Haus des Ordens bei des Tags Verf\u00e4rben", "tokens": ["Durchs", "Haus", "des", "Or\u00b7dens", "bei", "des", "Tags", "Ver\u00b7f\u00e4r\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schleicht unheilvolle Kunde hin und her:", "tokens": ["Schleicht", "un\u00b7heil\u00b7vol\u00b7le", "Kun\u00b7de", "hin", "und", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbder Tempelmeister Odo liegt im Sterben.\u00ab", "tokens": ["\u00bb", "der", "Tem\u00b7pel\u00b7meis\u00b7ter", "O\u00b7do", "liegt", "im", "Ster\u00b7ben", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "NE", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und jedem, der sie h\u00f6rt, bew\u00f6lkt sich schwer", "tokens": ["Und", "je\u00b7dem", ",", "der", "sie", "h\u00f6rt", ",", "be\u00b7w\u00f6lkt", "sich", "schwer"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die heitre Stirn, und seine Lippen fragen:", "tokens": ["Die", "heit\u00b7re", "Stirn", ",", "und", "sei\u00b7ne", "Lip\u00b7pen", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbist's m\u00f6glich? ", "tokens": ["\u00bb", "ist's", "m\u00f6g\u00b7lich", "?"], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Er geht dahin, der noch vor wenig Tagen", "tokens": ["Er", "geht", "da\u00b7hin", ",", "der", "noch", "vor", "we\u00b7nig", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "$,", "PRELS", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den wilden Berberhengst zu st\u00f6hnen zwang,", "tokens": ["Den", "wil\u00b7den", "Ber\u00b7ber\u00b7hengst", "zu", "st\u00f6h\u00b7nen", "zwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der mit der Faust den Panther j\u00fcngst erschlagen?", "tokens": ["Der", "mit", "der", "Faust", "den", "Pan\u00b7ther", "j\u00fcngst", "er\u00b7schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der in der Feldschlacht wildverworrnem Drang,", "tokens": ["Der", "in", "der", "Feld\u00b7schlacht", "wild\u00b7ver\u00b7worr\u00b7nem", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bespritzt mit Blut bis zu den G\u00fcrtelschnallen,", "tokens": ["Be\u00b7spritzt", "mit", "Blut", "bis", "zu", "den", "G\u00fcr\u00b7tel\u00b7schnal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu Todesstreichen Liebeslieder sang?", "tokens": ["Zu", "To\u00b7des\u00b7strei\u00b7chen", "Lie\u00b7bes\u00b7lie\u00b7der", "sang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Auch er! So soll er nie beim W\u00fcrfelfallen", "tokens": ["Auch", "er", "!", "So", "soll", "er", "nie", "beim", "W\u00fcr\u00b7fel\u00b7fal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$.", "ADV", "VMFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit uns durchzechen mehr die tiefe Nacht,", "tokens": ["Mit", "uns", "durch\u00b7ze\u00b7chen", "mehr", "die", "tie\u00b7fe", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Der einzige, der n\u00fcchtern bleibt von allen;", "tokens": ["Der", "ein\u00b7zi\u00b7ge", ",", "der", "n\u00fcch\u00b7tern", "bleibt", "von", "al\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ADJD", "VVFIN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Nie soll er mehr, von toller Brunst entfacht,", "tokens": ["Nie", "soll", "er", "mehr", ",", "von", "tol\u00b7ler", "Brunst", "ent\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein hold schwarz\u00e4ugig Heidenkind umwinden,", "tokens": ["Ein", "hold", "schwarz\u00b7\u00e4u\u00b7gig", "Hei\u00b7den\u00b7kind", "um\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von dessen Lippen hei\u00df die Wollust lacht!", "tokens": ["Von", "des\u00b7sen", "Lip\u00b7pen", "hei\u00df", "die", "Wol\u00b7lust", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Auch werden wir ihn nimmer wandelnd finden", "tokens": ["Auch", "wer\u00b7den", "wir", "ihn", "nim\u00b7mer", "wan\u00b7delnd", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Mondschein auf der Mauern weitem Rund", "tokens": ["Im", "Mond\u00b7schein", "auf", "der", "Mau\u00b7ern", "wei\u00b7tem", "Rund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und mit den Sternen sprechend, mit den Winden.", "tokens": ["Und", "mit", "den", "Ster\u00b7nen", "spre\u00b7chend", ",", "mit", "den", "Win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Denn mancherlei Geheimnis ward ihm kund,", "tokens": ["Denn", "man\u00b7cher\u00b7lei", "Ge\u00b7heim\u00b7nis", "ward", "ihm", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und seltsam mag's um seinen Glauben stehen;", "tokens": ["Und", "selt\u00b7sam", "mag's", "um", "sei\u00b7nen", "Glau\u00b7ben", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch tat er nie dar\u00fcber auf den Mund.\u00ab", "tokens": ["Doch", "tat", "er", "nie", "da\u00b7r\u00fc\u00b7ber", "auf", "den", "Mund", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PAV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "So summt die Rede, und die Ritter gehen", "tokens": ["So", "summt", "die", "Re\u00b7de", ",", "und", "die", "Rit\u00b7ter", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu Odos Zelle, noch ein letztes Mal", "tokens": ["Zu", "O\u00b7dos", "Zel\u00b7le", ",", "noch", "ein", "letz\u00b7tes", "Mal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihn, der des Ordens Pfeiler war, zu sehen.", "tokens": ["Ihn", ",", "der", "des", "Or\u00b7dens", "Pfei\u00b7ler", "war", ",", "zu", "se\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "NN", "VAFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Sie treten ein. Im fahlen D\u00e4mmerstrahl", "tokens": ["Sie", "tre\u00b7ten", "ein", ".", "Im", "fah\u00b7len", "D\u00e4m\u00b7mer\u00b7strahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf seinem Binsenlager ruht der Blasse;", "tokens": ["Auf", "sei\u00b7nem", "Bin\u00b7sen\u00b7la\u00b7ger", "ruht", "der", "Blas\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus seinem Auge brennt des Fiebers Qual.", "tokens": ["Aus", "sei\u00b7nem", "Au\u00b7ge", "brennt", "des", "Fie\u00b7bers", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Die Hand, als ob sie noch nach Leben fasse,", "tokens": ["Die", "Hand", ",", "als", "ob", "sie", "noch", "nach", "Le\u00b7ben", "fas\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOKOM", "KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Greift irr umher, die Lippe krampft sich an,", "tokens": ["Greift", "irr", "um\u00b7her", ",", "die", "Lip\u00b7pe", "krampft", "sich", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sie des Schmerzes Schrei hervor nicht lasse.", "tokens": ["Da\u00df", "sie", "des", "Schmer\u00b7zes", "Schrei", "her\u00b7vor", "nicht", "las\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "PTKVZ", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Da naht im ernsten Zuge der Kaplan", "tokens": ["Da", "naht", "im", "erns\u00b7ten", "Zu\u00b7ge", "der", "Ka\u00b7plan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Kreuz und Kerzen beim Gesang der Lieder,", "tokens": ["Mit", "Kreuz", "und", "Ker\u00b7zen", "beim", "Ge\u00b7sang", "der", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Kranke soll den letzten Trost empfahn.", "tokens": ["Der", "Kran\u00b7ke", "soll", "den", "letz\u00b7ten", "Trost", "em\u00b7pfahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Und vor dem Sakramente sinken nieder", "tokens": ["Und", "vor", "dem", "Sak\u00b7ra\u00b7men\u00b7te", "sin\u00b7ken", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aufs Knie die rotbekreuzten Br\u00fcder all,", "tokens": ["Aufs", "Knie", "die", "rot\u00b7be\u00b7kreuz\u00b7ten", "Br\u00fc\u00b7der", "all", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er aber richtet auf die hagern Glieder.", "tokens": ["Er", "a\u00b7ber", "rich\u00b7tet", "auf", "die", "ha\u00b7gern", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Und seine Stimme ruft mit dumpfem Schall,", "tokens": ["Und", "sei\u00b7ne", "Stim\u00b7me", "ruft", "mit", "dum\u00b7pfem", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie wenn im Sturm geborstne Glocken l\u00e4uten:", "tokens": ["Wie", "wenn", "im", "Sturm", "ge\u00b7borst\u00b7ne", "Glo\u00b7cken", "l\u00e4u\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "APPRART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbhinweg! Nicht bin ich eurer Furcht Vasall!", "tokens": ["\u00bb", "hin\u00b7weg", "!", "Nicht", "bin", "ich", "eu\u00b7rer", "Furcht", "Va\u00b7sall", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "PTKNEG", "VAFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Hinweg mit Formeln, die mir nichts bedeuten!", "tokens": ["Hin\u00b7weg", "mit", "For\u00b7meln", ",", "die", "mir", "nichts", "be\u00b7deu\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "PRELS", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich will nicht Tr\u00f6stung. Immer war's mein Brauch,", "tokens": ["Ich", "will", "nicht", "Tr\u00f6s\u00b7tung", ".", "Im\u00b7mer", "wa\u00b7r's", "mein", "Brauch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "$.", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das, was mir not war, selbst mir zu erbeuten;", "tokens": ["Das", ",", "was", "mir", "not", "war", ",", "selbst", "mir", "zu", "er\u00b7beu\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "NN", "VAFIN", "$,", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Den Sieg der Schlacht, der Minne gl\u00fchnden Hauch,", "tokens": ["Den", "Sieg", "der", "Schlacht", ",", "der", "Min\u00b7ne", "gl\u00fchn\u00b7den", "Hauch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Wahrheit selber, die ich nackend schaute;", "tokens": ["Die", "Wahr\u00b7heit", "sel\u00b7ber", ",", "die", "ich", "na\u00b7ckend", "schau\u00b7te", ";"], "token_info": ["w