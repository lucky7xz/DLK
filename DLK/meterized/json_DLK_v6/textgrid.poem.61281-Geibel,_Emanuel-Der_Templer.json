-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Drauf sie zum Wappen Schwert und Mantel legen;", "tokens": ["Drauf", "sie", "zum", "Wap\u00b7pen", "Schwert", "und", "Man\u00b7tel", "le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "APPRART", "NN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann w\u00e4lzt sich dr\u00fcber hohlen Schalls der Stein.", "tokens": ["Dann", "w\u00e4lzt", "sich", "dr\u00fc\u00b7ber", "hoh\u00b7len", "Schalls", "der", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PAV", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.24": {"line.1": {"text": "Ein kurz Gebet - und auf geschiednen Wegen", "tokens": ["Ein", "kurz", "Ge\u00b7bet", "und", "auf", "ge\u00b7schied\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$(", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sucht jeder sein Gemach verst\u00f6rt im Sinn", "tokens": ["Sucht", "je\u00b7der", "sein", "Ge\u00b7mach", "ver\u00b7st\u00f6rt", "im", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und tr\u00e4umet bang dem Morgenrot entgegen.", "tokens": ["Und", "tr\u00e4u\u00b7met", "bang", "dem", "Mor\u00b7gen\u00b7rot", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.25": {"line.1": {"text": "Es steigt der Tag, und ruhig vom Beg