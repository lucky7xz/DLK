{"textgrid.poem.43457": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Deutschland, was fehlt dir? was klagest du so?", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutschland, was fehlt dir? was klagest du so?", "tokens": ["Deutschland", ",", "was", "fehlt", "dir", "?", "was", "kla\u00b7gest", "du", "so", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "PPER", "$.", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Bist ja so gl\u00fccklich, und dennoch nicht froh!", "tokens": ["Bist", "ja", "so", "gl\u00fcck\u00b7lich", ",", "und", "den\u00b7noch", "nicht", "froh", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "$,", "KON", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Deutschland, so klage, so traure doch nicht!", "tokens": ["Deutschland", ",", "so", "kla\u00b7ge", ",", "so", "trau\u00b7re", "doch", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Hast du nicht Alles was vielen gebricht?", "tokens": ["Hast", "du", "nicht", "Al\u00b7les", "was", "vie\u00b7len", "ge\u00b7bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PIS", "PWS", "PIS", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "W\u00e4lder und Felder, und Th\u00e4ler und H\u00f6h'n,", "tokens": ["W\u00e4l\u00b7der", "und", "Fel\u00b7der", ",", "und", "Th\u00e4\u00b7ler", "und", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Alles voll Segen, so herrlich und sch\u00f6n;", "tokens": ["Al\u00b7les", "voll", "Se\u00b7gen", ",", "so", "herr\u00b7lich", "und", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Honig und Butter, und Bier und auch Wein \u2013", "tokens": ["Ho\u00b7nig", "und", "But\u00b7ter", ",", "und", "Bier", "und", "auch", "Wein", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "KON", "NN", "KON", "ADV", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Sag, warum willst du denn fr\u00f6hlich nicht sein?", "tokens": ["Sag", ",", "wa\u00b7rum", "willst", "du", "denn", "fr\u00f6h\u00b7lich", "nicht", "sein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VMFIN", "PPER", "ADV", "ADJD", "PTKNEG", "VAINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Handel und Wandel, und Wissen und Kunst,", "tokens": ["Han\u00b7del", "und", "Wan\u00b7del", ",", "und", "Wis\u00b7sen", "und", "Kunst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Drei\u00dfig Regenten mit f\u00fcrstlicher Gunst,", "tokens": ["Drei\u00b7\u00dfig", "Re\u00b7gen\u00b7ten", "mit", "f\u00fcrst\u00b7li\u00b7cher", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Adel und Pfaffen und Staatskanzelei'n \u2013", "tokens": ["A\u00b7del", "und", "Pfaf\u00b7fen", "und", "Staats\u00b7kan\u00b7ze\u00b7lei'n", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$("], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Sag, warum willst du denn fr\u00f6hlich nicht sein?", "tokens": ["Sag", ",", "wa\u00b7rum", "willst", "du", "denn", "fr\u00f6h\u00b7lich", "nicht", "sein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VMFIN", "PPER", "ADV", "ADJD", "PTKNEG", "VAINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "K\u00fcnstler, Gelehrte, wie Sand an dem Meer,", "tokens": ["K\u00fcnst\u00b7ler", ",", "Ge\u00b7lehr\u00b7te", ",", "wie", "Sand", "an", "dem", "Meer", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Tapfrer Soldaten manch pr\u00e4chtiges Heer,", "tokens": ["Tapf\u00b7rer", "Sol\u00b7da\u00b7ten", "manch", "pr\u00e4ch\u00b7ti\u00b7ges", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Freie Censur und den freien Rhein \u2013", "tokens": ["Frei\u00b7e", "Cen\u00b7sur", "und", "den", "frei\u00b7en", "Rhein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ART", "ADJA", "NE", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sag, warum willst du denn fr\u00f6hlich nicht sein?", "tokens": ["Sag", ",", "wa\u00b7rum", "willst", "du", "denn", "fr\u00f6h\u00b7lich", "nicht", "sein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VMFIN", "PPER", "ADV", "ADJD", "PTKNEG", "VAINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Constitutionen beschworen so fest,", "tokens": ["Con\u00b7sti\u00b7tu\u00b7ti\u00b7o\u00b7nen", "be\u00b7schwo\u00b7ren", "so", "fest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Da\u00df sich kein Wort davon austilgen l\u00e4\u00dft;", "tokens": ["Da\u00df", "sich", "kein", "Wort", "da\u00b7von", "aus\u00b7til\u00b7gen", "l\u00e4\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PIAT", "NN", "PAV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "V\u00f6lker und F\u00fcrsten im trauten Verein \u2013", "tokens": ["V\u00f6l\u00b7ker", "und", "F\u00fcrs\u00b7ten", "im", "trau\u00b7ten", "Ver\u00b7ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Sag, warum willst du denn fr\u00f6hlich nicht sein?", "tokens": ["Sag", ",", "wa\u00b7rum", "willst", "du", "denn", "fr\u00f6h\u00b7lich", "nicht", "sein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VMFIN", "PPER", "ADV", "ADJD", "PTKNEG", "VAINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Deutschland, was fehlt dir? was klagest du so?", "tokens": ["Deutschland", ",", "was", "fehlt", "dir", "?", "was", "kla\u00b7gest", "du", "so", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "PPER", "$.", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Bist ja so gl\u00fccklich, und dennoch nicht froh! \u2013", "tokens": ["Bist", "ja", "so", "gl\u00fcck\u00b7lich", ",", "und", "den\u00b7noch", "nicht", "froh", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "$,", "KON", "ADV", "PTKNEG", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbfr\u00f6hlich wol sollt' ich und wollt' ich auch sein,", "tokens": ["\u00bb", "fr\u00f6h\u00b7lich", "wol", "sollt'", "ich", "und", "wollt'", "ich", "auch", "sein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ADV", "VMFIN", "PPER", "KON", "VMFIN", "PPER", "ADV", "VAINF", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Deutschland nur fehlt mir, nur Deutschland allein.\u00ab", "tokens": ["Deutschland", "nur", "fehlt", "mir", ",", "nur", "Deutschland", "al\u00b7lein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "$,", "ADV", "NE", "ADV", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}