{"dta.poem.4410": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Abermalige  \n Betrachtung des Mond-Scheins.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allgegenw\u00e4rt\u2019ger Sch\u00f6pfer, lenke,", "tokens": ["All\u00b7ge\u00b7gen\u00b7w\u00e4rt'\u00b7ger", "Sch\u00f6p\u00b7fer", ",", "len\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ach lenke meinen Sinn", "tokens": ["Ach", "len\u00b7ke", "mei\u00b7nen", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch Deine Gnade doch dahin,", "tokens": ["Durch", "Dei\u00b7ne", "Gna\u00b7de", "doch", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich an Deine Gr\u00f6ss\u2019 und Allmacht oft ge-", "tokens": ["Da\u00df", "ich", "an", "Dei\u00b7ne", "Gr\u00f6ss'", "und", "All\u00b7macht", "oft", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "KON", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "denke!", "tokens": ["den\u00b7ke", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}