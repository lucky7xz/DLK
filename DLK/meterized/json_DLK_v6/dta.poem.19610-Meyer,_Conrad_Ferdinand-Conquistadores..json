, "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da wir zu Schiffe stiegen dort,", "tokens": ["Da", "wir", "zu", "Schif\u00b7fe", "stie\u00b7gen", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Zierden aller Lande,", "tokens": ["Die", "Zier\u00b7den", "al\u00b7ler", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zirpt' Heimchen mir im Busen fort,", "tokens": ["Zirpt'", "Heim\u00b7chen", "mir", "im", "Bu\u00b7sen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als weidet's noch am Strande.", "tokens": ["Als", "wei\u00b7det's", "noch", "am", "Stran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das gr\u00fcne Vorgebirg verschwand,", "tokens": ["Das", "gr\u00fc\u00b7ne", "Vor\u00b7ge\u00b7birg", "ver\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Heimchen ward es schaurig,", "tokens": ["Dem", "Heim\u00b7chen", "ward", "es", "schau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Beklommen sa\u00df es an der Wand", "tokens": ["Be\u00b7klom\u00b7men", "sa\u00df", "es", "an", "der", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und wurde faul und traurig.", "tokens": ["Und", "wur\u00b7de", "faul", "und", "trau\u00b7rig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "So darbt's und d\u00e4mmert's langezeit,", "tokens": ["So", "darbt's", "und", "d\u00e4m\u00b7mert's", "lan\u00b7ge\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon gab ich es verloren,", "tokens": ["Schon", "gab", "ich", "es", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und nun, bei meiner Seligkeit,", "tokens": ["Und", "nun", ",", "bei", "mei\u00b7ner", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist Heimchen neu geboren!", "tokens": ["Ist", "Heim\u00b7chen", "neu", "ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bedenkt, es hockte gram und lahm", "tokens": ["Be\u00b7denkt", ",", "es", "hock\u00b7te", "gram", "und", "lahm"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "NE", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An Dielen und an W\u00e4nden,", "tokens": ["An", "Die\u00b7len", "und", "an", "W\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Jetzt jubelt's wie ein Br\u00e4utigam", "tokens": ["Jetzt", "ju\u00b7belt's", "wie", "ein", "Br\u00e4u\u00b7ti\u00b7gam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "KOKOM", "ART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und kann nur gar nicht enden!\u201c", "tokens": ["Und", "kann", "nur", "gar", "nicht", "en\u00b7den", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Miguel ist fort und wieder da,", "tokens": ["Mi\u00b7guel", "ist", "fort", "und", "wie\u00b7der", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKVZ", "KON", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fingerspitze zeigend:", "tokens": ["Die", "Fin\u00b7ger\u00b7spit\u00b7ze", "zei\u00b7gend", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da sitzt es ja! da singt es ja!", "tokens": ["Da", "sitzt", "es", "ja", "!", "da", "singt", "es", "ja", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die M\u00e4nner lauschen schweigend \u2014", "tokens": ["Die", "M\u00e4n\u00b7ner", "lau\u00b7schen", "schwei\u00b7gend"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann sinnen sie der Sache nach,", "tokens": ["Dann", "sin\u00b7nen", "sie", "der", "Sa\u00b7che", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Lustgesang im Ohre,", "tokens": ["Den", "Lust\u00b7ge\u00b7sang", "im", "Oh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie sch\u00fctteln sich die H\u00e4nde jach", "tokens": ["Sie", "sch\u00fct\u00b7teln", "sich", "die", "H\u00e4n\u00b7de", "jach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und schrei'n in wildem Chore:", "tokens": ["Und", "schrei'n", "in", "wil\u00b7dem", "Cho\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "\u201edas Heimchen zirpt! Das Heimchen zirpt!", "tokens": ["\u201e", "das", "Heim\u00b7chen", "zirpt", "!", "Das", "Heim\u00b7chen", "zirpt", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bald schwelgen wir in Beute!", "tokens": ["Bald", "schwel\u00b7gen", "wir", "in", "Beu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "