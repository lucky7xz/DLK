{"dta.poem.19610": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Conquistadores.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei edle Spanier halten Wacht", "tokens": ["Zwei", "ed\u00b7le", "Spa\u00b7nier", "hal\u00b7ten", "Wacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und einer spricht zum andern:", "tokens": ["Und", "ei\u00b7ner", "spricht", "zum", "an\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201ese\u00f1or, mir d\u00e4ucht, der Teufel lacht,", "tokens": ["\u201e", "se\u00f1or", ",", "mir", "d\u00e4ucht", ",", "der", "Teu\u00b7fel", "lacht", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie wir ins Leere wandern!", "tokens": ["Wie", "wir", "ins", "Lee\u00b7re", "wan\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Segel rauscht, es rauscht der Kiel,", "tokens": ["Das", "Se\u00b7gel", "rauscht", ",", "es", "rauscht", "der", "Kiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch keines Strandes Boten \u2014", "tokens": ["Noch", "kei\u00b7nes", "Stran\u00b7des", "Bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die H\u00f6lle treibt mit uns ihr Spiel,", "tokens": ["Die", "H\u00f6l\u00b7le", "treibt", "mit", "uns", "ihr", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wir wandern zu den Todten!", "tokens": ["Wir", "wan\u00b7dern", "zu", "den", "Tod\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wer einem Genuesen traut,", "tokens": ["Wer", "ei\u00b7nem", "Ge\u00b7nu\u00b7e\u00b7sen", "traut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Hat den Verstand verloren!", "tokens": ["Hat", "den", "Ver\u00b7stand", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Klugen hat er schlecht erbaut,", "tokens": ["Die", "Klu\u00b7gen", "hat", "er", "schlecht", "er\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch lockt er alle Thoren \u2014", "tokens": ["Doch", "lockt", "er", "al\u00b7le", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Rund sei die Erde, log er mir,", "tokens": ["Rund", "sei", "die", "Er\u00b7de", ",", "log", "er", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Pomeranzenb\u00e4lle,", "tokens": ["Wie", "Po\u00b7me\u00b7ran\u00b7zen\u00b7b\u00e4l\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch unerme\u00dflich fluthet hier", "tokens": ["Doch", "un\u00b7er\u00b7me\u00df\u00b7lich", "flut\u00b7het", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nur Welle hinter Welle!\u201c", "tokens": ["Nur", "Wel\u00b7le", "hin\u00b7ter", "Wel\u00b7le", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Andre blickt ins Meer hinaus", "tokens": ["Der", "And\u00b7re", "blickt", "ins", "Meer", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "APPRART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und runzelt finstre Brauen:", "tokens": ["Und", "run\u00b7zelt", "finst\u00b7re", "Brau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201ese\u00f1or, mich zog Columb ins Haus,", "tokens": ["\u201e", "se\u00f1or", ",", "mich", "zog", "Co\u00b7lumb", "ins", "Haus", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PPER", "VVFIN", "NE", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lie\u00df mich die Karten schauen,", "tokens": ["Lie\u00df", "mich", "die", "Kar\u00b7ten", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was er docirt', verstand ich nicht,", "tokens": ["Was", "er", "do\u00b7cirt'", ",", "ver\u00b7stand", "ich", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich lie\u00df es alles gelten \u2014", "tokens": ["Ich", "lie\u00df", "es", "al\u00b7les", "gel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sein \u00fcberm\u00e4chtig Angesicht", "tokens": ["Sein", "\u00fc\u00b7ber\u00b7m\u00e4ch\u00b7tig", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verhie\u00df mir neue Welten!", "tokens": ["Ver\u00b7hie\u00df", "mir", "neu\u00b7e", "Wel\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Betrog er sich und haben wir", "tokens": ["Be\u00b7trog", "er", "sich", "und", "ha\u00b7ben", "wir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "KON", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uns in das Nichts verlaufen,", "tokens": ["Uns", "in", "das", "Nichts", "ver\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein r\u00e4ud'ger Hund, Seor, wie Ihr", "tokens": ["Ein", "r\u00e4u\u00b7d'\u00b7ger", "Hund", ",", "Se\u00b7or", ",", "wie", "Ihr"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "NE", "$,", "PWAV", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Darf fr\u00f6hlich mit ersaufen!\u201c", "tokens": ["Darf", "fr\u00f6h\u00b7lich", "mit", "er\u00b7sau\u00b7fen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADJD", "APPR", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u2014 \u201eSeor, da betet Ihr nicht gut!", "tokens": ["\u201e", "Se\u00b7or", ",", "da", "be\u00b7tet", "Ihr", "nicht", "gut", "!"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "NE", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Zur\u00fcck Euch in den Rachen", "tokens": ["Zu\u00b7r\u00fcck", "Euch", "in", "den", "Ra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den r\u00e4ud'gen Hund! Ihr raucht von Blut", "tokens": ["Den", "r\u00e4u\u00b7d'\u00b7gen", "Hund", "!", "Ihr", "raucht", "von", "Blut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und Ihr entsprangt den Wachen!\u201c", "tokens": ["Und", "Ihr", "ent\u00b7sprangt", "den", "Wa\u00b7chen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u201eseor, ich dolcht' ein falsches Weib,", "tokens": ["\u201e", "se\u00b7or", ",", "ich", "dolcht'", "ein", "fal\u00b7sches", "Weib", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bekenn' ich unverhohlen!", "tokens": ["Be\u00b7kenn'", "ich", "un\u00b7ver\u00b7hoh\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht hab' dem B\u00e4cker einen Laib", "tokens": ["Nicht", "hab'", "dem", "B\u00e4\u00b7cker", "ei\u00b7nen", "Laib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Bret ich weggestohlen!", "tokens": ["Vom", "Bret", "ich", "weg\u00b7ge\u00b7stoh\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Seor, Ihr seid ein Galgenstrick!\u201c", "tokens": ["Se\u00b7or", ",", "Ihr", "seid", "ein", "Gal\u00b7gen\u00b7strick", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u2014 \u201eSeor, Ihr seid nicht besser!\u201c", "tokens": ["\u201e", "Se\u00b7or", ",", "Ihr", "seid", "nicht", "bes\u00b7ser", "!", "\u201c"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "$(", "NE", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie ziehen mit entflammtem Blick", "tokens": ["Sie", "zie\u00b7hen", "mit", "ent\u00b7flamm\u00b7tem", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und kreuzen blanke Messer ...", "tokens": ["Und", "kreu\u00b7zen", "blan\u00b7ke", "Mes\u00b7ser", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da zwischen ihre Messer walzt", "tokens": ["Da", "zwi\u00b7schen", "ih\u00b7re", "Mes\u00b7ser", "walzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In tollem Freudensprunge,", "tokens": ["In", "tol\u00b7lem", "Freu\u00b7den\u00b7sprun\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit \u00f6lgetr\u00e4nkten Fingern schnalzt", "tokens": ["Mit", "\u00f6l\u00b7ge\u00b7tr\u00e4nk\u00b7ten", "Fin\u00b7gern", "schnalzt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Miguel, der K\u00fcchenjunge.", "tokens": ["Mi\u00b7guel", ",", "der", "K\u00fc\u00b7chen\u00b7jun\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er dr\u00fcckt die Lider blinzelnd ein", "tokens": ["Er", "dr\u00fcckt", "die", "Li\u00b7der", "blin\u00b7zelnd", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit schlauem Wimperzwinken,", "tokens": ["Mit", "schlau\u00b7em", "Wim\u00b7per\u00b7zwin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bald h\u00fcpft er auf dem rechten Bein,", "tokens": ["Bald", "h\u00fcpft", "er", "auf", "dem", "rech\u00b7ten", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bald hopst er auf dem linken,", "tokens": ["Bald", "hopst", "er", "auf", "dem", "lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "In L\u00fcften bl\u00e4ht sich sein Gewand,", "tokens": ["In", "L\u00fcf\u00b7ten", "bl\u00e4ht", "sich", "sein", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es puffen ihm die Hosen \u2014", "tokens": ["Es", "puf\u00b7fen", "ihm", "die", "Ho\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Neugierig kommen hergerannt", "tokens": ["Neu\u00b7gie\u00b7rig", "kom\u00b7men", "her\u00b7ge\u00b7rannt"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soldaten und Matrosen.", "tokens": ["Sol\u00b7da\u00b7ten", "und", "Mat\u00b7ro\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Junge redet kunterbunt,", "tokens": ["Der", "Jun\u00b7ge", "re\u00b7det", "kun\u00b7ter\u00b7bunt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als ob's im Kopf ihm fehle,", "tokens": ["Als", "ob's", "im", "Kopf", "ihm", "feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dann \u00f6ffnet er den gro\u00dfen Mund", "tokens": ["Dann", "\u00f6ff\u00b7net", "er", "den", "gro\u00b7\u00dfen", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und singt aus voller Kehle:", "tokens": ["Und", "singt", "aus", "vol\u00b7ler", "Keh\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u201edas Heimchen zirpt, das Heimchen zirpt,", "tokens": ["\u201e", "das", "Heim\u00b7chen", "zirpt", ",", "das", "Heim\u00b7chen", "zirpt", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stimmt Laudes an und Psalmen!", "tokens": ["Stimmt", "Lau\u00b7des", "an", "und", "Psal\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn's mir nicht vor Freude stirbt,", "tokens": ["Und", "wenn's", "mir", "nicht", "vor", "Freu\u00b7de", "stirbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald weidet's unter Halmen!", "tokens": ["Bald", "wei\u00b7det's", "un\u00b7ter", "Hal\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich schw\u00f6r' es Euch bei Gottes Haupt:", "tokens": ["Ich", "schw\u00f6r'", "es", "Euch", "bei", "Got\u00b7tes", "Haupt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PRF", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es athmet duft'ge Weiden,", "tokens": ["Es", "ath\u00b7met", "duft'\u00b7ge", "Wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es wittert W\u00e4lder dichtbelaubt", "tokens": ["Es", "wit\u00b7tert", "W\u00e4l\u00b7der", "dicht\u00b7be\u00b7laubt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und unermessne Haiden!", "tokens": ["Und", "un\u00b7er\u00b7mess\u00b7ne", "Hai\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Erlauchte Herren, gebet Acht,", "tokens": ["Er\u00b7lauch\u00b7te", "Her\u00b7ren",