{"textgrid.poem.31555": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der alle Spei\u00df' erschuff / l\u00e4st sich hier selber essen.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der alle Spei\u00df' erschuff / l\u00e4st sich hier selber essen.", "tokens": ["Der", "al\u00b7le", "Spei\u00df'", "er\u00b7schuff", "/", "l\u00e4st", "sich", "hier", "sel\u00b7ber", "es\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PTKVZ", "$(", "VVFIN", "PRF", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der selber hat erbaut die Zunge und den Mund /", "tokens": ["Der", "sel\u00b7ber", "hat", "er\u00b7baut", "die", "Zun\u00b7ge", "und", "den", "Mund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "VVPP", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "erniedert sich so tieff und kommt in ihren Schlund.", "tokens": ["er\u00b7nie\u00b7dert", "sich", "so", "tieff", "und", "kommt", "in", "ih\u00b7ren", "Schlund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADJD", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der / dem die Erd' ein Staub' / ist auf der Zung gesessen.", "tokens": ["Der", "/", "dem", "die", "Erd'", "ein", "Staub'", "/", "ist", "auf", "der", "Zung", "ge\u00b7ses\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ART", "ART", "NN", "ART", "NN", "$(", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der kan mit einer Spann das grosse Rund \u00fcmmessen /", "tokens": ["Der", "kan", "mit", "ei\u00b7ner", "Spann", "das", "gros\u00b7se", "Rund", "\u00fcm\u00b7mes\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "geht sichtbar ungesehn in unser kleines Rund.", "tokens": ["geht", "sicht\u00b7bar", "un\u00b7ge\u00b7sehn", "in", "un\u00b7ser", "klei\u00b7nes", "Rund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Di\u00df Leib-und Geists-beyseyn / ist unser f\u00e4ster Grund:", "tokens": ["Di\u00df", "Leib\u00b7\u00b7und", "Geist\u00b7s\u00b7bey\u00b7seyn", "/", "ist", "un\u00b7ser", "f\u00e4s\u00b7ter", "Grund", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "NN", "$(", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "der doppelt' Gott mit-uns werd' unser nicht vergessen.", "tokens": ["der", "dop\u00b7pelt'", "Gott", "mit\u00b7uns", "werd'", "un\u00b7ser", "nicht", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VAFIN", "PPOSAT", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Vnerm\u00e4\u00dflichkeit / so unser Fleisch annahm /", "tokens": ["Die", "Vner\u00b7m\u00e4\u00df\u00b7lich\u00b7keit", "/", "so", "un\u00b7ser", "Fleisch", "an\u00b7nahm", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "mit ihm in Brod und Wein warhafftig zu uns kam.", "tokens": ["mit", "ihm", "in", "Brod", "und", "Wein", "war\u00b7haff\u00b7tig", "zu", "uns", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "KON", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "O Wunder-reicher Schatz / Geheimnus ohne Ziel!", "tokens": ["O", "Wun\u00b7der\u00b7rei\u00b7cher", "Schatz", "/", "Ge\u00b7heim\u00b7nus", "oh\u00b7ne", "Ziel", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "ergr\u00fcnden kan ich nichts / doch glauben kan ich viel /", "tokens": ["er\u00b7gr\u00fcn\u00b7den", "kan", "ich", "nichts", "/", "doch", "glau\u00b7ben", "kan", "ich", "viel", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "VMFIN", "PPER", "PIS", "$(", "ADV", "VVINF", "VMFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "mein Mund empf\u00e4ngt dich g'wi\u00df / der Glaube jauchzt und springt:", "tokens": ["mein", "Mund", "emp\u00b7f\u00e4ngt", "dich", "g'\u00b7wi\u00df", "/", "der", "Glau\u00b7be", "jauchzt", "und", "springt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$(", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "das Wie stell' ich dir heim. Zu Heil es doch gelingt.", "tokens": ["das", "Wie", "stell'", "ich", "dir", "heim", ".", "Zu", "Heil", "es", "doch", "ge\u00b7lingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PWAV", "VVFIN", "PPER", "PPER", "PTKVZ", "$.", "APPR", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}