{"dta.poem.7028": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wein- und E\u00dfig-Rosen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den rothen, weiss- und wilden Rosen-Strauch", "tokens": ["Den", "ro\u00b7then", ",", "weiss", "und", "wil\u00b7den", "Ro\u00b7sen\u00b7Strauch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hab ich, durch ihren Schmuck dazu getrieben,", "tokens": ["Hab", "ich", ",", "durch", "ih\u00b7ren", "Schmuck", "da\u00b7zu", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "APPR", "PPOSAT", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu ihres Sch\u00f6pfers Ruhm beschrieben.", "tokens": ["Zu", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "Ruhm", "be\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt find ich, da\u00df annoch verschiednes \u00fcbrig blieben,", "tokens": ["Jetzt", "find", "ich", ",", "da\u00df", "an\u00b7noch", "ver\u00b7schied\u00b7nes", "\u00fcb\u00b7rig", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ADV", "ADJA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das eben auch", "tokens": ["Das", "e\u00b7ben", "auch"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Zum selben Zweck ger\u00fchmt zu seyn begehrt,", "tokens": ["Zum", "sel\u00b7ben", "Zweck", "ge\u00b7r\u00fchmt", "zu", "seyn", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "PTKZU", "VAINF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und welches es nicht minder wehrt.", "tokens": ["Und", "wel\u00b7ches", "es", "nicht", "min\u00b7der", "wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}