{"textgrid.poem.64110": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Gesang der Legionen", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch Alpenschnee, durch Parthersand", "tokens": ["Durch", "Al\u00b7pen\u00b7schnee", ",", "durch", "Par\u00b7ther\u00b7sand"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit immer stetem Schritte,", "tokens": ["Mit", "im\u00b7mer", "ste\u00b7tem", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir tragen mit das Vaterland", "tokens": ["Wir", "tra\u00b7gen", "mit", "das", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und R\u00f6mer Recht und Sitte.", "tokens": ["Und", "R\u00f6\u00b7mer", "Recht", "und", "Sit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wo der Feldherr Lager schlug,", "tokens": ["Und", "wo", "der", "Feld\u00b7herr", "La\u00b7ger", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da kann uns Heimat werden:", "tokens": ["Da", "kann", "uns", "Hei\u00b7mat", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir folgen unsrer Adler Flug", "tokens": ["Wir", "fol\u00b7gen", "uns\u00b7rer", "Ad\u00b7ler", "Flug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und unser ist die Erden.", "tokens": ["Und", "un\u00b7ser", "ist", "die", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und nach dem Sieg das Schwert gesenkt", "tokens": ["Und", "nach", "dem", "Sieg", "das", "Schwert", "ge\u00b7senkt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Pflug gef\u00fchrt und Spaten:", "tokens": ["Und", "Pflug", "ge\u00b7f\u00fchrt", "und", "Spa\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Land, das r\u00f6misch Blut getr\u00e4nkt,", "tokens": ["Das", "Land", ",", "das", "r\u00f6\u00b7misch", "Blut", "ge\u00b7tr\u00e4nkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ward r\u00f6mischer Penaten.", "tokens": ["Ward", "r\u00f6\u00b7mi\u00b7scher", "Pe\u00b7na\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Am Euphrat und am Donaustrom", "tokens": ["Am", "Eu\u00b7ph\u00b7rat", "und", "am", "Do\u00b7naus\u00b7trom"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NE", "KON", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bl\u00fcht heil'ger Dienst der Laren", "tokens": ["Bl\u00fcht", "heil'\u00b7ger", "Dienst", "der", "La\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und rings ersteht ein kleines Rom", "tokens": ["Und", "rings", "er\u00b7steht", "ein", "klei\u00b7nes", "Rom"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Staunen der Barbaren.", "tokens": ["Zum", "Stau\u00b7nen", "der", "Bar\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Sumpf versiegt, der Urwald f\u00e4llt,", "tokens": ["Der", "Sumpf", "ver\u00b7siegt", ",", "der", "Ur\u00b7wald", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nahn sich des Liktors St\u00e4be:", "tokens": ["Nahn", "sich", "des", "Lik\u00b7tors", "St\u00e4\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wir bringen eine sch\u00f6n're Welt:", "tokens": ["Wir", "brin\u00b7gen", "ei\u00b7ne", "sch\u00f6n'\u00b7re", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den \u00d6lbaum und die Rebe.", "tokens": ["Den", "\u00d6l\u00b7baum", "und", "die", "Re\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wir bauen Stra\u00dfen von Granit,", "tokens": ["Wir", "bau\u00b7en", "Stra\u00b7\u00dfen", "von", "Gra\u00b7nit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die noch in fernsten Tagen", "tokens": ["Die", "noch", "in", "ferns\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den eh'rnen Schritt, den Siegesschritt", "tokens": ["Den", "eh'r\u00b7nen", "Schritt", ",", "den", "Sie\u00b7ges\u00b7schritt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schlachtkohorten tragen.", "tokens": ["Der", "Schlacht\u00b7ko\u00b7hor\u00b7ten", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Denn uns ist aus Orakelmund", "tokens": ["Denn", "uns", "ist", "aus", "O\u00b7ra\u00b7kel\u00b7mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Schicksalswort verk\u00fcndet:", "tokens": ["Das", "Schick\u00b7sals\u00b7wort", "ver\u00b7k\u00fcn\u00b7det", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So ewig steht im Erdenrund", "tokens": ["So", "e\u00b7wig", "steht", "im", "Er\u00b7den\u00b7rund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das R\u00f6merreich gegr\u00fcndet,", "tokens": ["Das", "R\u00f6\u00b7mer\u00b7reich", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "So ewig ziehn von Pol zu Pol", "tokens": ["So", "e\u00b7wig", "ziehn", "von", "Pol", "zu", "Pol"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVINF", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die r\u00f6mischen Legionen \u2013", "tokens": ["Die", "r\u00f6\u00b7mi\u00b7schen", "Le\u00b7gi\u00b7o\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als am bet\u00fcrmten Kapitol", "tokens": ["Als", "am", "be\u00b7t\u00fcrm\u00b7ten", "Ka\u00b7pi\u00b7tol"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ew'gen G\u00f6tter thronen.", "tokens": ["Die", "ew'\u00b7gen", "G\u00f6t\u00b7ter", "thro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}