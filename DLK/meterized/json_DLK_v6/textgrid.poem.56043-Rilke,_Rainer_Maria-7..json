{"textgrid.poem.56043": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aber Schalen sind, drin der Najaden", "tokens": ["A\u00b7ber", "Scha\u00b7len", "sind", ",", "drin", "der", "Na\u00b7ja\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "$,", "ADV", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Spiegelbilder, die sie nicht mehr baden,", "tokens": ["Spie\u00b7gel\u00b7bil\u00b7der", ",", "die", "sie", "nicht", "mehr", "ba\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "wie ertrunken liegen, sehr verzerrt;", "tokens": ["wie", "er\u00b7trun\u00b7ken", "lie\u00b7gen", ",", "sehr", "ver\u00b7zerrt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VVFIN", "$,", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "die Alleen sind durch Balustraden", "tokens": ["die", "Al\u00b7leen", "sind", "durch", "Ba\u00b7lust\u00b7ra\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "in der Ferne wie versperrt.", "tokens": ["in", "der", "Fer\u00b7ne", "wie", "ver\u00b7sperrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOKOM", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Immer geht ein feuchter Bl\u00e4tterfall", "tokens": ["Im\u00b7mer", "geht", "ein", "feuch\u00b7ter", "Bl\u00e4t\u00b7ter\u00b7fall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "durch die Luft hinunter wie auf Stufen,", "tokens": ["durch", "die", "Luft", "hin\u00b7un\u00b7ter", "wie", "auf", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "KOKOM", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "jeder Vogelruf ist wie verrufen,", "tokens": ["je\u00b7der", "Vo\u00b7gel\u00b7ruf", "ist", "wie", "ver\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "KOKOM", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "wie vergiftet jede Nachtigall.", "tokens": ["wie", "ver\u00b7gif\u00b7tet", "je\u00b7de", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Selbst der Fr\u00fchling ist da nicht mehr gebend,", "tokens": ["Selbst", "der", "Fr\u00fch\u00b7ling", "ist", "da", "nicht", "mehr", "ge\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "diese B\u00fcsche glauben nicht an ihn;", "tokens": ["die\u00b7se", "B\u00fc\u00b7sche", "glau\u00b7ben", "nicht", "an", "ihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PTKNEG", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "ungern duftet tr\u00fcbe, \u00fcberlebend", "tokens": ["un\u00b7gern", "duf\u00b7tet", "tr\u00fc\u00b7be", ",", "\u00fc\u00b7berl\u00b7e\u00b7bend"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ADJA", "$,", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "abgestandener Jasmin", "tokens": ["ab\u00b7ge\u00b7stan\u00b7de\u00b7ner", "Jas\u00b7min"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--++", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "alt und mit Zerfallendem vermischt.", "tokens": ["alt", "und", "mit", "Zer\u00b7fal\u00b7len\u00b7dem", "ver\u00b7mischt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mit dir weiter r\u00fcckt ein B\u00fcndel M\u00fccken,", "tokens": ["Mit", "dir", "wei\u00b7ter", "r\u00fcckt", "ein", "B\u00fcn\u00b7del", "M\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "so als w\u00fcrde hinter deinem R\u00fccken", "tokens": ["so", "als", "w\u00fcr\u00b7de", "hin\u00b7ter", "dei\u00b7nem", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "alles gleich vernichtet und verwischt.", "tokens": ["al\u00b7les", "gleich", "ver\u00b7nich\u00b7tet", "und", "ver\u00b7wischt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}