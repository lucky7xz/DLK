{"textgrid.poem.57257": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht in den Ozean der Welten alle", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht in den Ozean der Welten alle", "tokens": ["Nicht", "in", "den", "O\u00b7ze\u00b7an", "der", "Wel\u00b7ten", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ART", "NN", "PIAT"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Will ich mich st\u00fcrzen! schweben nicht,", "tokens": ["Will", "ich", "mich", "st\u00fcr\u00b7zen", "!", "schwe\u00b7ben", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$.", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo die ersten Erschafnen, die Jubelch\u00f6re der S\u00f6hne des Lichts,", "tokens": ["Wo", "die", "ers\u00b7ten", "Er\u00b7schaf\u00b7nen", ",", "die", "Ju\u00b7belc\u00b7h\u00f6\u00b7re", "der", "S\u00f6h\u00b7ne", "des", "Lichts", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Anbeten, tief anbeten! und in Entz\u00fcckung vergehn!", "tokens": ["An\u00b7be\u00b7ten", ",", "tief", "an\u00b7be\u00b7ten", "!", "und", "in", "Ent\u00b7z\u00fc\u00b7ckung", "ver\u00b7gehn", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVINF", "$.", "KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Nur um den Tropfen am Eimer,", "tokens": ["Nur", "um", "den", "Trop\u00b7fen", "am", "Ei\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Um die Erde nur, will ich schweben, und anbeten!", "tokens": ["Um", "die", "Er\u00b7de", "nur", ",", "will", "ich", "schwe\u00b7ben", ",", "und", "an\u00b7be\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADV", "$,", "VMFIN", "PPER", "VVINF", "$,", "KON", "VVINF", "$."], "meter": "--+--+-+--+--", "measure": "anapaest.di.plus"}, "line.3": {"text": "Halleluja! Halleluja! Der Tropfen am Eimer", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!", "Hal\u00b7le\u00b7lu\u00b7ja", "!", "Der", "Trop\u00b7fen", "am", "Ei\u00b7mer"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ITJ", "$.", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Rann aus der Hand des Allm\u00e4chtigen auch!", "tokens": ["Rann", "aus", "der", "Hand", "des", "All\u00b7m\u00e4ch\u00b7ti\u00b7gen", "auch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Da der Hand des Allm\u00e4chtigen", "tokens": ["Da", "der", "Hand", "des", "All\u00b7m\u00e4ch\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die gr\u00f6sseren Erden entquollen!", "tokens": ["Die", "gr\u00f6s\u00b7se\u00b7ren", "Er\u00b7den", "ent\u00b7quol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Str\u00f6me des Lichts rauschten, und Siebengestirne wurden,", "tokens": ["Die", "Str\u00f6\u00b7me", "des", "Lichts", "rauschten", ",", "und", "Sie\u00b7ben\u00b7ges\u00b7tir\u00b7ne", "wur\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,", "KON", "NN", "VAFIN", "$,"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Da entrannest du, Tropfen, der Hand des Allm\u00e4chtigen!", "tokens": ["Da", "ent\u00b7ran\u00b7nest", "du", ",", "Trop\u00b7fen", ",", "der", "Hand", "des", "All\u00b7m\u00e4ch\u00b7ti\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+--+--+--", "measure": "anapaest.tetra.plus"}}, "stanza.4": {"line.1": {"text": "Da ein Strom des Lichts rauscht', und unsre Sonne wurde!", "tokens": ["Da", "ein", "Strom", "des", "Lichts", "rauscht'", ",", "und", "uns\u00b7re", "Son\u00b7ne", "wur\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ein Wogensturz sich st\u00fcrzte wie vom Felsen", "tokens": ["Ein", "Wo\u00b7gen\u00b7sturz", "sich", "st\u00fcrz\u00b7te", "wie", "vom", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Wolk' herab und den Orion g\u00fcrtete,", "tokens": ["Der", "Wol\u00b7k'", "her\u00b7ab", "und", "den", "O\u00b7rion", "g\u00fcr\u00b7te\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da entrannest du, Tropfen, der Hand des Allm\u00e4chtigen!", "tokens": ["Da", "ent\u00b7ran\u00b7nest", "du", ",", "Trop\u00b7fen", ",", "der", "Hand", "des", "All\u00b7m\u00e4ch\u00b7ti\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+--+--+--", "measure": "anapaest.tetra.plus"}}, "stanza.5": {"line.1": {"text": "Wer sind die tausendmal tausend, wer die Myriaden alle,", "tokens": ["Wer", "sind", "die", "tau\u00b7send\u00b7mal", "tau\u00b7send", ",", "wer", "die", "My\u00b7ria\u00b7den", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADV", "ADJD", "$,", "PWS", "ART", "NN", "PIS", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Welche den Tropfen bewohnen, und bewohnten? und wer bin ich?", "tokens": ["Wel\u00b7che", "den", "Trop\u00b7fen", "be\u00b7woh\u00b7nen", ",", "und", "be\u00b7wohn\u00b7ten", "?", "und", "wer", "bin", "ich", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "VVINF", "$,", "KON", "VVFIN", "$.", "KON", "PWS", "VAFIN", "PPER", "$."], "meter": "+--+--+-+-+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Halleluja dem Schaffenden! mehr wie die Erden, die quollen!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "dem", "Schaf\u00b7fen\u00b7den", "!", "mehr", "wie", "die", "Er\u00b7den", ",", "die", "quol\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$.", "ADV", "KOKOM", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+--+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Mehr, wie die Siebengestirne, die aus Strahlen zusammenstr\u00f6mten!", "tokens": ["Mehr", ",", "wie", "die", "Sie\u00b7ben\u00b7ges\u00b7tir\u00b7ne", ",", "die", "aus", "Strah\u00b7len", "zu\u00b7sam\u00b7men\u00b7str\u00f6m\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWAV", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+--+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.6": {"line.1": {"text": "Aber du Fr\u00fchlingsw\u00fcrmchen,", "tokens": ["A\u00b7ber", "du", "Fr\u00fch\u00b7lings\u00b7w\u00fcrm\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Das gr\u00fcnlichgolden neben mir spielt,", "tokens": ["Das", "gr\u00fcn\u00b7lich\u00b7gol\u00b7den", "ne\u00b7ben", "mir", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Du lebst; und bist vielleicht", "tokens": ["Du", "lebst", ";", "und", "bist", "viel\u00b7leicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KON", "VAFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ach nicht unsterblich!", "tokens": ["Ach", "nicht", "uns\u00b7terb\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "PTKNEG", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Ich bin heraus gegangen anzubeten,", "tokens": ["Ich", "bin", "he\u00b7raus", "ge\u00b7gan\u00b7gen", "an\u00b7zu\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ich weine? Vergieb, vergieb", "tokens": ["Und", "ich", "wei\u00b7ne", "?", "Ver\u00b7gieb", ",", "ver\u00b7gieb"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "NN", "$,", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Auch diese Thr\u00e4ne dem Endlichen,", "tokens": ["Auch", "die\u00b7se", "Thr\u00e4\u00b7ne", "dem", "End\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "O du, der seyn wird!", "tokens": ["O", "du", ",", "der", "seyn", "wird", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "VAINF", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Du wirst die Zweifel alle mir enth\u00fcllen,", "tokens": ["Du", "wirst", "die", "Zwei\u00b7fel", "al\u00b7le", "mir", "ent\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PIS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O du, der mich durch das dunkle Thal", "tokens": ["O", "du", ",", "der", "mich", "durch", "das", "dunk\u00b7le", "Thal"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Des Todes f\u00fchren wird! Ich lerne dann,", "tokens": ["Des", "To\u00b7des", "f\u00fch\u00b7ren", "wird", "!", "Ich", "ler\u00b7ne", "dann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VAFIN", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ob eine Seele das goldene W\u00fcrmchen hatte.", "tokens": ["Ob", "ei\u00b7ne", "See\u00b7le", "das", "gol\u00b7de\u00b7ne", "W\u00fcrm\u00b7chen", "hat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Bist du nur gebildeter Staub,", "tokens": ["Bist", "du", "nur", "ge\u00b7bil\u00b7de\u00b7ter", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sohn des Mays, so werde denn", "tokens": ["Sohn", "des", "Mays", ",", "so", "wer\u00b7de", "denn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NE", "$,", "ADV", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wieder verfliegender Staub,", "tokens": ["Wie\u00b7der", "ver\u00b7flie\u00b7gen\u00b7der", "Staub", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Oder was sonst der Ewige will!", "tokens": ["O\u00b7der", "was", "sonst", "der", "E\u00b7wi\u00b7ge", "will", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ART", "ADJA", "VMFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.10": {"line.1": {"text": "Ergeuss von neuem du, mein Auge,", "tokens": ["Er\u00b7geuss", "von", "neu\u00b7em", "du", ",", "mein", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Freudenthr\u00e4nen!", "tokens": ["Freu\u00b7dent\u00b7hr\u00e4\u00b7nen", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Du, meine Harfe,", "tokens": ["Du", ",", "mei\u00b7ne", "Har\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Preise den Herrn!", "tokens": ["Prei\u00b7se", "den", "Herrn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.11": {"line.1": {"text": "Umwunden wieder, mit Palmen", "tokens": ["Um\u00b7wun\u00b7den", "wie\u00b7der", ",", "mit", "Pal\u00b7men"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "$,", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ist meine Harf' umwunden! ich singe dem Herrn!", "tokens": ["Ist", "mei\u00b7ne", "Ha\u00b7rf'", "um\u00b7wun\u00b7den", "!", "ich", "sin\u00b7ge", "dem", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hier steh ich. Rund um mich", "tokens": ["Hier", "steh", "ich", ".", "Rund", "um", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADJD", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist Alles Allmacht! und Wunder Alles!", "tokens": ["Ist", "Al\u00b7les", "All\u00b7macht", "!", "und", "Wun\u00b7der", "Al\u00b7les", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$.", "KON", "NN", "PIS", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Mit tiefer Ehrfurcht schau ich die Sch\u00f6pfung an,", "tokens": ["Mit", "tie\u00b7fer", "Ehr\u00b7furcht", "schau", "ich", "die", "Sch\u00f6p\u00b7fung", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Denn Du!", "tokens": ["Denn", "Du", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "PPER", "$."], "meter": "++", "measure": "spondeus"}, "line.3": {"text": "Namenloser, Du!", "tokens": ["Na\u00b7men\u00b7lo\u00b7ser", ",", "Du", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Schufest sie!", "tokens": ["Schu\u00b7fest", "sie", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.13": {"line.1": {"text": "L\u00fcfte, die um mich wehn, und sanfte K\u00fchlung", "tokens": ["L\u00fcf\u00b7te", ",", "die", "um", "mich", "wehn", ",", "und", "sanf\u00b7te", "K\u00fch\u00b7lung"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "PPER", "VVINF", "$,", "KON", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Auf mein gl\u00fchendes Angesicht hauchen,", "tokens": ["Auf", "mein", "gl\u00fc\u00b7hen\u00b7des", "An\u00b7ge\u00b7sicht", "hau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Euch, wunderbare L\u00fcfte,", "tokens": ["Euch", ",", "wun\u00b7der\u00b7ba\u00b7re", "L\u00fcf\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sandte der Herr! der Unendliche!", "tokens": ["Sand\u00b7te", "der", "Herr", "!", "der", "Un\u00b7end\u00b7li\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "+--+--+--", "measure": "dactylic.tri"}}, "stanza.14": {"line.1": {"text": "Aber jetzt werden sie still, kaum athmen sie.", "tokens": ["A\u00b7ber", "jetzt", "wer\u00b7den", "sie", "still", ",", "kaum", "ath\u00b7men", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die Morgensonne wird schw\u00fcl!", "tokens": ["Die", "Mor\u00b7gen\u00b7son\u00b7ne", "wird", "schw\u00fcl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wolken str\u00f6men herauf!", "tokens": ["Wol\u00b7ken", "str\u00f6\u00b7men", "her\u00b7auf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Sichtbar ist, der komt, der Ewige!", "tokens": ["Sicht\u00b7bar", "ist", ",", "der", "komt", ",", "der", "E\u00b7wi\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PRELS", "VVFIN", "$,", "ART", "ADJA", "$."], "meter": "+-+-+----", "measure": "unknown.measure.tri"}}, "stanza.15": {"line.1": {"text": "Nun schweben sie, rauschen sie, wirbeln die Winde", "tokens": ["Nun", "schwe\u00b7ben", "sie", ",", "rau\u00b7schen", "sie", ",", "wir\u00b7beln", "die", "Win\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie beugt sich der Wald! wie hebt sich der Strom!", "tokens": ["Wie", "beugt", "sich", "der", "Wald", "!", "wie", "hebt", "sich", "der", "Strom", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN", "$.", "PWAV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sichtbar, wie du es Sterblichen seyn kanst,", "tokens": ["Sicht\u00b7bar", ",", "wie", "du", "es", "Sterb\u00b7li\u00b7chen", "seyn", "kanst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPER", "PPER", "NN", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ja, das bist du, sichtbar, Unendlicher!", "tokens": ["Ja", ",", "das", "bist", "du", ",", "sicht\u00b7bar", ",", "Un\u00b7end\u00b7li\u00b7cher", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "PPER", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}}, "stanza.16": {"line.1": {"text": "Der Wald neigt sich, der Strom fliehet, und ich", "tokens": ["Der", "Wald", "neigt", "sich", ",", "der", "Strom", "flie\u00b7het", ",", "und", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "ART", "NN", "VVFIN", "$,", "KON", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Falle nicht auf mein Angesicht?", "tokens": ["Fal\u00b7le", "nicht", "auf", "mein", "An\u00b7ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Herr! Herr! Gott! barmherzig und gn\u00e4dig!", "tokens": ["Herr", "!", "Herr", "!", "Gott", "!", "barm\u00b7her\u00b7zig", "und", "gn\u00e4\u00b7dig", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$.", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Du Naher! erbarme dich meiner!", "tokens": ["Du", "Na\u00b7her", "!", "er\u00b7bar\u00b7me", "dich", "mei\u00b7ner", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.17": {"line.1": {"text": "Z\u00fcrnest du, Herr,", "tokens": ["Z\u00fcr\u00b7nest", "du", ",", "Herr", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,"], "meter": "---+", "measure": "unknown.measure.single"}, "line.2": {"text": "Weil Nacht dein Gewand ist?", "tokens": ["Weil", "Nacht", "dein", "Ge\u00b7wand", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Diese Nacht ist Segen der Erde", "tokens": ["Die\u00b7se", "Nacht", "ist", "Se\u00b7gen", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "NN", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Vater, du z\u00fcrnest nicht!", "tokens": ["Va\u00b7ter", ",", "du", "z\u00fcr\u00b7nest", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.18": {"line.1": {"text": "Sie komt, Erfrischung auszusch\u00fctten,", "tokens": ["Sie", "komt", ",", "Er\u00b7fri\u00b7schung", "aus\u00b7zu\u00b7sch\u00fct\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00dcber den st\u00e4rkenden Halm!", "tokens": ["\u00dc\u00b7ber", "den", "st\u00e4r\u00b7ken\u00b7den", "Halm", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "\u00dcber die herzerfreuende Traube!", "tokens": ["\u00dc\u00b7ber", "die", "her\u00b7zer\u00b7freu\u00b7en\u00b7de", "Trau\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Vater, du z\u00fcrnest nicht!", "tokens": ["Va\u00b7ter", ",", "du", "z\u00fcr\u00b7nest", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.19": {"line.1": {"text": "Alles ist still vor dir, du Naher!", "tokens": ["Al\u00b7les", "ist", "still", "vor", "dir", ",", "du", "Na\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "APPR", "PPER", "$,", "PPER", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Rings umher ist alles still!", "tokens": ["Rings", "um\u00b7her", "ist", "al\u00b7les", "still", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "VAFIN", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch das W\u00fcrmchen mit Golde bedeckt, merkt auf!", "tokens": ["Auch", "das", "W\u00fcrm\u00b7chen", "mit", "Gol\u00b7de", "be\u00b7deckt", ",", "merkt", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "VVPP", "$,", "VVFIN", "PTKVZ", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Ist es vielleicht nicht seelenlos? ist es unsterblich?", "tokens": ["Ist", "es", "viel\u00b7leicht", "nicht", "see\u00b7len\u00b7los", "?", "ist", "es", "uns\u00b7terb\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$.", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.20": {"line.1": {"text": "Ach, verm\u00f6cht' ich dich, Herr, wie ich d\u00fcrste, zu preisen!", "tokens": ["Ach", ",", "ver\u00b7m\u00f6cht'", "ich", "dich", ",", "Herr", ",", "wie", "ich", "d\u00fcrs\u00b7te", ",", "zu", "prei\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "PRF", "$,", "NN", "$,", "PWAV", "PPER", "VMFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+--++-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Immer herlicher offenbarest du dich!", "tokens": ["Im\u00b7mer", "her\u00b7li\u00b7cher", "of\u00b7fen\u00b7ba\u00b7rest", "du", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Immer dunkler wird die Nacht um dich,", "tokens": ["Im\u00b7mer", "dunk\u00b7ler", "wird", "die", "Nacht", "um", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VAFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und voller von Segen!", "tokens": ["Und", "vol\u00b7ler", "von", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.21": {"line.1": {"text": "Seht ihr den Zeugen des Nahen den z\u00fcckenden Strahl?", "tokens": ["Seht", "ihr", "den", "Zeu\u00b7gen", "des", "Na\u00b7hen", "den", "z\u00fc\u00b7cken\u00b7den", "Strahl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "H\u00f6rt ihr Jehova's Donner?", "tokens": ["H\u00f6rt", "ihr", "Je\u00b7ho\u00b7va's", "Don\u00b7ner", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "H\u00f6rt ihr ihn? h\u00f6rt ihr ihn,", "tokens": ["H\u00f6rt", "ihr", "ihn", "?", "h\u00f6rt", "ihr", "ihn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "$.", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Den ersch\u00fctternden Donner des Herrn?", "tokens": ["Den", "er\u00b7sch\u00fct\u00b7tern\u00b7den", "Don\u00b7ner", "des", "Herrn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.22": {"line.1": {"text": "Herr! Herr! Gott!", "tokens": ["Herr", "!", "Herr", "!", "Gott", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Barmherzig, und gn\u00e4dig!", "tokens": ["Barm\u00b7her\u00b7zig", ",", "und", "gn\u00e4\u00b7dig", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Angebetet, gepriesen", "tokens": ["An\u00b7ge\u00b7be\u00b7tet", ",", "ge\u00b7prie\u00b7sen"], "token_info": ["word", "punct", "word"], "pos": ["VVPP", "$,", "VVPP"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sey dein herlicher Name!", "tokens": ["Sey", "dein", "her\u00b7li\u00b7cher", "Na\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.23": {"line.1": {"text": "Und die Gewitterwinde? sie tragen den Donner!", "tokens": ["Und", "die", "Ge\u00b7wit\u00b7ter\u00b7win\u00b7de", "?", "sie", "tra\u00b7gen", "den", "Don\u00b7ner", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie sie rauschen! wie sie mit lauter Woge den Wald durchstr\u00f6men!", "tokens": ["Wie", "sie", "rau\u00b7schen", "!", "wie", "sie", "mit", "lau\u00b7ter