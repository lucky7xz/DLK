{"textgrid.poem.43061": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn ich ihn manchmal sah,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich ihn manchmal sah,", "tokens": ["Wenn", "ich", "ihn", "manch\u00b7mal", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hab' ich gezittert, gebangt;", "tokens": ["Hab'", "ich", "ge\u00b7zit\u00b7tert", ",", "ge\u00b7bangt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Und dennoch wieder hab' ich", "tokens": ["Und", "den\u00b7noch", "wie\u00b7der", "hab'", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VAFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur ihn zu sehen verlangt.", "tokens": ["Nur", "ihn", "zu", "se\u00b7hen", "ver\u00b7langt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Und wenn er im Vorbeigehen", "tokens": ["Und", "wenn", "er", "im", "Vor\u00b7bei\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nur leicht mein Kleid ber\u00fchrt,", "tokens": ["Nur", "leicht", "mein", "Kleid", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hab' ich noch lang dar\u00fcber", "tokens": ["Hab'", "ich", "noch", "lang", "da\u00b7r\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ADJD", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit den Blumen diskurirt.", "tokens": ["Mit", "den", "Blu\u00b7men", "dis\u00b7ku\u00b7rirt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}