{"textgrid.poem.36192": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Altumajor sammelt wieder", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.85", "no:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Altumajor sammelt wieder", "tokens": ["Al\u00b7tu\u00b7ma\u00b7jor", "sam\u00b7melt", "wie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "ADV"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Seine Heiden bei Corduba.", "tokens": ["Sei\u00b7ne", "Hei\u00b7den", "bei", "Cor\u00b7du\u00b7ba", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Hierher, dorther kommen Scharen,", "tokens": ["Hier\u00b7her", ",", "dor\u00b7ther", "kom\u00b7men", "Scha\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ibrahim, Seviliens Sultan,", "tokens": ["Ib\u00b7ra\u00b7him", ",", "Se\u00b7vi\u00b7li\u00b7ens", "Sul\u00b7tan", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Andr' aus andern Landen Spaniens,", "tokens": ["Andr'", "aus", "an\u00b7dern", "Lan\u00b7den", "Spa\u00b7ni\u00b7ens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Von Granad' und von Abula.", "tokens": ["Von", "Gra\u00b7nad'", "und", "von", "A\u00b7bu\u00b7la", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie der Heiden Volk versammelt,", "tokens": ["Wie", "der", "Hei\u00b7den", "Volk", "ver\u00b7sam\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ward es bald dem Kaiser ruchtbar.", "tokens": ["Ward", "es", "bald", "dem", "Kai\u00b7ser", "rucht\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Der zog mit sechstausend Mannen", "tokens": ["Der", "zog", "mit", "sech\u00b7stau\u00b7send", "Man\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "CARD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Froh hin, wie zu einem Lustkampf.", "tokens": ["Froh", "hin", ",", "wie", "zu", "ei\u00b7nem", "Lust\u00b7kampf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PWAV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Da die Schlacht nun soll beginnen", "tokens": ["Da", "die", "Schlacht", "nun", "soll", "be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Au\u00dfen an der hohen Burg Wall,", "tokens": ["Au\u00b7\u00dfen", "an", "der", "ho\u00b7hen", "Burg", "Wall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "W\u00e4hlt er all die besten Ritter,", "tokens": ["W\u00e4hlt", "er", "all", "die", "bes\u00b7ten", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Reiht des ganzen Heeres Grundkraft,", "tokens": ["Reiht", "des", "gan\u00b7zen", "Hee\u00b7res", "Grund\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Ordnet die zum ersten Haufen,", "tokens": ["Ord\u00b7net", "die", "zum", "ers\u00b7ten", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Stellt zum zweiten dann die Fu\u00dfmacht,", "tokens": ["Stellt", "zum", "zwei\u00b7ten", "dann", "die", "Fu\u00df\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "In den dritten wieder Reiter,", "tokens": ["In", "den", "drit\u00b7ten", "wie\u00b7der", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADV", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "So zu meiden jeden Unfall.", "tokens": ["So", "zu", "mei\u00b7den", "je\u00b7den", "Un\u00b7fall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "So auch stellen sich die Heiden,", "tokens": ["So", "auch", "stel\u00b7len", "sich", "die", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Hatten wohl von jenen Kundschaft,", "tokens": ["Hat\u00b7ten", "wohl", "von", "je\u00b7nen", "Kund\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Ritter doch gemischt mit Fu\u00dfvolk,", "tokens": ["Rit\u00b7ter", "doch", "ge\u00b7mischt", "mit", "Fu\u00df\u00b7volk", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Dessen sah man bald die Ursach.", "tokens": ["Des\u00b7sen", "sah", "man", "bald", "die", "Ur\u00b7sach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Larven standen bei den Rossen,", "tokens": ["Lar\u00b7ven", "stan\u00b7den", "bei", "den", "Ros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Grimmer Larven eine Unzahl,", "tokens": ["Grim\u00b7mer", "Lar\u00b7ven", "ei\u00b7ne", "Un\u00b7zahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "B\u00e4rtig und geh\u00f6rnt wie Teufel.", "tokens": ["B\u00e4r\u00b7tig", "und", "ge\u00b7h\u00f6rnt", "wie", "Teu\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "KOKOM", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Wie bei h\u00f6ll'scher Geister Luftfahrt", "tokens": ["Wie", "bei", "h\u00f6ll'\u00b7scher", "Geis\u00b7ter", "Luft\u00b7fahrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Durch die wilden Wirbelwinde", "tokens": ["Durch", "die", "wil\u00b7den", "Wir\u00b7bel\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Sich die innre Bosheit Luft schafft", "tokens": ["Sich", "die", "inn\u00b7re", "Bos\u00b7heit", "Luft", "schafft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "ADJA", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "In Gekreisch und wildem Schalle;", "tokens": ["In", "Ge\u00b7kreisch", "und", "wil\u00b7dem", "Schal\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "So erklingen, heulen furchtbar,", "tokens": ["So", "er\u00b7klin\u00b7gen", ",", "heu\u00b7len", "furcht\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "T\u00f6ne seltsam von den Larven,", "tokens": ["T\u00f6\u00b7ne", "selt\u00b7sam", "von", "den", "Lar\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Zwischen dem Geklirr und Hufschlag,", "tokens": ["Zwi\u00b7schen", "dem", "Ge\u00b7klirr", "und", "Huf\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Da\u00df der Christen Rosse pl\u00f6tzlich,", "tokens": ["Da\u00df", "der", "Chris\u00b7ten", "Ros\u00b7se", "pl\u00f6tz\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Wie wenn sie des B\u00f6sen Wut fa\u00dft,", "tokens": ["Wie", "wenn", "sie", "des", "B\u00f6\u00b7sen", "Wut", "fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.35": {"text": "Unaufhaltsam, hiehin, dorthin,", "tokens": ["Un\u00b7auf\u00b7halt\u00b7sam", ",", "hie\u00b7hin", ",", "dor\u00b7thin", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "PAV", "$,", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Fliehn auf unwegsamen Fu\u00dfpfad.", "tokens": ["Fliehn", "auf", "un\u00b7weg\u00b7sa\u00b7men", "Fu\u00df\u00b7pfad", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Schon gebrochen sind der Christen", "tokens": ["Schon", "ge\u00b7bro\u00b7chen", "sind", "der", "Chris\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Scharen alle durch den Unfall.", "tokens": ["Scha\u00b7ren", "al\u00b7le", "durch", "den", "Un\u00b7fall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Karl ersinnt alsbalde Rettung,", "tokens": ["Karl", "er\u00b7sinnt", "als\u00b7bal\u00b7de", "Ret\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Da\u00df verschwinden gleich von Stund' an", "tokens": ["Da\u00df", "ver\u00b7schwin\u00b7den", "gleich", "von", "Stund'", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "ADV", "APPR", "NN", "APPR"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Mu\u00df des eitlen Zaubers Trugbild.", "tokens": ["Mu\u00df", "des", "eit\u00b7len", "Zau\u00b7bers", "Trug\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Mit dem Kreuz, der H\u00f6lle furchtbar,", "tokens": ["Mit", "dem", "Kreuz", ",", "der", "H\u00f6l\u00b7le", "furcht\u00b7bar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Stellt Turpin sich in die Reihen,", "tokens": ["Stellt", "Tur\u00b7pin", "sich", "in", "die", "Rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.44": {"text": "Dann von Leinen manchen Umhang", "tokens": ["Dann", "von", "Lei\u00b7nen", "man\u00b7chen", "Um\u00b7hang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Um der Rosse Haupt sie binden,", "tokens": ["Um", "der", "Ros\u00b7se", "Haupt", "sie", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Da\u00df der Zauber schwand von Stund' an.", "tokens": ["Da\u00df", "der", "Zau\u00b7ber", "schwand", "von", "Stund'", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "In der Sarazenen Scharen", "tokens": ["In", "der", "Sa\u00b7ra\u00b7ze\u00b7nen", "Scha\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Ward alsbald ein grauses Blutbad,", "tokens": ["Ward", "als\u00b7bald", "ein", "grau\u00b7ses", "Blut\u00b7bad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Und es flog wohl hiehin, dorthin,", "tokens": ["Und", "es", "flog", "wohl", "hie\u00b7hin", ",", "dor\u00b7thin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PAV", "$,", "ADV", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.50": {"text": "Mit dem Kopfe mancher Turban.", "tokens": ["Mit", "dem", "Kop\u00b7fe", "man\u00b7cher", "Tur\u00b7ban", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.51": {"text": "Nur die rote Fahne steht noch,", "tokens": ["Nur", "die", "ro\u00b7te", "Fah\u00b7ne", "steht", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Die den Heiden wieder Mut gab;", "tokens": ["Die", "den", "Hei\u00b7den", "wie\u00b7der", "Mut", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Keiner flieht, so lang sie weht noch,", "tokens": ["Kei\u00b7ner", "flieht", ",", "so", "lang", "sie", "weht", "noch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ADV", "ADJD", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Die auf goldnen Wagens Grund stand,", "tokens": ["Die", "auf", "gold\u00b7nen", "Wa\u00b7gens", "Grund", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Den acht wei\u00dfe Stiere zogen,", "tokens": ["Den", "acht", "wei\u00b7\u00dfe", "Stie\u00b7re", "zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Goldgeschm\u00fcckt, als wie zur Prunkfahrt.", "tokens": ["Gold\u00b7ge\u00b7schm\u00fcckt", ",", "als", "wie", "zur", "Prunk\u00b7fahrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "KOKOM", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Gott vertrauend und der R\u00fcstung,", "tokens": ["Gott", "ver\u00b7trau\u00b7end", "und", "der", "R\u00fcs\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5