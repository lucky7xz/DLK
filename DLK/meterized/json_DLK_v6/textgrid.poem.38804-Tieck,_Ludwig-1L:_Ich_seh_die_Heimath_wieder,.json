info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPRART", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.26": {"line.1": {"text": "Wie fliehen viele Wogen", "tokens": ["Wie", "flie\u00b7hen", "vie\u00b7le", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinab in Strom und Meer,", "tokens": ["Hin\u00b7ab", "in", "Strom", "und", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und muthig angeflogen", "tokens": ["Und", "mut\u00b7hig", "an\u00b7ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schwimmt neue Flut daher.", "tokens": ["Schwimmt", "neu\u00b7e", "Flut", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.27": {"line.1": {"text": "Liebe kann nicht versiegen,", "tokens": ["Lie\u00b7be", "kann", "nicht", "ver\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Sie ist ein ewger Quell,", "tokens": ["Sie", "ist", "ein", "ew\u00b7ger", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Will jedes Bild verfliegen,", "tokens": ["Will", "je\u00b7des", "Bild", "ver\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bleibt doch ihr Antlitz hell.", "tokens": ["Bleibt", "doch", "ihr", "Ant\u00b7litz", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.28": {"line.1": {"text": "Drum will ich nicht verzagen,", "tokens": ["Drum", "will", "ich", "nicht", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun singe, neues Herz,", "tokens": ["Nun", "sin\u00b7ge", ",", "neu\u00b7es", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und will ich Leiden klagen", "tokens": ["Und", "will", "ich", "Lei\u00b7den", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Versch\u00f6nt Gesang den Schmerz. \u2013", "tokens": ["Ver\u00b7sch\u00f6nt", "Ge\u00b7sang", "den", "Schmerz", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}