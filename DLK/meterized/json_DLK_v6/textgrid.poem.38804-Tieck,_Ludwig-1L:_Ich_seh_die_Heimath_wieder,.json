{"textgrid.poem.38804": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich seh die Heimath wieder,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich seh die Heimath wieder,", "tokens": ["Ich", "seh", "die", "Hei\u00b7math", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die lange ferne blieb,", "tokens": ["Die", "lan\u00b7ge", "fer\u00b7ne", "blieb", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie tr\u00e4ufelt Wonne nieder,", "tokens": ["Sie", "tr\u00e4u\u00b7felt", "Won\u00b7ne", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie hat ihr Kind so lieb.", "tokens": ["Sie", "hat", "ihr", "Kind", "so", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Voll Liebe reichen B\u00e4ume", "tokens": ["Voll", "Lie\u00b7be", "rei\u00b7chen", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir froh die gr\u00fcne Hand,", "tokens": ["Mir", "froh", "die", "gr\u00fc\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich steh und sinn' und tr\u00e4ume,", "tokens": ["Ich", "steh", "und", "sinn'", "und", "tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVIMP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alles thut bekannt.", "tokens": ["Und", "al\u00b7les", "thut", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Versp\u00e4t'te Bl\u00fcmchen ragen", "tokens": ["Ver\u00b7sp\u00e4t'\u00b7te", "Bl\u00fcm\u00b7chen", "ra\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Neugierig aus dem Gras,", "tokens": ["Neu\u00b7gie\u00b7rig", "aus", "dem", "Gras", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist als ob sie fragen", "tokens": ["Es", "ist", "als", "ob", "sie", "fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Recht z\u00e4rtlich: wer ist das?", "tokens": ["Recht", "z\u00e4rt\u00b7lich", ":", "wer", "ist", "das", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$.", "PWS", "VAFIN", "PDS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich mu\u00df sie alle gr\u00fc\u00dfen", "tokens": ["Ich", "mu\u00df", "sie", "al\u00b7le", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wieder traulich sein;", "tokens": ["Und", "wie\u00b7der", "trau\u00b7lich", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df, Blume, dich noch k\u00fcssen,", "tokens": ["La\u00df", ",", "Blu\u00b7me", ",", "dich", "noch", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie oft gedacht ich dein!", "tokens": ["Wie", "oft", "ge\u00b7dacht", "ich", "dein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVPP", "PPER", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da sind die gr\u00fcnen G\u00e4nge,", "tokens": ["Da", "sind", "die", "gr\u00fc\u00b7nen", "G\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Steine wohl bekannt,", "tokens": ["Die", "Stei\u00b7ne", "wohl", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wunderbare Kl\u00e4nge", "tokens": ["Und", "wun\u00b7der\u00b7ba\u00b7re", "Kl\u00e4n\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sind hier noch fest gebannt.", "tokens": ["Sind", "hier", "noch", "fest", "ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es ist die Nachtigalle,", "tokens": ["Es", "ist", "die", "Nach\u00b7ti\u00b7gal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie blieb an diesem Ort,", "tokens": ["Sie", "blieb", "an", "die\u00b7sem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sagt mit s\u00fc\u00dfem Schalle", "tokens": ["Und", "sagt", "mit", "s\u00fc\u00b7\u00dfem", "Schal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir noch ein scheidend Wort.", "tokens": ["Mir", "noch", "ein", "schei\u00b7dend", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wie treu ist dieser S\u00e4nger,", "tokens": ["Wie", "treu", "ist", "die\u00b7ser", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df er noch mein gedacht. \u2013", "tokens": ["Da\u00df", "er", "noch", "mein", "ge\u00b7dacht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir wird im Herzen b\u00e4nger", "tokens": ["Mir", "wird", "im", "Her\u00b7zen", "b\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier in der gr\u00fcnen Nacht.", "tokens": ["Hier", "in", "der", "gr\u00fc\u00b7nen", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie fliegen fort die T\u00f6ne,", "tokens": ["Sie", "flie\u00b7gen", "fort", "die", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Erde nimmt das Laub,", "tokens": ["Die", "Er\u00b7de", "nimmt", "das", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was gestern gr\u00fcnte sch\u00f6ne", "tokens": ["Was", "ge\u00b7stern", "gr\u00fcn\u00b7te", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist heut des Windes Raub.", "tokens": ["Ist", "heut", "des", "Win\u00b7des", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O Fr\u00fchling, hintergangen", "tokens": ["O", "Fr\u00fch\u00b7ling", ",", "hin\u00b7ter\u00b7gan\u00b7gen"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "NE", "$,", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hast du die arme Welt,", "tokens": ["Hast", "du", "die", "ar\u00b7me", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erst schl\u00e4gst du auf mit Prangen", "tokens": ["Erst", "schl\u00e4gst", "du", "auf", "mit", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und lachend dein Gezelt.", "tokens": ["Und", "la\u00b7chend", "dein", "Ge\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Es stehn wie Dienerschaaren", "tokens": ["Es", "stehn", "wie", "Die\u00b7ner\u00b7schaa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit blitzendem Gewehr,", "tokens": ["Mit", "blit\u00b7zen\u00b7dem", "Ge\u00b7wehr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor Unfall dich zu wahren,", "tokens": ["Vor", "Un\u00b7fall", "dich", "zu", "wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Blumen um dich her.", "tokens": ["Die", "Blu\u00b7men", "um", "dich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Die Wasser wie Herolde", "tokens": ["Die", "Was\u00b7ser", "wie", "He\u00b7rol\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rufen dein Kommen aus,", "tokens": ["Ru\u00b7fen", "dein", "Kom\u00b7men", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ganz ausgeschm\u00fcckt mit Golde", "tokens": ["Ganz", "aus\u00b7ge\u00b7schm\u00fcckt", "mit", "Gol\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVPP", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist deine Flur und Haus.", "tokens": ["Ist", "dei\u00b7ne", "Flur", "und", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die V\u00f6gel fliehn und ziehen,", "tokens": ["Die", "V\u00f6\u00b7gel", "fliehn", "und", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Wolken spielen sie,", "tokens": ["Mit", "Wol\u00b7ken", "spie\u00b7len", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und alle Blumen bl\u00fchen", "tokens": ["Und", "al\u00b7le", "Blu\u00b7men", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und duften sp\u00e4t und fr\u00fch.", "tokens": ["Und", "duf\u00b7ten", "sp\u00e4t", "und", "fr\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Die Rose kommt mit Scheinen,", "tokens": ["Die", "Ro\u00b7se", "kommt", "mit", "Schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ruft: nun liebet all!", "tokens": ["Und", "ruft", ":", "nun", "lie\u00b7bet", "all", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "VVFIN", "PIAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer sollte wohl nicht weinen", "tokens": ["Wer", "soll\u00b7te", "wohl", "nicht", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei diesem s\u00fcssen Schall?", "tokens": ["Bei", "die\u00b7sem", "s\u00fcs\u00b7sen", "Schall", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und wie man sich besinnet,", "tokens": ["Und", "wie", "man", "sich", "be\u00b7sin\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Auge thr\u00e4nenschwer,", "tokens": ["Das", "Au\u00b7ge", "thr\u00e4\u00b7nen\u00b7schwer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Bl\u00fcthe Frucht gewinnet", "tokens": ["Die", "Bl\u00fc\u00b7the", "Frucht", "ge\u00b7win\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ruft den Sommer her.", "tokens": ["Und", "ruft", "den", "Som\u00b7mer", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Was hilft es doch, zu fl\u00fcchten,", "tokens": ["Was", "hilft", "es", "doch", ",", "zu", "fl\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum gr\u00fcnen, k\u00fchlen Wald,", "tokens": ["Zum", "gr\u00fc\u00b7nen", ",", "k\u00fch\u00b7len", "Wald", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn hier aus allen dichten", "tokens": ["Wenn", "hier", "aus", "al\u00b7len", "dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PIAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zweigen ein Klaglied schallt?", "tokens": ["Zwei\u00b7gen", "ein", "Klag\u00b7lied", "schallt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.16": {"line.1": {"text": "Die Nachtgall will verk\u00fcnden", "tokens": ["Die", "Nacht\u00b7gall", "will", "ver\u00b7k\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was Schmerz und Liebe sey,", "tokens": ["Was", "Schmerz", "und", "Lie\u00b7be", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie kann den Ton wohl finden", "tokens": ["Sie", "kann", "den", "Ton", "wohl", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und singt ihr Herze frei.", "tokens": ["Und", "singt", "ihr", "Her\u00b7ze", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Bald werden stumm die B\u00e4ume,", "tokens": ["Bald", "wer\u00b7den", "stumm", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Blumen bl\u00fchen ab,", "tokens": ["Die", "Blu\u00b7men", "bl\u00fc\u00b7hen", "ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erwachen alle Tr\u00e4ume,", "tokens": ["Er\u00b7wa\u00b7chen", "al\u00b7le", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sehn vor sich ein Grab.", "tokens": ["Und", "sehn", "vor", "sich", "ein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Es fallen wie die Todten", "tokens": ["Es", "fal\u00b7len", "wie", "die", "Tod\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wunsch, Lust und Leben hin,", "tokens": ["Wunsch", ",", "Lust", "und", "Le\u00b7ben", "hin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verlieren gern den Othem,", "tokens": ["Ver\u00b7lie\u00b7ren", "gern", "den", "O\u00b7them", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach Sterben geht ihr Sinn.", "tokens": ["Nach", "Ster\u00b7ben", "geht", "ihr", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Da wird erzeugt in Schmerzen", "tokens": ["Da", "wird", "er\u00b7zeugt", "in", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zuletzt der hei\u00dfe Wein,", "tokens": ["Zu\u00b7letzt", "der", "hei\u00b7\u00dfe", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er ist ein wildes Scherzen", "tokens": ["Er", "ist", "ein", "wil\u00b7des", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom Tod sich zu befrein.", "tokens": ["Vom", "Tod", "sich", "zu", "be\u00b7fr\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Nun f\u00fchl' ich mich verloren", "tokens": ["Nun", "f\u00fchl'", "ich", "mich", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In finstrer Einsamkeit,", "tokens": ["In", "finst\u00b7rer", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es wird der Tod geboren,", "tokens": ["Es", "wird", "der", "Tod", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er bringt mir tiefes Leid.", "tokens": ["Er", "bringt", "mir", "tie\u00b7fes", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Die Erde ungeschm\u00fccket,", "tokens": ["Die", "Er\u00b7de", "un\u00b7ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Blumlos und ohne Gras, \u2013", "tokens": ["Blum\u00b7los", "und", "oh\u00b7ne", "Gras", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "KON", "APPR", "NN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl hab ich dich erblicket,", "tokens": ["Wohl", "hab", "ich", "dich", "er\u00b7bli\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Heimath ist nun das.", "tokens": ["Die", "Hei\u00b7math", "ist", "nun", "das", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PDS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Du rufst mit stillem Winken", "tokens": ["Du", "rufst", "mit", "stil\u00b7lem", "Win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich wie das Laub herab,", "tokens": ["Mich", "wie", "das", "Laub", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOKOM", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und gern will ich versinken", "tokens": ["Und", "gern", "will", "ich", "ver\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In dieses offne Grab.", "tokens": ["In", "die\u00b7ses", "off\u00b7ne", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Doch kommt nicht Fr\u00fchling wieder?", "tokens": ["Doch", "kommt", "nicht", "Fr\u00fch\u00b7ling", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bleibt nicht die Liebe neu?", "tokens": ["Bleibt", "nicht", "die", "Lie\u00b7be", "neu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es stehn ja muntre Lieder", "tokens": ["Es", "stehn", "ja", "mun\u00b7tre", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir baldigst wieder bei.", "tokens": ["Mir", "bal\u00b7digst", "wie\u00b7der", "bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "Hab 