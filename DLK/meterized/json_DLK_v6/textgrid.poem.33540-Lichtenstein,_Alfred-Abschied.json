{"textgrid.poem.33540": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl war ganz sch\u00f6n, ein Jahr Soldat zu sein.", "tokens": ["Wohl", "war", "ganz", "sch\u00f6n", ",", "ein", "Jahr", "Sol\u00b7dat", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "$,", "ART", "NN", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch sch\u00f6ner ist, sich wieder frei zu f\u00fchlen.", "tokens": ["Doch", "sch\u00f6\u00b7ner", "ist", ",", "sich", "wie\u00b7der", "frei", "zu", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PRF", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es gab genug Verkommenheit und Pein", "tokens": ["Es", "gab", "ge\u00b7nug", "Ver\u00b7kom\u00b7men\u00b7heit", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In diesen unbarmherzgen Menschenm\u00fchlen.", "tokens": ["In", "die\u00b7sen", "un\u00b7barm\u00b7herz\u00b7gen", "Men\u00b7schen\u00b7m\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sergeanten, Bretterw\u00e4nde, lebet wohl.", "tokens": ["Ser\u00b7ge\u00b7an\u00b7ten", ",", "Bret\u00b7ter\u00b7w\u00e4n\u00b7de", ",", "le\u00b7bet", "wohl", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Lebt wohl, Kantinen, Marschkolonnenlieder.", "tokens": ["Lebt", "wohl", ",", "Kan\u00b7ti\u00b7nen", ",", "Marschko\u00b7lon\u00b7nen\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Leichtherzig lass ich Stadt und Kapitol.", "tokens": ["Leicht\u00b7her\u00b7zig", "lass", "ich", "Stadt", "und", "Ka\u00b7pi\u00b7tol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Kuno geht, der Kuno kommt nicht wieder.", "tokens": ["Der", "Ku\u00b7no", "geht", ",", "der", "Ku\u00b7no", "kommt", "nicht", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "ART", "NE", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun, Schicksal, treib mich, wohin dir gef\u00e4llt.", "tokens": ["Nun", ",", "Schick\u00b7sal", ",", "treib", "mich", ",", "wo\u00b7hin", "dir", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ich zerre nicht an meiner Zukunft H\u00fcllen.", "tokens": ["Ich", "zer\u00b7re", "nicht", "an", "mei\u00b7ner", "Zu\u00b7kunft", "H\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich hebe meine Augen in die Welt.", "tokens": ["Ich", "he\u00b7be", "mei\u00b7ne", "Au\u00b7gen", "in", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Wind f\u00e4ngt an. Lokomotiven br\u00fcllen.", "tokens": ["Ein", "Wind", "f\u00e4ngt", "an", ".", "Lo\u00b7ko\u00b7mo\u00b7ti\u00b7ven", "br\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "NN", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}