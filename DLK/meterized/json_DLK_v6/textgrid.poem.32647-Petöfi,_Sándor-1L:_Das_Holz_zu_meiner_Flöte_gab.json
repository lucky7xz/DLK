{"textgrid.poem.32647": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Holz zu meiner Fl\u00f6te gab", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Holz zu meiner Fl\u00f6te gab", "tokens": ["Das", "Holz", "zu", "mei\u00b7ner", "Fl\u00f6\u00b7te", "gab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Trauerweide mir,", "tokens": ["Die", "Trau\u00b7er\u00b7wei\u00b7de", "mir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die steht an einem frischen Grab,", "tokens": ["Die", "steht", "an", "ei\u00b7nem", "fri\u00b7schen", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drum t\u00f6nt es bang aus ihr.", "tokens": ["Drum", "t\u00f6nt", "es", "bang", "aus", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In dieses Grab sankst du, mein Stern,", "tokens": ["In", "die\u00b7ses", "Grab", "sankst", "du", ",", "mein", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und strahlst mir nimmermehr,", "tokens": ["Und", "strahlst", "mir", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So dunkel ist's um mich herum,", "tokens": ["So", "dun\u00b7kel", "ist's", "um", "mich", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Leben z\u00e4hlt nicht mehr!", "tokens": ["Mein", "Le\u00b7ben", "z\u00e4hlt", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn abends heim die Herde zieht,", "tokens": ["Wenn", "a\u00b7bends", "heim", "die", "Her\u00b7de", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wall' ich zu diesem Grab,", "tokens": ["Wall'", "ich", "zu", "die\u00b7sem", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Fl\u00f6te seufzt ihr Klagelied,", "tokens": ["Die", "Fl\u00f6\u00b7te", "seufzt", "ihr", "Kla\u00b7ge\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mond schaut bleich herab ...", "tokens": ["Der", "Mond", "schaut", "bleich", "her\u00b7ab", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So lange zehrt der Gram an mir,", "tokens": ["So", "lan\u00b7ge", "zehrt", "der", "Gram", "an", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis meine Seele, m\u00fcd,", "tokens": ["Bis", "mei\u00b7ne", "See\u00b7le", ",", "m\u00fcd", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich mit der Fl\u00f6te Ton vereint,", "tokens": ["Sich", "mit", "der", "Fl\u00f6\u00b7te", "Ton", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in das Jenseits zieht!", "tokens": ["Und", "in", "das", "Jen\u00b7seits", "zieht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}