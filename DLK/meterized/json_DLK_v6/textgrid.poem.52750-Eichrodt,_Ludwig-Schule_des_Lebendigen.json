{"textgrid.poem.52750": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Schule des Lebendigen", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Loos der M\u00e4rtyrer, so bitters\u00fc\u00df! Sei's", "tokens": ["O", "Loos", "der", "M\u00e4r\u00b7ty\u00b7rer", ",", "so", "bit\u00b7ter\u00b7s\u00fc\u00df", "!", "Sei's"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "NE", "ART", "NN", "$,", "ADV", "ADJD", "$.", "NE"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Denn d'rum gewagt! Der schlanke W\u00fcrfel fiel.", "tokens": ["Denn", "d'\u00b7rum", "ge\u00b7wagt", "!", "Der", "schlan\u00b7ke", "W\u00fcr\u00b7fel", "fiel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$.", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nur im Exil ist heute noch Asyl.", "tokens": ["Nur", "im", "E\u00b7xil", "ist", "heu\u00b7te", "noch", "A\u00b7syl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der alte Ocean wird dich umrauschen,", "tokens": ["Der", "al\u00b7te", "O\u00b7cean", "wird", "dich", "um\u00b7rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dich hat die schale Welt verkannt;", "tokens": ["Dich", "hat", "die", "scha\u00b7le", "Welt", "ver\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Mit Menschen nimmer eitle Worte tauschen,", "tokens": ["Mit", "Men\u00b7schen", "nim\u00b7mer", "eit\u00b7le", "Wor\u00b7te", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Seit jedes Herz ein Sykofant.", "tokens": ["Seit", "je\u00b7des", "Herz", "ein", "Sy\u00b7ko\u00b7fant", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Einst grollt' auch ich: Vom Haupt die Nebelkappen!", "tokens": ["Einst", "grollt'", "auch", "ich", ":", "Vom", "Haupt", "die", "Ne\u00b7bel\u00b7kap\u00b7pen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$.", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Teufel mit der kahlen Klerisei!", "tokens": ["Zum", "Teu\u00b7fel", "mit", "der", "kah\u00b7len", "Kle\u00b7ri\u00b7sei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und frei zu sein, seid einmal nur so frei.", "tokens": ["Und", "frei", "zu", "sein", ",", "seid", "ein\u00b7mal", "nur", "so", "frei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VAINF", "$,", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jetzt kann euch meine Stimme nimmer retten,", "tokens": ["Jetzt", "kann", "euch", "mei\u00b7ne", "Stim\u00b7me", "nim\u00b7mer", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Seitdem vers\u00e4umt der Augenblick \u2013", "tokens": ["Seit\u00b7dem", "ver\u00b7s\u00e4umt", "der", "Au\u00b7gen\u00b7blick", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O knirschet nur in eure Sclavenketten,", "tokens": ["O", "knir\u00b7schet", "nur", "in", "eu\u00b7re", "Scla\u00b7ven\u00b7ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ihr macht nicht mehr die gr\u00f6\u00dfte aller Wetten!", "tokens": ["Ihr", "macht", "nicht", "mehr", "die", "gr\u00f6\u00df\u00b7te", "al\u00b7ler", "Wet\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verlor'ner Donner: ", "tokens": ["Ver\u00b7lor'\u00b7ner", "Don\u00b7ner", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ein Bess'rer war zu rechter Zeit ein Tadler,", "tokens": ["Ein", "Bess'\u00b7rer", "war", "zu", "rech\u00b7ter", "Zeit", "ein", "Tad\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Alpenschnee ging seine Spur verlor'n \u2013", "tokens": ["Im", "Al\u00b7pen\u00b7schnee", "ging", "sei\u00b7ne", "Spur", "ver\u00b7lor'n", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch hass' ich den ", "tokens": ["Noch", "hass'", "ich", "den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Doch Jene trifft mein einsam keuscher Zorn,", "tokens": ["Doch", "Je\u00b7ne", "trifft", "mein", "ein\u00b7sam", "keu\u00b7scher", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die halben Weges feige stehen bleiben", "tokens": ["Die", "hal\u00b7ben", "We\u00b7ges", "fei\u00b7ge", "ste\u00b7hen", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und mit Proclamationen nur", "tokens": ["Und", "mit", "Pro\u00b7cla\u00b7ma\u00b7ti\u00b7o\u00b7nen", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ihr eig'nes Todesurtheil unterschreiben,", "tokens": ["Ihr", "eig'\u00b7nes", "To\u00b7des\u00b7urt\u00b7heil", "un\u00b7ter\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Freiheit Genius mu\u00df sich selbst entleiben,", "tokens": ["Der", "Frei\u00b7heit", "Ge\u00b7nius", "mu\u00df", "sich", "selbst", "ent\u00b7lei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Seitdem die Freiheit Sinekur.", "tokens": ["Seit\u00b7dem", "die", "Frei\u00b7heit", "Si\u00b7ne\u00b7kur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Rei\u00dft ihr die Throne aus dem Dung der Erden,", "tokens": ["Rei\u00dft", "ihr", "die", "Thro\u00b7ne", "aus", "dem", "Dung", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie wuchern nach \u2013 das ist das Weltgericht.", "tokens": ["Sie", "wu\u00b7chern", "nach", "\u2013", "das", "ist", "das", "Welt\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "$(", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So wird Europa niemals urbar werden,", "tokens": ["So", "wird", "Eu\u00b7ro\u00b7pa", "nie\u00b7mals", "ur\u00b7bar", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Herren seid die rechten Pfl\u00fcger nicht!", "tokens": ["Ihr", "Her\u00b7ren", "seid", "die", "rech\u00b7ten", "Pfl\u00fc\u00b7ger", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr werdet nun und nimmermehr Hellenen,", "tokens": ["Ihr", "wer\u00b7det", "nun", "und", "nim\u00b7mer\u00b7mehr", "Hel\u00b7le\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KON", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das ist des Pudels letzter Kern \u2013", "tokens": ["Das", "ist", "des", "Pu\u00b7dels", "letz\u00b7ter", "Kern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So wenig als die Liebe mit Typh\u00f6nen,", "tokens": ["So", "we\u00b7nig", "als", "die", "Lie\u00b7be", "mit", "Ty\u00b7ph\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "KOKOM", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Cultur mit Barbarei sich l\u00e4\u00dft vers\u00f6hnen,", "tokens": ["Cul\u00b7tur", "mit", "Bar\u00b7ba\u00b7rei", "sich", "l\u00e4\u00dft", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PRF", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Als Judith mit dem Holofern.", "tokens": ["Als", "Ju\u00b7dith", "mit", "dem", "Ho\u00b7lo\u00b7fern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr wollt den Geist in kranke H\u00fcrden pferchen,", "tokens": ["Ihr", "wollt", "den", "Geist", "in", "kran\u00b7ke", "H\u00fcr\u00b7den", "pfer\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Angesicht des jungen Morgenroths?", "tokens": ["Im", "An\u00b7ge\u00b7sicht", "des", "jun\u00b7gen", "Mor\u00b7gen\u00b7roths", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr wahrlich h\u00f6rt nicht schmettern seine Lerchen,", "tokens": ["Ihr", "wahr\u00b7lich", "h\u00f6rt", "nicht", "schmet\u00b7tern", "sei\u00b7ne", "Ler\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Seht unter Rosen nicht das Schwert des Tod's!", "tokens": ["Seht", "un\u00b7ter", "Ro\u00b7sen", "nicht", "das", "Schwert", "des", "To\u00b7d's", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Noch ward kein Feldherr unter Euch geboren,", "tokens": ["Noch", "ward", "kein", "Feld\u00b7herr", "un\u00b7ter", "Euch", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Denn eure Schlachten waren Hohn;", "tokens": ["Denn", "eu\u00b7re", "Schlach\u00b7ten", "wa\u00b7ren", "Hohn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich habe k\u00fchnern Fahnen zugeschworen,", "tokens": ["Ich", "ha\u00b7be", "k\u00fch\u00b7nern", "Fah\u00b7nen", "zu\u00b7ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zehntausend Griechen waren nicht verloren,", "tokens": ["Zehn\u00b7tau\u00b7send", "Grie\u00b7chen", "wa\u00b7ren", "nicht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Doch Ihr habt keinen Xenophon.", "tokens": ["Doch", "Ihr", "habt", "kei\u00b7nen", "Xe\u00b7no\u00b7phon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Kredenze, Lieb', die Qual nicht zu verl\u00e4ngern,", "tokens": ["Kre\u00b7den\u00b7ze", ",", "Lieb'", ",", "die", "Qual", "nicht", "zu", "ver\u00b7l\u00e4n\u00b7gern", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die letzte Z\u00e4hre uns'res Vater Rhein!", "tokens": ["Die", "letz\u00b7te", "Z\u00e4h\u00b7re", "un\u00b7s'\u00b7res", "Va\u00b7ter", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein Pereat Europens M\u00fc\u00dfigg\u00e4ngern!", "tokens": ["Ein", "Pe\u00b7reat", "Eu\u00b7ro\u00b7pens", "M\u00fc\u00b7\u00dfig\u00b7g\u00e4n\u00b7gern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "So \u2013 lass' mich schl\u00fcrfen diese Neige Wein.", "tokens": ["So", "\u2013", "lass'", "mich", "schl\u00fcr\u00b7fen", "die\u00b7se", "Nei\u00b7ge", "Wein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VVIMP", "PPER", "VVFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von nun an soll mein Lied verblutend feiern,", "tokens": ["Von", "nun", "an", "soll", "mein", "Lied", "ver\u00b7blu\u00b7tend", "fei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APZR", "VMFIN", "PPOSAT", "NN", "VVPP", "VVINF", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Das einst der Freiheit eine Gasse war,", "tokens": ["Das", "einst", "der", "Frei\u00b7heit", "ei\u00b7ne", "Gas\u00b7se", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das Lied des M\u00fcden, welcher nicht mit euern", "tokens": ["Das", "Lied", "des", "M\u00fc\u00b7den", ",", "wel\u00b7cher", "nicht", "mit", "eu\u00b7ern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PTKNEG", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Befleckten Flaggen wollte planlos steuern,", "tokens": ["Be\u00b7fleck\u00b7ten", "Flag\u00b7gen", "woll\u00b7te", "plan\u00b7los", "steu\u00b7ern", ",