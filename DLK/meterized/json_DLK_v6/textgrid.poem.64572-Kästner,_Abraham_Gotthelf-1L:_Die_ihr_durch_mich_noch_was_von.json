{"textgrid.poem.64572": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die ihr durch mich noch was von ", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ihr durch mich noch was von ", "tokens": ["Die", "ihr", "durch", "mich", "noch", "was", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPER", "ADV", "PWS", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Den schon die Welt, in der er schreibt, vergi\u00dft,", "tokens": ["Den", "schon", "die", "Welt", ",", "in", "der", "er", "schreibt", ",", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Lernt, Enkel, da\u00df mich's reu't, Witz so verthan zu haben:", "tokens": ["Lernt", ",", "En\u00b7kel", ",", "da\u00df", "mich's", "reu't", ",", "Witz", "so", "ver\u00b7than", "zu", "ha\u00b7ben", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "KOUS", "PIS", "VVFIN", "$,", "NN", "ADV", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-++-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn Bernstein ein Insect umschlie\u00dft,", "tokens": ["Wenn", "Bern\u00b7stein", "ein", "In\u00b7sect", "um\u00b7schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu kostbar liegt die Spinne da begraben.", "token