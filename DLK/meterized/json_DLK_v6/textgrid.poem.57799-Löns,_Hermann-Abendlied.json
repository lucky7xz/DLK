{"textgrid.poem.57799": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Abendlied", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rose Marie, Rose Marie,", "tokens": ["Ro\u00b7se", "Ma\u00b7rie", ",", "Ro\u00b7se", "Ma\u00b7rie", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sieben Jahre mein Herz nach dir schrie,", "tokens": ["Sie\u00b7ben", "Jah\u00b7re", "mein", "Herz", "nach", "dir", "schrie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Rose Marie, Rose Marie,", "tokens": ["Ro\u00b7se", "Ma\u00b7rie", ",", "Ro\u00b7se", "Ma\u00b7rie", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Aber du h\u00f6rtest es nie.", "tokens": ["A\u00b7ber", "du", "h\u00f6r\u00b7test", "es", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Jedwede Nacht, jedwede Nacht,", "tokens": ["Jed\u00b7we\u00b7de", "Nacht", ",", "jed\u00b7we\u00b7de", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Hat mir im Traume dein Bild zugelacht,", "tokens": ["Hat", "mir", "im", "Trau\u00b7me", "dein", "Bild", "zu\u00b7ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kam dann der Tag, kam dann der Tag,", "tokens": ["Kam", "dann", "der", "Tag", ",", "kam", "dann", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wieder alleine ich lag.", "tokens": ["Wie\u00b7der", "al\u00b7lei\u00b7ne", "ich", "lag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Jetzt bin ich alt, jetzt bin ich alt,", "tokens": ["Jetzt", "bin", "ich", "alt", ",", "jetzt", "bin", "ich", "alt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aber mein Herz ist noch immer nicht kalt,", "tokens": ["A\u00b7ber", "mein", "Herz", "ist", "noch", "im\u00b7mer", "nicht", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Schl\u00e4ft wohl schon bald, schl\u00e4ft wohl schon bald,", "tokens": ["Schl\u00e4ft", "wohl", "schon", "bald", ",", "schl\u00e4ft", "wohl", "schon", "bald", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "$,", "VVFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch bis zuletzt es noch hallt:", "tokens": ["Doch", "bis", "zu\u00b7letzt", "es", "noch", "hallt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Rose Marie, Rose Marie,", "tokens": ["Ro\u00b7se", "Ma\u00b7rie", ",", "Ro\u00b7se", "Ma\u00b7rie", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sieben Jahre mein Herz nach dir schrie,", "tokens": ["Sie\u00b7ben", "Jah\u00b7re", "mein", "Herz", "nach", "dir", "schrie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Rose Marie, Rose Marie,", "tokens": ["Ro\u00b7se", "Ma\u00b7rie", ",", "Ro\u00b7se", "Ma\u00b7rie", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Aber du h\u00f6rtest es nie.", "tokens": ["A\u00b7ber", "du", "h\u00f6r\u00b7test", "es", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}