{"textgrid.poem.33063": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als du gestorben, hab' um's Haupt ich dir", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als du gestorben, hab' um's Haupt ich dir", "tokens": ["Als", "du", "ge\u00b7stor\u00b7ben", ",", "hab'", "um's", "Haupt", "ich", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "$,", "VAFIN", "ADJA", "NN", "PPER", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den letzten Schmuck gewunden,", "tokens": ["Den", "letz\u00b7ten", "Schmuck", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus Rosen, Myrthen und Cypressengr\u00fcn", "tokens": ["Aus", "Ro\u00b7sen", ",", "Myr\u00b7then", "und", "Cyp\u00b7res\u00b7sen\u00b7gr\u00fcn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von meiner Hand gebunden.", "tokens": ["Von", "mei\u00b7ner", "Hand", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sanft wirst du ruh'n in deiner stillen Gruft,", "tokens": ["Sanft", "wirst", "du", "ruh'n", "in", "dei\u00b7ner", "stil\u00b7len", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich aber weine, weine,", "tokens": ["Ich", "a\u00b7ber", "wei\u00b7ne", ",", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf Blatt und Bl\u00fcthe fallen Thr\u00e4nen hell", "tokens": ["Auf", "Blatt", "und", "Bl\u00fc\u00b7the", "fal\u00b7len", "Thr\u00e4\u00b7nen", "hell"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie thau'ge Edelsteine.", "tokens": ["Wie", "thau'\u00b7ge", "E\u00b7del\u00b7stei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Rosen, nimm' sie alle mit hinab,", "tokens": ["Die", "Ro\u00b7sen", ",", "nimm'", "sie", "al\u00b7le", "mit", "hin\u00b7ab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "PIS", "APPR", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich will sie dir nicht neiden,", "tokens": ["Ich", "will", "sie", "dir", "nicht", "nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Myrthen auch, es wird ihr sanfter Schmuck", "tokens": ["Die", "Myr\u00b7then", "auch", ",", "es", "wird", "ihr", "sanf\u00b7ter", "Schmuck"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nie meine Stirne kleiden.", "tokens": ["Nie", "mei\u00b7ne", "Stir\u00b7ne", "klei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hier oben will ich Eines nur f\u00fcr mich", "tokens": ["Hier", "o\u00b7ben", "will", "ich", "Ei\u00b7nes", "nur", "f\u00fcr", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PIS", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Behalten an dem Herzen;", "tokens": ["Be\u00b7hal\u00b7ten", "an", "dem", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von dir, Cypresse, nur den d\u00fcstren Zweig,", "tokens": ["Von", "dir", ",", "Cyp\u00b7res\u00b7se", ",", "nur", "den", "d\u00fcst\u00b7ren", "Zweig", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Du Bild der Trennungsschmerzen.", "tokens": ["Du", "Bild", "der", "Tren\u00b7nungs\u00b7schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So d\u00fcster ist, wie du, die Seele mir,", "tokens": ["So", "d\u00fcs\u00b7ter", "ist", ",", "wie", "du", ",", "die", "See\u00b7le", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "PWAV", "PPER", "$,", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Leid und Nacht erf\u00fcllet,", "tokens": ["Die", "Leid", "und", "Nacht", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So bl\u00fcthenlos, wie du, des Lebens Pfad", "tokens": ["So", "bl\u00fc\u00b7then\u00b7los", ",", "wie", "du", ",", "des", "Le\u00b7bens", "Pfad"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPER", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich meinem Blick enth\u00fcllet!", "tokens": ["Sich", "mei\u00b7nem", "Blick", "ent\u00b7h\u00fcl\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}