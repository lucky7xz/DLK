{"textgrid.poem.33867": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Virgo et Mater", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Jungfrau Bild,", "tokens": ["Der", "Jung\u00b7frau", "Bild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Im Arm das Kind,", "tokens": ["Im", "Arm", "das", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Blickt sanft und mild", "tokens": ["Blickt", "sanft", "und", "mild"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Durch Nacht und Wind.", "tokens": ["Durch", "Nacht", "und", "Wind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ein armes M\u00e4gdlein knie't davor,", "tokens": ["Ein", "ar\u00b7mes", "M\u00e4gd\u00b7lein", "knie't", "da\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie schaut nur dann und wann empor,", "tokens": ["Sie", "schaut", "nur", "dann", "und", "wann", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "KON", "PWAV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch, wenn das L\u00e4mpchen Funken spr\u00fcht,", "tokens": ["Doch", ",", "wenn", "das", "L\u00e4mp\u00b7chen", "Fun\u00b7ken", "spr\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So sieht man, wie sie gl\u00fcht.", "tokens": ["So", "sieht", "man", ",", "wie", "sie", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Lampe geht", "tokens": ["Die", "Lam\u00b7pe", "geht"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Auf einmal aus;", "tokens": ["Auf", "ein\u00b7mal", "aus", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ihr Athem steht,", "tokens": ["Ihr", "A\u00b7them", "steht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Sie schwankt nach Haus.", "tokens": ["Sie", "schwankt", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die Jungfrau kann ihr nicht verzeih'n,", "tokens": ["Die", "Jung\u00b7frau", "kann", "ihr", "nicht", "ver\u00b7zeih'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Mutter wird sie benedei'n,", "tokens": ["Die", "Mut\u00b7ter", "wird", "sie", "be\u00b7ne\u00b7dei'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Stellt sie der Heil'gen \u00fcber's Jahr", "tokens": ["Stellt", "sie", "der", "Heil'\u00b7gen", "\u00fc\u00b7ber's", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit ihrem Kind sich dar.", "tokens": ["Mit", "ih\u00b7rem", "Kind", "sich", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie f\u00fchlt's, und spricht:", "tokens": ["Sie", "f\u00fchlt's", ",", "und", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Du reine Magd,", "tokens": ["Du", "rei\u00b7ne", "Magd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dir gleich' ich nicht,", "tokens": ["Dir", "gleich'", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Doch unverzagt!", "tokens": ["Doch", "un\u00b7ver\u00b7zagt", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dir, Mutter, die der Sohn erkannt,", "tokens": ["Dir", ",", "Mut\u00b7ter", ",", "die", "der", "Sohn", "er\u00b7kannt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die unter'm Kreuz noch bei ihm stand,", "tokens": ["Die", "un\u00b7ter'm", "Kreuz", "noch", "bei", "ihm", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dir will ich gleichen f\u00fcr und f\u00fcr,", "tokens": ["Dir", "will", "ich", "glei\u00b7chen", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJA", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und dann vergiebst du mir!", "tokens": ["Und", "dann", "ver\u00b7giebst", "du", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}