{"dta.poem.1551": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uberschrifft auff die Grufft Hertzog CARLS  \n von Lotthringen/ sieghafften K\u00e4yserlichen  \n Generalissimi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "So geht zur Mittags-Zeit der Tugend Sonne nieder/", "tokens": ["So", "geht", "zur", "Mit\u00b7tags\u00b7Zeit", "der", "Tu\u00b7gend", "Son\u00b7ne", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und l\u00e4st die schwartze Grufft verdunckeln ihren Schein/", "tokens": ["Und", "l\u00e4st", "die", "schwart\u00b7ze", "Grufft", "ver\u00b7dun\u00b7ckeln", "ih\u00b7ren", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nein: Ihre Stralen schleust kein d\u00fcstres Grabmahl ein/", "tokens": ["Nein", ":", "Ih\u00b7re", "Stra\u00b7len", "schleust", "kein", "d\u00fcst\u00b7res", "Grab\u00b7mahl", "ein", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPOSAT", "NN", "VVFIN", "PIAT", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr Ruff schallt in der Lufft wie Memnons S\u00e4ule wider.", "tokens": ["Ihr", "Ruff", "schallt", "in", "der", "Lufft", "wie", "Mem\u00b7nons", "S\u00e4u\u00b7le", "wi\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "KOKOM", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mu\u00df hier der theure Zweig von Boullion verwesen/", "tokens": ["Mu\u00df", "hier", "der", "theu\u00b7re", "Zweig", "von", "Boul\u00b7li\u00b7on", "ver\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So kan man seinen Ruhm doch bey den Sternen lesen.", "tokens": ["So", "kan", "man", "sei\u00b7nen", "Ruhm", "doch", "bey", "den", "Ster\u00b7nen", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vor CARLES gro\u00dfen Geist war Lotthringen zu enge;", "tokens": ["Vor", "CaR\u00b7LES", "gro\u00b7\u00dfen", "Geist", "war", "Lot\u00b7thrin\u00b7gen", "zu", "en\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "VAFIN", "NN", "APPR", "ADJA", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Nachdem er keinen Raum in seinen Gr\u00e4ntzen fand/", "tokens": ["Nach\u00b7dem", "er", "kei\u00b7nen", "Raum", "in", "sei\u00b7nen", "Gr\u00e4nt\u00b7zen", "fand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So waren Ost und West sein Sitz und Vaterland.", "tokens": ["So", "wa\u00b7ren", "Ost", "und", "West", "sein", "Sitz", "und", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Rhein- und Donau-Strom z\u00e4hlt seiner Thaten Menge.", "tokens": ["Der", "Rhein", "und", "Do\u00b7nau\u00b7Strom", "z\u00e4hlt", "sei\u00b7ner", "Tha\u00b7ten", "Men\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er hie\u00df die stoltze Sonn in vollem Lauffe stehen/", "tokens": ["Er", "hie\u00df", "die", "stolt\u00b7ze", "Sonn", "in", "vol\u00b7lem", "Lauf\u00b7fe", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den Monden von Byzanz in tr\u00fcben Wolcken gehen.", "tokens": ["Den", "Mon\u00b7den", "von", "By\u00b7zanz", "in", "tr\u00fc\u00b7ben", "Wol\u00b7cken", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}