{"dta.poem.2319": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Versuch  \n von den Eigenschaften  \n eines  \n  Kunstrichters.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1741", "urn": "urn:nbn:de:kobv:b4-200905198534", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Dichter f\u00fchrt uns stets durch die smaragdnen Felder.", "tokens": ["Ein", "Dich\u00b7ter", "f\u00fchrt", "uns", "stets", "durch", "die", "sma\u00b7ragd\u00b7nen", "Fel\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wohin? Ich wei\u00df es schon. In dikbelaubte W\u00e4lder;", "tokens": ["Wo\u00b7hin", "?", "Ich", "wei\u00df", "es", "schon", ".", "In", "dik\u00b7be\u00b7laub\u00b7te", "W\u00e4l\u00b7der", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VVFIN", "PPER", "ADV", "$.", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da sucht sein Coridon im Schatten seine Ruh,", "tokens": ["Da", "sucht", "sein", "Co\u00b7ri\u00b7don", "im", "Schat\u00b7ten", "sei\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und schleu\u00dft (bald thu ichs auch) die m\u00fcden Augen zu.", "tokens": ["Und", "schleu\u00dft", "(", "bald", "thu", "ichs", "auch", ")", "die", "m\u00fc\u00b7den", "Au\u00b7gen", "zu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ADV", "VVFIN", "PIS", "ADV", "$(", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}