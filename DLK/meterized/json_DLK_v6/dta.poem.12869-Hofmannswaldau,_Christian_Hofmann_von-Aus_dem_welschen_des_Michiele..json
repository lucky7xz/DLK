{"dta.poem.12869": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem welschen des   Michiele.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kommt dich ein starcker durst bey hei\u00dfem wetter an,", "tokens": ["Kommt", "dich", "ein", "star\u00b7cker", "durst", "bey", "hei\u00b7\u00dfem", "wet\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So will dir ieder brunn mit frischem wasser dienen.", "tokens": ["So", "will", "dir", "ie\u00b7der", "brunn", "mit", "fri\u00b7schem", "was\u00b7ser", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie aber, da\u00df mein geist aus deinen mund-rubinen", "tokens": ["Wie", "a\u00b7ber", ",", "da\u00df", "mein", "geist", "aus", "dei\u00b7nen", "mun\u00b7dru\u00b7bi\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht auch, wie Sylvia, erqvickung sch\u00f6pffen kan?", "tokens": ["Nicht", "auch", ",", "wie", "Syl\u00b7via", ",", "er\u00b7qvic\u00b7kung", "sch\u00f6pf\u00b7fen", "kan", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "PWAV", "NE", "$,", "NN", "VVINF", "VMFIN", "$."], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Die kalten brunnen sind mitleidender als du;", "tokens": ["Die", "kal\u00b7ten", "brun\u00b7nen", "sind", "mit\u00b7lei\u00b7den\u00b7der", "als", "du", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie stillen dir den durst, den sie doch nicht erwecket,", "tokens": ["Sie", "stil\u00b7len", "dir", "den", "durst", ",", "den", "sie", "doch", "nicht", "er\u00b7we\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADV", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du aber hast mein hertz selbst in den brand gestecket,", "tokens": ["Du", "a\u00b7ber", "hast", "mein", "hertz", "selbst", "in", "den", "brand", "ge\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und schlie\u00dfest ihm gleichwol den brunn der k\u00fchlung zu.", "tokens": ["Und", "schlie\u00b7\u00dfest", "ihm", "gleich\u00b7wol", "den", "brunn", "der", "k\u00fch\u00b7lung", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}