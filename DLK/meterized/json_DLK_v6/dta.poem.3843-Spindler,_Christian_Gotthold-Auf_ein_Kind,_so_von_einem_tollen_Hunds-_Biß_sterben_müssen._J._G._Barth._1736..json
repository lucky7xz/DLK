{"dta.poem.3843": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein Kind, so von einem tollen Hunds-  \n Bi\u00df sterben m\u00fcssen.  \n  J. G. Barth.   1736.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Leser! wilt du etwa wissen,", "tokens": ["Mein", "Le\u00b7ser", "!", "wilt", "du", "et\u00b7wa", "wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Woran denn ich gestorben bin;", "tokens": ["Wo\u00b7ran", "denn", "ich", "ge\u00b7stor\u00b7ben", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KON", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich hat ein toller Hund gebissen,", "tokens": ["Mich", "hat", "ein", "tol\u00b7ler", "Hund", "ge\u00b7bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So fiel ich gantz entgeistert hin.", "tokens": ["So", "fiel", "ich", "gantz", "ent\u00b7geis\u00b7tert", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und so hab ich mein junges Leben", "tokens": ["Und", "so", "hab", "ich", "mein", "jun\u00b7ges", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Durch einen Hunds-Bi\u00df aufgegeben.", "tokens": ["Durch", "ei\u00b7nen", "Hunds\u00b7Bi\u00df", "auf\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Drum, Leser! mercke di\u00df dabey,", "tokens": ["Drum", ",", "Le\u00b7ser", "!", "mer\u00b7cke", "di\u00df", "da\u00b7bey", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$.", "VVFIN", "PDS", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Todes-Art ist vielerley.", "tokens": ["Die", "To\u00b7des\u00b7Art", "ist", "vie\u00b7ler\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}