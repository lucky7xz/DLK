{"dta.poem.23568": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die beiden Strau\u00dfe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei Strau\u00dfe sind anjetzt vorhanden,", "tokens": ["Zwei", "Strau\u00b7\u00dfe", "sind", "an\u00b7jetzt", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Zwei Strau\u00dfe von verschiedner Art;", "tokens": ["Zwei", "Strau\u00b7\u00dfe", "von", "ver\u00b7schied\u00b7ner", "Art", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Paar wie sich's in allen Landen", "tokens": ["Ein", "Paar", "wie", "sich's", "in", "al\u00b7len", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "PIS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch niemals hat geoffenbart.", "tokens": ["Noch", "nie\u00b7mals", "hat", "ge\u00b7of\u00b7fen\u00b7bart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Man muss sie h\u00f6ren, muss sie lesen,", "tokens": ["Man", "muss", "sie", "h\u00f6\u00b7ren", ",", "muss", "sie", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und mancher wird davon entz\u00fcckt,", "tokens": ["Und", "man\u00b7cher", "wird", "da\u00b7von", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mancher kann nicht mehr genesen,", "tokens": ["Und", "man\u00b7cher", "kann", "nicht", "mehr", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er wird halb n\u00e4rrisch und verr\u00fcckt.", "tokens": ["Er", "wird", "halb", "n\u00e4r\u00b7risch", "und", "ver\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wenn der eine musicieret,", "tokens": ["Und", "wenn", "der", "ei\u00b7ne", "mu\u00b7si\u00b7cie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Spatzieren wir ins Himmelshaus,", "tokens": ["Spat\u00b7zie\u00b7ren", "wir", "ins", "Him\u00b7mels\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn der andre disputieret,", "tokens": ["Und", "wenn", "der", "and\u00b7re", "dis\u00b7pu\u00b7tie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jagt er Gott Vater selbst hinaus.", "tokens": ["Jagt", "er", "Gott", "Va\u00b7ter", "selbst", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "NN", "ADV", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "K\u00f6nnt' ich ein kleines F\u00fcrstlein werden", "tokens": ["K\u00f6nnt'", "ich", "ein", "klei\u00b7nes", "F\u00fcrst\u00b7lein", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VAINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Gottes Gnad' und Volkes Gold,", "tokens": ["Von", "Got\u00b7tes", "Gnad'", "und", "Vol\u00b7kes", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So n\u00e4hm' ich f\u00fcr die Volksbeschwerden", "tokens": ["So", "n\u00e4hm'", "ich", "f\u00fcr", "die", "Volks\u00b7be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die beiden Strau\u00df' in meinen Sold.", "tokens": ["Die", "bei\u00b7den", "Strau\u00df'", "in", "mei\u00b7nen", "Sold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der eine w\u00e4re mein Minister", "tokens": ["Der", "ei\u00b7ne", "w\u00e4\u00b7re", "mein", "Mi\u00b7nis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr's Budjet und die Kabbala,", "tokens": ["F\u00fcr's", "Bud\u00b7jet", "und", "die", "Kab\u00b7ba\u00b7la", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er lehrte dann die Herrn Philister,", "tokens": ["Er", "lehr\u00b7te", "dann", "die", "Herrn", "Phi\u00b7lis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sie einstimmig spr\u00e4chen: ", "tokens": ["Wie", "sie", "ein\u00b7stim\u00b7mig", "spr\u00e4\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er sollte darthun in Sermonen", "tokens": ["Er", "soll\u00b7te", "dar\u00b7thun", "in", "Ser\u00b7mo\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PAV", "APPR", "NN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Begreiflich f\u00fcr ein jedes Kind,", "tokens": ["Be\u00b7greif\u00b7lich", "f\u00fcr", "ein", "je\u00b7des", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Volk und Constitutionen", "tokens": ["Da\u00df", "Volk", "und", "Con\u00b7sti\u00b7tu\u00b7ti\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht viel, gar nichts, nur Mythen sind.", "tokens": ["Nicht", "viel", ",", "gar", "nichts", ",", "nur", "My\u00b7then", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "ADV", "PIS", "$,", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Den andern w\u00fcrd' ich nur verwenden,", "tokens": ["Den", "an\u00b7dern", "w\u00fcrd'", "ich", "nur", "ver\u00b7wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn's Aufruhr g\u00e4b' und Mord und Brand,", "tokens": ["Wenn's", "Auf\u00b7ruhr", "g\u00e4b'", "und", "Mord", "und", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er w\u00fcrde mit der Geig' in H\u00e4nden", "tokens": ["Er", "w\u00fcr\u00b7de", "mit", "der", "Geig'", "in", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich b\u00e4ndigen das ganze Land.", "tokens": ["Gleich", "b\u00e4n\u00b7di\u00b7gen", "das", "gan\u00b7ze", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Trotzdem hab' ich in unsern Tagen", "tokens": ["Trotz\u00b7dem", "hab'", "ich", "in", "un\u00b7sern", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor keinem Strau\u00dfe Furcht und Graun:", "tokens": ["Vor", "kei\u00b7nem", "Strau\u00b7\u00dfe", "Furcht", "und", "Graun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Zeit hat einen Strau\u00dfenmagen,", "tokens": ["Die", "Zeit", "hat", "ei\u00b7nen", "Strau\u00b7\u00dfen\u00b7ma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird auch den Doctor Strau\u00df verdaun.", "tokens": ["Wird", "auch", "den", "Doc\u00b7tor", "Strau\u00df", "ver\u00b7daun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}