{"dta.poem.928": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "HeLIAN", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "In den einsamen Stunden des Geistes", "tokens": ["In", "den", "ein\u00b7sa\u00b7men", "Stun\u00b7den", "des", "Geis\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ist es sch\u00f6n, in der Sonne zu gehn", "tokens": ["Ist", "es", "sch\u00f6n", ",", "in", "der", "Son\u00b7ne", "zu", "gehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "An den gelben Mauern des Sommers hin.", "tokens": ["An", "den", "gel\u00b7ben", "Mau\u00b7ern", "des", "Som\u00b7mers", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Leise klingen die Schritte im Gras; doch immer schl\u00e4ft", "tokens": ["Lei\u00b7se", "klin\u00b7gen", "die", "Schrit\u00b7te", "im", "Gras", ";", "doch", "im\u00b7mer", "schl\u00e4ft"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$.", "ADV", "ADV", "VVFIN"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Der Sohn des Pan im grauen Marmor.", "tokens": ["Der", "Sohn", "des", "Pan", "im", "grau\u00b7en", "Mar\u00b7mor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Abends auf der Terasse betranken wir uns mit braunem Wein.", "tokens": ["A\u00b7bends", "auf", "der", "Ter\u00b7as\u00b7se", "be\u00b7tran\u00b7ken", "wir", "uns", "mit", "brau\u00b7nem", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "R\u00f6tlich gl\u00fcht der Pfirsich im Laub;", "tokens": ["R\u00f6t\u00b7lich", "gl\u00fcht", "der", "Pfir\u00b7sich", "im", "Laub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sanfte Sonate, frohes Lachen.", "tokens": ["Sanf\u00b7te", "So\u00b7na\u00b7te", ",", "fro\u00b7hes", "La\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6n ist die Stille der Nacht.", "tokens": ["Sch\u00f6n", "ist", "die", "Stil\u00b7le", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Auf dunklem Plan", "tokens": ["Auf", "dunk\u00b7lem", "Plan"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Begegnen wir uns mit Hirten und wei\u00dfen Sternen.", "tokens": ["Be\u00b7geg\u00b7nen", "wir", "uns", "mit", "Hir\u00b7ten", "und", "wei\u00b7\u00dfen", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Wenn es Herbst geworden ist", "tokens": ["Wenn", "es", "Herbst", "ge\u00b7wor\u00b7den", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "VAPP", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeigt sich n\u00fcchterne Klarheit im Hain.", "tokens": ["Zeigt", "sich", "n\u00fcch\u00b7ter\u00b7ne", "Klar\u00b7heit", "im", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Bes\u00e4nftigte wandeln wir an roten Mauern hin", "tokens": ["Be\u00b7s\u00e4nf\u00b7tig\u00b7te", "wan\u00b7deln", "wir", "an", "ro\u00b7ten", "Mau\u00b7ern", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und die runden Augen folgen dem Flug der V\u00f6gel.", "tokens": ["Und", "die", "run\u00b7den", "Au\u00b7gen", "fol\u00b7gen", "dem", "Flug", "der", "V\u00f6\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Am Abend sinkt das wei\u00dfe Wasser in Graburnen.", "tokens": ["Am", "A\u00b7bend", "sinkt", "das", "wei\u00b7\u00dfe", "Was\u00b7ser", "in", "Gra\u00b7bur\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+--++-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "In kahlen Gezweigen feiert der Himmel.", "tokens": ["In", "kah\u00b7len", "Ge\u00b7zwei\u00b7gen", "fei\u00b7ert", "der", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In reinen H\u00e4nden tr\u00e4gt der Landmann Brot und Wein", "tokens": ["In", "rei\u00b7nen", "H\u00e4n\u00b7den", "tr\u00e4gt", "der", "Land\u00b7mann", "Brot", "und", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und friedlich reifen die Fr\u00fcchte in sonniger Kammer.", "tokens": ["Und", "fried\u00b7lich", "rei\u00b7fen", "die", "Fr\u00fcch\u00b7te", "in", "son\u00b7ni\u00b7ger", "Kam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "O wie ernst ist das Antlitz der teueren Toten.", "tokens": ["O", "wie", "ernst", "ist", "das", "Ant\u00b7litz", "der", "teu\u00b7e\u00b7ren", "To\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Doch die Seele erfreut gerechtes Anschaun.", "tokens": ["Doch", "die", "See\u00b7le", "er\u00b7freut", "ge\u00b7rech\u00b7tes", "An\u00b7schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Gewaltig ist das Schweigen des verw\u00fcsteten Gartens", "tokens": ["Ge\u00b7wal\u00b7tig", "ist", "das", "Schwei\u00b7gen", "des", "ver\u00b7w\u00fcs\u00b7te\u00b7ten", "Gar\u00b7tens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da der junge Novize die Stirne mit braunem Laub bekr\u00e4nzt,", "tokens": ["Da", "der", "jun\u00b7ge", "No\u00b7vi\u00b7ze", "die", "Stir\u00b7ne", "mit", "brau\u00b7nem", "Laub", "be\u00b7kr\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Sein Odem eisiges Gold trinkt.", "tokens": ["Sein", "O\u00b7dem", "ei\u00b7si\u00b7ges", "Gold", "trinkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+++", "measure": "unknown.measure.penta"}}, "stanza.8": {"line.1": {"text": "Die H\u00e4nde r\u00fchren das Alter bl\u00e4ulicher Wasser", "tokens": ["Die", "H\u00e4n\u00b7de", "r\u00fch\u00b7ren", "das", "Al\u00b7ter", "bl\u00e4u\u00b7li\u00b7cher", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Oder in kalter Nacht die wei\u00dfen Wangen der Schwestern.", "tokens": ["O\u00b7der", "in", "kal\u00b7ter", "Nacht", "die", "wei\u00b7\u00dfen", "Wan\u00b7gen", "der", "Schwes\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Leise und harmonisch ist ein Gang an freundlichen Zimmern hin,", "tokens": ["Lei\u00b7se", "und", "har\u00b7mo\u00b7nisch", "ist", "ein", "Gang", "an", "freund\u00b7li\u00b7chen", "Zim\u00b7mern", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+-+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wo Einsamkeit ist und das Rauschen des Ahorns,", "tokens": ["Wo", "Ein\u00b7sam\u00b7keit", "ist", "und", "das", "Rau\u00b7schen", "des", "A\u00b7horns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wo vielleicht noch die Drossel singt.", "tokens": ["Wo", "viel\u00b7leicht", "noch", "die", "Dros\u00b7sel", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Sch\u00f6n ist der Mensch und erscheinend im Dunkel,", "tokens": ["Sch\u00f6n", "ist", "der", "Mensch", "und", "er\u00b7schei\u00b7nend", "im", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "KON", "VVPP", "APPRART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Wenn er staunend Arme und Beine bewegt,", "tokens": ["Wenn", "er", "stau\u00b7nend", "Ar\u00b7me", "und", "Bei\u00b7ne", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und in purpurnen H\u00f6hlen stille die Augen rollen.", "tokens": ["Und", "in", "pur\u00b7pur\u00b7nen", "H\u00f6h\u00b7len", "stil\u00b7le", "die", "Au\u00b7gen", "rol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "Zur Vesper verliert sich der Fremdling in schwarzer Novemberzerst\u00f6rung,", "tokens": ["Zur", "Ves\u00b7per", "ver\u00b7liert", "sich", "der", "Fremd\u00b7ling", "in", "schwar\u00b7zer", "No\u00b7vem\u00b7ber\u00b7zer\u00b7st\u00f6\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "Unter morschem Ge\u00e4st, an Mauern voll Aussatz hin,", "tokens": ["Un\u00b7ter", "mor\u00b7schem", "Ge\u00b7\u00e4st", ",", "an", "Mau\u00b7ern", "voll", "Aus\u00b7satz", "hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "ADJD", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wo vordem der heilige Bruder gegangen,", "tokens": ["Wo", "vor\u00b7dem", "der", "hei\u00b7li\u00b7ge", "Bru\u00b7der", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "----+--+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Versunken in das sanfte Saitenspiel seines Wahnsinns,", "tokens": ["Ver\u00b7sun\u00b7ken", "in", "das", "sanf\u00b7te", "Sai\u00b7ten\u00b7spiel", "sei\u00b7nes", "Wahn\u00b7sinns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "O wie einsam endet der Abendwind.", "tokens": ["O", "wie", "ein\u00b7sam", "en\u00b7det", "der", "Ab\u00b7end\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ersterbend neigt sich das Haupt im Dunkel des \u00d6lbaums.", "tokens": ["E\u00b7rster\u00b7bend", "neigt", "sich", "das", "Haupt", "im", "Dun\u00b7kel", "des", "\u00d6l\u00b7baums", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.13": {"line.1": {"text": "Ersch\u00fctternd ist der Untergang des Geschlechts.", "tokens": ["Er\u00b7sch\u00fct\u00b7ternd", "ist", "der", "Un\u00b7ter\u00b7gang", "des", "Ge\u00b7schlechts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "In dieser Stunde f\u00fcllen sich die Augen des Schauenden", "tokens": ["In", "die\u00b7ser", "Stun\u00b7de", "f\u00fcl\u00b7len", "sich", "die", "Au\u00b7gen", "des", "Schau\u00b7en\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PRF", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+--+--", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mit dem Gold seiner Sterne.", "tokens": ["Mit", "dem", "Gold", "sei\u00b7ner", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.14": {"line.1": {"text": "Am Abend versinkt ein Glockenspiel, das nicht mehr t\u00f6nt", "tokens": ["Am", "A\u00b7bend", "ver\u00b7sinkt", "ein", "Glo\u00b7cken\u00b7spiel", ",", "das", "nicht", "mehr", "t\u00f6nt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "PTKNEG", "ADV", "VVFIN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Verfallen die schwarzen Mauern am Platz,", "tokens": ["Ver\u00b7fal\u00b7len", "die", "schwar\u00b7zen", "Mau\u00b7ern", "am", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ruft der tote Soldat zum Gebet.", "tokens": ["Ruft", "der", "to\u00b7te", "Sol\u00b7dat", "zum", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.15": {"line.1": {"text": "Ein bleicher Engel", "tokens": ["Ein", "blei\u00b7cher", "En\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Tritt der Sohn ins leere Haus seiner V\u00e4ter.", "tokens": ["Tritt", "der", "Sohn", "ins", "lee\u00b7re", "Haus", "sei\u00b7ner", "V\u00e4\u00b7ter", "."], "token_info": ["word", "word", "word", "w