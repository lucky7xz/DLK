{"textgrid.poem.66785": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Eisgang", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein leiser Tauwind pfiff wohl \u00fcber Nacht \u2013", "tokens": ["Ein", "lei\u00b7ser", "Tau\u00b7wind", "pfiff", "wohl", "\u00fc\u00b7ber", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6rst du, wie fern die starre Decke kracht?", "tokens": ["H\u00f6rst", "du", ",", "wie", "fern", "die", "star\u00b7re", "De\u00b7cke", "kracht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Froste war der Welle Trieb verdorrt,", "tokens": ["Im", "Fros\u00b7te", "war", "der", "Wel\u00b7le", "Trieb", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nun heult ein Wind, das ist das Losungswort.", "tokens": ["Nun", "heult", "ein", "Wind", ",", "das", "ist", "das", "Lo\u00b7sungs\u00b7wort", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den warmen Hauch versp\u00fcrt die steife Flut,", "tokens": ["Den", "war\u00b7men", "Hauch", "ver\u00b7sp\u00fcrt", "die", "stei\u00b7fe", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Laut \u00e4chzt und st\u00f6hnt, die todesmatt geruht.", "tokens": ["Laut", "\u00e4chzt", "und", "st\u00f6hnt", ",", "die", "to\u00b7des\u00b7matt", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "KON", "VVFIN", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Schwer seufzend ringt der Schrei der Not sich los,", "tokens": ["Schwer", "seuf\u00b7zend", "ringt", "der", "Schrei", "der", "Not", "sich", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "ART", "NN", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Bann der l\u00e4hmenden Gewalt ist gro\u00df,", "tokens": ["Der", "Bann", "der", "l\u00e4h\u00b7men\u00b7den", "Ge\u00b7walt", "ist", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die Flut will Luft, und wildgequ\u00e4lt emp\u00f6rt", "tokens": ["Die", "Flut", "will", "Luft", ",", "und", "wild\u00b7ge\u00b7qu\u00e4lt", "em\u00b7p\u00f6rt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "NN", "$,", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Sie sich der Fessel, die ihr Atmen st\u00f6rt.", "tokens": ["Sie", "sich", "der", "Fes\u00b7sel", ",", "die", "ihr", "At\u00b7men", "st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "H\u00f6rst du das Schnauben dort? Sieh da den Ri\u00df!", "tokens": ["H\u00f6rst", "du", "das", "Schnau\u00b7ben", "dort", "?", "Sieh", "da", "den", "Ri\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$.", "NE", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Schon quillt die Flut aus enger Finsternis;", "tokens": ["Schon", "quillt", "die", "Flut", "aus", "en\u00b7ger", "Fins\u00b7ter\u00b7nis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ein neuer Spalt, schrill klagend birst das Eis,", "tokens": ["Ein", "neu\u00b7er", "Spalt", ",", "schrill", "kla\u00b7gend", "birst", "das", "Eis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Das keinen Halt vor Flut und Winden wei\u00df.", "tokens": ["Das", "kei\u00b7nen", "Halt", "vor", "Flut", "und", "Win\u00b7den", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der See springt auf, allm\u00e4chtig wogt der Hauch,", "tokens": ["Der", "See", "springt", "auf", ",", "all\u00b7m\u00e4ch\u00b7tig", "wogt", "der", "Hauch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Eist\u00e4nzer ihr, ahnt die Gefahr ihr auch?", "tokens": ["Eis\u00b7t\u00e4n\u00b7zer", "ihr", ",", "ahnt", "die", "Ge\u00b7fahr", "ihr", "auch", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VVFIN", "ART", "NN", "PPER", "ADV", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.17": {"text": "La\u00dft nun das Spiel, das mit Verderben droht,", "tokens": ["La\u00dft", "nun", "das", "Spiel", ",", "das", "mit", "Ver\u00b7der\u00b7ben", "droht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und geht ans Werk, und zimmert euch ein Boot!", "tokens": ["Und", "geht", "ans", "Werk", ",", "und", "zim\u00b7mert", "euch", "ein", "Boot", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ein Boot aus gutem, wohlgef\u00fcgtem Holz,", "tokens": ["Ein", "Boot", "aus", "gu\u00b7tem", ",", "wohl\u00b7ge\u00b7f\u00fcg\u00b7tem", "Holz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Wild schweift die Woge, wenn sie jauchzend schmolz.", "tokens": ["Wild", "schweift", "die", "Wo\u00b7ge", ",", "wenn", "sie", "jauch\u00b7zend", "schmolz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "ADJD", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.21": {"text": "La\u00dft Mast und Segel fest und sicher sein,", "tokens": ["La\u00dft", "Mast", "und", "Se\u00b7gel", "fest", "und", "si\u00b7cher", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "PTKVZ", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Denn jetzo braust ein Fr\u00fchlingssturm herein", "tokens": ["Denn", "jet\u00b7zo", "braust", "ein", "Fr\u00fch\u00b7lings\u00b7sturm", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und w\u00fchlt empor die Tiefe, die euch tr\u00e4gt,", "tokens": ["Und", "w\u00fchlt", "em\u00b7por", "die", "Tie\u00b7fe", ",", "die", "euch", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und opfert jeden, der nicht rasch sich regt,", "tokens": ["Und", "op\u00b7fert", "je\u00b7den", ",", "der", "nicht", "rasch", "sich", "regt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "$,", "PRELS", "PTKNEG", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Das Steuer richtend mit der starken Hand", "tokens": ["Das", "Steu\u00b7er", "rich\u00b7tend", "mit", "der", "star\u00b7ken", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Auf unsrer Zukunft neues Menschenland.", "tokens": ["Auf", "uns\u00b7rer", "Zu\u00b7kunft", "neu\u00b7es", "Men\u00b7schen\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}