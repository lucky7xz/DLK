{"textgrid.poem.55927": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Das Kapit\u00e4l", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sich aus eines Traumes Ausgeburten", "tokens": ["Wie", "sich", "aus", "ei\u00b7nes", "Trau\u00b7mes", "Aus\u00b7ge\u00b7bur\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "aufsteigend aus verwirrendem Gequ\u00e4l", "tokens": ["auf\u00b7stei\u00b7gend", "aus", "ver\u00b7wir\u00b7ren\u00b7dem", "Ge\u00b7qu\u00e4l"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der n\u00e4chste Tag erhebt: so gehn die Gurten", "tokens": ["der", "n\u00e4chs\u00b7te", "Tag", "er\u00b7hebt", ":", "so", "gehn", "die", "Gur\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "der W\u00f6lbung aus dem wirren Kapit\u00e4l", "tokens": ["der", "W\u00f6l\u00b7bung", "aus", "dem", "wir\u00b7ren", "Ka\u00b7pi\u00b7t\u00e4l"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "und lassen drin, gedr\u00e4ngt und r\u00e4tselhaft", "tokens": ["und", "las\u00b7sen", "drin", ",", "ge\u00b7dr\u00e4ngt", "und", "r\u00e4t\u00b7sel\u00b7haft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "VVPP", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "verschlungen, fl\u00fcgelschlagende Gesch\u00f6pfe:", "tokens": ["ver\u00b7schlun\u00b7gen", ",", "fl\u00fc\u00b7gel\u00b7schla\u00b7gen\u00b7de", "Ge\u00b7sch\u00f6p\u00b7fe", ":"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ihr Z\u00f6gern und das Pl\u00f6tzliche der K\u00f6pfe", "tokens": ["ihr", "Z\u00f6\u00b7gern", "und", "das", "Pl\u00f6tz\u00b7li\u00b7che", "der", "K\u00f6p\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und jene starken Bl\u00e4tter, deren Saft", "tokens": ["und", "je\u00b7ne", "star\u00b7ken", "Bl\u00e4t\u00b7ter", ",", "de\u00b7ren", "Saft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "wie J\u00e4hzorn steigt, sich schlie\u00dflich \u00fcberschlagend", "tokens": ["wie", "J\u00e4h\u00b7zorn", "steigt", ",", "sich", "schlie\u00df\u00b7lich", "\u00fc\u00b7bersc\u00b7hla\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "$,", "PRF", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in einer schnellen Geste, die sich ballt", "tokens": ["in", "ei\u00b7ner", "schnel\u00b7len", "Ges\u00b7te", ",", "die", "sich", "ballt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und sich heraush\u00e4lt \u2013: alles aufw\u00e4rtsjagend,", "tokens": ["und", "sich", "her\u00b7aus\u00b7h\u00e4lt", "\u2013", ":", "al\u00b7les", "auf\u00b7w\u00e4rts\u00b7ja\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "$(", "$.", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "was immer wieder mit dem Dunkel kalt", "tokens": ["was", "im\u00b7mer", "wie\u00b7der", "mit", "dem", "Dun\u00b7kel", "kalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "herunterf\u00e4llt, wie Regen Sorge tragend", "tokens": ["her\u00b7un\u00b7ter\u00b7f\u00e4llt", ",", "wie", "Re\u00b7gen", "Sor\u00b7ge", "tra\u00b7gend"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "PWAV", "NN", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "f\u00fcr dieses alten Wachstums Unterhalt.", "tokens": ["f\u00fcr", "die\u00b7ses", "al\u00b7ten", "Wachs\u00b7tums", "Un\u00b7ter\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}