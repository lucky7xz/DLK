{"dta.poem.14647": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der G\u00f6tter Irrfahrt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und f\u00fcr immer da verschlagen", "tokens": ["Und", "f\u00fcr", "im\u00b7mer", "da", "ver\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blieben sie im fremden Land,", "tokens": ["Blie\u00b7ben", "sie", "im", "frem\u00b7den", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rten Nachts des Vaters Klagen", "tokens": ["H\u00f6r\u00b7ten", "Nachts", "des", "Va\u00b7ters", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oft noch fern vom G\u00f6tterstrand. \u2014", "tokens": ["Oft", "noch", "fern", "vom", "G\u00f6t\u00b7ter\u00b7strand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und nun Kindeskinder m\u00fcssen", "tokens": ["Und", "nun", "Kin\u00b7des\u00b7kin\u00b7der", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nach der Heimath seh'n in's Meer,", "tokens": ["Nach", "der", "Hei\u00b7math", "seh'n", "in's", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und es kommt im Wind ein Gr\u00fc\u00dfen,", "tokens": ["Und", "es", "kommt", "im", "Wind", "ein", "Gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und sie wissen nicht woher.", "tokens": ["Und", "sie", "wis\u00b7sen", "nicht", "wo\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}