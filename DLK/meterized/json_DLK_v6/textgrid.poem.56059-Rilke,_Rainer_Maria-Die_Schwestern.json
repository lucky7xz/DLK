{"textgrid.poem.56059": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Schwestern", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh, wie sie dieselben M\u00f6glichkeiten", "tokens": ["Sieh", ",", "wie", "sie", "die\u00b7sel\u00b7ben", "M\u00f6g\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "PPER", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "anders an sich tragen und verstehn,", "tokens": ["an\u00b7ders", "an", "sich", "tra\u00b7gen", "und", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "so als s\u00e4he man verschiedne Zeiten", "tokens": ["so", "als", "s\u00e4\u00b7he", "man", "ver\u00b7schied\u00b7ne", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "VVFIN", "PIS", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "durch zwei gleiche Zimmer gehn.", "tokens": ["durch", "zwei", "glei\u00b7che", "Zim\u00b7mer", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jede meint die andere zu st\u00fctzen,", "tokens": ["Je\u00b7de", "meint", "die", "an\u00b7de\u00b7re", "zu", "st\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "w\u00e4hrend sie doch m\u00fcde an ihr ruht;", "tokens": ["w\u00e4h\u00b7rend", "sie", "doch", "m\u00fc\u00b7de", "an", "ihr", "ruht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "und sie k\u00f6nnen nicht einander n\u00fctzen,", "tokens": ["und", "sie", "k\u00f6n\u00b7nen", "nicht", "ein\u00b7an\u00b7der", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "denn sie legen Blut auf Blut,", "tokens": ["denn", "sie", "le\u00b7gen", "Blut", "auf", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "wenn sie sich wie fr\u00fcher sanft ber\u00fchren", "tokens": ["wenn", "sie", "sich", "wie", "fr\u00fc\u00b7her", "sanft", "be\u00b7r\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "KOKOM", "ADJD", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und versuchen, die Allee entlang", "tokens": ["und", "ver\u00b7su\u00b7chen", ",", "die", "Al\u00b7lee", "ent\u00b7lang"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVINF", "$,", "ART", "NN", "APPO"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "sich gef\u00fchrt zu f\u00fchlen und zu f\u00fchren:", "tokens": ["sich", "ge\u00b7f\u00fchrt", "zu", "f\u00fch\u00b7len", "und", "zu", "f\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVPP", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ach, sie haben nicht denselben Gang.", "tokens": ["Ach", ",", "sie", "ha\u00b7ben", "nicht", "den\u00b7sel\u00b7ben", "Gang", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PTKNEG", "PDAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}