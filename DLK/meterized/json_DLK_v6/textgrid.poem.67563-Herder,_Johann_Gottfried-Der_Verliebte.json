{"textgrid.poem.67563": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Verliebte", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie so bla\u00df und bleich, o J\u00fcngling,", "tokens": ["Wie", "so", "bla\u00df", "und", "bleich", ",", "o", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "KON", "ADJD", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie so bleich und bla\u00df?", "tokens": ["Wie", "so", "bleich", "und", "bla\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Kannst Du munter nicht gefallen,", "tokens": ["Kannst", "Du", "mun\u00b7ter", "nicht", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleich \u2013 gef\u00e4llt denn das?", "tokens": ["Bleich", "\u2013", "ge\u00b7f\u00e4llt", "denn", "das", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "VVPP", "KON", "PDS", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wie so stumm und dumm, o J\u00fcngling,", "tokens": ["Wie", "so", "stumm", "und", "dumm", ",", "o", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "KON", "ADJD", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie so stumm und dumm?", "tokens": ["Wie", "so", "stumm", "und", "dumm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Kannst Du sprechend sie nicht r\u00fchren,", "tokens": ["Kannst", "Du", "spre\u00b7chend", "sie", "nicht", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00fchrt man sie denn stumm?", "tokens": ["R\u00fchrt", "man", "sie", "denn", "stumm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Sprich, und sch\u00e4me Dich, o J\u00fcngling,", "tokens": ["Sprich", ",", "und", "sch\u00e4\u00b7me", "Dich", ",", "o", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KON", "VVFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwingt das Alles wol?", "tokens": ["Zwingt", "das", "Al\u00b7les", "wol", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIS", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Will sie willig Dich nicht lieben,", "tokens": ["Will", "sie", "wil\u00b7lig", "Dich", "nicht", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprich, warum sie soll?", "tokens": ["Sprich", ",", "wa\u00b7rum", "sie", "soll", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Sprich, bist Du nicht toll?", "tokens": ["Sprich", ",", "bist", "Du", "nicht", "toll", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}