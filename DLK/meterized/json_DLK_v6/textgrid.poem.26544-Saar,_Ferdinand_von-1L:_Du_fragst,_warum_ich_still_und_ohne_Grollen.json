{"textgrid.poem.26544": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du fragst, warum ich still und ohne Grollen", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du fragst, warum ich still und ohne Grollen", "tokens": ["Du", "fragst", ",", "wa\u00b7rum", "ich", "still", "und", "oh\u00b7ne", "Grol\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "ADJD", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich schm\u00e4hen lasse, statt in Wort und Liedern", "tokens": ["Mich", "schm\u00e4\u00b7hen", "las\u00b7se", ",", "statt", "in", "Wort", "und", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "VVFIN", "$,", "KOUI", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf Dies und Jenes strafend zu erwidern", "tokens": ["Auf", "Dies", "und", "Je\u00b7nes", "stra\u00b7fend", "zu", "er\u00b7wi\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "KON", "PDS", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und zu entlarven, die mir \u00fcbel wollen.", "tokens": ["Und", "zu", "ent\u00b7lar\u00b7ven", ",", "die", "mir", "\u00fc\u00b7bel", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "ADJD", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Befrag' mich nicht! Des Lebens Stunden rollen,", "tokens": ["Be\u00b7frag'", "mich", "nicht", "!", "Des", "Le\u00b7bens", "Stun\u00b7den", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$.", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wer da k\u00e4mpfen wollte mit dem Nieder'n", "tokens": ["Und", "wer", "da", "k\u00e4mp\u00b7fen", "woll\u00b7te", "mit", "dem", "Nie\u00b7der'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "VVINF", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und endlich gar sein Wesen noch zergliedern:", "tokens": ["Und", "end\u00b7lich", "gar", "sein", "We\u00b7sen", "noch", "zer\u00b7glie\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der d\u00fcrfte nie dem dunklen F\u00e4hrmann zollen.", "tokens": ["Der", "d\u00fcrf\u00b7te", "nie", "dem", "dunk\u00b7len", "F\u00e4hr\u00b7mann", "zol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Faust ist l\u00e4ngst verp\u00f6nt in uns'ren Tagen,", "tokens": ["Die", "Faust", "ist", "l\u00e4ngst", "ver\u00b7p\u00f6nt", "in", "un\u00b7s'\u00b7ren", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "D'rum kannst du deine St\u00e4rke nur bezeigen", "tokens": ["D'\u00b7rum", "kannst", "du", "dei\u00b7ne", "St\u00e4r\u00b7ke", "nur", "be\u00b7zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Im stolzen \u00dcbersehen und Ertragen.", "tokens": ["Im", "stol\u00b7zen", "\u00dc\u00b7ber\u00b7se\u00b7hen", "und", "Er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es braus't zu laut umher der tolle Reigen;", "tokens": ["Es", "braus't", "zu", "laut", "um\u00b7her", "der", "tol\u00b7le", "Rei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Thor mag immerhin die Lunge wagen \u2013", "tokens": ["Ein", "Thor", "mag", "im\u00b7mer\u00b7hin", "die", "Lun\u00b7ge", "wa\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich habe l\u00e4ngst, mein Freund, gelernt zu schweigen.", "tokens": ["Ich", "ha\u00b7be", "l\u00e4ngst", ",", "mein", "Freund", ",", "ge\u00b7lernt", "zu", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}