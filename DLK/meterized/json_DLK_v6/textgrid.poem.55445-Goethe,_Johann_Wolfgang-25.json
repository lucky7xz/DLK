{"textgrid.poem.55445": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "25", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Luthers Fest mit gl\u00e4ubiger Schar", "tokens": ["Als", "Lu\u00b7thers", "Fest", "mit", "gl\u00e4u\u00b7bi\u00b7ger", "Schar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Im vorigen Herbst gefeiert war,", "tokens": ["Im", "vo\u00b7ri\u00b7gen", "Herbst", "ge\u00b7fei\u00b7ert", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dacht ich, es brauche hundert Jahr',", "tokens": ["Dacht", "ich", ",", "es", "brau\u00b7che", "hun\u00b7dert", "Jahr'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um es mit W\u00fcrde zu erneuen;", "tokens": ["Um", "es", "mit", "W\u00fcr\u00b7de", "zu", "er\u00b7neu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch beim verliehnen Ehrenbild,", "tokens": ["Doch", "beim", "ver\u00b7lieh\u00b7nen", "Eh\u00b7ren\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie ernst es ist und kr\u00e4ftig mild,", "tokens": ["Wie", "ernst", "es", "ist", "und", "kr\u00e4f\u00b7tig", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VAFIN", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Beim Herkules und seinem Schild", "tokens": ["Beim", "Her\u00b7ku\u00b7les", "und", "sei\u00b7nem", "Schild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kann ich der Feier mich an jedem Tage freuen.", "tokens": ["Kann", "ich", "der", "Fei\u00b7er", "mich", "an", "je\u00b7dem", "Ta\u00b7ge", "freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PRF", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}