{"textgrid.poem.60563": {"metadata": {"author": {"name": "Lange, Samuel Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auch ich mein Thirsis greiffe nach dem Lorbeer;", "genre": "verse", "period": "N.A.", "pub_year": 1746, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch ich mein Thirsis greiffe nach dem Lorbeer;", "tokens": ["Auch", "ich", "mein", "Thir\u00b7sis", "greif\u00b7fe", "nach", "dem", "Lor\u00b7beer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich habe muthiger als Herkules", "tokens": ["Ich", "ha\u00b7be", "mut\u00b7hi\u00b7ger", "als", "Her\u00b7ku\u00b7les"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "KOKOM", "NE"], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Den alten Vorurtheilen angesieget.", "tokens": ["Den", "al\u00b7ten", "Vor\u00b7urt\u00b7hei\u00b7len", "an\u00b7ge\u00b7sie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die harte Clio wird mir nun geneigter,", "tokens": ["Die", "har\u00b7te", "Clio", "wird", "mir", "nun", "ge\u00b7neig\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sie ist der unbewegten Treu gewichen,", "tokens": ["Sie", "ist", "der", "un\u00b7be\u00b7weg\u00b7ten", "Treu", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und ihre, mir zwar nicht geschenckte, Gunst", "tokens": ["Und", "ih\u00b7re", ",", "mir", "zwar", "nicht", "ge\u00b7schenck\u00b7te", ",", "Gunst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPOSAT", "$,", "PPER", "ADV", "PTKNEG", "VVFIN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Darf ich doch einmahl zu erlangen hoffen.", "tokens": ["Darf", "ich", "doch", "ein\u00b7mahl", "zu", "er\u00b7lan\u00b7gen", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du aber wirst wohl meinen Stoltz befahren;", "tokens": ["Du", "a\u00b7ber", "wirst", "wohl", "mei\u00b7nen", "Stoltz", "be\u00b7fah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du schaust mitleidig von der H\u00f6h herab,", "tokens": ["Du", "schaust", "mit\u00b7lei\u00b7dig", "von", "der", "H\u00f6h", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und siehst das Zittern meiner schwachen Knochen,", "tokens": ["Und", "siehst", "das", "Zit\u00b7tern", "mei\u00b7ner", "schwa\u00b7chen", "Kno\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und die dem schwachen Fu\u00df zu schwere M\u00fche;", "tokens": ["Und", "die", "dem", "schwa\u00b7chen", "Fu\u00df", "zu", "schwe\u00b7re", "M\u00fc\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch, siehst du nicht, wie, an dem rechten Arme", "tokens": ["Doch", ",", "siehst", "du", "nicht", ",", "wie", ",", "an", "dem", "rech\u00b7ten", "Ar\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Horatz, mich leitend, geht? Wie er mein Spiel", "tokens": ["Ho\u00b7ratz", ",", "mich", "lei\u00b7tend", ",", "geht", "?", "Wie", "er", "mein", "Spiel"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "ADJD", "$,", "VVFIN", "$.", "PWAV", "PPER", "PPOSAT", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Selbst stimmt, und mir die hohen Griffe zeiget?", "tokens": ["Selbst", "stimmt", ",", "und", "mir", "die", "ho\u00b7hen", "Grif\u00b7fe", "zei\u00b7get", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KON", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Lezt war ich an der Doris Seit entschlafen;", "tokens": ["Lezt", "war", "ich", "an", "der", "Do\u00b7ris", "Seit", "ent\u00b7schla\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als Flaccus nebst der Clio zu mir kam.", "tokens": ["Als", "Flac\u00b7cus", "nebst", "der", "Clio", "zu", "mir", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Der Epheu beugte sich um seine Scheitel,", "tokens": ["Der", "E\u00b7pheu", "beug\u00b7te", "sich", "um", "sei\u00b7ne", "Schei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein R\u00f6misches Gewand flo\u00df von den Schultern.", "tokens": ["Ein", "R\u00f6\u00b7mi\u00b7sches", "Ge\u00b7wand", "flo\u00df", "von", "den", "Schul\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich sahe sein dir \u00e4hnliches Gesichte;", "tokens": ["Ich", "sa\u00b7he", "sein", "dir", "\u00e4hn\u00b7li\u00b7ches", "Ge\u00b7sich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nur strich er langsam an dem Boden hin,", "tokens": ["Nur", "strich", "er", "lang\u00b7sam", "an", "dem", "Bo\u00b7den", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und warf auf mich die aufmercksamen Augen.", "tokens": ["Und", "warf", "auf", "mich", "die", "auf\u00b7merck\u00b7sa\u00b7men", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du, der mein Lied, sprach er, allein erh\u00f6het,", "tokens": ["Du", ",", "der", "mein", "Lied", ",", "sprach", "er", ",", "al\u00b7lein", "er\u00b7h\u00f6\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich sehe deinen Schwei\u00df mitleidig an;", "tokens": ["Ich", "se\u00b7he", "dei\u00b7nen", "Schwei\u00df", "mit\u00b7lei\u00b7dig", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du wilst mein R\u00f6misch Spiel die Deutschen lehren,", "tokens": ["Du", "wilst", "mein", "R\u00f6\u00b7misch", "Spiel", "die", "Deut\u00b7schen", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das du nicht durch den leeren Reim verstimmest;", "tokens": ["Das", "du", "nicht", "durch", "den", "lee\u00b7ren", "Reim", "ver\u00b7stim\u00b7mest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du gibst dir M\u00fch dich in die H\u00f6h zu schwingen;", "tokens": ["Du", "gibst", "dir", "M\u00fch", "dich", "in", "die", "H\u00f6h", "zu", "schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So gehst du mir auf allen Schritten nach,", "tokens": ["So", "gehst", "du", "mir", "auf", "al\u00b7len", "Schrit\u00b7ten", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Drum will jetzt Clio deinen Wunsch erf\u00fcllen.", "tokens": ["Drum", "will", "jetzt", "Clio", "dei\u00b7nen", "Wunsch", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ADV", "NE", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Drauf gab er mir die krumme Lesbsche Leyer;", "tokens": ["Drauf", "gab", "er", "mir", "die", "krum\u00b7me", "Lesb\u00b7sche", "Le\u00b7yer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ich griff begierig zu. Es wich der Schlaf.", "tokens": ["Ich", "griff", "be\u00b7gie\u00b7rig", "zu", ".", "Es", "wich", "der", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich f\u00fchlt in meiner Brust ein feurig Rasen,", "tokens": ["Ich", "f\u00fchlt", "in", "mei\u00b7ner", "Brust", "ein", "feu\u00b7rig", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit fl\u00fcchtgen Schwingen steig ich in die H\u00f6he", "tokens": ["Mit", "fl\u00fccht\u00b7gen", "Schwin\u00b7gen", "steig", "ich", "in", "die", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und sehe unter mir den tr\u00e4gen P\u00f6bel,", "tokens": ["Und", "se\u00b7he", "un\u00b7ter", "mir", "den", "tr\u00e4\u00b7gen", "P\u00f6\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und liege schwebend sicher auf der Luft,", "tokens": ["Und", "lie\u00b7ge", "schwe\u00b7bend", "si\u00b7cher", "auf", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und \u00fcbe die noch ungewohnten Fl\u00fcgel.", "tokens": ["Und", "\u00fc\u00b7be", "die", "noch", "un\u00b7ge\u00b7wohn\u00b7ten", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So f\u00fchlt der k\u00fchne Adler sich zum ersten,", "tokens": ["So", "f\u00fchlt", "der", "k\u00fch\u00b7ne", "Ad\u00b7ler", "sich", "zum", "ers\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PRF", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn nun sein nackter Fl\u00fcgel sich verkielt.", "tokens": ["Wenn", "nun", "sein", "nack\u00b7ter", "Fl\u00fc\u00b7gel", "sich", "ver\u00b7kielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er schl\u00e4gt die Fittige, die er versuchet,", "tokens": ["Er", "schl\u00e4gt", "die", "Fit\u00b7ti\u00b7ge", ",", "die", "er", "ver\u00b7su\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und fliegt erst furchtsam auf die n\u00e4chsten Aeste,", "tokens": ["Und", "fliegt", "erst", "furcht\u00b7sam", "auf", "die", "n\u00e4chs\u00b7ten", "A\u00b7es\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Von dannen hebt er sich begierig weiter,", "tokens": ["Von", "dan\u00b7nen", "hebt", "er", "sich", "be\u00b7gie\u00b7rig", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und steiget auf den n\u00e4chsten hohen Baum,", "tokens": ["Und", "stei\u00b7get", "auf", "den", "n\u00e4chs\u00b7ten", "ho\u00b7hen", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und sieht den Himmel und den Wald verwundernd.", "tokens": ["Und", "sieht", "den", "Him\u00b7mel", "und", "den", "Wald", "ver\u00b7wun\u00b7dernd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Hier wird mein Blick des hellen Lichts gewohnet;", "tokens": ["Hier", "wird", "mein", "Blick", "des", "hel\u00b7len", "Lichts", "ge\u00b7woh\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann eilt mein Flug mit starckem Schwung zur Sonne", "tokens": ["Dann", "eilt", "mein", "Flug", "mit", "star\u00b7ckem", "Schwung", "zur", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du bist mein unerschrockener Gef\u00e4hrt,", "tokens": ["Du", "bist", "mein", "un\u00b7er\u00b7schro\u00b7cke\u00b7ner", "Ge\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.4": {"text": "Und ungekr\u00e4nckt, da\u00df uns kein K\u00f6nig h\u00f6ret,", "tokens": ["Und", "un\u00b7ge\u00b7kr\u00e4nckt", ",", "da\u00df", "uns", "kein", "K\u00f6\u00b7nig", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,",