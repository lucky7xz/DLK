{"textgrid.poem.47016": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo Mittagsgluten br\u00fcten auf den Thalen,", "tokens": ["Wo", "Mit\u00b7tags\u00b7glu\u00b7ten", "br\u00fc\u00b7ten", "auf", "den", "Tha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ohne Regung stehn des Berges Eichen,", "tokens": ["Und", "oh\u00b7ne", "Re\u00b7gung", "stehn", "des", "Ber\u00b7ges", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Weg der Kirsche Wangen rot sich malen,", "tokens": ["Am", "Weg", "der", "Kir\u00b7sche", "Wan\u00b7gen", "rot", "sich", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sanft am Abhang Sommersaaten bleichen;", "tokens": ["Und", "sanft", "am", "Ab\u00b7hang", "Som\u00b7mer\u00b7saa\u00b7ten", "blei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Heb' ich mich hin zu meiner Liebe Reichen", "tokens": ["Heb'", "ich", "mich", "hin", "zu", "mei\u00b7ner", "Lie\u00b7be", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf alten Pfaden aber-, abermalen,", "tokens": ["Auf", "al\u00b7ten", "Pfa\u00b7den", "a\u00b7ber", ",", "a\u00b7ber\u00b7ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "TRUNC", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stets hoffend auch mit meiner Inbrunst Qualen", "tokens": ["Stets", "hof\u00b7fend", "auch", "mit", "mei\u00b7ner", "In\u00b7brunst", "Qua\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Ziel als wie der Sommer zu erreichen.", "tokens": ["Mein", "Ziel", "als", "wie", "der", "Som\u00b7mer", "zu", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOUS", "KOKOM", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch eh' ich auch nur eines Keimchens Schimmer", "tokens": ["Doch", "eh'", "ich", "auch", "nur", "ei\u00b7nes", "Keim\u00b7chens", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entlocken kann, ist mir der Tag zerronnen,", "tokens": ["Ent\u00b7lo\u00b7cken", "kann", ",", "ist", "mir", "der", "Tag", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kalt geh' ich mit der kalten Nacht von hinnen", "tokens": ["Kalt", "geh'", "ich", "mit", "der", "kal\u00b7ten", "Nacht", "von", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "APPR", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und schw\u00f6r's beim blassen Mond: Nun kehr' ich nimmer!", "tokens": ["Und", "schw\u00f6r's", "beim", "blas\u00b7sen", "Mond", ":", "Nun", "kehr'", "ich", "nim\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch ach, schon morgen sehn die gl\u00fch'nden Sonnen", "tokens": ["Doch", "ach", ",", "schon", "mor\u00b7gen", "sehn", "die", "gl\u00fch'n\u00b7den", "Son\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "ADV", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den neuen Kreislauf gl\u00fchend mich beginnen.", "tokens": ["Den", "neu\u00b7en", "Kreis\u00b7lauf", "gl\u00fc\u00b7hend", "mich", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}