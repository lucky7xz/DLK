{"textgrid.poem.64662": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Wuth, sich Andern vorzulesen,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Wuth, sich Andern vorzulesen,", "tokens": ["Die", "Wuth", ",", "sich", "An\u00b7dern", "vor\u00b7zu\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRF", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist sonst der Dichter Wuth gewesen;", "tokens": ["Ist", "sonst", "der", "Dich\u00b7ter", "Wuth", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ist euch ", "tokens": ["Doch", "ist", "euch"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "So les't ihn vor, da\u00df er euch h\u00f6rt.", "tokens": ["So", "les't", "ihn", "vor", ",", "da\u00df", "er", "euch", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}