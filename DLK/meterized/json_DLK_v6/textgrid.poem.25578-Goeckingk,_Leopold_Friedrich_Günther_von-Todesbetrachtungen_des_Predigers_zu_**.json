{"textgrid.poem.25578": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Todesbetrachtungen des Predigers zu **", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O da\u00df ich tausend Zungen h\u00e4tte,", "tokens": ["O", "da\u00df", "ich", "tau\u00b7send", "Zun\u00b7gen", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "CARD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Euch zuzurufen: Kurz ist unsers Lebens Lauf!", "tokens": ["Euch", "zu\u00b7zu\u00b7ru\u00b7fen", ":", "Kurz", "ist", "un\u00b7sers", "Le\u00b7bens", "Lauf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "$.", "ADJD", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ach! wie mancher ging gesund zu Bette,", "tokens": ["Und", "ach", "!", "wie", "man\u00b7cher", "ging", "ge\u00b7sund", "zu", "Bet\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PWAV", "PIS", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und stand todt wieder auf!", "tokens": ["Und", "stand", "todt", "wie\u00b7der", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}