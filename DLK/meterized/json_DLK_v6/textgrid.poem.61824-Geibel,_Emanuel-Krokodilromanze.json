{"textgrid.poem.61824": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Krokodilromanze", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein altes Krokodil", "tokens": ["Ich", "bin", "ein", "al\u00b7tes", "Kro\u00b7ko\u00b7dil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sah schon die Osirisfeier;", "tokens": ["Und", "sah", "schon", "die", "O\u00b7si\u00b7ris\u00b7fei\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Bei Tage sonn' ich mich im Nil,", "tokens": ["Bei", "Ta\u00b7ge", "sonn'", "ich", "mich", "im", "Nil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei Nacht am Strande leg' ich Eier.", "tokens": ["Bei", "Nacht", "am", "Stran\u00b7de", "leg'", "ich", "Ei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df mit list'gem Wehgekreisch", "tokens": ["Ich", "wei\u00df", "mit", "list'\u00b7gem", "Weh\u00b7ge\u00b7kreisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir stets die Mahlzeit zu erw\u00fcrken;", "tokens": ["Mir", "stets", "die", "Mahl\u00b7zeit", "zu", "er\u00b7w\u00fcr\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gew\u00f6hnlich fress' ich Mohrenfleisch", "tokens": ["Ge\u00b7w\u00f6hn\u00b7lich", "fress'", "ich", "Moh\u00b7ren\u00b7fleisch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Sonntags manchmal einen T\u00fcrken.", "tokens": ["Und", "Sonn\u00b7tags", "manch\u00b7mal", "ei\u00b7nen", "T\u00fcr\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wenn im gelben Mondlicht rings", "tokens": ["Und", "wenn", "im", "gel\u00b7ben", "Mond\u00b7licht", "rings"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPRART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Strand liegt und die Felsenbr\u00fcche,", "tokens": ["Der", "Strand", "liegt", "und", "die", "Fel\u00b7sen\u00b7br\u00fc\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Tanz' ich vor einer alten Sphinx", "tokens": ["Tanz'", "ich", "vor", "ei\u00b7ner", "al\u00b7ten", "Sphinx"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lausch' auf ihrer Weisheit Spr\u00fcche.", "tokens": ["Und", "lausch'", "auf", "ih\u00b7rer", "Weis\u00b7heit", "Spr\u00fc\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Klauen in den Sand gepflanzt,", "tokens": ["Die", "Klau\u00b7en", "in", "den", "Sand", "ge\u00b7pflanzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tiefsinnig spricht sie: \u00bbTochter Thebens,", "tokens": ["Tief\u00b7sin\u00b7nig", "spricht", "sie", ":", "\u00bb", "Toch\u00b7ter", "The\u00b7bens", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$.", "$(", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Fri\u00df nur, was du verdauen kannst!", "tokens": ["Fri\u00df", "nur", ",", "was", "du", "ver\u00b7dau\u00b7en", "kannst", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist das R\u00e4tsel deines Lebens.\u00ab", "tokens": ["Das", "ist", "das", "R\u00e4t\u00b7sel", "dei\u00b7nes", "Le\u00b7bens", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}