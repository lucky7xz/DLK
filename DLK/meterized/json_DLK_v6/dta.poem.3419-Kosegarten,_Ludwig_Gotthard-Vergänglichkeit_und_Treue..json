{"dta.poem.3419": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Verg\u00e4nglichkeit und Treue.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Ida, unsre sch\u00f6ne Flur", "tokens": ["O", "I\u00b7da", ",", "uns\u00b7re", "sch\u00f6\u00b7ne", "Flur"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tr\u00e4gt trauernd der Verwesung Spur;", "tokens": ["Tr\u00e4gt", "trau\u00b7ernd", "der", "Ver\u00b7we\u00b7sung", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Ida, unser trautes Thal", "tokens": ["O", "I\u00b7da", ",", "un\u00b7ser", "trau\u00b7tes", "Thal"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Steht w\u00fcst und \u00f6de, falb und fahl.", "tokens": ["Steht", "w\u00fcst", "und", "\u00f6\u00b7de", ",", "falb", "und", "fahl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Rings saust der Herbstwind hohl und rauh;", "tokens": ["Rings", "saust", "der", "Herbst\u00b7wind", "hohl", "und", "rauh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Reif gefriert der Fr\u00fche Thau.", "tokens": ["Zum", "Reif", "ge\u00b7friert", "der", "Fr\u00fc\u00b7he", "Thau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sonne blasses Angesicht", "tokens": ["Der", "Son\u00b7ne", "blas\u00b7ses", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Begl\u00e4nzt den Reif, und schmelzt ihn nicht.", "tokens": ["Be\u00b7gl\u00e4nzt", "den", "Reif", ",", "und", "schmelzt", "ihn", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Des Tages Strahl ist m\u00fcd' und blass.", "tokens": ["Des", "Ta\u00b7ges", "Strahl", "ist", "m\u00fcd'", "und", "blass", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es welkt das starrgefrorne Gras.", "tokens": ["Es", "welkt", "das", "starr\u00b7ge\u00b7fror\u00b7ne", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Blume, die sich kaum erschloss,", "tokens": ["Die", "Blu\u00b7me", ",", "die", "sich", "kaum", "er\u00b7schloss", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verwelket duft- und farbelos.", "tokens": ["Ver\u00b7wel\u00b7ket", "duft", "und", "far\u00b7be\u00b7los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "TRUNC", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Entbl\u00e4ttert bebt der Espen Haupt.", "tokens": ["Ent\u00b7bl\u00e4t\u00b7tert", "bebt", "der", "Es\u00b7pen", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Liebe Laube steht entlaubt.", "tokens": ["Der", "Lie\u00b7be", "Lau\u00b7be", "steht", "ent\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es tummeln gelb und roth und bunt", "tokens": ["Es", "tum\u00b7meln", "gelb", "und", "roth", "und", "bunt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bl\u00e4tter auf dem schwarzen Grund.", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "auf", "dem", "schwar\u00b7zen", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach, alles, was dem Staub' entspriesst,", "tokens": ["Ach", ",", "al\u00b7les", ",", "was", "dem", "Staub'", "ent\u00b7spriesst", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was bl\u00fcht und reift, und Samen schiesst,", "tokens": ["Was", "bl\u00fcht", "und", "reift", ",", "und", "Sa\u00b7men", "schiesst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was einer Mutter Schooss gebar,", "tokens": ["Was", "ei\u00b7ner", "Mut\u00b7ter", "Scho\u00b7oss", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das welkt und kr\u00e4nkelt immerdar.", "tokens": ["Das", "welkt", "und", "kr\u00e4n\u00b7kelt", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Noch steh' ich da in meiner Kraft,", "tokens": ["Noch", "steh'", "ich", "da", "in", "mei\u00b7ner", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Herz entflammt von Leidenschaft.", "tokens": ["Das", "Herz", "ent\u00b7flammt", "von", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Auge blitzt, mein Busen schwillt", "tokens": ["Mein", "Au\u00b7ge", "blitzt", ",", "mein", "Bu\u00b7sen", "schwillt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Hochgef\u00fchlen mild und wild.", "tokens": ["Von", "Hoch\u00b7ge\u00b7f\u00fch\u00b7len", "mild", "und", "wild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Des Geistes rege Fantasie", "tokens": ["Des", "Geis\u00b7tes", "re\u00b7ge", "Fan\u00b7ta\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schafft Welten und zertr\u00fcmmert sie.", "tokens": ["Schafft", "Wel\u00b7ten", "und", "zer\u00b7tr\u00fcm\u00b7mert", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es sch\u00fcttelt mein unruhig Herz", "tokens": ["Es", "sch\u00fct\u00b7telt", "mein", "un\u00b7ru\u00b7hig", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Entz\u00fcckung itzt, itzt Folterschmerz.", "tokens": ["Ent\u00b7z\u00fc\u00b7ckung", "itzt", ",", "itzt", "Fol\u00b7ter\u00b7schmerz", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Mein schmelzend Lied, mein Hochgesang", "tokens": ["Mein", "schmel\u00b7zend", "Lied", ",", "mein", "Hoch\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weckt leises Weh und heissen Drang.", "tokens": ["Weckt", "lei\u00b7ses", "Weh", "und", "heis\u00b7sen", "Drang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es klingt mein goldnes Saitenspiel", "tokens": ["Es", "klingt", "mein", "gold\u00b7nes", "Sai\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und um mich girrt und \u00e4chzt Gef\u00fchl.", "tokens": ["Und", "um", "mich", "girrt", "und", "\u00e4chzt", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wie bald, und meine Kraft versiegt!", "tokens": ["Wie", "bald", ",", "und", "mei\u00b7ne", "Kraft", "ver\u00b7siegt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der S\u00e4nger schweigt. Der Sturmwind kriegt", "tokens": ["Der", "S\u00e4n\u00b7ger", "schweigt", ".", "Der", "Sturm\u00b7wind", "kriegt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um seine Asche. Leichenstill", "tokens": ["Um", "sei\u00b7ne", "A\u00b7sche", ".", "Lei\u00b7chen\u00b7still"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KOUI", "PPOSAT", "NN", "$.", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Liegt sein entsaitet Saitenspiel.", "tokens": ["Liegt", "sein", "ent\u00b7sai\u00b7tet", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Noch bl\u00fcht, mit jedem Reiz geschm\u00fcckt,", "tokens": ["Noch", "bl\u00fcht", ",", "mit", "je\u00b7dem", "Reiz", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der M\u00e4dchen Erste. Noch entz\u00fcckt", "tokens": ["Der", "M\u00e4d\u00b7chen", "Ers\u00b7te", ".", "Noch", "ent\u00b7z\u00fcckt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJA", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich ihres Wuchses schlanker Bau,", "tokens": ["Mich", "ih\u00b7res", "Wuch\u00b7ses", "schlan\u00b7ker", "Bau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des sanften Auges Lasurblau.", "tokens": ["Des", "sanf\u00b7ten", "Au\u00b7ges", "La\u00b7surb\u00b7lau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ihr bl\u00fchend Antlitz roth und weiss", "tokens": ["Ihr", "bl\u00fc\u00b7hend", "Ant\u00b7litz", "roth", "und", "weiss"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Raubt Apfelbl\u00fcthen ihren Preis,", "tokens": ["Raubt", "Ap\u00b7fel\u00b7bl\u00fc\u00b7then", "ih\u00b7ren", "Preis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihres Busens hoher Schnee", "tokens": ["Und", "ih\u00b7res", "Bu\u00b7sens", "ho\u00b7her", "Schnee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Besch\u00e4mt die Brust der Lilie.", "tokens": ["Be\u00b7sch\u00e4mt", "die", "Brust", "der", "Li\u00b7lie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.12": {"line.1": {"text": "Wie bald, und ihrer Sch\u00f6nheit Pracht", "tokens": ["Wie", "bald", ",", "und", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "Pracht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bedeckt des Grabes \u00f6de Nacht!", "tokens": ["Be\u00b7deckt", "des", "Gra\u00b7bes", "\u00f6\u00b7de", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An ihrer F\u00fclle schwelgt der Wurm.", "tokens": ["An", "ih\u00b7rer", "F\u00fcl\u00b7le", "schwelgt", "der", "Wurm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um ihre Asche kriegt der Sturm.", "tokens": ["Um", "ih\u00b7re", "A\u00b7sche", "kriegt", "der", "Sturm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Ach, alles, was der Mond bescheint,", "tokens": ["Ach", ",", "al\u00b7les", ",", "was", "der", "Mond", "be\u00b7scheint", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was l\u00e4chelt und was Thr\u00e4nen weint,", "tokens": ["Was", "l\u00e4\u00b7chelt", "und", "was", "Thr\u00e4\u00b7nen", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "PWS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was einer Mutter Schooss gebar,", "tokens": ["Was", "ei\u00b7ner", "Mut\u00b7ter", "Scho\u00b7oss", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das welkt und altert immerdar.", "tokens": ["Das", "welkt", "und", "al\u00b7tert", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Nur Eines, Ida, altre nie!", "tokens": ["Nur", "Ei\u00b7nes", ",", "I\u00b7da", ",", "alt\u00b7re", "nie", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "NE", "$,", "ADJA", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es \u00e4ndre und es kr\u00e4nkle nie!", "tokens": ["Es", "\u00e4nd\u00b7re", "und", "es", "kr\u00e4nk\u00b7le", "nie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das s\u00fcsse Band, das uns umflicht,", "tokens": ["Das", "s\u00fcs\u00b7se", "Band", ",", "das", "uns", "um\u00b7flicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "A