{"textgrid.poem.33352": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00e4dchen und der Vogel", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Vogel kam geflogen", "tokens": ["Ein", "Vo\u00b7gel", "kam", "ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "J\u00fcngst in mein K\u00e4mmerchen", "tokens": ["J\u00fcngst", "in", "mein", "K\u00e4m\u00b7mer\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.3": {"text": "Auf Fl\u00fcgeln, wie der Bogen", "tokens": ["Auf", "Fl\u00fc\u00b7geln", ",", "wie", "der", "Bo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Iris, bunt und sch\u00f6n.", "tokens": ["Der", "I\u00b7ris", ",", "bunt", "und", "sch\u00f6n", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er flog um mich im Kreise,", "tokens": ["Er", "flog", "um", "mich", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sang ohn' Unterla\u00df", "tokens": ["Und", "sang", "ohn'", "Un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So r\u00fchrend, sanft und leise,", "tokens": ["So", "r\u00fch\u00b7rend", ",", "sanft", "und", "lei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als b\u00e4t' er mich um was.", "tokens": ["Als", "b\u00e4t'", "er", "mich", "um", "was", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PRF", "APPR", "PRELS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er machte da sich immer", "tokens": ["Er", "mach\u00b7te", "da", "sich", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PRF", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um mich etwas zu thun,", "tokens": ["Um", "mich", "et\u00b7was", "zu", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und lie\u00df mich Arme nimmer,", "tokens": ["Und", "lie\u00df", "mich", "Ar\u00b7me", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn ich allein war, ruh'n.", "tokens": ["Wenn", "ich", "al\u00b7lein", "war", ",", "ruh'", "n."], "token_info": ["word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "$,", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Bald tippt' er mir die Wangen,", "tokens": ["Bald", "tippt'", "er", "mir", "die", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bald sang er mir in's Ohr,", "tokens": ["Bald", "sang", "er", "mir", "in's", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald hatt' er mit den Spangen", "tokens": ["Bald", "hatt'", "er", "mit", "den", "Span\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am Mieder etwas vor.", "tokens": ["Am", "Mie\u00b7der", "et\u00b7was", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mir war sein Spiel beh\u00e4glich,", "tokens": ["Mir", "war", "sein", "Spiel", "be\u00b7h\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und unterhielt mich sehr;", "tokens": ["Und", "un\u00b7ter\u00b7hielt", "mich", "sehr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Vogel wurde t\u00e4glich", "tokens": ["Der", "Vo\u00b7gel", "wur\u00b7de", "t\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "VAFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir unentbehrlicher;", "tokens": ["Mir", "un\u00b7ent\u00b7behr\u00b7li\u00b7cher", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADJD", "$."], "meter": "++-+--", "measure": "unknown.measure.tri"}}, "stanza.6": {"line.1": {"text": "Und da\u00df ich sicher w\u00e4re,", "tokens": ["Und", "da\u00df", "ich", "si\u00b7cher", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihn stets um mich zu seh'n,", "tokens": ["Ihn", "stets", "um", "mich", "zu", "seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Stutzt' ich mit einer Scheere", "tokens": ["Stutzt'", "ich", "mit", "ei\u00b7ner", "Schee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm beide Fl\u00fcgelchen.", "tokens": ["Ihm", "bei\u00b7de", "Fl\u00fc\u00b7gel\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.7": {"line.1": {"text": "Nun war er nur noch zahmer,", "tokens": ["Nun", "war", "er", "nur", "noch", "zah\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und gl\u00fccklicher sein Loos:", "tokens": ["Und", "gl\u00fcck\u00b7li\u00b7cher", "sein", "Loos", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So oft ich rief, so kam er,", "tokens": ["So", "oft", "ich", "rief", ",", "so", "kam", "er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schlief in meinem Schoo\u00df.", "tokens": ["Und", "schlief", "in", "mei\u00b7nem", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Er spielte manche Stunde", "tokens": ["Er", "spiel\u00b7te", "man\u00b7che", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um meines Mieders Rand;", "tokens": ["Um", "mei\u00b7nes", "Mie\u00b7ders", "Rand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er trank mir aus dem Munde,", "tokens": ["Er", "trank", "mir", "aus", "dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und a\u00df mir aus der Hand.", "tokens": ["Und", "a\u00df", "mir", "aus", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Doch w\u00e4hrend ich ihn pflegte,", "tokens": ["Doch", "w\u00e4h\u00b7rend", "ich", "ihn", "pfleg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wuchs ihm sein Fl\u00fcgelpaar:", "tokens": ["Wuchs", "ihm", "sein", "Fl\u00fc\u00b7gel\u00b7paar", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und ach! zu sp\u00e4t entdeckte", "tokens": ["Und", "ach", "!", "zu", "sp\u00e4t", "ent\u00b7deck\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "XY", "$.", "PTKA", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich, da\u00df er flicke war.", "tokens": ["Ich", ",", "da\u00df", "er", "fli\u00b7cke", "war", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Er flog vor meinem Blicke", "tokens": ["Er", "flog", "vor", "mei\u00b7nem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Davon, und sang im Flieh'n;", "tokens": ["Da\u00b7von", ",", "und", "sang", "im", "Flieh'n", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich kehre nicht zur\u00fccke,", "tokens": ["Ich", "keh\u00b7re", "nicht", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So wahr ich Amor bin!", "tokens": ["So", "wahr", "ich", "A\u00b7mor", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "NE", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}