{"textgrid.poem.57926": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Der Wundermann", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In V\u00f6lksen wohnt ein Wundermann,", "tokens": ["In", "V\u00f6lk\u00b7sen", "wohnt", "ein", "Wun\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der jede Krankheit heilen kann:", "tokens": ["Der", "je\u00b7de", "Krank\u00b7heit", "hei\u00b7len", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zahnweh und Friesel und den Mumps,", "tokens": ["Zahn\u00b7weh", "und", "Frie\u00b7sel", "und", "den", "Mumps", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schwindsucht und den Fu\u00df des Klumps.", "tokens": ["Die", "Schwind\u00b7sucht", "und", "den", "Fu\u00df", "des", "Klumps", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er hat nicht Medizin studiert,", "tokens": ["Er", "hat", "nicht", "Me\u00b7di\u00b7zin", "stu\u00b7diert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat nicht zum Doktor promoviert,", "tokens": ["Hat", "nicht", "zum", "Dok\u00b7tor", "pro\u00b7mo\u00b7viert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit einer Flasche Fliedertee", "tokens": ["Mit", "ei\u00b7ner", "Fla\u00b7sche", "Flie\u00b7der\u00b7tee"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kuriert er jedes Ach und Weh.", "tokens": ["Ku\u00b7riert", "er", "je\u00b7des", "Ach", "und", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Kolik und Infaulentia,", "tokens": ["Ko\u00b7lik", "und", "In\u00b7fau\u00b7len\u00b7tia", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Die Wassersucht, das Podagra,", "tokens": ["Die", "Was\u00b7ser\u00b7sucht", ",", "das", "Po\u00b7da\u00b7gra", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr Gallenstein, f\u00fcr H\u00fcfteweh,", "tokens": ["F\u00fcr", "Gal\u00b7len\u00b7stein", ",", "f\u00fcr", "H\u00fcf\u00b7te\u00b7weh", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr alles hilft der Fliedertee.", "tokens": ["F\u00fcr", "al\u00b7les", "hilft", "der", "Flie\u00b7der\u00b7tee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das hei\u00dft, dem Wundermann hilft er,", "tokens": ["Das", "hei\u00dft", ",", "dem", "Wun\u00b7der\u00b7mann", "hilft", "er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bisher war seine B\u00f6rse leer,", "tokens": ["Bis\u00b7her", "war", "sei\u00b7ne", "B\u00f6r\u00b7se", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt ist stets voll sein Portemonnaie,", "tokens": ["Jetzt", "ist", "stets", "voll", "sein", "Por\u00b7te\u00b7mon\u00b7nai\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So sehr hilft dieser Fliedertee.", "tokens": ["So", "sehr", "hilft", "die\u00b7ser", "Flie\u00b7der\u00b7tee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "F\u00fcr kalten Brand und dickes Blut", "tokens": ["F\u00fcr", "kal\u00b7ten", "Brand", "und", "di\u00b7ckes", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist Fliedertee vorz\u00fcglich gut,", "tokens": ["Ist", "Flie\u00b7der\u00b7tee", "vor\u00b7z\u00fcg\u00b7lich", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr Kr\u00e4tze, Krebs und auch f\u00fcr Gicht,", "tokens": ["F\u00fcr", "Kr\u00e4t\u00b7ze", ",", "Krebs", "und", "auch", "f\u00fcr", "Gicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blo\u00df gegen Dummheit hilft er nicht.", "tokens": ["Blo\u00df", "ge\u00b7gen", "Dumm\u00b7heit", "hilft", "er", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}