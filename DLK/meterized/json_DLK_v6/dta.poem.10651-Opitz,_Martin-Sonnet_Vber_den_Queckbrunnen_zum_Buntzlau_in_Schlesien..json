{"dta.poem.10651": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet  \n  Vber den Queckbrunnen zum Buntzlau  \n in Schlesien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Dv vnersch\u00f6pffte Lust/ du Wohnhau\u00df aller Freuden/", "tokens": ["Dv", "vner\u00b7sch\u00f6pff\u00b7te", "Lust", "/", "du", "Wohn\u00b7hau\u00df", "al\u00b7ler", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "PPER", "NN", "PIAT", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Du Bad der Najaden/ du k\u00f6stliche Fonteyn/", "tokens": ["Du", "Bad", "der", "Na\u00b7ja\u00b7den", "/", "du", "k\u00f6st\u00b7li\u00b7che", "Fon\u00b7teyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "ART", "NN", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "So lieblich/ als von dir entspringe Milch vnd Wein/", "tokens": ["So", "lieb\u00b7lich", "/", "als", "von", "dir", "ent\u00b7sprin\u00b7ge", "Milch", "vnd", "Wein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "KOKOM", "APPR", "PPER", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bey dessen gr\u00fcner Lust die Schafe sicher weiden/", "tokens": ["Bey", "des\u00b7sen", "gr\u00fc\u00b7ner", "Lust", "die", "Scha\u00b7fe", "si\u00b7cher", "wei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "La\u00df mich/ den vberflu\u00df der Eitelkeit zumeiden/", "tokens": ["La\u00df", "mich", "/", "den", "vberf\u00b7lu\u00df", "der", "Ei\u00b7tel\u00b7keit", "zu\u00b7mei\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Bey deiner Silberquell von Sorgen ledig sein/", "tokens": ["Bey", "dei\u00b7ner", "Sil\u00b7ber\u00b7quell", "von", "Sor\u00b7gen", "le\u00b7dig", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df dich ja nimmermehr der Sonnen heisser Schein/", "tokens": ["Da\u00df", "dich", "ja", "nim\u00b7mer\u00b7mehr", "der", "Son\u00b7nen", "heis\u00b7ser", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Noch deine B\u00e4che/ was vnreines thu beleiden/", "tokens": ["Noch", "dei\u00b7ne", "B\u00e4\u00b7che", "/", "was", "vnr\u00b7ei\u00b7nes", "thu", "be\u00b7lei\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$(", "PWS", "ADJA", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bey dir ich w\u00fcnsch zusein als meinem Vatterlandt/", "tokens": ["Bey", "dir", "ich", "w\u00fcnsch", "zu\u00b7sein", "als", "mei\u00b7nem", "Vat\u00b7ter\u00b7landt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "ADJD", "PTKVZ", "KOKOM", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hieher hab ich allein Hertz/ Muth vnd Sinn gewandt/", "tokens": ["Hie\u00b7her", "hab", "ich", "al\u00b7lein", "Hertz", "/", "Muth", "vnd", "Sinn", "ge\u00b7wandt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "NN", "$(", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mir ist die gantze Welt bey deinen sch\u00f6nen Fl\u00fcssen/", "tokens": ["Mir", "ist", "die", "gant\u00b7ze", "Welt", "bey", "dei\u00b7nen", "sch\u00f6\u00b7nen", "Fl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Drumb soll dich auch hinfort erheben meine Handt/", "tokens": ["Drumb", "soll", "dich", "auch", "hin\u00b7fort", "er\u00b7he\u00b7ben", "mei\u00b7ne", "Handt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So weit der grosse Rhein vnd Tonaw sich ergiessen/", "tokens": ["So", "weit", "der", "gros\u00b7se", "Rhein", "vnd", "To\u00b7naw", "sich", "er\u00b7gies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NE", "KON", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wirst auch du edler Brunn sein vberal bekandt.", "tokens": ["Wirst", "auch", "du", "ed\u00b7ler", "Brunn", "sein", "vbe\u00b7ral", "be\u00b7kandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ADJA", "NN", "PPOSAT", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}