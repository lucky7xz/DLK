{"dta.poem.1238": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die K\u00fcsse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Cupido raubt einmahl den Bienen ihren Safft/", "tokens": ["Cu\u00b7pi\u00b7do", "raubt", "ein\u00b7mahl", "den", "Bie\u00b7nen", "ih\u00b7ren", "Safft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ward dabey verlezt. Er trug voll Zorn und Rache", "tokens": ["Und", "ward", "da\u00b7bey", "ver\u00b7lezt", ".", "Er", "trug", "voll", "Zorn", "und", "Ra\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PAV", "VVPP", "$.", "PPER", "VVFIN", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den angenehmen Raub auff meiner Fillis Mund/", "tokens": ["Den", "an\u00b7ge\u00b7neh\u00b7men", "Raub", "auff", "mei\u00b7ner", "Fil\u00b7lis", "Mund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NE", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sprach: Da\u00df die Welt niemahls vergesse dieser Sache/", "tokens": ["Sprach", ":", "Da\u00df", "die", "Welt", "nie\u00b7mahls", "ver\u00b7ges\u00b7se", "die\u00b7ser", "Sa\u00b7che", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "ART", "NN", "ADV", "VVFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So schmecke/ wer dich k\u00fc\u00dft/ des Honigs s\u00fcsse Krafft/", "tokens": ["So", "schme\u00b7cke", "/", "wer", "dich", "k\u00fc\u00dft", "/", "des", "Ho\u00b7nigs", "s\u00fcs\u00b7se", "Krafft", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "PWS", "PPER", "VVFIN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und werde/ gleich wie ich/ doch an dem Hertzen/ wund!", "tokens": ["Und", "wer\u00b7de", "/", "gleich", "wie", "ich", "/", "doch", "an", "dem", "Hert\u00b7zen", "/", "wund", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "$(", "ADV", "KOKOM", "PPER", "$(", "ADV", "APPR", "ART", "NN", "$(", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}