{"textgrid.poem.56915": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Meine Liebe ist gro\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Liebe ist gro\u00df", "tokens": ["Mei\u00b7ne", "Lie\u00b7be", "ist", "gro\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "wie die weite Welt,", "tokens": ["wie", "die", "wei\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "und nichts ist au\u00dfer ihr,", "tokens": ["und", "nichts", "ist", "au\u00b7\u00dfer", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "wie die Sonne alles", "tokens": ["wie", "die", "Son\u00b7ne", "al\u00b7les"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PIS"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "erw\u00e4rmt, erhellt,", "tokens": ["er\u00b7w\u00e4rmt", ",", "er\u00b7hellt", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "so tut sie der Welt von mir!", "tokens": ["so", "tut", "sie", "der", "Welt", "von", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Da ist kein Gras,", "tokens": ["Da", "ist", "kein", "Gras", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "da ist kein Stein,", "tokens": ["da", "ist", "kein", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "darin mein Liebe nicht w\u00e4r,", "tokens": ["da\u00b7rin", "mein", "Lie\u00b7be", "nicht", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "da ist kein L\u00fcftlein", "tokens": ["da", "ist", "kein", "L\u00fcft\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "noch W\u00e4sserlein,", "tokens": ["noch", "W\u00e4s\u00b7ser\u00b7lein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "darin sie nicht z\u00f6g einher!", "tokens": ["da\u00b7rin", "sie", "nicht", "z\u00f6g", "ein\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PTKNEG", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Da ist kein Tier", "tokens": ["Da", "ist", "kein", "Tier"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "vom M\u00fccklein an", "tokens": ["vom", "M\u00fcck\u00b7lein", "an"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "bis zu uns Menschen empor,", "tokens": ["bis", "zu", "uns", "Men\u00b7schen", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "NN", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "darin mein Herze", "tokens": ["da\u00b7rin", "mein", "Her\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "nicht wohnen kann,", "tokens": ["nicht", "woh\u00b7nen", "kann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "daran ich es nicht verlor!", "tokens": ["da\u00b7ran", "ich", "es", "nicht", "ver\u00b7lor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Meine Liebe ist weit", "tokens": ["Mei\u00b7ne", "Lie\u00b7be", "ist", "weit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "wie die Seele mein,", "tokens": ["wie", "die", "See\u00b7le", "mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "alle Dinge ruhen in ihr,", "tokens": ["al\u00b7le", "Din\u00b7ge", "ru\u00b7hen", "in", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "sie alle, alle", "tokens": ["sie", "al\u00b7le", ",", "al\u00b7le"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "PIS", "$,", "PIAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "bin ich allein,", "tokens": ["bin", "ich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": 