{"textgrid.poem.47676": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[woferne meine Worte dich]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Woferne meine Worte dich", "tokens": ["Wo\u00b7fer\u00b7ne", "mei\u00b7ne", "Wor\u00b7te", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit falschem Eid ber\u00fccken, \u2013", "tokens": ["Mit", "fal\u00b7schem", "Eid", "be\u00b7r\u00fc\u00b7cken", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So viele goldne Sterne dort", "tokens": ["So", "vie\u00b7le", "gold\u00b7ne", "Ster\u00b7ne", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf uns herunter blicken,", "tokens": ["Auf", "uns", "her\u00b7un\u00b7ter", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So vieler Dolche spitze Wuth", "tokens": ["So", "vie\u00b7ler", "Dol\u00b7che", "spit\u00b7ze", "Wuth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Soll meine Brust durchz\u00fccken.", "tokens": ["Soll", "mei\u00b7ne", "Brust", "durch\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}