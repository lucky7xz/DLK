{"dta.poem.21655": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Berliner Abendbild.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wagen rollen in langen Reih\u2019n,", "tokens": ["Wa\u00b7gen", "rol\u00b7len", "in", "lan\u00b7gen", "Reih'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Magisch leuchtet der blaue Schein.", "tokens": ["Ma\u00b7gisch", "leuch\u00b7tet", "der", "blau\u00b7e", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Bannt mich arabische Zaubermacht?", "tokens": ["Bannt", "mich", "a\u00b7ra\u00b7bi\u00b7sche", "Zau\u00b7ber\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Tageshelle in dunkler Nacht!", "tokens": ["Ta\u00b7ges\u00b7hel\u00b7le", "in", "dunk\u00b7ler", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Hastig huschen Gestalten vorbei,", "tokens": ["Has\u00b7tig", "hu\u00b7schen", "Ge\u00b7stal\u00b7ten", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Keine fragt, wer die and\u2019re sei,", "tokens": ["Kei\u00b7ne", "fragt", ",", "wer", "die", "an\u00b7d'\u00b7re", "sei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWS", "ART", "PIS", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Keine fragt dich nach Lust und Schmerz,", "tokens": ["Kei\u00b7ne", "fragt", "dich", "nach", "Lust", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Keine horcht auf der andern Herz.", "tokens": ["Kei\u00b7ne", "horcht", "auf", "der", "an\u00b7dern", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Keine sorgt, ob du krank und schwach,", "tokens": ["Kei\u00b7ne", "sorgt", ",", "ob", "du", "krank", "und", "schwach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Jede rennt dem Gl\u00fccke nach,", "tokens": ["Je\u00b7de", "rennt", "dem", "Gl\u00fc\u00b7cke", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Jede st\u00fcrzt ohne Rast und Ruh", "tokens": ["Je\u00b7de", "st\u00fcrzt", "oh\u00b7ne", "Rast", "und", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Der hinrollenden Dirne zu.", "tokens": ["Der", "hin\u00b7rol\u00b7len\u00b7den", "Dir\u00b7ne", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Langsam schlendr\u2019 ich im Schwarm allein \u2014", "tokens": ["Lang\u00b7sam", "schlendr'", "ich", "im", "Schwarm", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Magisch leuchtet der blaue Schein.", "tokens": ["Ma\u00b7gisch", "leuch\u00b7tet", "der", "blau\u00b7e", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "Kaufmann, Werkmann, Student, Soldat,", "tokens": ["Kauf\u00b7mann", ",", "Werk\u00b7mann", ",", "Stu\u00b7dent", ",", "Sol\u00b7dat", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PWAV", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "Bettler in Fetzen, Hure im Staat.", "tokens": ["Bett\u00b7ler", "in", "Fet\u00b7zen", ",", "Hu\u00b7re", "im", "Staat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "APPRART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Rechnend dr\u00e4ngt sich der Kaufmann hin.", "tokens": ["Rech\u00b7nend", "dr\u00e4ngt", "sich", "der", "Kauf\u00b7mann", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.18": {"text": "Rechnet des Tages Verlust und Gewinn.", "tokens": ["Rech\u00b7net", "des", "Ta\u00b7ges", "Ver\u00b7lust", "und", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.19": {"text": "Werkmann bebt vor der Winters Noth:", "tokens": ["Werk\u00b7mann", "bebt", "vor", "der", "Win\u00b7ters", "Noth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.20": {"text": "\u201ef\u00e4nd\u2019 ich, ach f\u00e4nd\u2019 ich mein t\u00e4glich Brod!", "tokens": ["\u201e", "f\u00e4nd'", "ich", ",", "ach", "f\u00e4nd'", "ich", "mein", "t\u00e4g\u00b7lich", "Brod", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "ITJ", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Hungernd wartet die Kinderschaar,", "tokens": ["Hun\u00b7gernd", "war\u00b7tet", "die", "Kin\u00b7der\u00b7schaar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.22": {"text": "\u2019s ist ein b\u00f6ses, ein b\u00f6ses Jahr.\u201c", "tokens": ["'s", "ist", "ein", "b\u00f6\u00b7ses", ",", "ein", "b\u00f6\u00b7ses", "Jahr", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Bruder Studio zum Freunde spricht:", "tokens": ["Bru\u00b7der", "Stu\u00b7dio", "zum", "Freun\u00b7de", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.24": {"text": "\u201ewarte, das M\u00e4del entkommt uns nicht!", "tokens": ["\u201e", "war\u00b7te", ",", "das", "M\u00e4\u00b7del", "ent\u00b7kommt", "uns", "nicht", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Siehst du, sie guckt; brillant, famos!", "tokens": ["Siehst", "du", ",", "sie", "guckt", ";", "bril\u00b7lant", ",", "fa\u00b7mos", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "$.", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Walter, nun sieh\u2019 doch \u2014 die Taille blo\u00df!\u201c", "tokens": ["Wal\u00b7ter", ",", "nun", "sieh'", "doch", "die", "Tail\u00b7le", "blo\u00df", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ADV", "$(", "ART", "NN", "ADV", "$.", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.27": {"text": "Steht der Gardist in Positur,", "tokens": ["Steht", "der", "Gar\u00b7dist", "in", "Po\u00b7si\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Weil der Hauptmann vor\u00fcber fuhr,", "tokens": ["Weil", "der", "Haupt\u00b7mann", "vor\u00b7\u00fc\u00b7ber", "fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Lie\u00df seine Donna im Stich \u2014 allein:", "tokens": ["Lie\u00df", "sei\u00b7ne", "Don\u00b7na", "im", "Stich", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$(", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "\u201eja, liebste Rosa, Respekt mu\u00df sein.\u201c", "tokens": ["\u201e", "ja", ",", "liebs\u00b7te", "Ro\u00b7sa", ",", "Res\u00b7pekt", "mu\u00df", "sein", ".", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$,", "ADJA", "NN", "$,", "NN", "VMFIN", "VAINF", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.31": {"text": "\u201eblumen, Blumen, o kauft ein Bouquet,", "tokens": ["\u201e", "blu\u00b7men", ",", "Blu\u00b7men", ",", "o", "kauft", "ein", "Bou\u00b7quet", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "FM", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.32": {"text": "Rosen und Veilchen, duftend und nett!", "tokens": ["Ro\u00b7sen", "und", "Veil\u00b7chen", ",", "duf\u00b7tend", "und", "nett", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "VVPP", "KON", "ADJD", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.33": {"text": "Bitte, mein Herr, ach so sei\u2019n Sie so gut!\u201c", "tokens": ["Bit\u00b7te", ",", "mein", "Herr", ",", "ach", "so", "sei'n", "Sie", "so", "gut", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "$,", "ITJ", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$.", "$("], "meter": "+--++-+--+", "measure": "iambic.penta.chol"}, "line.34": {"text": "\u201escheer\u2019 dich zum Teufel, du Gassenbrut!", "tokens": ["\u201e", "scheer'", "dich", "zum", "Teu\u00b7fel", ",", "du", "Gas\u00b7sen\u00b7brut", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPRART", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.35": {"text": "Retzow, auf Ehre, wahrer Skandal.\u201c", "tokens": ["Ret\u00b7zow", ",", "auf", "Eh\u00b7re", ",", "wah\u00b7rer", "Skan\u00b7dal", ".", "\u201c"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "APPR", "NN", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "\u201eunter Kam\u2019raden ganz egal.\u201c", "tokens": ["\u201e", "un\u00b7ter", "Kam'\u00b7ra\u00b7den", "ganz", "e\u00b7gal", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NN", "ADV", "ADV", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.37": {"text": "\u201esehen Sie, bitte! Grandiose Figur,", "tokens": ["\u201e", "se\u00b7hen", "Sie", ",", "bit\u00b7te", "!", "Gran\u00b7di\u00b7o\u00b7se", "Fi\u00b7gur", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "PTKANT", "$.", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.38": {"text": "Wirklich charmant, merveilleuse Frisur.\u201c", "tokens": ["Wirk\u00b7lich", "char\u00b7mant", ",", "mer\u00b7veil\u00b7leu\u00b7se", "Fri\u00b7sur", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJD", "ADJD", "$,", "ADJA", "NN", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.39": {"text": "\u201eecht garantirt? Doch das macht nichts aus.", "tokens": ["\u201e", "echt", "ga\u00b7ran\u00b7tirt", "?", "Doch", "das", "macht", "nichts", "aus", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVPP", "$.", "KON", "PDS", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.40": {"text": "Hm! Begleiten wir sie zu Haus?\u201c", "tokens": ["Hm", "!", "Be\u00b7glei\u00b7ten", "wir", "sie", "zu", "Haus", "?", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "PPER", "APPR", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.41": {"text": "\u201eneuestes Extrablatt! Schwurgericht!\u201c", "tokens": ["\u201e", "neu\u00b7es\u00b7tes", "Ext\u00b7ra\u00b7blatt", "!", "Schwur\u00b7ge\u00b7richt", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "NN", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.42": {"text": "Hei, das dr\u00e4ngt sich neugierig dicht.", "tokens": ["Hei", ",", "das", "dr\u00e4ngt", "sich", "neu\u00b7gie\u00b7rig", "dicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDS", "VVFIN", "PRF", "ADJD", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.43": {"text": "\u201eso ein Schwindler, ein frecher Hund,", "tokens": ["\u201e", "so", "ein", "Schwind\u00b7ler", ",", "ein", "fre\u00b7cher", "Hund", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.44": {"text": "Schl\u00e4gt erst todt und leugnet es rund.\u201c", "tokens": ["Schl\u00e4gt", "erst", "todt", "und", "leug\u00b7net", "es", "rund", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "KON", "VVFIN", "PPER", "ADJD", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.45": {"text": "Wie das rasselt, summt und braust!", "tokens": ["Wie", "das", "ras\u00b7selt", ",", "summt", "und", "braust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Wie es mir vor den Ohren saust!", "tokens": ["Wie", "es", "mir", "vor", "den", "Oh\u00b7ren", "saust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Jahrmarkt des Lebens, so gro\u00df \u2014 so klein!", "tokens": ["Jahr\u00b7markt", "des", "Le\u00b7bens", ",", "so", "gro\u00df", "so", "klein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADV", "ADJD", "$(", "ADV", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.48": {"text": "Magisch leuchtet der blaue Schein.", "tokens": ["Ma\u00b7gisch", "leuch\u00b7tet", "der", "blau\u00b7e", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}