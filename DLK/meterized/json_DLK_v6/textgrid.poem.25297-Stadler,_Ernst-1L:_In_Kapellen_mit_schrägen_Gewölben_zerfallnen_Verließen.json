{"textgrid.poem.25297": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Kapellen mit schr\u00e4gen Gew\u00f6lben zerfallnen Verlie\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Kapellen mit schr\u00e4gen Gew\u00f6lben zerfallnen Verlie\u00dfen", "tokens": ["In", "Ka\u00b7pel\u00b7len", "mit", "schr\u00e4\u00b7gen", "Ge\u00b7w\u00f6l\u00b7ben", "zer\u00b7fall\u00b7nen", "Ver\u00b7lie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "und Scheiben flammrot wie Mohn und wie Perlen gr\u00fcn", "tokens": ["und", "Schei\u00b7ben", "flamm\u00b7rot", "wie", "Mohn", "und", "wie", "Per\u00b7len", "gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "KOKOM", "NN", "KON", "PWAV", "NN", "ADJD"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "und Marmoralt\u00e4ren \u00fcber verwitterten Fliesen", "tokens": ["und", "Mar\u00b7mo\u00b7ral\u00b7t\u00e4\u00b7ren", "\u00fc\u00b7ber", "ver\u00b7wit\u00b7ter\u00b7ten", "Flie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "sah ich die N\u00e4chte wie goldne Gew\u00e4sser verbl\u00fchn:", "tokens": ["sah", "ich", "die", "N\u00e4ch\u00b7te", "wie", "gold\u00b7ne", "Ge\u00b7w\u00e4s\u00b7ser", "ver\u00b7bl\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KOKOM", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}, "stanza.2": {"line.1": {"text": "der schlaffe Rauch zerst\u00e4ubt aus geschwungnen Fialen", "tokens": ["der", "schlaf\u00b7fe", "Rauch", "zer\u00b7st\u00e4ubt", "aus", "ge\u00b7schwung\u00b7nen", "Fi\u00b7a\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "hing noch wie Nebel schwankend in starrender Luft", "tokens": ["hing", "noch", "wie", "Ne\u00b7bel", "schwan\u00b7kend", "in", "star\u00b7ren\u00b7der", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KOKOM", "NN", "ADJD", "APPR", "ADJA", "NN"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "auf Scharlachgewirken die bernsteinschillernden Schalen", "tokens": ["auf", "Schar\u00b7lach\u00b7ge\u00b7wir\u00b7ken", "die", "bern\u00b7stein\u00b7schil\u00b7lern\u00b7den", "Scha\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+--+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "schwammen wie Meergrundwunder im bl\u00e4ulichen Duft.", "tokens": ["schwam\u00b7men", "wie", "Meer\u00b7grund\u00b7wun\u00b7der", "im", "bl\u00e4u\u00b7li\u00b7chen", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "In d\u00e4mmrigen Nischen die alten s\u00fc\u00dfen Madonnen", "tokens": ["In", "d\u00e4mm\u00b7ri\u00b7gen", "Ni\u00b7schen", "die", "al\u00b7ten", "s\u00fc\u00b7\u00dfen", "Ma\u00b7don\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "l\u00e4chelten m\u00fcd und wonnig aus goldrundem Schein.", "tokens": ["l\u00e4\u00b7chel\u00b7ten", "m\u00fcd", "und", "won\u00b7nig", "aus", "gol\u00b7drun\u00b7dem", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Rieselnde Tr\u00e4ume hielten mich rankend umsponnen", "tokens": ["Rie\u00b7seln\u00b7de", "Tr\u00e4u\u00b7me", "hiel\u00b7ten", "mich", "ran\u00b7kend", "um\u00b7spon\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "s\u00e4uselnde Lieder sangen mich selig ein.", "tokens": ["s\u00e4u\u00b7seln\u00b7de", "Lie\u00b7der", "san\u00b7gen", "mich", "se\u00b7lig", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Des wirbelnden Fr\u00fchlings leise girrendes Locken", "tokens": ["Des", "wir\u00b7beln\u00b7den", "Fr\u00fch\u00b7lings", "lei\u00b7se", "gir\u00b7ren\u00b7des", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "der Sommern\u00e4chte Duftrausch weckte mich nicht:", "tokens": ["der", "Som\u00b7mer\u00b7n\u00e4ch\u00b7te", "Duf\u00b7trausch", "weck\u00b7te", "mich", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Bla\u00df aus Fernen l\u00e4uteten wei\u00dfe Glocken ...", "tokens": ["Bla\u00df", "aus", "Fer\u00b7nen", "l\u00e4u\u00b7te\u00b7ten", "wei\u00b7\u00dfe", "Glo\u00b7cken", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Gr\u00fcn aus Kuppeln sickerte goldiges Licht ...", "tokens": ["Gr\u00fcn", "aus", "Kup\u00b7peln", "si\u00b7cker\u00b7te", "gol\u00b7di\u00b7ges", "Licht", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "ADJA", "NN", "$("], "meter": "+-+----+--+", "measure": "iambic.tetra.chol"}}}}}