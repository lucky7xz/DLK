{"textgrid.poem.62455": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "An ein Gemach", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O schm\u00fccke dich mit heiligem Ger\u00e4the,", "tokens": ["O", "schm\u00fc\u00b7cke", "dich", "mit", "hei\u00b7li\u00b7gem", "Ge\u00b7r\u00e4\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gemach, das einen Himmel bald umh\u00fcllt,", "tokens": ["Ge\u00b7mach", ",", "das", "ei\u00b7nen", "Him\u00b7mel", "bald", "um\u00b7h\u00fcllt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das bald, wie Duft an einem Rosenbeete,", "tokens": ["Das", "bald", ",", "wie", "Duft", "an", "ei\u00b7nem", "Ro\u00b7sen\u00b7bee\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "$,", "PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Herrliche mit ihrer Gottheit f\u00fcllt.", "tokens": ["Die", "Herr\u00b7li\u00b7che", "mit", "ih\u00b7rer", "Got\u00b7theit", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die K\u00f6nigin von allen K\u00f6niginnen,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "von", "al\u00b7len", "K\u00f6\u00b7ni\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie will hier schlummern, will dich liebgewinnen.", "tokens": ["Sie", "will", "hier", "schlum\u00b7mern", ",", "will", "dich", "lieb\u00b7ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Noch gr\u00f6\u00dfern Ruhm, Gemach, sollst du erringen,", "tokens": ["Noch", "gr\u00f6\u00b7\u00dfern", "Ruhm", ",", "Ge\u00b7mach", ",", "sollst", "du", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "NN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den keine Zunge, ein Gesang erreicht,", "tokens": ["Den", "kei\u00b7ne", "Zun\u00b7ge", ",", "ein", "Ge\u00b7sang", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Lichtglanz soll aus deinen Mauern dringen,", "tokens": ["Ein", "Licht\u00b7glanz", "soll", "aus", "dei\u00b7nen", "Mau\u00b7ern", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der einzig Ihr an G\u00f6ttersch\u00f6nheit gleicht.", "tokens": ["Der", "ein\u00b7zig", "Ihr", "an", "G\u00f6t\u00b7ter\u00b7sch\u00f6n\u00b7heit", "gleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es schaut in stillem, gl\u00e4ubigem Entz\u00fccken", "tokens": ["Es", "schaut", "in", "stil\u00b7lem", ",", "gl\u00e4u\u00b7bi\u00b7gem", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein treues Volk nach dir mit Hoffnungsblicken.", "tokens": ["Ein", "treu\u00b7es", "Volk", "nach", "dir", "mit", "Hoff\u00b7nungs\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wol tausend Ritter m\u00f6chten dich bewahren;", "tokens": ["Wol", "tau\u00b7send", "Rit\u00b7ter", "m\u00f6ch\u00b7ten", "dich", "be\u00b7wah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Dein be\u00dfrer H\u00fcter ist der Geist in dir.", "tokens": ["Dein", "be\u00df\u00b7rer", "H\u00fc\u00b7ter", "ist", "der", "Geist", "in", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es fliehen fern die Schrecken, die Gefahren \u2013", "tokens": ["Es", "flie\u00b7hen", "fern", "die", "Schre\u00b7cken", ",", "die", "Ge\u00b7fah\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Ortes Heiligkeit verjagt sie hier,", "tokens": ["Des", "Or\u00b7tes", "Hei\u00b7lig\u00b7keit", "ver\u00b7jagt", "sie", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und alle Genien und G\u00f6tter stellen", "tokens": ["Und", "al\u00b7le", "Ge\u00b7ni\u00b7en", "und", "G\u00f6t\u00b7ter", "stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "KON", "NN", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Als Richter sich an dieses Tempels Schwellen.", "tokens": ["Als", "Rich\u00b7ter", "sich", "an", "die\u00b7ses", "Tem\u00b7pels", "Schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}}}}}