{"textgrid.poem.24537": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was rechte Lieblichkeit vor Wunderwercke kan.", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was rechte Lieblichkeit vor Wunderwercke kan.", "tokens": ["Was", "rech\u00b7te", "Lieb\u00b7lich\u00b7keit", "vor", "Wun\u00b7der\u00b7wer\u00b7cke", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "APPR", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und rief: ach w\u00fcrdest du mich sch\u00f6nste nicht besch\u00e4men/", "tokens": ["Und", "rief", ":", "ach", "w\u00fcr\u00b7dest", "du", "mich", "sch\u00f6ns\u00b7te", "nicht", "be\u00b7sch\u00e4\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "PRF", "VVFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wolt' ich dich vergn\u00fcgt zur zehnden Muse nehmen.", "tokens": ["So", "wolt'", "ich", "dich", "ver\u00b7gn\u00fcgt", "zur", "zehn\u00b7den", "Mu\u00b7se", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADJD", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}