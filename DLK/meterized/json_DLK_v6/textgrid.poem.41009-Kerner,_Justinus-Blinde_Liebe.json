{"textgrid.poem.41009": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Blinde Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heda! wer klopft da drau\u00dfen", "tokens": ["He\u00b7da", "!", "wer", "klopft", "da", "drau\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWS", "VVFIN", "ADV", "ADV"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "So sp\u00e4t noch in der Nacht?", "tokens": ["So", "sp\u00e4t", "noch", "in", "der", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbalmosen einem Blinden,", "tokens": ["\u00bb", "al\u00b7mo\u00b7sen", "ei\u00b7nem", "Blin\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Liebe blind gemacht!\u00ab", "tokens": ["Den", "Lie\u00b7be", "blind", "ge\u00b7macht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wer blind ist, geht gef\u00fchret,", "tokens": ["Wer", "blind", "ist", ",", "geht", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht so in Nacht allein! \u2013", "tokens": ["Nicht", "so", "in", "Nacht", "al\u00b7lein", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da schl\u00e4gt er auf sein Auge,", "tokens": ["Da", "schl\u00e4gt", "er", "auf", "sein", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Funkelnd wie Sonnenschein.", "tokens": ["Fun\u00b7kelnd", "wie", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Wohlan! du lieber Blinder;", "tokens": ["Wo\u00b7hlan", "!", "du", "lie\u00b7ber", "Blin\u00b7der", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herein aus kalter Nacht!", "tokens": ["Her\u00b7ein", "aus", "kal\u00b7ter", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Auge, ach, dein Auge", "tokens": ["Dein", "Au\u00b7ge", ",", "ach", ",", "dein", "Au\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ITJ", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat mich nun blind gemacht.", "tokens": ["Hat", "mich", "nun", "blind", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}