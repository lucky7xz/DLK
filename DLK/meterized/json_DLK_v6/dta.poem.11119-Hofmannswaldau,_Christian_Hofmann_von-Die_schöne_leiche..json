{"dta.poem.11119": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die sch\u00f6ne leiche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schau hier, mein wanderer! die asche voller flammen,", "tokens": ["Schau", "hier", ",", "mein", "wan\u00b7de\u00b7rer", "!", "die", "asc\u00b7he", "vol\u00b7ler", "flam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPOSAT", "NN", "$.", "ART", "ADJA", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den tod voll liebligkeit, den sarg voll tausend-sch\u00f6n.", "tokens": ["Den", "tod", "voll", "lieb\u00b7lig\u00b7keit", ",", "den", "sarg", "voll", "tau\u00b7sen\u00b7dsch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "ART", "VVFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Begehrest du zu sehn, wie sonnen untergehn?", "tokens": ["Be\u00b7geh\u00b7rest", "du", "zu", "sehn", ",", "wie", "son\u00b7nen", "un\u00b7ter\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "$,", "PWAV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hier siehst du morgen-r\u00f6th und abend-r\u00f6th beysammen.", "tokens": ["Hier", "siehst", "du", "mor\u00b7gen\u00b7r\u00f6\u00b7th", "und", "a\u00b7ben\u00b7dr\u00f6\u00b7th", "bey\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER"