{"textgrid.poem.64378": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "101. Das Schwalbenpaar", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Schwalbenpaar f\u00fchrte der Lenz mir herbei;", "tokens": ["Ein", "Schwal\u00b7ben\u00b7paar", "f\u00fchr\u00b7te", "der", "Lenz", "mir", "her\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie bauten ihr Nest mir \u00fcber die Th\u00fcr.", "tokens": ["Sie", "bau\u00b7ten", "ihr", "Nest", "mir", "\u00fc\u00b7ber", "die", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie flogen sie her, wie flogen sie hin,", "tokens": ["Wie", "flo\u00b7gen", "sie", "her", ",", "wie", "flo\u00b7gen", "sie", "hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu holen den Lehm; wie schlugen sie oft", "tokens": ["Zu", "ho\u00b7len", "den", "Lehm", ";", "wie", "schlu\u00b7gen", "sie", "oft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$.", "PWAV", "VVFIN", "PPER", "ADV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mit dem Schn\u00e4belchen an, zu verkitten das Nest!", "tokens": ["Mit", "dem", "Schn\u00e4\u00b7bel\u00b7chen", "an", ",", "zu", "ver\u00b7kit\u00b7ten", "das", "Nest", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "PTKZU", "VVFIN", "ART", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Sie verkleibten gar wohl und sp\u00fcndeten zart", "tokens": ["Sie", "ver\u00b7kleib\u00b7ten", "gar", "wohl", "und", "sp\u00fcn\u00b7de\u00b7ten", "zart"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "KON", "VVFIN", "ADJD"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Ihr kleines Gemach und bezogen's mit Flaum;", "tokens": ["Ihr", "klei\u00b7nes", "Ge\u00b7mach", "und", "be\u00b7zo\u00b7gen's", "mit", "Flaum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NE", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Sie legte hinein vier Eier und trug", "tokens": ["Sie", "leg\u00b7te", "hin\u00b7ein", "vier", "Ei\u00b7er", "und", "trug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "NN", "KON", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Den Jungen wohl fr\u00fch, den Jungen wohl sp\u00e4t", "tokens": ["Den", "Jun\u00b7gen", "wohl", "fr\u00fch", ",", "den", "Jun\u00b7gen", "wohl", "sp\u00e4t"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "ART", "NN", "ADV", "ADJD"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Die Speise; nicht Ruhe sie hatte, nicht Rast.", "tokens": ["Die", "Spei\u00b7se", ";", "nicht", "Ru\u00b7he", "sie", "hat\u00b7te", ",", "nicht", "Rast", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PTKNEG", "NN", "PPER", "VAFIN", "$,", "PTKNEG", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.11": {"text": "Das helle Geschrei der hungernden Brut", "tokens": ["Das", "hel\u00b7le", "Ge\u00b7schrei", "der", "hun\u00b7gern\u00b7den", "Brut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Erweckte sie fr\u00fch, erweckte sie spat;", "tokens": ["Er\u00b7weck\u00b7te", "sie", "fr\u00fch", ",", "er\u00b7weck\u00b7te", "sie", "spat", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Die Fliegen sind schnell, und die Ameisen schwer", "tokens": ["Die", "Flie\u00b7gen", "sind", "schnell", ",", "und", "die", "A\u00b7mei\u00b7sen", "schwer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "ART", "NN", "ADJD"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Zu ersp\u00e4hn, und die Piependen fordern so viel!", "tokens": ["Zu", "er\u00b7sp\u00e4hn", ",", "und", "die", "Pie\u00b7pen\u00b7den", "for\u00b7dern", "so", "viel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KON", "ART", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.15": {"text": "Sie fastete selbst, um zu \u00e4tzen die Brut;", "tokens": ["Sie", "fas\u00b7te\u00b7te", "selbst", ",", "um", "zu", "\u00e4t\u00b7zen", "die", "Brut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUI", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "Die wuchs nun heran und verlangte noch mehr.", "tokens": ["Die", "wuchs", "nun", "he\u00b7ran", "und", "ver\u00b7lang\u00b7te", "noch", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKVZ", "KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.17": {"text": "Da ermattete schier die Mutter und kam", "tokens": ["Da", "er\u00b7mat\u00b7te\u00b7te", "schier", "die", "Mut\u00b7ter", "und", "kam"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "KON", "VVFIN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.18": {"text": "Mit wankendem Flug, vermochte mit M\u00fch'", "tokens": ["Mit", "wan\u00b7ken\u00b7dem", "Flug", ",", "ver\u00b7moch\u00b7te", "mit", "M\u00fch'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "APPR", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Den Schnabel noch halb zu \u00f6ffnen, und flog", "tokens": ["Den", "Schna\u00b7bel", "noch", "halb", "zu", "\u00f6ff\u00b7nen", ",", "und", "flog"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$,", "KON", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Bald wieder auf Jagd, denn Liebe macht stark.", "tokens": ["Bald", "wie\u00b7der", "auf", "Jagd", ",", "denn", "Lie\u00b7be", "macht", "stark", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$,", "KON", "NN", "VVFIN", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Sie h\u00e4rmte sich ab mit Kummer und M\u00fch',", "tokens": ["Sie", "h\u00e4rm\u00b7te", "sich", "ab", "mit", "Kum\u00b7mer", "und", "M\u00fch'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Sie sorgte mit Angst, in dem Neste sei Not,", "tokens": ["Sie", "sorg\u00b7te", "mit", "Angst", ",", "in", "dem", "Nes\u00b7te", "sei", "Not", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "APPR", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.23": {"text": "Doch hatten vollauf die Jungen; da schlief", "tokens": ["Doch", "hat\u00b7ten", "vol\u00b7lauf", "die", "Jun\u00b7gen", ";", "da", "schlief"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Ein jegliches satt, bis sie weckte der Flug", "tokens": ["Ein", "jeg\u00b7li\u00b7ches", "satt", ",", "bis", "sie", "weck\u00b7te", "der", "Flug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.25": {"text": "Der Mutter, dann schrie wie verschmachtend die Brut,", "tokens": ["Der", "Mut\u00b7ter", ",", "dann", "schrie", "wie", "ver\u00b7schmach\u00b7tend", "die", "Brut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "KOKOM", "VVPP", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.26": {"text": "Doch waren sie satt und die wachsende Kraft", "tokens": ["Doch", "wa\u00b7ren", "sie", "satt", "und", "die", "wach\u00b7sen\u00b7de", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "KON", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.27": {"text": "Trieb schwellende Kiel' aus dem gelblichten Flaum,", "tokens": ["Trieb", "schwel\u00b7len\u00b7de", "Kiel'", "aus", "dem", "gelb\u00b7lich\u00b7ten", "Flaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.28": {"text": "Den Kielen entwuchs der Fittiche Paar,", "tokens": ["Den", "Kie\u00b7len", "ent\u00b7wuchs", "der", "Fit\u00b7ti\u00b7che", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Der Schnabel ward hart und verschnappte schon oft", "tokens": ["Der", "Schna\u00b7bel", "ward", "hart", "und", "ver\u00b7schnapp\u00b7te", "schon", "oft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "ADV", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.30": {"text": "Die Fliege, so keck sich dem Neste genaht;", "tokens": ["Die", "Flie\u00b7ge", ",", "so", "keck", "sich", "dem", "Nes\u00b7te", "ge\u00b7naht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.31": {"text": "Nun flogen sie auf zum benachbarten Dach,", "tokens": ["Nun", "flo\u00b7gen", "sie", "auf", "zum", "be\u00b7nach\u00b7bar\u00b7ten", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Von dem Dache zum Baum und vom Baume davon.", "tokens": ["Von", "dem", "Da\u00b7che", "zum", "Baum", "und", "vom", "Bau\u00b7me", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "PTKVZ", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.33": {"text": "Die Mutter kam heim zum ver\u00f6deten Nest,", "tokens": ["Die", "Mut\u00b7ter", "kam", "heim", "zum", "ver\u00b7\u00f6\u00b7de\u00b7ten", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.34": {"text": "Sie jammerte laut, sie lockte, sie flog", "tokens": ["Sie", "jam\u00b7mer\u00b7te", "laut", ",", "sie", "lock\u00b7te", ",", "sie", "flog"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.35": {"text": "Vom Nestchen zum Baum und vom Baume zum Nest.", "tokens": ["Vom", "Nest\u00b7chen", "zum", "Baum", "und", "vom", "Bau\u00b7me", "zum", "Nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.36": {"text": "Sie flatterten hin, sie flatterten her;", "tokens": ["Sie", "flat\u00b7ter\u00b7ten", "hin", ",", "sie", "flat\u00b7ter\u00b7ten", "her", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.37": {"text": "Sie fastet den Tag, sie seufzet die Nacht.", "tokens": ["Sie", "fas\u00b7tet", "den", "Tag", ",", "sie", "seuf\u00b7zet", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.38": {"text": "Ach Schw\u00e4lbchen, du hast vergessen, wie du", "tokens": ["Ach", "Schw\u00e4lb\u00b7chen", ",", "du", "hast", "ver\u00b7ges\u00b7sen", ",", "wie", "du"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,", "PWAV", "PPER"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.39": {"text": "Die Mutter dereinst verlie\u00dfest, auch sie", "tokens": ["Die", "Mut\u00b7ter", "de\u00b7reinst", "ver\u00b7lie\u00b7\u00dfest", ",", "auch", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,", "ADV", "PPER"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.40": {"text": "Hat \u00e4ngstlich geklagt, als die Jungen entflohn!", "tokens": ["Hat", "\u00e4ngst\u00b7lich", "ge\u00b7klagt", ",", "als", "die", "Jun\u00b7gen", "ent\u00b7flohn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$,", "KOUS", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}