{"textgrid.poem.52534": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Nec bene cum nec sine illis", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmit ihnen ist zu leben nicht,", "tokens": ["\u00bb", "mit", "ih\u00b7nen", "ist", "zu", "le\u00b7ben", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "VAFIN", "PTKZU", "VVINF", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ohne sie wer hielt es aus!\u00ab", "tokens": ["Und", "oh\u00b7ne", "sie", "wer", "hielt", "es", "aus", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "PPER", "PWS", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein weiser Mann erfand den Spruch,", "tokens": ["Ein", "wei\u00b7ser", "Mann", "er\u00b7fand", "den", "Spruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Weise nahm sich viel heraus.", "tokens": ["Der", "Wei\u00b7se", "nahm", "sich", "viel", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von Weibern sprach er, und ich glaub,", "tokens": ["Von", "Wei\u00b7bern", "sprach", "er", ",", "und", "ich", "glaub", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Biedermann, er hatte Recht,", "tokens": ["Der", "Bie\u00b7der\u00b7mann", ",", "er", "hat\u00b7te", "Recht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer w\u00fc\u00dfte Herr sich ohne sie,", "tokens": ["Wer", "w\u00fc\u00df\u00b7te", "Herr", "sich", "oh\u00b7ne", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wer liebend f\u00fchlte sich kein Knecht?", "tokens": ["Wer", "lie\u00b7bend", "f\u00fchl\u00b7te", "sich", "kein", "Knecht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "PRF", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wer w\u00fcnschte sich der Schmerzen Zahl,", "tokens": ["Wer", "w\u00fcnschte", "sich", "der", "Schmer\u00b7zen", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Die Leidenschaft ohn Ende bringt,", "tokens": ["Die", "Lei\u00b7den\u00b7schaft", "ohn", "En\u00b7de", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wer lebte dieses Leben aus,", "tokens": ["Wer", "leb\u00b7te", "die\u00b7ses", "Le\u00b7ben", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der Liebe fern, die einzig j\u00fcngt?", "tokens": ["Der", "Lie\u00b7be", "fern", ",", "die", "ein\u00b7zig", "j\u00fcngt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wer jauchzte nicht zum ersten Ku\u00df,", "tokens": ["Wer", "jauchz\u00b7te", "nicht", "zum", "ers\u00b7ten", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wer fluchte nicht der Eifersucht \u2013", "tokens": ["Wer", "fluch\u00b7te", "nicht", "der", "Ei\u00b7fer\u00b7sucht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Es ist die Lieb das tolle Meer,", "tokens": ["Es", "ist", "die", "Lieb", "das", "tol\u00b7le", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die Lieb die stille Hafenbucht.", "tokens": ["Die", "Lieb", "die", "stil\u00b7le", "Ha\u00b7fen\u00b7bucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Entsetzen und Entz\u00fccken bald,", "tokens": ["Ent\u00b7set\u00b7zen", "und", "Ent\u00b7z\u00fc\u00b7cken", "bald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Der Liebe Freud, der Grillen Leid", "tokens": ["Der", "Lie\u00b7be", "Freud", ",", "der", "Gril\u00b7len", "Leid"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Kommt \u00fcber uns, da schwindelt uns \u2013", "tokens": ["Kommt", "\u00fc\u00b7ber", "uns", ",", "da", "schwin\u00b7delt", "uns", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "ADV", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und das ist H\u00f6llenseligkeit.", "tokens": ["Und", "das", "ist", "H\u00f6l\u00b7len\u00b7se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}