{"textgrid.poem.31255": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Augen/ schwartze Feuer-Ballen/", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Augen/ schwartze Feuer-Ballen/", "tokens": ["Au\u00b7gen", "/", "schwart\u00b7ze", "Feu\u00b7er\u00b7Bal\u00b7len", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und du Gold-geflammtes Hahr/", "tokens": ["und", "du", "Gold\u00b7ge\u00b7flamm\u00b7tes", "Hahr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "soll ich denn itzt gantz und gar", "tokens": ["soll", "ich", "denn", "itzt", "gantz", "und", "gar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADV", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00fcmb euch in Bedr\u00fchbni\u00df fallen?", "tokens": ["\u00fcmb", "euch", "in", "Be\u00b7dr\u00fchb\u00b7ni\u00df", "fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bloh\u00df weil ich fast \u00fcber hoffen", "tokens": ["Bloh\u00df", "weil", "ich", "fast", "\u00fc\u00b7ber", "hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADV", "APPR", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "gestern sie im Teich bedroffen?", "tokens": ["ge\u00b7stern", "sie", "im", "Teich", "be\u00b7drof\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Bloh\u00df weil ich mich unterstund/", "tokens": ["Bloh\u00df", "weil", "ich", "mich", "un\u00b7ter\u00b7stund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "da\u00df ich wa\u00df zu reitzend fund?", "tokens": ["da\u00df", "ich", "wa\u00df", "zu", "reit\u00b7zend", "fund", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PTKA", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "durch den gr\u00fcnen Sommer-Wald", "tokens": ["durch", "den", "gr\u00fc\u00b7nen", "Som\u00b7mer\u00b7Wald"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "dummelte sich manigfalt", "tokens": ["dum\u00b7mel\u00b7te", "sich", "ma\u00b7nig\u00b7falt"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "ihre wei\u00df-bewollte Heerde.", "tokens": ["ih\u00b7re", "wei\u00df\u00b7be\u00b7woll\u00b7te", "Heer\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter P\u00fcschen/ die kaum wichen/", "tokens": ["Un\u00b7ter", "P\u00fc\u00b7schen", "/", "die", "kaum", "wi\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "ART", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "hatte ich sie schlau beschlichen;", "tokens": ["hat\u00b7te", "ich", "sie", "schlau", "be\u00b7schli\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "gantz von Farrnen dikk bed\u00e4kkt/", "tokens": ["gantz", "von", "Farr\u00b7nen", "dikk", "be\u00b7d\u00e4kkt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "NE", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "hielt ich heymlig mich verst\u00e4kkt.", "tokens": ["hielt", "ich", "heym\u00b7lig", "mich", "ver\u00b7st\u00e4kkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwischen zweenen B\u00fcchen-Esten", "tokens": ["Zwi\u00b7schen", "zwee\u00b7nen", "B\u00fc\u00b7chen\u00b7Es\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "hing benebst dem Sch\u00e4ffer-Stokk", "tokens": ["hing", "be\u00b7nebst", "dem", "Sch\u00e4f\u00b7fer\u00b7Stokk"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ihr gebl\u00fchmbter Athla\u00df-Rokk", "tokens": ["ihr", "ge\u00b7bl\u00fchmb\u00b7ter", "Ath\u00b7la\u00df\u00b7Rokk"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "bey dem g\u00f6nstigsten S\u00fcd-Westen.", "tokens": ["bey", "dem", "g\u00f6ns\u00b7tigs\u00b7ten", "S\u00fcd\u00b7Wes\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Str\u00fcmpffgens/ St\u00f6kkel-Sch\u00fchchens/ H\u00f6sgens/", "tokens": ["Str\u00fcmpff\u00b7gens", "/", "St\u00f6k\u00b7kel\u00b7Sch\u00fch\u00b7chens", "/", "H\u00f6s\u00b7gens", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$(", "NE", "$(", "NE", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "all die lihben netten Ch\u00f6sgens/", "tokens": ["all", "die", "lih\u00b7ben", "net\u00b7ten", "Ch\u00f6s\u00b7gens", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "bundt sie mit ge\u00fcbter Hand", "tokens": ["bundt", "sie", "mit", "ge\u00b7\u00fcb\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "an ein Rohsen-rohtes Band.", "tokens": ["an", "ein", "Roh\u00b7sen\u00b7roh\u00b7tes", "Band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Itzt so worff sie ab ihr Mihder/", "tokens": ["Itzt", "so", "worff", "sie", "ab", "ihr", "Mih\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "itzt so glitt ihr Hembd ins Gra\u00df/", "tokens": ["itzt", "so", "glitt", "ihr", "Hembd", "ins", "Gra\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "das bolihrte Nimpffen-Na\u00df", "tokens": ["das", "bo\u00b7lihr\u00b7te", "Nimpf\u00b7fen\u00b7Na\u00df"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "spihgelte sie blizz-blanck wihder.", "tokens": ["spih\u00b7gel\u00b7te", "sie", "bliz\u00b7zblanck", "wih\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Ihre wohl-geformte L\u00e4nge", "tokens": ["Ih\u00b7re", "wohl\u00b7ge\u00b7form\u00b7te", "L\u00e4n\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "bracht mich selig ins Gedr\u00e4nge;", "tokens": ["bracht", "mich", "se\u00b7lig", "ins", "Ge\u00b7dr\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "nichts nicht/ wa\u00df sich mir nicht bot/", "tokens": ["nichts", "nicht", "/", "wa\u00df", "sich", "mir", "nicht", "bot", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKNEG", "$(", "VVFIN", "PRF", "PPER", "PTKNEG", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "vor Vergn\u00fcgen lag ich dodt!", "tokens": ["vor", "Ver\u00b7gn\u00fc\u00b7gen", "lag", "ich", "dodt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zahrt farbirt die s\u00fcssen B\u00e4kkgen/", "tokens": ["Zahrt", "far\u00b7birt", "die", "s\u00fcs\u00b7sen", "B\u00e4kk\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "stund sie munter in dem Klee/", "tokens": ["stund", "sie", "mun\u00b7ter", "in", "dem", "Klee", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "sch\u00f6ner noch al\u00df ", "tokens": ["sch\u00f6\u00b7ner", "noch", "al\u00df"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADV", "KOUS"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "ohne irgend jedes G\u00e4kkgen.", "tokens": ["oh\u00b7ne", "ir\u00b7gend", "je\u00b7des", "G\u00e4kk\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00dcmb die himmlisch runde Dinger", "tokens": ["\u00dcmb", "die", "himm\u00b7lisch", "run\u00b7de", "Din\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "spihlten die verlihbten Finger/", "tokens": ["spihl\u00b7ten", "die", "ver\u00b7lihb\u00b7ten", "Fin\u00b7ger", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "beyde Ackseln kunt ich sehn/", "tokens": ["bey\u00b7de", "Ack\u00b7seln", "kunt", "ich", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "PPER", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "die voll g\u00f6ldner H\u00e4rgens stehn!", "tokens": ["die", "voll", "g\u00f6ld\u00b7ner", "H\u00e4r\u00b7gens", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "brohbend hub sie erst ihr Bein/", "tokens": ["broh\u00b7bend", "hub", "sie", "erst", "ihr", "Bein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "dan so tukkte sie sich dreyn/", "tokens": ["dan", "so", "tukk\u00b7te", "sie", "sich", "dreyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wo die feuchte Fischgens schwummen.", "tokens": ["wo", "die", "feuch\u00b7te", "Fischgens", "schwum\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wei\u00df die Schultern/ wei\u00df die Waden/", "tokens": ["Wei\u00df", "die", "Schul\u00b7tern", "/", "wei\u00df", "die", "Wa\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "so pflag ", "tokens": ["so", "pflag"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "itzt halb f\u00fcr und itzt zur\u00fckk/", "tokens": ["itzt", "halb", "f\u00fcr", "und", "itzt", "zu\u00b7r\u00fckk", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "KON", "ADV", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "o du sch\u00f6nes Meister-St\u00fckk!", "tokens": ["o", "du", "sch\u00f6\u00b7nes", "Meis\u00b7ter\u00b7St\u00fc\u00b7kk", "!"], "token_info": ["word",