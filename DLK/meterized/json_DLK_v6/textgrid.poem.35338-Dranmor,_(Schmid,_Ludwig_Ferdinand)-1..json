{"textgrid.poem.35338": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freundin! bewahre deinen leichten Sinn;", "tokens": ["Freun\u00b7din", "!", "be\u00b7wah\u00b7re", "dei\u00b7nen", "leich\u00b7ten", "Sinn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.2": {"text": "Was k\u00fcmmern dich der Liebessehnsucht Leiden?", "tokens": ["Was", "k\u00fcm\u00b7mern", "dich", "der", "Lie\u00b7bes\u00b7sehn\u00b7sucht", "Lei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur ich sei der Gequ\u00e4lte von uns Beiden,", "tokens": ["Nur", "ich", "sei", "der", "Ge\u00b7qu\u00e4l\u00b7te", "von", "uns", "Bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ART", "NN", "APPR", "PPER", "PIS", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und dein betrognes Herz sei mein Gewinn,", "tokens": ["Und", "dein", "be\u00b7trog\u00b7nes", "Herz", "sei", "mein", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was du dem armen Schw\u00e4rmer dargebracht", "tokens": ["Was", "du", "dem", "ar\u00b7men", "Schw\u00e4r\u00b7mer", "dar\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit deines holden Leibes wilder Lohe,", "tokens": ["Mit", "dei\u00b7nes", "hol\u00b7den", "Lei\u00b7bes", "wil\u00b7der", "Lo\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist jetzt der Dichterpreis, der einzig hohe,", "tokens": ["Ist", "jetzt", "der", "Dich\u00b7ter\u00b7preis", ",", "der", "ein\u00b7zig", "ho\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der neue Lebenslust in mir entfacht.", "tokens": ["Der", "neu\u00b7e", "Le\u00b7bens\u00b7lust", "in", "mir", "ent\u00b7facht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das stete Ringen, das mi\u00dfg\u00f6nnte Ziel,", "tokens": ["Das", "ste\u00b7te", "Rin\u00b7gen", ",", "das", "mi\u00df\u00b7g\u00f6nn\u00b7te", "Ziel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das T\u00e4ndeln mit den glei\u00dfnerischen Musen,", "tokens": ["Das", "T\u00e4n\u00b7deln", "mit", "den", "glei\u00df\u00b7ne\u00b7ri\u00b7schen", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf immer sei es nun an deinem Busen", "tokens": ["Auf", "im\u00b7mer", "sei", "es", "nun", "an", "dei\u00b7nem", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verfehmt, als nichtiges Gedankenspiel.", "tokens": ["Ver\u00b7fehmt", ",", "als", "nich\u00b7ti\u00b7ges", "Ge\u00b7dan\u00b7ken\u00b7spiel", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Entfliehen will ich enger Sitte Joch,", "tokens": ["Ent\u00b7flie\u00b7hen", "will", "ich", "en\u00b7ger", "Sit\u00b7te", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verleugnen all die lyrischen Erg\u00fcsse", "tokens": ["Ver\u00b7leug\u00b7nen", "all", "die", "ly\u00b7ri\u00b7schen", "Er\u00b7g\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr deine Trostesworte, deine K\u00fcsse \u2013", "tokens": ["F\u00fcr", "dei\u00b7ne", "Tros\u00b7tes\u00b7wor\u00b7te", ",", "dei\u00b7ne", "K\u00fcs\u00b7se", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O Freundin! Nur dies eine bleibt mir noch.", "tokens": ["O", "Freun\u00b7din", "!", "Nur", "dies", "ei\u00b7ne", "bleibt", "mir", "noch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "PDS", "ART", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}