{"textgrid.poem.60409": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Himmel! seit vierzehn Tagen unabl\u00e4ssig", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Himmel! seit vierzehn Tagen unabl\u00e4ssig", "tokens": ["Him\u00b7mel", "!", "seit", "vier\u00b7zehn", "Ta\u00b7gen", "un\u00b7ab\u00b7l\u00e4s\u00b7sig"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "CARD", "NN", "ADJD"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Bist du so geh\u00e4ssig und regenn\u00e4ssig,", "tokens": ["Bist", "du", "so", "ge\u00b7h\u00e4s\u00b7sig", "und", "re\u00b7gen\u00b7n\u00e4s\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Bald ein Sch\u00fctten in Str\u00f6men, bald Getr\u00e4ufel;", "tokens": ["Bald", "ein", "Sch\u00fct\u00b7ten", "in", "Str\u00f6\u00b7men", ",", "bald", "Ge\u00b7tr\u00e4u\u00b7fel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "$,", "ADV", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Himmel, o Himmel, es hole dich der Teufel!", "tokens": ["Him\u00b7mel", ",", "o", "Him\u00b7mel", ",", "es", "ho\u00b7le", "dich", "der", "Teu\u00b7fel", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Gurgelst wieder herab die schmutzigen Lieder,", "tokens": ["Gur\u00b7gelst", "wie\u00b7der", "her\u00b7ab", "die", "schmut\u00b7zi\u00b7gen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "H\u00e4ngen vom Leibe dir die Fetzen nieder,", "tokens": ["H\u00e4n\u00b7gen", "vom", "Lei\u00b7be", "dir", "die", "Fet\u00b7zen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Taumelst gleich einem versoffnen zitternden Lumpen,", "tokens": ["Tau\u00b7melst", "gleich", "ei\u00b7nem", "ver\u00b7soff\u00b7nen", "zit\u00b7tern\u00b7den", "Lum\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "---+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hin von Berge zu Berge mit vollem Humpen!", "tokens": ["Hin", "von", "Ber\u00b7ge", "zu", "Ber\u00b7ge", "mit", "vol\u00b7lem", "Hum\u00b7pen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Warfst den Bergen die Kinder aus ihren Betten,", "tokens": ["Warfst", "den", "Ber\u00b7gen", "die", "Kin\u00b7der", "aus", "ih\u00b7ren", "Bet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Alle B\u00e4che heraus, und plump zertreten", "tokens": ["Al\u00b7le", "B\u00e4\u00b7che", "he\u00b7raus", ",", "und", "plump", "zer\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "PTKVZ", "$,", "KON", "ADJD", "VVINF"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Hast du die reifende Saat den armen Bauern;", "tokens": ["Hast", "du", "die", "rei\u00b7fen\u00b7de", "Saat", "den", "ar\u00b7men", "Bau\u00b7ern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Unband! wie lange noch soll dein Unfug dauern?", "tokens": ["Un\u00b7band", "!", "wie", "lan\u00b7ge", "noch", "soll", "dein", "Un\u00b7fug", "dau\u00b7ern", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "ADV", "ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Wenn doch endlich t\u00fcchtige Winde brausten", "tokens": ["Wenn", "doch", "end\u00b7lich", "t\u00fcch\u00b7ti\u00b7ge", "Win\u00b7de", "braus\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ADJA", "NN", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Und dich rasch von dannen peitschten und zausten!", "tokens": ["Und", "dich", "rasch", "von", "dan\u00b7nen", "peitschten", "und", "zaus\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "APPR", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aber du wirst von Stunde zu Stunde noch frecher,", "tokens": ["A\u00b7ber", "du", "wirst", "von", "Stun\u00b7de", "zu", "Stun\u00b7de", "noch", "fre\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "NN", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "L\u00fcmmelst schon dich herein bis auf unsre D\u00e4cher.", "tokens": ["L\u00fcm\u00b7melst", "schon", "dich", "her\u00b7ein", "bis", "auf", "uns\u00b7re", "D\u00e4\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "PTKVZ", "KON", "APPR", "PPOSAT", "NN", "$."], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Hast an harten Felsen den Kopf zerschlagen,", "tokens": ["Hast", "an", "har\u00b7ten", "Fel\u00b7sen", "den", "Kopf", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Und noch bist du nicht hin! seit vierzehn Tagen!", "tokens": ["Und", "noch", "bist", "du", "nicht", "hin", "!", "seit", "vier\u00b7zehn", "Ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PTKNEG", "PTKVZ", "$.", "APPR", "CARD", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Blinder Unhold! es ist das Auge der Sonnen", "tokens": ["Blin\u00b7der", "Un\u00b7hold", "!", "es", "ist", "das", "Au\u00b7ge", "der", "Son\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und das Auge des Monds dir ausgeronnen.", "tokens": ["Und", "das", "Au\u00b7ge", "des", "Monds", "dir", "aus\u00b7ge\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Ungastfreundlicher Strolch! die sch\u00f6nsten Frauen", "tokens": ["Un\u00b7gast\u00b7freund\u00b7li\u00b7cher", "Strolch", "!", "die", "sch\u00f6ns\u00b7ten", "Frau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Kamen, zu baden und das Gebirg zu schauen;", "tokens": ["Ka\u00b7men", ",", "zu", "ba\u00b7den", "und", "das", "Ge\u00b7birg", "zu", "schau\u00b7en", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKZU", "VVINF", "KON", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Baden k\u00f6nnen sie gnug, doch den Hals nie strecken", "tokens": ["Ba\u00b7den", "k\u00f6n\u00b7nen", "sie", "gnug", ",", "doch", "den", "Hals", "nie", "stre\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADV", "$,", "ADV", "ART", "NN", "ADV", "VVINF"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Aus dem Tale, dem riesigen Badebecken.", "tokens": ["Aus", "dem", "Ta\u00b7le", ",", "dem", "rie\u00b7si\u00b7gen", "Ba\u00b7de\u00b7be\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}}, "stanza.7": {"line.1": {"text": "H\u00e4tte Ischl nur dich und seine Solen,", "tokens": ["H\u00e4t\u00b7te", "Ischl", "nur", "dich", "und", "sei\u00b7ne", "So\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "H\u00e4tt ich mit einem Fluche mich l\u00e4ngst empfohlen;", "tokens": ["H\u00e4tt", "ich", "mit", "ei\u00b7nem", "Flu\u00b7che", "mich", "l\u00e4ngst", "emp\u00b7foh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch nebst dir und deinem Wolkengewimmel", "tokens": ["Doch", "nebst", "dir", "und", "dei\u00b7nem", "Wol\u00b7ken\u00b7ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hat es zum Gl\u00fcck noch einen andern Himmel!", "tokens": ["Hat", "es", "zum", "Gl\u00fcck", "noch", "ei\u00b7nen", "an\u00b7dern", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}