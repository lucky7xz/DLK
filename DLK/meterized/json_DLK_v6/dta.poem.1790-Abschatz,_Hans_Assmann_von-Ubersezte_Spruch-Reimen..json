{"dta.poem.1790": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.85", "af:0.14"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Die blasse Sorge wird mit dir zu Segel gehn/", "tokens": ["Die", "blas\u00b7se", "Sor\u00b7ge", "wird", "mit", "dir", "zu", "Se\u00b7gel", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und unter Welt und Wind dir an der Seite stehn/", "tokens": ["Und", "un\u00b7ter", "Welt", "und", "Wind", "dir", "an", "der", "Sei\u00b7te", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ie sitzet hinter dich auff deines Pferdes R\u00fccken/", "tokens": ["ie", "sit\u00b7zet", "hin\u00b7ter", "dich", "auff", "dei\u00b7nes", "Pfer\u00b7des", "R\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PRF", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ie l\u00e4st sich Tag und Nacht vor deinen Augen blicken.", "tokens": ["ie", "l\u00e4st", "sich", "Tag", "und", "Nacht", "vor", "dei\u00b7nen", "Au\u00b7gen", "bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "ie h\u00e4lt kein blanckes Schwerdt/ kein starckes Waffen auff/", "tokens": ["ie", "h\u00e4lt", "kein", "blan\u00b7ckes", "Schwerdt", "/", "kein", "star\u00b7ckes", "Waf\u00b7fen", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "$(", "PIAT", "ADJA", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "es schnellen Adlers Flug/ der leichten Hunde Lauff", "tokens": ["es", "schnel\u00b7len", "Ad\u00b7lers", "Flug", "/", "der", "leich\u00b7ten", "Hun\u00b7de", "Lauff"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "st langsam gegen ihr/ sie ist vielmehr geschwinde", "tokens": ["st", "lang\u00b7sam", "ge\u00b7gen", "ihr", "/", "sie", "ist", "viel\u00b7mehr", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "$(", "PPER", "VAFIN", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "enn der bepfeilte Stahl und die erz\u00fcrnten Winde.", "tokens": ["enn", "der", "be\u00b7pfeil\u00b7te", "Stahl", "und", "die", "er\u00b7z\u00fcrn\u00b7ten", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}