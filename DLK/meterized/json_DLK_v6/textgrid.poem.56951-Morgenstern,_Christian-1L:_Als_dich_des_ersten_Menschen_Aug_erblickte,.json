{"textgrid.poem.56951": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als dich des ersten Menschen Aug erblickte,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als dich des ersten Menschen Aug erblickte,", "tokens": ["Als", "dich", "des", "ers\u00b7ten", "Men\u00b7schen", "Aug", "er\u00b7blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "empfand er schauernd: Meer! und aber: Meer!", "tokens": ["emp\u00b7fand", "er", "schau\u00b7ernd", ":", "Meer", "!", "und", "a\u00b7ber", ":", "Meer", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "NN", "$.", "KON", "ADV", "$.", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch eine Stimme sprach dazwischen: tot!", "tokens": ["Doch", "ei\u00b7ne", "Stim\u00b7me", "sprach", "da\u00b7zwi\u00b7schen", ":", "tot", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PAV", "$.", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und eine d\u00fcstre Trauer fiel auf ihn,", "tokens": ["Und", "ei\u00b7ne", "d\u00fcst\u00b7re", "Trau\u00b7er", "fiel", "auf", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df seine Sprache sich verwandelte,", "tokens": ["da\u00df", "sei\u00b7ne", "Spra\u00b7che", "sich", "ver\u00b7wan\u00b7del\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "wie wenn ein Vogel unter Wolken fliegt,", "tokens": ["wie", "wenn", "ein", "Vo\u00b7gel", "un\u00b7ter", "Wol\u00b7ken", "fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "die ihn verdunkeln.", "tokens": ["die", "ihn", "ver\u00b7dun\u00b7keln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}