{"textgrid.poem.66664": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ist wahres Liebesgl\u00fcck denn Sturm der Sinne?", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist wahres Liebesgl\u00fcck denn Sturm der Sinne?", "tokens": ["Ist", "wah\u00b7res", "Lie\u00b7bes\u00b7gl\u00fcck", "denn", "Sturm", "der", "Sin\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O nein! Im Sturme der Zusammenklang", "tokens": ["O", "nein", "!", "Im", "Stur\u00b7me", "der", "Zu\u00b7sam\u00b7men\u00b7klang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKANT", "$.", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verm\u00e4hlter Seelen, die zu starker Minne", "tokens": ["Ver\u00b7m\u00e4hl\u00b7ter", "See\u00b7len", ",", "die", "zu", "star\u00b7ker", "Min\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Sehnsuchtsruf der Geisterwelt durchdrang.", "tokens": ["Der", "Sehn\u00b7suchts\u00b7ruf", "der", "Geis\u00b7ter\u00b7welt", "durch\u00b7drang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Liebe schaut von morgenreiner Zinne", "tokens": ["Die", "Lie\u00b7be", "schaut", "von", "mor\u00b7gen\u00b7rei\u00b7ner", "Zin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In G\u00e4rten voller Blumen und Gesang:", "tokens": ["In", "G\u00e4r\u00b7ten", "vol\u00b7ler", "Blu\u00b7men", "und", "Ge\u00b7sang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Blumen himmelstauerquickt \u2013 Ges\u00e4nge,", "tokens": ["Die", "Blu\u00b7men", "him\u00b7mel\u00b7stau\u00b7er\u00b7quickt", "\u2013", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie wenn ein Kinderchor zum Licht sich schw\u00e4nge.", "tokens": ["Wie", "wenn", "ein", "Kin\u00b7der\u00b7chor", "zum", "Licht", "sich", "schw\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "NN", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn ich der Liebe keinen Hymnus schenkte,", "tokens": ["Wenn", "ich", "der", "Lie\u00b7be", "kei\u00b7nen", "Hym\u00b7nus", "schenk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich w\u00e4r' ein heillos undankbarer Mann,", "tokens": ["Ich", "w\u00e4r'", "ein", "heil\u00b7los", "un\u00b7dank\u00b7ba\u00b7rer", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der ich mit ihr mein Leben ordnend lenkte,", "tokens": ["Der", "ich", "mit", "ihr", "mein", "Le\u00b7ben", "ord\u00b7nend", "lenk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ohne sie vielleicht zu nichts zerrann.", "tokens": ["Das", "oh\u00b7ne", "sie", "viel\u00b7leicht", "zu", "nichts", "zer\u00b7rann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "ADV", "PTKA", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wohl mir, da\u00df ich aus dunklem Trieb versenkte", "tokens": ["Wohl", "mir", ",", "da\u00df", "ich", "aus", "dunk\u00b7lem", "Trieb", "ver\u00b7senk\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mich in den ungeheuren Seelenbann,", "tokens": ["Mich", "in", "den", "un\u00b7ge\u00b7heu\u00b7ren", "See\u00b7len\u00b7bann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Darin ich meinen aufgew\u00fchlten Geistern", "tokens": ["Da\u00b7rin", "ich", "mei\u00b7nen", "auf\u00b7ge\u00b7w\u00fchl\u00b7ten", "Geis\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Kraftpunkt gab, sich sammelnd zu bemeistern.", "tokens": ["Den", "Kraft\u00b7punkt", "gab", ",", "sich", "sam\u00b7melnd", "zu", "be\u00b7meis\u00b7tern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Erst war's ein Sto\u00df und dann ein zweifelnd Schwanken,", "tokens": ["Erst", "wa\u00b7r's", "ein", "Sto\u00df", "und", "dann", "ein", "zwei\u00b7felnd", "Schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Ein pendelnd Werben, Flucht und z\u00f6gernd Nahn,", "tokens": ["Ein", "pen\u00b7delnd", "Wer\u00b7ben", ",", "Flucht", "und", "z\u00f6\u00b7gernd", "Nahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein wiederkehrend Treiben der Gedanken", "tokens": ["Ein", "wie\u00b7der\u00b7keh\u00b7rend", "Trei\u00b7ben", "der", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf sonnenkreisender Planetenbahn.", "tokens": ["Auf", "son\u00b7nen\u00b7krei\u00b7sen\u00b7der", "Pla\u00b7ne\u00b7ten\u00b7bahn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Irrsterne rannten schreckend in die Flanken", "tokens": ["Irrs\u00b7ter\u00b7ne", "rann\u00b7ten", "schre\u00b7ckend", "in", "die", "Flan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Dem Lichtkern, den sie fest sich bilden sahn.", "tokens": ["Dem", "Licht\u00b7kern", ",", "den", "sie", "fest", "sich", "bil\u00b7den", "sahn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er wuchs und wuchs, trotz allem Widerstande,", "tokens": ["Er", "wuchs", "und", "wuchs", ",", "trotz", "al\u00b7lem", "Wi\u00b7der\u00b7stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit Venus-Jupiter im Schutzverbande.", "tokens": ["Mit", "Ve\u00b7nus\u00b7Ju\u00b7pi\u00b7ter", "im", "Schutz\u00b7ver\u00b7ban\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und so geschah's, da\u00df sich der Ring geschlossen,", "tokens": ["Und", "so", "ge\u00b7scha\u00b7h's", ",", "da\u00df", "sich", "der", "Ring", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "$,", "KOUS", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der mir ein Ring des neuen Lebens war,", "tokens": ["Der", "mir", "ein", "Ring", "des", "neu\u00b7en", "Le\u00b7bens", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein goldiger Lichtstrom hat sich ausgegossen", "tokens": ["Ein", "gol\u00b7di\u00b7ger", "Licht\u00b7strom", "hat", "sich", "aus\u00b7ge\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PRF", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Auf mein Gem\u00fct, das schon des Schimmers bar.", "tokens": ["Auf", "mein", "Ge\u00b7m\u00fct", ",", "das", "schon", "des", "Schim\u00b7mers", "bar", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich habe reine Seligkeit genossen", "tokens": ["Ich", "ha\u00b7be", "rei\u00b7ne", "Se\u00b7lig\u00b7keit", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und sah des Gl\u00fcckes Himmel tief und klar \u2013", "tokens": ["Und", "sah", "des", "Gl\u00fc\u00b7ckes", "Him\u00b7mel", "tief", "und", "klar", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Gut gewann ich durch der Liebe Glauben,", "tokens": ["Ein", "Gut", "ge\u00b7wann", "ich", "durch", "der", "Lie\u00b7be", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Des warmen Glanz kann mir kein Mi\u00dfgeist rauben.", "tokens": ["Des", "war\u00b7men", "Glanz", "kann", "mir", "kein", "Mi\u00df\u00b7geist", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}