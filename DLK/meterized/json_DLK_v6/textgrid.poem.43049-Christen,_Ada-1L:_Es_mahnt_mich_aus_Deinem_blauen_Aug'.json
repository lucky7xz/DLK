{"textgrid.poem.43049": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es mahnt mich aus Deinem blauen Aug'", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es mahnt mich aus Deinem blauen Aug'", "tokens": ["Es", "mahnt", "mich", "aus", "Dei\u00b7nem", "blau\u00b7en", "Aug'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein w\u00fcster Jugendtraum;", "tokens": ["Ein", "w\u00fcs\u00b7ter", "Ju\u00b7gend\u00b7traum", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da nickt ein blasses M\u00e4dchengesicht \u2013", "tokens": ["Da", "nickt", "ein", "blas\u00b7ses", "M\u00e4d\u00b7chen\u00b7ge\u00b7sicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ach, ich erkenn' es kaum.", "tokens": ["Ach", ",", "ich", "er\u00b7kenn'", "es", "kaum", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und ein entg\u00f6tterter Himmel liegt", "tokens": ["Und", "ein", "ent\u00b7g\u00f6t\u00b7ter\u00b7ter", "Him\u00b7mel", "liegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor mir \u2013 ach, Alles bricht! \u2013", "tokens": ["Vor", "mir", "\u2013", "ach", ",", "Al\u00b7les", "bricht", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "$(", "XY", "$,", "PIS", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch mildert das letzte, grellste Bild", "tokens": ["Doch", "mil\u00b7dert", "das", "letz\u00b7te", ",", "grells\u00b7te", "Bild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das s\u00fc\u00dfe blaue Licht.", "tokens": ["Das", "s\u00fc\u00b7\u00dfe", "blau\u00b7e", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}