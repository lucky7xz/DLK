{"dta.poem.19327": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du denkest, was du denkst, das m\u00fc\u00dfe drum so seyn;", "tokens": ["Du", "den\u00b7kest", ",", "was", "du", "denkst", ",", "das", "m\u00fc\u00b7\u00dfe", "drum", "so", "seyn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "PDS", "VMFIN", "PAV", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch denke: denkest du denn auf der Welt allein?", "tokens": ["Doch", "den\u00b7ke", ":", "den\u00b7kest", "du", "denn", "auf", "der", "Welt", "al\u00b7lein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Viel andre denken auch, viel andres denken sie,", "tokens": ["Viel", "and\u00b7re", "den\u00b7ken", "auch", ",", "viel", "and\u00b7res", "den\u00b7ken", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADV", "$,", "ADV", "PIS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch anders wird das Seyn durch anders denken nie.", "tokens": ["Doch", "an\u00b7ders", "wird", "das", "Seyn", "durch", "an\u00b7ders", "den\u00b7ken", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "APPR", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es l\u00e4\u00dft sich so und so von unserm Denken fassen,", "tokens": ["Es", "l\u00e4\u00dft", "sich", "so", "und", "so", "von", "un\u00b7serm", "Den\u00b7ken", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "KON", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bleibt was es ist, und sieht dem Spiele zu gelassen.", "tokens": ["Bleibt", "was", "es", "ist", ",", "und", "sieht", "dem", "Spie\u00b7le", "zu", "ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "PPER", "VAFIN", "$,", "KON", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}