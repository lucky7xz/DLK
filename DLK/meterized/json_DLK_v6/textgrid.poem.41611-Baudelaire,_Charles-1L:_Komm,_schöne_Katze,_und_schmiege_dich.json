{"textgrid.poem.41611": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Komm, sch\u00f6ne Katze, und schmiege dich", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, sch\u00f6ne Katze, und schmiege dich", "tokens": ["Komm", ",", "sch\u00f6\u00b7ne", "Kat\u00b7ze", ",", "und", "schmie\u00b7ge", "dich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "KON", "VVFIN", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An mein Herz, halt zur\u00fcck deine Kralle.", "tokens": ["An", "mein", "Herz", ",", "halt", "zu\u00b7r\u00fcck", "dei\u00b7ne", "Kral\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "PPOSAT", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "La\u00df den Blick in dein Auge tauchen mich,", "tokens": ["La\u00df", "den", "Blick", "in", "dein", "Au\u00b7ge", "tau\u00b7chen", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "In dein Aug' von Achat und Metalle.", "tokens": ["In", "dein", "Aug'", "von", "A\u00b7chat", "und", "Me\u00b7tal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "So oft dich mein Finger gem\u00e4chlich streift,", "tokens": ["So", "oft", "dich", "mein", "Fin\u00b7ger", "ge\u00b7m\u00e4ch\u00b7lich", "streift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Deinen Kopf und R\u00fccken zu schmeicheln,", "tokens": ["Dei\u00b7nen", "Kopf", "und", "R\u00fc\u00b7cken", "zu", "schmei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und tr\u00e4umende Lust meine Hand ergreift,", "tokens": ["Und", "tr\u00e4u\u00b7men\u00b7de", "Lust", "mei\u00b7ne", "Hand", "er\u00b7greift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die magnetischen Glieder zu streicheln,", "tokens": ["Die", "mag\u00b7ne\u00b7ti\u00b7schen", "Glie\u00b7der", "zu", "strei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Schau ich im Geist meine Frau. Der Strahl", "tokens": ["Schau", "ich", "im", "Geist", "mei\u00b7ne", "Frau", ".", "Der", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN", "PPOSAT", "NN", "$.", "ART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ihres Blicks, mein Tier, gleicht dem deinen,", "tokens": ["Ih\u00b7res", "Blicks", ",", "mein", "Tier", ",", "gleicht", "dem", "dei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ART", "PPOSAT", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ist tief und kalt wie ein schneidender Stahl.", "tokens": ["Ist", "tief", "und", "kalt", "wie", "ein", "schnei\u00b7den\u00b7der", "Stahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "In schmiegsamem Spiel haucht den feinen,", "tokens": ["In", "schmiegs\u00b7a\u00b7mem", "Spiel", "haucht", "den", "fei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Gef\u00e4hrlichen Duft, wie Schmeichelgru\u00df,", "tokens": ["Ge\u00b7f\u00e4hr\u00b7li\u00b7chen", "Duft", ",", "wie", "Schmei\u00b7chel\u00b7gru\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr brauner Leib von Kopf zu Fu\u00df.", "tokens": ["Ihr", "brau\u00b7ner", "Leib", "von", "Kopf", "zu", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}