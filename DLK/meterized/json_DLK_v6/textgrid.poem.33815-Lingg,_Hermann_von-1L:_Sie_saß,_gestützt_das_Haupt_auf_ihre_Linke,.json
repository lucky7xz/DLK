{"textgrid.poem.33815": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie sa\u00df, gest\u00fctzt das Haupt auf ihre Linke,", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie sa\u00df, gest\u00fctzt das Haupt auf ihre Linke,", "tokens": ["Sie", "sa\u00df", ",", "ge\u00b7st\u00fctzt", "das", "Haupt", "auf", "ih\u00b7re", "Lin\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ins Zimmer brach ein tr\u00fcber Sonnenschein.", "tokens": ["Ins", "Zim\u00b7mer", "brach", "ein", "tr\u00fc\u00b7ber", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Still traten und erwartend ihre Winke", "tokens": ["Still", "tra\u00b7ten", "und", "er\u00b7war\u00b7tend", "ih\u00b7re", "Win\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "KON", "VVPP", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Brot und Fr\u00fcchten ihre Diener ein.", "tokens": ["Mit", "Brot", "und", "Fr\u00fcch\u00b7ten", "ih\u00b7re", "Die\u00b7ner", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch ob Granat' und Goldorange blinke,", "tokens": ["Doch", "ob", "Gra\u00b7nat'", "und", "Gol\u00b7do\u00b7ran\u00b7ge", "blin\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ob aus dem Becher funkle s\u00fc\u00dfer Wein,", "tokens": ["Ob", "aus", "dem", "Be\u00b7cher", "funk\u00b7le", "s\u00fc\u00b7\u00dfer", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Sie blickt nicht auf, ihr Mund ist fest geschlossen,", "tokens": ["Sie", "blickt", "nicht", "auf", ",", "ihr", "Mund", "ist", "fest", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Wein und Fr\u00fcchte werden nicht genossen.", "tokens": ["Und", "Wein", "und", "Fr\u00fcch\u00b7te", "wer\u00b7den", "nicht", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da naht sich ihr und unterbricht das Schweigen", "tokens": ["Da", "naht", "sich", "ihr", "und", "un\u00b7ter\u00b7bricht", "das", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PPER", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein junger Neger mit gebeugtem Knie.", "tokens": ["Ein", "jun\u00b7ger", "Ne\u00b7ger", "mit", "ge\u00b7beug\u00b7tem", "Knie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf goldner Schale reicht er s\u00fc\u00dfe Feigen", "tokens": ["Auf", "gold\u00b7ner", "Scha\u00b7le", "reicht", "er", "s\u00fc\u00b7\u00dfe", "Fei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und spricht: O F\u00fcrstin, sch\u00f6n're sahst du nie.", "tokens": ["Und", "spricht", ":", "O", "F\u00fcrs\u00b7tin", ",", "sch\u00f6n'\u00b7re", "sahst", "du", "nie", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "NN", "$,", "VVFIN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erst seit drei Tagen sind sie von den Zweigen,", "tokens": ["Erst", "seit", "drei", "Ta\u00b7gen", "sind", "sie", "von", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Himmel meiner Heimat reifte sie;", "tokens": ["Der", "Him\u00b7mel", "mei\u00b7ner", "Hei\u00b7mat", "reif\u00b7te", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein guter Fahrwind lie\u00df es uns gelingen,", "tokens": ["Ein", "gu\u00b7ter", "Fahr\u00b7wind", "lie\u00df", "es", "uns", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Von Afrika sie frisch nach Rom zu bringen.", "tokens": ["Von", "Af\u00b7ri\u00b7ka", "sie", "frisch", "nach", "Rom", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "ADJD", "APPR", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbvon Afrika!\u00ab \u2013 und ihre Blicke flammen \u2013", "tokens": ["\u00bb", "von", "Af\u00b7ri\u00b7ka", "!", "\u00ab", "\u2013", "und", "ih\u00b7re", "Bli\u00b7cke", "flam\u00b7men", "\u2013"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NE", "$.", "$(", "$(", "KON", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbund sahst du dort die gro\u00dfen Helden nicht,", "tokens": ["\u00bb", "und", "sahst", "du", "dort", "die", "gro\u00b7\u00dfen", "Hel\u00b7den", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die aus dem wunderbaren Norden stammen,", "tokens": ["Die", "aus", "dem", "wun\u00b7der\u00b7ba\u00b7ren", "Nor\u00b7den", "stam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von deren Mut und Kraft die Sage spricht,", "tokens": ["Von", "de\u00b7ren", "Mut", "und", "Kraft", "die", "Sa\u00b7ge", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df sie gepanzert manchen Sund durchschwammen,", "tokens": ["Da\u00df", "sie", "ge\u00b7pan\u00b7zert", "man\u00b7chen", "Sund", "durch\u00b7schwam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ja, da\u00df sie mit der Waffen Erzgewicht", "tokens": ["Ja", ",", "da\u00df", "sie", "mit", "der", "Waf\u00b7fen", "Erz\u00b7ge\u00b7wicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dem Drang der Wogen sich entgegenstemmten", "tokens": ["Dem", "Drang", "der", "Wo\u00b7gen", "sich", "ent\u00b7ge\u00b7gens\u00b7temm\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und so den Flu\u00df in seiner Str\u00f6mung hemmten?", "tokens": ["Und", "so", "den", "Flu\u00df", "in", "sei\u00b7ner", "Str\u00f6\u00b7mung", "hemm\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und sahst du ihren K\u00f6nig, jenen d\u00fcstern", "tokens": ["Und", "sahst", "du", "ih\u00b7ren", "K\u00f6\u00b7nig", ",", "je\u00b7nen", "d\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PDS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vandalen Geiserich? Sein wilder Mut,", "tokens": ["Van\u00b7da\u00b7len", "Gei\u00b7se\u00b7rich", "?", "Sein", "wil\u00b7der", "Mut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nach unsern bl\u00fchendsten Provinzen l\u00fcstern,", "tokens": ["Nach", "un\u00b7sern", "bl\u00fc\u00b7hends\u00b7ten", "Pro\u00b7vin\u00b7zen", "l\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ist eine Sorg' uns, welche nimmer ruht.", "tokens": ["Ist", "ei\u00b7ne", "Sor\u00b7g'", "uns", ",", "wel\u00b7che", "nim\u00b7mer", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Hier nennt man seinen Namen nur mit Fl\u00fcstern,", "tokens": ["Hier", "nennt", "man", "sei\u00b7nen", "Na\u00b7men", "nur", "mit", "Fl\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch sag, blieb auch in eurer Sonnenglut", "tokens": ["Doch", "sag", ",", "blieb", "auch", "in", "eu\u00b7rer", "Son\u00b7nen\u00b7glut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Kriegslust seiner Scharen unerm\u00fcdlich?", "tokens": ["Die", "Kriegs\u00b7lust", "sei\u00b7ner", "Scha\u00b7ren", "un\u00b7er\u00b7m\u00fcd\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ward noch ihr Herz nicht \u00fcppig, weich und s\u00fcdlich?\u00ab", "tokens": ["Ward", "noch", "ihr", "Herz", "nicht", "\u00fcp\u00b7pig", ",", "weich", "und", "s\u00fcd\u00b7lich", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "ADJD", "$,", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbnein, F\u00fcrstin, m\u00e4chtig saust noch ihre Lanze,", "tokens": ["\u00bb", "nein", ",", "F\u00fcrs\u00b7tin", ",", "m\u00e4ch\u00b7tig", "saust", "noch", "ih\u00b7re", "Lan\u00b7ze", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "NN", "$,", "ADJD", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Karthago dr\u00f6hnt von ihrem Eisenschritt.", "tokens": ["Kar\u00b7tha\u00b7go", "dr\u00f6hnt", "von", "ih\u00b7rem", "Ei\u00b7sen\u00b7schritt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.3": {"text": "Wir sahn sie n\u00e4chtlich oft beim Fackelglanze,", "tokens": ["Wir", "sahn", "sie", "n\u00e4cht\u00b7lich", "oft", "beim", "Fa\u00b7ckel\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn aus dem Hafen ihre Flotte glitt.", "tokens": ["Wenn", "aus", "dem", "Ha\u00b7fen", "ih\u00b7re", "Flot\u00b7te", "glitt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auch nahmen sie zum k\u00fchnen Waffentanze", "tokens": ["Auch", "nah\u00b7men", "sie", "zum", "k\u00fch\u00b7nen", "Waf\u00b7fen\u00b7tan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und in die W\u00fcste mich zum Weidwerk mit;", "tokens": ["Und", "in", "die", "W\u00fcs\u00b7te", "mich", "zum", "Weid\u00b7werk", "mit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich sah sie von des Tigers Blut ger\u00f6tet,", "tokens": ["Ich", "sah", "sie", "von", "des", "Ti\u00b7gers", "Blut", "ge\u00b7r\u00f6\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den sie mit Einem Schwertesstreich get\u00f6tet.", "tokens": ["Den", "sie", "mit", "Ei\u00b7nem", "Schwer\u00b7tes\u00b7streich", "ge\u00b7t\u00f6\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ihr K\u00f6nig thront, vom L\u00f6wenpaar begleitet,", "tokens": ["Ihr", "K\u00f6\u00b7nig", "thront", ",", "vom", "L\u00f6\u00b7wen\u00b7paar", "be\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im d\u00fcstern Schlo\u00df, dem alle bang nur nahn.", "tokens": ["Im", "d\u00fcs\u00b7tern", "Schlo\u00df", ",", "dem", "al\u00b7le", "bang", "nur", "nahn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "PIS", "ADJD", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man sagt, wenn durch sein Arsenal er schreitet,", "tokens": ["Man", "sagt", ",", "wenn", "durch", "sein", "Ar\u00b7se\u00b7nal", "er", "schrei\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Waffen fingen sich zu r\u00fchren an,", "tokens": ["Die", "Waf\u00b7fen", "fin\u00b7gen", "sich", "zu", "r\u00fch\u00b7ren", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Solch eine Str\u00f6mung dunkler Kraft verbreitet", "tokens": ["Solch", "ei\u00b7ne", "Str\u00f6\u00b7mung", "dunk\u00b7ler", "Kraft", "ver\u00b7brei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sein Kriegergeist. Zieht er der Schar voran,", "tokens": ["Sein", "Krie\u00b7ger\u00b7geist", ".", "Zieht", "er", "der", "Schar", "vo\u00b7ran", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amph