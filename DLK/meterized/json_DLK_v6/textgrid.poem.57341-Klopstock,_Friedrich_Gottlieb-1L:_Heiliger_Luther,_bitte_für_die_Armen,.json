{"textgrid.poem.57341": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heiliger Luther, bitte f\u00fcr die Armen,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heiliger Luther, bitte f\u00fcr die Armen,", "tokens": ["Hei\u00b7li\u00b7ger", "Lu\u00b7ther", ",", "bit\u00b7te", "f\u00fcr", "die", "Ar\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Denen Geistes Beruf nicht scholl, und die doch", "tokens": ["De\u00b7nen", "Geis\u00b7tes", "Be\u00b7ruf", "nicht", "scholl", ",", "und", "die", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "PTKNEG", "ADJD", "$,", "KON", "ART", "ADV"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nachdolmetschen, dass sie zur Selbsterkentniss", "tokens": ["Nach\u00b7dol\u00b7met\u00b7schen", ",", "dass", "sie", "zur", "Selbs\u00b7ter\u00b7kent\u00b7niss"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Endlich genesen!", "tokens": ["End\u00b7lich", "ge\u00b7ne\u00b7sen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Weder die Sitte, noch der Sprache Weise", "tokens": ["We\u00b7der", "die", "Sit\u00b7te", ",", "noch", "der", "Spra\u00b7che", "Wei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ADV", "ART", "NN", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Kennen sie, und es ist der reinen Keuschheit", "tokens": ["Ken\u00b7nen", "sie", ",", "und", "es", "ist", "der", "rei\u00b7nen", "Keuschheit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ihnen M\u00e4rchen! was sich erhebt, was Kraft hat,", "tokens": ["Ih\u00b7nen", "M\u00e4r\u00b7chen", "!", "was", "sich", "er\u00b7hebt", ",", "was", "Kraft", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "PRELS", "PRF", "VVFIN", "$,", "PWS", "NN", "VAFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Edleres, Thorheit!", "tokens": ["Ed\u00b7le\u00b7res", ",", "Thor\u00b7heit", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJA", "$,", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Dunkel auf immer ihnen jener Gipfel,", "tokens": ["Dun\u00b7kel", "auf", "im\u00b7mer", "ih\u00b7nen", "je\u00b7ner", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADV", "PPER", "PDAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Den du muthig erstiegst, und dort des Vater", "tokens": ["Den", "du", "mut\u00b7hig", "er\u00b7stiegst", ",", "und", "dort", "des", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,", "KON", "ADV", "ART", "NN"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Landes Sprache bildetest, zu der Engel", "tokens": ["Lan\u00b7des", "Spra\u00b7che", "bil\u00b7de\u00b7test", ",", "zu", "der", "En\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Sprach', und der Menschen.", "tokens": ["Sprach'", ",", "und", "der", "Men\u00b7schen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Zeiten entflohn: allein die umgeschafne", "tokens": ["Zei\u00b7ten", "ent\u00b7flohn", ":", "al\u00b7lein", "die", "um\u00b7ge\u00b7schaf\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "ADV", "ART", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Blieb; und diese Gestalt wird nie sich wandeln!", "tokens": ["Blieb", ";", "und", "die\u00b7se", "Ge\u00b7stalt", "wird", "nie", "sich", "wan\u00b7deln", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "PDAT", "NN", "VAFIN", "ADV", "PRF", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "L\u00e4cheln wird, wie wir, sie dereinst der Enkel,", "tokens": ["L\u00e4\u00b7cheln", "wird", ",", "wie", "wir", ",", "sie", "de\u00b7reinst", "der", "En\u00b7kel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PWAV", "PPER", "$,", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Ernst sie, wie wir, sehn.", "tokens": ["Ernst", "sie", ",", "wie", "wir", ",", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPER", "$,", "PWAV", "PPER", "$,", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Heiliger Luther, bitte f\u00fcr die Armen,", "tokens": ["Hei\u00b7li\u00b7ger", "Lu\u00b7ther", ",", "bit\u00b7te", "f\u00fcr", "die", "Ar\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dass ihr stammelnd Gered' ihr Ohr vernehme,", "tokens": ["Dass", "ihr", "stam\u00b7melnd", "Ge\u00b7red'", "ihr", "Ohr", "ver\u00b7neh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und sie dastehn, Thr\u00e4nen der Reu im Blick, die", "tokens": ["Und", "sie", "das\u00b7tehn", ",", "Thr\u00e4\u00b7nen", "der", "Reu", "im", "Blick", ",", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VVINF", "$,", "NN", "ART", "NE", "APPRART", "NN", "$,", "PRELS"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Hand auf dem Munde!", "tokens": ["Hand", "auf", "dem", "Mun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}