{"textgrid.poem.61894": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von des Sonnengotts Geschossen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von des Sonnengotts Geschossen", "tokens": ["Von", "des", "Son\u00b7nen\u00b7gotts", "Ge\u00b7schos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegen Wald und Flur versengt,", "tokens": ["Lie\u00b7gen", "Wald", "und", "Flur", "ver\u00b7sengt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dr\u00fcber, wie aus Stahl gegossen,", "tokens": ["Dr\u00fc\u00b7ber", ",", "wie", "aus", "Stahl", "ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wolkenlose Bl\u00e4ue h\u00e4ngt.", "tokens": ["Wol\u00b7ken\u00b7lo\u00b7se", "Bl\u00e4u\u00b7e", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In der glutgeborstnen Erde", "tokens": ["In", "der", "glut\u00b7ge\u00b7borst\u00b7nen", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stirbt das Saatkorn, durstig \u00e4chzt", "tokens": ["Stirbt", "das", "Saat\u00b7korn", ",", "durs\u00b7tig", "\u00e4chzt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Am versiegten Bach die Herde,", "tokens": ["Am", "ver\u00b7sieg\u00b7ten", "Bach", "die", "Her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Hirsch im Forste lechzt.", "tokens": ["Und", "der", "Hirsch", "im", "Fors\u00b7te", "lechzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Kein Gesang mehr in den Zweigen!", "tokens": ["Kein", "Ge\u00b7sang", "mehr", "in", "den", "Zwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keine Lilie mehr am Rain! \u2013", "tokens": ["Kei\u00b7ne", "Li\u00b7lie", "mehr", "am", "Rain", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "ADV", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O wann wirst du niedersteigen,", "tokens": ["O", "wann", "wirst", "du", "nie\u00b7ders\u00b7tei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Donnerer, wir harren dein.", "tokens": ["Don\u00b7ne\u00b7rer", ",", "wir", "har\u00b7ren", "dein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Komm, o komm in Wetterschl\u00e4gen!", "tokens": ["Komm", ",", "o", "komm", "in", "Wet\u00b7ter\u00b7schl\u00e4\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Braut vergeht vor Weh \u2013", "tokens": ["Dei\u00b7ne", "Braut", "ver\u00b7geht", "vor", "Weh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm herab im goldnen Regen", "tokens": ["Komm", "her\u00b7ab", "im", "gold\u00b7nen", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zur verschmachtenden Danae!", "tokens": ["Zur", "ver\u00b7schmach\u00b7ten\u00b7den", "Da\u00b7nae", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}