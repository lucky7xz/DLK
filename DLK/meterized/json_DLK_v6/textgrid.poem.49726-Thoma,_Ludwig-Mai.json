{"textgrid.poem.49726": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Mai", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was uns der sch\u00f6ne Fr\u00fchling tut,", "tokens": ["Was", "uns", "der", "sch\u00f6\u00b7ne", "Fr\u00fch\u00b7ling", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist lauter Lieb und Wonne.", "tokens": ["Ist", "lau\u00b7ter", "Lieb", "und", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den M\u00e4deln wird es so zumut", "tokens": ["Den", "M\u00e4\u00b7deln", "wird", "es", "so", "zu\u00b7mut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Katzen in der Sonne.", "tokens": ["Wie", "Kat\u00b7zen", "in", "der", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie schnurren rings um uns herum,", "tokens": ["Sie", "schnur\u00b7ren", "rings", "um", "uns", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie l\u00e4cheln und sie schmeicheln,", "tokens": ["Sie", "l\u00e4\u00b7cheln", "und", "sie", "schmei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man f\u00fchlt was wie ein Fluidum,", "tokens": ["Man", "f\u00fchlt", "was", "wie", "ein", "Flu\u00b7i\u00b7dum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man mu\u00df die Tierchen streicheln.", "tokens": ["Man", "mu\u00df", "die", "Tier\u00b7chen", "strei\u00b7cheln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und kommt man auch nur leis daran,", "tokens": ["Und", "kommt", "man", "auch", "nur", "leis", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "ADV", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So ist 's um uns geschehen,", "tokens": ["So", "ist", "'s", "um", "uns", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem Fr\u00fchling und dem Baldrian", "tokens": ["Dem", "Fr\u00fch\u00b7ling", "und", "dem", "Bald\u00b7ri\u00b7an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann keiner widerstehen.", "tokens": ["Kann", "kei\u00b7ner", "wi\u00b7der\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Herr Kirchenrat, Sie schweigen still!", "tokens": ["Herr", "Kir\u00b7chen\u00b7rat", ",", "Sie", "schwei\u00b7gen", "still", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es l\u00e4\u00dft sich nicht vermeiden.", "tokens": ["Es", "l\u00e4\u00dft", "sich", "nicht", "ver\u00b7mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn Gott die Sache selbst nicht will,", "tokens": ["Wenn", "Gott", "die", "Sa\u00b7che", "selbst", "nicht", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ADV", "PTKNEG", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df er die Kater schneiden.", "tokens": ["Mu\u00df", "er", "die", "Ka\u00b7ter", "schnei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}