{"textgrid.poem.34686": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied von der Rache", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heran, heran! \u2013 Die Kriegstrompeten schmettern.", "tokens": ["He\u00b7ran", ",", "he\u00b7ran", "!", "\u2013", "Die", "Kriegs\u00b7trom\u00b7pe\u00b7ten", "schmet\u00b7tern", "."], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PTKVZ", "$.", "$(", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Heran! Der Donner braust! \u2013", "tokens": ["He\u00b7ran", "!", "Der", "Don\u00b7ner", "braust", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PTKVZ", "$.", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Rache ruft in zack'gen Flammenwettern", "tokens": ["Die", "Ra\u00b7che", "ruft", "in", "zack'\u00b7gen", "Flam\u00b7men\u00b7wet\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der deutschen R\u00e4cherfaust!", "tokens": ["Der", "deut\u00b7schen", "R\u00e4\u00b7cher\u00b7faust", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Heran, heran zum wilden Furientanze,", "tokens": ["He\u00b7ran", ",", "he\u00b7ran", "zum", "wil\u00b7den", "Fu\u00b7ri\u00b7en\u00b7tan\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Noch lebt und gl\u00fcht der Molch!", "tokens": ["Noch", "lebt", "und", "gl\u00fcht", "der", "Molch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drauf, Br\u00fcder, drauf mit B\u00fcchse, Schwert und Lanze,", "tokens": ["Drauf", ",", "Br\u00fc\u00b7der", ",", "drauf", "mit", "B\u00fcch\u00b7se", ",", "Schwert", "und", "Lan\u00b7ze", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$,", "PAV", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drauf, drauf mit Gift und Dolch!", "tokens": ["Drauf", ",", "drauf", "mit", "Gift", "und", "Dolch", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PAV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was V\u00f6lkerrecht? \u2013 Was sich der Nacht verpf\u00e4ndet,", "tokens": ["Was", "V\u00f6l\u00b7ker\u00b7recht", "?", "\u2013", "Was", "sich", "der", "Nacht", "ver\u00b7pf\u00e4n\u00b7det", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$.", "$(", "PWS", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist reife H\u00f6llensaat.", "tokens": ["Ist", "rei\u00b7fe", "H\u00f6l\u00b7len\u00b7saat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo ist das Recht, das nicht der Hund gesch\u00e4ndet", "tokens": ["Wo", "ist", "das", "Recht", ",", "das", "nicht", "der", "Hund", "ge\u00b7sch\u00e4n\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "PTKNEG", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Mord und mit Verrat?", "tokens": ["Mit", "Mord", "und", "mit", "Ver\u00b7rat", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "S\u00fchnt Blut mit Blut! \u2013 Was Waffen tr\u00e4gt, schlagt nieder!", "tokens": ["S\u00fchnt", "Blut", "mit", "Blut", "!", "\u2013", "Was", "Waf\u00b7fen", "tr\u00e4gt", ",", "schlagt", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "$.", "$(", "PWS", "NN", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "'s ist alles Schurkenbrut!", "tokens": ["'s", "ist", "al\u00b7les", "Schur\u00b7ken\u00b7brut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denkt unsres Schwurs, denkt der verrat'nen Br\u00fcder", "tokens": ["Denkt", "uns\u00b7res", "Schwurs", ",", "denkt", "der", "ver\u00b7rat'\u00b7nen", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und sauft euch satt in Blut!", "tokens": ["Und", "sauft", "euch", "satt", "in", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wenn sie winselnd auf den Knien liegen", "tokens": ["Und", "wenn", "sie", "win\u00b7selnd", "auf", "den", "Kni\u00b7en", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und zitternd Gnade schrei'n \u2013", "tokens": ["Und", "zit\u00b7ternd", "Gna\u00b7de", "schrei'n", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00dft nicht des Mitleids feige Stimme siegen,", "tokens": ["La\u00dft", "nicht", "des", "Mit\u00b7leids", "fei\u00b7ge", "Stim\u00b7me", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sto\u00dft ohn' Erbarmen drein!", "tokens": ["Sto\u00dft", "ohn'", "Er\u00b7bar\u00b7men", "drein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und r\u00fchmten sie, da\u00df Blut von deutschen Helden", "tokens": ["Und", "r\u00fchm\u00b7ten", "sie", ",", "da\u00df", "Blut", "von", "deut\u00b7schen", "Hel\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In ihren Adern rinnt \u2013", "tokens": ["In", "ih\u00b7ren", "A\u00b7dern", "rinnt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die k\u00f6nnen nicht des Landes S\u00f6hne gelten,", "tokens": ["Die", "k\u00f6n\u00b7nen", "nicht", "des", "Lan\u00b7des", "S\u00f6h\u00b7ne", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die seine Teufel sind.", "tokens": ["Die", "sei\u00b7ne", "Teu\u00b7fel", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ha, welche Lust, wenn an dem Lanzenknopfe", "tokens": ["Ha", ",", "wel\u00b7che", "Lust", ",", "wenn", "an", "dem", "Lan\u00b7zen\u00b7knop\u00b7fe"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAT", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ein Schurkenherz zerbebt", "tokens": ["Ein", "Schur\u00b7ken\u00b7herz", "zer\u00b7bebt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und das Gehirn aus dem gespalt'nen Kopfe", "tokens": ["Und", "das", "Ge\u00b7hirn", "aus", "dem", "ge\u00b7spalt'\u00b7nen", "Kop\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am blut'gen Schwerte klebt!", "tokens": ["Am", "blut'\u00b7gen", "Schwer\u00b7te", "klebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Welch Ohrenschmaus, wenn wir bei Siegesrufen,", "tokens": ["Welch", "Oh\u00b7ren\u00b7schmaus", ",", "wenn", "wir", "bei", "Sie\u00b7ges\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Pulverdampf umqualmt,", "tokens": ["Vom", "Pul\u00b7ver\u00b7dampf", "um\u00b7qualmt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie winseln h\u00f6ren, von der Rosse Hufen", "tokens": ["Sie", "win\u00b7seln", "h\u00f6\u00b7ren", ",", "von", "der", "Ros\u00b7se", "Hu\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "VVINF", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf deutschem Grund zermalmt!", "tokens": ["Auf", "deut\u00b7schem", "Grund", "zer\u00b7malmt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Gott ist mit uns! \u2013 Der H\u00f6lle Nebel weichen,", "tokens": ["Gott", "ist", "mit", "uns", "!", "\u2013", "Der", "H\u00f6l\u00b7le", "Ne\u00b7bel", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "$.", "$(", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinauf, du Stern, hinauf!", "tokens": ["Hin\u00b7auf", ",", "du", "Stern", ",", "hin\u00b7auf", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PPER", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir t\u00fcrmen dir die H\u00fcgel ihrer Leichen", "tokens": ["Wir", "t\u00fcr\u00b7men", "dir", "die", "H\u00fc\u00b7gel", "ih\u00b7rer", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zur Pyramide auf!", "tokens": ["Zur", "Py\u00b7ra\u00b7mi\u00b7de", "auf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dann brennt sie an, \u2013 und streut es in die L\u00fcfte,", "tokens": ["Dann", "brennt", "sie", "an", ",", "\u2013", "und", "streut", "es", "in", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "$(", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was nicht die Flamme fra\u00df,", "tokens": ["Was", "nicht", "die", "Flam\u00b7me", "fra\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Damit kein Grab das deutsche Land vergifte", "tokens": ["Da\u00b7mit", "kein", "Grab", "das", "deut\u00b7sche", "Land", "ver\u00b7gif\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PIAT", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit \u00fcberrhein'schem Aas!", "tokens": ["Mit", "\u00fc\u00b7ber\u00b7rhein'\u00b7schem", "Aas", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}