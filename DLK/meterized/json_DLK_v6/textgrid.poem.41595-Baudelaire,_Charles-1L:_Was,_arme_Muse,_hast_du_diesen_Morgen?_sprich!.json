{"textgrid.poem.41595": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was, arme Muse, hast du diesen Morgen? sprich!", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was, arme Muse, hast du diesen Morgen? sprich!", "tokens": ["Was", ",", "ar\u00b7me", "Mu\u00b7se", ",", "hast", "du", "die\u00b7sen", "Mor\u00b7gen", "?", "sprich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "$,", "ADJA", "NN", "$,", "VAFIN", "PPER", "PDAT", "NN", "$.", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch bebt dein hohler Blick vom Traum, der dich bedr\u00e4ngte,", "tokens": ["Noch", "bebt", "dein", "hoh\u00b7ler", "Blick", "vom", "Traum", ",", "der", "dich", "be\u00b7dr\u00e4ng\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Abwechselnd breiten bleich auf deinem Antlitz sich", "tokens": ["Ab\u00b7wech\u00b7selnd", "brei\u00b7ten", "bleich", "auf", "dei\u00b7nem", "Ant\u00b7litz", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "ADJD", "APPR", "PPOSAT", "NN", "PRF"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Wahnsinn und Schreck, der stumm und eisig dich beengte.", "tokens": ["Wahn\u00b7sinn", "und", "Schreck", ",", "der", "stumm", "und", "ei\u00b7sig", "dich", "be\u00b7eng\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "War es ein gr\u00fcner Elf, ein rot Gespenst, das dich", "tokens": ["War", "es", "ein", "gr\u00fc\u00b7ner", "Elf", ",", "ein", "rot", "Ge\u00b7spenst", ",", "das", "dich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,", "ART", "ADJD", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Liebe oder Furcht aus seiner Urne tr\u00e4nkte?", "tokens": ["Mit", "Lie\u00b7be", "o\u00b7der", "Furcht", "aus", "sei\u00b7ner", "Ur\u00b7ne", "tr\u00e4nk\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "War es ein schwerer Traum, der herb und f\u00fcrchterlich", "tokens": ["War", "es", "ein", "schwe\u00b7rer", "Traum", ",", "der", "herb", "und", "f\u00fcrch\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In einem zaubrischen Minturn\u00e4 dich versenkte?", "tokens": ["In", "ei\u00b7nem", "zaub\u00b7ri\u00b7schen", "Min\u00b7tur\u00b7n\u00e4", "dich", "ver\u00b7senk\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich wollte, es enthaucht' den Duft gesunder Kraft", "tokens": ["Ich", "woll\u00b7te", ",", "es", "ent\u00b7haucht'", "den", "Duft", "ge\u00b7sun\u00b7der", "Kraft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Busen, der stets neu Gedanken formt und schafft,", "tokens": ["Dein", "Bu\u00b7sen", ",", "der", "stets", "neu", "Ge\u00b7dan\u00b7ken", "formt", "und", "schafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "ADJD", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es fl\u00f6ss dein christlich Blut in Rhythmen auf und nieder.", "tokens": ["Es", "fl\u00f6ss", "dein", "christ\u00b7lich", "Blut", "in", "Rhyth\u00b7men", "auf", "und", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJD", "NN", "APPR", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie mannigfaltiges Get\u00f6n antiker Lieder,", "tokens": ["Wie", "man\u00b7nig\u00b7fal\u00b7ti\u00b7ges", "Ge\u00b7t\u00f6n", "an\u00b7ti\u00b7ker", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Da, wo mit Ph\u00f6bus, dem die Sangkunst untertan,", "tokens": ["Da", ",", "wo", "mit", "Ph\u00f6\u00b7bus", ",", "dem", "die", "Sang\u00b7kunst", "un\u00b7ter\u00b7tan", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "APPR", "NE", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vereint, der Ernte Herr regiert, der gro\u00dfe Pan.", "tokens": ["Ver\u00b7eint", ",", "der", "Ern\u00b7te", "Herr", "re\u00b7giert", ",", "der", "gro\u00b7\u00dfe", "Pan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}