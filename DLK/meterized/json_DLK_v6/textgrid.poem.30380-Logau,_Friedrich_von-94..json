{"textgrid.poem.30380": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "94.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist Falsus ein Apostel? die Zung ist ihm zertheilt.", "tokens": ["Ist", "Fal\u00b7sus", "ein", "A\u00b7pos\u00b7tel", "?", "die", "Zung", "ist", "ihm", "zer\u00b7theilt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ART", "NN", "$.", "ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "O nein! es ist nur sonsten ein \u00dcbel, das nicht heilt.", "tokens": ["O", "nein", "!", "es", "ist", "nur", "sons\u00b7ten", "ein", "\u00dc\u00b7bel", ",", "das", "nicht", "heilt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}