{"dta.poem.7170": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Gr\u00f6sse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du, als der Unwandelbare,", "tokens": ["Du", ",", "als", "der", "Un\u00b7wan\u00b7del\u00b7ba\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "ART", "NN", "$,"], "meter": "++-++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Legest keine Zeit zur\u00fcck.", "tokens": ["Le\u00b7gest", "kei\u00b7ne", "Zeit", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Stunden, Tag und Jahre", "tokens": ["Dei\u00b7ne", "Stun\u00b7den", ",", "Tag", "und", "Jah\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind ein steter Augenblick.", "tokens": ["Sind", "ein", "ste\u00b7ter", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "GoTT, vor dem best\u00e4ndig stehet,", "tokens": ["GoTT", ",", "vor", "dem", "be\u00b7st\u00e4n\u00b7dig", "ste\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was entsteht, und was vergehet!", "tokens": ["Was", "ent\u00b7steht", ",", "und", "was", "ver\u00b7ge\u00b7het", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KON", "PWS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Quell der grauen Ewigkeit,", "tokens": ["Quell", "der", "grau\u00b7en", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ewigs Jtzt, best\u00e4ndigs Heut!", "tokens": ["E\u00b7wigs", "Jtzt", ",", "be\u00b7st\u00e4n\u00b7digs", "Heut", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}