{"dta.poem.1692": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Erg\u00f6tzet die Sinnen ", "tokens": ["Er\u00b7g\u00f6t\u00b7zet", "die", "Sin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mit frohem Beginnen/", "tokens": ["Mit", "fro\u00b7hem", "Be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Doch dencket dabey/", "tokens": ["Doch", "den\u00b7cket", "da\u00b7bey", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Da\u00df Leyden und Scheiden", "tokens": ["Da\u00df", "Ley\u00b7den", "und", "Schei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Der irdischen Freuden", "tokens": ["Der", "ir\u00b7di\u00b7schen", "Freu\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Verwechselung sey.", "tokens": ["Ver\u00b7wech\u00b7se\u00b7lung", "sey", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Die S\u00e4hne vom Bogen", "tokens": ["Die", "S\u00e4h\u00b7ne", "vom", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die immer gezogen/", "tokens": ["Die", "im\u00b7mer", "ge\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Bricht endlich und reist;", "tokens": ["Bricht", "end\u00b7lich", "und", "reist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "In stetigem Trauren", "tokens": ["In", "ste\u00b7ti\u00b7gem", "Trau\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Das Hertze vermauren", "tokens": ["Das", "Hert\u00b7ze", "ver\u00b7mau\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Erstecket den Geist.", "tokens": ["Er\u00b7ste\u00b7cket", "den", "Geist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Vern\u00fcnfftiges Schertzen", "tokens": ["Ver\u00b7n\u00fcnff\u00b7ti\u00b7ges", "Schert\u00b7zen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ermuntert die Hertzen", "tokens": ["Er\u00b7mun\u00b7tert", "die", "Hert\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Erfrischet das Blutt/", "tokens": ["Er\u00b7fri\u00b7schet", "das", "Blutt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Drum brauchet der Jugend", "tokens": ["Drum", "brau\u00b7chet", "der", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "In Ehren und Tugend/", "tokens": ["In", "Eh\u00b7ren", "und", "Tu\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Mit fr\u00f6lichem Mutt.", "tokens": ["Mit", "fr\u00f6\u00b7li\u00b7chem", "Mutt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Lacht/ spielet und singet/", "tokens": ["Lacht", "/", "spie\u00b7let", "und", "sin\u00b7get", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Schw\u00e4zt/ tantzet und klinget/", "tokens": ["Schw\u00e4zt", "/", "tant\u00b7zet", "und", "klin\u00b7get", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Bl\u00fctte geht hin/", "tokens": ["Die", "Bl\u00fct\u00b7te", "geht", "hin", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Di\u00df was ihr empfunden", "tokens": ["Di\u00df", "was", "ihr", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PRELS", "PPER", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "In lustigen Stunden/", "tokens": ["In", "lus\u00b7ti\u00b7gen", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ist euer Gewinn.", "tokens": ["Ist", "eu\u00b7er", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Die traurigen Tage", "tokens": ["Die", "trau\u00b7ri\u00b7gen", "Ta\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Verdr\u00fc\u00dfliche Plage/", "tokens": ["Ver\u00b7dr\u00fc\u00df\u00b7li\u00b7che", "Pla\u00b7ge", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Samt kr\u00e4ncklicher Zeit/", "tokens": ["Samt", "kr\u00e4n\u00b7ck\u00b7li\u00b7cher", "Zeit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sind Fr\u00fcchte der Jahre/", "tokens": ["Sind", "Fr\u00fcch\u00b7te", "der", "Jah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Bi\u00df Sterben und Bahre", "tokens": ["Bi\u00df", "Ster\u00b7ben", "und", "Bah\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ja Rechnung bereit.", "tokens": ["Ja", "Rech\u00b7nung", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}