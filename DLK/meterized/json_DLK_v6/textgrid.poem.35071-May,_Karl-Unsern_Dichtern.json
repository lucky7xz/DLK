{"textgrid.poem.35071": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Unsern Dichtern", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geht nach dem Morgenland; vernehmt die Weisen,", "tokens": ["Geht", "nach", "dem", "Mor\u00b7gen\u00b7land", ";", "ver\u00b7nehmt", "die", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die einst zum Saitenspiele dort erklungen.", "tokens": ["Die", "einst", "zum", "Sai\u00b7ten\u00b7spie\u00b7le", "dort", "er\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie sollten Gott, den Einzigen, nur preisen", "tokens": ["Sie", "soll\u00b7ten", "Gott", ",", "den", "Ein\u00b7zi\u00b7gen", ",", "nur", "prei\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "$,", "ART", "NN", "$,", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wurden doch f\u00fcr Andre auch gesungen.", "tokens": ["Und", "wur\u00b7den", "doch", "f\u00fcr", "And\u00b7re", "auch", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die S\u00e4nger starben, doch seht ihr die Noten", "tokens": ["Die", "S\u00e4n\u00b7ger", "star\u00b7ben", ",", "doch", "seht", "ihr", "die", "No\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der Lieder noch, wenn ihr vor S\u00e4ulen steht,", "tokens": ["Der", "Lie\u00b7der", "noch", ",", "wenn", "ihr", "vor", "S\u00e4u\u00b7len", "steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und mit dem Auge h\u00f6rt ihr noch der Todten", "tokens": ["Und", "mit", "dem", "Au\u00b7ge", "h\u00f6rt", "ihr", "noch", "der", "Tod\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ges\u00e4nge, wenn ihr durch die Tr\u00fcmmer geht.", "tokens": ["Ge\u00b7s\u00e4n\u00b7ge", ",", "wenn", "ihr", "durch", "die", "Tr\u00fcm\u00b7mer", "geht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Psalter und die Harfen sind zerbrochen,", "tokens": ["Die", "Psal\u00b7ter", "und", "die", "Har\u00b7fen", "sind", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu denen Davids Stimme man geh\u00f6rt,", "tokens": ["Zu", "de\u00b7nen", "Da\u00b7vids", "Stim\u00b7me", "man", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NE", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wo der Herr durch Steine einst gesprochen,", "tokens": ["Und", "wo", "der", "Herr", "durch", "Stei\u00b7ne", "einst", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Liegt ihre Harmonie, ihr Reim zerst\u00f6rt.", "tokens": ["Liegt", "ih\u00b7re", "Har\u00b7mo\u00b7nie", ",", "ihr", "Reim", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch seht ihr wo ein Kapit\u00e4l noch ragen,", "tokens": ["Doch", "seht", "ihr", "wo", "ein", "Ka\u00b7pi\u00b7t\u00e4l", "noch", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PWAV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein steinern Lied, im zarten Mondesschein,", "tokens": ["Ein", "stei\u00b7nern", "Lied", ",", "im", "zar\u00b7ten", "Mon\u00b7des\u00b7schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So d\u00fcrft ihr im Gedicht es heimw\u00e4rts tragen", "tokens": ["So", "d\u00fcrft", "ihr", "im", "Ge\u00b7dicht", "es", "heim\u00b7w\u00e4rts", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und der Verstorbnen fromme Erben sein.", "tokens": ["Und", "der", "Ver\u00b7storb\u00b7nen", "from\u00b7me", "Er\u00b7ben", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Geht nach dem Morgenland; vernehmt die Weisen,", "tokens": ["Geht", "nach", "dem", "Mor\u00b7gen\u00b7land", ";", "ver\u00b7nehmt", "die", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die dorten einst in Wort und Werk erklungen.", "tokens": ["Die", "dor\u00b7ten", "einst", "in", "Wort", "und", "Werk", "er\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie sollten Gott, den Einzigen, nur preisen", "tokens": ["Sie", "soll\u00b7ten", "Gott", ",", "den", "Ein\u00b7zi\u00b7gen", ",", "nur", "prei\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "$,", "ART", "NN", "$,", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wurden doch f\u00fcr ihn nicht ausgesungen.", "tokens": ["Und", "wur\u00b7den", "doch", "f\u00fcr", "ihn", "nicht", "aus\u00b7ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die T\u00f6ne h\u00f6rt, die sich aus Tr\u00fcmmern ringen;", "tokens": ["Die", "T\u00f6\u00b7ne", "h\u00f6rt", ",", "die", "sich", "aus", "Tr\u00fcm\u00b7mern", "rin\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vernehmt ihr Klagen, und befreiet sie;", "tokens": ["Ver\u00b7nehmt", "ihr", "Kla\u00b7gen", ",", "und", "be\u00b7frei\u00b7et", "sie", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dann wird in Euern Liedern neu erklingen", "tokens": ["Dann", "wird", "in", "Eu\u00b7ern", "Lie\u00b7dern", "neu", "er\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Des Morgenlandes Gottespoesie!", "tokens": ["Des", "Mor\u00b7gen\u00b7lan\u00b7des", "Got\u00b7tes\u00b7po\u00b7e\u00b7sie", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}