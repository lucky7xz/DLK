{"textgrid.poem.42487": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbmein ist die Erbschaft laut Pergament,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmein ist die Erbschaft laut Pergament,", "tokens": ["\u00bb", "mein", "ist", "die", "Erb\u00b7schaft", "laut", "Per\u00b7ga\u00b7ment", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und mir geh\u00f6rt sie zu!\u00ab", "tokens": ["Und", "mir", "ge\u00b7h\u00f6rt", "sie", "zu", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Regenten in Meldorf schlagens ihm ab:", "tokens": ["Die", "Re\u00b7gen\u00b7ten", "in", "Mel\u00b7dorf", "schla\u00b7gens", "ihm", "ab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nun la\u00df uns endlich in Ruh!", "tokens": ["Nun", "la\u00df", "uns", "end\u00b7lich", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Wiben Peter setzt sich auf sein wei\u00dfes Pferd,", "tokens": ["Wi\u00b7ben", "Pe\u00b7ter", "setzt", "sich", "auf", "sein", "wei\u00b7\u00dfes", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Er reitet auf Markt und Gassen,", "tokens": ["Er", "rei\u00b7tet", "auf", "Markt", "und", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Das Landesbuch links, in der andern das Schwert:", "tokens": ["Das", "Lan\u00b7des\u00b7buch", "links", ",", "in", "der", "an\u00b7dern", "das", "Schwert", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPR", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "\u00bbsie m\u00fcssen mein Recht mir lassen!\u00ab", "tokens": ["\u00bb", "sie", "m\u00fcs\u00b7sen", "mein", "Recht", "mir", "las\u00b7sen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Holla! Er h\u00e4lt und l\u00e4\u00dft in der Hand", "tokens": ["Hol\u00b7la", "!", "Er", "h\u00e4lt", "und", "l\u00e4\u00dft", "in", "der", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Die beiden im Sonnenlicht blinken.", "tokens": ["Die", "bei\u00b7den", "im", "Son\u00b7nen\u00b7licht", "blin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "Das hilft ihm nichts, er wird verbannt;", "tokens": ["Das", "hilft", "ihm", "nichts", ",", "er", "wird", "ver\u00b7bannt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIS", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sein Hengst f\u00fchlt unlieb die Zinken.", "tokens": ["Sein", "Hengst", "f\u00fchlt", "un\u00b7lieb", "die", "Zin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Er reitet ins Elend. Aber voll Mut", "tokens": ["Er", "rei\u00b7tet", "ins", "E\u00b7lend", ".", "A\u00b7ber", "voll", "Mut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "KON", "ADJD", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Will er erzwingen sein Recht", "tokens": ["Will", "er", "er\u00b7zwin\u00b7gen", "sein", "Recht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Vor F\u00fcrsten und Rat, vor Kaiser und Reich;", "tokens": ["Vor", "F\u00fcrs\u00b7ten", "und", "Rat", ",", "vor", "Kai\u00b7ser", "und", "Reich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch gelingt ihm sein Vorhaben schlecht.", "tokens": ["Doch", "ge\u00b7lingt", "ihm", "sein", "Vor\u00b7ha\u00b7ben", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "\u00dcberall weisen sie kl\u00e4glich ihn ab,", "tokens": ["\u00dc\u00b7be\u00b7rall", "wei\u00b7sen", "sie", "kl\u00e4g\u00b7lich", "ihn", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Und immer mu\u00df ers erneuen;", "tokens": ["Und", "im\u00b7mer", "mu\u00df", "ers", "er\u00b7neu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Stets wieder bringt man ihn auf den Trab,", "tokens": ["Stets", "wie\u00b7der", "bringt", "man", "ihn", "auf", "den", "Trab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und endlich wirds ihn gereuen.", "tokens": ["Und", "end\u00b7lich", "wirds", "ihn", "ge\u00b7reu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Da keiner ihm hilft, spricht er den Schwur:", "tokens": ["Da", "kei\u00b7ner", "ihm", "hilft", ",", "spricht", "er", "den", "Schwur", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ART", "VVFIN", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "\u00bbich will allein mir n\u00fctzen!\u00ab", "tokens": ["\u00bb", "ich", "will", "al\u00b7lein", "mir", "n\u00fct\u00b7zen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und galoppiert grimmig durch Wald und Flur,", "tokens": ["Und", "ga\u00b7lop\u00b7piert", "grim\u00b7mig", "durch", "Wald", "und", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Es spritzen Sand und Pf\u00fctzen.", "tokens": ["Es", "sprit\u00b7zen", "Sand", "und", "Pf\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und bremst erst in seinem Vaterland,", "tokens": ["Und", "bremst", "erst", "in", "sei\u00b7nem", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Grenze hielt ihn nicht auf.", "tokens": ["Die", "Gren\u00b7ze", "hielt", "ihn", "nicht", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er droht mit der Faust: \u00bbMin L\u00e4nneken deep!\u00ab", "tokens": ["Er", "droht", "mit", "der", "Faust", ":", "\u00bb", "Min", "L\u00e4n\u00b7ne\u00b7ken", "deep", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "$(", "PPOSAT", "NN", "NE", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und umklemmt seiner Klinge Knauf.", "tokens": ["Und", "um\u00b7klemmt", "sei\u00b7ner", "Klin\u00b7ge", "Knauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "S\u00f6ldner und Schnapph\u00e4hne str\u00f6men heran,", "tokens": ["S\u00f6ld\u00b7ner", "und", "Schnap\u00b7ph\u00e4h\u00b7ne", "str\u00f6\u00b7men", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Die nimmt er in Dienst und Pflichten", "tokens": ["Die", "nimmt", "er", "in", "Dienst", "und", "Pflich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und h\u00e4lt sie fest in seinem Bann.", "tokens": ["Und", "h\u00e4lt", "sie", "fest", "in", "sei\u00b7nem", "Bann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Seine Rache will Alles vernichten.", "tokens": ["Sei\u00b7ne", "Ra\u00b7che", "will", "Al\u00b7les", "ver\u00b7nich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Die M\u00fchlen brennen, die Nacht ist voll Greul,", "tokens": ["Die", "M\u00fch\u00b7len", "bren\u00b7nen", ",", "die", "Nacht", "ist", "voll", "Greul", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Voller Herdenraub, Zittern und Zeter,", "tokens": ["Vol\u00b7ler", "Her\u00b7den\u00b7raub", ",", "Zit\u00b7tern", "und", "Ze\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Und mitten drin steht im M\u00f6rderkn\u00e4ul", "tokens": ["Und", "mit\u00b7ten", "drin", "steht", "im", "M\u00f6r\u00b7der\u00b7kn\u00e4ul"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Breitbeinig im Blut Wiben Peter.", "tokens": ["Breit\u00b7bei\u00b7nig", "im", "Blut", "Wi\u00b7ben", "Pe\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "NE", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er reitet noch immer sein wei\u00dfes Pferd,", "tokens": ["Er", "rei\u00b7tet", "noch", "im\u00b7mer", "sein", "wei\u00b7\u00dfes", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Grasfarbig sind Z\u00fcgel und Zaum.", "tokens": ["Gras\u00b7far\u00b7big", "sind", "Z\u00fc\u00b7gel", "und", "Zaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "KON", "NN", "$."], "meter": "++--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mit ihm reitet sein Wappenspruch:", "tokens": ["Mit", "ihm", "rei\u00b7tet", "sein", "Wap\u00b7pen\u00b7spruch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "\u00bbund wieder gr\u00fcnt der Baum.\u00ab", "tokens": ["\u00bb", "und", "wie\u00b7der", "gr\u00fcnt", "der", "Baum", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als Helmsturz weht ihm ein knallroter Busch", "tokens": ["Als", "Helm\u00b7sturz", "weht", "ihm", "ein", "knall\u00b7ro\u00b7ter", "Busch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bis hinunter tief in den Nacken;", "tokens": ["Bis", "hin\u00b7un\u00b7ter", "tief", "in", "den", "Na\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wind, Sonne, Schatten wollen im Husch", "tokens": ["Wind", ",", "Son\u00b7ne", ",", "Schat\u00b7ten", "wol\u00b7len", "im", "Husch"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "VMFIN", "APPRART", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Ihn wie ein Wipfelblatt packen.", "tokens": ["Ihn", "wie", "ein", "Wip\u00b7fel\u00b7blatt", "pa\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Sein strohgelber Bart pilgert lang und fahl", "tokens": ["Sein", "stroh\u00b7gel\u00b7ber", "Bart", "pil\u00b7gert", "lang", "und", "fahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "KON", "NN"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "\u00dcber den eisernen Halsring in Z\u00f6pfen,", "tokens": ["\u00dc\u00b7ber", "den", "ei\u00b7ser\u00b7nen", "Hals\u00b7ring", "in", "Z\u00f6p\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.11": {"text": "Wie sich K\u00f6nig Assurannibal", "tokens": ["Wie", "sich", "K\u00f6\u00b7nig", "As\u00b7su\u00b7ran\u00b7ni\u00b7bal"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PRF", "NE", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Einst lie\u00df den Kinnbart kn\u00f6pfen.", "tokens": ["Einst", "lie\u00df", "den", "Kinn\u00b7bart", "kn\u00f6p\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Als er endlich umstellt ist, bedroht und bedr\u00e4ngt,", "tokens": ["Als", "er", "end\u00b7lich", "um\u00b7stellt", "ist", ",", "be\u00b7droht", "und", "be\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Flieht er rechtzeitig an Bord", "tokens": ["Flieht", "er", "recht\u00b7zei\u00b7tig", "an", "Bord"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Und nimmt auf dem alten Hilligenland", "tokens": ["Und", "nimmt", "auf", "dem", "al\u00b7ten", "Hil\u00b7li\u00b7gen\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "w