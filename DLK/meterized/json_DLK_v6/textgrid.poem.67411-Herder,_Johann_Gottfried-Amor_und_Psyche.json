{"textgrid.poem.67411": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Amor und Psyche", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Hand, die dieses holde Haupt ber\u00fchrt", "tokens": ["Die", "Hand", ",", "die", "die\u00b7ses", "hol\u00b7de", "Haupt", "be\u00b7r\u00fchrt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PDAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und still hinab es zum Geliebten f\u00fchrt \u2013", "tokens": ["Und", "still", "hin\u00b7ab", "es", "zum", "Ge\u00b7lieb\u00b7ten", "f\u00fchrt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der leise Hauch, der um die Lippen schwebt", "tokens": ["Der", "lei\u00b7se", "Hauch", ",", "der", "um", "die", "Lip\u00b7pen", "schwebt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sanft den Arm und sanft den Busen hebt \u2013", "tokens": ["Und", "sanft", "den", "Arm", "und", "sanft", "den", "Bu\u00b7sen", "hebt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "KON", "ADJD", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Blick, der nicht zur Sprache werden kann", "tokens": ["Der", "Blick", ",", "der", "nicht", "zur", "Spra\u00b7che", "wer\u00b7den", "kann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "APPRART", "NN", "VAINF", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "(denn Seelen schaun sich in einander an),", "tokens": ["(", "denn", "See\u00b7len", "schaun", "sich", "in", "ein\u00b7an\u00b7der", "an", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "NN", "VVFIN", "PRF", "APPR", "PRF", "PTKVZ", "$(", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Inde\u00df sich Herz zum Herzen sch\u00fcchtern dr\u00e4ngt,", "tokens": ["In\u00b7de\u00df", "sich", "Herz", "zum", "Her\u00b7zen", "sch\u00fcch\u00b7tern", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "NN", "APPRART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und Geist an Geist, an Lippe Lippe h\u00e4ngt \u2013", "tokens": ["Und", "Geist", "an", "Geist", ",", "an", "Lip\u00b7pe", "Lip\u00b7pe", "h\u00e4ngt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$,", "APPR", "NE", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der nur verlangend-s\u00fc\u00dfeste Genu\u00df", "tokens": ["Der", "nur", "ver\u00b7lan\u00b7gen\u00b7ds\u00fc\u00b7\u00dfes\u00b7te", "Ge\u00b7nu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Des Wiederfindens \u2013 seht, ist dieser Ku\u00df;", "tokens": ["Des", "Wie\u00b7der\u00b7fin\u00b7dens", "\u2013", "seht", ",", "ist", "die\u00b7ser", "Ku\u00df", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "$,", "VAFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Es schwebt in ihm des Himmels reinstes Gl\u00fcck.", "tokens": ["Es", "schwebt", "in", "ihm", "des", "Him\u00b7mels", "reins\u00b7tes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Anschauend tretet, tretet still zur\u00fcck!", "tokens": ["An\u00b7schau\u00b7end", "tre\u00b7tet", ",", "tre\u00b7tet", "still", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}