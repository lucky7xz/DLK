{"textgrid.poem.52745": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2. Revolutzlich", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ade, ihr Biederm\u00e4nner,", "tokens": ["A\u00b7de", ",", "ihr", "Bie\u00b7der\u00b7m\u00e4n\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die Blut, ein Tropfen, schreckt!", "tokens": ["Die", "Blut", ",", "ein", "Trop\u00b7fen", ",", "schreckt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Gott hat noch im J\u00e4nner", "tokens": ["Kein", "Gott", "hat", "noch", "im", "J\u00e4n\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Fr\u00fchling auferweckt.", "tokens": ["Den", "Fr\u00fch\u00b7ling", "au\u00b7fer\u00b7weckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es gilt ein Aderlassen,", "tokens": ["Es", "gilt", "ein", "A\u00b7der\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Einen Tummelplatz der Wuth,", "tokens": ["Ei\u00b7nen", "Tum\u00b7mel\u00b7platz", "der", "Wuth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Es werden alle Gassen", "tokens": ["Es", "wer\u00b7den", "al\u00b7le", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Strombette f\u00fcr das Blut!", "tokens": ["Strom\u00b7bet\u00b7te", "f\u00fcr", "das", "Blut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Heran die Guillotine,", "tokens": ["He\u00b7ran", "die", "Guil\u00b7lo\u00b7ti\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Heran das Beil des Volks;", "tokens": ["He\u00b7ran", "das", "Beil", "des", "Volks", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Heil, damit es gr\u00fcne,", "tokens": ["Dein", "Heil", ",", "da\u00b7mit", "es", "gr\u00fc\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Proletariat verfolg's!", "tokens": ["Pro\u00b7le\u00b7ta\u00b7ri\u00b7at", "ver\u00b7fol\u00b7g's", "!"], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der Strahl des V\u00f6lkerlenzes", "tokens": ["Der", "Strahl", "des", "V\u00f6l\u00b7ker\u00b7len\u00b7zes"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bricht in die Nacht herein,", "tokens": ["Bricht", "in", "die", "Nacht", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ha, ", "tokens": ["Ha", ","], "token_info": ["word", "punct"], "pos": ["ITJ", "$,"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Nachtmahl von Brod und Wein!", "tokens": ["Nacht\u00b7mahl", "von", "Brod", "und", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Der Herrscher Vielerleiheit", "tokens": ["Der", "Herr\u00b7scher", "Vie\u00b7ler\u00b7lei\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Thut nun und nimmer gut;", "tokens": ["Thut", "nun", "und", "nim\u00b7mer", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohlan! die Braut hei\u00dft Freiheit;", "tokens": ["Wo\u00b7hlan", "!", "die", "Braut", "hei\u00dft", "Frei\u00b7heit", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Br\u00e4utigam hei\u00dft Blut.", "tokens": ["Der", "Br\u00e4u\u00b7ti\u00b7gam", "hei\u00dft", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und Priester sind die Henker", "tokens": ["Und", "Pries\u00b7ter", "sind", "die", "Hen\u00b7ker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Altar das Schaffot,", "tokens": ["Und", "Al\u00b7tar", "das", "Schaf\u00b7fot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Jahrhundert du der Denker", "tokens": ["Jahr\u00b7hun\u00b7dert", "du", "der", "Den\u00b7ker"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Begrabe deinen Gott!", "tokens": ["Be\u00b7gra\u00b7be", "dei\u00b7nen", "Gott", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Den Samen der Betr\u00fcger", "tokens": ["Den", "Sa\u00b7men", "der", "Be\u00b7tr\u00fc\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verweht das Sturmgebraus \u2013", "tokens": ["Ver\u00b7weht", "das", "Sturm\u00b7ge\u00b7braus", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr aber, neue Pfl\u00fcger,", "tokens": ["Ihr", "a\u00b7ber", ",", "neu\u00b7e", "Pfl\u00fc\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Streut andern Samen aus.", "tokens": ["Streut", "an\u00b7dern", "Sa\u00b7men", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr w\u00fchlt mit freiem Pfluge", "tokens": ["Ihr", "w\u00fchlt", "mit", "frei\u00b7em", "Pflu\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und mit dem Ro\u00df der Wuth,", "tokens": ["Und", "mit", "dem", "Ro\u00df", "der", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und Euer Arnold Ruge", "tokens": ["Und", "Eu\u00b7er", "Ar\u00b7nold", "Ru\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Jahrb\u00fccher schreibt mit Blut.", "tokens": ["Jahr\u00b7b\u00fc\u00b7cher", "schreibt", "mit", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}