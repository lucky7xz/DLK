{"textgrid.poem.67915": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "10. Lied an die Gesundheit", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gesundheit, Himmelskind!", "tokens": ["Ge\u00b7sund\u00b7heit", ",", "Him\u00b7mel\u00b7skind", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der besten Gaben Quelle du,", "tokens": ["Der", "bes\u00b7ten", "Ga\u00b7ben", "Quel\u00b7le", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus der uns Segen, Lust und Ruh", "tokens": ["Aus", "der", "uns", "Se\u00b7gen", ",", "Lust", "und", "Ruh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In s\u00fcssen Str\u00f6men rinnt.", "tokens": ["In", "s\u00fcs\u00b7sen", "Str\u00f6\u00b7men", "rinnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Womit erz\u00fcrnt' ich dich?", "tokens": ["Wo\u00b7mit", "er\u00b7z\u00fcrnt'", "ich", "dich", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df du die kleine H\u00fctte fliehst,", "tokens": ["Da\u00df", "du", "die", "klei\u00b7ne", "H\u00fct\u00b7te", "fliehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Alles dich so gern genie\u00dft", "tokens": ["Wo", "Al\u00b7les", "dich", "so", "gern", "ge\u00b7nie\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PRF", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und athmet dankbarlich.", "tokens": ["Und", "ath\u00b7met", "dank\u00b7bar\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Seit du von mir entflohn,", "tokens": ["Seit", "du", "von", "mir", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ist Leben und Vergn\u00fcgen hin,", "tokens": ["Ist", "Le\u00b7ben", "und", "Ver\u00b7gn\u00fc\u00b7gen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und keine Pflanze will mir bl\u00fchn", "tokens": ["Und", "kei\u00b7ne", "Pflan\u00b7ze", "will", "mir", "bl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ich verwelke schon \u2013", "tokens": ["Und", "ich", "ver\u00b7wel\u00b7ke", "schon", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "In bester Jugend Gr\u00fcn.", "tokens": ["In", "bes\u00b7ter", "Ju\u00b7gend", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du solltest noch mir Freundin seyn,", "tokens": ["Du", "soll\u00b7test", "noch", "mir", "Freun\u00b7din", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Lebensfr\u00fcchten mich erfreun", "tokens": ["Mit", "Le\u00b7bens\u00b7fr\u00fcch\u00b7ten", "mich", "er\u00b7freun"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine Bl\u00fcthen fliehn.", "tokens": ["Und", "mei\u00b7ne", "Bl\u00fc\u00b7then", "fliehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du liebst das freie Land,", "tokens": ["Du", "liebst", "das", "frei\u00b7e", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich suche dich durch Thal und H\u00f6hn", "tokens": ["Ich", "su\u00b7che", "dich", "durch", "Thal", "und", "H\u00f6hn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dich zu erathmen, dich zu sehn,", "tokens": ["Dich", "zu", "e\u00b7rath\u00b7men", ",", "dich", "zu", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohin? wohin? gewandt.", "tokens": ["Wo\u00b7hin", "?", "wo\u00b7hin", "?", "ge\u00b7wandt", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "$.", "PWAV", "$.", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich tauch' ins kalte Meer", "tokens": ["Ich", "tauch'", "ins", "kal\u00b7te", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und trinke Quellen, wo dein Bild", "tokens": ["Und", "trin\u00b7ke", "Quel\u00b7len", ",", "wo", "dein", "Bild"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "$,", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In jeder Well' und Woge quillt,", "tokens": ["In", "je\u00b7der", "Well'", "und", "Wo\u00b7ge", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und d\u00fcrste lechzender.", "tokens": ["Und", "d\u00fcrs\u00b7te", "lech\u00b7zen\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ach, als ich dich geno\u00df,", "tokens": ["Ach", ",", "als", "ich", "dich", "ge\u00b7no\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie war mir jeder Morgen neu,", "tokens": ["Wie", "war", "mir", "je\u00b7der", "Mor\u00b7gen", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie athmet' ich so frisch und frei", "tokens": ["Wie", "ath\u00b7met'", "ich", "so", "frisch", "und", "frei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In deiner G\u00fcte Schoos.", "tokens": ["In", "dei\u00b7ner", "G\u00fc\u00b7te", "Schoos", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wo bist du, seelge Zeit?", "tokens": ["Wo", "bist", "du", ",", "seel\u00b7ge", "Zeit", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was fand ich denn auf aller Welt,", "tokens": ["Was", "fand", "ich", "denn", "auf", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das mich um dich entschadet h\u00e4lt,", "tokens": ["Das", "mich", "um", "dich", "ent\u00b7scha\u00b7det", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Lebens Fr\u00f6hlichkeit.", "tokens": ["O", "Le\u00b7bens", "Fr\u00f6h\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O k\u00e4mst du wieder mir,", "tokens": ["O", "k\u00e4mst", "du", "wie\u00b7der", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und schl\u00fcge wieder frisch mein Herz,", "tokens": ["Und", "schl\u00fc\u00b7ge", "wie\u00b7der", "frisch", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lachte Gl\u00fccks und Ruhmes Scherz", "tokens": ["Ich", "lach\u00b7te", "Gl\u00fccks", "und", "Ruh\u00b7mes", "Scherz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und diente, diente dir.", "tokens": ["Und", "dien\u00b7te", ",", "dien\u00b7te", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Auf fr\u00fchem Thaualtar,", "tokens": ["Auf", "fr\u00fc\u00b7hem", "Thau\u00b7al\u00b7tar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Br\u00e4cht' ich, mit emsig reiner Hand,", "tokens": ["Br\u00e4cht'", "ich", ",", "mit", "em\u00b7sig", "rei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir t\u00e4glich meines Herzens Pfand,", "tokens": ["Dir", "t\u00e4g\u00b7lich", "mei\u00b7nes", "Her\u00b7zens", "Pfand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gebet und Liebe dar.", "tokens": ["Ge\u00b7bet", "und", "Lie\u00b7be", "dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und Flei\u00df und M\u00e4\u00dfigkeit", "tokens": ["Und", "Flei\u00df", "und", "M\u00e4\u00b7\u00dfig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sollt' hie und da am Altar stehn,", "tokens": ["Sollt'", "hie", "und", "da", "am", "Al\u00b7tar", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "KON", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Unschuld mir zur Seite gehn,", "tokens": ["Und", "Un\u00b7schuld", "mir", "zur", "Sei\u00b7te", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die frohe Lebenszeit.", "tokens": ["Die", "fro\u00b7he", "Le\u00b7bens\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}