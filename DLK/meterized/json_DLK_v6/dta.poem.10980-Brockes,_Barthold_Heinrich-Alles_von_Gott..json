{"dta.poem.10980": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Alles von Gott.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich des Sch\u00f6pfers Huld erwege,", "tokens": ["Wenn", "ich", "des", "Sch\u00f6p\u00b7fers", "Huld", "er\u00b7we\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie viel er Gutes zu mir lenkt,", "tokens": ["Wie", "viel", "er", "Gu\u00b7tes", "zu", "mir", "lenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alles, was er mir geschenkt,", "tokens": ["Und", "al\u00b7les", ",", "was", "er", "mir", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bedachtsam bey mir \u00fcberlege:", "tokens": ["Be\u00b7dacht\u00b7sam", "bey", "mir", "\u00fc\u00b7berl\u00b7e\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So scheints, ich m\u00fc\u00dfte mich fast sch\u00e4men,", "tokens": ["So", "scheints", ",", "ich", "m\u00fc\u00df\u00b7te", "mich", "fast", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So gar viel Gutes hinzunehmen,", "tokens": ["So", "gar", "viel", "Gu\u00b7tes", "hin\u00b7zu\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da ich von dem, was mir beschehrt,", "tokens": ["Da", "ich", "von", "dem", ",", "was", "mir", "be\u00b7schehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des allerwenigsten kaum werth.", "tokens": ["Des", "al\u00b7ler\u00b7we\u00b7nigs\u00b7ten", "kaum", "werth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "ADJD", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.9": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Es f\u00e4llt mir dabey ein:", "tokens": ["Es", "f\u00e4llt", "mir", "da\u00b7bey", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Mein Geist, was willt du dich erk\u00fchnen?", "tokens": ["Mein", "Geist", ",", "was", "willt", "du", "dich", "er\u00b7k\u00fch\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Willt du denn Gott was abverdienen?", "tokens": ["Willt", "du", "denn", "Gott", "was", "ab\u00b7ver\u00b7die\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Soll etwan, was er schenkt, ein Lohn, von deinen guten Wer-", "tokens": ["Soll", "et\u00b7wan", ",", "was", "er", "schenkt", ",", "ein", "Lohn", ",", "von", "dei\u00b7nen", "gu\u00b7ten", "Wer"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,", "ART", "NN", "$,", "APPR", "PPOSAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Bey dieser Meynung ist dein schw\u00e4rmender Verstand", "tokens": ["Bey", "die\u00b7ser", "Mey\u00b7nung", "ist", "dein", "schw\u00e4r\u00b7men\u00b7der", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mit einem Jrrthum stark beladen.", "tokens": ["Mit", "ei\u00b7nem", "Jrr\u00b7thum", "stark", "be\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Es wird die Gottheit, blo\u00df aus Gnaden,", "tokens": ["Es", "wird", "die", "Got\u00b7theit", ",", "blo\u00df", "aus", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Der Geber aus der Gab erkannt.", "tokens": ["Der", "Ge\u00b7ber", "aus", "der", "Gab", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ach la\u00df mich denn, mit Lust und Freuden,", "tokens": ["Ach", "la\u00df", "mich", "denn", ",", "mit", "Lust", "und", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "PPER", "ADV", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Den Geist an deinen Gaben weiden;", "tokens": ["Den", "Geist", "an", "dei\u00b7nen", "Ga\u00b7ben", "wei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Denn auch das Wollen steht nicht einst in meiner Macht;", "tokens": ["Denn", "auch", "das", "Wol\u00b7len", "steht", "nicht", "einst", "in", "mei\u00b7ner", "Macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und la\u00df, wenn sich darauf so Geist als Sinnen lenken,", "tokens": ["Und", "la\u00df", ",", "wenn", "sich", "da\u00b7rauf", "so", "Geist", "als", "Sin\u00b7nen", "len\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "KOUS", "PRF", "PAV", "ADV", "NN", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mich oft und stets an dich gedenken!", "tokens": ["Mich", "oft", "und", "stets", "an", "dich", "ge\u00b7den\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}