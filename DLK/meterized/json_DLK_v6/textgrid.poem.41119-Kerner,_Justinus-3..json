{"textgrid.poem.41119": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O da\u00df du wardst hinweggenommen,", "tokens": ["O", "da\u00df", "du", "wardst", "hin\u00b7weg\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu kehren nimmermehr zur\u00fcck!", "tokens": ["Zu", "keh\u00b7ren", "nim\u00b7mer\u00b7mehr", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von einem Meere fortgeschwommen", "tokens": ["Von", "ei\u00b7nem", "Mee\u00b7re", "fort\u00b7ge\u00b7schwom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist seitdem all mein, all mein Gl\u00fcck!", "tokens": ["Ist", "seit\u00b7dem", "all", "mein", ",", "all", "mein", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "PIAT", "PPOSAT", "$,", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vergebens blick' ich nach dem fernen", "tokens": ["Ver\u00b7ge\u00b7bens", "blick'", "ich", "nach", "dem", "fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Azurnen Himmel, rufend dir.", "tokens": ["A\u00b7zur\u00b7nen", "Him\u00b7mel", ",", "ru\u00b7fend", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Antwortete von all den Sternen", "tokens": ["Ant\u00b7wor\u00b7te\u00b7te", "von", "all", "den", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PIAT", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Doch einer nur: \u00bbEr ist bei mir!\u00ab", "tokens": ["Doch", "ei\u00b7ner", "nur", ":", "\u00bb", "Er", "ist", "bei", "mir", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADV", "$.", "$(", "PPER", "VAFIN", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O spr\u00e4ch's nur aus den regen Winden,", "tokens": ["O", "spr\u00e4ch's", "nur", "aus", "den", "re\u00b7gen", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O spr\u00e4ch's nur aus dem Baum, dem Kraut:", "tokens": ["O", "spr\u00e4ch's", "nur", "aus", "dem", "Baum", ",", "dem", "Kraut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bber ist in uns!\u00ab wollt's \u00fcberwinden!", "tokens": ["\u00bb", "er", "ist", "in", "uns", "!", "\u00ab", "wollt's", "\u00fc\u00b7berw\u00b7in\u00b7den", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "PPER", "$.", "$(", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ach! von ihm kein Wort, kein Laut!", "tokens": ["Doch", "ach", "!", "von", "ihm", "kein", "Wort", ",", "kein", "Laut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "APPR", "PPER", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die V\u00f6gel singen bunte Weisen,", "tokens": ["Die", "V\u00f6\u00b7gel", "sin\u00b7gen", "bun\u00b7te", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Quellen murmeln Lust und Gram,", "tokens": ["Die", "Quel\u00b7len", "mur\u00b7meln", "Lust", "und", "Gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sprechen. O ich wollt' sie preisen,", "tokens": ["Sie", "spre\u00b7chen", ".", "O", "ich", "wollt'", "sie", "prei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "PPER", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verrieten sie, wohin er kam!", "tokens": ["Ver\u00b7rie\u00b7ten", "sie", ",", "wo\u00b7hin", "er", "kam", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Stern' erklingen, Winde tragen", "tokens": ["Die", "Stern'", "er\u00b7klin\u00b7gen", ",", "Win\u00b7de", "tra\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So manchen Laut durch Wald und Flur,", "tokens": ["So", "man\u00b7chen", "Laut", "durch", "Wald", "und", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "APPR", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch will man nach den Toten fragen,", "tokens": ["Doch", "will", "man", "nach", "den", "To\u00b7ten", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gibt keine Antwort die Natur.", "tokens": ["Gibt", "kei\u00b7ne", "Ant\u00b7wort", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}