{"textgrid.poem.38946": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den Teufel kennt fast Niemand", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Teufel kennt fast Niemand", "tokens": ["Den", "Teu\u00b7fel", "kennt", "fast", "Nie\u00b7mand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und w\u00e4r' er noch so dick;", "tokens": ["Und", "w\u00e4r'", "er", "noch", "so", "dick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Auge sieht nicht die Hand", "tokens": ["Das", "Au\u00b7ge", "sieht", "nicht", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und das ist gro\u00dfes Gl\u00fcck.", "tokens": ["Und", "das", "ist", "gro\u00b7\u00dfes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sonst lebte sich's so sicher nicht", "tokens": ["Sonst", "leb\u00b7te", "sich's", "so", "si\u00b7cher", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADJD", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am Tageslicht, am Tageslicht.", "tokens": ["Am", "Ta\u00b7ges\u00b7licht", ",", "am", "Ta\u00b7ges\u00b7licht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Tugend kennt ein Jeder", "tokens": ["Die", "Tu\u00b7gend", "kennt", "ein", "Je\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und w\u00e4r' sie unsichtbar;", "tokens": ["Und", "w\u00e4r'", "sie", "un\u00b7sicht\u00b7bar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es sucht sie keiner, weder", "tokens": ["Es", "sucht", "sie", "kei\u00b7ner", ",", "we\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$,", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei blond' noch greisem Haare.", "tokens": ["Bei", "blond'", "noch", "grei\u00b7sem", "Haa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum lebt ein jeder so in Ruh", "tokens": ["Drum", "lebt", "ein", "je\u00b7der", "so", "in", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "PIS", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Frisch immer zu, frisch immer zu.", "tokens": ["Frisch", "im\u00b7mer", "zu", ",", "frisch", "im\u00b7mer", "zu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "$,", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}