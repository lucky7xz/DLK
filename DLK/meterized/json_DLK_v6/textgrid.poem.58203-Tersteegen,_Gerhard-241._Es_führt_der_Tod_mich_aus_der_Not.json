{"textgrid.poem.58203": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "241. Es f\u00fchrt der Tod mich aus der Not", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gedenk' ich an den Stand, da man nicht s\u00fcndigt mehr,", "tokens": ["Ge\u00b7denk'", "ich", "an", "den", "Stand", ",", "da", "man", "nicht", "s\u00fcn\u00b7digt", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,", "KOUS", "PIS", "PTKNEG", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da keine Eigenheit die Ruh in Gott kann st\u00f6ren,", "tokens": ["Da", "kei\u00b7ne", "Ei\u00b7gen\u00b7heit", "die", "Ruh", "in", "Gott", "kann", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ART", "NN", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da man Gott reine liebt und gibt vollkommen Ehr',", "tokens": ["Da", "man", "Gott", "rei\u00b7ne", "liebt", "und", "gibt", "voll\u00b7kom\u00b7men", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "ADJA", "VVFIN", "KON", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So w\u00fcnsch' ich mir den Tod, um da hinein zu kehren.", "tokens": ["So", "w\u00fcn\u00b7sch'", "ich", "mir", "den", "Tod", ",", "um", "da", "hin\u00b7ein", "zu", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,", "KOUI", "ADV", "APZR", "PTKZU", "VVINF", "$."], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}}}}}