{"textgrid.poem.38359": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "S s Schwalbe", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schw\u00e4zzerlein wie schw\u00e4tzst so toll, und plauderst hin und her,", "tokens": ["Schw\u00e4z\u00b7zer\u00b7lein", "wie", "schw\u00e4tz\u00b7st", "so", "toll", ",", "und", "plau\u00b7derst", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VVFIN", "ADV", "ADJD", "$,", "KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Fr\u00fch hast du Kisten und Kasten voll, Abends ist alles le le leer,", "tokens": ["Fr\u00fch", "hast", "du", "Kis\u00b7ten", "und", "Kas\u00b7ten", "voll", ",", "A\u00b7bends", "ist", "al\u00b7les", "le", "le", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "NN", "KON", "NN", "ADJD", "$,", "ADV", "VAFIN", "PIS", "FM", "FM", "FM", "$,"], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.3": {"text": "Zu morgen eh die Sonn aufsteht, erz\u00e4hlst du deinen Traum,", "tokens": ["Zu", "mor\u00b7gen", "eh", "die", "Sonn", "auf\u00b7steht", ",", "er\u00b7z\u00e4hlst", "du", "dei\u00b7nen", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KOUS", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Und Abends wenn sie niedergeht, hast du geendet kaum.", "tokens": ["Und", "A\u00b7bends", "wenn", "sie", "nie\u00b7der\u00b7geht", ",", "hast", "du", "ge\u00b7en\u00b7det", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "VVPP", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}