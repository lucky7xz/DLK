{"textgrid.poem.25153": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "[wenn Furcht mich fa\u00dft, mein Dasein k\u00f6nne enden]", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Furcht mich fa\u00dft, mein Dasein k\u00f6nne enden,", "tokens": ["Wenn", "Furcht", "mich", "fa\u00dft", ",", "mein", "Da\u00b7sein", "k\u00f6n\u00b7ne", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch eh' die Feder, was mein Hirn erdachte,", "tokens": ["Noch", "eh'", "die", "Fe\u00b7der", ",", "was", "mein", "Hirn", "er\u00b7dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Schrift, in B\u00fcchern wu\u00dfte zu vollenden,", "tokens": ["In", "Schrift", ",", "in", "B\u00fc\u00b7chern", "wu\u00df\u00b7te", "zu", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das reife Korn in volle Speicher brachte \u2013", "tokens": ["Das", "rei\u00b7fe", "Korn", "in", "vol\u00b7le", "Spei\u00b7cher", "brach\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn wolkengleich tief seltsame Legenden", "tokens": ["Wenn", "wol\u00b7ken\u00b7gleich", "tief", "selt\u00b7sa\u00b7me", "Le\u00b7gen\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+--+-+-+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Nacht besterntes Antlitz \u00fcberflie\u00dfen,", "tokens": ["Der", "Nacht", "bes\u00b7tern\u00b7tes", "Ant\u00b7litz", "\u00fc\u00b7berf\u00b7lie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ich es wei\u00df, da\u00df nie mit Zufallsh\u00e4nden", "tokens": ["Und", "ich", "es", "wei\u00df", ",", "da\u00df", "nie", "mit", "Zu\u00b7falls\u00b7h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PPER", "VVFIN", "$,", "KOUS", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Gl\u00fcck mir hilft, ihr Bild in Form zu gie\u00dfen \u2013", "tokens": ["Das", "Gl\u00fcck", "mir", "hilft", ",", "ihr", "Bild", "in", "Form", "zu", "gie\u00b7\u00dfen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wenn ich f\u00fchle, Sch\u00f6nste du von allen,", "tokens": ["Und", "wenn", "ich", "f\u00fch\u00b7le", ",", "Sch\u00f6ns\u00b7te", "du", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df nur die fl\u00fcchtige Stunde uns umf\u00e4ngt,", "tokens": ["Da\u00df", "nur", "die", "fl\u00fcch\u00b7ti\u00b7ge", "Stun\u00b7de", "uns", "um\u00b7f\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df nie mein Herz in jenen Zauberfallen", "tokens": ["Da\u00df", "nie", "mein", "Herz", "in", "je\u00b7nen", "Zau\u00b7ber\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gedankenloser Liebe tr\u00e4umend h\u00e4ngt \u2013", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7lo\u00b7ser", "Lie\u00b7be", "tr\u00e4u\u00b7mend", "h\u00e4ngt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dann steh ich einsam vor den Ewigkeiten,", "tokens": ["Dann", "steh", "ich", "ein\u00b7sam", "vor", "den", "E\u00b7wig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis Ruhm und Liebe in ein Nichts entgleiten.", "tokens": ["Bis", "Ruhm", "und", "Lie\u00b7be", "in", "ein", "Nichts", "ent\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}