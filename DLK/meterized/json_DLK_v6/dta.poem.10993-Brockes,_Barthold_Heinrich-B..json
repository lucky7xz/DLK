{"dta.poem.10993": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "B.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wofern man eine Sch\u00fcssel Fr\u00fcchte, um deine Zunge zu", "tokens": ["Wo\u00b7fern", "man", "ei\u00b7ne", "Sch\u00fcs\u00b7sel", "Fr\u00fcch\u00b7te", ",", "um", "dei\u00b7ne", "Zun\u00b7ge", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "NN", "$,", "KOUI", "PPOSAT", "NN", "PTKZU"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Auf etwa einer Viertelstund, dir lie\u00df auf deine Tafel setzen:", "tokens": ["Auf", "et\u00b7wa", "ei\u00b7ner", "Vier\u00b7tel\u00b7stund", ",", "dir", "lie\u00df", "auf", "dei\u00b7ne", "Ta\u00b7fel", "set\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.3": {"text": "So weis ich nicht, ob du derselben, dich wegern w\u00fcrdest, zu", "tokens": ["So", "weis", "ich", "nicht", ",", "ob", "du", "der\u00b7sel\u00b7ben", ",", "dich", "we\u00b7gern", "w\u00fcr\u00b7dest", ",", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PTKVZ", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "PDAT", "$,", "PPER", "VVFIN", "VAFIN", "$,", "APPR"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Blo\u00df darum, weil die Diener etwan sie, auf dem Tisch,", "tokens": ["Blo\u00df", "da\u00b7rum", ",", "weil", "die", "Die\u00b7ner", "et\u00b7wan", "sie", ",", "auf", "dem", "Tisch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "$,", "KOUS", "ART", "NN", "ADV", "PPER", "$,", "APPR", "ART", "NN", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Du w\u00fcrdest dich, so viel ich denke, vermuthlich desto eh be-", "tokens": ["Du", "w\u00fcr\u00b7dest", "dich", ",", "so", "viel", "ich", "den\u00b7ke", ",", "ver\u00b7muth\u00b7lich", "des\u00b7to", "eh", "be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "ADV", "ADV", "PPER", "VVFIN", "$,", "ADV", "ADV", "KOUS", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Dieselben desto eh zu nehmen.", "tokens": ["Die\u00b7sel\u00b7ben", "des\u00b7to", "eh", "zu", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADV", "KOUS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So la\u00df auch einen sch\u00f6nen Tag, ohn ihn, im Denken, zu", "tokens": ["So", "la\u00df", "auch", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Tag", ",", "ohn", "ihn", ",", "im", "Den\u00b7ken", ",", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVIMP", "ADV", "ART", "ADJA", "NN", "$,", "KOUI", "PPER", "$,", "APPRART", "NN", "$,", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "An den, der dir denselben schenkt, doch ungepr\u00fcfet nicht", "tokens": ["An", "den", ",", "der", "dir", "den\u00b7sel\u00b7ben", "schenkt", ",", "doch", "un\u00b7ge\u00b7pr\u00fc\u00b7fet", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "PDS", "VVFIN", "$,", "ADV", "ADJD", "PTKNEG"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}