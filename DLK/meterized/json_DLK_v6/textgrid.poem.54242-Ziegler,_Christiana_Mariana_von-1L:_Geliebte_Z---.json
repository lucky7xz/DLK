{"textgrid.poem.54242": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geliebte Z---", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geliebte Z---", "tokens": ["Ge\u00b7lieb\u00b7te", "Z\u00b7"], "token_info": ["word", "word"], "pos": ["NN", "TRUNC"], "meter": "meter.parsing.error", "measure": "measure.parsing.error"}, "line.2": {"text": "Du l\u00e4st mich wunderns-voll", "tokens": ["Du", "l\u00e4st", "mich", "wun\u00b7derns\u00b7voll"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den ungemeinen Geist in deinen Brief erblicken;", "tokens": ["Den", "un\u00b7ge\u00b7mei\u00b7nen", "Geist", "in", "dei\u00b7nen", "Brief", "er\u00b7bli\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum sag ich, da\u00df man jetzt nicht mehr, wie vormahls, soll", "tokens": ["Drum", "sag", "ich", ",", "da\u00df", "man", "jetzt", "nicht", "mehr", ",", "wie", "vor\u00b7mahls", ",", "soll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KOUS", "PIS", "ADV", "PTKNEG", "ADV", "$,", "PWAV", "ADV", "$,", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zum Zeichen unsrer Schuld den G\u00f6ttern Opffer schicken.", "tokens": ["Zum", "Zei\u00b7chen", "uns\u00b7rer", "Schuld", "den", "G\u00f6t\u00b7tern", "Opf\u00b7fer", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jedoch was sag ich hier? Du selbst bist in der Zahl,", "tokens": ["Je\u00b7doch", "was", "sag", "ich", "hier", "?", "Du", "selbst", "bist", "in", "der", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "ADV", "$.", "PPER", "ADV", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Derjenigen, die sich mit Nahmen Musen nennen;", "tokens": ["Der\u00b7je\u00b7ni\u00b7gen", ",", "die", "sich", "mit", "Nah\u00b7men", "Mu\u00b7sen", "nen\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PRF", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Man lieset sonsten wohl von neunen \u00fcberall,", "tokens": ["Man", "lie\u00b7set", "sons\u00b7ten", "wohl", "von", "neu\u00b7nen", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dir aber mu\u00df man jetzt die zehnte Stelle g\u00f6nnen.", "tokens": ["Dir", "a\u00b7ber", "mu\u00df", "man", "jetzt", "die", "zehn\u00b7te", "Stel\u00b7le", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PIS", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Drum wundert michs nicht mehr, wie es kan m\u00f6glich seyn,", "tokens": ["Drum", "wun\u00b7dert", "michs", "nicht", "mehr", ",", "wie", "es", "kan", "m\u00f6g\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "PTKNEG", "ADV", "$,", "PWAV", "PPER", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df dir die Verse stets so rein und deutlich fliessen,", "tokens": ["Da\u00df", "dir", "die", "Ver\u00b7se", "stets", "so", "rein", "und", "deut\u00b7lich", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADV", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da ich die Ursach wei\u00df, f\u00e4llt mir di\u00df nimmer ein,", "tokens": ["Da", "ich", "die", "Ur\u00b7sach", "wei\u00df", ",", "f\u00e4llt", "mir", "di\u00df", "nim\u00b7mer", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PDS", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sie kan der G\u00f6tter Huld und Umgang nicht geniessen.", "tokens": ["Sie", "kan", "der", "G\u00f6t\u00b7ter", "Huld", "und", "Um\u00b7gang", "nicht", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "KON", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Trifft man bey ihnen denn noch was von Fehlern an?", "tokens": ["Trifft", "man", "bey", "ih\u00b7nen", "denn", "noch", "was", "von", "Feh\u00b7lern", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PPER", "ADV", "ADV", "PWS", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Kan man bey selbigen noch was unm\u00f6glichs finden?", "tokens": ["Kan", "man", "bey", "sel\u00b7bi\u00b7gen", "noch", "was", "un\u00b7m\u00f6g\u00b7lichs", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "ADJA", "KON", "PWS", "PIS", "VVINF", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Da die von Z-- bey ihnen sitzen kan,", "tokens": ["Da", "die", "von", "Z\u00b7", "bey", "ih\u00b7nen", "sit\u00b7zen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "TRUNC", "APPR", "PPER", "VVINF", "VMFIN", "$,"], "meter": "meter.parsing.error", "measure": "measure.parsing.error"}, "line.17": {"text": "So darff sie wohl gewi\u00df die Ohnmacht nicht mehr binden,", "tokens": ["So", "darff", "sie", "wohl", "ge\u00b7wi\u00df", "die", "Ohn\u00b7macht", "nicht", "mehr", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Zwar aller Titul Pracht, den du mir beygelegt,", "tokens": ["Zwar", "al\u00b7ler", "Ti\u00b7tul", "Pracht", ",", "den", "du", "mir", "bey\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Gilt gleich so viel bey mir, wie ich im G\u00f6tter-Orden;", "tokens": ["Gilt", "gleich", "so", "viel", "bey", "mir", ",", "wie", "ich", "im", "G\u00f6t\u00b7ter\u00b7Or\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "APPR", "PPER", "$,", "PWAV", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Du hast mir wohl dadurch gar schlechten Wahn erregt,", "tokens": ["Du", "hast", "mir", "wohl", "da\u00b7durch", "gar", "schlech\u00b7ten", "Wahn", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PAV", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da\u00df ich die Sonne sey bey euch, ihr Sterne, worden.", "tokens": ["Da\u00df", "ich", "die", "Son\u00b7ne", "sey", "bey", "euch", ",", "ihr", "Ster\u00b7ne", ",", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sonst ist der G\u00f6tter-Spruch vor allen andern wahr,", "tokens": ["Sonst", "ist", "der", "G\u00f6t\u00b7ter\u00b7Spruch", "vor", "al\u00b7len", "an\u00b7dern", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PIAT", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und das, was sie gesagt, mu\u00df unbetr\u00fcglich bleiben;", "tokens": ["Und", "das", ",", "was", "sie", "ge\u00b7sagt", ",", "mu\u00df", "un\u00b7be\u00b7tr\u00fcg\u00b7lich", "blei\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "PPER", "VVPP", "$,", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Allein verzeihe mir, es giebt gewi\u00df Gefahr,", "tokens": ["Al\u00b7lein", "ver\u00b7zei\u00b7he", "mir", ",", "es", "giebt", "ge\u00b7wi\u00df", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wann ich die Wahrheit will bey dir aufs h\u00f6chste treiben.", "tokens": ["Wann", "ich", "die", "Wahr\u00b7heit", "will", "bey", "dir", "aufs", "h\u00f6chs\u00b7te", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VMFIN", "APPR", "PPER", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Jedoch ich wei\u00df es wohl, warum du es gethan,", "tokens": ["Je\u00b7doch", "ich", "wei\u00df", "es", "wohl", ",", "wa\u00b7rum", "du", "es", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da\u00df du mir hast so viel von Ruhm und Lob gegeben,", "tokens": ["Da\u00df", "du", "mir", "hast", "so", "viel", "von", "Ruhm", "und", "Lob", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "ADV", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Weil dein gelehrter Kiel so zierlich schreiben kan,", "tokens": ["Weil", "dein", "ge\u00b7lehr\u00b7ter", "Kiel", "so", "zier\u00b7lich", "schrei\u00b7ben", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So soll dich Schlesien bi\u00df an den Pol erheben.", "tokens": ["So", "soll", "dich", "Schle\u00b7si\u00b7en", "bi\u00df", "an", "den", "Pol", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die\u00df ist dir nicht genung, da\u00df zwar in Sachsen-Land", "tokens": ["Die\u00df", "ist", "dir", "nicht", "ge\u00b7nung", ",", "da\u00df", "zwar", "in", "Sach\u00b7sen\u00b7Land"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "ADV", "$,", "KOUS", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ein jeder der dich kennt, von deiner Klugheit zeiget,", "tokens": ["Ein", "je\u00b7der", "der", "dich", "kennt", ",", "von", "dei\u00b7ner", "Klug\u00b7heit", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Du machst zum Uberflu\u00df dich auch bey uns bekandt,", "tokens": ["Du", "machst", "zum", "U\u00b7ber\u00b7flu\u00df", "dich", "auch", "bey", "uns", "be\u00b7kandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "ADV", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Da deiner Muse Thon nicht ihre Kunst verschweiget.", "tokens": ["Da", "dei\u00b7ner", "Mu\u00b7se", "Thon", "nicht", "ih\u00b7re", "Kunst", "ver\u00b7schwei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ich habe, glaub es nur, schon offtmahls dran gedacht,", "tokens": ["Ich", "ha\u00b7be", ",", "glaub", "es", "nur", ",", "schon", "offt\u00b7mahls", "dran", "ge\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wie kommt es, da\u00df sie doch noch kein Poet besinget?", "tokens": ["Wie", "kommt", "es", ",", "da\u00df", "sie", "doch", "noch", "kein", "Po\u00b7et", "be\u00b7sin\u00b7get", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wie kommts, da\u00df sich niemand an ihre Lieder macht", "tokens": ["Wie", "kommts", ",", "da\u00df", "sich", "nie\u00b7mand", "an", "ih\u00b7re", "Lie\u00b7der", "macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "$,", "KOUS", "PRF", "PIS", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Und seiner Sayten-Schall hier in die H\u00f6he zwinget?", "tokens": ["Und", "sei\u00b7ner", "Say\u00b7ten\u00b7Schall", "hier", "in", "die", "H\u00f6\u00b7he", "zwin\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Es macht sich mancher breit mit seiner Dichterey,", "tokens": ["Es", "macht", "sich", "man\u00b7cher", "breit", "mit", "sei\u00b7ner", "Dich\u00b7te\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIAT", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Er giebt sich grosse M\u00fch dieselbe raus zu streichen,", "tokens": ["Er", "giebt", "sich", "gros\u00b7se", "M\u00fch", "die\u00b7sel\u00b7be", "raus", "zu", "strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJA", "NN", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Von dem die Wahrheit spricht, da\u00df ers nicht w\u00fcrdig sey,", "tokens": ["Von", "dem", "die", "Wahr\u00b7heit", "spricht", ",", "da\u00df", "ers", "nicht", "w\u00fcr\u00b7dig", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$,", "KOUS", "PIS", "PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Da\u00df er der Z-- mu\u00df, wie Gra\u00df den Cedern weichen.", "tokens": ["Da\u00df", "er", "der", "Z\u00b7", "mu\u00df", ",", "wie", "Gra\u00df", "den", "Ce\u00b7dern", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "TRUNC", "VMFIN", "$,", "PWAV", "NN", "ART", "NN", "VVINF", "$."], "meter": "meter.parsing.error", "measure": "measure.parsing.error"}, "line.42": {"text": "Ja h\u00e4tte mir der Fall nicht allen Sinn und Geist,", "tokens": ["Ja", "h\u00e4t\u00b7te", "mir", "der", "Fall", "nicht", "al\u00b7len", "Sinn", "und", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Samt der Empfindungs Krafft, gantz auf einmahl geschlagen,", "tokens": ["Samt", "der", "Emp\u00b7fin\u00b7dungs", "Krafft", ",", "gantz", "auf", "ein\u00b7mahl", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,", "ADV", "APPR", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Da\u00df mich der lange Schmertz noch in den Adern reist,", "tokens": ["Da\u00df", "mich", "der", "lan\u00b7ge", "Schmertz", "noch", "in", "den", "A\u00b7dern", "reist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "So wolt ich selber Holtz zu deinem Herde tragen.", "tokens": ["So", "wolt", "ich", "sel\u00b7ber", "Holtz", "zu", "dei\u00b7nem", "Her\u00b7de", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Da aber jetzt mein Thon in etwas heischer klingt,", "tokens": ["Da", "a\u00b7ber", "jetzt", "mein", "Thon", "in", "et\u00b7was", "hei\u00b7scher", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPOSAT", "NN", "APPR", "PIAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "So schweigen gantz mit recht die d\u00fcstern Pierinnen.", "tokens": ["So", "schwei\u00b7gen", "gantz", "mit", "recht", "die", "d\u00fcs\u00b7tern", "Pie\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.48": {"text": "Eh mich die Poesie nicht zu dem Entzweck bringt,", "tokens": ["Eh", "mich", "die", "Poe\u00b7sie", "nicht", "zu", "dem", "Ent\u00b7zweck", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.49": {"text": "So mag ein anderer hierauff mit Nachdruck sinnen.", "tokens": ["So", "mag", "ein", "an\u00b7de\u00b7rer", "hier\u00b7auff", "mit", "Nach\u00b7druck", "sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "PAV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Allein wer untersteht sich dieses wohl zu thun?", "tokens": ["Al\u00b7lein", "wer", "un\u00b7ter\u00b7steht", "sich", "die\u00b7ses", "wohl", "zu", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PRF", "PDAT", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Dein Ruhm ist allzugro\u00df, dein r\u00fchmliches Bem\u00fchen", "tokens": ["Dein", "Ruhm", "ist", "all\u00b7zu\u00b7gro\u00df", ",", "dein", "r\u00fchm\u00b7li\u00b7ches", "Be\u00b7m\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Das dich gewi\u00dflich l\u00e4\u00dft zu keinen Zeiten ruhn,", "tokens": ["Das", "dich", "ge\u00b7wi\u00df\u00b7lich", "l\u00e4\u00dft", "zu", "kei\u00b7nen", "Zei\u00b7ten", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVFIN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Kan die gelehrte Welt selbst in Erstaunen ziehen.", "tokens": ["Kan", "die", "ge\u00b7lehr\u00b7te", "Welt", "selbst", "in", "Er\u00b7stau\u00b7nen", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Doch ob dich gleich niemand nach W\u00fcrden kan erh\u00f6hn,", "tokens": ["Doch", "ob", "dich", "gleich", "nie\u00b7mand", "nach", "W\u00fcr\u00b7den", "kan", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PIS", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.55": {"text": "So m\u00fcste man im Fall die Mahler imitiren,", "tokens": ["So", "m\u00fcs\u00b7te", "man", "im", "Fall", "die", "Mah\u00b7ler", "i\u00b7mi\u00b7ti\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Die, wenn die Farben gleich schon noch so sch\u00f6ne stehn,", "tokens": ["Die", ",", "wenn", "die", "Far\u00b7ben", "gleich", "schon", "noch", "so", "sch\u00f6\u00b7ne", "stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ART", "NN", "ADV", "ADV", "ADV", "ADV", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Nichts, als das Schattenwerck des Bildes, bey sich f\u00fchren,", "tokens": ["Nichts", ",", "als", "das", "Schat\u00b7ten\u00b7werck", "des", "Bil\u00b7des", ",", "bey", "sich", "f\u00fch\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ART", "NN", "ART", "NN", "$,", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Denn wer dich auf der Welt, nach deinen grossen Werth,", "tokens": ["Denn", "wer", "dich", "auf", "der", "Welt", ",", "nach", "dei\u00b7nen", "gros\u00b7sen", "Werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "ART", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "So wie du es verdienst, mit Ehren wolte kr\u00f6nen,", "tokens": ["So", "wie", "du", "es", "ver\u00b7dienst", ",", "mit", "Eh\u00b7ren", "wol\u00b7te", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PPER", "VVFIN", "$,", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Der m\u00fcste, wenn er es nach Wunsch zu thun begehrt,", "tokens": ["Der", "m\u00fcs\u00b7te", ",", "wenn", "er", "es", "nach", "Wunsch", "zu", "thun", "be\u00b7gehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KOUS", "PPER", "PPER", "APPR", "NN", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Sich von dem Phoebo selbst so Farb als Pinsel lehnen;", "tokens": ["Sich", "von", "dem", "Phoe\u00b7bo", "selbst", "so", "Farb", "als", "Pin\u00b7sel", "leh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NE", "ADV", "ADV", "NN", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "So w\u00fcrde wohl vielleicht die Sache gut gemacht,", "tokens": ["So", "w\u00fcr\u00b7de", "wohl", "viel\u00b7leicht", "die", "Sa\u00b7che", "gut", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Wenn dieses G\u00f6tter-Haupt darzu den Grund-Ri\u00df schenckte;", "tokens": ["Wenn", "die\u00b7ses", "G\u00f6t\u00b7ter\u00b7Haupt", "dar\u00b7zu", "den", "Grun\u00b7dRi\u00df", "schenck\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "So w\u00fcrde recht mit Lust das Werck zu Stande bracht,", "tokens": ["So", "w\u00fcr\u00b7de", "recht", "mit", "Lust", "das", "Werck", "zu", "Stan\u00b7de", "bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Wenn er im Schreiben selbst die schwache Feder lenckte.", "tokens": ["Wenn", "er", "im", "Schrei\u00b7ben", "selbst", "die", "schwa\u00b7che", "Fe\u00b7der", "lenck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Doch wo er dis nicht thut, so bleibt es ungethan,", "tokens": ["Doch", "wo", "er", "dis", "nicht", "thut", ",", "so", "bleibt", "es", "un\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PDS", "PTKNEG", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Und niemand wird gewi\u00df dir eine Lobschrifft schreiben;", "tokens": ["Und", "nie\u00b7mand", "wird", "ge\u00b7wi\u00df", "dir", "ei\u00b7ne", "Lob\u00b7schrifft", "schrei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Den ungebahnten Weg tritt keiner willig an,", "tokens": ["Den", "un\u00b7ge\u00b7bahn\u00b7ten", "Weg", "tritt", "kei\u00b7ner", "wil\u00b7lig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Aus Furcht, er m\u00f6chte wohl darinnen stecken bleiben.", "tokens": ["Aus", "Furcht", ",", "er", "m\u00f6ch\u00b7te", "wohl", "da\u00b7rin\u00b7nen", "ste\u00b7cken", "blei\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Drum Phoebe, lege doch die Hand schon selbst zum Werck,", "tokens": ["Drum", "Phoe\u00b7be", ",", "le\u00b7ge", "doch", "die", "Hand", "schon", "selbst", "zum", "Werck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "La\u00df die Gerechtigkeit hierinnen vor sich gehen,", "tokens": ["La\u00df", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "hie\u00b7rin\u00b7nen", "vor", "sich", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PAV", "APPR", "PRF", "VVINF", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.72": {"text": "Es soll dein G\u00f6tter-Ruhm, und deine Macht und St\u00e4rck", "tokens": ["Es", "soll", "dein", "G\u00f6t\u00b7ter\u00b7Ruhm", ",", "und", "dei\u00b7ne", "Macht", "und", "St\u00e4rck"], "token_info": ["word