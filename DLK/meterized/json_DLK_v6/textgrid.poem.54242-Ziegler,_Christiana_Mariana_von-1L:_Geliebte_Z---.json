": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Nicht so, wie ich, zuletzt die Arbeit noch zerreissen.", "tokens": ["Nicht", "so", ",", "wie", "ich", ",", "zu\u00b7letzt", "die", "Ar\u00b7beit", "noch", "zer\u00b7reis\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "PWAV", "PPER", "$,", "ADV", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Geliebte Z-- so wird dein Nahm und Ruhm,", "tokens": ["Ge\u00b7lieb\u00b7te", "Z\u00b7", "so", "wird", "dein", "Nahm", "und", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "meter.parsing.error", "measure": "measure.parsing.error"}, "line.79": {"text": "Bey der gelehrten Welt mit Ehren ausgebreitet,", "tokens": ["Bey", "der", "ge\u00b7lehr\u00b7ten", "Welt", "mit", "Eh\u00b7ren", "aus\u00b7ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": "Di\u00df ist dein gr\u00f6ster Schatz, di\u00df bleibt dein Eigenthum,", "tokens": ["Di\u00df", "ist", "dein", "gr\u00f6s\u00b7ter", "Schatz", ",", "di\u00df", "bleibt", "dein", "Ei\u00b7gen\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,", "PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.81": {"text": "Da\u00df dir der Ph\u00f6bus selbst die Lobschrifft zubereitet.", "tokens": ["Da\u00df", "dir", "der", "Ph\u00f6\u00b7bus", "selbst", "die", "Lob\u00b7