{"dta.poem.10979": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zur Nothwendigkeit  \n  der N\u00e4chsten Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Glaub! alles was dein N\u00e4chster thut,", "tokens": ["Glaub", "!", "al\u00b7les", "was", "dein", "N\u00e4chs\u00b7ter", "thut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PIS", "PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "(dir schein es unrecht, oder gut,)", "tokens": ["(", "dir", "schein", "es", "un\u00b7recht", ",", "o\u00b7der", "gut", ",", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "NN", "$,", "KON", "ADJD", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df er es, ja so wohl als du,", "tokens": ["Da\u00df", "er", "es", ",", "ja", "so", "wohl", "als", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "ADV", "ADV", "ADV", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus einer guten Meynung, thu.", "tokens": ["Aus", "ei\u00b7ner", "gu\u00b7ten", "Mey\u00b7nung", ",", "thu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Thut er nicht recht; hat er geirret.", "tokens": ["Thut", "er", "nicht", "recht", ";", "hat", "er", "ge\u00b7ir\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJD", "$.", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Doch denk, ob es nicht so der Brauch,", "tokens": ["Doch", "denk", ",", "ob", "es", "nicht", "so", "der", "Brauch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Da\u00df, ja so wohl als ihn, dich auch,", "tokens": ["Da\u00df", ",", "ja", "so", "wohl", "als", "ihn", ",", "dich", "auch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADV", "ADV", "ADV", "KOUS", "PPER", "$,", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Oft Nutz, oft ", "tokens": ["Oft", "Nutz", ",", "oft"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "NN", "$,", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}}}}}