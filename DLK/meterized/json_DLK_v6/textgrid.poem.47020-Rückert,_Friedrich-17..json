{"textgrid.poem.47020": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Feindsel'ge Fee, die du mit Zaubertraum", "tokens": ["Feind\u00b7sel'\u00b7ge", "Fee", ",", "die", "du", "mit", "Zau\u00b7ber\u00b7traum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Luft, Himmel, Erd' und Fluten h\u00e4ltst umsponnen,", "tokens": ["Luft", ",", "Him\u00b7mel", ",", "Erd'", "und", "Flu\u00b7ten", "h\u00e4ltst", "um\u00b7spon\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So da\u00df, wie du mir z\u00fcrnst, das Licht der Sonnen", "tokens": ["So", "da\u00df", ",", "wie", "du", "mir", "z\u00fcrnst", ",", "das", "Licht", "der", "Son\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht l\u00e4cheln kann, und gr\u00fcnen nicht der Raum,", "tokens": ["Nicht", "l\u00e4\u00b7cheln", "kann", ",", "und", "gr\u00fc\u00b7nen", "nicht", "der", "Raum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$,", "KON", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Wind nicht k\u00fchlen, schatten nicht der Baum,", "tokens": ["Der", "Wind", "nicht", "k\u00fch\u00b7len", ",", "schat\u00b7ten", "nicht", "der", "Baum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$,", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Strauch nicht duften, rauschen nicht der Bronnen;", "tokens": ["Der", "Strauch", "nicht", "duf\u00b7ten", ",", "rau\u00b7schen", "nicht", "der", "Bron\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$,", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O h\u00e4ltst du, um die letzte mir der Wonnen", "tokens": ["O", "h\u00e4ltst", "du", ",", "um", "die", "letz\u00b7te", "mir", "der", "Won\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "KOUI", "ART", "ADJA", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu wehren, nun den Traumgott auch am Zaum?", "tokens": ["Zu", "weh\u00b7ren", ",", "nun", "den", "Traum\u00b7gott", "auch", "am", "Zaum", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADV", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da\u00df, so wie du dich wachend mir versagest,", "tokens": ["Da\u00df", ",", "so", "wie", "du", "dich", "wa\u00b7chend", "mir", "ver\u00b7sa\u00b7gest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADV", "KOKOM", "PPER", "PRF", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er dich mir auch versagen mu\u00df im Schlafe,", "tokens": ["Er", "dich", "mir", "auch", "ver\u00b7sa\u00b7gen", "mu\u00df", "im", "Schla\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir nie dein s\u00fc\u00dfes Antlitz l\u00e4\u00dft erscheinen;", "tokens": ["Mir", "nie", "dein", "s\u00fc\u00b7\u00dfes", "Ant\u00b7litz", "l\u00e4\u00dft", "er\u00b7schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Als ob du ihm gedroht: Wenn du es wagest,", "tokens": ["Als", "ob", "du", "ihm", "ge\u00b7droht", ":", "Wenn", "du", "es", "wa\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPER", "VVPP", "$.", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf seinen Augen je zu ruhn, zur Strafe", "tokens": ["Auf", "sei\u00b7nen", "Au\u00b7gen", "je", "zu", "ruhn", ",", "zur", "Stra\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sollst du hinfort nie ruhen mehr auf meinen.", "tokens": ["Sollst", "du", "hin\u00b7fort", "nie", "ru\u00b7hen", "mehr", "auf", "mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVFIN", "ADV", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}