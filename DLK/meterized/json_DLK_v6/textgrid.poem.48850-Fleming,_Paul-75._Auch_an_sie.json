{"textgrid.poem.48850": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "75. Auch an sie", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du dreier Treueste, die eine Mutter brachte,", "tokens": ["Du", "drei\u00b7er", "Treu\u00b7es\u00b7te", ",", "die", "ei\u00b7ne", "Mut\u00b7ter", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "darf ich noch so, wie vor ich m\u00fcndlich oft getan,", "tokens": ["darf", "ich", "noch", "so", ",", "wie", "vor", "ich", "m\u00fcnd\u00b7lich", "oft", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "$,", "PWAV", "APPR", "PPER", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "auch itzund doch durch Schrift dich sicher reden an,", "tokens": ["auch", "it\u00b7zund", "doch", "durch", "Schrift", "dich", "si\u00b7cher", "re\u00b7den", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "NN", "PPER", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "so lies di\u00df Brieflein hier, das ich dir, Sch\u00f6ne, machte,", "tokens": ["so", "lies", "di\u00df", "Brie\u00b7flein", "hier", ",", "das", "ich", "dir", ",", "Sch\u00f6\u00b7ne", ",", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "NN", "ADV", "$,", "PRELS", "PPER", "PPER", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "als ich so weit von dir und deiner Stadt gedachte.", "tokens": ["als", "ich", "so", "weit", "von", "dir", "und", "dei\u00b7ner", "Stadt", "ge\u00b7dach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erinnre dich an dir und denke stetigs dran,", "tokens": ["E\u00b7rinn\u00b7re", "dich", "an", "dir", "und", "den\u00b7ke", "ste\u00b7tigs", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "KON", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "was ich so \u00f6ffentlich nicht schreiben darf, noch kan,", "tokens": ["was", "ich", "so", "\u00f6f\u00b7fent\u00b7lich", "nicht", "schrei\u00b7ben", "darf", ",", "noch", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "PTKNEG", "VVINF", "VMFIN", "$,", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "nach dem ich Nacht und Tag und alle Stunden trachte.", "tokens": ["nach", "dem", "ich", "Nacht", "und", "Tag", "und", "al\u00b7le", "Stun\u00b7den", "trach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "KON", "NN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Kl\u00fcgling mag ihm das nun deuten, wie er will,", "tokens": ["Ein", "Kl\u00fcg\u00b7ling", "mag", "ihm", "das", "nun", "deu\u00b7ten", ",", "wie", "er", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PDS", "ADV", "VVINF", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mag raten di\u00df und das, der Glossen machen viel,", "tokens": ["mag", "ra\u00b7ten", "di\u00df", "und", "das", ",", "der", "Glos\u00b7sen", "ma\u00b7chen", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PDS", "KON", "PDS", "$,", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "die Warheit bleibt bei uns. Es ist nicht Ha\u00df, nicht Liebe,", "tokens": ["die", "War\u00b7heit", "bleibt", "bei", "uns", ".", "Es", "ist", "nicht", "Ha\u00df", ",", "nicht", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$.", "PPER", "VAFIN", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "nicht etwas und doch was, erlogen und doch wahr,", "tokens": ["nicht", "et\u00b7was", "und", "doch", "was", ",", "er\u00b7lo\u00b7gen", "und", "doch", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KON", "ADV", "PIS", "$,", "VVPP", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "nichts Heimlichs, aber doch daher nicht offenbar.", "tokens": ["nichts", "Heim\u00b7lichs", ",", "a\u00b7ber", "doch", "da\u00b7her", "nicht", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "$,", "ADV", "ADV", "PAV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Di\u00df, Jungfrau, leg' ihm f\u00fcr, da\u00df er sich drinnen \u00fcbe.", "tokens": ["Di\u00df", ",", "Jung\u00b7frau", ",", "leg'", "ihm", "f\u00fcr", ",", "da\u00df", "er", "sich", "drin\u00b7nen", "\u00fc\u00b7be", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "$,", "VVFIN", "PPER", "APPR", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}