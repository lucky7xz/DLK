{"textgrid.poem.40483": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie kommt's, da\u00df mit dem Pfeil im Herzen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie kommt's, da\u00df mit dem Pfeil im Herzen", "tokens": ["Wie", "kommt's", ",", "da\u00df", "mit", "dem", "Pfeil", "im", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Schmerz ich sang der Liebe Lust?", "tokens": ["Im", "Schmerz", "ich", "sang", "der", "Lie\u00b7be", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie kommt's, da\u00df nur von heitern Scherzen", "tokens": ["Wie", "kommt's", ",", "da\u00df", "nur", "von", "hei\u00b7tern", "Scher\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "$,", "KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir quillt die todeswunde Brust? \u2013", "tokens": ["Mir", "quillt", "die", "to\u00b7des\u00b7wun\u00b7de", "Brust", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es segelt sanft auf Silberwogen", "tokens": ["Es", "se\u00b7gelt", "sanft", "auf", "Sil\u00b7ber\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Schneegewand der stolze Schwan,", "tokens": ["Im", "Schnee\u00b7ge\u00b7wand", "der", "stol\u00b7ze", "Schwan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gesanglos ist er lang gezogen", "tokens": ["Ge\u00b7sang\u00b7los", "ist", "er", "lang", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In stummer Lust die stille Bahn.", "tokens": ["In", "stum\u00b7mer", "Lust", "die", "stil\u00b7le", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Morgenroth, im Mondenscheine", "tokens": ["Im", "Mor\u00b7gen\u00b7roth", ",", "im", "Mon\u00b7den\u00b7schei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fluth durchschifft' er frei \u2013 und schwieg;", "tokens": ["Die", "Fluth", "durch\u00b7schifft'", "er", "frei", "\u2013", "und", "schwieg", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADJD", "$(", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Ufer bl\u00fchten Rosenhaine,", "tokens": ["Am", "U\u00b7fer", "bl\u00fch\u00b7ten", "Ro\u00b7sen\u00b7hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er segelte vorbei \u2013 und schwieg.", "tokens": ["Er", "se\u00b7gel\u00b7te", "vor\u00b7bei", "\u2013", "und", "schwieg", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$(", "KON", "VVFIN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Jetzt, da der Pfeil sein Herz durchdrungen,", "tokens": ["Jetzt", ",", "da", "der", "Pfeil", "sein", "Herz", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da ihm der Tod im Busen gl\u00fcht,", "tokens": ["Da", "ihm", "der", "Tod", "im", "Bu\u00b7sen", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was er in Wonne nie gesungen,", "tokens": ["Was", "er", "in", "Won\u00b7ne", "nie", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er singt's in Schmerz: sein erstes Lied.", "tokens": ["Er", "singt's", "in", "Schmerz", ":", "sein", "ers\u00b7tes", "Lied", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}