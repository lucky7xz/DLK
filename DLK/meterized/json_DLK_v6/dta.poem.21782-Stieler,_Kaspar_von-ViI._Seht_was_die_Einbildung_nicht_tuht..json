{"dta.poem.21782": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n  Seht was die Einbildung nicht  \n tuht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Es sagte mir die Flatter-sch\u00f6ne/", "tokens": ["Es", "sag\u00b7te", "mir", "die", "Flat\u00b7ter\u00b7sch\u00f6\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die eingebildte Pusserene:", "tokens": ["die", "ein\u00b7ge\u00b7bild\u00b7te", "Pus\u00b7se\u00b7re\u00b7ne", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "du hast mich lange Zeit geliebt/", "tokens": ["du", "hast", "mich", "lan\u00b7ge", "Zeit", "ge\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "dich lang\u2019 um meine Gunst beworben/", "tokens": ["dich", "lang'", "um", "mei\u00b7ne", "Gunst", "be\u00b7wor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "darumb hastu dich so betr\u00fcbt/", "tokens": ["da\u00b7rumb", "has\u00b7tu", "dich", "so", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "da\u00df du auch neulich bald gestorben.", "tokens": ["da\u00df", "du", "auch", "neu\u00b7lich", "bald", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie kommt es denn/ wenn ich dich frage/", "tokens": ["Wie", "kommt", "es", "denn", "/", "wenn", "ich", "dich", "fra\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$(", "KOUS", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und dir von Nehmen etwas sage/", "tokens": ["und", "dir", "von", "Neh\u00b7men", "et\u00b7was", "sa\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "da\u00df du so sonder Antwort bist?", "tokens": ["da\u00df", "du", "so", "son\u00b7der", "Ant\u00b7wort", "bist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "das Wort verstarrt dir in dem Munde/", "tokens": ["das", "Wort", "ver\u00b7starrt", "dir", "in", "dem", "Mun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "du must ia nur au\u00df Hinterlist", "tokens": ["du", "must", "i\u00b7a", "nur", "au\u00df", "Hin\u00b7ter\u00b7list"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "erdenken eine falsche Wunde.", "tokens": ["er\u00b7den\u00b7ken", "ei\u00b7ne", "fal\u00b7sche", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie offt hastu nicht nachgelassen/", "tokens": ["Wie", "offt", "has\u00b7tu", "nicht", "nach\u00b7ge\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ich m\u00f6chte dich denn einst umb fassen", "tokens": ["ich", "m\u00f6ch\u00b7te", "dich", "denn", "einst", "umb", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "ADV", "APPR", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "wie prachertstu um einen Ku\u00df/", "tokens": ["wie", "pra\u00b7cherts\u00b7tu", "um", "ei\u00b7nen", "Ku\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "das andre wil ich gern verschweigen/", "tokens": ["das", "and\u00b7re", "wil", "ich", "gern", "ver\u00b7schwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "da\u00df ich zwar stets gedenken mu\u00df/", "tokens": ["da\u00df", "ich", "zwar", "stets", "ge\u00b7den\u00b7ken", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "darff aber keinem an-es zeigen.", "tokens": ["darff", "a\u00b7ber", "kei\u00b7nem", "an\u00b7es", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein/ (sprach ich) la\u00df mich doch zufrieden/", "tokens": ["Mein", "/", "(", "sprach", "ich", ")", "la\u00df", "mich", "doch", "zu\u00b7frie\u00b7den", "/"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$(", "$(", "VVFIN", "PPER", "$(", "VVIMP", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die Ursach wird nicht einem ieden", "tokens": ["die", "Ur\u00b7sach", "wird", "nicht", "ei\u00b7nem", "ie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ART", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "wer alles sagt und wenig denkt/", "tokens": ["wer", "al\u00b7les", "sagt", "und", "we\u00b7nig", "denkt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "KON", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der kan sich deinen Freund nicht nennen.", "tokens": ["der", "kan", "sich", "dei\u00b7nen", "Freund", "nicht", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VMFIN", "PRF", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch/ soll ichs/ Zeit-lieb/ dir entdekken/", "tokens": ["Doch", "/", "soll", "ichs", "/", "Zeit\u00b7lieb", "/", "dir", "ent\u00b7dek\u00b7ken", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$(", "VMFIN", "PIS", "$(", "NN", "$(", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und nichts nicht untern Stuel verstekken", "tokens": ["und", "nichts", "nicht", "un\u00b7tern", "Stu\u00b7el", "vers\u00b7tek\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "PTKNEG", "ADJA", "NN", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich wei\u00df es was ich m\u00fcndlich sage", "tokens": ["Ich", "wei\u00df", "es", "was", "ich", "m\u00fcnd\u00b7lich", "sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PWS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "urteilestu wie ungeb\u00fchr/", "tokens": ["ur\u00b7tei\u00b7le\u00b7stu", "wie", "un\u00b7ge\u00b7b\u00fchr", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "als Unrecht Falsch und L\u00fcgen-klage.", "tokens": ["als", "Un\u00b7recht", "Falsch", "und", "L\u00fc\u00b7gen\u00b7kla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Drauff hab\u2019 ich ihr di\u00df zugeschrieben:", "tokens": ["Drauff", "hab'", "ich", "ihr", "di\u00df", "zu\u00b7ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PPER", "PDS", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich kan dich/ Larve/ treu nicht lieben/", "tokens": ["Ich", "kan", "dich", "/", "Lar\u00b7ve", "/", "treu", "nicht", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$(", "NE", "$(", "ADJD", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "ich bin nicht so/ wie du/ gesinnt.", "tokens": ["ich", "bin", "nicht", "so", "/", "wie", "du", "/", "ge\u00b7sinnt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "$(", "PWAV", "PPER", "$(", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Ich liebe Tugend/ Zucht und Treue/", "tokens": ["Ich", "lie\u00b7be", "Tu\u00b7gend", "/", "Zucht", "und", "Treu\u00b7e", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "w\u00e4r\u2019 ich wie du ein falsches Kind/", "tokens": ["w\u00e4r'", "ich", "wie", "du", "ein", "fal\u00b7sches", "Kind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PWAV", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "h\u00e4tt\u2019 ich vor deinem Strikk nicht Scheue.", "tokens": ["h\u00e4tt'", "ich", "vor", "dei\u00b7nem", "Strikk", "nicht", "Scheu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Meineyd ist dir angebohren", "tokens": ["Der", "Mei\u00b7neyd", "ist", "dir", "an\u00b7ge\u00b7boh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die Schaam und Zuchthastu verschworen", "tokens": ["die", "Schaam", "und", "Zucht\u00b7has\u00b7tu", "ver\u00b7schwo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur Schminke sch\u00f6net dein Gesicht.", "tokens": ["Nur", "Schmin\u00b7ke", "sch\u00f6\u00b7net", "dein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Runzeln k\u00f6ntstu nicht bedekken/", "tokens": ["Die", "Run\u00b7zeln", "k\u00f6nts\u00b7tu", "nicht", "be\u00b7dek\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "h\u00e4ttstu die falsche Kreyde nicht", "tokens": ["h\u00e4tts\u00b7tu", "die", "fal\u00b7sche", "Krey\u00b7de", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "den Dekkel deiner schwarzen Flekken.", "tokens": ["den", "Dek\u00b7kel", "dei\u00b7ner", "schwar\u00b7zen", "Flek\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch wil ich noch was dein verbleiben/", "tokens": ["Doch", "wil", "ich", "noch", "was", "dein", "ver\u00b7blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PWS", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "bi\u00df mein Verh\u00e4ngn\u00fcs mich wird treiben", "tokens": ["bi\u00df", "mein", "Ver\u00b7h\u00e4ng\u00b7n\u00fcs", "mich", "wird", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VAFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "auff ein bequeemers Zielmaa\u00df hin.", "tokens": ["auff", "ein", "be\u00b7que\u00b7e\u00b7mers", "Ziel\u00b7maa\u00df", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "O/ wie verdro\u00df es Pusserenen!", "tokens": ["O", "/", "wie", "ver\u00b7dro\u00df", "es", "Pus\u00b7se\u00b7re\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PWAV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ey/ da\u00df ich auch zu k\u00fchne bin", "tokens": ["Ey", "/", "da\u00df", "ich", "auch", "zu", "k\u00fch\u00b7ne", "bin"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "KOUS", "PPER", "ADV", "PTKA", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "doch auch/ wer achtt der Flatter-sch\u00f6nen.", "tokens": ["doch", "auch", "/", "wer", "achtt", "der", "Flat\u00b7ter\u00b7sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}