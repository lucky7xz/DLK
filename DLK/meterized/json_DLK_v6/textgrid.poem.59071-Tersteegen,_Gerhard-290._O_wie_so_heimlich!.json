{"textgrid.poem.59071": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "290. O wie so heimlich!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du willst noch heimlich etwas sein;", "tokens": ["Du", "willst", "noch", "heim\u00b7lich", "et\u00b7was", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "PIS", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dies h\u00e4lt dich heimlich hart gefangen.", "tokens": ["Dies", "h\u00e4lt", "dich", "heim\u00b7lich", "hart", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sink wahrlich in dein Nichts hinein;", "tokens": ["Sink", "wahr\u00b7lich", "in", "dein", "Nichts", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott will dich nackt in sich empfangen!", "tokens": ["Gott", "will", "dich", "nackt", "in", "sich", "emp\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "ADJD", "APPR", "PRF", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}