{"textgrid.poem.67098": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Diotima", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lange tot und tiefverschlossen,", "tokens": ["Lan\u00b7ge", "tot", "und", "tief\u00b7ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fc\u00dft mein Herz die sch\u00f6ne Welt,", "tokens": ["Gr\u00fc\u00dft", "mein", "Herz", "die", "sch\u00f6\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Zweige bl\u00fchn und sprossen,", "tokens": ["Sei\u00b7ne", "Zwei\u00b7ge", "bl\u00fchn", "und", "spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neu von Lebenskraft geschwellt;", "tokens": ["Neu", "von", "Le\u00b7bens\u00b7kraft", "ge\u00b7schwellt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O! ich kehre noch ins Leben,", "tokens": ["O", "!", "ich", "keh\u00b7re", "noch", "ins", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie heraus in Luft und Licht", "tokens": ["Wie", "he\u00b7raus", "in", "Luft", "und", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Meiner Blumen selig Streben", "tokens": ["Mei\u00b7ner", "Blu\u00b7men", "se\u00b7lig", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aus der d\u00fcrren H\u00fclse bricht.", "tokens": ["Aus", "der", "d\u00fcr\u00b7ren", "H\u00fcl\u00b7se", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die ihr meine Klage kanntet,", "tokens": ["Die", "ihr", "mei\u00b7ne", "Kla\u00b7ge", "kann\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihr liebez\u00fcrnend oft", "tokens": ["Die", "ihr", "lie\u00b7be\u00b7z\u00fcr\u00b7nend", "oft"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meines Sinnes Fehle nanntet", "tokens": ["Mei\u00b7nes", "Sin\u00b7nes", "Feh\u00b7le", "nann\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und geduldet und gehofft,", "tokens": ["Und", "ge\u00b7dul\u00b7det", "und", "ge\u00b7hofft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "KON", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eure Not ist aus, ihr Lieben!", "tokens": ["Eu\u00b7re", "Not", "ist", "aus", ",", "ihr", "Lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKVZ", "$,", "PPOSAT", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und das Dornenbett ist leer,", "tokens": ["Und", "das", "Dor\u00b7nen\u00b7bett", "ist", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ihr kennt den immertr\u00fcben", "tokens": ["Und", "ihr", "kennt", "den", "im\u00b7mer\u00b7tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kranken Weinenden nicht mehr.", "tokens": ["Kran\u00b7ken", "Wei\u00b7nen\u00b7den", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie so anders ists geworden!", "tokens": ["Wie", "so", "an\u00b7ders", "ists", "ge\u00b7wor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VAFIN", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles was ich ha\u00dft und mied,", "tokens": ["Al\u00b7les", "was", "ich", "ha\u00dft", "und", "mied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stimmt in freundlichen Akkorden", "tokens": ["Stimmt", "in", "freund\u00b7li\u00b7chen", "Ak\u00b7kor\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun in meines Lebens Lied,", "tokens": ["Nun", "in", "mei\u00b7nes", "Le\u00b7bens", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mit jedem Stundenschlage", "tokens": ["Und", "mit", "je\u00b7dem", "Stun\u00b7den\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Werd ich wunderbar gemahnt", "tokens": ["Werd", "ich", "wun\u00b7der\u00b7bar", "ge\u00b7mahnt"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "An der Kindheit goldne Tage,", "tokens": ["An", "der", "Kind\u00b7heit", "gold\u00b7ne", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seit ich dieses Eine fand.", "tokens": ["Seit", "ich", "die\u00b7ses", "Ei\u00b7ne", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Diotima! selig Wesen!", "tokens": ["Dio\u00b7ti\u00b7ma", "!", "se\u00b7lig", "We\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Herrliche, durch die mein Geist,", "tokens": ["Herr\u00b7li\u00b7che", ",", "durch", "die", "mein", "Geist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "PPOSAT", "NN", "$,"], "meter": "+--++-+", "measure": "dactylic.init"}, "line.3": {"text": "Von des Lebens Angst genesen,", "tokens": ["Von", "des", "Le\u00b7bens", "Angst", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "G\u00f6tterjugend sich verhei\u00dft!", "tokens": ["G\u00f6t\u00b7ter\u00b7ju\u00b7gend", "sich", "ver\u00b7hei\u00dft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unser Himmel wird bestehen,", "tokens": ["Un\u00b7ser", "Him\u00b7mel", "wird", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unergr\u00fcndlich sich verwandt", "tokens": ["Un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "sich", "ver\u00b7wandt"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "PRF", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat, noch eh wir uns gesehen,", "tokens": ["Hat", ",", "noch", "eh", "wir", "uns", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADV", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unser Wesen sich gekannt.", "tokens": ["Un\u00b7ser", "We\u00b7sen", "sich", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Da ich noch in Kindertr\u00e4umen,", "tokens": ["Da", "ich", "noch", "in", "Kin\u00b7der\u00b7tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Friedlich wie der blaue Tag,", "tokens": ["Fried\u00b7lich", "wie", "der", "blau\u00b7e", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter meines Gartens B\u00e4umen", "tokens": ["Un\u00b7ter", "mei\u00b7nes", "Gar\u00b7tens", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der warmen Erde lag,", "tokens": ["Auf", "der", "war\u00b7men", "Er\u00b7de", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da mein erst Gef\u00fchl sich regte,", "tokens": ["Da", "mein", "erst", "Ge\u00b7f\u00fchl", "sich", "reg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADV", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Da zum erstenmale sich", "tokens": ["Da", "zum", "ers\u00b7ten\u00b7ma\u00b7le", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "G\u00f6ttliches in mir bewegte,", "tokens": ["G\u00f6tt\u00b7li\u00b7ches", "in", "mir", "be\u00b7weg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "S\u00e4uselte dein Geist um mich.", "tokens": ["S\u00e4u\u00b7sel\u00b7te", "dein", "Geist", "um", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach und da mein sch\u00f6ner Friede,", "tokens": ["Ach", "und", "da", "mein", "sch\u00f6\u00b7ner", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KON", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein Saitenspiel, zerri\u00df,", "tokens": ["Wie", "ein", "Sai\u00b7ten\u00b7spiel", ",", "zer\u00b7ri\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da von Ha\u00df und Liebe m\u00fcde", "tokens": ["Da", "von", "Ha\u00df", "und", "Lie\u00b7be", "m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich mein guter Geist verlie\u00df,", "tokens": ["Mich", "mein", "gu\u00b7ter", "Geist", "ver\u00b7lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kamst du, wie vom Himmel nieder", "tokens": ["Kamst", "du", ",", "wie", "vom", "Him\u00b7mel", "nie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "APPRART", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und es gab mein einzig Gl\u00fcck,", "tokens": ["Und", "es", "gab", "mein", "ein\u00b7zig", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Meines Sinnes Wohllaut wieder", "tokens": ["Mei\u00b7nes", "Sin\u00b7nes", "Wohl\u00b7laut", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir ein Traum von dir zur\u00fcck.", "tokens": ["Mir", "ein", "Traum", "von", "dir", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Da ich flehend mich vergebens", "tokens": ["Da", "ich", "fle\u00b7hend", "mich", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An der Wesen kleinstes hing,", "tokens": ["An", "der", "We\u00b7sen", "kleins\u00b7tes", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch den Sonnenschein des Lebens", "tokens": ["Durch", "den", "Son\u00b7nen\u00b7schein", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsam, wie ein Blinder, ging,", "tokens": ["Ein\u00b7sam", ",", "wie", "ein", "Blin\u00b7der", ",", "ging", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Oft vor treuem Angesichte", "tokens": ["Oft", "vor", "treu\u00b7em", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stand und keine Deutung fand,", "tokens": ["Stand", "und", "kei\u00b7ne", "Deu\u00b7tung", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Darbend vor des Himmels Lichte,", "tokens": ["Dar\u00b7bend", "vor", "des", "Him\u00b7mels", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Vor der Mutter Erde stand,", "tokens": ["Vor", "der", "Mut\u00b7ter", "Er\u00b7de", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Lieblich Bild, mit deinem Strahle", "tokens": ["Lieb\u00b7lich", "Bild", ",", "mit", "dei\u00b7nem", "Strah\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drangst du da in meine Nacht!", "tokens": ["Drangst", "du", "da", "in", "mei\u00b7ne", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Neu an meinem Ideale,", "tokens": ["Neu", "an", "mei\u00b7nem", "I\u00b7dea\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Neu und stark war ich erwacht;", "tokens": ["Neu", "und", "stark", "war", "ich", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure"