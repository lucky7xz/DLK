{"textgrid.poem.58374": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "412. Wer an nichts klebt, in Ruhe lebt", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df alle Kreatur und auch dich selber los,", "tokens": ["La\u00df", "al\u00b7le", "Kre\u00b7a\u00b7tur", "und", "auch", "dich", "sel\u00b7ber", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "KON", "ADV", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was Gott dir nimmt, gib hin und lieb ihn rein und blo\u00df!", "tokens": ["Was", "Gott", "dir", "nimmt", ",", "gib", "hin", "und", "lieb", "ihn", "rein", "und", "blo\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "VVFIN", "$,", "VVIMP", "PTKVZ", "KON", "ADJD", "PPER", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer so entbl\u00f6\u00dft ist, kann sich frei in Gott erheben,", "tokens": ["Wer", "so", "ent\u00b7bl\u00f6\u00dft", "ist", ",", "kann", "sich", "frei", "in", "Gott", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "VAFIN", "$,", "VMFIN", "PRF", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von St\u00fcrmen unber\u00fchrt, in sel'ger Stille leben.", "tokens": ["Von", "St\u00fcr\u00b7men", "un\u00b7be\u00b7r\u00fchrt", ",", "in", "sel'\u00b7ger", "Stil\u00b7le", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}