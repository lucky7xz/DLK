{"textgrid.poem.30754": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sc\u00e6vus wird mit Ewigkeit immer in die wette leben;", "tokens": ["Sc\u00e6vus", "wird", "mit", "E\u00b7wig\u00b7keit", "im\u00b7mer", "in", "die", "wet\u00b7te", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.2": {"text": "Tugend wird das Alter nicht, Bosheit wird ihm solches geben.", "tokens": ["Tu\u00b7gend", "wird", "das", "Al\u00b7ter", "nicht", ",", "Bos\u00b7heit", "wird", "ihm", "sol\u00b7ches", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "PTKNEG", "$,", "NN", "VAFIN", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}