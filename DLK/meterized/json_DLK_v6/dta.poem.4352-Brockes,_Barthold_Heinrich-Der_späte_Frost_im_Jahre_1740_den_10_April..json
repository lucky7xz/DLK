{"dta.poem.4352": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der sp\u00e4te Frost im Jahre 1740  \n den 10 April.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O GOtt! Der Du die rege Zeit", "tokens": ["O", "Gott", "!", "Der", "Du", "die", "re\u00b7ge", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "ART", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Zum Ursprung aller Fruchtbarkeit", "tokens": ["Zum", "Ur\u00b7sprung", "al\u00b7ler", "Frucht\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jm steten Wechsel eingerichtet,", "tokens": ["Jm", "ste\u00b7ten", "Wech\u00b7sel", "ein\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, la\u00df von Deinem heil\u2019gen Schlu\u00df,", "tokens": ["Ach", ",", "la\u00df", "von", "Dei\u00b7nem", "heil'\u00b7gen", "Schlu\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus welchem aller Ueberflu\u00df,", "tokens": ["Aus", "wel\u00b7chem", "al\u00b7ler", "Ue\u00b7berf\u00b7lu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und aller Segen quillt allein,", "tokens": ["Und", "al\u00b7ler", "Se\u00b7gen", "quillt", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die weise Ordnung nicht zernichtet,", "tokens": ["Die", "wei\u00b7se", "Ord\u00b7nung", "nicht", "zer\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Noch die Natur ver\u00e4ndert seyn!", "tokens": ["Noch", "die", "Na\u00b7tur", "ver\u00b7\u00e4n\u00b7dert", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ach, la\u00df die W\u00e4rme wiederkehren!", "tokens": ["Ach", ",", "la\u00df", "die", "W\u00e4r\u00b7me", "wie\u00b7der\u00b7keh\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "La\u00df sich den Lebens-Balsam mehren,", "tokens": ["La\u00df", "sich", "den", "Le\u00b7bens\u00b7Bal\u00b7sam", "meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Den Du derselben eingesenkt,", "tokens": ["Den", "Du", "der\u00b7sel\u00b7ben", "ein\u00b7ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wodurch sich Thier\u2019 und Menschen n\u00e4hren,", "tokens": ["Wo\u00b7durch", "sich", "Thier'", "und", "Men\u00b7schen", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wodurch sich jede Pflanze tr\u00e4nkt!", "tokens": ["Wo\u00b7durch", "sich", "je\u00b7de", "Pflan\u00b7ze", "tr\u00e4nkt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Du sprachst: ", "tokens": ["Du", "sprachst", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "So lang' als sich die Erde dreh't, durch meine Lieb'", "tokens": ["So", "lang'", "als", "sich", "die", "Er\u00b7de", "dreh't", ",", "durch", "mei\u00b7ne", "Lieb'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "und weise Macht,", "tokens": ["und", "wei\u00b7se", "Macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "So Saam' als Erndte, Frost und Hitze. Du hast", "tokens": ["So", "Saam'", "als", "Ernd\u00b7te", ",", "Frost", "und", "Hit\u00b7ze", ".", "Du", "hast"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "NE", "KOUS", "NN", "$,", "NN", "KON", "NN", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.18": {"text": "verboten aufzuh\u00f6ren", "tokens": ["ver\u00b7bo\u00b7ten", "auf\u00b7zu\u00b7h\u00f6\u00b7ren"], "token_info": ["word", "word"], "pos": ["VVPP", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Dem Sommer, Winter, Tag und Nacht.", "tokens": ["Dem", "Som\u00b7mer", ",", "Win\u00b7ter", ",", "Tag", "und", "Nacht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Wir Armen k\u00f6nnen anders nichts,", "tokens": ["Wir", "Ar\u00b7men", "k\u00f6n\u00b7nen", "an\u00b7ders", "nichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VMFIN", "ADV", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Als Dich, Du ew\u2019ger Born des Lichts,", "tokens": ["Als", "Dich", ",", "Du", "ew'\u00b7ger", "Born", "des", "Lichts", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPER", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der W\u00e4rm\u2019 und Lebens, anzugehn,", "tokens": ["Der", "W\u00e4rm'", "und", "Le\u00b7bens", ",", "an\u00b7zu\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Dich zuversichtlich anzuflehn,", "tokens": ["Dich", "zu\u00b7ver\u00b7sicht\u00b7lich", "an\u00b7zu\u00b7flehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Jm Sonnen-Licht uns anzusehn:", "tokens": ["Jm", "Son\u00b7nen\u00b7Licht", "uns", "an\u00b7zu\u00b7sehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "So werden laue Winde weh\u2019n;", "tokens": ["So", "wer\u00b7den", "lau\u00b7e", "Win\u00b7de", "weh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "So wird Gras, Laub und Bl\u00fcht\u2019 entstehn;", "tokens": ["So", "wird", "Gras", ",", "Laub", "und", "Bl\u00fcht'", "ent\u00b7stehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "So wird die ganze Erde sch\u00f6n;", "tokens": ["So", "wird", "die", "gan\u00b7ze", "Er\u00b7de", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "So kann das Vieh zur Weide gehn.", "tokens": ["So", "kann", "das", "Vieh", "zur", "Wei\u00b7de", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "So bald Du es nur willt, geschichts.", "tokens": ["So", "bald", "Du", "es", "nur", "willt", ",", "ge\u00b7schichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "ADV", "VMFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}