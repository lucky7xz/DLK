{"textgrid.poem.64876": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als bang die Kunde deines Tod's erscholl,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als bang die Kunde deines Tod's erscholl,", "tokens": ["Als", "bang", "die", "Kun\u00b7de", "dei\u00b7nes", "To\u00b7d's", "er\u00b7scholl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Da schlugen sie die Brust verzweiflungsvoll,", "tokens": ["Da", "schlu\u00b7gen", "sie", "die", "Brust", "ver\u00b7zwei\u00b7flungs\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da rangen schmerzbew\u00e4ltigt sie die H\u00e4nde", "tokens": ["Da", "ran\u00b7gen", "schmerz\u00b7be\u00b7w\u00e4l\u00b7tigt", "sie", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und klagten um dein allzufr\u00fches Ende.", "tokens": ["Und", "klag\u00b7ten", "um", "dein", "all\u00b7zu\u00b7fr\u00fc\u00b7hes", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zerrissen nannten sie ihr eignes Sein,", "tokens": ["Zer\u00b7ris\u00b7sen", "nann\u00b7ten", "sie", "ihr", "eig\u00b7nes", "Sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von jedem Trost sich blutend abgeschnitten,", "tokens": ["Von", "je\u00b7dem", "Trost", "sich", "blu\u00b7tend", "ab\u00b7ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Verwais't an Gl\u00fcck ihr brechend Herz, inmitten", "tokens": ["Ver\u00b7wais't", "an", "Gl\u00fcck", "ihr", "bre\u00b7chend", "Herz", ",", "in\u00b7mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "APPR", "NN", "PPER", "ADJD", "NN", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der lauten Welt verlassen und allein!", "tokens": ["Der", "lau\u00b7ten", "Welt", "ver\u00b7las\u00b7sen", "und", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "KON", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch jetzt, \u2013 wie bittrer Hohn beschleicht es mich,", "tokens": ["Doch", "jetzt", ",", "\u2013", "wie", "bit\u00b7trer", "Hohn", "be\u00b7schleicht", "es", "mich", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "$(", "KOKOM", "ADJA", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denk' ich, wie schnell der edle Gram entwich! \u2013", "tokens": ["Denk'", "ich", ",", "wie", "schnell", "der", "ed\u00b7le", "Gram", "ent\u00b7wich", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "ADJD", "ART", "ADJA", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt liegt, versenkt, im Lebensmeer ihr Kummer;", "tokens": ["Jetzt", "liegt", ",", "ver\u00b7senkt", ",", "im", "Le\u00b7bens\u00b7meer", "ihr", "Kum\u00b7mer", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "$,", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Welt Syrenenlied sang ihn in Schlummer", "tokens": ["Der", "Welt", "Sy\u00b7re\u00b7nen\u00b7lied", "sang", "ihn", "in", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und dein Ged\u00e4chtni\u00df ward der Welt zum Raub.", "tokens": ["Und", "dein", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "ward", "der", "Welt", "zum", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Eh deinem H\u00fcgel Blumen noch entwallen,", "tokens": ["Eh", "dei\u00b7nem", "H\u00fc\u00b7gel", "Blu\u00b7men", "noch", "ent\u00b7wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Eh du in deinem Grab zu Staub zerfallen,", "tokens": ["Eh", "du", "in", "dei\u00b7nem", "Grab", "zu", "Staub", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wardst du im Herzen Deiner Lieben Staub!", "tokens": ["Wardst", "du", "im", "Her\u00b7zen", "Dei\u00b7ner", "Lie\u00b7ben", "Staub", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und ich allein, die, als du noch gelebt,", "tokens": ["Und", "ich", "al\u00b7lein", ",", "die", ",", "als", "du", "noch", "ge\u00b7lebt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$,", "PRELS", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von dir gefordert nichts und nichts erstrebt,", "tokens": ["Von", "dir", "ge\u00b7for\u00b7dert", "nichts", "und", "nichts", "er\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "PIS", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und deren Wunsch sich nie zu dir vermessen,", "tokens": ["Und", "de\u00b7ren", "Wunsch", "sich", "nie", "zu", "dir", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "PRF", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich denke dein, da alle dich vergessen!", "tokens": ["Ich", "den\u00b7ke", "dein", ",", "da", "al\u00b7le", "dich", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,", "KOUS", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Durch ihrer lauten Festges\u00e4nge Chor", "tokens": ["Durch", "ih\u00b7rer", "lau\u00b7ten", "Fest\u00b7ge\u00b7s\u00e4n\u00b7ge", "Chor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "T\u00f6nt mir dein Sterbeseufzer tief und leise,", "tokens": ["T\u00f6nt", "mir", "dein", "Ster\u00b7be\u00b7seuf\u00b7zer", "tief", "und", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und aus der bl\u00fchenden Gestalten Kreise", "tokens": ["Und", "aus", "der", "bl\u00fc\u00b7hen\u00b7den", "Ge\u00b7stal\u00b7ten", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Taucht schmerzlich mir dein bleiches Bild empor!", "tokens": ["Taucht", "schmerz\u00b7lich", "mir", "dein", "blei\u00b7ches", "Bild", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}