{"textgrid.poem.31544": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir m\u00fcssen offen seyn / dieweil uns aufgeschlossen", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir m\u00fcssen offen seyn / dieweil uns aufgeschlossen", "tokens": ["Wir", "m\u00fcs\u00b7sen", "of\u00b7fen", "seyn", "/", "die\u00b7weil", "uns", "auf\u00b7ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF", "$(", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "des Welt-Erl\u00f6sers Tod / der unser Schl\u00fcssel ist.", "tokens": ["des", "Welt\u00b7Er\u00b7l\u00f6\u00b7sers", "Tod", "/", "der", "un\u00b7ser", "Schl\u00fcs\u00b7sel", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Auferstehung sich im Grab zum Sieg-Pracht r\u00fcst /", "tokens": ["Die", "Auf\u00b7er\u00b7ste\u00b7hung", "sich", "im", "Grab", "zum", "Sieg\u00b7Pracht", "r\u00fcst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPRART", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "macht Sterbend selbst dem Tod den gr\u00f6sten Spott und Bossen.", "tokens": ["macht", "Ster\u00b7bend", "selbst", "dem", "Tod", "den", "gr\u00f6s\u00b7ten", "Spott", "und", "Bos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Pfeil / mit dem er ihn traff / hat ihn selbst erschossen.", "tokens": ["Der", "Pfeil", "/", "mit", "dem", "er", "ihn", "traff", "/", "hat", "ihn", "selbst", "er\u00b7schos\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "PRELS", "PPER", "PPER", "VVFIN", "$(", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Tod as sich zu todt am Leben / ach der List!", "tokens": ["Der", "Tod", "as", "sich", "zu", "todt", "am", "Le\u00b7ben", "/", "ach", "der", "List", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PRF", "PTKA", "ADJD", "APPRART", "NN", "$(", "XY", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "gar willig geben wir auch her in kurzer Frist", "tokens": ["gar", "wil\u00b7lig", "ge\u00b7ben", "wir", "auch", "her", "in", "kur\u00b7zer", "Frist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "da\u00df uns-vertraute Pfand / die seines Siegs genossen.", "tokens": ["da\u00df", "un\u00b7sver\u00b7trau\u00b7te", "Pfand", "/", "die", "sei\u00b7nes", "Siegs", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$(", "ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Auf! auf! ihr schlaffenden / auf! helffet Triumphiren", "tokens": ["Auf", "!", "auf", "!", "ihr", "schlaf\u00b7fen\u00b7den", "/", "auf", "!", "helf\u00b7fet", "Tri\u00b7um\u00b7phi\u00b7ren"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "$.", "PTKVZ", "$.", "PPER", "VVFIN", "$(", "PTKVZ", "$.", "VVFIN", "NN"], "meter": "-+-+--+-++-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "dem Lebens-F\u00fcrsten bald! wir machen uns schon auf.", "tokens": ["dem", "Le\u00b7bens\u00b7F\u00fcrs\u00b7ten", "bald", "!", "wir", "ma\u00b7chen", "uns", "schon", "auf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Folgt dem Erz-Sieges-Held / wie vor im Leidens-Lauf /", "tokens": ["Folgt", "dem", "Er\u00b7zSie\u00b7ges\u00b7Held", "/", "wie", "vor", "im", "Lei\u00b7dens\u00b7Lauf", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "KOKOM", "APPR", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "jetzt in die Herrlichkeit / den Sieges-Pracht zu zieren.", "tokens": ["jetzt", "in", "die", "Herr\u00b7lich\u00b7keit", "/", "den", "Sie\u00b7ges\u00b7Pracht", "zu", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$(", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Am All-erstehungs-Tag / wird unser aller Mund", "tokens": ["Am", "All\u00b7er\u00b7ste\u00b7hungs\u00b7Tag", "/", "wird", "un\u00b7ser", "al\u00b7ler", "Mund"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$(", "VAFIN", "PPOSAT", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "euch geben all' hervor / wie auch des Meeres Schlund.", "tokens": ["euch", "ge\u00b7ben", "all'", "her\u00b7vor", "/", "wie", "auch", "des", "Mee\u00b7res", "Schlund", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKVZ", "$(", "KOKOM", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}