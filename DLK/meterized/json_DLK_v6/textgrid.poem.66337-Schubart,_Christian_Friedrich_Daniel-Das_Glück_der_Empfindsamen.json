{"textgrid.poem.66337": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Das Gl\u00fcck der Empfindsamen", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weg, Plutus, mit der goldnen Gabe!", "tokens": ["Weg", ",", "Plu\u00b7tus", ",", "mit", "der", "gold\u00b7nen", "Ga\u00b7be", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bin ich nicht reich genug? Ich habe", "tokens": ["Bin", "ich", "nicht", "reich", "ge\u00b7nug", "?", "Ich", "ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "ADV", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Herze voll Gef\u00fchl.", "tokens": ["Ein", "Her\u00b7ze", "voll", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da nimm dein Gold! und gib's den Reichen,", "tokens": ["Da", "nimm", "dein", "Gold", "!", "und", "gib's", "den", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPOSAT", "NN", "$.", "KON", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die steinernen Kolossen gleichen!", "tokens": ["Die", "stei\u00b7ner\u00b7nen", "Ko\u00b7los\u00b7sen", "glei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich habe schon zu viel.", "tokens": ["Ich", "ha\u00b7be", "schon", "zu", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKA", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Fr\u00fchlingstag, ein Sommermorgen", "tokens": ["Ein", "Fr\u00fch\u00b7lings\u00b7tag", ",", "ein", "Som\u00b7mer\u00b7mor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zerstreuet alle meine Sorgen;", "tokens": ["Zer\u00b7streu\u00b7et", "al\u00b7le", "mei\u00b7ne", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es darf die Lerche nur", "tokens": ["Es", "darf", "die", "Ler\u00b7che", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hoch in den blauen L\u00fcften trillern,", "tokens": ["Hoch", "in", "den", "blau\u00b7en", "L\u00fcf\u00b7ten", "tril\u00b7lern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So wandl' ich froher in der stillern", "tokens": ["So", "wandl'", "ich", "fro\u00b7her", "in", "der", "stil\u00b7lern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Thau bedeckten Flur.", "tokens": ["Mit", "Thau", "be\u00b7deck\u00b7ten", "Flur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gedr\u00fcckt vom widrigen Geschicke,", "tokens": ["Ge\u00b7dr\u00fcckt", "vom", "wid\u00b7ri\u00b7gen", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lass' ich das Stadtget\u00f6s, und pfl\u00fccke", "tokens": ["Lass'", "ich", "das", "Stadt\u00b7ge\u00b7t\u00f6s", ",", "und", "pfl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Bl\u00fcmchen auf der Au'.", "tokens": ["Ein", "Bl\u00fcm\u00b7chen", "auf", "der", "Au'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "F\u00e4llt auch ein Z\u00e4hrchen hin und wieder", "tokens": ["F\u00e4llt", "auch", "ein", "Z\u00e4hr\u00b7chen", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auf's wei\u00dfe Wiesenbl\u00fcmchen nieder;", "tokens": ["Auf's", "wei\u00b7\u00dfe", "Wie\u00b7sen\u00b7bl\u00fcm\u00b7chen", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So denk' ich, es sei Thau.", "tokens": ["So", "denk'", "ich", ",", "es", "sei", "Thau", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Oft spiel' ich klagend auf dem Fl\u00fcgel,", "tokens": ["Oft", "spiel'", "ich", "kla\u00b7gend", "auf", "dem", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Luna gl\u00e4nzt. Von Sions H\u00fcgel", "tokens": ["Wenn", "Lu\u00b7na", "gl\u00e4nzt", ".", "Von", "Si\u00b7ons", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "$.", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "K\u00f6mmt G\u00f6ttin ", "tokens": ["K\u00f6mmt", "G\u00f6t\u00b7tin"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und haucht Begeistrung in die Finger,", "tokens": ["Und", "haucht", "Be\u00b7geis\u00b7trung", "in", "die", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und jenes Lebens Trost. Geringer", "tokens": ["Und", "je\u00b7nes", "Le\u00b7bens", "Trost", ".", "Ge\u00b7rin\u00b7ger"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PDAT", "NN", "NN", "$.", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wird dann der Schmerz durch sie.", "tokens": ["Wird", "dann", "der", "Schmerz", "durch", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "J\u00fcngst w\u00fcnscht' ich mir den Tod. Da lauschte", "tokens": ["J\u00fcngst", "w\u00fcnscht'", "ich", "mir", "den", "Tod", ".", "Da", "lauschte"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein M\u00e4dchen in dem Busch und rauschte", "tokens": ["Mein", "M\u00e4d\u00b7chen", "in", "dem", "Busch", "und", "rauschte"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hervor im Sonnenhut;", "tokens": ["Her\u00b7vor", "im", "Son\u00b7nen\u00b7hut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gleich seufzt' ich nimmer um mein Ende;", "tokens": ["Gleich", "seufzt'", "ich", "nim\u00b7mer", "um", "mein", "En\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn ach, sie dr\u00fcckte mir die H\u00e4nde", "tokens": ["Denn", "ach", ",", "sie", "dr\u00fcck\u00b7te", "mir", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sprach: Ich bin dir gut.", "tokens": ["Und", "sprach", ":", "Ich", "bin", "dir", "gut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wenn Arme an den Dornenst\u00e4ben", "tokens": ["Wenn", "Ar\u00b7me", "an", "den", "Dor\u00b7nen\u00b7st\u00e4\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gekr\u00fcmmt vor meiner H\u00fctte beben,", "tokens": ["Ge\u00b7kr\u00fcmmt", "vor", "mei\u00b7ner", "H\u00fct\u00b7te", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da klopft mir zwar die Brust;", "tokens": ["Da", "klopft", "mir", "zwar", "die", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch, wenn ich eine kleine Gabe", "tokens": ["Doch", ",", "wenn", "ich", "ei\u00b7ne", "klei\u00b7ne", "Ga\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bei eigner Armuth \u00fcbrig habe,", "tokens": ["Bei", "eig\u00b7ner", "Ar\u00b7muth", "\u00fcb\u00b7rig", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So f\u00fchl' ich Engellust.", "tokens": ["So", "f\u00fchl'", "ich", "En\u00b7gel\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich wohne gern in meiner H\u00fctte.", "tokens": ["Ich", "woh\u00b7ne", "gern", "in", "mei\u00b7ner", "H\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gew\u00e4hre mir nur eine Bitte,", "tokens": ["Ge\u00b7w\u00e4h\u00b7re", "mir", "nur", "ei\u00b7ne", "Bit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wohlth\u00e4tige Natur!", "tokens": ["Wohlt\u00b7h\u00e4\u00b7ti\u00b7ge", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nie will ich mich der Armuth sch\u00e4men;", "tokens": ["Nie", "will", "ich", "mich", "der", "Ar\u00b7muth", "sch\u00e4\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du darfst mir alles, alles nehmen,", "tokens": ["Du", "darfst", "mir", "al\u00b7les", ",", "al\u00b7les", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$,", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Herze la\u00df mir nur!", "tokens": ["Mein", "Her\u00b7ze", "la\u00df", "mir", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VVIMP", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}