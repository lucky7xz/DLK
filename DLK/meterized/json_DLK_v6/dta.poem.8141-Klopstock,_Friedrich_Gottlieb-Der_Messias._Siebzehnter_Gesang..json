{"dta.poem.8141": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Der Messias.  \n Siebzehnter Gesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Didymus hatte sich lange von seinen Br\u00fcdern gesondert,", "tokens": ["Di\u00b7dy\u00b7mus", "hat\u00b7te", "sich", "lan\u00b7ge", "von", "sei\u00b7nen", "Br\u00fc\u00b7dern", "ge\u00b7son\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Jezo kehrt\u2019 er zur\u00fcck, und kam zu der H\u00fctt\u2019 an dem Tempel.", "tokens": ["Je\u00b7zo", "kehrt'", "er", "zu\u00b7r\u00fcck", ",", "und", "kam", "zu", "der", "H\u00fctt'", "an", "dem", "Tem\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aber er s\u00e4umet\u2019, und gieng nicht hinein, und wandelt\u2019 am Eingang", "tokens": ["A\u00b7ber", "er", "s\u00e4u\u00b7met'", ",", "und", "gieng", "nicht", "hin\u00b7ein", ",", "und", "wan\u00b7delt'", "am", "Ein\u00b7gang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KON", "VVFIN", "PTKNEG", "PTKVZ", "$,", "KON", "VVFIN", "APPRART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Unter den Palmen. Er gieng jezt, lehnte sich jezt an der Palmen", "tokens": ["Un\u00b7ter", "den", "Pal\u00b7men", ".", "Er", "gieng", "jezt", ",", "lehn\u00b7te", "sich", "jezt", "an", "der", "Pal\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "PPER", "VVFIN", "ADV", "$,", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Eine. Bald h\u00f6rt\u2019 er sie singen. Da kam er, und blieb an der Pforte", "tokens": ["Ei\u00b7ne", ".", "Bald", "h\u00f6rt'", "er", "sie", "sin\u00b7gen", ".", "Da", "kam", "er", ",", "und", "blieb", "an", "der", "Pfor\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$.", "ADV", "VVFIN", "PPER", "PPER", "VVINF", "$.", "ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Stehen. Sie sangen ein Lied der Auserstehung, der neuen", "tokens": ["Ste\u00b7hen", ".", "Sie", "san\u00b7gen", "ein", "Lied", "der", "Aus\u00b7er\u00b7ste\u00b7hung", ",", "der", "neu\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Lieder eins, wie am Throne die Seelen der M\u00e4rtyrer singen.", "tokens": ["Lie\u00b7der", "eins", ",", "wie", "am", "Thro\u00b7ne", "die", "See\u00b7len", "der", "M\u00e4r\u00b7ty\u00b7rer", "sin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "PWAV", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}}, "stanza.2": {"line.1": {"text": "Jesus Christus erstand! Er wird die Seinen erwecken!", "tokens": ["Je\u00b7sus", "Chris\u00b7tus", "er\u00b7stand", "!", "Er", "wird", "die", "Sei\u00b7nen", "er\u00b7we\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Seine Kindlein werden nicht ewig im Schoo\u00dfe der Erde", "tokens": ["Sei\u00b7ne", "Kin\u00b7dlein", "wer\u00b7den", "nicht", "e\u00b7wig", "im", "Schoo\u00b7\u00dfe", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Liegen, entstellt von der Hand der Verwesung. Die Stimme des", "tokens": ["Lie\u00b7gen", ",", "ent\u00b7stellt", "von", "der", "Hand", "der", "Ver\u00b7we\u00b7sung", ".", "Die", "Stim\u00b7me", "des"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$.", "ART", "NN", "ART"], "meter": "+--+--+--+--+--", "measure": "diphilius"}, "line.4": {"text": "Segens", "tokens": ["Se\u00b7gens"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Wird ert\u00f6nen, vor ihr verstummen des Fluches letzter", "tokens": ["Wird", "er\u00b7t\u00f6\u00b7nen", ",", "vor", "ihr", "ver\u00b7stum\u00b7men", "des", "Flu\u00b7ches", "letz\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVINF", "$,", "APPR", "PPER", "VVFIN", "ART", "NN", "ADJA"], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Laut! Erzengel werden sich freun, und leuchtender strahlen", "tokens": ["Laut", "!", "Er\u00b7zen\u00b7gel", "wer\u00b7den", "sich", "freun", ",", "und", "leuch\u00b7ten\u00b7der", "strah\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "$.", "NN", "VAFIN", "PRF", "VVINF", "$,", "KON", "ADJA", "NN"], "meter": "+---+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Von den s\u00fc\u00dfen Entz\u00fcckungen \u00fcber die Todten, die leben!", "tokens": ["Von", "den", "s\u00fc\u00b7\u00dfen", "Ent\u00b7z\u00fc\u00b7ckun\u00b7gen", "\u00fc\u00b7ber", "die", "Tod\u00b7ten", ",", "die", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,", "PRELS", "VVINF", "$."], "meter": "--+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ach da\u00df jezo nicht mehr das Grab ist, nicht mehr die Verwesung", "tokens": ["Ach", "da\u00df", "je\u00b7zo", "nicht", "mehr", "das", "Grab", "ist", ",", "nicht", "mehr", "die", "Ver\u00b7we\u00b7sung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "KOUS", "ADV", "PTKNEG", "ADV", "ART", "NN", "VAFIN", "$,", "PTKNEG", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Herrscht, noch in Gr\u00fcften zerst\u00f6rt der hohen Seele Geno\u00df liegt!", "tokens": ["Herrscht", ",", "noch", "in", "Gr\u00fcf\u00b7ten", "zer\u00b7st\u00f6rt", "der", "ho\u00b7hen", "See\u00b7le", "Ge\u00b7no\u00df", "liegt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "NE", "VVFIN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Wehet, Winde, von Morgen, und bringt den Staub der Zerst\u00f6rung!", "tokens": ["We\u00b7het", ",", "Win\u00b7de", ",", "von", "Mor\u00b7gen", ",", "und", "bringt", "den", "Staub", "der", "Zer\u00b7st\u00f6\u00b7rung", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "APPR", "NN", "$,", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.11": {"text": "Bringt der Zerst\u00f6rung Staub, ihr wehenden Winde, von Abend!", "tokens": ["Bringt", "der", "Zer\u00b7st\u00f6\u00b7rung", "Staub", ",", "ihr", "we\u00b7hen\u00b7den", "Win\u00b7de", ",", "von", "A\u00b7bend", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "$."], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Brause, Sturm der Mitternacht, und bringe die Tr\u00fcmmern!", "tokens": ["Brau\u00b7se", ",", "Sturm", "der", "Mit\u00b7ter\u00b7nacht", ",", "und", "brin\u00b7ge", "die", "Tr\u00fcm\u00b7mern", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.13": {"text": "Jesus Christus erstand! Er wird die Seinen erwecken!", "tokens": ["Je\u00b7sus", "Chris\u00b7tus", "er\u00b7stand", "!", "Er", "wird", "die", "Sei\u00b7nen", "er\u00b7we\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Seine Kindlein werden nicht ewig im Schoosse der Erde", "tokens": ["Sei\u00b7ne", "Kin\u00b7dlein", "wer\u00b7den", "nicht", "e\u00b7wig", "im", "Schoos\u00b7se", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.15": {"text": "Liegen, entstellt von der Hand der Verwesung. Wie Tr\u00e4umenden wird es", "tokens": ["Lie\u00b7gen", ",", "ent\u00b7stellt", "von", "der", "Hand", "der", "Ver\u00b7we\u00b7sung", ".", "Wie", "Tr\u00e4u\u00b7men\u00b7den", "wird", "es"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$.", "PWAV", "NN", "VAFIN", "PPER"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Dann uns seyn, wenn ins Leben der Engel wir wiederkehren.", "tokens": ["Dann", "uns", "seyn", ",", "wenn", "ins", "Le\u00b7ben", "der", "En\u00b7gel", "wir", "wie\u00b7der\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAINF", "$,", "KOUS", "APPRART", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.17": {"text": "Wehet, Winde, von Morgen, da\u00df wir in das Leben der Engel", "tokens": ["We\u00b7het", ",", "Win\u00b7de", ",", "von", "Mor\u00b7gen", ",", "da\u00df", "wir", "in", "das", "Le\u00b7ben", "der", "En\u00b7gel"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "APPR", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Wiederkehren! O s\u00e4us\u2019le die Todten Gottes her\u00fcber,", "tokens": ["Wie\u00b7der\u00b7keh\u00b7ren", "!", "O", "s\u00e4us'\u00b7le", "die", "Tod\u00b7ten", "Got\u00b7tes", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "VVFIN", "ART", "NN", "NN", "ADV", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.19": {"text": "Mittagswind, zu dem neugeschaffenen Paradiese.", "tokens": ["Mit\u00b7tags\u00b7wind", ",", "zu", "dem", "neu\u00b7ge\u00b7schaf\u00b7fe\u00b7nen", "Pa\u00b7ra\u00b7die\u00b7se", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Sieh, an der Pforte des ewigen Edens schrecket des Cherubs", "tokens": ["Sieh", ",", "an", "der", "Pfor\u00b7te", "des", "e\u00b7wi\u00b7gen", "E\u00b7dens", "schre\u00b7cket", "des", "Che\u00b7rubs"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.21": {"text": "Schweigen nie, droht nie die hohe Flamme des Schwerdtes!", "tokens": ["Schwei\u00b7gen", "nie", ",", "droht", "nie", "die", "ho\u00b7he", "Flam\u00b7me", "des", "Schwerd\u00b7tes", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Denn wir halten das Mahl mit dem Sohn, in der Lebensb\u00e4ume", "tokens": ["Denn", "wir", "hal\u00b7ten", "das", "Mahl", "mit", "dem", "Sohn", ",", "in", "der", "Le\u00b7bens\u00b7b\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.23": {"text": "K\u00fchle, das Mahl, in dem S\u00e4useln um uns der Gegenwart Gottes!", "tokens": ["K\u00fch\u00b7le", ",", "das", "Mahl", ",", "in", "dem", "S\u00e4u\u00b7seln", "um", "uns", "der", "Ge\u00b7gen\u00b7wart", "Got\u00b7tes", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,", "APPR", "ART", "NN", "APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Denn erstanden ist er, der bis zu dem Tode die Seinen", "tokens": ["Denn", "er\u00b7stan\u00b7den", "ist", "er", ",", "der", "bis", "zu", "dem", "To\u00b7de", "die", "Sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Liebte, bis zu dem Tod\u2019 am Kreuz! .. So hatte sie Thomas", "tokens": ["Lieb\u00b7te", ",", "bis", "zu", "dem", "Tod'", "am", "Kreuz", "!", "..", "So", "hat\u00b7te", "sie", "Tho\u00b7mas"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "APPRART", "NN", "$.", "$(", "ADV", "VAFIN", "PPER", "NE"], "meter": "+-+--+-+-+-++-", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Preisen geh\u00f6rt, und war auf die Schwelle gesunken. Er deckte", "tokens": ["Prei\u00b7sen", "ge\u00b7h\u00f6rt", ",", "und", "war", "auf", "die", "Schwel\u00b7le", "ge\u00b7sun\u00b7ken", ".", "Er", "deck\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KON", "VAFIN", "APPR", "ART", "NN", "VVPP", "$.", "PPER", "VVFIN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.27": {"text": "Mit der H\u00fclle sein Antlitz! ihm flo\u00df die Thr\u00e4ne, wie Blut dem", "tokens": ["Mit", "der", "H\u00fcl\u00b7le", "sein", "Ant\u00b7litz", "!", "ihm", "flo\u00df", "die", "Thr\u00e4\u00b7ne", ",", "wie", "Blut", "dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "NN", "ART"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.28": {"text": "Flie\u00dft, der am Leben verzweifelnd im Kampfgefilde gestreckt liegt,", "tokens": ["Flie\u00dft", ",", "der", "am", "Le\u00b7ben", "ver\u00b7zwei\u00b7felnd", "im", "Kampf\u00b7ge\u00b7fil\u00b7de", "ge\u00b7streckt", "liegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "APPRART", "NN", "VVPP", "APPRART", "NN", "VVPP", "VVFIN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.29": {"text": "Und, ihr Gef\u00e4hrt, den Siegsruf h\u00f6rt der Streiter f\u00fcr Freyheit.", "tokens": ["Und", ",", "ihr", "Ge\u00b7f\u00e4hrt", ",", "den", "Siegs\u00b7ruf", "h\u00f6rt", "der", "Strei\u00b7ter", "f\u00fcr", "Frey\u00b7heit", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Noch vermocht er nicht aufzustehn. In sein m\u00fcdes Gebein drang", "tokens": ["Noch", "ver\u00b7mocht", "er", "nicht", "auf\u00b7zu\u00b7stehn", ".", "In", "sein", "m\u00fc\u00b7des", "Ge\u00b7bein", "drang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "VVINF", "$.", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.31": {"text": "Str\u00f6mender Duft der Mitternacht. Er f\u00fchlt\u2019 ihn nicht, weinte,", "tokens": ["Str\u00f6\u00b7men\u00b7der", "Duft", "der", "Mit\u00b7ter\u00b7nacht", ".", "Er", "f\u00fchlt'", "ihn", "nicht", ",", "wein\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "VVFIN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Weinte laut, mit Wehmuthschauer auf Wehmuthschauer,", "tokens": ["Wein\u00b7te", "laut", ",", "mit", "Weh\u00b7muth\u00b7schau\u00b7er", "auf", "Weh\u00b7muth\u00b7schau\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.33": {"text": "Da\u00df ihm die ganze Seele zerflo\u00df. Er ri\u00df sich mit Eil\u2019 auf,", "tokens": ["Da\u00df", "ihm", "die", "gan\u00b7ze", "See\u00b7le", "zer\u00b7flo\u00df", ".", "Er", "ri\u00df", "sich", "mit", "Eil'", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.34": {"text": "Ging zu den Br\u00fcdern hinein. Nun sahen sie endlich wieder", "tokens": ["Ging", "zu", "den", "Br\u00fc\u00b7dern", "hin\u00b7ein", ".", "Nun", "sa\u00b7hen", "sie", "end\u00b7lich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+--+--+-+--+-+-", "measure": "dactylic.di.plus"}, "line.35": {"text": "Thomas, ihren Bruder, und kamen mit ihrer Wonne", "tokens": ["Tho\u00b7mas", ",", "ih\u00b7ren", "Bru\u00b7der", ",", "und", "ka\u00b7men", "mit", "ih\u00b7rer", "Won\u00b7ne"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.36": {"text": "Lebenswort ihm entgegen. Er h\u00f6rt\u2019 es, und lange verstummt\u2019 er.", "tokens": ["Le\u00b7bens\u00b7wort", "ihm", "ent\u00b7ge\u00b7gen", ".", "Er", "h\u00f6rt'", "es", ",", "und", "lan\u00b7ge", "ver\u00b7stummt'", "er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "$,", "KON", "ADV", "VVFIN", "PPER", "$."], "meter": "+----+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.37": {"text": "Aber es kehrete jezt in die Seel\u2019 ihm wieder des Leidens", "tokens": ["A\u00b7ber", "es", "keh\u00b7re\u00b7te", "jezt", "in", "die", "Seel'", "ihm", "wie\u00b7der", "des", "Lei\u00b7dens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "PPER", "ADV", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.38": {"text": "Furchtbare K\u00e4lte, senket\u2019 auf ihn den lastenden, starken,", "tokens": ["Furcht\u00b7ba\u00b7re", "K\u00e4l\u00b7te", ",", "sen\u00b7ket'", "auf", "ihn", "den", "las\u00b7ten\u00b7den", ",", "star\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "APPR", "PPER", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.39": {"text": "Eisernen Arm; er rufte: Seh ich in seinen H\u00e4nden", "tokens": ["Ei\u00b7ser\u00b7nen", "Arm", ";", "er", "ruf\u00b7te", ":", "Seh", "ich", "in", "sei\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "$.", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.40": {"text": "Nicht die Maale der N\u00e4gel, und leg\u2019 ich in diese Maale", "tokens": ["Nicht", "die", "Maa\u00b7le", "der", "N\u00e4\u00b7gel", ",", "und", "leg'", "ich", "in", "die\u00b7se", "Maa\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.41": {"text": "Meine Finger nicht, und nicht in seine Seite", "tokens": ["Mei\u00b7ne", "Fin\u00b7ger", "nicht", ",", "und", "nicht", "in", "sei\u00b7ne", "Sei\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKNEG", "$,", "KON", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.42": {"text": "Meine Hand; so glaub\u2019 ich es nicht! Der H\u00f6renden Wange", "tokens": ["Mei\u00b7ne", "Hand", ";", "so", "glaub'", "ich", "es", "nicht", "!", "Der", "H\u00f6\u00b7ren\u00b7den", "Wan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.43": {"text": "Gl\u00fchete, wurde bleich. Schon rauschten der Cherubim Fl\u00fcgel", "tokens": ["Gl\u00fc\u00b7he\u00b7te", ",", "wur\u00b7de", "bleich", ".", "Schon", "rauschten", "der", "Che\u00b7ru\u00b7bim", "Fl\u00fc\u00b7gel"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VAFIN", "ADJD", "$.", "ADV", "VVFIN", "ART", "NE", "NN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.44": {"text": "Unter den Palmen der H\u00fctte, schon tr\u00e4ufelt\u2019 ihr Auge von Wonne!", "tokens": ["Un\u00b7ter", "den", "Pal\u00b7men", "der", "H\u00fct\u00b7te", ",", "schon", "tr\u00e4u\u00b7felt'", "ihr", "Au\u00b7ge", "von", "Won\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.45": {"text": "Schon erbarmete sich des Gottvers\u00f6hners Erbarmung!", "tokens": ["Schon", "er\u00b7bar\u00b7me\u00b7te", "sich", "des", "Gott\u00b7ver\u00b7s\u00f6h\u00b7ners", "Er\u00b7bar\u00b7mung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.46": {"text": "Und der G\u00f6ttliche stand vor seinen J\u00fcngern. So sch\u00f6pfen", "tokens": ["Und", "der", "G\u00f6tt\u00b7li\u00b7che", "stand", "vor", "sei\u00b7nen", "J\u00fcn\u00b7gern", ".", "So", "sch\u00f6p\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "ADV", "VVFIN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.47": {"t