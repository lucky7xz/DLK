{"dta.poem.12050": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht durch Beweise kannst du st\u00fctzen deinen Glauben,", "tokens": ["Nicht", "durch", "Be\u00b7wei\u00b7se", "kannst", "du", "st\u00fct\u00b7zen", "dei\u00b7nen", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch Widerlegungen ihm auch die Macht nicht rauben.", "tokens": ["Durch", "Wi\u00b7der\u00b7le\u00b7gun\u00b7gen", "ihm", "auch", "die", "Macht", "nicht", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Mit Worten kannst du ihn verh\u00fcllen und bedecken,", "tokens": ["Mit", "Wor\u00b7ten", "kannst", "du", "ihn", "ver\u00b7h\u00fcl\u00b7len", "und", "be\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht ihn begraben, noch von Todten auferwecken.", "tokens": ["Nicht", "ihn", "be\u00b7gra\u00b7ben", ",", "noch", "von", "Tod\u00b7ten", "au\u00b7fer\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "VVPP", "$,", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Oft, was ihn sichern soll, wird ihn nur irre machen,", "tokens": ["Oft", ",", "was", "ihn", "si\u00b7chern", "soll", ",", "wird", "ihn", "nur", "ir\u00b7re", "ma\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,", "VAFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was bet\u00e4uben ihn, davon wird er erwachen.", "tokens": ["Und", "was", "be\u00b7t\u00e4u\u00b7ben", "ihn", ",", "da\u00b7von", "wird", "er", "er\u00b7wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "$,", "PAV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Er steht mit ewiger allgegenw\u00e4rt'ger Macht", "tokens": ["Er", "steht", "mit", "e\u00b7wi\u00b7ger", "all\u00b7ge\u00b7gen\u00b7w\u00e4rt'\u00b7ger", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Sonn' an deinem Tag, als Stern in deiner Nacht.", "tokens": ["Als", "Sonn'", "an", "dei\u00b7nem", "Tag", ",", "als", "Stern", "in", "dei\u00b7ner", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "$,", "KOUS", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was auch bei Nacht und Tag dein Auge mache blind,", "tokens": ["Was", "auch", "bei", "Nacht", "und", "Tag", "dein", "Au\u00b7ge", "ma\u00b7che", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "KON", "NN", "PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du wei\u00dft da\u00df \u00fcber dir doch Sonn' und Sterne sind.", "tokens": ["Du", "wei\u00dft", "da\u00df", "\u00fc\u00b7ber", "dir", "doch", "Sonn'", "und", "Ster\u00b7ne", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "APPR", "PPER", "ADV", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}