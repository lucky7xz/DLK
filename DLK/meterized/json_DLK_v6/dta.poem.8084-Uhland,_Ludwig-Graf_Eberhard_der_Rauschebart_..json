{"dta.poem.8084": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Graf Eberhard der Rauschebart .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Ist denn im Schwabenlande verschollen aller Sang,", "tokens": ["Ist", "denn", "im", "Schwa\u00b7ben\u00b7lan\u00b7de", "ver\u00b7schol\u00b7len", "al\u00b7ler", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wo einst so hell vom Staufen die Ritterharfe klang?", "tokens": ["Wo", "einst", "so", "hell", "vom", "Stau\u00b7fen", "die", "Rit\u00b7ter\u00b7har\u00b7fe", "klang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ADJD", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und wenn er nicht verschollen, warum vergi\u00dft er ganz", "tokens": ["Und", "wenn", "er", "nicht", "ver\u00b7schol\u00b7len", ",", "wa\u00b7rum", "ver\u00b7gi\u00dft", "er", "ganz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "VVINF", "$,", "PWAV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der tapfern V\u00e4ter Thaten, der alten Waffen Glanz?", "tokens": ["Der", "tap\u00b7fern", "V\u00e4\u00b7ter", "Tha\u00b7ten", ",", "der", "al\u00b7ten", "Waf\u00b7fen", "Glanz", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Man lispelt leichte Liedchen, man spitzt manch Sinngedicht,", "tokens": ["Man", "lis\u00b7pelt", "leich\u00b7te", "Lied\u00b7chen", ",", "man", "spitzt", "manch", "Sinn\u00b7ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJA", "NN", "$,", "PIS", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Man h\u00f6hnt die holden Frauen, des alten Liedes Licht;", "tokens": ["Man", "h\u00f6hnt", "die", "hol\u00b7den", "Frau\u00b7en", ",", "des", "al\u00b7ten", "Lie\u00b7des", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wo r\u00fcstig Heldenleben l\u00e4ngst auf Beschw\u00f6rung lauscht,", "tokens": ["Wo", "r\u00fcs\u00b7tig", "Hel\u00b7den\u00b7le\u00b7ben", "l\u00e4ngst", "auf", "Be\u00b7schw\u00f6\u00b7rung", "lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da trippelt man vor\u00fcber und schauert, wenn es rauscht.", "tokens": ["Da", "trip\u00b7pelt", "man", "vor\u00b7\u00fc\u00b7ber", "und", "schau\u00b7ert", ",", "wenn", "es", "rauscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKVZ", "KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Brich denn aus deinem Sarge, steig aus dem d\u00fcstern Chor", "tokens": ["Brich", "denn", "aus", "dei\u00b7nem", "Sar\u00b7ge", ",", "steig", "aus", "dem", "d\u00fcs\u00b7tern", "Chor"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mit deinem Heldensohne, du Rauschebart, hervor! ", "tokens": ["Mit", "dei\u00b7nem", "Hel\u00b7den\u00b7soh\u00b7ne", ",", "du", "Rau\u00b7sche\u00b7bart", ",", "her\u00b7vor", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Du schlugst dich unverw\u00fcstlich noch greise Jahr\u2019 entlang,", "tokens": ["Du", "schlugst", "dich", "un\u00b7ver\u00b7w\u00fcst\u00b7lich", "noch", "grei\u00b7se", "Jahr'", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Brich auch durch unsre Zeiten mit hellem Schwerdtesklang!", "tokens": ["Brich", "auch", "durch", "uns\u00b7re", "Zei\u00b7ten", "mit", "hel\u00b7lem", "Schwerd\u00b7tes\u00b7klang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}