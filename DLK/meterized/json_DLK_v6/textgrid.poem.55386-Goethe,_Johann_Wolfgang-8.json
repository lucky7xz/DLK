{"textgrid.poem.55386": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "8", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Personen ganz verschieden", "tokens": ["Zwei", "Per\u00b7so\u00b7nen", "ganz", "ver\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "ADV", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Luden sich bei mir zu Tafel,", "tokens": ["Lu\u00b7den", "sich", "bei", "mir", "zu", "Ta\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Diesmal lebten sie in Frieden,", "tokens": ["Dies\u00b7mal", "leb\u00b7ten", "sie", "in", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fuchs und Kranich, sagt die Fabel.", "tokens": ["Fuchs", "und", "Kra\u00b7nich", ",", "sagt", "die", "Fa\u00b7bel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Beiden macht ich was zurechte,", "tokens": ["Bei\u00b7den", "macht", "ich", "was", "zu\u00b7rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rupfte gleich die j\u00fcngsten Tauben;", "tokens": ["Rupf\u00b7te", "gleich", "die", "j\u00fcng\u00b7sten", "Tau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil er von Schakals Geschlechte,", "tokens": ["Weil", "er", "von", "Scha\u00b7kals", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Legt ich bei geschwollne Trauben.", "tokens": ["Legt", "ich", "bei", "ge\u00b7schwoll\u00b7ne", "Trau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Langgeh\u00e4lstes Glasgef\u00e4\u00dfe", "tokens": ["Lang\u00b7ge\u00b7h\u00e4ls\u00b7tes", "Glas\u00b7ge\u00b7f\u00e4\u00b7\u00dfe"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Setzt ich unges\u00e4umt dagegen,", "tokens": ["Setzt", "ich", "un\u00b7ge\u00b7s\u00e4umt", "da\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo sich klar im Elemente", "tokens": ["Wo", "sich", "klar", "im", "E\u00b7le\u00b7men\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ADJD", "APPRART", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.4": {"text": "Gold- und Silberfischlein regen.", "tokens": ["Gold", "und", "Sil\u00b7ber\u00b7fisc\u00b7hlein", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "H\u00e4ttet ihr den Fuchs gesehen", "tokens": ["H\u00e4t\u00b7tet", "ihr", "den", "Fuchs", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NE", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der flachen Sch\u00fcssel hausen,", "tokens": ["Auf", "der", "fla\u00b7chen", "Sch\u00fcs\u00b7sel", "hau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Neidisch m\u00fc\u00dftet ihr gestehen:", "tokens": ["Nei\u00b7disch", "m\u00fc\u00df\u00b7tet", "ihr", "ge\u00b7ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welch ein Appetit zum Schmausen!", "tokens": ["Welch", "ein", "Ap\u00b7pe\u00b7tit", "zum", "Schmau\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn der Vogel ganz bed\u00e4chtig", "tokens": ["Wenn", "der", "Vo\u00b7gel", "ganz", "be\u00b7d\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NE", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich auf einem Fu\u00dfe wiegte,", "tokens": ["Sich", "auf", "ei\u00b7nem", "Fu\u00b7\u00dfe", "wieg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hals und Schnabel, zart und schm\u00e4chtig,", "tokens": ["Hals", "und", "Schna\u00b7bel", ",", "zart", "und", "schm\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zierlich nach den Fischlein schmiegte.", "tokens": ["Zier\u00b7lich", "nach", "den", "Fisc\u00b7hlein", "schmieg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dankend freuten sie beim Wandern", "tokens": ["Dan\u00b7kend", "freu\u00b7ten", "sie", "beim", "Wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich der Tauben, sich der Fischchen;", "tokens": ["Sich", "der", "Tau\u00b7ben", ",", "sich", "der", "Fischchen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$,", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder spottete des andern", "tokens": ["Je\u00b7der", "spot\u00b7te\u00b7te", "des", "an\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als gen\u00e4hrt am Katzentischchen.", "tokens": ["Als", "ge\u00b7n\u00e4hrt", "am", "Kat\u00b7zen\u00b7ti\u00b7schchen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Willst nicht Salz und Schmalz verlieren,", "tokens": ["Willst", "nicht", "Salz", "und", "Schmalz", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00dft, gem\u00e4\u00df den Urgeschichten,", "tokens": ["Mu\u00dft", ",", "ge\u00b7m\u00e4\u00df", "den", "Ur\u00b7ge\u00b7schich\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Leute willst gastieren,", "tokens": ["Wenn", "die", "Leu\u00b7te", "willst", "gas\u00b7tie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich nach Schnauz und Schnabel richten.", "tokens": ["Dich", "nach", "Schnauz", "und", "Schna\u00b7bel", "rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}