{"dta.poem.19549": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Ohne Datum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Du scherzest, da\u00df ein Datum ich verga\u00df,", "tokens": ["Du", "scher\u00b7zest", ",", "da\u00df", "ein", "Da\u00b7tum", "ich", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.2": {"text": "Und meinst, ich d\u00fcrfte bei dem Stundenma\u00df", "tokens": ["Und", "meinst", ",", "ich", "d\u00fcrf\u00b7te", "bei", "dem", "Stun\u00b7den\u00b7ma\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit einem Federstriche mich verweilen.", "tokens": ["Mit", "ei\u00b7nem", "Fe\u00b7der\u00b7stri\u00b7che", "mich", "ver\u00b7wei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du schreibst: \u201eDatire k\u00fcnftig deine Zeilen!\u201c", "tokens": ["Du", "schreibst", ":", "\u201e", "Da\u00b7ti\u00b7re", "k\u00fcnf\u00b7tig", "dei\u00b7ne", "Zei\u00b7len", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "ADJD", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch war das Z\u00e4hlen meine Sache nie,", "tokens": ["Doch", "war", "das", "Z\u00e4h\u00b7len", "mei\u00b7ne", "Sa\u00b7che", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nach dem Wievielten such' ich stets vergebens,", "tokens": ["Nach", "dem", "Wie\u00b7viel\u00b7ten", "such'", "ich", "stets", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auch diese Zeilen, wie datir' ich sie?", "tokens": ["Auch", "die\u00b7se", "Zei\u00b7len", ",", "wie", "da\u00b7tir'", "ich", "sie", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,", "PWAV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "\u201eaus allen Augenblicken meines Lebens!\u201c", "tokens": ["\u201e", "aus", "al\u00b7len", "Au\u00b7gen\u00b7bli\u00b7cken", "mei\u00b7nes", "Le\u00b7bens", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PIAT", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kurz ist und eilig eines Menschen Tag,", "tokens": ["Kurz", "ist", "und", "ei\u00b7lig", "ei\u00b7nes", "Men\u00b7schen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "KON", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er dr\u00e4ngt, er pulst, er flutet, Schlag um Schlag,", "tokens": ["Er", "dr\u00e4ngt", ",", "er", "pulst", ",", "er", "flu\u00b7tet", ",", "Schlag", "um", "Schlag", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie eines Herzens ungest\u00fcmes Klopfen ...", "tokens": ["Wie", "ei\u00b7nes", "Her\u00b7zens", "un\u00b7ge\u00b7st\u00fc\u00b7mes", "Klop\u00b7fen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer teilt die Jagd des Bluts und seiner Tropfen?", "tokens": ["Wer", "teilt", "die", "Jagd", "des", "Bluts", "und", "sei\u00b7ner", "Trop\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es ist ein Sturm, der nie zur R\u00fcste geht,", "tokens": ["Es", "ist", "ein", "Sturm", ",", "der", "nie", "zur", "R\u00fcs\u00b7te", "geht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Wechselglut des Nehmens und des Gebens,", "tokens": ["Die", "Wech\u00b7sel\u00b7glut", "des", "Neh\u00b7mens", "und", "des", "Ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und meine Haare flattern windverweht", "tokens": ["Und", "mei\u00b7ne", "Haa\u00b7re", "flat\u00b7tern", "wind\u00b7ver\u00b7weht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In allen Augenblicken meines Lebens.", "tokens": ["In", "al\u00b7len", "Au\u00b7gen\u00b7bli\u00b7cken", "mei\u00b7nes", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zu ruhn ist mir versagt, es treibt mich fort,", "tokens": ["Zu", "ruhn", "ist", "mir", "ver\u00b7sagt", ",", "es", "treibt", "mich", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "PPER", "VVPP", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Stunde rennt \u2014 doch hab' ich einen Hort,", "tokens": ["Die", "Stun\u00b7de", "rennt", "doch", "hab'", "ich", "ei\u00b7nen", "Hort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den keine mir entf\u00fchrt, in deiner Treue;", "tokens": ["Den", "kei\u00b7ne", "mir", "ent\u00b7f\u00fchrt", ",", "in", "dei\u00b7ner", "Treu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPER", "VVPP", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie ist die alte wie die ewig neue,", "tokens": ["Sie", "ist", "die", "al\u00b7te", "wie", "die", "e\u00b7wig", "neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "KOKOM", "ART", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie ist die Rast in dieser Flucht und Flut,", "tokens": ["Sie", "ist", "die", "Rast", "in", "die\u00b7ser", "Flucht", "und", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PDAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein fromm Geleite leisen Fl\u00fcgelschwebens,", "tokens": ["Ein", "fromm", "Ge\u00b7lei\u00b7te", "lei\u00b7sen", "Fl\u00fc\u00b7gel\u00b7schwe\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Sie ist der Segen, der best\u00e4ndig ruht", "tokens": ["Sie", "ist", "der", "Se\u00b7gen", ",", "der", "be\u00b7st\u00e4n\u00b7dig", "ruht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Auf allen Augenblicken meines Lebens.", "tokens": ["Auf", "al\u00b7len", "Au\u00b7gen\u00b7bli\u00b7cken", "mei\u00b7nes", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich hemme die beschwingten Rosse nicht,", "tokens": ["Ich", "hem\u00b7me", "die", "be\u00b7schwing\u00b7ten", "Ros\u00b7se", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich freue mich, mit jedem neuen Licht", "tokens": ["Ich", "freu\u00b7e", "mich", ",", "mit", "je\u00b7dem", "neu\u00b7en", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Feld gestreckten Laufes zu durchmessen \u2014", "tokens": ["Das", "Feld", "ge\u00b7streck\u00b7ten", "Lau\u00b7fes", "zu", "durch\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein fernes, dunkles Gestern zu vergessen \u2014", "tokens": ["Ein", "fer\u00b7nes", ",", "dunk\u00b7les", "Ge\u00b7stern", "zu", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Ich fliege \u2014 hinter mir versinkt die Zeit \u2014", "tokens": ["Ich", "flie\u00b7ge", "hin\u00b7ter", "mir", "ver\u00b7sinkt", "die", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "APPR", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Morgensonnenstrahl verj\u00fcngten Strebens! ...", "tokens": ["Im", "Mor\u00b7gen\u00b7son\u00b7nen\u00b7strahl", "ver\u00b7j\u00fcng\u00b7ten", "Stre\u00b7bens", "!", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Vorbei! ... Nur du allein wei\u00dft noch Bescheid", "tokens": ["Vor\u00b7bei", "!", "...", "Nur", "du", "al\u00b7lein", "wei\u00dft", "noch", "Be\u00b7scheid"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "$(", "ADV", "PPER", "ADV", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Von allen Augenblicken meines Lebens.", "tokens": ["Von", "al\u00b7len", "Au\u00b7gen\u00b7bli\u00b7cken", "mei\u00b7nes", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}