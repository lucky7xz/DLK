{"textgrid.poem.47300": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Himmel thu' ich jede Nacht den Liebesruf,", "tokens": ["Zum", "Him\u00b7mel", "thu'", "ich", "je\u00b7de", "Nacht", "den", "Lie\u00b7bes\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sch\u00f6nheit Gottes voll, mit Macht den Liebesruf.", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "Got\u00b7tes", "voll", ",", "mit", "Macht", "den", "Lie\u00b7bes\u00b7ruf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "$,", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mir jeden Morgen Sonn' und Mond im Herzen tanzt,", "tokens": ["Mir", "je\u00b7den", "Mor\u00b7gen", "Sonn'", "und", "Mond", "im", "Her\u00b7zen", "tanzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "NN", "KON", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu Sonn' und Mond thu' ich erwacht den Liebesruf.", "tokens": ["Zu", "Sonn'", "und", "Mond", "thu'", "ich", "er\u00b7wacht", "den", "Lie\u00b7bes\u00b7ruf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf jeder Au' ergl\u00e4nzt ein Strahl von Gottes Licht,", "tokens": ["Auf", "je\u00b7der", "Au'", "er\u00b7gl\u00e4nzt", "ein", "Strahl", "von", "Got\u00b7tes", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich thu' an Gottes Sch\u00f6pferpracht den Liebesruf.", "tokens": ["Ich", "thu'", "an", "Got\u00b7tes", "Sch\u00f6p\u00b7fer\u00b7pracht", "den", "Lie\u00b7bes\u00b7ruf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Turteltaub' im Laub, erweckt von meinem Gru\u00df,", "tokens": ["Die", "Tur\u00b7tel\u00b7taub'", "im", "Laub", ",", "er\u00b7weckt", "von", "mei\u00b7nem", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Thut mir entgegen girrend sacht den Liebesruf.", "tokens": ["Thut", "mir", "ent\u00b7ge\u00b7gen", "gir\u00b7rend", "sacht", "den", "Lie\u00b7bes\u00b7ruf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dem Felsen, der zu deinem Preis mit Licht sich kr\u00f6nt,", "tokens": ["Dem", "Fel\u00b7sen", ",", "der", "zu", "dei\u00b7nem", "Preis", "mit", "Licht", "sich", "kr\u00f6nt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zuruf' ich, und er nimmt in acht den Liebesruf.", "tokens": ["Zu\u00b7ruf'", "ich", ",", "und", "er", "nimmt", "in", "acht", "den", "Lie\u00b7bes\u00b7ruf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KON", "PPER", "VVFIN", "APPR", "CARD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dir thu' ich f\u00fcr die Blum' im Feld, die sch\u00fcchtern schweigt,", "tokens": ["Dir", "thu'", "ich", "f\u00fcr", "die", "Blum'", "im", "Feld", ",", "die", "sch\u00fcch\u00b7tern", "schweigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "F\u00fcrs W\u00fcrmlein, das du stumm gemacht, den Liebesruf.", "tokens": ["F\u00fcrs", "W\u00fcrm\u00b7lein", ",", "das", "du", "stumm", "ge\u00b7macht", ",", "den", "Lie\u00b7bes\u00b7ruf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das Weltmeer preist mit Rauschen dich, doch ohne Wort;", "tokens": ["Das", "Welt\u00b7meer", "preist", "mit", "Rau\u00b7schen", "dich", ",", "doch", "oh\u00b7ne", "Wort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PPER", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich hab' in Worte ihm gebracht den Liebesruf.", "tokens": ["Ich", "hab'", "in", "Wor\u00b7te", "ihm", "ge\u00b7bracht", "den", "Lie\u00b7bes\u00b7ruf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "PPER", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dir thu' ich als das Laub am Baum, als Tropf' im Meer,", "tokens": ["Dir", "thu'", "ich", "als", "das", "Laub", "am", "Baum", ",", "als", "Tropf'", "im", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "ART", "NN", "APPRART", "NN", "$,", "KOUS", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dir als der Edelstein im Schacht den Liebesruf.", "tokens": ["Dir", "als", "der", "E\u00b7del\u00b7stein", "im", "Schacht", "den", "Lie\u00b7bes\u00b7ruf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich ward in allem alles, sah in allem Gott,", "tokens": ["Ich", "ward", "in", "al\u00b7lem", "al\u00b7les", ",", "sah", "in", "al\u00b7lem", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "PIS", "$,", "VVFIN", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und that, von Einheitglut entfacht, den Liebesruf.", "tokens": ["Und", "that", ",", "von", "Ein\u00b7heit\u00b7glut", "ent\u00b7facht", ",", "den", "Lie\u00b7bes\u00b7ruf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}