{"textgrid.poem.49617": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An die Nationalen", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr alle, die ihr treu und bieder seid,", "tokens": ["Ihr", "al\u00b7le", ",", "die", "ihr", "treu", "und", "bie\u00b7der", "seid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PRELS", "PPER", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und die ihr patriotisch uns verdammt,", "tokens": ["Und", "die", "ihr", "pat\u00b7ri\u00b7o\u00b7tisch", "uns", "ver\u00b7dammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und denen hinter'm scharfen Brillenglas", "tokens": ["Und", "de\u00b7nen", "hin\u00b7ter'm", "schar\u00b7fen", "Bril\u00b7len\u00b7glas"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das blaue Auge in Entr\u00fcstung flammt,", "tokens": ["Das", "blau\u00b7e", "Au\u00b7ge", "in", "Ent\u00b7r\u00fcs\u00b7tung", "flammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr hebt die H\u00e4nde drohend gegen uns,", "tokens": ["Ihr", "hebt", "die", "H\u00e4n\u00b7de", "dro\u00b7hend", "ge\u00b7gen", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schwarze Galle flie\u00dft euch ins Gem\u00fct,", "tokens": ["Und", "schwar\u00b7ze", "Gal\u00b7le", "flie\u00dft", "euch", "ins", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr sagt, da\u00df uns die Heimatliebe fehlt,", "tokens": ["Ihr", "sagt", ",", "da\u00df", "uns", "die", "Hei\u00b7mat\u00b7lie\u00b7be", "fehlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die euch so deutsch in tapfern Herzen gl\u00fcht.", "tokens": ["Die", "euch", "so", "deutsch", "in", "tap\u00b7fern", "Her\u00b7zen", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich bitte euch, ihr M\u00e4nner guter Art,", "tokens": ["Ich", "bit\u00b7te", "euch", ",", "ihr", "M\u00e4n\u00b7ner", "gu\u00b7ter", "Art", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ihr das Vaterland so br\u00fcnstig liebt,", "tokens": ["Wenn", "ihr", "das", "Va\u00b7ter\u00b7land", "so", "br\u00fcns\u00b7tig", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Warum ist keiner unter euch so deutsch,", "tokens": ["Wa\u00b7rum", "ist", "kei\u00b7ner", "un\u00b7ter", "euch", "so", "deutsch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "APPR", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df er der Wahrheit furchtlos Ehre gibt,", "tokens": ["Da\u00df", "er", "der", "Wahr\u00b7heit", "furcht\u00b7los", "Eh\u00b7re", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn unserm Land von oben Schaden droht?", "tokens": ["Wenn", "un\u00b7serm", "Land", "von", "o\u00b7ben", "Scha\u00b7den", "droht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Macht euch nicht schamrot der verdiente Hohn,", "tokens": ["Macht", "euch", "nicht", "scham\u00b7rot", "der", "ver\u00b7dien\u00b7te", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Der in dem L\u00e4cheln unsrer Nachbarn liegt?", "tokens": ["Der", "in", "dem", "L\u00e4\u00b7cheln", "uns\u00b7rer", "Nach\u00b7barn", "liegt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was steht ihr feige schweigend vor dem Thron?", "tokens": ["Was", "steht", "ihr", "fei\u00b7ge", "schwei\u00b7gend", "vor", "dem", "Thron", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sagt doch ein W\u00f6rtchen! Sagt, was jeder denkt!", "tokens": ["Sagt", "doch", "ein", "W\u00f6rt\u00b7chen", "!", "Sagt", ",", "was", "je\u00b7der", "denkt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$.", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00e4llt euch das Deutschtum in das Hosenbein?", "tokens": ["F\u00e4llt", "euch", "das", "Deutschtum", "in", "das", "Ho\u00b7sen\u00b7bein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und seid ihr bieder auch beim Festkommers,", "tokens": ["Und", "seid", "ihr", "bie\u00b7der", "auch", "beim", "Fest\u00b7kom\u00b7mers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "In F\u00fcrstens\u00e4len k\u00f6nnt ihr's nimmer sein.", "tokens": ["In", "F\u00fcrs\u00b7ten\u00b7s\u00e4\u00b7len", "k\u00f6nnt", "ih\u00b7r's", "nim\u00b7mer", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "Theaterhelden, schert euch fort! Und wenn", "tokens": ["The\u00b7a\u00b7ter\u00b7hel\u00b7den", ",", "schert", "euch", "fort", "!", "Und", "wenn"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "PTKVZ", "$.", "KON", "KOUS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur einem unter euch die Feigheit schwand,", "tokens": ["Nur", "ei\u00b7nem", "un\u00b7ter", "euch", "die", "Feig\u00b7heit", "schwand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann sei das Predigen euch gern erlaubt,", "tokens": ["Dann", "sei", "das", "Pre\u00b7di\u00b7gen", "euch", "gern", "er\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dann schw\u00e4tzt uns wieder von dem Vaterland.", "tokens": ["Dann", "schw\u00e4tzt", "uns", "wie\u00b7der", "von", "dem", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}