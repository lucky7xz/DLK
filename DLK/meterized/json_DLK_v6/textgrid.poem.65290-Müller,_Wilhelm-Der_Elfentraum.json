{"textgrid.poem.65290": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Elfentraum", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Nachtviolenkelchen eingeschlossen,", "tokens": ["In", "Nacht\u00b7vi\u00b7o\u00b7len\u00b7kel\u00b7chen", "ein\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verschliefen einen hei\u00dfen Tag die Elfen.", "tokens": ["Ver\u00b7schlie\u00b7fen", "ei\u00b7nen", "hei\u00b7\u00dfen", "Tag", "die", "El\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun \u00f6ffnen sie die schummertrunknen Augen", "tokens": ["Nun", "\u00f6ff\u00b7nen", "sie", "die", "schum\u00b7mer\u00b7trunk\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und blinzeln, weil zu nah die Funkenw\u00fcrmchen", "tokens": ["Und", "blin\u00b7zeln", ",", "weil", "zu", "nah", "die", "Fun\u00b7ken\u00b7w\u00fcrm\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PTKA", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Um ihre Lager schw\u00e4rmen. \u2013 Gut geschlafen?", "tokens": ["Um", "ih\u00b7re", "La\u00b7ger", "schw\u00e4r\u00b7men", ".", "\u2013", "Gut", "ge\u00b7schla\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVINF", "$.", "$(", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Fr\u00e4gt Ariel sein Liebchen Ariella. \u2013", "tokens": ["Fr\u00e4gt", "A\u00b7riel", "sein", "Lieb\u00b7chen", "A\u00b7riel\u00b7la", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "NE", "PPOSAT", "NN", "NE", "$.", "$("], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.7": {"text": "Ach nein, mein Herz, ich hatte bange Tr\u00e4ume.", "tokens": ["Ach", "nein", ",", "mein", "Herz", ",", "ich", "hat\u00b7te", "ban\u00b7ge", "Tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$,", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich sahe dich, du warst in einen Tropfen", "tokens": ["Ich", "sa\u00b7he", "dich", ",", "du", "warst", "in", "ei\u00b7nen", "Trop\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Eiskalten Thau, der tief versteckt im Kelche", "tokens": ["Eis\u00b7kal\u00b7ten", "Thau", ",", "der", "tief", "ver\u00b7steckt", "im", "Kel\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADJD", "VVPP", "APPRART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.10": {"text": "Der Nachtviole lag, hineingefallen.", "tokens": ["Der", "Nacht\u00b7vi\u00b7o\u00b7le", "lag", ",", "hin\u00b7ein\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ich schrie und rief zu H\u00fclfe, was von Elfen", "tokens": ["Ich", "schrie", "und", "rief", "zu", "H\u00fcl\u00b7fe", ",", "was", "von", "El\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Im ganzen Kelche war \u2013 sie kamen alle,", "tokens": ["Im", "gan\u00b7zen", "Kel\u00b7che", "war", "\u2013", "sie", "ka\u00b7men", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "$(", "PPER", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "So weit sie meine Stimme nur vernahmen,", "tokens": ["So", "weit", "sie", "mei\u00b7ne", "Stim\u00b7me", "nur", "ver\u00b7nah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Bis von den allerh\u00f6chsten Bl\u00e4tterspitzen \u2013", "tokens": ["Bis", "von", "den", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Bl\u00e4t\u00b7ter\u00b7spit\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ach ja, die Noth lehrt schreien, mein Geliebter!", "tokens": ["Ach", "ja", ",", "die", "Noth", "lehrt", "schrei\u00b7en", ",", "mein", "Ge\u00b7lieb\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "ART", "NN", "VVFIN", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und flugs hing eins sich an des andern Fl\u00fcgel,", "tokens": ["Und", "flugs", "hing", "eins", "sich", "an", "des", "an\u00b7dern", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Wie Glieder einer Kette sich verbindend,", "tokens": ["Wie", "Glie\u00b7der", "ei\u00b7ner", "Ket\u00b7te", "sich", "ver\u00b7bin\u00b7dend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und unsre Kette ward so lang, mein Herzchen,", "tokens": ["Und", "uns\u00b7re", "Ket\u00b7te", "ward", "so", "lang", ",", "mein", "Herz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "So lang, wie ich gesehn noch keine andre,", "tokens": ["So", "lang", ",", "wie", "ich", "ge\u00b7sehn", "noch", "kei\u00b7ne", "and\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPER", "VVPP", "ADV", "PIAT", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Selbst nicht bei unsres K\u00f6nigs Hochzeitfeier,", "tokens": ["Selbst", "nicht", "bei", "uns\u00b7res", "K\u00f6\u00b7nigs", "Hoch\u00b7zeit\u00b7fei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Im gro\u00dfen Reigen, welchen alle G\u00e4ste", "tokens": ["Im", "gro\u00b7\u00dfen", "Rei\u00b7gen", ",", "wel\u00b7chen", "al\u00b7le", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAT", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Mittanzen mu\u00dften auf dem Lilienplane.", "tokens": ["Mit\u00b7tan\u00b7zen", "mu\u00df\u00b7ten", "auf", "dem", "Li\u00b7li\u00b7en\u00b7pla\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Ich war das unterste der Glieder, wurde", "tokens": ["Ich", "war", "das", "un\u00b7ters\u00b7te", "der", "Glie\u00b7der", ",", "wur\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ART", "NN", "$,", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Hinabgelassen in den tiefen Tropfen,", "tokens": ["Hin\u00b7ab\u00b7ge\u00b7las\u00b7sen", "in", "den", "tie\u00b7fen", "Trop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Und sahe dich \u2013 du lagst und zappeltest", "tokens": ["Und", "sa\u00b7he", "dich", "\u2013", "du", "lagst", "und", "zap\u00b7pel\u00b7test"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Und strecktest sehnlich deine lieben Arme", "tokens": ["Und", "streck\u00b7test", "sehn\u00b7lich", "dei\u00b7ne", "lie\u00b7ben", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Zu mir empor \u2013 ich aber sehnt' und dehnte", "tokens": ["Zu", "mir", "em\u00b7por", "\u2013", "ich", "a\u00b7ber", "sehnt'", "und", "dehn\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKVZ", "$(", "PPER", "ADV", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Mich aus mit allen Kr\u00e4ften \u2013 Ach, vergebens!", "tokens": ["Mich", "aus", "mit", "al\u00b7len", "Kr\u00e4f\u00b7ten", "\u2013", "Ach", ",", "ver\u00b7ge\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "PIAT", "NN", "$(", "ITJ", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Die Kette war zu kurz, und alle Elfen", "tokens": ["Die", "Ket\u00b7te", "war", "zu", "kurz", ",", "und", "al\u00b7le", "El\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$,", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Schrien hinter mir: Sie rei\u00dft, sie rei\u00dft, die Kette!", "tokens": ["Schri\u00b7en", "hin\u00b7ter", "mir", ":", "Sie", "rei\u00dft", ",", "sie", "rei\u00dft", ",", "die", "Ket\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.31": {"text": "Da wacht' ich auf und lag in deinen Armen", "tokens": ["Da", "wacht'", "ich", "auf", "und", "lag", "in", "dei\u00b7nen", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Und mu\u00dfte dich mit meinen K\u00fcssen wecken,", "tokens": ["Und", "mu\u00df\u00b7te", "dich", "mit", "mei\u00b7nen", "K\u00fcs\u00b7sen", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Zu sehn, ob du auch wirklich unversehrt bist.", "tokens": ["Zu", "sehn", ",", "ob", "du", "auch", "wirk\u00b7lich", "un\u00b7ver\u00b7sehrt", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "PPER", "ADV", "ADJD", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}