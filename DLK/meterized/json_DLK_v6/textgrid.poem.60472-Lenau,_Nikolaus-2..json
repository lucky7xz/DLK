{"textgrid.poem.60472": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die V\u00f6gel fliehn geschwind", "tokens": ["Die", "V\u00f6\u00b7gel", "fliehn", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zum Nest im Wetterhauche,", "tokens": ["Zum", "Nest", "im", "Wet\u00b7ter\u00b7hau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch schleudert sie der Wind", "tokens": ["Doch", "schleu\u00b7dert", "sie", "der", "Wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Weitab von ihrem Strauche.", "tokens": ["Weit\u00b7ab", "von", "ih\u00b7rem", "Strau\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das Wild mit banger Hast", "tokens": ["Das", "Wild", "mit", "ban\u00b7ger", "Hast"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ist ins Geb\u00fcsch verkrochen;", "tokens": ["Ist", "ins", "Ge\u00b7b\u00fcsch", "ver\u00b7kro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Manch gr\u00fcnend frischer Ast", "tokens": ["Manch", "gr\u00fc\u00b7nend", "fri\u00b7scher", "Ast"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "St\u00fcrzt nieder, sturmgebrochen.", "tokens": ["St\u00fcrzt", "nie\u00b7der", ",", "sturm\u00b7ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das Heer der Wolken schweift", "tokens": ["Das", "Heer", "der", "Wol\u00b7ken", "schweift"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit roten Blitzesfahnen,", "tokens": ["Mit", "ro\u00b7ten", "Blit\u00b7zes\u00b7fah\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aufspielend wirbelt, pfeift", "tokens": ["Auf\u00b7spie\u00b7lend", "wir\u00b7belt", ",", "pfeift"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVPP", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Bande von Orkanen.", "tokens": ["Die", "Ban\u00b7de", "von", "Or\u00b7ka\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das B\u00e4chlein, sonst so mild,", "tokens": ["Das", "B\u00e4ch\u00b7lein", ",", "sonst", "so", "mild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ist au\u00dfer sich geraten,", "tokens": ["Ist", "au\u00b7\u00dfer", "sich", "ge\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Springt auf an B\u00e4umen wild,", "tokens": ["Springt", "auf", "an", "B\u00e4u\u00b7men", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verw\u00fcstend in die Saaten.", "tokens": ["Ver\u00b7w\u00fcs\u00b7tend", "in", "die", "Saa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Donner bricht herein,", "tokens": ["Der", "Don\u00b7ner", "bricht", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es kracht die Welt in Wettern,", "tokens": ["Es", "kracht", "die", "Welt", "in", "Wet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als wollt am Felsgestein", "tokens": ["Als", "wollt", "am", "Fels\u00b7ge\u00b7stein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Himmel sich zerschmettern.", "tokens": ["Der", "Him\u00b7mel", "sich", "zer\u00b7schmet\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Regen braust; nun schwand", "tokens": ["Der", "Re\u00b7gen", "braust", ";", "nun", "schwand"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das Tal in seiner Dichte;", "tokens": ["Das", "Tal", "in", "sei\u00b7ner", "Dich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verpf\u00e4hlt hat er das Land", "tokens": ["Ver\u00b7pf\u00e4hlt", "hat", "er", "das", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vor meinem Augenlichte.", "tokens": ["Vor", "mei\u00b7nem", "Au\u00b7gen\u00b7lich\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch mir im Herzensgrund", "tokens": ["Doch", "mir", "im", "Her\u00b7zens\u00b7grund"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ist Heiterkeit und Stille;", "tokens": ["Ist", "Hei\u00b7ter\u00b7keit", "und", "Stil\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir w\u00e4chst in solcher Stund", "tokens": ["Mir", "w\u00e4chst", "in", "sol\u00b7cher", "Stund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und h\u00e4rtet sich der Wille.", "tokens": ["Und", "h\u00e4r\u00b7tet", "sich", "der", "Wil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}