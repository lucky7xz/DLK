{"textgrid.poem.46547": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[was hilft der Sonnschein dann]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was hilft der Sonnschein dann,", "tokens": ["Was", "hilft", "der", "Sonn\u00b7schein", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wo man nicht sehn ihn kann,", "tokens": ["Wo", "man", "nicht", "sehn", "ihn", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PTKNEG", "VVFIN", "PPER", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil Nacht das Aug' umspann?", "tokens": ["Weil", "Nacht", "das", "Aug'", "um\u00b7spann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Sonnschein aber flieht,", "tokens": ["Der", "Sonn\u00b7schein", "a\u00b7ber", "flieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das Auge Nacht umzieht,", "tokens": ["Das", "Au\u00b7ge", "Nacht", "um\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo man nichts Liebes sieht.", "tokens": ["Wo", "man", "nichts", "Lie\u00b7bes", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PIS", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich sehe nicht mein Kind,", "tokens": ["Ich", "se\u00b7he", "nicht", "mein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und frage nicht, ob blind", "tokens": ["Und", "fra\u00b7ge", "nicht", ",", "ob", "blind"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sonn' oder Augen sind.", "tokens": ["Sonn'", "o\u00b7der", "Au\u00b7gen", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}