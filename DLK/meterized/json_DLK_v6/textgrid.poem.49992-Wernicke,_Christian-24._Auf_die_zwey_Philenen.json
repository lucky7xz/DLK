{"textgrid.poem.49992": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "24. Auf die zwey Philenen", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Sie ", "tokens": ["Sie"], "token_info": ["word"], "pos": ["PPER"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Denn dass des Eifers Macht im Tod auch nicht nehm' ab,", "tokens": ["Denn", "dass", "des", "Ei\u00b7fers", "Macht", "im", "Tod", "auch", "nicht", "nehm'", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "APPRART", "NN", "ADV", "PTKNEG", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So legt sie auf einmahl den ", "tokens": ["So", "legt", "sie", "auf", "ein\u00b7mahl", "den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Carthago der es ", "tokens": ["Car\u00b7tha\u00b7go", "der", "es"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Und sorgt, dass man nicht mehr nach solchen Thaten tracht',", "tokens": ["Und", "sorgt", ",", "dass", "man", "nicht", "mehr", "nach", "sol\u00b7chen", "Tha\u00b7ten", "tracht'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PIS", "PTKNEG", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dadurch sich mancher Held im Tod unsterblich macht,", "tokens": ["Da\u00b7durch", "sich", "man\u00b7cher", "Held", "im", "Tod", "uns\u00b7terb\u00b7lich", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "PIAT", "NN", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil was ", "tokens": ["Weil", "was"], "token_info": ["word", "word"], "pos": ["KOUS", "PIS"], "meter": "++", "measure": "spondeus"}}}}}