{"textgrid.poem.63818": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Resignation", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine warme Regennacht", "tokens": ["Ei\u00b7ne", "war\u00b7me", "Re\u00b7gen\u00b7nacht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat am Feigenbaum im Garten", "tokens": ["Hat", "am", "Fei\u00b7gen\u00b7baum", "im", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Zweig' in Flor gebracht,", "tokens": ["Al\u00b7le", "Zweig'", "in", "Flor", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die noch kahl des Fr\u00fchlings harrten.", "tokens": ["Die", "noch", "kahl", "des", "Fr\u00fch\u00b7lings", "harr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zu den Bl\u00e4ttlein dicht gesellt", "tokens": ["Zu", "den", "Bl\u00e4tt\u00b7lein", "dicht", "ge\u00b7sellt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schauen tausend winz'ge Knollen", "tokens": ["Schau\u00b7en", "tau\u00b7send", "winz'\u00b7ge", "Knol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Putzig in die gr\u00fcne Welt,", "tokens": ["Put\u00b7zig", "in", "die", "gr\u00fc\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die zu Fr\u00fcchten reifen sollen.", "tokens": ["Die", "zu", "Fr\u00fcch\u00b7ten", "rei\u00b7fen", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch die s\u00fc\u00dfe Lieblingsfrucht", "tokens": ["Doch", "die", "s\u00fc\u00b7\u00dfe", "Lieb\u00b7lings\u00b7frucht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll mir ungenossen bleiben,", "tokens": ["Soll", "mir", "un\u00b7ge\u00b7nos\u00b7sen", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird mich doch der Tage Flucht", "tokens": ["Wird", "mich", "doch", "der", "Ta\u00b7ge", "Flucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur zu bald von hinnen treiben.", "tokens": ["Nur", "zu", "bald", "von", "hin\u00b7nen", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "APPR", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "So auch an der Menschheit Baum", "tokens": ["So", "auch", "an", "der", "Menschheit", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "NE"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Seh' ich manche Knospe sprie\u00dfen,", "tokens": ["Seh'", "ich", "man\u00b7che", "Knos\u00b7pe", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zu hoffen wag' ich kaum,", "tokens": ["Und", "zu", "hof\u00b7fen", "wag'", "ich", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihrer Reife zu genie\u00dfen.", "tokens": ["Ih\u00b7rer", "Rei\u00b7fe", "zu", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Los des Alters! Das sei fern,", "tokens": ["Los", "des", "Al\u00b7ters", "!", "Das", "sei", "fern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$.", "PDS", "VAFIN", "ADJD", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Drum zu trauern im Gem\u00fcte.", "tokens": ["Drum", "zu", "trau\u00b7ern", "im", "Ge\u00b7m\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur noch aufbl\u00fchn s\u00e4h' ich gern", "tokens": ["Nur", "noch", "auf\u00b7bl\u00fchn", "s\u00e4h'", "ich", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVIZU", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Jenes vielgeliebte Kind", "tokens": ["Je\u00b7nes", "viel\u00b7ge\u00b7lieb\u00b7te", "Kind"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den strahlend gro\u00dfen Augen,", "tokens": ["Mit", "den", "strah\u00b7lend", "gro\u00b7\u00dfen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die so \u00fcberm\u00fctig sind,", "tokens": ["Die", "so", "\u00fc\u00b7berm\u00b7\u00fc\u00b7tig", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sie wohl zum Hexen taugen.", "tokens": ["Wie", "sie", "wohl", "zum", "He\u00b7xen", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Gerne kennt' ich noch den Mann,", "tokens": ["Ger\u00b7ne", "kennt'", "ich", "noch", "den", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sich z\u00e4hmt die s\u00fc\u00dfe Wilde,", "tokens": ["Der", "sich", "z\u00e4hmt", "die", "s\u00fc\u00b7\u00dfe", "Wil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00e4h' als Frau und Mutter dann", "tokens": ["S\u00e4h'", "als", "Frau", "und", "Mut\u00b7ter", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "NN", "KON", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Selig l\u00e4cheln meine Thilde.", "tokens": ["Se\u00b7lig", "l\u00e4\u00b7cheln", "mei\u00b7ne", "Thil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ach, auf diesen Lieblingstraum", "tokens": ["Ach", ",", "auf", "die\u00b7sen", "Lieb\u00b7lings\u00b7traum"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann ich nicht so leicht verzichten,", "tokens": ["Kann", "ich", "nicht", "so", "leicht", "ver\u00b7zich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie da\u00df dort der Feigenbaum", "tokens": ["Wie", "da\u00df", "dort", "der", "Fei\u00b7gen\u00b7baum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "ADV", "ART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Mich noch labt mit seinen Fr\u00fcchten!", "tokens": ["Mich", "noch", "labt", "mit", "sei\u00b7nen", "Fr\u00fcch\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}