{"textgrid.poem.23937": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Froh und fromm", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blauer Himmel und wei\u00dfe Bl\u00fcten,", "tokens": ["Blau\u00b7er", "Him\u00b7mel", "und", "wei\u00b7\u00dfe", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ein g\u00f6ttliches Beg\u00fcten", "tokens": ["Ein", "g\u00f6tt\u00b7li\u00b7ches", "Be\u00b7g\u00fc\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Liegt \u00fcber aller Welt;", "tokens": ["Liegt", "\u00fc\u00b7ber", "al\u00b7ler", "Welt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es ist ein himmlisch H\u00fcten,", "tokens": ["Es", "ist", "ein", "himm\u00b7lisch", "H\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das uns in Armen h\u00e4lt.", "tokens": ["Das", "uns", "in", "Ar\u00b7men", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wei\u00df nicht, wohin michs leite,", "tokens": ["Wei\u00df", "nicht", ",", "wo\u00b7hin", "michs", "lei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wei\u00df nicht, wohin ich schreite,", "tokens": ["Wei\u00df", "nicht", ",", "wo\u00b7hin", "ich", "schrei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein Herz ist wohl bestellt:", "tokens": ["Mein", "Herz", "ist", "wohl", "be\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ich wandre in die Weite,", "tokens": ["Ich", "wand\u00b7re", "in", "die", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wohin es Gott gef\u00e4llt.", "tokens": ["Wo\u00b7hin", "es", "Gott", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der hat mit tausend Bl\u00fcten", "tokens": ["Der", "hat", "mit", "tau\u00b7send", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir meinen Weg erhellt.", "tokens": ["Mir", "mei\u00b7nen", "Weg", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}