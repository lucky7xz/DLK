{"textgrid.poem.51081": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Laut st\u00fcrmt der Schall der Glocken durch die Nacht,", "tokens": ["Laut", "st\u00fcrmt", "der", "Schall", "der", "Glo\u00b7cken", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und Sch\u00fcsse dr\u00f6hnen von des Berges Wacht;", "tokens": ["Und", "Sch\u00fcs\u00b7se", "dr\u00f6h\u00b7nen", "von", "des", "Ber\u00b7ges", "Wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In allen Gassen t\u00f6nt's: es brennt! es brennt!", "tokens": ["In", "al\u00b7len", "Gas\u00b7sen", "t\u00f6nt's", ":", "es", "brennt", "!", "es", "brennt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NE", "$.", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und jeder angstvoll an sein Fenster rennt.", "tokens": ["Und", "je\u00b7der", "angst\u00b7voll", "an", "sein", "Fens\u00b7ter", "rennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der erste Blick: ist es in unserm Haus?", "tokens": ["Der", "ers\u00b7te", "Blick", ":", "ist", "es", "in", "un\u00b7serm", "Haus", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der zweite mindert schon den Schreck und Graus,", "tokens": ["Der", "zwei\u00b7te", "min\u00b7dert", "schon", "den", "Schreck", "und", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn weit, o weit die \u00bbfurchtbar sch\u00f6ne\u00ab Glut", "tokens": ["Wenn", "weit", ",", "o", "weit", "die", "\u00bb", "furcht\u00b7bar", "sch\u00f6\u00b7ne", "\u00ab", "Glut"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "ADJD", "$,", "FM", "ADJD", "ART", "$(", "ADJD", "ADJA", "$(", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Behaglich dort am fernen Himmel ruht.", "tokens": ["Be\u00b7hag\u00b7lich", "dort", "am", "fer\u00b7nen", "Him\u00b7mel", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun str\u00f6mt der Neugier B\u00e4chlein ungehemmt,", "tokens": ["Nun", "str\u00f6mt", "der", "Neu\u00b7gier", "B\u00e4ch\u00b7lein", "un\u00b7ge\u00b7hemmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ungewaschen wohl und ungek\u00e4mmt,", "tokens": ["Und", "un\u00b7ge\u00b7wa\u00b7schen", "wohl", "und", "un\u00b7ge\u00b7k\u00e4mmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der ohne Str\u00fcmpfe, jener ohne Schuh',", "tokens": ["Der", "oh\u00b7ne", "Str\u00fcmp\u00b7fe", ",", "je\u00b7ner", "oh\u00b7ne", "Schuh'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "$,", "PDAT", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "L\u00e4uft alles dem willkommnen Schauspiel zu.", "tokens": ["L\u00e4uft", "al\u00b7les", "dem", "will\u00b7komm\u00b7nen", "Schau\u00b7spiel", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und manchem ehrlichen Philister bangt,", "tokens": ["Und", "man\u00b7chem", "ehr\u00b7li\u00b7chen", "Phi\u00b7lis\u00b7ter", "bangt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es k\u00f6nnte enden, eh er angelangt;", "tokens": ["Es", "k\u00f6nn\u00b7te", "en\u00b7den", ",", "eh", "er", "an\u00b7ge\u00b7langt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch der Poet, er watschelt mit hinaus", "tokens": ["Auch", "der", "Po\u00b7et", ",", "er", "wat\u00b7schelt", "mit", "hin\u00b7aus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sendet seinen Kennerblick voraus.", "tokens": ["Und", "sen\u00b7det", "sei\u00b7nen", "Ken\u00b7ner\u00b7blick", "vo\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da wallt vom Berg mit ungebrochnem Lauf", "tokens": ["Da", "wallt", "vom", "Berg", "mit", "un\u00b7ge\u00b7broch\u00b7nem", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die rote Lohe hell zum Himmel auf;", "tokens": ["Die", "ro\u00b7te", "Lo\u00b7he", "hell", "zum", "Him\u00b7mel", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Feuerlilien ein gewalt'ger Strau\u00df:", "tokens": ["Von", "Feu\u00b7er\u00b7li\u00b7li\u00b7en", "ein", "ge\u00b7walt'\u00b7ger", "Strau\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NE", "$."], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So bl\u00fcht und gl\u00fcht das gro\u00dfe Bauernhaus.", "tokens": ["So", "bl\u00fcht", "und", "gl\u00fcht", "das", "gro\u00b7\u00dfe", "Bau\u00b7ern\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Es ist die allersch\u00f6nste Maiennacht;", "tokens": ["Es", "ist", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Mai\u00b7en\u00b7nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Gold durchwirkt, tiefblau der Himmel lacht.", "tokens": ["Von", "Gold", "durch\u00b7wirkt", ",", "tief\u00b7blau", "der", "Him\u00b7mel", "lacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Eng zwischen G\u00e4rten ganz im Fr\u00fchlingsflor", "tokens": ["Eng", "zwi\u00b7schen", "G\u00e4r\u00b7ten", "ganz", "im", "Fr\u00fch\u00b7lings\u00b7flor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "ADV", "APPRART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Zu Feuers Hofstatt f\u00fchrt der Weg empor.", "tokens": ["Zu", "Feu\u00b7ers", "Hof\u00b7statt", "f\u00fchrt", "der", "Weg", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Da sitzt der helle Geist auf seinem Raub", "tokens": ["Da", "sitzt", "der", "hel\u00b7le", "Geist", "auf", "sei\u00b7nem", "Raub"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und macht den morschen Kram zu Asch und Staub;", "tokens": ["Und", "macht", "den", "mor\u00b7schen", "Kram", "zu", "Asch", "und", "Staub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Umsonst bel\u00e4stigt ihn der Menschenschwarm,", "tokens": ["Um\u00b7sonst", "be\u00b7l\u00e4s\u00b7tigt", "ihn", "der", "Men\u00b7schen\u00b7schwarm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er wehrt ihn ruhig ab mit gl\u00fchndem Arm.", "tokens": ["Er", "wehrt", "ihn", "ru\u00b7hig", "ab", "mit", "gl\u00fchn\u00b7dem", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Es brennt der Hof dem reichen Bauersmann,", "tokens": ["Es", "brennt", "der", "Hof", "dem", "rei\u00b7chen", "Bau\u00b7ers\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der nie genug sehn und erraffen kann;", "tokens": ["Der", "nie", "ge\u00b7nug", "sehn", "und", "er\u00b7raf\u00b7fen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVINF", "KON", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "L\u00e4ngst hat der Sohn ein neues Haus begehrt,", "tokens": ["L\u00e4ngst", "hat", "der", "Sohn", "ein", "neu\u00b7es", "Haus", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wogegen sich der Alte stets gewehrt.", "tokens": ["Wo\u00b7ge\u00b7gen", "sich", "der", "Al\u00b7te", "stets", "ge\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Nun steht er da und schlottert j\u00e4mmerlich,", "tokens": ["Nun", "steht", "er", "da", "und", "schlot\u00b7tert", "j\u00e4m\u00b7mer\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wei\u00df nicht zu raten noch zu helfen sich;", "tokens": ["Wei\u00df", "nicht", "zu", "ra\u00b7ten", "noch", "zu", "hel\u00b7fen", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PTKZU", "VVFIN", "ADV", "PTKZU", "VVINF", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch alle sind in guter Sicherheit,", "tokens": ["Doch", "al\u00b7le", "sind", "in", "gu\u00b7ter", "Si\u00b7cher\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kein Nachbarhaus gef\u00e4hrdet weit und breit.", "tokens": ["Kein", "Nach\u00b7bar\u00b7haus", "ge\u00b7f\u00e4hr\u00b7det", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Drum la\u00dft uns keck ein wenig n\u00e4her gehn,", "tokens": ["Drum", "la\u00dft", "uns", "keck", "ein", "we\u00b7nig", "n\u00e4\u00b7her", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "ART", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die hei\u00dfe Wirtschaft besser zu besehn,", "tokens": ["Die", "hei\u00b7\u00dfe", "Wirt\u00b7schaft", "bes\u00b7ser", "zu", "be\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu lesen in des Feuers Angesicht", "tokens": ["Zu", "le\u00b7sen", "in", "des", "Feu\u00b7ers", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und was es heimlich mit den Sternen spricht!", "tokens": ["Und", "was", "es", "heim\u00b7lich", "mit", "den", "Ster\u00b7nen", "spricht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}