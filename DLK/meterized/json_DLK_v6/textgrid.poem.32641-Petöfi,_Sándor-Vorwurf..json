{"textgrid.poem.32641": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "Vorwurf.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herz, mein Herz, das h\u00e4tt' ich doch", "tokens": ["Herz", ",", "mein", "Herz", ",", "das", "h\u00e4tt'", "ich", "doch"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nie von dir gedacht!", "tokens": ["Nie", "von", "dir", "ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Du, mein braver Kamerad.", "tokens": ["Du", ",", "mein", "bra\u00b7ver", "Ka\u00b7me\u00b7rad", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was hast du gemacht?", "tokens": ["Was", "hast", "du", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Herz, mein Herz, was bist du falsch,", "tokens": ["Herz", ",", "mein", "Herz", ",", "was", "bist", "du", "falsch", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PWS", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wie undankbar!", "tokens": ["Und", "wie", "un\u00b7dank\u00b7bar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sprich, wer hat dich denn besch\u00fctzt,", "tokens": ["Sprich", ",", "wer", "hat", "dich", "denn", "be\u00b7sch\u00fctzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn du in Gefahr?", "tokens": ["Wenn", "du", "in", "Ge\u00b7fahr", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Sprich, wenn dich das Schicksal peitscht,", "tokens": ["Sprich", ",", "wenn", "dich", "das", "Schick\u00b7sal", "peitscht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df du bluten mu\u00dft,", "tokens": ["Da\u00df", "du", "blu\u00b7ten", "mu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wer nimmt dich in Schutz und Hut,", "tokens": ["Wer", "nimmt", "dich", "in", "Schutz", "und", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wenn nicht meine Brust?", "tokens": ["Wenn", "nicht", "mei\u00b7ne", "Brust", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Nun, da sich das Blatt gewandt,", "tokens": ["Nun", ",", "da", "sich", "das", "Blatt", "ge\u00b7wandt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Pl\u00f6tzlich, \u00fcber Nacht,", "tokens": ["Pl\u00f6tz\u00b7lich", ",", "\u00fc\u00b7ber", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und mit gnadenreichem Blick", "tokens": ["Und", "mit", "gna\u00b7den\u00b7rei\u00b7chem", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir Fortuna lacht:", "tokens": ["Dir", "For\u00b7tu\u00b7na", "lacht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "$."], "meter": "----+", "measure": "unknown.measure.single"}}, "stanza.5": {"line.1": {"text": "Nun best\u00fcrmst du undankbar,", "tokens": ["Nun", "be\u00b7st\u00fcrmst", "du", "un\u00b7dank\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "(wei\u00dft du, was du tust?)", "tokens": ["(", "wei\u00dft", "du", ",", "was", "du", "tust", "?", ")"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ungest\u00fcm die eigne Wehr',", "tokens": ["Un\u00b7ge\u00b7st\u00fcm", "die", "eig\u00b7ne", "Wehr'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tobst du in der Brust!", "tokens": ["Tobst", "du", "in", "der", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Und du gl\u00fchst und treibst und jagst,", "tokens": ["Und", "du", "gl\u00fchst", "und", "treibst", "und", "jagst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie mir's deuten, wie?", "tokens": ["Wie", "mir's", "deu\u00b7ten", ",", "wie", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NE", "VVINF", "$,", "PWAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Bald versengst du mir die Brust,", "tokens": ["Bald", "ver\u00b7sengst", "du", "mir", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald zersprengst du sie!", "tokens": ["Bald", "zer\u00b7sprengst", "du", "sie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}