{"textgrid.poem.61497": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bist du betr\u00fcbt, beseligt, Herz,", "tokens": ["Bist", "du", "be\u00b7tr\u00fcbt", ",", "be\u00b7se\u00b7ligt", ",", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "VVPP", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So meide der Gesellschaft Fratzen;", "tokens": ["So", "mei\u00b7de", "der", "Ge\u00b7sell\u00b7schaft", "Frat\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein h\u00f6chstes Gl\u00fcck, dein tiefster Schmerz", "tokens": ["Dein", "h\u00f6chs\u00b7tes", "Gl\u00fcck", ",", "dein", "tiefs\u00b7ter", "Schmerz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind ihnen nichts als Stoff zum Schwatzen.", "tokens": ["Sind", "ih\u00b7nen", "nichts", "als", "Stoff", "zum", "Schwat\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "KOKOM", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}