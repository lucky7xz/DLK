{"dta.poem.262": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er beklaget sich \u00fcber ihre H\u00e4rtigkeit.  \n  Folie d\u2019 Espagne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Clarinde sch\u00f6nstes Bild", "tokens": ["Cla\u00b7rin\u00b7de", "sch\u00f6ns\u00b7tes", "Bild"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ist denn ein Stein verh\u00fcllt", "tokens": ["Ist", "denn", "ein", "Stein", "ver\u00b7h\u00fcllt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In deiner zarten Brust?", "tokens": ["In", "dei\u00b7ner", "zar\u00b7ten", "Brust", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df du mit Hertzens Lust", "tokens": ["Da\u00df", "du", "mit", "Hert\u00b7zens", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kanst meinen Kummer sehn?", "tokens": ["Kanst", "mei\u00b7nen", "Kum\u00b7mer", "sehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Du l\u00e4ssest mich vergehen/", "tokens": ["Du", "l\u00e4s\u00b7sest", "mich", "ver\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da dein Mund ", "tokens": ["Da", "dein", "Mund"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Den Schmertzen lindern kan.", "tokens": ["Den", "Schmert\u00b7zen", "lin\u00b7dern", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Den Schmertzen/ den du mir", "tokens": ["Den", "Schmert\u00b7zen", "/", "den", "du", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "PPER", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Durch deine Sch\u00f6nheit-Zier/", "tokens": ["Durch", "dei\u00b7ne", "Sch\u00f6n\u00b7heit\u00b7Zier", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und deiner Augen Macht", "tokens": ["Und", "dei\u00b7ner", "Au\u00b7gen", "Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In meine Brust gebracht.", "tokens": ["In", "mei\u00b7ne", "Brust", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wodurch ich werd verzehrt/", "tokens": ["Wo\u00b7durch", "ich", "werd", "ver\u00b7zehrt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und bald in Nichts verkehrt/", "tokens": ["Und", "bald", "in", "Nichts", "ver\u00b7kehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIS", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn nicht dein Zucker-Mund", "tokens": ["Wenn", "nicht", "dein", "Zu\u00b7cke\u00b7rMund"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mein Hertze macht gesund", "tokens": ["Mein", "Hert\u00b7ze", "macht", "ge\u00b7sund"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Ku\u00df von deinem Mund", "tokens": ["Ein", "Ku\u00df", "von", "dei\u00b7nem", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Macht gleich mein Hertz gesund", "tokens": ["Macht", "gleich", "mein", "Hertz", "ge\u00b7sund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PPOSAT", "NN", "VVPP"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ein freundlich Augen-Blick", "tokens": ["Ein", "freund\u00b7lich", "Au\u00b7gen\u00b7Blick"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Treibt meine Quaal zur\u00fcck;", "tokens": ["Treibt", "mei\u00b7ne", "Qua\u00b7al", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Das dr\u00fccken deiner Hand", "tokens": ["Das", "dr\u00fc\u00b7cken", "dei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fchrt ins Gelobte-Land/", "tokens": ["F\u00fchrt", "ins", "Ge\u00b7lob\u00b7te\u00b7Land", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Allwo mein matter Geist", "tokens": ["All\u00b7wo", "mein", "mat\u00b7ter", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.8": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "Doch deine Grausahmkeit/", "tokens": ["Doch", "dei\u00b7ne", "Grau\u00b7sahm\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der du dich gantz geweyht/", "tokens": ["Der", "du", "dich", "gantz", "ge\u00b7weyht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "L\u00e4st dich nicht freundlich seyn/", "tokens": ["L\u00e4st", "dich", "nicht", "freund\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ob gleich die Mittel klein.", "tokens": ["Ob", "gleich", "die", "Mit\u00b7tel", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dein Hertz ist Stein und Stahl/", "tokens": ["Dein", "Hertz", "ist", "Stein", "und", "Stahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Es lachet meiner Quaal/", "tokens": ["Es", "la\u00b7chet", "mei\u00b7ner", "Qua\u00b7al", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und weigert mir den Ku\u00df", "tokens": ["Und", "wei\u00b7gert", "mir", "den", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ohn dem ich sterben mu\u00df.", "tokens": ["Ohn", "dem", "ich", "ster\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich sterbe edles Kind", "tokens": ["Ich", "ster\u00b7be", "ed\u00b7les", "Kind"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Durch deine Pracht entz\u00fcndt/", "tokens": ["Durch", "dei\u00b7ne", "Pracht", "ent\u00b7z\u00fcndt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo deiner Lippen-Safft", "tokens": ["Wo", "dei\u00b7ner", "Lip\u00b7pen\u00b7\u00b7S\u00b7afft"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Mir kein Vergn\u00fcgen schafft/", "tokens": ["Mir", "kein", "Ver\u00b7gn\u00fc\u00b7gen", "schafft", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Noch mich befreyt aus Noht;", "tokens": ["Noch", "mich", "be\u00b7freyt", "aus", "Noht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der ich den herben Tod", "tokens": ["Der", "ich", "den", "her\u00b7ben", "Tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Schon in dem Rachen bin", "tokens": ["Schon", "in", "dem", "Ra\u00b7chen", "bin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Erweicht sich nicht dein Sinn.", "tokens": ["Er\u00b7weicht", "sich", "nicht", "dein", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Allein! du bleibest Stein", "tokens": ["Al\u00b7lein", "!", "du", "blei\u00b7best", "Stein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "PPER", "VVFIN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bey meiner Liebes-Pein/", "tokens": ["Bey", "mei\u00b7ner", "Lie\u00b7bes\u00b7Pein", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es sauget deine Brust", "tokens": ["Es", "sau\u00b7get", "dei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Aus meinen Schmertzen Lust.", "tokens": ["Aus", "mei\u00b7nen", "Schmert\u00b7zen", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Qu\u00e4hlen kitzelt dich/", "tokens": ["Mein", "Qu\u00e4h\u00b7len", "kit\u00b7zelt", "dich", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dein Kitzeln qu\u00e4hlet mich/", "tokens": ["Dein", "Kit\u00b7zeln", "qu\u00e4h\u00b7let", "mich", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch liebet stets mein Geist", "tokens": ["Doch", "lie\u00b7bet", "stets", "mein", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Was sein Verderben heist.", "tokens": ["Was", "sein", "Ver\u00b7der\u00b7ben", "heist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}