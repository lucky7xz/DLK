{"textgrid.poem.48497": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "18. Auf ihr Abwesen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich irrte hin und her und suchte mich in mir,", "tokens": ["Ich", "irr\u00b7te", "hin", "und", "her", "und", "such\u00b7te", "mich", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "KON", "VVFIN", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und wuste dieses nicht, da\u00df ich ganz war in dir.", "tokens": ["und", "wus\u00b7te", "die\u00b7ses", "nicht", ",", "da\u00df", "ich", "ganz", "war", "in", "dir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach! tu dich mir doch auf, du Wohnhaus meiner Seelen!", "tokens": ["Ach", "!", "tu", "dich", "mir", "doch", "auf", ",", "du", "Wohn\u00b7haus", "mei\u00b7ner", "See\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$,", "PPER", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Komm, Sch\u00f6ne, gieb mich mir, benim mir dieses Qu\u00e4len!", "tokens": ["Komm", ",", "Sch\u00f6\u00b7ne", ",", "gieb", "mich", "mir", ",", "be\u00b7nim", "mir", "die\u00b7ses", "Qu\u00e4\u00b7len", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVIMP", "PPER", "PPER", "$,", "VVIMP", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Schau, wie er sich betr\u00fcbt, mein Geist, der in dir lebt!", "tokens": ["Schau", ",", "wie", "er", "sich", "be\u00b7tr\u00fcbt", ",", "mein", "Geist", ",", "der", "in", "dir", "lebt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "PRF", "VVPP", "$,", "PPOSAT", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.6": {"text": "T\u00f6tst du den, der dich liebt? Itzt hat er ausgelebt.", "tokens": ["T\u00f6tst", "du", "den", ",", "der", "dich", "liebt", "?", "Itzt", "hat", "er", "aus\u00b7ge\u00b7lebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "$,", "PRELS", "PPER", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch gieb mich nicht aus dir! Ich mag nicht in mich kehren.", "tokens": ["Doch", "gieb", "mich", "nicht", "aus", "dir", "!", "Ich", "mag", "nicht", "in", "mich", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PTKNEG", "APPR", "PPER", "$.", "PPER", "VMFIN", "PTKNEG", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kein Tod hat Macht an mir, du kanst mich leben lehren.", "tokens": ["Kein", "Tod", "hat", "Macht", "an", "mir", ",", "du", "kanst", "mich", "le\u00b7ben", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "NN", "APPR", "PPER", "$,", "PPER", "VMFIN", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich sei auch, wo ich sei, bin ich, Schatz, nicht bei dir,", "tokens": ["Ich", "sei", "auch", ",", "wo", "ich", "sei", ",", "bin", "ich", ",", "Schatz", ",", "nicht", "bei", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PWAV", "PPER", "VAFIN", "$,", "VAFIN", "PPER", "$,", "NN", "$,", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "so bin ich nimmermehr seihest in und bei mir.", "tokens": ["so", "bin", "ich", "nim\u00b7mer\u00b7mehr", "sei\u00b7hest", "in", "und", "bei", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VAFIN", "APPR", "KON", "APPR", "PPER", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}