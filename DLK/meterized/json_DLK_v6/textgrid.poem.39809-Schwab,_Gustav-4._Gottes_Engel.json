{"textgrid.poem.39809": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "4. Gottes Engel", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seiner Boten einen", "tokens": ["Sei\u00b7ner", "Bo\u00b7ten", "ei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "ART"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Aus der Engel Schar", "tokens": ["Aus", "der", "En\u00b7gel", "Schar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "L\u00e4\u00dft der Herr erscheinen", "tokens": ["L\u00e4\u00dft", "der", "Herr", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Jedes neue Jahr.", "tokens": ["Je\u00b7des", "neu\u00b7e", "Jahr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Aus der Zeiten Pforte", "tokens": ["Aus", "der", "Zei\u00b7ten", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Schwebt der Himmelgeist,", "tokens": ["Schwebt", "der", "Him\u00b7mel\u00b7geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Den er seine Worte", "tokens": ["Den", "er", "sei\u00b7ne", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Uns verk\u00fcnden hei\u00dft.", "tokens": ["Uns", "ver\u00b7k\u00fcn\u00b7den", "hei\u00dft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Oft erscheint ein Engel,", "tokens": ["Oft", "er\u00b7scheint", "ein", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Der den Lilienst\u00e4ngel", "tokens": ["Der", "den", "Li\u00b7li\u00b7en\u00b7st\u00e4n\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["ART", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Neigt ob allem Land:", "tokens": ["Neigt", "ob", "al\u00b7lem", "Land", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "PIS", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "In die Beete nieder", "tokens": ["In", "die", "Bee\u00b7te", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sinkt der Blumenstaub,", "tokens": ["Sinkt", "der", "Blu\u00b7men\u00b7staub", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da erwachen wieder", "tokens": ["Da", "er\u00b7wa\u00b7chen", "wie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Bl\u00fct' und Frucht im Laub,", "tokens": ["Bl\u00fct'", "und", "Frucht", "im", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Oft herab zur Erde", "tokens": ["Oft", "her\u00b7ab", "zur", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Fleugt der Engel ", "tokens": ["Fleugt", "der", "En\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Mit dem Racheschwerte", "tokens": ["Mit", "dem", "Ra\u00b7ch\u00b7e\u00b7schwer\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Gottes oft der ", "tokens": ["Got\u00b7tes", "oft", "der"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "ART"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Und als Schnitter stehen", "tokens": ["Und", "als", "Schnit\u00b7ter", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sie in reifer Welt,", "tokens": ["Sie", "in", "rei\u00b7fer", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Fangen an zu m\u00e4hen", "tokens": ["Fan\u00b7gen", "an", "zu", "m\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wie es ihm gef\u00e4llt. \u2013", "tokens": ["Wie", "es", "ihm", "ge\u00b7f\u00e4llt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Uns in dieses Jahr?", "tokens": ["Uns", "in", "die\u00b7ses", "Jahr", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wen, emporgewendet", "tokens": ["Wen", ",", "em\u00b7por\u00b7ge\u00b7wen\u00b7det"], "token_info": ["word", "punct", "word"], "pos": ["PWS", "$,", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wird der Blick gewahr?", "tokens": ["Wird", "der", "Blick", "ge\u00b7wahr", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Ach, es ist der ernste", "tokens": ["Ach", ",", "es", "ist", "der", "erns\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Diener, ist der ", "tokens": ["Die\u00b7ner", ",", "ist", "der"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "VAFIN", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Tr\u00e4gt bis an das fernste", "tokens": ["Tr\u00e4gt", "bis", "an", "das", "ferns\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ufer sein Gebot.", "tokens": ["U\u00b7fer", "sein", "Ge\u00b7bot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Einem F\u00fcrstengreise", "tokens": ["Ei\u00b7nem", "F\u00fcrs\u00b7ten\u00b7grei\u00b7se"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schob er j\u00fcngst den Arm", "tokens": ["Schob", "er", "j\u00fcngst", "den", "Arm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Unter's Haupt, das leise", "tokens": ["Un\u00b7ter's", "Haupt", ",", "das", "lei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Fortschl\u00e4ft ohne Harm.", "tokens": ["Fort\u00b7schl\u00e4ft", "oh\u00b7ne", "Harm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Dann von Thron zu Throne", "tokens": ["Dann", "von", "Thron", "zu", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Zog er, und dem Zar", "tokens": ["Zog", "er", ",", "und", "dem", "Zar"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nahm die goldne Krone", "tokens": ["Nahm", "die", "gold\u00b7ne", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Er vom blonden Haar;", "tokens": ["Er", "vom", "blon\u00b7den", "Haar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Und worauf hienieden", "tokens": ["Und", "wo\u00b7rauf", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sein Gedanke sann:", "tokens": ["Sein", "Ge\u00b7dan\u00b7ke", "sann", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Zu dem ew'gen Frieden", "tokens": ["Zu", "dem", "ew'\u00b7gen", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Zeigt' er ihm die Bahn.", "tokens": ["Zeigt'", "er", "ihm", "die", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Und es geht der Engel", "tokens": ["Und", "es", "geht", "der", "En\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Weiter seinen Pfad,", "tokens": ["Wei\u00b7ter", "sei\u00b7nen", "Pfad", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Vor ihm gr\u00fcnt der M\u00e4ngel", "tokens": ["Vor", "ihm", "gr\u00fcnt", "der", "M\u00e4n\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und der S\u00fcnden Saat.", "tokens": ["Und", "der", "S\u00fcn\u00b7den", "Saat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Gr\u00e4ber stehen offen:", "tokens": ["Gr\u00e4\u00b7ber", "ste\u00b7hen", "of\u00b7fen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Doch \u2013 was kommen mag \u2013", "tokens": ["Doch", "\u2013", "was", "kom\u00b7men", "mag", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PWS", "VVINF", "VMFIN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Freunde, la\u00dft uns hoffen,", "tokens": ["Freun\u00b7de", ",", "la\u00dft", "uns", "hof\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "La\u00dft den Dichter schw\u00e4rmen,", "tokens": ["La\u00dft", "den", "Dich\u00b7ter", "schw\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "La\u00dft ihn prophezein,", "tokens": ["La\u00dft", "ihn", "pro\u00b7phe\u00b7zein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sonnen sich und w\u00e4rmen", "tokens": ["Son\u00b7nen", "sich", "und", "w\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PRF", "KON", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "An der Ahnung Schein:", "tokens": ["An", "der", "Ah\u00b7nung", "Schein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.15": {"line.1": {"text": "Fremden Regionen", "tokens": ["Frem\u00b7den", "Re\u00b7gi\u00b7o\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Eilt der Engel zu,", "tokens": ["Eilt", "der", "En\u00b7gel", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Euch, ihr H\u00fctten, Thronen,", "tokens": ["Euch", ",", "ihr", "H\u00fct\u00b7ten", ",", "Thro\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "G\u00f6nnt er lange Ruh!", "tokens": ["G\u00f6nnt", "er", "lan\u00b7ge", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.16": {"line.1": {"text": "Dort wo m\u00fcde Streiter", "tokens": ["Dort", "wo", "m\u00fc\u00b7de", "Strei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PWAV", "ADJD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bang gen Himmel sehn,", "tokens": ["Bang", "gen", "Him\u00b7mel", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wo Egyptens Reiter", "tokens": ["Wo", "E\u00b7gyp\u00b7tens", "Rei\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NE", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dicht, wie Mauern, stehn:", "tokens": ["Dicht", ",", "wie", "Mau\u00b7ern", ",", "stehn", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.17": {"line.1": {"text": "Dorthin seht ihn fliegen,", "tokens": ["Dor\u00b7thin", "seht", "ihn", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Seht ihn unbemerkt", "tokens": ["Seht", "ihn", "un\u00b7be\u00b7merkt"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "In den Reihen liegen,", "tokens": ["In", "den", "Rei\u00b7hen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Die kein Andrer st\u00e4rkt!", "tokens": ["Die", "kein", "A\u00b7ndrer", "st\u00e4rkt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.18": {"line.1": {"text": "Ist er nicht der alte", "tokens": ["Ist", "er", "nicht", "der", "al\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "W\u00fcrger ohne Schlacht,", "tokens": ["W\u00fcr\u00b7ger", "oh\u00b7ne", "Schlacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dessen K\u00f6cher schallte", "tokens": ["Des\u00b7sen", "K\u00f6\u00b7cher", "schall\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In der Mitternacht?", "tokens": ["In", "der", "Mit\u00b7ter\u00b7nacht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.19": {"line.1": {"text": "Der die Erstgebornen", "tokens": ["Der", "die", "Erst\u00b7ge\u00b7bor\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "In Egypten schlug,", "tokens": ["In", "E\u00b7gyp\u00b7ten", "schlug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Bis da\u00df Halbverlornen", "tokens": ["Bis"