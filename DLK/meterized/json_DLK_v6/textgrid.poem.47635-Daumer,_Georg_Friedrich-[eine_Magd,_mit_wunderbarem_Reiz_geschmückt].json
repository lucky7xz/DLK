{"textgrid.poem.47635": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[eine Magd, mit wunderbarem Reiz geschm\u00fcckt]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Magd, mit wunderbarem Reiz geschm\u00fcckt,", "tokens": ["Ei\u00b7ne", "Magd", ",", "mit", "wun\u00b7der\u00b7ba\u00b7rem", "Reiz", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Der es Leid ist, da\u00df sie unser Aug' entz\u00fcckt!", "tokens": ["Der", "es", "Leid", "ist", ",", "da\u00df", "sie", "un\u00b7ser", "Aug'", "ent\u00b7z\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn du ihr zu nah'n befiehlst, so naht sie dir", "tokens": ["Wenn", "du", "ihr", "zu", "nah'n", "be\u00b7fiehlst", ",", "so", "naht", "sie", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "PTKA", "ADJD", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Kalt und ernst, von Freude nie das Herz begl\u00fcckt;", "tokens": ["Kalt", "und", "ernst", ",", "von", "Freu\u00b7de", "nie", "das", "Herz", "be\u00b7gl\u00fcckt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "APPR", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn du ihr zu geh'n gebeutst, sie wallt dahin,", "tokens": ["Wenn", "du", "ihr", "zu", "geh'n", "ge\u00b7beutst", ",", "sie", "wallt", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKZU", "VVINF", "VVFIN", "$,", "PPER", "VVFIN", "PAV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Und du siehst ihr Wesen nicht von Harm gedr\u00fcckt.", "tokens": ["Und", "du", "siehst", "ihr", "We\u00b7sen", "nicht", "von", "Harm", "ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Welch ein Eden w\u00e4re diese stolze Magd,", "tokens": ["Welch", "ein", "E\u00b7den", "w\u00e4\u00b7re", "die\u00b7se", "stol\u00b7ze", "Magd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VAFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Von der Liebe Leidenschaft und Lust ber\u00fcckt!", "tokens": ["Von", "der", "Lie\u00b7be", "Lei\u00b7den\u00b7schaft", "und", "Lust", "be\u00b7r\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}