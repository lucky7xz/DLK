{"dta.poem.15711": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weh dir, o Poesie in dieser Zeit Gedr\u00e4nge!", "tokens": ["Weh", "dir", ",", "o", "Poe\u00b7sie", "in", "die\u00b7ser", "Zeit", "Ge\u00b7dr\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "FM", "NN", "APPR", "PDAT", "NN", "NN", "$."], "meter": "+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Du bist nicht ernst genug der ernst gelehrten Menge;", "tokens": ["Du", "bist", "nicht", "ernst", "ge\u00b7nug", "der", "ernst", "ge\u00b7lehr\u00b7ten", "Men\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "ADV", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zu ernst der leichten Welt, die Unterhaltung sucht;", "tokens": ["Zu", "ernst", "der", "leich\u00b7ten", "Welt", ",", "die", "Un\u00b7ter\u00b7hal\u00b7tung", "sucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So nimmt Gelehrt und Ungelehrt vor dir die Flucht.", "tokens": ["So", "nimmt", "Ge\u00b7lehrt", "und", "Un\u00b7ge\u00b7lehrt", "vor", "dir", "die", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-