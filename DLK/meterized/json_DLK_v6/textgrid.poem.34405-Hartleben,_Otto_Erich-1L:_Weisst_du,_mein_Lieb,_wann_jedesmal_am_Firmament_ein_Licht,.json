{"textgrid.poem.34405": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weisst du, mein Lieb, wann jedesmal am Firmament ein Licht,", "genre": "verse", "period": "N.A.", "pub_year": 1885, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weisst du, mein Lieb, wann jedesmal am Firmament ein Licht,", "tokens": ["Weisst", "du", ",", "mein", "Lieb", ",", "wann", "je\u00b7des\u00b7mal", "am", "Fir\u00b7ma\u00b7ment", "ein", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PWAV", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "ein Stern entsteht? Du th\u00f6richt Kind, nicht wahr, das weisst du nicht.", "tokens": ["ein", "Stern", "ent\u00b7steht", "?", "Du", "th\u00f6\u00b7richt", "Kind", ",", "nicht", "wahr", ",", "das", "weisst", "du", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "ADJD", "NN", "$,", "PTKNEG", "ADJD", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ich muss es dir erz\u00e4hlen, komm, und lege traulich sacht", "tokens": ["Ich", "muss", "es", "dir", "er\u00b7z\u00e4h\u00b7len", ",", "komm", ",", "und", "le\u00b7ge", "trau\u00b7lich", "sacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "VVINF", "$,", "VVFIN", "$,", "KON", "VVFIN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "dein K\u00f6pfchen mir ans warme Herz \u2013 and\u00e4mmern lass die Nacht.", "tokens": ["dein", "K\u00f6pf\u00b7chen", "mir", "ans", "war\u00b7me", "Herz", "\u2013", "an\u00b7d\u00e4m\u00b7mern", "lass", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "APPRART", "ADJA", "NN", "$(", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Siehst du: der dunkle Himmel dort ist ein unendlicher Garten,", "tokens": ["Siehst", "du", ":", "der", "dunk\u00b7le", "Him\u00b7mel", "dort", "ist", "ein", "un\u00b7end\u00b7li\u00b7cher", "Gar\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ART", "ADJA", "NN", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "drin stille Engel unsichtbar goldener Blumen warten.", "tokens": ["drin", "stil\u00b7le", "En\u00b7gel", "un\u00b7sicht\u00b7bar", "gol\u00b7de\u00b7ner", "Blu\u00b7men", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und jedesmal, wann drunten hier zwei Seelen sich entz\u00fcnden,", "tokens": ["Und", "je\u00b7des\u00b7mal", ",", "wann", "drun\u00b7ten", "hier", "zwei", "See\u00b7len", "sich", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "ADV", "ADV", "CARD", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "sich, zu einander heiss gebannt, in Gl\u00fcck und Gluth verb\u00fcnden,", "tokens": ["sich", ",", "zu", "ein\u00b7an\u00b7der", "heiss", "ge\u00b7bannt", ",", "in", "Gl\u00fcck", "und", "Gluth", "ver\u00b7b\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "APPR", "PRF", "APZR", "VVPP", "$,", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "dann pflanzen eine Blume sie dem tiefen Grunde ein", "tokens": ["dann", "pflan\u00b7zen", "ei\u00b7ne", "Blu\u00b7me", "sie", "dem", "tie\u00b7fen", "Grun\u00b7de", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ART", "ADJA", "NN", "ART"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "und segnen jede junge Lust mit jungem Sternenschein. \u2013", "tokens": ["und", "seg\u00b7nen", "je\u00b7de", "jun\u00b7ge", "Lust", "mit", "jun\u00b7gem", "Ster\u00b7nen\u00b7schein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "O sieh: schon ist die heilige Nacht gemach herangetreten,", "tokens": ["O", "sieh", ":", "schon", "ist", "die", "hei\u00b7li\u00b7ge", "Nacht", "ge\u00b7mach", "her\u00b7an\u00b7ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "ADV", "VAFIN", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "die Blumen leuchten ungez\u00e4hlt her von den ewigen Beeten,", "tokens": ["die", "Blu\u00b7men", "leuch\u00b7ten", "un\u00b7ge\u00b7z\u00e4hlt", "her", "von", "den", "e\u00b7wi\u00b7gen", "Bee\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "und alle k\u00fcnden und zeugen nur von irdischer Menschen Liebe \u2013", "tokens": ["und", "al\u00b7le", "k\u00fcn\u00b7den", "und", "zeu\u00b7gen", "nur", "von", "ir\u00b7di\u00b7scher", "Men\u00b7schen", "Lie\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "KON", "VVFIN", "ADV", "APPR", "ADJA", "NN", "NN", "$("], "meter": "-+-+--+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "o dass auch unseres Gl\u00fcckes Stern ewig uns leuchten bliebe!", "tokens": ["o", "dass", "auch", "un\u00b7se\u00b7res", "Gl\u00fc\u00b7ckes", "Stern", "e\u00b7wig", "uns", "leuch\u00b7ten", "blie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "KOUS", "ADV", "PPOSAT", "NN", "NN", "ADJD", "PPER", "VVFIN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}}}}}