{"textgrid.poem.61794": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Am H\u00fcnengrabe", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So w\u00f6lbst du wieder \u00fcber mir", "tokens": ["So", "w\u00f6lbst", "du", "wie\u00b7der", "\u00fc\u00b7ber", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Schattenzelt von Ast zu Ast?", "tokens": ["Dein", "Schat\u00b7ten\u00b7zelt", "von", "Ast", "zu", "Ast", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Willkommen, trautes Waldrevier,", "tokens": ["Will\u00b7kom\u00b7men", ",", "trau\u00b7tes", "Wald\u00b7re\u00b7vier", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du St\u00e4tte meiner Jugendrast!", "tokens": ["Du", "St\u00e4t\u00b7te", "mei\u00b7ner", "Ju\u00b7gen\u00b7drast", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dahingerauscht sind zwanzig Jahr',", "tokens": ["Da\u00b7hin\u00b7ge\u00b7rauscht", "sind", "zwan\u00b7zig", "Jahr'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Seit ich bei dir zu Gaste war.", "tokens": ["Seit", "ich", "bei", "dir", "zu", "Gas\u00b7te", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Sonne scheint herab auf euch,", "tokens": ["Die", "Son\u00b7ne", "scheint", "her\u00b7ab", "auf", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Buchen, wie sie weiland schien,", "tokens": ["Ihr", "Bu\u00b7chen", ",", "wie", "sie", "wei\u00b7land", "schien", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es singt im bl\u00fchnden Dorngestr\u00e4uch", "tokens": ["Es", "singt", "im", "bl\u00fchn\u00b7den", "Dorn\u00b7ge\u00b7str\u00e4uch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Fink die alten Melodien;", "tokens": ["Der", "Fink", "die", "al\u00b7ten", "Me\u00b7lo\u00b7dien", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das B\u00e4chlein rauscht am alten Ort,", "tokens": ["Das", "B\u00e4ch\u00b7lein", "rauscht", "am", "al\u00b7ten", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wie im Traume wandl' ich fort.", "tokens": ["Und", "wie", "im", "Trau\u00b7me", "wandl'", "ich", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch pl\u00f6tzlich hier zum Meer hinab", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "hier", "zum", "Meer", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vertauscht erscheint mir rings die Welt;", "tokens": ["Ver\u00b7tauscht", "er\u00b7scheint", "mir", "rings", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Walde lag das H\u00fcnengrab,", "tokens": ["Im", "Wal\u00b7de", "lag", "das", "H\u00fc\u00b7nen\u00b7grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun liegt es auf dem freien Feld,", "tokens": ["Nun", "liegt", "es", "auf", "dem", "frei\u00b7en", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wo der J\u00fcngling einst dem Horn", "tokens": ["Und", "wo", "der", "J\u00fcng\u00b7ling", "einst", "dem", "Horn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des J\u00e4gers lauschte, wogt das Korn.", "tokens": ["Des", "J\u00e4\u00b7gers", "lauschte", ",", "wogt", "das", "Korn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Gesegnet sei dem Bauersmann", "tokens": ["Ge\u00b7seg\u00b7net", "sei", "dem", "Bau\u00b7ers\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des treu bestellten Ackers Frucht!", "tokens": ["Des", "treu", "be\u00b7stell\u00b7ten", "A\u00b7ckers", "Frucht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch tiefe Wehmut f\u00e4llt mich an,", "tokens": ["Doch", "tie\u00b7fe", "Weh\u00b7mut", "f\u00e4llt", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gedenk' ich an der Dinge Flucht.", "tokens": ["Ge\u00b7denk'", "ich", "an", "der", "Din\u00b7ge", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, wie das Gr\u00fcn des Waldes schwand", "tokens": ["Ach", ",", "wie", "das", "Gr\u00fcn", "des", "Wal\u00b7des", "schwand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Bl\u00fcte, drin mein Leben stand.", "tokens": ["Die", "Bl\u00fc\u00b7te", ",", "drin", "mein", "Le\u00b7ben", "stand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo sind die Tage klar und reich,", "tokens": ["Wo", "sind", "die", "Ta\u00b7ge", "klar", "und", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ich im laub'gen Junimond", "tokens": ["Da", "ich", "im", "laub'\u00b7gen", "Ju\u00b7ni\u00b7mond"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der sommerfrohen Schwalbe gleich", "tokens": ["Der", "som\u00b7mer\u00b7fro\u00b7hen", "Schwal\u00b7be", "gleich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im alten Forsthaus dort gewohnt,", "tokens": ["Im", "al\u00b7ten", "Forst\u00b7haus", "dort", "ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da jedes Fr\u00fchrot, jede Nacht", "tokens": ["Da", "je\u00b7des", "Fr\u00fch\u00b7rot", ",", "je\u00b7de", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Begl\u00fcckend mir ein Lied gebracht?", "tokens": ["Be\u00b7gl\u00fc\u00b7ckend", "mir", "ein", "Lied", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wo sind die Freunde, die mir dort", "tokens": ["Wo", "sind", "die", "Freun\u00b7de", ",", "die", "mir", "dort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Becher gastlich eingeschenkt,", "tokens": ["Den", "Be\u00b7cher", "gast\u00b7lich", "ein\u00b7ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der starke Bruder, dessen Wort", "tokens": ["Der", "star\u00b7ke", "Bru\u00b7der", ",", "des\u00b7sen", "Wort"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Begeisternd uns wie Wein getr\u00e4nkt?", "tokens": ["Be\u00b7geis\u00b7ternd", "uns", "wie", "Wein", "ge\u00b7tr\u00e4nkt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, hingesunken, Haupt an Haupt,", "tokens": ["Ach", ",", "hin\u00b7ge\u00b7sun\u00b7ken", ",", "Haupt", "an", "Haupt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIZU", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Wipfeln gleich, die hier gelaubt.", "tokens": ["Den", "Wip\u00b7feln", "gleich", ",", "die", "hier", "ge\u00b7laubt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Genug des Harms! Empor, mein Herz,", "tokens": ["Ge\u00b7nug", "des", "Harms", "!", "Em\u00b7por", ",", "mein", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und halt im Wechsel mutig Stand!", "tokens": ["Und", "halt", "im", "Wech\u00b7sel", "mu\u00b7tig", "Stand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu tragen lerne gro\u00dfen Schmerz,", "tokens": ["Zu", "tra\u00b7gen", "ler\u00b7ne", "gro\u00b7\u00dfen", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer gro\u00dfe Freuden einst gekannt,", "tokens": ["Wer", "gro\u00b7\u00dfe", "Freu\u00b7den", "einst", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wer im Eignen Schiffbruch litt,", "tokens": ["Und", "wer", "im", "Eig\u00b7nen", "Schiff\u00b7bruch", "litt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der leb' im Ganzen doppelt mit.", "tokens": ["Der", "leb'", "im", "Gan\u00b7zen", "dop\u00b7pelt", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Rasen deckt mein bestes Gl\u00fcck,", "tokens": ["Der", "Ra\u00b7sen", "deckt", "mein", "bes\u00b7tes", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schleichend Siechtum blies mich an;", "tokens": ["Und", "schlei\u00b7chend", "Siech\u00b7tum", "blies", "mich", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NE", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch preis' ich dankbar mein Geschick,", "tokens": ["Doch", "preis'", "ich", "dank\u00b7bar", "mein", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das mir bis heut den Faden spann:", "tokens": ["Das", "mir", "bis", "heut", "den", "Fa\u00b7den", "spann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ich sah's noch, wie mein Vaterland", "tokens": ["Ich", "sah's", "noch", ",", "wie", "mein", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu jungen Ehren auferstand.", "tokens": ["Zu", "jun\u00b7gen", "Eh\u00b7ren", "auf\u00b7er\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und ob der Rost der Jahre mir", "tokens": ["Und", "ob", "der", "Rost", "der", "Jah\u00b7re", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gemach den Ton der Harfe d\u00e4mpft,", "tokens": ["Ge\u00b7mach", "den", "Ton", "der", "Har\u00b7fe", "d\u00e4mpft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch flattert meines Lieds Panier,", "tokens": ["Noch", "flat\u00b7tert", "mei\u00b7nes", "Lieds", "Pa\u00b7nier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo man f\u00fcr Reich und Kaiser k\u00e4mpft,", "tokens": ["Wo", "man", "f\u00fcr", "Reich", "und", "Kai\u00b7ser", "k\u00e4mpft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mahnt, wo zwischen Gau und Gau", "tokens": ["Und", "mahnt", ",", "wo", "zwi\u00b7schen", "Gau", "und", "Gau"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Main sich w\u00e4lzt, zum Br\u00fcckenbau.", "tokens": ["Der", "Main", "sich", "w\u00e4lzt", ",", "zum", "Br\u00fc\u00b7cken\u00b7bau", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Getrost denn, einsam Herz! Es zieht", "tokens": ["Ge\u00b7trost", "denn", ",", "ein\u00b7sam", "Herz", "!", "Es", "zieht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "$,", "ADJD", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hell vor dir her wie Fr\u00fchrotschein;", "tokens": ["Hell", "vor", "dir", "her", "wie", "Fr\u00fch\u00b7rot\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "PTKVZ", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du darfst vielleicht dein letztes Lied", "tokens": ["Du", "darfst", "viel\u00b7leicht", "dein", "letz\u00b7tes", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Tag noch aller Deutschen weihn,", "tokens": ["Dem", "Tag", "noch", "al\u00b7ler", "Deut\u00b7schen", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem Tag des Heils, von dem du k\u00fchn", "tokens": ["Dem", "Tag", "des", "Heils", ",", "von", "dem", "du", "k\u00fchn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hier einst getr\u00e4umt im Waldesgr\u00fcn.", "tokens": ["Hier", "einst", "ge\u00b7tr\u00e4umt", "im", "Wal\u00b7des\u00b7gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}