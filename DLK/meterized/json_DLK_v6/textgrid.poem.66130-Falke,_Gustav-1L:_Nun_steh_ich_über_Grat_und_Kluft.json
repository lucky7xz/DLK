{"textgrid.poem.66130": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun steh ich \u00fcber Grat und Kluft", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun steh ich \u00fcber Grat und Kluft", "tokens": ["Nun", "steh", "ich", "\u00fc\u00b7ber", "Grat", "und", "Kluft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In abendlichen Rosen", "tokens": ["In", "a\u00b7bend\u00b7li\u00b7chen", "Ro\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Und h\u00f6re durch die klare Luft", "tokens": ["Und", "h\u00f6\u00b7re", "durch", "die", "kla\u00b7re", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Leben tief vertosen.", "tokens": ["Das", "Le\u00b7ben", "tief", "ver\u00b7to\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Adler rauscht ins Tal hinab,", "tokens": ["Ein", "Ad\u00b7ler", "rauscht", "ins", "Tal", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo meine Toten schlafen,", "tokens": ["Wo", "mei\u00b7ne", "To\u00b7ten", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was ich geliebt dort unten hab,", "tokens": ["Was", "ich", "ge\u00b7liebt", "dort", "un\u00b7ten", "hab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wei\u00df ich in sicherm Hafen.", "tokens": ["Wei\u00df", "ich", "in", "si\u00b7cherm", "Ha\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Und bin nun \u00fcber Leid und Zeit", "tokens": ["Und", "bin", "nun", "\u00fc\u00b7ber", "Leid", "und", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und meinen Sternen n\u00e4her", "tokens": ["Und", "mei\u00b7nen", "Ster\u00b7nen", "n\u00e4\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schaue in die Ewigkeit,", "tokens": ["Und", "schau\u00b7e", "in", "die", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein stillgemuter Sp\u00e4her.", "tokens": ["Ein", "still\u00b7ge\u00b7mu\u00b7ter", "Sp\u00e4\u00b7her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Durch eine selige Bl\u00e4ue schwimmt", "tokens": ["Durch", "ei\u00b7ne", "se\u00b7li\u00b7ge", "Bl\u00e4u\u00b7e", "schwimmt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ein Nachen da her\u00fcber,", "tokens": ["Ein", "Na\u00b7chen", "da", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Naht, neigt den schwanken Bord und nimmt", "tokens": ["Naht", ",", "neigt", "den", "schwan\u00b7ken", "Bord", "und", "nimmt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sanft schaukelnd mich hin\u00fcber.", "tokens": ["Sanft", "schau\u00b7kelnd", "mich", "hin\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}