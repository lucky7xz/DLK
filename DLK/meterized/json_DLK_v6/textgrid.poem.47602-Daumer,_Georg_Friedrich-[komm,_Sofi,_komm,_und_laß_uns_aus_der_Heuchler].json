{"textgrid.poem.47602": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[komm, Sofi, komm, und la\u00df uns aus der Heuchler]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, Sofi, komm, und la\u00df uns aus der Heuchler", "tokens": ["Komm", ",", "So\u00b7fi", ",", "komm", ",", "und", "la\u00df", "uns", "aus", "der", "Heuch\u00b7ler"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NE", "$,", "VVFIN", "$,", "KON", "VVIMP", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Befleckt Gewand zieh'n,", "tokens": ["Be\u00b7fleckt", "Ge\u00b7wand", "zieh'n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Uns \u00fcber ihre freche L\u00fcgentafel", "tokens": ["Uns", "\u00fc\u00b7ber", "ih\u00b7re", "fre\u00b7che", "L\u00fc\u00b7gen\u00b7ta\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die nasse Hand zieh'n!", "tokens": ["Die", "nas\u00b7se", "Hand", "zieh'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "La\u00df, \u00f6der Zelle Dunkelheit verfluchend,", "tokens": ["La\u00df", ",", "\u00f6\u00b7der", "Zel\u00b7le", "Dun\u00b7kel\u00b7heit", "ver\u00b7flu\u00b7chend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Weinpokal uns", "tokens": ["Den", "Wein\u00b7po\u00b7kal", "uns"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Aufstecken als Panier und also jauchzend", "tokens": ["Auf\u00b7ste\u00b7cken", "als", "Pa\u00b7nier", "und", "al\u00b7so", "jauch\u00b7zend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "NN", "KON", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch's weite Land zieh'n!", "tokens": ["Durch's", "wei\u00b7te", "Land", "zieh'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Jungholde Wesen, welche scheu und sch\u00fcchtern", "tokens": ["Jung\u00b7hol\u00b7de", "We\u00b7sen", ",", "wel\u00b7che", "scheu", "und", "sch\u00fcch\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADJD", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wonne meiden,", "tokens": ["Die", "Won\u00b7ne", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "La\u00df uns herbei an einem Strick von Rosen", "tokens": ["La\u00df", "uns", "her\u00b7bei", "an", "ei\u00b7nem", "Strick", "von", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Heilverband zieh'n!", "tokens": ["Zum", "Heil\u00b7ver\u00b7band", "zieh'n", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Wir wollen nichts, als gute Thaten \u00fcben;", "tokens": ["Wir", "wol\u00b7len", "nichts", ",", "als", "gu\u00b7te", "Tha\u00b7ten", "\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "$,", "KOUS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df zwischen sie uns", "tokens": ["La\u00df", "zwi\u00b7schen", "sie", "uns"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPER", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und nachtgeborne Fanatismen endlich", "tokens": ["Und", "nacht\u00b7ge\u00b7bor\u00b7ne", "Fa\u00b7na\u00b7tis\u00b7men", "end\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die scharfe Wand zieh'n!", "tokens": ["Die", "schar\u00b7fe", "Wand", "zieh'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "La\u00df uns der Liebe s\u00fc\u00dfe Fackel schleudern!", "tokens": ["La\u00df", "uns", "der", "Lie\u00b7be", "s\u00fc\u00b7\u00dfe", "Fa\u00b7ckel", "schleu\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In jede Seele", "tokens": ["In", "je\u00b7de", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Soll ein die Lust und aus Entbehrungstrauer", "tokens": ["Soll", "ein", "die", "Lust", "und", "aus", "Ent\u00b7beh\u00b7rungs\u00b7trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ART", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Unverstand zieh'n!", "tokens": ["Und", "Un\u00b7ver\u00b7stand", "zieh'n", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Glanz helle von Pallaste zu Pallaste", "tokens": ["Glanz", "hel\u00b7le", "von", "Pal\u00b7las\u00b7te", "zu", "Pal\u00b7las\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von H\u00fctte lodernd", "tokens": ["Von", "H\u00fct\u00b7te", "lo\u00b7dernd"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Zu H\u00fctte soll der weltreformatorisch", "tokens": ["Zu", "H\u00fct\u00b7te", "soll", "der", "welt\u00b7re\u00b7for\u00b7ma\u00b7to\u00b7risch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Entfachte Brand zieh'n.", "tokens": ["Ent\u00b7fach\u00b7te", "Brand", "zieh'", "n."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}}}}}