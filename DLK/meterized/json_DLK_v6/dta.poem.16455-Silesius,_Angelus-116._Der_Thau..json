{"dta.poem.16455": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "116.  Der Thau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Thau erquikt das Feld: Sol er mein Hertze laben/", "tokens": ["Der", "Thau", "er\u00b7quikt", "das", "Feld", ":", "Sol", "er", "mein", "Hert\u00b7ze", "la\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "VMFIN", "PPER", "PPOSAT", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mu\u00df er seinen fall vom Hertzen JEsu haben.", "tokens": ["So", "mu\u00df", "er", "sei\u00b7nen", "fall", "vom", "Hert\u00b7zen", "Je\u00b7su", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "NE", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}