{"textgrid.poem.46211": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An S. Churf. Durchl. Carl Ludwigen", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vnn eurer kindheit an hat sich bald der verstand", "tokens": ["Vnn", "eu\u00b7rer", "kind\u00b7heit", "an", "hat", "sich", "bald", "der", "ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APZR", "VAFIN", "PRF", "ADV", "ART", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "in euch, o gro\u00dfer prinz, recht g\u00f6tlich sehen lassen,", "tokens": ["in", "euch", ",", "o", "gro\u00b7\u00dfer", "prinz", ",", "recht", "g\u00f6t\u00b7lich", "se\u00b7hen", "las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "ADJA", "NN", "$,", "ADV", "ADJD", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "indem flei\u00df, arbeit, m\u00fch euch f\u00fchrten bei der hand,", "tokens": ["in\u00b7dem", "flei\u00df", ",", "ar\u00b7beit", ",", "m\u00fch", "euch", "f\u00fchr\u00b7ten", "bei", "der", "hand", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "ADJD", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df ihr (weis) ohn verzug die eitelkeit verlassen;", "tokens": ["da\u00df", "ihr", "(", "weis", ")", "ohn", "ver\u00b7zug", "die", "ei\u00b7tel\u00b7keit", "ver\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "PTKVZ", "$(", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Daher erleuchtet euch der tugend klarer brand,", "tokens": ["Da\u00b7her", "er\u00b7leuch\u00b7tet", "euch", "der", "tu\u00b7gend", "kla\u00b7rer", "brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "dadurch begierig ihr was recht und gut erfassen,", "tokens": ["da\u00b7durch", "be\u00b7gie\u00b7rig", "ihr", "was", "recht", "und", "gut", "er\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "PPOSAT", "PWS", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "und zu erheben euch hoch \u00fcber euern stand", "tokens": ["und", "zu", "er\u00b7he\u00b7ben", "euch", "hoch", "\u00fc\u00b7ber", "eu\u00b7ern", "stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKZU", "VVINF", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "so pfleget ihr was b\u00f6s und ungerecht zu hassen.", "tokens": ["so", "pfle\u00b7get", "ihr", "was", "b\u00f6s", "und", "un\u00b7ge\u00b7recht", "zu", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Darum wan jedem noch wird recht thun der gerecht,", "tokens": ["Da\u00b7rum", "wan", "je\u00b7dem", "noch", "wird", "recht", "thun", "der", "ge\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWAV", "PIS", "ADV", "VAFIN", "ADJD", "VVINF", "ART", "ADJD", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "so wird auch noch die arm und darbende welt prangen", "tokens": ["so", "wird", "auch", "noch", "die", "arm", "und", "dar\u00b7ben\u00b7de", "welt", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "ADJD", "KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.11": {"text": "mit euerm wahren wert und w\u00fcrdigen geschlecht:", "tokens": ["mit", "eu\u00b7erm", "wah\u00b7ren", "wert", "und", "w\u00fcr\u00b7di\u00b7gen", "ge\u00b7schlecht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJD", "KON", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und alsdan soll das reich, nach billichem verlangen,", "tokens": ["Und", "als\u00b7dan", "soll", "das", "reich", ",", "nach", "bil\u00b7li\u00b7chem", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ART", "ADJD", "$,", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "gezieret wieder ganz mit billichkeit und recht,", "tokens": ["ge\u00b7zie\u00b7ret", "wie\u00b7der", "ganz", "mit", "bil\u00b7lich\u00b7keit", "und", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "ADV", "APPR", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "mehr trost und lob von euch, dan ihr von ihm, empfangen.", "tokens": ["mehr", "trost", "und", "lob", "von", "euch", ",", "dan", "ihr", "von", "ihm", ",", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "APPR", "PPER", "$,", "ADV", "PPER", "APPR", "PPER", "$,", "VVPP", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}}}}