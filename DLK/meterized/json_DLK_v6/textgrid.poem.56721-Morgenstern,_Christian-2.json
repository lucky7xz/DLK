{"textgrid.poem.56721": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist, als h\u00e4tte die K\u00f6chin", "tokens": ["Es", "ist", ",", "als", "h\u00e4t\u00b7te", "die", "K\u00f6\u00b7chin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "des gro\u00dfen Pan", "tokens": ["des", "gro\u00b7\u00dfen", "Pan"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "\u2013 und warum sollte der gro\u00dfe Pan", "tokens": ["\u2013", "und", "wa\u00b7rum", "soll\u00b7te", "der", "gro\u00b7\u00dfe", "Pan"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "PWAV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "keine K\u00f6chin haben?", "tokens": ["kei\u00b7ne", "K\u00f6\u00b7chin", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Eine Leibnymphe,", "tokens": ["Ei\u00b7ne", "Leib\u00b7nym\u00b7phe", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "die ihm in Kratern", "tokens": ["die", "ihm", "in", "Kra\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "und Gletschert\u00f6pfen", "tokens": ["und", "Glet\u00b7scher\u00b7t\u00f6p\u00b7fen"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "k\u00f6stliche Bissen br\u00e4t", "tokens": ["k\u00f6st\u00b7li\u00b7che", "Bis\u00b7sen", "br\u00e4t"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "und ihm des Winters", "tokens": ["und", "ihm", "des", "Win\u00b7ters"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Geysir-P\u00fcnsche", "tokens": ["Gey\u00b7sir\u00b7P\u00fcn\u00b7sche"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "sorglich kredenzt? \u2013", "tokens": ["sorg\u00b7lich", "kre\u00b7denzt", "?", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Als h\u00e4tte diese K\u00f6chin", "tokens": ["Als", "h\u00e4t\u00b7te", "die\u00b7se", "K\u00f6\u00b7chin"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "eine Sch\u00fcssel mit Rotkohl", "tokens": ["ei\u00b7ne", "Sch\u00fcs\u00b7sel", "mit", "Rot\u00b7kohl"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.14": {"text": "an die Messingwand", "tokens": ["an", "die", "Mes\u00b7sing\u00b7wand"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "des Abendhimmels geschleudert.", "tokens": ["des", "A\u00b7bend\u00b7him\u00b7mels", "ge\u00b7schleu\u00b7dert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Vielleicht im Zorn,", "tokens": ["Viel\u00b7leicht", "im", "Zorn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "weil ihn der gro\u00dfe Pan", "tokens": ["weil", "ihn", "der", "gro\u00b7\u00dfe", "Pan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "nicht essen wollte ...", "tokens": ["nicht", "es\u00b7sen", "woll\u00b7te", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}