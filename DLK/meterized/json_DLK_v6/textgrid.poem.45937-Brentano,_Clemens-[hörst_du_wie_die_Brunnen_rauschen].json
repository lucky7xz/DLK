{"textgrid.poem.45937": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[h\u00f6rst du wie die Brunnen rauschen]", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rst du wie die Brunnen rauschen,", "tokens": ["H\u00f6rst", "du", "wie", "die", "Brun\u00b7nen", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rst du wie die Grille zirpt?", "tokens": ["H\u00f6rst", "du", "wie", "die", "Gril\u00b7le", "zirpt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stille, stille, la\u00df uns lauschen,", "tokens": ["Stil\u00b7le", ",", "stil\u00b7le", ",", "la\u00df", "uns", "lau\u00b7schen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selig, wer in Tr\u00e4umen stirbt.", "tokens": ["Se\u00b7lig", ",", "wer", "in", "Tr\u00e4u\u00b7men", "stirbt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Selig, wen die Wolken wiegen,", "tokens": ["Se\u00b7lig", ",", "wen", "die", "Wol\u00b7ken", "wie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wem der Mond ein Schlaflied singt,", "tokens": ["Wem", "der", "Mond", "ein", "Schlaf\u00b7lied", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "O wie selig kann der fliegen,", "tokens": ["O", "wie", "se\u00b7lig", "kann", "der", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VMFIN", "ART", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dem der Traum den Fl\u00fcgel schwingt,", "tokens": ["Dem", "der", "Traum", "den", "Fl\u00fc\u00b7gel", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Da\u00df an blauer Himmelsdecke", "tokens": ["Da\u00df", "an", "blau\u00b7er", "Him\u00b7mels\u00b7de\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sterne er wie Blumen pfl\u00fcckt:", "tokens": ["Ster\u00b7ne", "er", "wie", "Blu\u00b7men", "pfl\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KOKOM", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Schlafe, tr\u00e4ume, flieg', ich wecke", "tokens": ["Schla\u00b7fe", ",", "tr\u00e4u\u00b7me", ",", "flieg'", ",", "ich", "we\u00b7cke"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Bald Dich auf und bin begl\u00fcckt.", "tokens": ["Bald", "Dich", "auf", "und", "bin", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKVZ", "KON", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}