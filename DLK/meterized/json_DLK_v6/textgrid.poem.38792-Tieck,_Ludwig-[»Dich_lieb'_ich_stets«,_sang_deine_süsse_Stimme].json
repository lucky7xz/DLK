{"textgrid.poem.38792": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[\u00bbDich lieb' ich stets\u00ab, sang deine s\u00fcsse Stimme]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdich lieb' ich stets\u00ab, sang deine s\u00fcsse Stimme,", "tokens": ["\u00bb", "dich", "lieb'", "ich", "stets", "\u00ab", ",", "sang", "dei\u00b7ne", "s\u00fcs\u00b7se", "Stim\u00b7me", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "$(", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mit dem Wort gabst du ein lieblich Blicken,", "tokens": ["Und", "mit", "dem", "Wort", "gabst", "du", "ein", "lieb\u00b7lich", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das fiel in's durstge Herz, labend Erquicken,", "tokens": ["Das", "fiel", "in's", "durst\u00b7ge", "Herz", ",", "la\u00b7bend", "Er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Als wenn im Dunkel Morgenr\u00f6the glimme.", "tokens": ["Als", "wenn", "im", "Dun\u00b7kel", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "glim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ja dich nur lieb' ich stets, auch wenn im Grimme", "tokens": ["Ja", "dich", "nur", "lieb'", "ich", "stets", ",", "auch", "wenn", "im", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "ADV", "VVFIN", "PPER", "ADV", "$,", "ADV", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir Leid, Weh, Noth das Leben noch will schicken,", "tokens": ["Mir", "Leid", ",", "Weh", ",", "Noth", "das", "Le\u00b7ben", "noch", "will", "schi\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "NN", "ART", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Worte sing' ich laut noch mit Entz\u00fccken", "tokens": ["Die", "Wor\u00b7te", "sing'", "ich", "laut", "noch", "mit", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn ich den dunkeln Flu\u00df hinunter schwimme.", "tokens": ["Wenn", "ich", "den", "dun\u00b7keln", "Flu\u00df", "hin\u00b7un\u00b7ter", "schwim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPO", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So t\u00f6nte Orpheus Laute in den Wogen", "tokens": ["So", "t\u00f6n\u00b7te", "Or\u00b7pheus", "Lau\u00b7te", "in", "den", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und widerklang das tiefe Bett des Flusses,", "tokens": ["Und", "wi\u00b7der\u00b7klang", "das", "tie\u00b7fe", "Bett", "des", "Flus\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Ufer sangen nach die herben Schmerzen.", "tokens": ["Die", "U\u00b7fer", "san\u00b7gen", "nach", "die", "her\u00b7ben", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn schon der Tod gespannt den finstern Bogen,", "tokens": ["Wenn", "schon", "der", "Tod", "ge\u00b7spannt", "den", "fins\u00b7tern", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denk' ich des Blicks, des Klangs, des s\u00fcssen Kusses,", "tokens": ["Denk'", "ich", "des", "Blicks", ",", "des", "Klangs", ",", "des", "s\u00fcs\u00b7sen", "Kus\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und singe leis': Du bl\u00fchst mir noch im Herzen.", "tokens": ["Und", "sin\u00b7ge", "leis'", ":", "Du", "bl\u00fchst", "mir", "noch", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}