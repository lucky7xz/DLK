{"textgrid.poem.42106": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun wird die Spur der F\u00fc\u00dfe langsam ungetan,", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun wird die Spur der F\u00fc\u00dfe langsam ungetan,", "tokens": ["Nun", "wird", "die", "Spur", "der", "F\u00fc\u00b7\u00dfe", "lang\u00b7sam", "un\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und aus der Tiefe, aus der tiefen Tiefe steigt", "tokens": ["Und", "aus", "der", "Tie\u00b7fe", ",", "aus", "der", "tie\u00b7fen", "Tie\u00b7fe", "steigt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Tr\u00fcbe, schwadengrauer Nebel himmelan.", "tokens": ["Das", "Tr\u00fc\u00b7be", ",", "schwa\u00b7den\u00b7grau\u00b7er", "Ne\u00b7bel", "him\u00b7me\u00b7lan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nun wird der Augen-Aufblick langsam leer,", "tokens": ["Nun", "wird", "der", "Au\u00b7gen\u00b7Auf\u00b7blick", "lang\u00b7sam", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und aus der H\u00f6he, aus der hohen H\u00f6he neigt", "tokens": ["Und", "aus", "der", "H\u00f6\u00b7he", ",", "aus", "der", "ho\u00b7hen", "H\u00f6\u00b7he", "neigt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Wolke sich, sinkt Nebel erdw\u00e4rts schwer.", "tokens": ["Die", "Wol\u00b7ke", "sich", ",", "sinkt", "Ne\u00b7bel", "erd\u00b7w\u00e4rts", "schwer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "$,", "VVFIN", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun dr\u00e4ngt zu dem verwandten Un-Gesicht", "tokens": ["Nun", "dr\u00e4ngt", "zu", "dem", "ver\u00b7wand\u00b7ten", "Un\u00b7Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Wesenlose aus den fahlen Gr\u00fcnden", "tokens": ["Das", "We\u00b7sen\u00b7lo\u00b7se", "aus", "den", "fah\u00b7len", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und hebt sich sehnend ins vers\u00e4umte Licht.", "tokens": ["Und", "hebt", "sich", "seh\u00b7nend", "ins", "ver\u00b7s\u00e4um\u00b7te", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun flieht, was war: es fliehen Busch und Baum,", "tokens": ["Nun", "flieht", ",", "was", "war", ":", "es", "flie\u00b7hen", "Busch", "und", "Baum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "VAFIN", "$.", "PPER", "ADJA", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Flieh'n Berg und Tal, die sich zur Flucht verb\u00fcnden,", "tokens": ["Flieh'n", "Berg", "und", "Tal", ",", "die", "sich", "zur", "Flucht", "ver\u00b7b\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es fliehst du, Herz. Es floh'n die Zeit, der Raum.", "tokens": ["Es", "fliehst", "du", ",", "Herz", ".", "Es", "floh'n", "die", "Zeit", ",", "der", "Raum", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Land wurde Meer. Meer wurde schw\u00e4lend Schaum.", "tokens": ["Land", "wur\u00b7de", "Meer", ".", "Meer", "wur\u00b7de", "schw\u00e4\u00b7lend", "Schaum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$.", "NN", "VAFIN", "ADJD", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ihn schl\u00fcrft, sich fr\u00f6stelnd zu entz\u00fcnden,", "tokens": ["Ihn", "schl\u00fcrft", ",", "sich", "fr\u00f6s\u00b7telnd", "zu", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das ungelebte Leben und der ungetr\u00e4umte Traum.", "tokens": ["Das", "un\u00b7ge\u00b7leb\u00b7te", "Le\u00b7ben", "und", "der", "un\u00b7ge\u00b7tr\u00e4um\u00b7te", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}