{"dta.poem.23315": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Rococo's Glaubensbekenntni\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich stimme f\u00fcr die Monarchie,", "tokens": ["Ich", "stim\u00b7me", "f\u00fcr", "die", "Mon\u00b7ar\u00b7chie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da giebt's noch R\u00e4ng' und St\u00e4nde;", "tokens": ["Da", "giebt's", "noch", "R\u00e4ng'", "und", "St\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Republik geht Poesie", "tokens": ["Mit", "Re\u00b7pub\u00b7lik", "geht", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "NN"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und alles Gl\u00fcck zu Ende.", "tokens": ["Und", "al\u00b7les", "Gl\u00fcck", "zu", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich stimme f\u00fcr die Monarchie;", "tokens": ["Ich", "stim\u00b7me", "f\u00fcr", "die", "Mon\u00b7ar\u00b7chie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn wir darin nicht w\u00e4ren,", "tokens": ["Wenn", "wir", "da\u00b7rin", "nicht", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie k\u00f6nnten wir doch ohne sie", "tokens": ["Wie", "k\u00f6nn\u00b7ten", "wir", "doch", "oh\u00b7ne", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So viele Leut' ern\u00e4hren.", "tokens": ["So", "vie\u00b7le", "Leut'", "er\u00b7n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich stimme f\u00fcr die Monarchie,", "tokens": ["Ich", "stim\u00b7me", "f\u00fcr", "die", "Mon\u00b7ar\u00b7chie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr W\u00fcrden, Titel, Orden;", "tokens": ["F\u00fcr", "W\u00fcr\u00b7den", ",", "Ti\u00b7tel", ",", "Or\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In Republiken sind noch nie", "tokens": ["In", "Re\u00b7pub\u00b7li\u00b7ken", "sind", "noch", "nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "ADV", "ADV"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Verdienste was geworden.", "tokens": ["Ver\u00b7diens\u00b7te", "was", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich stimme f\u00fcr die Monarchie,", "tokens": ["Ich", "stim\u00b7me", "f\u00fcr", "die", "Mon\u00b7ar\u00b7chie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo die Censur noch waltet,", "tokens": ["Wo", "die", "Cen\u00b7sur", "noch", "wal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wo nicht der Presse Despotie", "tokens": ["Wo", "nicht", "der", "Pres\u00b7se", "Des\u00b7po\u00b7tie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Herzensl\u00fcsten schaltet.", "tokens": ["Nach", "Her\u00b7zens\u00b7l\u00fcs\u00b7ten", "schal\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich stimme f\u00fcr die Monarchie,", "tokens": ["Ich", "stim\u00b7me", "f\u00fcr", "die", "Mon\u00b7ar\u00b7chie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo weise wird regieret,", "tokens": ["Wo", "wei\u00b7se", "wird", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weil Grundbesitz mit Hab' und Vieh", "tokens": ["Weil", "Grund\u00b7be\u00b7sitz", "mit", "Hab'", "und", "Vieh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur ist repr\u00e4sentieret.", "tokens": ["Nur", "ist", "re\u00b7pr\u00e4\u00b7sen\u00b7tie\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich stimme f\u00fcr die Monarchie,", "tokens": ["Ich", "stim\u00b7me", "f\u00fcr", "die", "Mon\u00b7ar\u00b7chie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die giebt noch gute Rente;", "tokens": ["Die", "giebt", "noch", "gu\u00b7te", "Ren\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es gab die Republik doch nie", "tokens": ["Es", "gab", "die", "Re\u00b7pub\u00b7lik", "doch", "nie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vier oder f\u00fcnf Procente.", "tokens": ["Vier", "o\u00b7der", "f\u00fcnf", "Pro\u00b7cen\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "KON", "CARD", "NN", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}}, "stanza.7": {"line.1": {"text": "Drum la\u00df ich mir die Monarchie", "tokens": ["Drum", "la\u00df", "ich", "mir", "die", "Mon\u00b7ar\u00b7chie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch nun und nimmer rauben:", "tokens": ["Auch", "nun", "und", "nim\u00b7mer", "rau\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir haben Eine Liturgie,", "tokens": ["Wir", "ha\u00b7ben", "Ei\u00b7ne", "Li\u00b7tur\u00b7gie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Einen Gott und Glauben.", "tokens": ["Und", "Ei\u00b7nen", "Gott", "und", "Glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}