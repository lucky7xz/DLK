{"textgrid.poem.43410": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht das Laster, nicht die Tugend,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht das Laster, nicht die Tugend,", "tokens": ["Nicht", "das", "Las\u00b7ter", ",", "nicht", "die", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht das Geld regiert die Welt.", "tokens": ["Nicht", "das", "Geld", "re\u00b7giert", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frag das Alter, frag die Jugend,", "tokens": ["Frag", "das", "Al\u00b7ter", ",", "frag", "die", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer den K\u00f6nigsscepter h\u00e4lt?", "tokens": ["Wer", "den", "K\u00f6\u00b7nigs\u00b7scep\u00b7ter", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Gewohnheit ist erhoben", "tokens": ["Die", "Ge\u00b7wohn\u00b7heit", "ist", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den allerh\u00f6chsten Thron,", "tokens": ["Auf", "den", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Thron", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist wie Gott im Himmel droben", "tokens": ["Ist", "wie", "Gott", "im", "Him\u00b7mel", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "KOKOM", "NN", "APPRART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vater, heil'ger Geist und Sohn.", "tokens": ["Va\u00b7ter", ",", "heil'\u00b7ger", "Geist", "und", "Sohn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wie soll es anders werden", "tokens": ["Und", "wie", "soll", "es", "an\u00b7ders", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "ADV", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr das menschliche Geschlecht?", "tokens": ["F\u00fcr", "das", "menschli\u00b7che", "Ge\u00b7schlecht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Gewohnheit herrscht auf Erden,", "tokens": ["Die", "Ge\u00b7wohn\u00b7heit", "herrscht", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihr Regiment ist schlecht.", "tokens": ["Und", "ihr", "Re\u00b7gi\u00b7ment", "ist", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}