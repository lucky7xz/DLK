{"textgrid.poem.25207": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im herben Wind, am Dornenzaun,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im herben Wind, am Dornenzaun,", "tokens": ["Im", "her\u00b7ben", "Wind", ",", "am", "Dor\u00b7nen\u00b7zaun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei toten, raschelnden Ranken,", "tokens": ["Bei", "to\u00b7ten", ",", "ra\u00b7scheln\u00b7den", "Ran\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ver\u00f6det mu\u00df dies Greisenhaupt", "tokens": ["Ver\u00b7\u00f6\u00b7det", "mu\u00df", "dies", "Grei\u00b7sen\u00b7haupt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "VMFIN", "PDS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die tr\u00fcben Tage durchwanken.", "tokens": ["Die", "tr\u00fc\u00b7ben", "Ta\u00b7ge", "durch\u00b7wan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und aschend\u00fcrr und aschenfahl,", "tokens": ["Und", "asc\u00b7hen\u00b7d\u00fcrr", "und", "asc\u00b7hen\u00b7fahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Gram gebeugt, hinab", "tokens": ["Von", "Gram", "ge\u00b7beugt", ",", "hin\u00b7ab"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "NE", "VVPP", "$,", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zur w\u00fcsten Erde starren:", "tokens": ["Zur", "w\u00fcs\u00b7ten", "Er\u00b7de", "star\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du meiner Hoffnung Grab! \u2013", "tokens": ["Du", "mei\u00b7ner", "Hoff\u00b7nung", "Grab", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach wohl, im Sommer! \u2013 als flammend hei\u00df", "tokens": ["Ach", "wohl", ",", "im", "Som\u00b7mer", "!", "\u2013", "als", "flam\u00b7mend", "hei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ITJ", "ADV", "$,", "APPRART", "NN", "$.", "$(", "KOUS", "ADJD", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Blauen die Sonne stand,", "tokens": ["Im", "Blau\u00b7en", "die", "Son\u00b7ne", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da war von \u00fcppigen Tr\u00e4umen", "tokens": ["Da", "war", "von", "\u00fcp\u00b7pi\u00b7gen", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mein jugendlich Haupt entbrannt.", "tokens": ["Mein", "ju\u00b7gend\u00b7lich", "Haupt", "ent\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich loderte glutig und d\u00fcnkte mich selbst", "tokens": ["Ich", "lo\u00b7der\u00b7te", "glu\u00b7tig", "und", "d\u00fcnk\u00b7te", "mich", "selbst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Solch herrlicher Flammenbronnen", "tokens": ["Solch", "herr\u00b7li\u00b7cher", "Flam\u00b7men\u00b7bron\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wollte im Herbste Garten und Flur", "tokens": ["Und", "woll\u00b7te", "im", "Herbs\u00b7te", "Gar\u00b7ten", "und", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPRART", "ADJA", "NN", "KON", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bes\u00e4en mit roten Sonnen.", "tokens": ["Be\u00b7s\u00e4\u00b7en", "mit", "ro\u00b7ten", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Doch als er kam, der Herbst \u2013 da ward", "tokens": ["Doch", "als", "er", "kam", ",", "der", "Herbst", "\u2013", "da", "ward"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ART", "NN", "$(", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich zage wie welkendes Laub,", "tokens": ["Ich", "za\u00b7ge", "wie", "wel\u00b7ken\u00b7des", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und als ich neigte mein Haupt zur Saat,", "tokens": ["Und", "als", "ich", "neig\u00b7te", "mein", "Haupt", "zur", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da war manch K\u00f6rnlein taub.", "tokens": ["Da", "war", "manch", "K\u00f6rn\u00b7lein", "taub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und etliches fiel auf d\u00fcrres Gestein,", "tokens": ["Und", "et\u00b7li\u00b7ches", "fiel", "auf", "d\u00fcr\u00b7res", "Ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Vogel hat es gepickt,", "tokens": ["Der", "Vo\u00b7gel", "hat", "es", "ge\u00b7pickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und etliches wird, wenn es keimt, zertreten", "tokens": ["Und", "et\u00b7li\u00b7ches", "wird", ",", "wenn", "es", "keimt", ",", "zer\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PIS", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Oder von Dornen erstickt.", "tokens": ["O\u00b7der", "von", "Dor\u00b7nen", "er\u00b7stickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Und etliches hat der barsche Sturm", "tokens": ["Und", "et\u00b7li\u00b7ches", "hat", "der", "bar\u00b7sche", "Sturm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Geschleudert \u2013 wei\u00df nicht wohin \u2013", "tokens": ["Ge\u00b7schleu\u00b7dert", "\u2013", "wei\u00df", "nicht", "wo\u00b7hin", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "VVFIN", "PTKNEG", "PWAV", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auch den vermessenen Jugendtraum", "tokens": ["Auch", "den", "ver\u00b7mes\u00b7se\u00b7nen", "Ju\u00b7gend\u00b7traum"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Gezaust mir aus dem Sinn. \u2013", "tokens": ["Ge\u00b7zaust", "mir", "aus", "dem", "Sinn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nun steh ich hier am Dornenzaun", "tokens": ["Nun", "steh", "ich", "hier", "am", "Dor\u00b7nen\u00b7zaun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei toten, raschelnden Ranken", "tokens": ["Bei", "to\u00b7ten", ",", "ra\u00b7scheln\u00b7den", "Ran\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und mu\u00df mit \u00f6dem Greisenhaupt", "tokens": ["Und", "mu\u00df", "mit", "\u00f6\u00b7dem", "Grei\u00b7sen\u00b7haupt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die tr\u00fcben Tage durchwanken.", "tokens": ["Die", "tr\u00fc\u00b7ben", "Ta\u00b7ge", "durch\u00b7wan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "O Jugend, du fliegst k\u00fchn und rasch,", "tokens": ["O", "Ju\u00b7gend", ",", "du", "fliegst", "k\u00fchn", "und", "rasch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wie die Schwalbe schnellt;", "tokens": ["So", "wie", "die", "Schwal\u00b7be", "schnellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch, gleich der Schnecke tr\u00e4ge, schleicht", "tokens": ["Doch", ",", "gleich", "der", "Schne\u00b7cke", "tr\u00e4\u00b7ge", ",", "schleicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "ADV", "ART", "NN", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Ewigkeit die Welt.", "tokens": ["In", "E\u00b7wig\u00b7keit", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}