{"textgrid.poem.54535": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df du den Bau gemacht/ den Bau der sch\u00f6nen Welt", "tokens": ["Da\u00df", "du", "den", "Bau", "ge\u00b7macht", "/", "den", "Bau", "der", "sch\u00f6\u00b7nen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$(", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und so viel tausend Heer unendlich heller Lichter", "tokens": ["Und", "so", "viel", "tau\u00b7send", "Heer", "un\u00b7end\u00b7lich", "hel\u00b7ler", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "CARD", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und C\u00f6rper die die Krafft gleich fallender Gewichter", "tokens": ["Und", "C\u00f6r\u00b7per", "die", "die", "Krafft", "gleich", "fal\u00b7len\u00b7der", "Ge\u00b7wich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "ART", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An dem gesetzten Ort/ durch deinen Schlu\u00df erh\u00e4lt/", "tokens": ["An", "dem", "ge\u00b7setz\u00b7ten", "Ort", "/", "durch", "dei\u00b7nen", "Schlu\u00df", "er\u00b7h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df du die C\u00f6rper selbst mit so viel Schmuck bestellt", "tokens": ["Da\u00df", "du", "die", "C\u00f6r\u00b7per", "selbst", "mit", "so", "viel", "Schmuck", "be\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "ADV", "PIAT", "NN", "VVFIN"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und auf der Erden Hau\u00df unzehlich Angesichter/", "tokens": ["Und", "auf", "der", "Er\u00b7den", "Hau\u00df", "un\u00b7zeh\u00b7lich", "An\u00b7ge\u00b7sich\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die ungleich dennoch gleich als vorgesetzte Richter", "tokens": ["Die", "un\u00b7gleich", "den\u00b7noch", "gleich", "als", "vor\u00b7ge\u00b7setz\u00b7te", "Rich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "ADV", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Aussprechen da\u00df nur dir nichts gleich wird hier vermeldt.", "tokens": ["Aus\u00b7spre\u00b7chen", "da\u00df", "nur", "dir", "nichts", "gleich", "wird", "hier", "ver\u00b7meldt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "ADV", "PPER", "PIS", "ADV", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Di\u00df r\u00fchm ich doch noch mehr da\u00df du mir wollen g\u00f6nnen/", "tokens": ["Di\u00df", "r\u00fchm", "ich", "doch", "noch", "mehr", "da\u00df", "du", "mir", "wol\u00b7len", "g\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "ADV", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df Herr/ dein Wunderwerck ich habe r\u00fchmen k\u00f6nnen:", "tokens": ["Da\u00df", "Herr", "/", "dein", "Wun\u00b7der\u00b7werck", "ich", "ha\u00b7be", "r\u00fch\u00b7men", "k\u00f6n\u00b7nen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "PPOSAT", "NN", "PPER", "VAFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df du die Augen mir zu schauen auffgemacht.", "tokens": ["Da\u00df", "du", "die", "Au\u00b7gen", "mir", "zu", "schau\u00b7en", "auff\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Di\u00df r\u00fchm ich doch noch mehr da\u00df du mir mehr wilt zeigen", "tokens": ["Di\u00df", "r\u00fchm", "ich", "doch", "noch", "mehr", "da\u00df", "du", "mir", "mehr", "wilt", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "ADV", "KOUS", "PPER", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Als diese Welt begreifft und mir versprichst zu eigen", "tokens": ["Als", "die\u00b7se", "Welt", "be\u00b7greifft", "und", "mir", "ver\u00b7sprichst", "zu", "ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "VVFIN", "KON", "PPER", "VVFIN", "PTKA", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dein Hau\u00df/ mehr dich/ den nichts satt schaut und satt betracht.", "tokens": ["Dein", "Hau\u00df", "/", "mehr", "dich", "/", "den", "nichts", "satt", "schaut", "und", "satt", "be\u00b7tracht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADV", "PPER", "$(", "ART", "PIS", "ADJD", "VVFIN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}