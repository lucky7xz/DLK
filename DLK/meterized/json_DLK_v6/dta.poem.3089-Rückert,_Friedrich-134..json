{"dta.poem.3089": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "134.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du dich lebenslang besch\u00e4ftigest mit W\u00f6rtern,", "tokens": ["Wenn", "du", "dich", "le\u00b7bens\u00b7lang", "be\u00b7sch\u00e4f\u00b7ti\u00b7gest", "mit", "W\u00f6r\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verachten dich mit Recht, die lieber Ding' er\u00f6rtern.", "tokens": ["Ver\u00b7ach\u00b7ten", "dich", "mit", "Recht", ",", "die", "lie\u00b7ber", "Ding'", "er\u00b7\u00f6r\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,", "PRELS", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn du dich wenigstens besch\u00e4ftigest mit Worten,", "tokens": ["Wenn", "du", "dich", "we\u00b7nigs\u00b7tens", "be\u00b7sch\u00e4f\u00b7ti\u00b7gest", "mit", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus welchen aufgebaut sind der Begriffe Pforten!", "tokens": ["Aus", "wel\u00b7chen", "auf\u00b7ge\u00b7baut", "sind", "der", "Be\u00b7grif\u00b7fe", "Pfor\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "VVPP", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch wenn du wirklich dich besch\u00e4ftigst mit dem Wort;", "tokens": ["Doch", "wenn", "du", "wirk\u00b7lich", "dich", "be\u00b7sch\u00e4f\u00b7tigst", "mit", "dem", "Wort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist nichts h\u00f6heres zu finden hier noch dort.", "tokens": ["Es", "ist", "nichts", "h\u00f6\u00b7he\u00b7res", "zu", "fin\u00b7den", "hier", "noch", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADJA", "PTKZU", "VVINF", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}