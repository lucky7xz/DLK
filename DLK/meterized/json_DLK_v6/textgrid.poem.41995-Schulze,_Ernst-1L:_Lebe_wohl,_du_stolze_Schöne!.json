{"textgrid.poem.41995": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lebe wohl, du stolze Sch\u00f6ne!", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lebe wohl, du stolze Sch\u00f6ne!", "tokens": ["Le\u00b7be", "wohl", ",", "du", "stol\u00b7ze", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich entfliehe weil ich will;", "tokens": ["Ich", "ent\u00b7flie\u00b7he", "weil", "ich", "will", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dr\u00fcckend sind mir deine Fesseln,", "tokens": ["Dr\u00fc\u00b7ckend", "sind", "mir", "dei\u00b7ne", "Fes\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und auf einem Bett von Nesseln", "tokens": ["Und", "auf", "ei\u00b7nem", "Bett", "von", "Nes\u00b7seln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liegt nur Manchas Ritter still.", "tokens": ["Liegt", "nur", "Man\u00b7chas", "Rit\u00b7ter", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lebe wohl, du stolze Sch\u00f6ne!", "tokens": ["Le\u00b7be", "wohl", ",", "du", "stol\u00b7ze", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwar dein Mund ist k\u00fcssenswerth,", "tokens": ["Zwar", "dein", "Mund", "ist", "k\u00fcs\u00b7sens\u00b7werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Feuer gl\u00fcht in deinen Blicken:", "tokens": ["Feu\u00b7er", "gl\u00fcht", "in", "dei\u00b7nen", "Bli\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber Thoren nur und M\u00fccken", "tokens": ["A\u00b7ber", "Tho\u00b7ren", "nur", "und", "M\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Traun dem Schein, der sie verzehrt.", "tokens": ["Traun", "dem", "Schein", ",", "der", "sie", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lebe wohl, du stolze Sch\u00f6ne!", "tokens": ["Le\u00b7be", "wohl", ",", "du", "stol\u00b7ze", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch du hast nicht ganz verspielt,", "tokens": ["Doch", "du", "hast", "nicht", "ganz", "ver\u00b7spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn du hieltest eine Woche", "tokens": ["Denn", "du", "hiel\u00b7test", "ei\u00b7ne", "Wo\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4nger mich in deinem Joche", "tokens": ["L\u00e4n\u00b7ger", "mich", "in", "dei\u00b7nem", "Jo\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Als mich je ein M\u00e4dchen hielt.", "tokens": ["Als", "mich", "je", "ein", "M\u00e4d\u00b7chen", "hielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lebe wohl, du stolze Sch\u00f6ne!", "tokens": ["Le\u00b7be", "wohl", ",", "du", "stol\u00b7ze", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fcnstlerisch bist du gewi\u00df,", "tokens": ["K\u00fcnst\u00b7le\u00b7risch", "bist", "du", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn das Netz, dem ich entronnen,", "tokens": ["Denn", "das", "Netz", ",", "dem", "ich", "ent\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "War so schlau, so zart gesponnen,", "tokens": ["War", "so", "schlau", ",", "so", "zart", "ge\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "War so fein da\u00df es \u2013 zerri\u00df.", "tokens": ["War", "so", "fein", "da\u00df", "es", "\u2013", "zer\u00b7ri\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KOUS", "PPER", "$(", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Lebe wohl, du stolze Sch\u00f6ne!", "tokens": ["Le\u00b7be", "wohl", ",", "du", "stol\u00b7ze", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dichter haben leichten Sinn;", "tokens": ["Dich\u00b7ter", "ha\u00b7ben", "leich\u00b7ten", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df die Veilchen heute schwinden,", "tokens": ["La\u00df", "die", "Veil\u00b7chen", "heu\u00b7te", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgen wirst du Rosen finden,", "tokens": ["Mor\u00b7gen", "wirst", "du", "Ro\u00b7sen", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jetzt erg\u00f6tzt und hin ist hin!", "tokens": ["Jetzt", "er\u00b7g\u00f6tzt", "und", "hin", "ist", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "ADV", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Lebe wohl, du stolze Sch\u00f6ne!", "tokens": ["Le\u00b7be", "wohl", ",", "du", "stol\u00b7ze", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmer st\u00f6rst du meine Ruh!", "tokens": ["Nim\u00b7mer", "st\u00f6rst", "du", "mei\u00b7ne", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00e4rbt die Scham nicht deine Wangen?", "tokens": ["F\u00e4rbt", "die", "Scham", "nicht", "dei\u00b7ne", "Wan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmetterling, du bist gefangen,", "tokens": ["Schmet\u00b7ter\u00b7ling", ",", "du", "bist", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich bin fl\u00fcchtiger als du!", "tokens": ["Ich", "bin", "fl\u00fcch\u00b7ti\u00b7ger", "als", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KOKOM", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}