{"textgrid.poem.49234": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Spiegel send' ich dir, du Spiegel aller Frauen,", "tokens": ["Den", "Spie\u00b7gel", "send'", "ich", "dir", ",", "du", "Spie\u00b7gel", "al\u00b7ler", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$,", "PPER", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du die G\u00f6ttlichkeit an dir recht m\u00f6gest schauen,", "tokens": ["Da\u00df", "du", "die", "G\u00f6tt\u00b7lich\u00b7keit", "an", "dir", "recht", "m\u00f6\u00b7gest", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob gleich kein Spiegel ist zu treffen irgend an,", "tokens": ["Ob", "gleich", "kein", "Spie\u00b7gel", "ist", "zu", "tref\u00b7fen", "ir\u00b7gend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "VAFIN", "PTKZU", "VVINF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der dich, du sch\u00f6nes Bild, schnurrecht entwerfen kan.", "tokens": ["Der", "dich", ",", "du", "sch\u00f6\u00b7nes", "Bild", ",", "schnur\u00b7recht", "ent\u00b7wer\u00b7fen", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PPER", "ADJA", "NN", "$,", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch soltest gleichwol du erkennen meine Sinnen,", "tokens": ["Doch", "sol\u00b7test", "gleich\u00b7wol", "du", "er\u00b7ken\u00b7nen", "mei\u00b7ne", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PPER", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du w\u00fcrdest dich gewi\u00df leibhaftig sehen k\u00f6nnen;", "tokens": ["Du", "w\u00fcr\u00b7dest", "dich", "ge\u00b7wi\u00df", "leib\u00b7haf\u00b7tig", "se\u00b7hen", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dann wi\u00df', ich bilde dich mir da so heftig ein,", "tokens": ["Dann", "wi\u00df'", ",", "ich", "bil\u00b7de", "dich", "mir", "da", "so", "hef\u00b7tig", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df du dir auch selbselbst nicht kanst so \u00e4hnlich sein.", "tokens": ["Da\u00df", "du", "dir", "auch", "selb\u00b7selbst", "nicht", "kanst", "so", "\u00e4hn\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "PTKNEG", "VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}