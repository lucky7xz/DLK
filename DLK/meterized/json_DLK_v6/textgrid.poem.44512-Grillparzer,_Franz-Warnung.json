{"textgrid.poem.44512": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Warnung", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit einem Schwerte spielt man nicht,", "tokens": ["Mit", "ei\u00b7nem", "Schwer\u00b7te", "spielt", "man", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIS", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man zieht es, wills die Not,", "tokens": ["Man", "zieht", "es", ",", "wills", "die", "Not", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch mancher, ders im Scherze schwang,", "tokens": ["Doch", "man\u00b7cher", ",", "ders", "im", "Scher\u00b7ze", "schwang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Traf, was ihm n\u00e4chst, zum Tod.", "tokens": ["Traf", ",", "was", "ihm", "n\u00e4chst", ",", "zum", "Tod", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Man spielt auch nicht mit Lieb und Wein,", "tokens": ["Man", "spielt", "auch", "nicht", "mit", "Lieb", "und", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum mindsten, wenn man klug,", "tokens": ["Zum", "minds\u00b7ten", ",", "wenn", "man", "klug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "KOUS", "PIS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Licht und Flamme, die der Wind", "tokens": ["Mit", "Licht", "und", "Flam\u00b7me", ",", "die", "der", "Wind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Oft weit und weiter trug.", "tokens": ["Oft", "weit", "und", "wei\u00b7ter", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So spiel auch du mit Freiheit nicht,", "tokens": ["So", "spiel", "auch", "du", "mit", "Frei\u00b7heit", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPR", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erweckend ist ihr Ton,", "tokens": ["Er\u00b7we\u00b7ckend", "ist", "ihr", "Ton", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn du sie nicht geben willst,", "tokens": ["Und", "wenn", "du", "sie", "nicht", "ge\u00b7ben", "willst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sprich auch nicht davon.", "tokens": ["So", "sprich", "auch", "nicht", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKNEG", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}