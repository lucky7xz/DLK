{"textgrid.poem.44324": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[des Winters Hauch]", "genre": "verse", "period": "N.A.", "pub_year": 1812, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Winters Hauch", "tokens": ["Des", "Win\u00b7ters", "Hauch"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "entbl\u00e4ttert den Strauch,", "tokens": ["ent\u00b7bl\u00e4t\u00b7tert", "den", "Strauch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "und w\u00fctende Sturmwinde heulen;", "tokens": ["und", "w\u00fc\u00b7ten\u00b7de", "Sturm\u00b7win\u00b7de", "heu\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "an des H\u00fcgels Hang,", "tokens": ["an", "des", "H\u00fc\u00b7gels", "Hang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "wo die Lerche sonst sang,", "tokens": ["wo", "die", "Ler\u00b7che", "sonst", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "erkr\u00e4chzen nun Raben und Eulen.", "tokens": ["er\u00b7kr\u00e4ch\u00b7zen", "nun", "Ra\u00b7ben", "und", "Eu\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Die Rose liegt", "tokens": ["Die", "Ro\u00b7se", "liegt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "vom Frost geknickt,", "tokens": ["vom", "Frost", "ge\u00b7knickt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "und jubelnd h\u00fcllet der Winter", "tokens": ["und", "ju\u00b7belnd", "h\u00fcl\u00b7let", "der", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "in raschem Flug", "tokens": ["in", "ra\u00b7schem", "Flug"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "sein Leichentuch", "tokens": ["sein", "Lei\u00b7chen\u00b7tuch"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "um Floras bl\u00fchende Kinder.", "tokens": ["um", "Flo\u00b7ras", "bl\u00fc\u00b7hen\u00b7de", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Schwalbe ruft", "tokens": ["Die", "Schwal\u00b7be", "ruft"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "aus rauher Luft", "tokens": ["aus", "rau\u00b7her", "Luft"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "ihr Lebewohl hernieder,", "tokens": ["ihr", "Le\u00b7be\u00b7wohl", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "blickt noch einmal herab", "tokens": ["blickt", "noch", "ein\u00b7mal", "her\u00b7ab"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "auf das weite Grab", "tokens": ["auf", "das", "wei\u00b7te", "Grab"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "und flieht dann auf schnellem Gefieder,", "tokens": ["und", "flieht", "dann", "auf", "schnel\u00b7lem", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "und alles ist stumm", "tokens": ["und", "al\u00b7les", "ist", "stumm"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "und tot ringsum,", "tokens": ["und", "tot", "ring\u00b7sum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "kein Laut ert\u00f6nt aus den H\u00f6hen,", "tokens": ["kein", "Laut", "er\u00b7t\u00f6nt", "aus", "den", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "nur am sumpfigen Teich,", "tokens": ["nur", "am", "sump\u00b7fi\u00b7gen", "Teich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "im matten Gestr\u00e4uch,", "tokens": ["im", "mat\u00b7ten", "Ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "tanzt ein Chor von kr\u00e4chzenden Kr\u00e4hen.", "tokens": ["tanzt", "ein", "Chor", "von", "kr\u00e4ch\u00b7zen\u00b7den", "Kr\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "...", "tokens": ["..."], "token_info": ["punct"], "pos": ["$("]}}}}}