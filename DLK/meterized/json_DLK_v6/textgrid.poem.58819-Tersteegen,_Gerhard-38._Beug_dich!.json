{"textgrid.poem.58819": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "38. Beug dich!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du mu\u00dft das Kreuz und Sterben lieben,", "tokens": ["Du", "mu\u00dft", "das", "Kreuz", "und", "Ster\u00b7ben", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Gott schickt andre, die dich \u00fcben.", "tokens": ["Und", "Gott", "schickt", "and\u00b7re", ",", "die", "dich", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PIS", "$,", "PRELS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Christ sich unter alles beugt,", "tokens": ["Ein", "Christ", "sich", "un\u00b7ter", "al\u00b7les", "beugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der S\u00fcnde nur er nimmer weicht.", "tokens": ["Der", "S\u00fcn\u00b7de", "nur", "er", "nim\u00b7mer", "weicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}