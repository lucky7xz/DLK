{"textgrid.poem.33539": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Vornehmer Morgen", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ewger Sonntag sieht die Stra\u00dfe aus.", "tokens": ["Wie", "ew\u00b7ger", "Sonn\u00b7tag", "sieht", "die", "Stra\u00b7\u00dfe", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Leichthin lehnt Gartenhaus an Gartenhaus.", "tokens": ["Leich\u00b7thin", "lehnt", "Gar\u00b7ten\u00b7haus", "an", "Gar\u00b7ten\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Chauffeure radeln herrschaftlich vorbei.", "tokens": ["Chauf\u00b7feu\u00b7re", "ra\u00b7deln", "herr\u00b7schaft\u00b7lich", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Drei feine B\u00fcrger gleiten still entlang.", "tokens": ["Drei", "fei\u00b7ne", "B\u00fcr\u00b7ger", "glei\u00b7ten", "still", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "K\u00fchl fliegt aus einem Fenster ein Gesang.", "tokens": ["K\u00fchl", "fliegt", "aus", "ei\u00b7nem", "Fens\u00b7ter", "ein", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von weither kommt im Wind ein Kinderschrei.", "tokens": ["Von", "weit\u00b7her", "kommt", "im", "Wind", "ein", "Kin\u00b7der\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und vor der Villa eines Herzogs steht,", "tokens": ["Und", "vor", "der", "Vil\u00b7la", "ei\u00b7nes", "Her\u00b7zogs", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie eine stramme Puppe eingen\u00e4ht,", "tokens": ["Wie", "ei\u00b7ne", "stram\u00b7me", "Pup\u00b7pe", "ein\u00b7ge\u00b7n\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In buntes Tuch, rotleuchtend wie der Mohn,", "tokens": ["In", "bun\u00b7tes", "Tuch", ",", "rot\u00b7leuch\u00b7tend", "wie", "der", "Mohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das funkelnde Gewehr in der gepflegten Hand,", "tokens": ["Das", "fun\u00b7keln\u00b7de", "Ge\u00b7wehr", "in", "der", "ge\u00b7pfleg\u00b7ten", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der k\u00f6niglich bayrische Gerichtspraktikant,", "tokens": ["Der", "k\u00f6\u00b7nig\u00b7lich", "bay\u00b7ri\u00b7sche", "Ge\u00b7richts\u00b7prak\u00b7ti\u00b7kant", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Herr Doktor juris Kuno Kohn.", "tokens": ["Herr", "Dok\u00b7tor", "ju\u00b7ris", "Ku\u00b7no", "Kohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NE", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}