{"dta.poem.480": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Da sie weinte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Augen lasset doch den Thr\u00e4nen-Bach verseigen/", "tokens": ["Ihr", "Au\u00b7gen", "las\u00b7set", "doch", "den", "Thr\u00e4\u00b7nen\u00b7Bach", "ver\u00b7sei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und werfft den Perlen-Stoff nicht auf den Boden hin/", "tokens": ["Und", "werfft", "den", "Per\u00b7len\u00b7Stoff", "nicht", "auf", "den", "Bo\u00b7den", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denckt nur/ die Erde wird das Unrecht nicht verschweigen.", "tokens": ["Denckt", "nur", "/", "die", "Er\u00b7de", "wird", "das", "Un\u00b7recht", "nicht", "ver\u00b7schwei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "ART", "NN", "VAFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Wenn ein so kostbahr Schatz verdirbt aus Eigen-Sinn.", "tokens": ["Wenn", "ein", "so", "kost\u00b7bahr", "Schatz", "ver\u00b7dirbt", "aus", "Ei\u00b7gen\u00b7Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADJD", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}