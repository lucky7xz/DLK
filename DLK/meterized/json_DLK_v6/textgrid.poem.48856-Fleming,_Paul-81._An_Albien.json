{"textgrid.poem.48856": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "81. An Albien", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe nun erkant die hochgeherzten ", "tokens": ["Ich", "ha\u00b7be", "nun", "er\u00b7kant", "die", "hoch\u00b7ge\u00b7herz\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ihr Wesen aufgemerkt, ihr weites Land durchschaut,", "tokens": ["ihr", "We\u00b7sen", "auf\u00b7ge\u00b7merkt", ",", "ihr", "wei\u00b7tes", "Land", "durch\u00b7schaut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die strengen ", "tokens": ["die", "stren\u00b7gen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "mit Mangel und Gefahr mich vielmal m\u00fcssen schmei\u00dfen,", "tokens": ["mit", "Man\u00b7gel", "und", "Ge\u00b7fahr", "mich", "viel\u00b7mal", "m\u00fcs\u00b7sen", "schmei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "bis ich auch das gesehn, was pr\u00e4chtig wird gehei\u00dfen", "tokens": ["bis", "ich", "auch", "das", "ge\u00b7sehn", ",", "was", "pr\u00e4ch\u00b7tig", "wird", "ge\u00b7hei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PDS", "VVPP", "$,", "PRELS", "ADJD", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "im edlen ", "tokens": ["im", "ed\u00b7len"], "token_info": ["word", "word"], "pos": ["APPRART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "dem leichten Gl\u00fccke nicht ins Ferner' anvertraut,", "tokens": ["dem", "leich\u00b7ten", "Gl\u00fc\u00b7cke", "nicht", "ins", "Fer\u00b7ner'", "an\u00b7ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und ziehe wieder heim in mein gew\u00fcndschtes ", "tokens": ["und", "zie\u00b7he", "wie\u00b7der", "heim", "in", "mein", "ge\u00b7w\u00fcnd\u00b7schtes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Da hoff' ich, ", "tokens": ["Da", "hoff'", "ich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "und dieser Zeiten Trost, den ersten Ku\u00df zu geben,", "tokens": ["und", "die\u00b7ser", "Zei\u00b7ten", "Trost", ",", "den", "ers\u00b7ten", "Ku\u00df", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "NN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das ich ins f\u00fcnfte Jahr nun habe nicht getan.", "tokens": ["das", "ich", "ins", "f\u00fcnf\u00b7te", "Jahr", "nun", "ha\u00b7be", "nicht", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "APPRART", "ADJA", "NN", "ADV", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Di\u00df soll das Endmal sein von allen meinen M\u00fchen,", "tokens": ["Di\u00df", "soll", "das", "End\u00b7mal", "sein", "von", "al\u00b7len", "mei\u00b7nen", "M\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "PPOSAT", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "von aller meiner Angst. Wenn ich um dich sein kan,", "tokens": ["von", "al\u00b7ler", "mei\u00b7ner", "Angst", ".", "Wenn", "ich", "um", "dich", "sein", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$.", "KOUS", "PPER", "APPR", "PPER", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so hab' ich mehr getan, als die die Welt umziehen.", "tokens": ["so", "hab'", "ich", "mehr", "ge\u00b7tan", ",", "als", "die", "die", "Welt", "um\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,", "KOUS", "ART", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}