{"textgrid.poem.43670": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bienenleben", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lustig ist das Bienenleben!", "tokens": ["Lus\u00b7tig", "ist", "das", "Bie\u00b7nen\u00b7le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lustig in dem Sonnenschein", "tokens": ["Lus\u00b7tig", "in", "dem", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um die duft'gen B\u00e4ume schweben,", "tokens": ["Um", "die", "duft'\u00b7gen", "B\u00e4u\u00b7me", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kosten edlen Bl\u00fcthenwein!", "tokens": ["Kos\u00b7ten", "ed\u00b7len", "Bl\u00fc\u00b7then\u00b7wein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alles horchet, wenn sie summen", "tokens": ["Al\u00b7les", "hor\u00b7chet", ",", "wenn", "sie", "sum\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Sommerwelt hinein,", "tokens": ["In", "die", "Som\u00b7mer\u00b7welt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja die L\u00fcfte selbst verstummen,", "tokens": ["Ja", "die", "L\u00fcf\u00b7te", "selbst", "ver\u00b7stum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauschen ihren Melodei'n.", "tokens": ["Lau\u00b7schen", "ih\u00b7ren", "Me\u00b7lo\u00b7de\u00b7i'", "n."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["APPR", "PPOSAT", "NN", "NE"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Bei der ersten Morgenhelle", "tokens": ["Bei", "der", "ers\u00b7ten", "Mor\u00b7gen\u00b7hel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind sie munter und bereit,", "tokens": ["Sind", "sie", "mun\u00b7ter", "und", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie verlassen ihre Zelle,", "tokens": ["Sie", "ver\u00b7las\u00b7sen", "ih\u00b7re", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und kein Weg ist je zu weit.", "tokens": ["Und", "kein", "Weg", "ist", "je", "zu", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Darum will der Sommer ihnen", "tokens": ["Da\u00b7rum", "will", "der", "Som\u00b7mer", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "ART", "NN", "PPER"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Lohnen auch ihr hei\u00dfes M\u00fch'n,", "tokens": ["Loh\u00b7nen", "auch", "ihr", "hei\u00b7\u00dfes", "M\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4sset f\u00fcr die lieben Bienen", "tokens": ["L\u00e4s\u00b7set", "f\u00fcr", "die", "lie\u00b7ben", "Bie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine bunten Blumen bl\u00fch'n.", "tokens": ["Sei\u00b7ne", "bun\u00b7ten", "Blu\u00b7men", "bl\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}