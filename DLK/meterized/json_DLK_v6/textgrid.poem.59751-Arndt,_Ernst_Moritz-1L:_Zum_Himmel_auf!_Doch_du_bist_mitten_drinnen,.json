{"textgrid.poem.59751": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zum Himmel auf! Doch du bist mitten drinnen,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Himmel auf! Doch du bist mitten drinnen,", "tokens": ["Zum", "Him\u00b7mel", "auf", "!", "Doch", "du", "bist", "mit\u00b7ten", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$.", "KON", "PPER", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein B\u00e4llchen Erde rollt mit Gottes Sonnen.", "tokens": ["Dein", "B\u00e4ll\u00b7chen", "Er\u00b7de", "rollt", "mit", "Got\u00b7tes", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach! w\u00e4rest du so mitten in den Wonnen", "tokens": ["Ach", "!", "w\u00e4\u00b7rest", "du", "so", "mit\u00b7ten", "in", "den", "Won\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VAFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des h\u00f6chsten Lichts mit allen deinen Sinnen!", "tokens": ["Des", "h\u00f6chs\u00b7ten", "Lichts", "mit", "al\u00b7len", "dei\u00b7nen", "Sin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn wieviel Nichts, was wir hienieden spinnen!", "tokens": ["Denn", "wie\u00b7viel", "Nichts", ",", "was", "wir", "hien\u00b7ie\u00b7den", "spin\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "$,", "PRELS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was wir als Sch\u00f6nstes w\u00e4hnen, wann gesponnen!", "tokens": ["Was", "wir", "als", "Sch\u00f6ns\u00b7tes", "w\u00e4h\u00b7nen", ",", "wann", "ge\u00b7spon\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "KOUS", "NN", "VVINF", "$,", "PWAV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir schauen auf \u2013 zerstoben und zerronnen", "tokens": ["Wir", "schau\u00b7en", "auf", "\u2013", "zer\u00b7sto\u00b7ben", "und", "zer\u00b7ron\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "$(", "VVPP", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fliegt, fliegt's mit deinem Wahn wie Spreu dir hinnen.", "tokens": ["Fliegt", ",", "fliegt's", "mit", "dei\u00b7nem", "Wahn", "wie", "Spreu", "dir", "hin\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "KOKOM", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Drum auf zum Himmel! la\u00df zur Erde sinken", "tokens": ["Drum", "auf", "zum", "Him\u00b7mel", "!", "la\u00df", "zur", "Er\u00b7de", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "APPRART", "NN", "$.", "VVIMP", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "All deinen Stolz, woran noch Erde hanget,", "tokens": ["All", "dei\u00b7nen", "Stolz", ",", "wo\u00b7ran", "noch", "Er\u00b7de", "han\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "PWAV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "All deinen bunten blanken Maulwurfsh\u00fcgel.", "tokens": ["All", "dei\u00b7nen", "bun\u00b7ten", "blan\u00b7ken", "Maul\u00b7wurfs\u00b7h\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn sollen Gottes Sterne in dir blinken,", "tokens": ["Denn", "sol\u00b7len", "Got\u00b7tes", "Ster\u00b7ne", "in", "dir", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So rufe: Weg! hinab! was irdisch pranget!", "tokens": ["So", "ru\u00b7fe", ":", "Weg", "!", "hin\u00b7ab", "!", "was", "ir\u00b7disch", "pran\u00b7get", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "NN", "$.", "PTKVZ", "$.", "PWS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wachsen nur zum Himmelsflug die Fl\u00fcgel.", "tokens": ["So", "wach\u00b7sen", "nur", "zum", "Him\u00b7mels\u00b7flug", "die", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}