{"textgrid.poem.43163": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "Goldschnittlyrik", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00fcbsch gelassen und h\u00fcbsch zahm", "tokens": ["H\u00fcbsch", "ge\u00b7las\u00b7sen", "und", "h\u00fcbsch", "zahm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "KON", "ADJD", "VVFIN"], "meter": "+-+-+++", "measure": "unknown.measure.penta"}, "line.2": {"text": "Und der Sitte h\u00fcbsch gehuldigt,", "tokens": ["Und", "der", "Sit\u00b7te", "h\u00fcbsch", "ge\u00b7hul\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die um jedes wahre Wort", "tokens": ["Die", "um", "je\u00b7des", "wah\u00b7re", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich zehntausendmal entschuldigt!", "tokens": ["Sich", "zehn\u00b7tau\u00b7send\u00b7mal", "ent\u00b7schul\u00b7digt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist der Pegasus auch lahm,", "tokens": ["Ist", "der", "Pe\u00b7ga\u00b7sus", "auch", "lahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NE", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und geh\u00f6rnt, anstatt gefl\u00fcgelt,", "tokens": ["Und", "ge\u00b7h\u00f6rnt", ",", "an\u00b7statt", "ge\u00b7fl\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "KOUI", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Trabt er h\u00fcbsch solid doch fort,", "tokens": ["Trabt", "er", "h\u00fcbsch", "so\u00b7lid", "doch", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VMFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Galoppirt nie \u2013 ungez\u00fcgelt!", "tokens": ["Ga\u00b7lop\u00b7pirt", "nie", "\u2013", "un\u00b7ge\u00b7z\u00fc\u00b7gelt", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "$(", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}