{"dta.poem.17551": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "172.  Alle Tugeden sind eine Tugend.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Schau alle Tugenden ist ein\u2019 ohn unterscheid:", "tokens": ["Schau", "al\u00b7le", "Tu\u00b7gen\u00b7den", "ist", "ein'", "ohn", "un\u00b7ter\u00b7scheid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VAFIN", "ART", "APPR", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wiltu den Rahmen h\u00f6rn? sie heist Gerechtigkeit.", "tokens": ["Wil\u00b7tu", "den", "Rah\u00b7men", "h\u00f6rn", "?", "sie", "heist", "Ge\u00b7rech\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$.", "PPER", "VAFIN", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}