{"textgrid.poem.32249": {"metadata": {"author": {"name": "Fischart, Johann", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1568, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dann jedes rechtes Regiment", "tokens": ["Dann", "je\u00b7des", "rech\u00b7tes", "Re\u00b7gi\u00b7ment"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll gleichsam gstimmt sein wie die Seyten,", "tokens": ["Soll", "gleich\u00b7sam", "gs\u00b7timmt", "sein", "wie", "die", "Sey\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVPP", "VAINF", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die sich all in einander leyten.", "tokens": ["Die", "sich", "all", "in", "ein\u00b7an\u00b7der", "ley\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PIAT", "APPR", "PRF", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wann aber auff dem Instrument", "tokens": ["Wann", "a\u00b7ber", "auff", "dem", "Inst\u00b7ru\u00b7ment"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die gr\u00f6bst Seyt sich von andern trennt,", "tokens": ["Die", "gr\u00f6bst", "Seyt", "sich", "von", "an\u00b7dern", "trennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PRF", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd wolt nicht mit jhn stimmen ein,", "tokens": ["Vnd", "wolt", "nicht", "mit", "jhn", "stim\u00b7men", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "APPR", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sonder derselben exlex sein,", "tokens": ["Son\u00b7der", "der\u00b7sel\u00b7ben", "ex\u00b7lex", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Da ist die Music schon gesch\u00e4ndt.", "tokens": ["Da", "ist", "die", "Mu\u00b7sic", "schon", "ge\u00b7sch\u00e4ndt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Also wann auch in K\u00f6nigreichen", "tokens": ["Al\u00b7so", "wann", "auch", "in", "K\u00f6\u00b7nig\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Das weisser soll dem albern weichen,", "tokens": ["Das", "weis\u00b7ser", "soll", "dem", "al\u00b7bern", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "ART", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Vnd das nicht herschen soll, wil gebieten,", "tokens": ["Vnd", "das", "nicht", "her\u00b7schen", "soll", ",", "wil", "ge\u00b7bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "PTKNEG", "VVINF", "VMFIN", "$,", "VMFIN", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.12": {"text": "Da nemmen solche Regiment", "tokens": ["Da", "nem\u00b7men", "sol\u00b7che", "Re\u00b7gi\u00b7ment"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Oder ein enderung oder end,", "tokens": ["O\u00b7der", "ein", "en\u00b7de\u00b7rung", "o\u00b7der", "end", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "VVPP", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.14": {"text": "Dann vneins Hirten nicht wol h\u00fcten.", "tokens": ["Dann", "vneins", "Hir\u00b7ten", "nicht", "wol", "h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}}}}