{"textgrid.poem.56062": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Das Rosen-Innere", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ist zu diesem Innen", "tokens": ["Wo", "ist", "zu", "die\u00b7sem", "In\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "ein Au\u00dfen? Auf welches Weh", "tokens": ["ein", "Au\u00b7\u00dfen", "?", "Auf", "wel\u00b7ches", "Weh"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "APPR", "PWAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "legt man solches Linnen ?", "tokens": ["legt", "man", "sol\u00b7ches", "Lin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Welche Himmel spiegeln sich drinnen", "tokens": ["Wel\u00b7che", "Him\u00b7mel", "spie\u00b7geln", "sich", "drin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "VVFIN", "PRF", "ADV"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "in dem Binnensee", "tokens": ["in", "dem", "Bin\u00b7nen\u00b7see"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "dieser offenen Rosen,", "tokens": ["die\u00b7ser", "of\u00b7fe\u00b7nen", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "dieser sorglosen, sieh:", "tokens": ["die\u00b7ser", "sorg\u00b7lo\u00b7sen", ",", "sieh", ":"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PDAT", "NN", "$,", "VVIMP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "wie sie lose im Losen", "tokens": ["wie", "sie", "lo\u00b7se", "im", "Lo\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "liegen, als k\u00f6nnte nie", "tokens": ["lie\u00b7gen", ",", "als", "k\u00f6nn\u00b7te", "nie"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "VMFIN", "ADV"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.10": {"text": "eine zitternde Hand sie versch\u00fctten.", "tokens": ["ei\u00b7ne", "zit\u00b7tern\u00b7de", "Hand", "sie", "ver\u00b7sch\u00fct\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Sie k\u00f6nnen sich selber kaum", "tokens": ["Sie", "k\u00f6n\u00b7nen", "sich", "sel\u00b7ber", "kaum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "halten; viele lie\u00dfen", "tokens": ["hal\u00b7ten", ";", "vie\u00b7le", "lie\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVINF", "$.", "PIS", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "sich \u00fcberf\u00fcllen und flie\u00dfen", "tokens": ["sich", "\u00fc\u00b7berf\u00b7\u00fcl\u00b7len", "und", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "VVINF", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "\u00fcber von Innenraum", "tokens": ["\u00fc\u00b7ber", "von", "In\u00b7nen\u00b7raum"], "token_info": ["word", "word", "word"], "pos": ["APPR", "APPR", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.15": {"text": "in die Tage, die immer", "tokens": ["in", "die", "Ta\u00b7ge", ",", "die", "im\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.16": {"text": "voller und voller sich schlie\u00dfen,", "tokens": ["vol\u00b7ler", "und", "vol\u00b7ler", "sich", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "KON", "ADJA", "PRF", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.17": {"text": "bis der ganze Sommer ein Zimmer", "tokens": ["bis", "der", "gan\u00b7ze", "Som\u00b7mer", "ein", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "wird, ein Zimmer in einem Traum.", "tokens": ["wird", ",", "ein", "Zim\u00b7mer", "in", "ei\u00b7nem", "Traum", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}