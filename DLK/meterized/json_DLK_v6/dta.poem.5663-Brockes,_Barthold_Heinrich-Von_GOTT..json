{"dta.poem.5663": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo in dem sch\u00f6nen Zweck, den ich mir auserlesen,", "tokens": ["Wo", "in", "dem", "sch\u00f6\u00b7nen", "Zweck", ",", "den", "ich", "mir", "au\u00b7ser\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wodurch ich recht entflammt bin und entbrannt,", "tokens": ["Wo\u00b7durch", "ich", "recht", "ent\u00b7flammt", "bin", "und", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVPP", "VAFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da Flei\u00df und Einsamkeit mein\u2019 einz\u2019ge Lust gewesen;", "tokens": ["Da", "Flei\u00df", "und", "Ein\u00b7sam\u00b7keit", "mein'", "einz'\u00b7ge", "Lust", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPOSAT", "ADJA", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo, sag\u2019 ich, fern von eitler Triebe Tand,", "tokens": ["Wo", ",", "sag'", "ich", ",", "fern", "von", "eit\u00b7ler", "Trie\u00b7be", "Tand", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVFIN", "PPER", "$,", "ADJD", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Sorg, ", "tokens": ["Die", "Sorg", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Die Seele mir hat eingenommen;", "tokens": ["Die", "See\u00b7le", "mir", "hat", "ein\u00b7ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So sey dem Vorsatz doch geneigt,", "tokens": ["So", "sey", "dem", "Vor\u00b7satz", "doch", "ge\u00b7neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wodurch so wol Verstand als Sinnen", "tokens": ["Wo\u00b7durch", "so", "wol", "Ver\u00b7stand", "als", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "NN", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ein freyes Licht gesuchet zu gewinnen!", "tokens": ["Ein", "frey\u00b7es", "Licht", "ge\u00b7su\u00b7chet", "zu", "ge\u00b7win\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Mach einen neuen Weg, wodurch man aufwerts steigt,", "tokens": ["Mach", "ei\u00b7nen", "neu\u00b7en", "Weg", ",", "wo\u00b7durch", "man", "auf\u00b7werts", "steigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PWAV", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mir doch anitzt bekannt! Sey g\u00fcnstig meinem Flehn,", "tokens": ["Mir", "doch", "a\u00b7nitzt", "be\u00b7kannt", "!", "Sey", "g\u00fcns\u00b7tig", "mei\u00b7nem", "Flehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "$.", "VAIMP", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und mache, da\u00df durch Wissenschafft und Klarheit", "tokens": ["Und", "ma\u00b7che", ",", "da\u00df", "durch", "Wis\u00b7sen\u00b7schafft", "und", "Klar\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Der Creatur, ich k\u00f6nne gehn", "tokens": ["Der", "Crea\u00b7tur", ",", "ich", "k\u00f6n\u00b7ne", "gehn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Zu der vollkommnen ", "tokens": ["Zu", "der", "voll\u00b7komm\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Die uns die Schrifft giebt zu verstehn!", "tokens": ["Die", "uns", "die", "Schrifft", "giebt", "zu", "ver\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}