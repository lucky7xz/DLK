{"dta.poem.9758": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines dachs-hundes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kein loch war mir zu tieff und keines auch zu klein/", "tokens": ["Kein", "loch", "war", "mir", "zu", "tieff", "und", "kei\u00b7nes", "auch", "zu", "klein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "KON", "PIS", "ADV", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich suchte fuchs und dachs/ dem j\u00e4ger zu gefallen/", "tokens": ["Ich", "such\u00b7te", "fuchs", "und", "dachs", "/", "dem", "j\u00e4\u00b7ger", "zu", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NE", "$(", "ART", "ADJA", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich b\u00fc\u00dfte letzlich doch im loch mein leben ein/", "tokens": ["Ich", "b\u00fc\u00df\u00b7te", "letz\u00b7lich", "doch", "im", "loch", "mein", "le\u00b7ben", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPRART", "VVFIN", "PPOSAT", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drumb trau den l\u00f6chern nicht und w\u00fchle nicht in allen.", "tokens": ["Drumb", "trau", "den", "l\u00f6\u00b7chern", "nicht", "und", "w\u00fch\u00b7le", "nicht", "in", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "w