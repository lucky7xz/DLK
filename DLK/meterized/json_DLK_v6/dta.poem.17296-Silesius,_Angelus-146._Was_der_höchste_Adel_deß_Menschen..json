{"dta.poem.17296": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "146.  Was der h\u00f6chste Adel de\u00df Menschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mein h\u00f6chster Adel ist/ da\u00df ich noch auff der Erden/", "tokens": ["Mein", "h\u00f6chs\u00b7ter", "A\u00b7del", "ist", "/", "da\u00df", "ich", "noch", "auff", "der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$(", "KOUS", "PPER", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein K\u00f6nig/ Kaiser/ Gott/ und was ich wil/ kan werden.", "tokens": ["Ein", "K\u00f6\u00b7nig", "/", "Kai\u00b7ser", "/", "Gott", "/", "und", "was", "ich", "wil", "/", "kan", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "$(", "NN", "$(", "KON", "PWS", "PPER", "VMFIN", "$(", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}