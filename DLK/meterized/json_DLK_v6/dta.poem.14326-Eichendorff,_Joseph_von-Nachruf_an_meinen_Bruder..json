{"dta.poem.14326": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachruf an meinen Bruder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Gebunden hier tr\u00e4umen", "tokens": ["Ge\u00b7bun\u00b7den", "hier", "tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Auf Feldern und B\u00e4umen,", "tokens": ["Auf", "Fel\u00b7dern", "und", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und ziehen mich nieder", "tokens": ["Und", "zie\u00b7hen", "mich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So m\u00fcde vor Weh", "tokens": ["So", "m\u00fc\u00b7de", "vor", "Weh"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Zu tiefstillem See \u2014", "tokens": ["Zu", "tief\u00b7stil\u00b7lem", "See"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "--+-+", "measure": "anapaest.init"}, "line.6": {"text": "O weckt nicht die Lieder!", "tokens": ["O", "weckt", "nicht", "die", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}