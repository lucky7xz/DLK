{"textgrid.poem.43973": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gedenck an mich und meine Liebe,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gedenck an mich und meine Liebe,", "tokens": ["Ge\u00b7denck", "an", "mich", "und", "mei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du mit Gewalt entri\u00dfnes Kind,", "tokens": ["Du", "mit", "Ge\u00b7walt", "ent\u00b7ri\u00df\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und glaube, da\u00df die reinen Triebe", "tokens": ["Und", "glau\u00b7be", ",", "da\u00df", "die", "rei\u00b7nen", "Trie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir jezt und allzeit dienstbahr sind", "tokens": ["Dir", "jezt", "und", "all\u00b7zeit", "dienst\u00b7bahr", "sind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "KON", "ADV", "PDS", "VAFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und da\u00df ich ewig auf der Erde", "tokens": ["Und", "da\u00df", "ich", "e\u00b7wig", "auf", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sonst nichts als dich verehren werde.", "tokens": ["Sonst", "nichts", "als", "dich", "ver\u00b7eh\u00b7ren", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "KOKOM", "PPER", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gedenck an mich in allem Leiden", "tokens": ["Ge\u00b7denck", "an", "mich", "in", "al\u00b7lem", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PRF", "APPR", "PIS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und tr\u00f6ste dich mit meiner Treu!", "tokens": ["Und", "tr\u00f6s\u00b7te", "dich", "mit", "mei\u00b7ner", "Treu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Luft mag jezt empfindlich schneiden,", "tokens": ["Die", "Luft", "mag", "jezt", "emp\u00b7find\u00b7lich", "schnei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wetter gehn doch all vorbey,", "tokens": ["Die", "Wet\u00b7ter", "gehn", "doch", "all", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PIAT", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nach dem ungeheuren Knallen", "tokens": ["Und", "nach", "dem", "un\u00b7ge\u00b7heu\u00b7ren", "Knal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wird auch ein fruchtbahr Regen fallen.", "tokens": ["Wird", "auch", "ein", "frucht\u00b7bahr", "Re\u00b7gen", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gedenck an mich in deinem Gl\u00fcck,", "tokens": ["Ge\u00b7denck", "an", "mich", "in", "dei\u00b7nem", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wenn es dir nach Wuntsche geht,", "tokens": ["Und", "wenn", "es", "dir", "nach", "Wunt\u00b7sche", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So seze nie den Freund zur\u00fccke,", "tokens": ["So", "se\u00b7ze", "nie", "den", "Freund", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der blos um dich in Sorgen steht!", "tokens": ["Der", "blos", "um", "dich", "in", "Sor\u00b7gen", "steht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch mir kan bey dem besten Leben", "tokens": ["Auch", "mir", "kan", "bey", "dem", "bes\u00b7ten", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nichts mehr als du Entz\u00fcckung geben.", "tokens": ["Nichts", "mehr", "als", "du", "Ent\u00b7z\u00fc\u00b7ckung", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "KOUS", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Gedenck an mich in deinem Sterben;", "tokens": ["Ge\u00b7denck", "an", "mich", "in", "dei\u00b7nem", "Ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Himmel halte dies noch auf,", "tokens": ["Der", "Him\u00b7mel", "hal\u00b7te", "dies", "noch", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDS", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch sollen wir uns nicht erwerben", "tokens": ["Doch", "sol\u00b7len", "wir", "uns", "nicht", "er\u00b7wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und z\u00fcrnt der Sterne b\u00f6ser Lauf,", "tokens": ["Und", "z\u00fcrnt", "der", "Ster\u00b7ne", "b\u00f6\u00b7ser", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So soll mir auch das Sterbek\u00fc\u00dfen", "tokens": ["So", "soll", "mir", "auch", "das", "Ster\u00b7be\u00b7k\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Hinfahrt durch dein Bild vers\u00fc\u00dfen.", "tokens": ["Die", "Hin\u00b7fahrt", "durch", "dein", "Bild", "ver\u00b7s\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gedenck an mich und meine Thr\u00e4nen,", "tokens": ["Ge\u00b7denck", "an", "mich", "und", "mei\u00b7ne", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die dir so oft das Herz ger\u00fchrt", "tokens": ["Die", "dir", "so", "oft", "das", "Herz", "ge\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und die dich durch mein kr\u00e4ftig Sehnen", "tokens": ["Und", "die", "dich", "durch", "mein", "kr\u00e4f\u00b7tig", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PRF", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum ersten auf die Bahn gef\u00fchrt,", "tokens": ["Zum", "ers\u00b7ten", "auf", "die", "Bahn", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo Ku\u00df und Liebe treuer Herzen", "tokens": ["Wo", "Ku\u00df", "und", "Lie\u00b7be", "treu\u00b7er", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Lebens Ungemach verschmerzen.", "tokens": ["Des", "Le\u00b7bens", "Un\u00b7ge\u00b7mach", "ver\u00b7schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gedenck auch endlich an die Stunde,", "tokens": ["Ge\u00b7denck", "auch", "end\u00b7lich", "an", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die mir das Herz vor Wehmuth brach,", "tokens": ["Die", "mir", "das", "Herz", "vor", "Weh\u00b7muth", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ich wie du mit schwachem Munde", "tokens": ["Als", "ich", "wie", "du", "mit", "schwa\u00b7chem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die lezten Abschiedsworte sprach;", "tokens": ["Die", "lez\u00b7ten", "Ab\u00b7schieds\u00b7wor\u00b7te", "sprach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gedenck an mich und meine Plagen!", "tokens": ["Ge\u00b7denck", "an", "mich", "und", "mei\u00b7ne", "Pla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mehr will und kan ich jezt nicht sagen.", "tokens": ["Mehr", "will", "und", "kan", "ich", "jezt", "nicht", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "KON", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}