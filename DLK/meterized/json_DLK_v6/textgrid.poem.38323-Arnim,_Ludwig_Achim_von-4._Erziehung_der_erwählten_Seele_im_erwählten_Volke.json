{"textgrid.poem.38323": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "4. Erziehung der erw\u00e4hlten Seele im erw\u00e4hlten Volke", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fahre fort mit Liebesschl\u00e4gen,", "tokens": ["Fah\u00b7re", "fort", "mit", "Lie\u00b7bes\u00b7schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fc\u00dfer Jesu, liebster Hort;", "tokens": ["S\u00fc\u00b7\u00dfer", "Je\u00b7su", ",", "liebs\u00b7ter", "Hort", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df sich Tr\u00fcbsalsst\u00fcrme regen,", "tokens": ["La\u00df", "sich", "Tr\u00fcb\u00b7sals\u00b7st\u00fcr\u00b7me", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PRF", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn sie treiben mich zum Port.", "tokens": ["Denn", "sie", "trei\u00b7ben", "mich", "zum", "Port", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da mein Herr, hier ist mein R\u00fccken,", "tokens": ["Da", "mein", "Herr", ",", "hier", "ist", "mein", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schlag nur zu, ich habs verschuldt;", "tokens": ["Schlag", "nur", "zu", ",", "ich", "habs", "ver\u00b7schuldt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$,", "PPER", "NE", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "An das Kreutz mit Liebesstricken", "tokens": ["An", "das", "Kreutz", "mit", "Lie\u00b7bes\u00b7stri\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zieht mich deine grosse Huld.", "tokens": ["Zieht", "mich", "dei\u00b7ne", "gros\u00b7se", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich bin lang von dir gewichen,", "tokens": ["Ich", "bin", "lang", "von", "dir", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lang war mir das Eitle lieb;", "tokens": ["Lang", "war", "mir", "das", "Eit\u00b7le", "lieb", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch bist du mir nachgeschlichen,", "tokens": ["Doch", "bist", "du", "mir", "nach\u00b7ge\u00b7schli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Weil dich deine Liebe trieb.", "tokens": ["Weil", "dich", "dei\u00b7ne", "Lie\u00b7be", "trieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe, die dir H\u00e4nd und F\u00fcsse", "tokens": ["Lie\u00b7be", ",", "die", "dir", "H\u00e4nd", "und", "F\u00fcs\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An das Kreutzesholz gespie\u00dft;", "tokens": ["An", "das", "Kreut\u00b7zes\u00b7holz", "ge\u00b7spie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebe, die so honigs\u00fc\u00dfe", "tokens": ["Lie\u00b7be", ",", "die", "so", "ho\u00b7nig\u00b7s\u00fc\u00b7\u00dfe"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf die armen S\u00fcnder flie\u00dft.", "tokens": ["Auf", "die", "ar\u00b7men", "S\u00fcn\u00b7der", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach so denke nicht, wie lange", "tokens": ["Ach", "so", "den\u00b7ke", "nicht", ",", "wie", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "PTKNEG", "$,", "PWAV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich dich Br\u00e4utgam nicht erkannt;", "tokens": ["Ich", "dich", "Br\u00e4ut\u00b7gam", "nicht", "er\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "NE", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ich mich zur alten Schlange", "tokens": ["Wie", "ich", "mich", "zur", "al\u00b7ten", "Schlan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oft mit Herz und Sinn gewandt.", "tokens": ["Oft", "mit", "Herz", "und", "Sinn", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sondern denk an deine Wunden,", "tokens": ["Son\u00b7dern", "denk", "an", "dei\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die dein heilig Fleisch durchritzt;", "tokens": ["Die", "dein", "hei\u00b7lig", "Fleisch", "durc\u00b7hritzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denk an deine Trauerstunden,", "tokens": ["Denk", "an", "dei\u00b7ne", "Trau\u00b7er\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da du Blut f\u00fcr mich geschwitzt.", "tokens": ["Da", "du", "Blut", "f\u00fcr", "mich", "ge\u00b7schwitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}