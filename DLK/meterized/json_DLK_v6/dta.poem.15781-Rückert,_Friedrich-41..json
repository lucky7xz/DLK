{"dta.poem.15781": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ein Botaniker nur von Profession", "tokens": ["Wie", "ein", "Bo\u00b7ta\u00b7ni\u00b7ker", "nur", "von", "Pro\u00b7fes\u00b7si\u00b7on"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "APPR", "NN"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bemerket, was uneingeweihtem Blick entflohn,", "tokens": ["Be\u00b7mer\u00b7ket", ",", "was", "un\u00b7ein\u00b7ge\u00b7weih\u00b7tem", "Blick", "ent\u00b7flohn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Der zarten Moose krausge\u00e4stetes Gewimmel,", "tokens": ["Der", "zar\u00b7ten", "Moo\u00b7se", "kraus\u00b7ge\u00b7\u00e4s\u00b7te\u00b7tes", "Ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von andern \u00fcbersehn als unscheinbarer Schimmel;", "tokens": ["Von", "an\u00b7dern", "\u00fc\u00b7ber\u00b7sehn", "als", "un\u00b7schein\u00b7ba\u00b7rer", "Schim\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVINF", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch wer mit rechtem Blick und Kunstsinn es gewahrt,", "tokens": ["Doch", "wer", "mit", "rech\u00b7tem", "Blick", "und", "Kunst\u00b7sinn", "es", "ge\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ADJA", "NN", "KON", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem ist des Sch\u00f6pfers Kunst auch darin offenbart,", "tokens": ["Dem", "ist", "des", "Sch\u00f6p\u00b7fers", "Kunst", "auch", "da\u00b7rin", "of\u00b7fen\u00b7bart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "ADV", "PAV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nicht minder als im glanzentfalteten Geb\u00e4ude", "tokens": ["Nicht", "min\u00b7der", "als", "im", "glan\u00b7zent\u00b7fal\u00b7te\u00b7ten", "Ge\u00b7b\u00e4u\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "KOKOM", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Buntkroniger Blumenpracht, jedermanns Augenfreude:", "tokens": ["Bunt\u00b7kro\u00b7ni\u00b7ger", "Blu\u00b7men\u00b7pracht", ",", "je\u00b7der\u00b7manns", "Au\u00b7gen\u00b7freu\u00b7de", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}}, "stanza.5": {"line.1": {"text": "So im von Meisterhand entworfenen Gedicht", "tokens": ["So", "im", "von", "Meis\u00b7ter\u00b7hand", "ent\u00b7wor\u00b7fe\u00b7nen", "Ge\u00b7dicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind Reize, die so leicht nicht fallen ins Gesicht,", "tokens": ["Sind", "Rei\u00b7ze", ",", "die", "so", "leicht", "nicht", "fal\u00b7len", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PRELS", "ADV", "ADJD", "PTKNEG", "VVINF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "In denen doch sich zeigt des Meisters Kunst und Macht", "tokens": ["In", "de\u00b7nen", "doch", "sich", "zeigt", "des", "Meis\u00b7ters", "Kunst", "und", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ADV", "PRF", "VVFIN", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht minder als im Schmuck erfindungsreichster Pracht;", "tokens": ["Nicht", "min\u00b7der", "als", "im", "Schmuck", "er\u00b7fin\u00b7dungs\u00b7reichs\u00b7ter", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Doch nur der Kenner und Liebhaber von Kleinheiten", "tokens": ["Doch", "nur", "der", "Ken\u00b7ner", "und", "Lieb\u00b7ha\u00b7ber", "von", "Klein\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "KON", "NN", "APPR", "NN"], "meter": "-+-+--+---++-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ergetzt sich an derlei verborgenen Feinheiten.", "tokens": ["Er\u00b7getzt", "sich", "an", "der\u00b7lei", "ver\u00b7bor\u00b7ge\u00b7nen", "Fein\u00b7hei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}