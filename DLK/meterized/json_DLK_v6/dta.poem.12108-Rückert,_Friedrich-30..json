{"dta.poem.12108": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dir w\u00fcnsch' ich, Wanderer des Weges und des Lebens,", "tokens": ["Dir", "w\u00fcn\u00b7sch'", "ich", ",", "Wan\u00b7de\u00b7rer", "des", "We\u00b7ges", "und", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Befriedigung der Lust, und Lust des Weiterstrebens,", "tokens": ["Be\u00b7frie\u00b7di\u00b7gung", "der", "Lust", ",", "und", "Lust", "des", "Wei\u00b7ter\u00b7stre\u00b7bens", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den Himmel blau und rein, die L\u00fcfte gleichgew\u00e4gt,", "tokens": ["Den", "Him\u00b7mel", "blau", "und", "rein", ",", "die", "L\u00fcf\u00b7te", "gleich\u00b7ge\u00b7w\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und soviel Sonnenschein als nur dein Aug' ertr\u00e4gt,", "tokens": ["Und", "so\u00b7viel", "Son\u00b7nen\u00b7schein", "als", "nur", "dein", "Aug'", "er\u00b7tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KOKOM", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und soviel Regen nur, da\u00df \u00fcber Nacht erlischt", "tokens": ["Und", "so\u00b7viel", "Re\u00b7gen", "nur", ",", "da\u00df", "\u00fc\u00b7ber", "Nacht", "er\u00b7lischt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ADV", "$,", "KOUS", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Staub, und Wald und Flur dir l\u00e4chelt neu erfrischt.", "tokens": ["Der", "Staub", ",", "und", "Wald", "und", "Flur", "dir", "l\u00e4\u00b7chelt", "neu", "er\u00b7frischt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "NN", "KON", "NN", "PPER", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}