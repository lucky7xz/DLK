{"textgrid.poem.50084": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "40. Auf Mutius Scevola", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ", "tokens": ["Als"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "So wie das ", "tokens": ["So", "wie", "das"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOKOM", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Und nach vollbrachter That erst zur Erk\u00e4ntn\u00fcss kam,", "tokens": ["Und", "nach", "voll\u00b7brach\u00b7ter", "That", "erst", "zur", "Er\u00b7k\u00e4nt\u00b7n\u00fcss", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da wust' er der ", "tokens": ["Da", "wust'", "er", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART"], "meter": "-+--", "measure": "dactylic.init"}, "line.6": {"text": "Und durch die ", "tokens": ["Und", "durch", "die"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Er machte dass der ", "tokens": ["Er", "mach\u00b7te", "dass", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Und weil die ", "tokens": ["Und", "weil", "die"], "token_info": ["word", "word", "word"], "pos": ["KON", "KOUS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}}}}