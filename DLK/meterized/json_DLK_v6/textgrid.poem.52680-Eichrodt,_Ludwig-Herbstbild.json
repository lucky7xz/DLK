{"textgrid.poem.52680": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Herbstbild", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Feuer leuchten durch die laue Nacht,", "tokens": ["Die", "Feu\u00b7er", "leuch\u00b7ten", "durch", "die", "lau\u00b7e", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zum Himmel spr\u00fcht und steigt die Funkenpracht.", "tokens": ["Zum", "Him\u00b7mel", "spr\u00fcht", "und", "steigt", "die", "Fun\u00b7ken\u00b7pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Jubeldonner kracht von Berg zu Berg,", "tokens": ["Der", "Ju\u00b7bel\u00b7don\u00b7ner", "kracht", "von", "Berg", "zu", "Berg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gesang und Tanz, Musik und Feuerwerk!", "tokens": ["Ge\u00b7sang", "und", "Tanz", ",", "Mu\u00b7sik", "und", "Feu\u00b7er\u00b7werk", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dort unten aber bei den schwarzen H\u00fctten,", "tokens": ["Dort", "un\u00b7ten", "a\u00b7ber", "bei", "den", "schwar\u00b7zen", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Kelter dr\u00f6hnt in stummer Winzer Mitten.", "tokens": ["Die", "Kel\u00b7ter", "dr\u00f6hnt", "in", "stum\u00b7mer", "Win\u00b7zer", "Mit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und von der schweren, hei\u00dfen Arbeit m\u00fcd,", "tokens": ["Und", "von", "der", "schwe\u00b7ren", ",", "hei\u00b7\u00dfen", "Ar\u00b7beit", "m\u00fcd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein M\u00e4dchen steht, sie seufzet auf und gl\u00fcht.", "tokens": ["Ein", "M\u00e4d\u00b7chen", "steht", ",", "sie", "seuf\u00b7zet", "auf", "und", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein guter Wein! Ihn bauten meine Eltern \u2013", "tokens": ["Ein", "gu\u00b7ter", "Wein", "!", "Ihn", "bau\u00b7ten", "mei\u00b7ne", "El\u00b7tern", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Weh, da str\u00f6mt er aus fremden Keltern!", "tokens": ["O", "Weh", ",", "da", "str\u00f6mt", "er", "aus", "frem\u00b7den", "Kel\u00b7tern", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Ein edler Wein! ha la\u00dft Raketen steigen!", "tokens": ["Ein", "ed\u00b7ler", "Wein", "!", "ha", "la\u00dft", "Ra\u00b7ke\u00b7ten", "stei\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NE", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor keiner wird sich seine Blume neigen.", "tokens": ["Vor", "kei\u00b7ner", "wird", "sich", "sei\u00b7ne", "Blu\u00b7me", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Bei allen Festen wird er reichlich flie\u00dfen,", "tokens": ["Bei", "al\u00b7len", "Fes\u00b7ten", "wird", "er", "reich\u00b7lich", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In alte Glieder neubelebend schie\u00dfen.", "tokens": ["In", "al\u00b7te", "Glie\u00b7der", "neu\u00b7be\u00b7le\u00b7bend", "schie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Bei Hochzeitmahlen wird er feurig kreisen,", "tokens": ["Bei", "Hoch\u00b7zeit\u00b7mah\u00b7len", "wird", "er", "feu\u00b7rig", "krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und aller Orten werden sie ihn preisen!", "tokens": ["Und", "al\u00b7ler", "Or\u00b7ten", "wer\u00b7den", "sie", "ihn", "prei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ich aber bin ein armes, krankes Kind,", "tokens": ["Ich", "a\u00b7ber", "bin", "ein", "ar\u00b7mes", ",", "kran\u00b7kes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich werde weinen, wenn sie fr\u00f6hlich sind.", "tokens": ["Ich", "wer\u00b7de", "wei\u00b7nen", ",", "wenn", "sie", "fr\u00f6h\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Ich aber werde niemals Hochzeit haben,", "tokens": ["Ich", "a\u00b7ber", "wer\u00b7de", "nie\u00b7mals", "Hoch\u00b7zeit", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und unser Wein, euch wird er niemals laben.", "tokens": ["Und", "un\u00b7ser", "Wein", ",", "euch", "wird", "er", "nie\u00b7mals", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "So klagt das Kind und in der Mondnacht drau\u00dfen", "tokens": ["So", "klagt", "das", "Kind", "und", "in", "der", "Mond\u00b7nacht", "drau\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Feuer steigen und die Feste brausen.", "tokens": ["Die", "Feu\u00b7er", "stei\u00b7gen", "und", "die", "Fes\u00b7te", "brau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "O stille Herz! Wird es auch rosig tagen?", "tokens": ["O", "stil\u00b7le", "Herz", "!", "Wird", "es", "auch", "ro\u00b7sig", "ta\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "VAFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wann wird kein M\u00e4gdelein mehr also klagen?", "tokens": ["Wann", "wird", "kein", "M\u00e4g\u00b7del\u00b7ein", "mehr", "al\u00b7so", "kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIAT", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}