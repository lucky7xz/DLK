{"dta.poem.105": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  Dulcinden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Dieweil mein Auge nicht die Strahlen an-kan schauen/", "tokens": ["Die\u00b7weil", "mein", "Au\u00b7ge", "nicht", "die", "Strah\u00b7len", "an\u00b7kan", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "NE", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die deiner Augen-Sonn mit starcken Flammen scheu\u00dft/", "tokens": ["Die", "dei\u00b7ner", "Au\u00b7gen\u00b7Sonn", "mit", "star\u00b7cken", "Flam\u00b7men", "scheu\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So mu\u00df ich meine Quaal der Feder anvertrauen/", "tokens": ["So", "mu\u00df", "ich", "mei\u00b7ne", "Qua\u00b7al", "der", "Fe\u00b7der", "an\u00b7ver\u00b7trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df sie in wei\u00df und schwartz die schuldge Ehr-furcht wei\u00dft.", "tokens": ["Da\u00df", "sie", "in", "wei\u00df", "und", "schwartz", "die", "schuld\u00b7ge", "Ehr\u00b7furcht", "wei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "VVFIN", "KON", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was aber soll Papier der Gluht sich widersetzen?", "tokens": ["Was", "a\u00b7ber", "soll", "Pa\u00b7pier", "der", "Gluht", "sich", "wi\u00b7der\u00b7set\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIAT", "NN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vor der gew\u00f6lbt Crystall nicht einst bestehen kan/", "tokens": ["Vor", "der", "ge\u00b7w\u00f6lbt", "Crys\u00b7tall", "nicht", "einst", "be\u00b7ste\u00b7hen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$("], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.7": {"text": "Wird nicht dein strenger Blitz es mit Gewalt verletzen", "tokens": ["Wird", "nicht", "dein", "stren\u00b7ger", "Blitz", "es", "mit", "Ge\u00b7walt", "ver\u00b7let\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Eh deine sch\u00f6ne Hand das Siegel auffgethan?", "tokens": ["Eh", "dei\u00b7ne", "sch\u00f6\u00b7ne", "Hand", "das", "Sie\u00b7gel", "auff\u00b7ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wann aber sch\u00f6nstes Kind das blitzen sich geleget/", "tokens": ["Wann", "a\u00b7ber", "sch\u00f6ns\u00b7tes", "Kind", "das", "blit\u00b7zen", "sich", "ge\u00b7le\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "NN", "PDS", "VVFIN", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So glaub/ da\u00df Ehr und Furcht die keusche Brunst erh\u00e4lt/", "tokens": ["So", "glaub", "/", "da\u00df", "Ehr", "und", "Furcht", "die", "keu\u00b7sche", "Brunst", "er\u00b7h\u00e4lt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df keine Geilheit wird in meiner Brust geheget/", "tokens": ["Da\u00df", "kei\u00b7ne", "Geil\u00b7heit", "wird", "in", "mei\u00b7ner", "Brust", "ge\u00b7he\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und da\u00df kein Fall-Brett ist dem ", "tokens": ["Und", "da\u00df", "kein", "Fall\u00b7Brett", "ist", "dem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIAT", "NN", "VAFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Das eh den Tod begehrt/ eh es sein Fell betr\u00fcbet", "tokens": ["Das", "eh", "den", "Tod", "be\u00b7gehrt", "/", "eh", "es", "sein", "Fell", "be\u00b7tr\u00fc\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "KOUS", "ART", "NN", "VVPP", "$(", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "In den verschantzten Koth\u2019 so rein nun dessen Haut/", "tokens": ["In", "den", "ver\u00b7schantz\u00b7ten", "Ko\u00b7th'", "so", "rein", "nun", "des\u00b7sen", "Haut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "ADJD", "ADV", "PDAT", "NN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "So keusch ist mein Gem\u00fcht/ dem Redlichkeit beliebet/", "tokens": ["So", "keusch", "ist", "mein", "Ge\u00b7m\u00fcht", "/", "dem", "Red\u00b7lich\u00b7keit", "be\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nicht grosse Schmeichelung von Zucker auffgebaut/", "tokens": ["Nicht", "gros\u00b7se", "Schmei\u00b7che\u00b7lung", "von", "Zu\u00b7cker", "auff\u00b7ge\u00b7baut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Worunter Kr\u00f6ten-Gifft der falschen Geilheit stecket/", "tokens": ["Wo\u00b7run\u00b7ter", "Kr\u00f6\u00b7ten\u00b7Gifft", "der", "fal\u00b7schen", "Geil\u00b7heit", "ste\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So bittre Wermuth zeugt/ und Bastlisken nehrt/", "tokens": ["So", "bitt\u00b7re", "Wer\u00b7muth", "zeugt", "/", "und", "Bast\u00b7lis\u00b7ken", "nehrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$(", "KON", "NN", "VVFIN", "$("], "meter": "-+--++-++-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Mit s\u00fcssen Uberzug der glatten Wort\u2019 bedecket/", "tokens": ["Mit", "s\u00fcs\u00b7sen", "Ub\u00b7er\u00b7zug", "der", "glat\u00b7ten", "Wort'", "be\u00b7de\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das den/ so leichtlich traut/ aufs \u00e4userste verzehrt.", "tokens": ["Das", "den", "/", "so", "leicht\u00b7lich", "traut", "/", "aufs", "\u00e4u\u00b7sers\u00b7te", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "$(", "ADV", "ADJD", "VVFIN", "$(", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Hier aber scheinet nichts als nur der Warheit-Schimmer/", "tokens": ["Hier", "a\u00b7ber", "schei\u00b7net", "nichts", "als", "nur", "der", "Wa\u00b7rheit\u00b7Schim\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "KOKOM", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der keine L\u00fcgen liebt/ dem Falschheit unbewust/", "tokens": ["Der", "kei\u00b7ne", "L\u00fc\u00b7gen", "liebt", "/", "dem", "Falschheit", "un\u00b7be\u00b7wust", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$(", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.23": {"text": "Der in Gedancken nie betr\u00fcbt ein Frauen-Zimmer/", "tokens": ["Der", "in", "Ge\u00b7dan\u00b7cken", "nie", "be\u00b7tr\u00fcbt", "ein", "Frau\u00b7en\u00b7Zim\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "In dessen Seele nicht die ungerechte Lust.", "tokens": ["In", "des\u00b7sen", "See\u00b7le", "nicht", "die", "un\u00b7ge\u00b7rech\u00b7te", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Je ihren Sitz gehabt noch Wohnung auffgeschlagen/", "tokens": ["Je", "ih\u00b7ren", "Sitz", "ge\u00b7habt", "noch", "Woh\u00b7nung", "auff\u00b7ge\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAPP", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Hier zeigt ein reiner Brief die l\u00e4ngst verborgne Glut/", "tokens": ["Hier", "zeigt", "ein", "rei\u00b7ner", "Brief", "die", "l\u00e4ngst", "ver\u00b7borg\u00b7ne", "Glut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So dein Gesicht erregt: Ach! k\u00f6nnt\u2019 ich sie vertragen/", "tokens": ["So", "dein", "Ge\u00b7sicht", "er\u00b7regt", ":", "Ach", "!", "k\u00f6nnt'", "ich", "sie", "ver\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$.", "ITJ", "$.", "VMFIN", "PPER", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Es solte dir mein Brief noch nicht mein letztes Guht", "tokens": ["Es", "sol\u00b7te", "dir", "mein", "Brief", "noch", "nicht", "mein", "letz\u00b7tes", "Guht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "PTKNEG", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Zu deinen F\u00fcssen streun. Jtzt aber da mein Leben", "tokens": ["Zu", "dei\u00b7nen", "F\u00fcs\u00b7sen", "streun", ".", "Jtzt", "a\u00b7ber", "da", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$.", "ADV", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Fast mit dem Tode ringt/ so will ich dir mein Hertz", "tokens": ["Fast", "mit", "dem", "To\u00b7de", "ringt", "/", "so", "will", "ich", "dir", "mein", "Hertz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$(", "ADV", "VMFIN", "PPER", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und deiner Sch\u00f6nheit-Zier zum Opffer \u00fcbergeben.", "tokens": ["Und", "dei\u00b7ner", "Sch\u00f6n\u00b7heit\u00b7Zier", "zum", "Opf\u00b7fer", "\u00fc\u00b7ber\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Will nun ", "tokens": ["Will", "nun"], "token_info": ["word", "word"], "pos": ["VMFIN", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.33": {"text": "So lasse deine Hand mir wenig Zeilen lesen/", "tokens": ["So", "las\u00b7se", "dei\u00b7ne", "Hand", "mir", "we\u00b7nig", "Zei\u00b7len", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Doch mache/ da\u00df dein Knecht nicht Brief und Leben bricht/", "tokens": ["Doch", "ma\u00b7che", "/", "da\u00df", "dein", "Knecht", "nicht", "Brief", "und", "Le\u00b7ben", "bricht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "PTKNEG", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ach straffe nicht zu hart/ wenn ich zu frech gewesen/", "tokens": ["Ach", "straf\u00b7fe", "nicht", "zu", "hart", "/", "wenn", "ich", "zu", "frech", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PTKNEG", "PTKA", "ADJD", "$(", "KOUS", "PPER", "PTKA", "ADJD", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Gedenck wer murrisch schweigt/ der ha\u00dft und liebet nicht.", "tokens": ["Ge\u00b7denck", "wer", "mur\u00b7risch", "schweigt", "/", "der", "ha\u00dft", "und", "lie\u00b7bet", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "ADJD", "VVFIN", "$(", "ART", "VVFIN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}