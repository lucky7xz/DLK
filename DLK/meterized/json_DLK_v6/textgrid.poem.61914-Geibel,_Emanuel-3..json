{"textgrid.poem.61914": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und die Stirne des Kaisers ward finster wie Nacht,", "tokens": ["Und", "die", "Stir\u00b7ne", "des", "Kai\u00b7sers", "ward", "fins\u00b7ter", "wie", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VAFIN", "ADJD", "KOKOM", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Und hinter sich stie\u00df er den Sessel mit Macht,", "tokens": ["Und", "hin\u00b7ter", "sich", "stie\u00df", "er", "den", "Ses\u00b7sel", "mit", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Hinwarf er den Mantel, den roten,", "tokens": ["Hin\u00b7warf", "er", "den", "Man\u00b7tel", ",", "den", "ro\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und er schlug an den Schild lautdr\u00f6hnenden Schalls,", "tokens": ["Und", "er", "schlug", "an", "den", "Schild", "laut\u00b7dr\u00f6h\u00b7nen\u00b7den", "Schalls", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und es stoben, die Z\u00fcgel verh\u00e4ngt, aus der Pfalz", "tokens": ["Und", "es", "sto\u00b7ben", ",", "die", "Z\u00fc\u00b7gel", "ver\u00b7h\u00e4ngt", ",", "aus", "der", "Pfalz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "$,", "ART", "NN", "VVFIN", "$,", "APPR", "ART", "NE"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Nach allen vier Winden die Boten.", "tokens": ["Nach", "al\u00b7len", "vier", "Win\u00b7den", "die", "Bo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "CARD", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Und die Gauen hindurch, wo die Donau schwillt,", "tokens": ["Und", "die", "Gau\u00b7en", "hin\u00b7durch", ",", "wo", "die", "Do\u00b7nau", "schwillt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PAV", "$,", "PWAV", "ART", "NE", "VVFIN", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Wo die Elbe sich w\u00e4lzt durch das Weizengefild',", "tokens": ["Wo", "die", "El\u00b7be", "sich", "w\u00e4lzt", "durch", "das", "Wei\u00b7zen\u00b7ge\u00b7fild'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Wo den strudelnden Rhein sie befahren,", "tokens": ["Wo", "den", "stru\u00b7deln\u00b7den", "Rhein", "sie", "be\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NE", "PPER", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Aufflammten die Feuer von Berg und von Turm,", "tokens": ["Auf\u00b7flamm\u00b7ten", "die", "Feu\u00b7er", "von", "Berg", "und", "von", "Turm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Und die Glocken erklangen und l\u00e4uteten Sturm,", "tokens": ["Und", "die", "Glo\u00b7cken", "er\u00b7klan\u00b7gen", "und", "l\u00e4u\u00b7te\u00b7ten", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "KON", "ADJA", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Und zum Heerbann str\u00f6mten die Scharen.", "tokens": ["Und", "zum", "Heer\u00b7bann", "str\u00f6m\u00b7ten", "die", "Scha\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}}}}