{"textgrid.poem.61776": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "In der Fr\u00fche", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frisch von k\u00fchlem Tau durchquollen", "tokens": ["Frisch", "von", "k\u00fch\u00b7lem", "Tau", "durch\u00b7quol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schauern Wald und Erlenbruch;", "tokens": ["Schau\u00b7ern", "Wald", "und", "Er\u00b7len\u00b7bruch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus des Ackers schwarzen Schollen", "tokens": ["Aus", "des", "A\u00b7ckers", "schwar\u00b7zen", "Schol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dampft ein kr\u00e4ft'ger Erdgeruch.", "tokens": ["Dampft", "ein", "kr\u00e4ft'\u00b7ger", "Erd\u00b7ge\u00b7ruch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Still noch ist's auf allen Wegen,", "tokens": ["Still", "noch", "ist's", "auf", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur vom Dorf die Glocke ruft", "tokens": ["Nur", "vom", "Dorf", "die", "Glo\u00b7cke", "ruft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fernher ihren Morgensegen", "tokens": ["Fern\u00b7her", "ih\u00b7ren", "Mor\u00b7gen\u00b7se\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die sonnendunst'ge Luft.", "tokens": ["Durch", "die", "son\u00b7nen\u00b7dunst'\u00b7ge", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Von dem Strom, wo ich gebadet,", "tokens": ["Von", "dem", "Strom", ",", "wo", "ich", "ge\u00b7ba\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh' der letzte Stern entfloh,", "tokens": ["Eh'", "der", "letz\u00b7te", "Stern", "ent\u00b7floh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit verj\u00fcngter Kraft begnadet", "tokens": ["Mit", "ver\u00b7j\u00fcng\u00b7ter", "Kraft", "be\u00b7gna\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kehr' ich heim, des Tages froh.", "tokens": ["Kehr'", "ich", "heim", ",", "des", "Ta\u00b7ges", "froh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ahnungsvoll im Busen klingt mir", "tokens": ["Ah\u00b7nungs\u00b7voll", "im", "Bu\u00b7sen", "klingt", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dunkler Melodien Gew\u00fchl,", "tokens": ["Dunk\u00b7ler", "Me\u00b7lo\u00b7dien", "Ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den leichten Schritt beschwingt mir", "tokens": ["Und", "den", "leich\u00b7ten", "Schritt", "be\u00b7schwingt", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein begl\u00fcckend Vorgef\u00fchl.", "tokens": ["Ein", "be\u00b7gl\u00fc\u00b7ckend", "Vor\u00b7ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Was bedeutet dies Empfinden?", "tokens": ["Was", "be\u00b7deu\u00b7tet", "dies", "Emp\u00b7fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll ich die Geliebte sehn?", "tokens": ["Soll", "ich", "die", "Ge\u00b7lieb\u00b7te", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder flutet in den Winden,", "tokens": ["O\u00b7der", "flu\u00b7tet", "in", "den", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Muse, deines Odems Wehn?", "tokens": ["Mu\u00b7se", ",", "dei\u00b7nes", "O\u00b7dems", "Wehn", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}