{"dta.poem.6838": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man spreche, da\u00df darin ein Stoff der himmlisch, rinne,", "tokens": ["Man", "spre\u00b7che", ",", "da\u00df", "da\u00b7rin", "ein", "Stoff", "der", "himm\u00b7lisch", ",", "rin\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PAV", "ART", "NN", "ART", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der unbegreifflich rein und d\u00fcnne,", "tokens": ["Der", "un\u00b7be\u00b7greif\u00b7flich", "rein", "und", "d\u00fcn\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df der Geister Meng\u2019 in ihm auch stets geschwinde", "tokens": ["Und", "da\u00df", "der", "Geis\u00b7ter", "Meng'", "in", "ihm", "auch", "stets", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "NE", "APPR", "PPER", "ADV", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Manch\u2019 ungez\u00e4hlte Th\u00fcr und Oeffnung finde;", "tokens": ["Man\u00b7ch'", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Th\u00fcr", "und", "Oeff\u00b7nung", "fin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Sind es doch C\u00f6rper nur, die leicht und schnell bewegt,", "tokens": ["Sind", "es", "doch", "C\u00f6r\u00b7per", "nur", ",", "die", "leicht", "und", "schnell", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NE", "ADV", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und deren Handlung doch, die blo\u00df nur c\u00f6rperlich,", "tokens": ["Und", "de\u00b7ren", "Hand\u00b7lung", "doch", ",", "die", "blo\u00df", "nur", "c\u00f6r\u00b7per\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "ADV", "$,", "PRELS", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nichts als ein schnell Bewegen hegt.", "tokens": ["Nichts", "als", "ein", "schnell", "Be\u00b7we\u00b7gen", "hegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So werden wir demnach das F\u00fchlen und das Wissen", "tokens": ["So", "wer\u00b7den", "wir", "dem\u00b7nach", "das", "F\u00fch\u00b7len", "und", "das", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Am andern Orte suchen m\u00fcssen.", "tokens": ["Am", "an\u00b7dern", "Or\u00b7te", "su\u00b7chen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}