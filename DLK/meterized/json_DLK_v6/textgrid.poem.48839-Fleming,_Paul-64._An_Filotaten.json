{"textgrid.poem.48839": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "64. An Filotaten", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr viel verweisen mir, ich lieb' ihr gar zu viel.", "tokens": ["Ihr", "viel", "ver\u00b7wei\u00b7sen", "mir", ",", "ich", "lieb'", "ihr", "gar", "zu", "viel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich selbst auch pflegs an mir oft in geheim zu schelten.", "tokens": ["Ich", "selbst", "auch", "pflegs", "an", "mir", "oft", "in", "ge\u00b7heim", "zu", "schel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "ADV", "APPR", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was aber kan ich tun? Wer sch\u00fctzt sich vor Gew\u00e4lten,", "tokens": ["Was", "a\u00b7ber", "kan", "ich", "tun", "?", "Wer", "sch\u00fctzt", "sich", "vor", "Ge\u00b7w\u00e4l\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PPER", "VVINF", "$.", "PWS", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die st\u00e4rker sind, als er? Ich bin ein einzigs Ziel,", "tokens": ["die", "st\u00e4r\u00b7ker", "sind", ",", "als", "er", "?", "Ich", "bin", "ein", "ein\u00b7zigs", "Ziel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "an dem ein Jederman zum Ritter werden will.", "tokens": ["an", "dem", "ein", "Je\u00b7der\u00b7man", "zum", "Rit\u00b7ter", "wer\u00b7den", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "PIS", "APPRART", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soll ich der Leute Gunst mit Liebe nicht vergelten?", "tokens": ["Soll", "ich", "der", "Leu\u00b7te", "Gunst", "mit", "Lie\u00b7be", "nicht", "ver\u00b7gel\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer oft gehasset wird, der liebet selbst gar selten,", "tokens": ["Wer", "oft", "ge\u00b7has\u00b7set", "wird", ",", "der", "lie\u00b7bet", "selbst", "gar", "sel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "VAFIN", "$,", "PRELS", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wer Lust zu Karten hat, den liebet auch das Spiel.", "tokens": ["wer", "Lust", "zu", "Kar\u00b7ten", "hat", ",", "den", "lie\u00b7bet", "auch", "das", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "NN", "VAFIN", "$,", "ART", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ist jemand Freundschaft gram, der hasse mich um Lieben,", "tokens": ["Ist", "je\u00b7mand", "Freund\u00b7schaft", "gram", ",", "der", "has\u00b7se", "mich", "um", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "NN", "NE", "$,", "PRELS", "VVFIN", "PRF", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ich lieb' ihr noch so viel, als hier stehn angeschrieben.", "tokens": ["ich", "lieb'", "ihr", "noch", "so", "viel", ",", "als", "hier", "stehn", "an\u00b7ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,", "KOUS", "ADV", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Lob w\u00e4chst mit der Zahl. Dir aber, sch\u00f6nster Brand,", "tokens": ["Mein", "Lob", "w\u00e4chst", "mit", "der", "Zahl", ".", "Dir", "a\u00b7ber", ",", "sch\u00f6ns\u00b7ter", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$.", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "der tausent Sachen Glut, sei di\u00df f\u00fcr andern allen", "tokens": ["der", "tau\u00b7sent", "Sa\u00b7chen", "Glut", ",", "sei", "di\u00df", "f\u00fcr", "an\u00b7dern", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "VAFIN", "PDS", "APPR", "PIS", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "geheim gesagt: Du bist ", "tokens": ["ge\u00b7heim", "ge\u00b7sagt", ":", "Du", "bist"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVPP", "$.", "PPER", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und hei\u00dfest, was du bist. Mehr darf mir nicht entfallen.", "tokens": ["und", "hei\u00b7\u00dfest", ",", "was", "du", "bist", ".", "Mehr", "darf", "mir", "nicht", "ent\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$.", "PIS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}