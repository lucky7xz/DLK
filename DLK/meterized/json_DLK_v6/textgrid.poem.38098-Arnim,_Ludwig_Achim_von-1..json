{"textgrid.poem.38098": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Urlaub Frau um euren werthen Dienstmann", "tokens": ["Mit", "Ur\u00b7laub", "Frau", "um", "eu\u00b7ren", "wert\u00b7hen", "Dienst\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geheissen war der Bremberger", "tokens": ["Ge\u00b7heis\u00b7sen", "war", "der", "Brem\u00b7ber\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein edler Ritter weise,", "tokens": ["Ein", "ed\u00b7ler", "Rit\u00b7ter", "wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In seinem Ton ich euch wohl singen kann,", "tokens": ["In", "sei\u00b7nem", "Ton", "ich", "euch", "wohl", "sin\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Darin mir niemand verdenke,", "tokens": ["Da\u00b7rin", "mir", "nie\u00b7mand", "ver\u00b7den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sein Lob ich immer preise", "tokens": ["Sein", "Lob", "ich", "im\u00b7mer", "prei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Er hat gesungen mannigfalt,", "tokens": ["Er", "hat", "ge\u00b7sun\u00b7gen", "man\u00b7nig\u00b7falt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das red ich auf die Treue mein", "tokens": ["Das", "red", "ich", "auf", "die", "Treu\u00b7e", "mein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Von einer sch\u00f6nen Frauen.", "tokens": ["Von", "ei\u00b7ner", "sch\u00f6\u00b7nen", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "An ihm geschah grosse Gewalt,", "tokens": ["An", "ihm", "ge\u00b7schah", "gros\u00b7se", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+--++--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Da\u00df er verlor das Leben sein,", "tokens": ["Da\u00df", "er", "ver\u00b7lor", "das", "Le\u00b7ben", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sein Leib der ward ihm zerhauen.", "tokens": ["Sein", "Leib", "der", "ward", "ihm", "zer\u00b7hau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Der Herr der sprach: \u00bbDu hast mir lieb die Fraue mein", "tokens": ["Der", "Herr", "der", "sprach", ":", "\u00bb", "Du", "hast", "mir", "lieb", "die", "Frau\u00b7e", "mein"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VVFIN", "$.", "$(", "PPER", "VAFIN", "PPER", "ADJD", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "O Bremberger es geht dir an das Leben dein!\u00ab", "tokens": ["O", "Brem\u00b7ber\u00b7ger", "es", "geht", "dir", "an", "das", "Le\u00b7ben", "dein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sein Haupt das ward ihm abgeschlagen", "tokens": ["Sein", "Haupt", "das", "ward", "ihm", "ab\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PDS", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Zu derselben Stund,", "tokens": ["Zu", "der\u00b7sel\u00b7ben", "Stund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Das Herz er in dem Leibe trug,", "tokens": ["Das", "Herz", "er", "in", "dem", "Lei\u00b7be", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Das a\u00df der Fraue rother Mund.", "tokens": ["Das", "a\u00df", "der", "Frau\u00b7e", "ro\u00b7ther", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}