{"textgrid.poem.26022": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Der Himmel \u00f6ffnet die blaue T\u00fcr", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel \u00f6ffnet die blaue T\u00fcr,", "tokens": ["Der", "Him\u00b7mel", "\u00f6ff\u00b7net", "die", "blau\u00b7e", "T\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da kommt gar sch\u00f6n gezieret", "tokens": ["Da", "kommt", "gar", "sch\u00f6n", "ge\u00b7zie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Jungfr\u00e4ulein herf\u00fcr.", "tokens": ["Ein", "Jung\u00b7fr\u00e4u\u00b7lein", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr leuchtet warmes z\u00e4rtliches Haar,", "tokens": ["Ihr", "leuch\u00b7tet", "war\u00b7mes", "z\u00e4rt\u00b7li\u00b7ches", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Davon werden die Wege klar,", "tokens": ["Da\u00b7von", "wer\u00b7den", "die", "We\u00b7ge", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und Segen kommt in das Jahr.", "tokens": ["Und", "Se\u00b7gen", "kommt", "in", "das", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich bin ihr nachgegangen,", "tokens": ["Ich", "bin", "ihr", "nach\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie hat mich auch empfangen;", "tokens": ["Sie", "hat", "mich", "auch", "emp\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt leb' ich ohne Speis' und Trank", "tokens": ["Jetzt", "leb'", "ich", "oh\u00b7ne", "Speis'", "und", "Trank"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von der S\u00fc\u00dfe ihrer Wangen.", "tokens": ["Von", "der", "S\u00fc\u00b7\u00dfe", "ih\u00b7rer", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}