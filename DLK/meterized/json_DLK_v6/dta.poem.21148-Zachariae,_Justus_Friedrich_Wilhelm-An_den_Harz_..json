{"dta.poem.21148": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An den Harz .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Gegend, schrecklich und rauh, wo melancholische\nBerge", "tokens": ["O", "Ge\u00b7gend", ",", "schreck\u00b7lich", "und", "rauh", ",", "wo", "me\u00b7lan\u00b7cho\u00b7li\u00b7sche", "Ber\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ADJD", "KON", "ADJD", "$,", "PWAV", "ADJA", "NN"], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.2": {"text": "Mit starrem Haupt die Gewitter durchschaun;", "tokens": ["Mit", "star\u00b7rem", "Haupt", "die", "Ge\u00b7wit\u00b7ter", "durch\u00b7schaun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wo um den drohenden Fels die werdenden Donner sich", "tokens": ["Wo", "um", "den", "dro\u00b7hen\u00b7den", "Fels", "die", "wer\u00b7den\u00b7den", "Don\u00b7ner", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PRF"], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und jede Wolke zum Regengu\u00df wird;", "tokens": ["Und", "je\u00b7de", "Wol\u00b7ke", "zum", "Re\u00b7gen\u00b7gu\u00df", "wird", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wo bald im rauschenden Bach die Kutsche des Reisen-", "tokens": ["Wo", "bald", "im", "rau\u00b7schen\u00b7den", "Bach", "die", "Kut\u00b7sche", "des", "Rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bald durch die engsten Felsen sich zwingt;", "tokens": ["Bald", "durch", "die", "engs\u00b7ten", "Fel\u00b7sen", "sich", "zwingt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Bald auf der Spitze des Bergs die Wolken um sich", "tokens": ["Bald", "auf", "der", "Spit\u00b7ze", "des", "Bergs", "die", "Wol\u00b7ken", "um", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "APPR", "PRF"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und bald in Th\u00e4lern, gleich Abgr\u00fcnden, irrt;", "tokens": ["Und", "bald", "in", "Th\u00e4\u00b7lern", ",", "gleich", "Ab\u00b7gr\u00fcn\u00b7den", ",", "irrt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "$,", "ADV", "NN", "$,", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wo nur der knarrende Karn von flimmernden Erzten", "tokens": ["Wo", "nur", "der", "knar\u00b7ren\u00b7de", "Karn", "von", "flim\u00b7mern\u00b7den", "Erz\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Thal vom rasenden Puchwerke schallt;", "tokens": ["Das", "Thal", "vom", "ra\u00b7sen\u00b7den", "Puch\u00b7wer\u00b7ke", "schallt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wo im ewigen Rauch, gleich einem dampfenden", "tokens": ["Und", "wo", "im", "e\u00b7wi\u00b7gen", "Rauch", ",", "gleich", "ei\u00b7nem", "damp\u00b7fen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "ADJA", "NN", "$,", "ADV", "ART", "ADJA"], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Manch H\u00fcttenwerk weite Geh\u00f6lze verschlingt;", "tokens": ["Manch", "H\u00fct\u00b7ten\u00b7werk", "wei\u00b7te", "Ge\u00b7h\u00f6l\u00b7ze", "ver\u00b7schlingt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Wo nur mit blassem Gesicht bey Hammerwerken und", "tokens": ["Wo", "nur", "mit", "blas\u00b7sem", "Ge\u00b7sicht", "bey", "Ham\u00b7mer\u00b7wer\u00b7ken", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN", "APPR", "NN", "KON"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein Bergmann etwa die Wege durchkreuzt;", "tokens": ["Ein", "Berg\u00b7mann", "et\u00b7wa", "die", "We\u00b7ge", "durch\u00b7kreuzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Verschwindet, wenn man ihn sieht, f\u00e4hrt in die Tiefen", "tokens": ["Ver\u00b7schwin\u00b7det", ",", "wenn", "man", "ihn", "sieht", ",", "f\u00e4hrt", "in", "die", "Tie\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und l\u00e4\u00dft den Wald so \u00f6d, als er war;", "tokens": ["Und", "l\u00e4\u00dft", "den", "Wald", "so", "\u00f6d", ",", "als", "er", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,", "KOUS", "PPER", "VAFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "O Harz, wofern auch in dir der l\u00e4chelnde Morgen sich", "tokens": ["O", "Harz", ",", "wo\u00b7fern", "auch", "in", "dir", "der", "l\u00e4\u00b7cheln\u00b7de", "Mor\u00b7gen", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "KOUS", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "PRF"], "meter": "-+--+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und Abends Purpur die Felsen bekr\u00f6nt;", "tokens": ["Und", "A\u00b7bends", "Pur\u00b7pur", "die", "Fel\u00b7sen", "be\u00b7kr\u00f6nt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So la\u00df auch den heutigen Tag mit aller der Anmuth", "tokens": ["So", "la\u00df", "auch", "den", "heu\u00b7ti\u00b7gen", "Tag", "mit", "al\u00b7ler", "der", "An\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ADV", "ART", "ADJA", "NN", "APPR", "PIAT", "ART", "NN"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die einen Harztag zu schm\u00fccken vermag.", "tokens": ["Die", "ei\u00b7nen", "Harz\u00b7tag", "zu", "schm\u00fc\u00b7cken", "ver\u00b7mag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "O Donner, rolle du nicht von ungeselligen Bergen;", "tokens": ["O", "Don\u00b7ner", ",", "rol\u00b7le", "du", "nicht", "von", "un\u00b7ge\u00b7sel\u00b7li\u00b7gen", "Ber\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und du, o Sturmwind, st\u00fcrme du nicht.", "tokens": ["Und", "du", ",", "o", "Sturm\u00b7wind", ",", "st\u00fcr\u00b7me", "du", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Der Westwind flattre durch euch, ihr tausendj\u00e4hrigen", "tokens": ["Der", "West\u00b7wind", "flatt\u00b7re", "durch", "euch", ",", "ihr", "tau\u00b7send\u00b7j\u00e4h\u00b7ri\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,", "PPOSAT", "NN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Tanne rausche Vergn\u00fcgen und Ruh;", "tokens": ["Die", "Tan\u00b7ne", "rau\u00b7sche", "Ver\u00b7gn\u00fc\u00b7gen", "und", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Da\u00df ihr Serenen nicht schreckt, wenn sie mit \u00e4ngstli-", "tokens": ["Da\u00df", "ihr", "Se\u00b7re\u00b7nen", "nicht", "schreckt", ",", "wenn", "sie", "mit", "\u00e4ngst\u00b7li"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,", "KOUS", "PPER", "APPR", "TRUNC"], "meter": "--++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die unabsehlichen W\u00e4lder erblickt.", "tokens": ["Die", "un\u00b7ab\u00b7seh\u00b7li\u00b7chen", "W\u00e4l\u00b7der", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der t\u00f6dtende H\u00fcttenrauch flieh, von sanften Westen", "tokens": ["Der", "t\u00f6d\u00b7ten\u00b7de", "H\u00fct\u00b7ten\u00b7rauch", "flieh", ",", "von", "sanf\u00b7ten", "Wes\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und fr\u00f6hlich ruf ihr der Bergmann: Gl\u00fcck auf!", "tokens": ["Und", "fr\u00f6h\u00b7lich", "ruf", "ihr", "der", "Berg\u00b7mann", ":", "Gl\u00fcck", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$.", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}