{"textgrid.poem.59700": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Mut des Verderbens", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und h\u00e4tt' ich zehntausend K\u00f6pfe", "tokens": ["Und", "h\u00e4tt'", "ich", "zehn\u00b7tau\u00b7send", "K\u00f6p\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "CARD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und tr\u00fcge keinen zu Haus,", "tokens": ["Und", "tr\u00fc\u00b7ge", "kei\u00b7nen", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die feigen Schurken und Tr\u00f6pfe,", "tokens": ["Die", "fei\u00b7gen", "Schur\u00b7ken", "und", "Tr\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie machen mir's zu kraus.", "tokens": ["Sie", "ma\u00b7chen", "mir's", "zu", "kraus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und tr\u00fcg' ich zehntausend Kronen,", "tokens": ["Und", "tr\u00fcg'", "ich", "zehn\u00b7tau\u00b7send", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "CARD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich w\u00fcrfe sie alle fort,", "tokens": ["Ich", "w\u00fcr\u00b7fe", "sie", "al\u00b7le", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vor allen H\u00f6hen und Thronen", "tokens": ["Vor", "al\u00b7len", "H\u00f6\u00b7hen", "und", "Thro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Steht h\u00f6chst das hohe Wort:", "tokens": ["Steht", "h\u00f6chst", "das", "ho\u00b7he", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das heilige Lutherzeichen,", "tokens": ["Das", "hei\u00b7li\u00b7ge", "Lu\u00b7ther\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das schuf und tr\u00e4gt die Welt,", "tokens": ["Das", "schuf", "und", "tr\u00e4gt", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Listen und Griffen der Bleichen", "tokens": ["Den", "Lis\u00b7ten", "und", "Grif\u00b7fen", "der", "Blei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und Feigen zu hoch gestellt.", "tokens": ["Und", "Fei\u00b7gen", "zu", "hoch", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ja presset mit eurer Presse,", "tokens": ["Ja", "pres\u00b7set", "mit", "eu\u00b7rer", "Pres\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Setzt, presset, drucket allein \u2013", "tokens": ["Setzt", ",", "pres\u00b7set", ",", "dru\u00b7cket", "al\u00b7lein", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ADV", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich kenn' eine feurige Esse,", "tokens": ["Ich", "kenn'", "ei\u00b7ne", "feu\u00b7ri\u00b7ge", "Es\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die schmiedet und gie\u00dft auch fein.", "tokens": ["Die", "schmie\u00b7det", "und", "gie\u00dft", "auch", "fein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Da sitzt der Meister der Meister,", "tokens": ["Da", "sitzt", "der", "Meis\u00b7ter", "der", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da schmiedet er fort und fort,", "tokens": ["Da", "schmie\u00b7det", "er", "fort", "und", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und seine Gesellen, die Geister,", "tokens": ["Und", "sei\u00b7ne", "Ge\u00b7sel\u00b7len", ",", "die", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die blasen und schaffen am Wort.", "tokens": ["Die", "bla\u00b7sen", "und", "schaf\u00b7fen", "am", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Die schmieden und gie\u00dfen die Lettern", "tokens": ["Die", "schmie\u00b7den", "und", "gie\u00b7\u00dfen", "die", "Let\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und streun sie lustig umher", "tokens": ["Und", "streun", "sie", "lus\u00b7tig", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und sammeln zu Donnerwettern", "tokens": ["Und", "sam\u00b7meln", "zu", "Don\u00b7ner\u00b7wet\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ihr leichtgefl\u00fcgeltes Heer.", "tokens": ["Ihr", "leicht\u00b7ge\u00b7fl\u00fc\u00b7gel\u00b7tes", "Heer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Doch weit \u00fcber H\u00f6ren und Sehen", "tokens": ["Doch", "weit", "\u00fc\u00b7ber", "H\u00f6\u00b7ren", "und", "Se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Gl\u00e4nzt Narren die blitzende Schrift;", "tokens": ["Gl\u00e4nzt", "Nar\u00b7ren", "die", "blit\u00b7zen\u00b7de", "Schrift", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Drum donnert, ihr M\u00e4chte der H\u00f6hen,", "tokens": ["Drum", "don\u00b7nert", ",", "ihr", "M\u00e4ch\u00b7te", "der", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und schleudert Blitz, welcher trifft!", "tokens": ["Und", "schleu\u00b7dert", "Blitz", ",", "wel\u00b7cher", "trifft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Und sammelt ihr auch die Lettern", "tokens": ["Und", "sam\u00b7melt", "ihr", "auch", "die", "Let\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Zu einem Vernichtungsspruch,", "tokens": ["Zu", "ei\u00b7nem", "Ver\u00b7nich\u00b7tungs\u00b7spruch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und mu\u00df ich mit in den Wettern,", "tokens": ["Und", "mu\u00df", "ich", "mit", "in", "den", "Wet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich lebte und liebte genug.", "tokens": ["Ich", "leb\u00b7te", "und", "lieb\u00b7te", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}