{"textgrid.poem.42440": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ob meine B\u00fccher dir bekannt,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob meine B\u00fccher dir bekannt,", "tokens": ["Ob", "mei\u00b7ne", "B\u00fc\u00b7cher", "dir", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die einst ich schrieb?", "tokens": ["Die", "einst", "ich", "schrieb", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und wissen m\u00f6cht' ich dann, ob sie", "tokens": ["Und", "wis\u00b7sen", "m\u00f6cht'", "ich", "dann", ",", "ob", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "ADV", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir wert und lieb.", "tokens": ["Dir", "wert", "und", "lieb", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Vielleicht von deines Ahnherrn Nest", "tokens": ["Viel\u00b7leicht", "von", "dei\u00b7nes", "Ahn\u00b7herrn", "Nest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Nordseestrand", "tokens": ["Am", "Nord\u00b7sees\u00b7trand"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bist weit du fern. Ich lebte noch", "tokens": ["Bist", "weit", "du", "fern", ".", "Ich", "leb\u00b7te", "noch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Holstenland.", "tokens": ["Im", "Hols\u00b7ten\u00b7land", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Du siehst in meinen Strophen nichts", "tokens": ["Du", "siehst", "in", "mei\u00b7nen", "Stro\u00b7phen", "nichts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Leid und Lust,", "tokens": ["Als", "Leid", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das gleiche, das auch immer zog", "tokens": ["Das", "glei\u00b7che", ",", "das", "auch", "im\u00b7mer", "zog"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "PRELS", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch deine Brust.", "tokens": ["Durch", "dei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Und dein Geschlecht, Normannenblut:", "tokens": ["Und", "dein", "Ge\u00b7schlecht", ",", "Nor\u00b7man\u00b7nen\u00b7blut", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gott sch\u00fctz' dein Haus", "tokens": ["Gott", "sch\u00fctz'", "dein", "Haus"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und l\u00f6sche seinem Herde nie", "tokens": ["Und", "l\u00f6\u00b7sche", "sei\u00b7nem", "Her\u00b7de", "nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Flammen aus.", "tokens": ["Die", "Flam\u00b7men", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Du Nobelmann mit Speer und Sporn,", "tokens": ["Du", "No\u00b7bel\u00b7mann", "mit", "Speer", "und", "Sporn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was klirrt dein Fu\u00df", "tokens": ["Was", "klirrt", "dein", "Fu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So zornig auf im Waffensaal,", "tokens": ["So", "zor\u00b7nig", "auf", "im", "Waf\u00b7fen\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein b\u00f6ser Gru\u00df.", "tokens": ["Ein", "b\u00f6\u00b7ser", "Gru\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und doch, du gl\u00e4ttest deine Stirn?", "tokens": ["Und", "doch", ",", "du", "gl\u00e4t\u00b7test", "dei\u00b7ne", "Stirn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vergiebst es gar,", "tokens": ["Ver\u00b7giebst", "es", "gar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Da\u00df einer deines Namens einst", "tokens": ["Da\u00df", "ei\u00b7ner", "dei\u00b7nes", "Na\u00b7mens", "einst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Dichter war?", "tokens": ["Ein", "Dich\u00b7ter", "war", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}