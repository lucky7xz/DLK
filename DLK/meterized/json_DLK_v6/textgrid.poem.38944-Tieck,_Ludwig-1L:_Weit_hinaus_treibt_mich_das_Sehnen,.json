{"textgrid.poem.38944": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weit hinaus treibt mich das Sehnen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weit hinaus treibt mich das Sehnen,", "tokens": ["Weit", "hin\u00b7aus", "treibt", "mich", "das", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APZR", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wundervolles Land zu schauen:", "tokens": ["Wun\u00b7der\u00b7vol\u00b7les", "Land", "zu", "schau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keiner darf sich selbst vertrauen,", "tokens": ["Kei\u00b7ner", "darf", "sich", "selbst", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder sich als weise w\u00e4hnen;", "tokens": ["O\u00b7der", "sich", "als", "wei\u00b7se", "w\u00e4h\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "KOKOM", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das erfordert manche K\u00fcnste,", "tokens": ["Das", "er\u00b7for\u00b7dert", "man\u00b7che", "K\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mancherlei mu\u00df man erfahren,", "tokens": ["Man\u00b7cher\u00b7lei", "mu\u00df", "man", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und oft sieht man erst nach Jahren,", "tokens": ["Und", "oft", "sieht", "man", "erst", "nach", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alles waren eitle D\u00fcnste.", "tokens": ["Al\u00b7les", "wa\u00b7ren", "eit\u00b7le", "D\u00fcns\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Darum will ich in die Weite,", "tokens": ["Da\u00b7rum", "will", "ich", "in", "die", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Manches Gl\u00fcck wird mir begegnen,", "tokens": ["Man\u00b7ches", "Gl\u00fcck", "wird", "mir", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Auch mag's manchmal Schl\u00e4ge regnen,", "tokens": ["Auch", "mag's", "manch\u00b7mal", "Schl\u00e4\u00b7ge", "reg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ADV", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Meist folgt Morgen auf das Heute.", "tokens": ["Meist", "folgt", "Mor\u00b7gen", "auf", "das", "Heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Jeder f\u00fchrt etwas im Schilde,", "tokens": ["Je\u00b7der", "f\u00fchrt", "et\u00b7was", "im", "Schil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und umsonst ist nichts auf Erden,", "tokens": ["Und", "um\u00b7sonst", "ist", "nichts", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "APPR", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.15": {"text": "Darum acht' ich nicht Beschwerden,", "tokens": ["Da\u00b7rum", "acht'", "ich", "nicht", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Wenn ich mich nur etwas bilde.", "tokens": ["Wenn", "ich", "mich", "nur", "et\u00b7was", "bil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}