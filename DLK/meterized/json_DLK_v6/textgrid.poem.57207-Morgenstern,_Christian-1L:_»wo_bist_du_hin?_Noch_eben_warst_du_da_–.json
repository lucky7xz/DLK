{"textgrid.poem.57207": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwo bist du hin? Noch eben warst du da \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwo bist du hin? Noch eben warst du da \u2013", "tokens": ["\u00bb", "wo", "bist", "du", "hin", "?", "Noch", "e\u00b7ben", "warst", "du", "da", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "PTKVZ", "$.", "ADV", "ADV", "VAFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was wandtest du dich wieder abw\u00e4rts, wehe,", "tokens": ["Was", "wand\u00b7test", "du", "dich", "wie\u00b7der", "ab\u00b7w\u00e4rts", ",", "we\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "nach jenem Leben, das ich nicht verstehe,", "tokens": ["nach", "je\u00b7nem", "Le\u00b7ben", ",", "das", "ich", "nicht", "ver\u00b7ste\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und warst mir j\u00fcngst doch noch so innig nah.", "tokens": ["und", "warst", "mir", "j\u00fcngst", "doch", "noch", "so", "in\u00b7nig", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich soll hinab mit dir in deine Welt,", "tokens": ["Ich", "soll", "hin\u00b7ab", "mit", "dir", "in", "dei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "aus der die Schauer der Verwesung hauchen,", "tokens": ["aus", "der", "die", "Schau\u00b7er", "der", "Ver\u00b7we\u00b7sung", "hau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ins Reich des Todes soll ich mit dir tauchen,", "tokens": ["ins", "Reich", "des", "To\u00b7des", "soll", "ich", "mit", "dir", "tau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VMFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "das wie ein Leichnam fort und fort zerf\u00e4llt?", "tokens": ["das", "wie", "ein", "Leich\u00b7nam", "fort", "und", "fort", "zer\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOKOM", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wohl gibt es meinesgleichen, eingeweiht", "tokens": ["Wohl", "gibt", "es", "mei\u00b7nes\u00b7glei\u00b7chen", ",", "ein\u00b7ge\u00b7weiht"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "in eure f\u00fcrchterlichen Daseinsstufen ...", "tokens": ["in", "eu\u00b7re", "f\u00fcrch\u00b7ter\u00b7li\u00b7chen", "Da\u00b7seins\u00b7stu\u00b7fen", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch ich bin's nicht. Nur wie verworrnes Rufen", "tokens": ["Doch", "ich", "bin's", "nicht", ".", "Nur", "wie", "ver\u00b7worr\u00b7nes", "Ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "$.", "ADV", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "erschreckt das Wort mich Eurer Zeitlichkeit.", "tokens": ["er\u00b7schreckt", "das", "Wort", "mich", "Eu\u00b7rer", "Zeit\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "La\u00df mich mein Haupt verh\u00fcllen, bis du neu", "tokens": ["La\u00df", "mich", "mein", "Haupt", "ver\u00b7h\u00fcl\u00b7len", ",", "bis", "du", "neu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mir wiederkehrst, so rein, wie ich dich liebe,", "tokens": ["mir", "wie\u00b7der\u00b7kehrst", ",", "so", "rein", ",", "wie", "ich", "dich", "lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADV", "ADJD", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "von nichts erf\u00fcllt als s\u00fc\u00dfem Geistestriebe", "tokens": ["von", "nichts", "er\u00b7f\u00fcllt", "als", "s\u00fc\u00b7\u00dfem", "Geis\u00b7te\u00b7strie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVPP", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und deinem Urbild wieder strahlend treu.\u00ab", "tokens": ["und", "dei\u00b7nem", "Ur\u00b7bild", "wie\u00b7der", "strah\u00b7lend", "treu", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJD", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}