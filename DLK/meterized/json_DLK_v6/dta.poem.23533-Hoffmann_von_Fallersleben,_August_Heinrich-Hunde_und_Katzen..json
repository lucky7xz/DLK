{"dta.poem.23533": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Hunde und Katzen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Hund' und die Katzen die stritten sich", "tokens": ["Die", "Hund'", "und", "die", "Kat\u00b7zen", "die", "strit\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "VVFIN", "PRF"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und zankten sich um die Wette,", "tokens": ["Und", "zank\u00b7ten", "sich", "um", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wer unter ihnen urkundlich", "tokens": ["Wer", "un\u00b7ter", "ih\u00b7nen", "ur\u00b7kund\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den \u00e4ltesten Adel h\u00e4tte.", "tokens": ["Den", "\u00e4l\u00b7tes\u00b7ten", "A\u00b7del", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201ewir haben ein ururaltes Diplom", "tokens": ["\u201e", "wir", "ha\u00b7ben", "ein", "u\u00b7rur\u00b7al\u00b7tes", "Dip\u00b7lom"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Lang her von undenklichen Tagen,", "tokens": ["Lang", "her", "von", "un\u00b7denk\u00b7li\u00b7chen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+--++--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Was Remus mit Romulus einst zu Rom", "tokens": ["Was", "Re\u00b7mus", "mit", "Ro\u00b7mu\u00b7lus", "einst", "zu", "Rom"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NE", "APPR", "NE", "ADV", "APPR", "NE"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Gab allen Isegrims-Magen.\u201c", "tokens": ["Gab", "al\u00b7len", "I\u00b7se\u00b7grims\u00b7Ma\u00b7gen", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u201ezeigt uns, erwiedern die Katzen, wohlan!", "tokens": ["\u201e", "zeigt", "uns", ",", "er\u00b7wie\u00b7dern", "die", "Kat\u00b7zen", ",", "wo\u00b7hlan", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Zeigt her die alten Briefe!", "tokens": ["Zeigt", "her", "die", "al\u00b7ten", "Brie\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was steht denn drin, was hangt denn dran?", "tokens": ["Was", "steht", "denn", "drin", ",", "was", "hangt", "denn", "dran", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "$,", "PWS", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo sind sie, in welchem Archive?\u201c", "tokens": ["Wo", "sind", "sie", ",", "in", "wel\u00b7chem", "Ar\u00b7chi\u00b7ve", "?", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "APPR", "PWAT", "NN", "$.", "$("], "meter": "+---+-+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Man schickte den Pudel eilig nach Rom", "tokens": ["Man", "schick\u00b7te", "den", "Pu\u00b7del", "ei\u00b7lig", "nach", "Rom"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADJD", "APPR", "NE"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zum Aerger der Katzen und Kater,", "tokens": ["Zum", "A\u00b7er\u00b7ger", "der", "Kat\u00b7zen", "und", "Ka\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der sollte holen das alte Diplom", "tokens": ["Der", "soll\u00b7te", "ho\u00b7len", "das", "al\u00b7te", "Dip\u00b7lom"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "VVINF", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Herbei vom heiligen Vater.", "tokens": ["Her\u00b7bei", "vom", "hei\u00b7li\u00b7gen", "Va\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Der Pudel kommt ganz ungenirt", "tokens": ["Der", "Pu\u00b7del", "kommt", "ganz", "un\u00b7ge\u00b7nirt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Papst hereingetreten;", "tokens": ["Zum", "Papst", "her\u00b7ein\u00b7ge\u00b7tre\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er hat den Pantoffel ihm apportiert", "tokens": ["Er", "hat", "den", "Pan\u00b7tof\u00b7fel", "ihm", "ap\u00b7por\u00b7tiert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und dann ihn h\u00f6flich gebeten.", "tokens": ["Und", "dann", "ihn", "h\u00f6f\u00b7lich", "ge\u00b7be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Der Pudel empfing aus des Papstes Hand", "tokens": ["Der", "Pu\u00b7del", "emp\u00b7fing", "aus", "des", "Paps\u00b7tes", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Was das Hundevolk begehrte;", "tokens": ["Was", "das", "Hun\u00b7de\u00b7volk", "be\u00b7gehr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann zog er wiederum in sein Land", "tokens": ["Dann", "zog", "er", "wie\u00b7de\u00b7rum", "in", "sein", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Auf seiner alten F\u00e4hrte.", "tokens": ["Auf", "sei\u00b7ner", "al\u00b7ten", "F\u00e4hr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und als er kam an den Po bei Rom,", "tokens": ["Und", "als", "er", "kam", "an", "den", "Po", "bei", "Rom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da schwamm vor ihm ein Braten,", "tokens": ["Da", "schwamm", "vor", "ihm", "ein", "Bra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er schnappte darnach, und verlor sein Diplom,", "tokens": ["Er", "schnapp\u00b7te", "dar\u00b7nach", ",", "und", "ver\u00b7lor", "sein", "Dip\u00b7lom", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und musst' es auf ewig entrathen.", "tokens": ["Und", "musst'", "es", "auf", "e\u00b7wig", "ent\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ADJD", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "So stand die Sache nun wie zuletzt,", "tokens": ["So", "stand", "die", "Sa\u00b7che", "nun", "wie", "zu\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "KOKOM", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Streit blieb unentschieden,", "tokens": ["Der", "Streit", "blieb", "un\u00b7ent\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Hund' und Katzen halten bis jetzt", "tokens": ["Und", "Hund'", "und", "Kat\u00b7zen", "hal\u00b7ten", "bis", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "KON", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Noch immer keinen Frieden.", "tokens": ["Noch", "im\u00b7mer", "kei\u00b7nen", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Hunde die denken noch immer so:", "tokens": ["Die", "Hun\u00b7de", "die", "den\u00b7ken", "noch", "im\u00b7mer", "so", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wir werden sie schon \u00fcberwinden!", "tokens": ["Wir", "wer\u00b7den", "sie", "schon", "\u00fc\u00b7berw\u00b7in\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie suchen und forschen noch immer am Po \u2014", "tokens": ["Sie", "su\u00b7chen", "und", "for\u00b7schen", "noch", "im\u00b7mer", "am", "Po"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "APPRART", "NE", "$("], "meter": "-+--+-+++-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und k\u00f6nnen den Adel nicht finden.", "tokens": ["Und", "k\u00f6n\u00b7nen", "den", "A\u00b7del", "nicht", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}