{"textgrid.poem.65913": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Wohlhabend", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Athenern brachte Demeter den Frieden:", "tokens": ["A\u00b7the\u00b7nern", "brach\u00b7te", "De\u00b7me\u00b7ter", "den", "Frie\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Sie tritt zum Herde, wo die Ehe gl\u00fcckt,", "tokens": ["Sie", "tritt", "zum", "Her\u00b7de", ",", "wo", "die", "E\u00b7he", "gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Weiheheim bleibt gabenreich geschm\u00fcckt,", "tokens": ["Ihr", "Wei\u00b7he\u00b7heim", "bleibt", "ga\u00b7ben\u00b7reich", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn Wohlstand ward den Auen mitbeschieden.", "tokens": ["Denn", "Wohl\u00b7stand", "ward", "den", "Au\u00b7en", "mit\u00b7be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie weilt auch gerne bei zufriednen Schmieden,", "tokens": ["Sie", "weilt", "auch", "ger\u00b7ne", "bei", "zu\u00b7fried\u00b7nen", "Schmie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat ihren Hang zum Harnisch rasch entr\u00fcckt,", "tokens": ["Hat", "ih\u00b7ren", "Hang", "zum", "Har\u00b7nisch", "rasch", "ent\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr j\u00fcngste Pfl\u00fcge H\u00e4mmernde entz\u00fcckt:", "tokens": ["F\u00fcr", "j\u00fcngs\u00b7te", "Pfl\u00fc\u00b7ge", "H\u00e4m\u00b7mern\u00b7de", "ent\u00b7z\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bescheidne Geister kreisen nun hienieden.", "tokens": ["Be\u00b7scheid\u00b7ne", "Geis\u00b7ter", "krei\u00b7sen", "nun", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie tritt bei T\u00f6pfern in die warmen Stuben.", "tokens": ["Sie", "tritt", "bei", "T\u00f6p\u00b7fern", "in", "die", "war\u00b7men", "Stu\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2013 Verschleiert oft \u2013 am Ernste drum erkannt.", "tokens": ["\u2013", "Ver\u00b7schlei\u00b7ert", "oft", "\u2013", "am", "Erns\u00b7te", "drum", "er\u00b7kannt", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$(", "APPRART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie bringt den Ton aus ungenannten Gruben,", "tokens": ["Sie", "bringt", "den", "Ton", "aus", "un\u00b7ge\u00b7nann\u00b7ten", "Gru\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Erfindet ein Gef\u00e4\u00df mit leichter Hand.", "tokens": ["Er\u00b7fin\u00b7det", "ein", "Ge\u00b7f\u00e4\u00df", "mit", "leich\u00b7ter", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der frohe Mann blickt fromm, verstummt die Buben:", "tokens": ["Der", "fro\u00b7he", "Mann", "blickt", "fromm", ",", "ver\u00b7stummt", "die", "Bu\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die fremde Frau beschattet keine Wand!", "tokens": ["Die", "frem\u00b7de", "Frau", "be\u00b7schat\u00b7tet", "kei\u00b7ne", "Wand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}