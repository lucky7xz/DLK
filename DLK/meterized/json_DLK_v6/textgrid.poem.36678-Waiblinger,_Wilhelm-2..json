{"textgrid.poem.36678": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch gedenk' ich jenes Morgens,", "tokens": ["Noch", "ge\u00b7denk'", "ich", "je\u00b7nes", "Mor\u00b7gens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da wir uns zum erstenmale", "tokens": ["Da", "wir", "uns", "zum", "ers\u00b7ten\u00b7ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So von ohngef\u00e4hr gefunden,", "tokens": ["So", "von", "ohn\u00b7ge\u00b7f\u00e4hr", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dem Esquilin! Des Klosters", "tokens": ["Auf", "dem", "E\u00b7squi\u00b7lin", "!", "Des", "Klos\u00b7ters"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stillem Garten sahn wir m\u00e4chtig", "tokens": ["Stil\u00b7lem", "Gar\u00b7ten", "sahn", "wir", "m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich der Palme Wuchs entheben,", "tokens": ["Sich", "der", "Pal\u00b7me", "Wuchs", "ent\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und in ihrer Herrlichkeit", "tokens": ["Und", "in", "ih\u00b7rer", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Roms Ruinen sich entfalten.", "tokens": ["Roms", "Ru\u00b7i\u00b7nen", "sich", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Oftmals wanderten wir einsam", "tokens": ["Oft\u00b7mals", "wan\u00b7der\u00b7ten", "wir", "ein\u00b7sam"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Der Metella Riesengrabe,", "tokens": ["Der", "Me\u00b7tel\u00b7la", "Rie\u00b7sen\u00b7gra\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Oft der Grotte der Egeria,", "tokens": ["Oft", "der", "Grot\u00b7te", "der", "E\u00b7ge\u00b7ria", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NE", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Oft des Pincio s\u00fc\u00dfen H\u00f6hen,", "tokens": ["Oft", "des", "Pin\u00b7cio", "s\u00fc\u00b7\u00dfen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oder wohl des Tibers Br\u00fccken", "tokens": ["O\u00b7der", "wohl", "des", "Ti\u00b7bers", "Br\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und des Forums Tempeltr\u00fcmmern,", "tokens": ["Und", "des", "Fo\u00b7rums", "Tem\u00b7pel\u00b7tr\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und dem Colosseum zu,", "tokens": ["Und", "dem", "Co\u00b7los\u00b7se\u00b7um", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo der Genius uns gef\u00fchret.", "tokens": ["Wo", "der", "Ge\u00b7nius", "uns", "ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Und wie um der R\u00f6mertempel", "tokens": ["Und", "wie", "um", "der", "R\u00f6\u00b7mer\u00b7tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Altergraue d\u00fcstre Reste", "tokens": ["Al\u00b7ter\u00b7grau\u00b7e", "d\u00fcst\u00b7re", "Res\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lustig Laub und heitre Blumen", "tokens": ["Lus\u00b7tig", "Laub", "und", "heit\u00b7re", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gern in fl\u00fccht'ger Bl\u00fcthe wuchern,", "tokens": ["Gern", "in", "fl\u00fccht'\u00b7ger", "Bl\u00fc\u00b7the", "wu\u00b7chern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wand durch ernstere Gespr\u00e4che", "tokens": ["Wand", "durch", "erns\u00b7te\u00b7re", "Ge\u00b7spr\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Still bed\u00e4chtliche Betrachtung", "tokens": ["Still", "be\u00b7d\u00e4cht\u00b7li\u00b7che", "Be\u00b7trach\u00b7tung"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich ein kecker muth'ger Scherz", "tokens": ["Sich", "ein", "ke\u00b7cker", "muth'\u00b7ger", "Scherz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "ADJA", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "In verweg'ner \u00fcpp'ger F\u00fclle.", "tokens": ["In", "ver\u00b7weg'\u00b7ner", "\u00fcpp'\u00b7ger", "F\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wahr ist es, auf meinem Boden", "tokens": ["Wahr", "ist", "es", ",", "auf", "mei\u00b7nem", "Bo\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wuchs des Unkrauts viel, zerst\u00f6rend", "tokens": ["Wuchs", "des", "Un\u00b7krauts", "viel", ",", "zer\u00b7st\u00f6\u00b7rend"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "NN", "ADV", "$,", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Traf ihn Sonnenbrand und St\u00fcrme;", "tokens": ["Traf", "ihn", "Son\u00b7nen\u00b7brand", "und", "St\u00fcr\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwar die vollsten Rosenkr\u00e4nze,", "tokens": ["Zwar", "die", "volls\u00b7ten", "Ro\u00b7sen\u00b7kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch der Dornen allzuviele", "tokens": ["Doch", "der", "Dor\u00b7nen", "all\u00b7zu\u00b7vie\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dr\u00fcckte mir auf's Haupt der Amor,", "tokens": ["Dr\u00fcck\u00b7te", "mir", "auf's", "Haupt", "der", "A\u00b7mor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dem ich in Genu\u00df und Lust", "tokens": ["Dem", "ich", "in", "Ge\u00b7nu\u00df", "und", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Als ein irrend Weltkind gl\u00fchte.", "tokens": ["Als", "ein", "ir\u00b7rend", "Welt\u00b7kind", "gl\u00fch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber du im Heiligthume,", "tokens": ["A\u00b7ber", "du", "im", "Hei\u00b7lig\u00b7thu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nie entweiht, hast ihm als Priester", "tokens": ["Nie", "ent\u00b7weiht", ",", "hast", "ihm", "als", "Pries\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "VAFIN", "PPER", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine geist'ge Flamm' erhalten.", "tokens": ["Sei\u00b7ne", "geist'\u00b7ge", "Flamm'", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich verstand dich wohl, und gerne", "tokens": ["Ich", "ver\u00b7stand", "dich", "wohl", ",", "und", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hast auch du mich stets geduldet,", "tokens": ["Hast", "auch", "du", "mich", "stets", "ge\u00b7dul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und so wehte mir die Schalkheit", "tokens": ["Und", "so", "weh\u00b7te", "mir", "die", "Schalk\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch ins Herz den Bl\u00fcthenduft", "tokens": ["Auch", "ins", "Herz", "den", "Bl\u00fc\u00b7then\u00b7duft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Deiner Muse, deiner Scherze.", "tokens": ["Dei\u00b7ner", "Mu\u00b7se", ",", "dei\u00b7ner", "Scher\u00b7ze", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Aber la\u00df nun, mich zu schelten!", "tokens": ["A\u00b7ber", "la\u00df", "nun", ",", "mich", "zu", "schel\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist die S\u00fcndfluth, die so schnelle", "tokens": ["Ist", "die", "S\u00fcnd\u00b7fluth", ",", "die", "so", "schnel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine kleine Welt zerst\u00f6ret,", "tokens": ["Mei\u00b7ne", "klei\u00b7ne", "Welt", "zer\u00b7st\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich doch zur\u00fcckgewichen,", "tokens": ["End\u00b7lich", "doch", "zu\u00b7r\u00fcck\u00b7ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die gr\u00fcnen lichten H\u00f6hen", "tokens": ["Und", "die", "gr\u00fc\u00b7nen", "lich\u00b7ten", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gl\u00e4nzen schon im Sonnenscheine,", "tokens": ["Gl\u00e4n\u00b7zen", "schon", "im", "Son\u00b7nen\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Friedensbogen ruht", "tokens": ["Und", "der", "Frie\u00b7dens\u00b7bo\u00b7gen", "ruht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "L\u00e4chelnd im entw\u00f6lkten Himmel.", "tokens": ["L\u00e4\u00b7chelnd", "im", "ent\u00b7w\u00f6lk\u00b7ten", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Eine Taube lie\u00df ich fliegen \u2013", "tokens": ["Ei\u00b7ne", "Tau\u00b7be", "lie\u00df", "ich", "flie\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deute sie \u2013 und einen Oelzweig", "tokens": ["Deu\u00b7te", "sie", "\u2013", "und", "ei\u00b7nen", "O\u00b7el\u00b7zweig"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$(", "KON", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Brachte sie zur\u00fcck! ich habe", "tokens": ["Brach\u00b7te", "sie", "zu\u00b7r\u00fcck", "!", "ich", "ha\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch mein Bestes mir gerettet.", "tokens": ["Doch", "mein", "Bes\u00b7tes", "mir", "ge\u00b7ret\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Freund, mein Herz! In frischer Weihe", "tokens": ["Freund", ",", "mein", "Herz", "!", "In", "fri\u00b7scher", "Wei\u00b7he"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat es der vers\u00f6hnten Gottheit,", "tokens": ["Hat", "es", "der", "ver\u00b7s\u00f6hn\u00b7ten", "Got\u00b7theit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Hat's der Muse, die dich kr\u00f6nt,", "tokens": ["Hat's", "der", "Mu\u00b7se", ",", "die", "dich", "kr\u00f6nt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ew'ge Treue schon geschworen.", "tokens": ["Ew'\u00b7ge", "Treu\u00b7e", "schon", "ge\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und so k\u00f6nnt' ich wohl es wagen,", "tokens": ["Und", "so", "k\u00f6nnt'", "ich", "wohl", "es", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir die Freundeshand zu bieten;", "tokens": ["Dir", "die", "Freun\u00b7des\u00b7hand", "zu", "bie\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4r' ich noch ein Schw\u00e4rmer, rief' ich", "tokens": ["W\u00e4r'", "ich", "noch", "ein", "Schw\u00e4r\u00b7mer", ",", "rie\u00b7f'", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Alle Tempel Roms zu Zeugen,", "tokens": ["Al\u00b7le", "Tem\u00b7pel", "Roms", "zu", "Zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NE", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch wozu? Du liebst zu schweigen,", "tokens": ["Doch", "wo\u00b7zu", "?", "Du", "liebst", "zu", "schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$.", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.6": {"text": "Liebst die Einsamkeit, und freilich", "tokens": ["Liebst", "die", "Ein\u00b7sam\u00b7keit", ",", "und", "frei\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dir verdenk' ich's nicht, du hast", "tokens": ["Dir", "ver\u00b7denk'", "ich's", "nicht", ",", "du", "hast"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "PTKNEG", "$,", "PPER", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle Grazien zu Gespielen.", "tokens": ["Al\u00b7le", "Gra\u00b7zi\u00b7en", "zu", "Ge\u00b7spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}}}}