{"textgrid.poem.40140": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bin ich ein K\u00f6nig? \u2013 Da ein ", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bin ich ein K\u00f6nig? \u2013 Da ein ", "tokens": ["Bin", "ich", "ein", "K\u00f6\u00b7nig", "?", "\u2013", "Da", "ein"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "$.", "$(", "KOUS", "ART"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.2": {"text": "da tr\u00e4umte ich von einem goldnen Throne,", "tokens": ["da", "tr\u00e4um\u00b7te", "ich", "von", "ei\u00b7nem", "gold\u00b7nen", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "von eines gro\u00dfen Volkes Jubelschaar,", "tokens": ["von", "ei\u00b7nes", "gro\u00b7\u00dfen", "Vol\u00b7kes", "Ju\u00b7bel\u00b7schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "von einem Purpurmantel, einer Krone.", "tokens": ["von", "ei\u00b7nem", "Pur\u00b7pur\u00b7man\u00b7tel", ",", "ei\u00b7ner", "Kro\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich ward ein J\u00fcngling, und der ", "tokens": ["Ich", "ward", "ein", "J\u00fcng\u00b7ling", ",", "und", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KON", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "verblich im Sonnenschein des ", "tokens": ["ver\u00b7blich", "im", "Son\u00b7nen\u00b7schein", "des"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "da tr\u00e4umte ich von einem andern Kranz,", "tokens": ["da", "tr\u00e4um\u00b7te", "ich", "von", "ei\u00b7nem", "an\u00b7dern", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "mit dem ein gro\u00dfes Volk mich sollte kr\u00f6nen.", "tokens": ["mit", "dem", "ein", "gro\u00b7\u00dfes", "Volk", "mich", "soll\u00b7te", "kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "ADJA", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Jetzt tr\u00e4um' ich nicht von Kronen, nicht von Kr\u00e4nzen,", "tokens": ["Jetzt", "tr\u00e4um'", "ich", "nicht", "von", "Kro\u00b7nen", ",", "nicht", "von", "Kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$,", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "von keinem Ziele, das der Stolz gebar;", "tokens": ["von", "kei\u00b7nem", "Zie\u00b7le", ",", "das", "der", "Stolz", "ge\u00b7bar", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ich forsche nicht, wo meines Reiches Grenzen;", "tokens": ["ich", "for\u00b7sche", "nicht", ",", "wo", "mei\u00b7nes", "Rei\u00b7ches", "Gren\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ich wei\u00df nur, Aufw\u00e4rts mu\u00df ich immerdar!", "tokens": ["ich", "wei\u00df", "nur", ",", "Auf\u00b7w\u00e4rts", "mu\u00df", "ich", "im\u00b7mer\u00b7dar", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "nur immer fliegen, wie der Adler fliegen,", "tokens": ["nur", "im\u00b7mer", "flie\u00b7gen", ",", "wie", "der", "Ad\u00b7ler", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "den es empor zum Quell des Lichtes rei\u00dft!", "tokens": ["den", "es", "em\u00b7por", "zum", "Quell", "des", "Lich\u00b7tes", "rei\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKVZ", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ich ", "tokens": ["ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "die flatternd ihre Ackerfurche preist!", "tokens": ["die", "flat\u00b7ternd", "ih\u00b7re", "A\u00b7cker\u00b7fur\u00b7che", "preist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich ", "tokens": ["Ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Gestalten nah'n, zerrei\u00dfen die Umh\u00fcllung!", "tokens": ["Ge\u00b7stal\u00b7ten", "nah'n", ",", "zer\u00b7rei\u00b7\u00dfen", "die", "Um\u00b7h\u00fcl\u00b7lung", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "nun streb' ich nur, des Strebens ", "tokens": ["nun", "streb'", "ich", "nur", ",", "des", "Stre\u00b7bens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "das ", "tokens": ["das"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.6": {"line.1": {"text": "da\u00df meine ganze Sprache mir zu wenig,", "tokens": ["da\u00df", "mei\u00b7ne", "gan\u00b7ze", "Spra\u00b7che", "mir", "zu", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "PTKA", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "all meines Stolzes Seligkeit zu zeigen, \u2013", "tokens": ["all", "mei\u00b7nes", "Stol\u00b7zes", "Se\u00b7lig\u00b7keit", "zu", "zei\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und jauchzend r\u00fchme ich: Ich ", "tokens": ["und", "jauch\u00b7zend", "r\u00fch\u00b7me", "ich", ":", "Ich"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$.", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}