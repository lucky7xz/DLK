{"textgrid.poem.55389": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "11", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Dorfe war ein gro\u00df Gelag,", "tokens": ["Im", "Dor\u00b7fe", "war", "ein", "gro\u00df", "Ge\u00b7lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man sagt', es sei ein Hochzeittag.", "tokens": ["Man", "sagt'", ",", "es", "sei", "ein", "Hoch\u00b7zeit\u00b7tag", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich zw\u00e4ngte mich in den Schenkensaal,", "tokens": ["Ich", "zw\u00e4ng\u00b7te", "mich", "in", "den", "Schen\u00b7ken\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da drehten die P\u00e4rchen allzumal,", "tokens": ["Da", "dreh\u00b7ten", "die", "P\u00e4r\u00b7chen", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ein jedes M\u00e4dchen mit seinem Wicht,", "tokens": ["Ein", "je\u00b7des", "M\u00e4d\u00b7chen", "mit", "sei\u00b7nem", "Wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da gab es manch verliebt Gesicht.", "tokens": ["Da", "gab", "es", "manch", "ver\u00b7liebt", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nun fragt ich endlich nach der Braut \u2013", "tokens": ["Nun", "fragt", "ich", "end\u00b7lich", "nach", "der", "Braut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mir einer starr ins Angesicht schaut:", "tokens": ["Mir", "ei\u00b7ner", "starr", "ins", "An\u00b7ge\u00b7sicht", "schaut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "\u00bbdas m\u00f6gt Ihr von einem andern h\u00f6ren!", "tokens": ["\u00bb", "das", "m\u00f6gt", "Ihr", "von", "ei\u00b7nem", "an\u00b7dern", "h\u00f6\u00b7ren", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VMFIN", "PPER", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Wir aber tanzen ihr zu Ehren,", "tokens": ["Wir", "a\u00b7ber", "tan\u00b7zen", "ihr", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir tanzen schon drei Tag und Nacht,", "tokens": ["Wir", "tan\u00b7zen", "schon", "drei", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "NN", "KON", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Und hat noch niemand an sie gedacht.\u00ab", "tokens": ["Und", "hat", "noch", "nie\u00b7mand", "an", "sie", "ge\u00b7dacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIS", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Will einer im Leben um sich schauen,", "tokens": ["Will", "ei\u00b7ner", "im", "Le\u00b7ben", "um", "sich", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "APPRART", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dergleichen wird man ihm viel vertrauen.", "tokens": ["Derg\u00b7lei\u00b7chen", "wird", "man", "ihm", "viel", "ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PIS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}