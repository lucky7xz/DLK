{"textgrid.poem.39939": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Reiter und der Bodensee", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Reiter reitet durchs helle Thal,", "tokens": ["Der", "Rei\u00b7ter", "rei\u00b7tet", "durchs", "hel\u00b7le", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf Schneefeld schimmert der Sonne Stral.", "tokens": ["Auf", "Schnee\u00b7feld", "schim\u00b7mert", "der", "Son\u00b7ne", "Stral", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Er trabet im Schwei\u00df durch den kalten Schnee,", "tokens": ["Er", "tra\u00b7bet", "im", "Schwei\u00df", "durch", "den", "kal\u00b7ten", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Er will noch heut an den Bodensee;", "tokens": ["Er", "will", "noch", "heut", "an", "den", "Bo\u00b7den\u00b7see", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Noch heut mit dem Pferd in den sichern Kahn,", "tokens": ["Noch", "heut", "mit", "dem", "Pferd", "in", "den", "si\u00b7chern", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Will dr\u00fcben landen vor Nacht noch an.", "tokens": ["Will", "dr\u00fc\u00b7ben", "lan\u00b7den", "vor", "Nacht", "noch", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "APPR", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Auf schlimmem Weg, \u00fcber Dorn und Stein,", "tokens": ["Auf", "schlim\u00b7mem", "Weg", ",", "\u00fc\u00b7ber", "Dorn", "und", "Stein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er braust auf r\u00fcstigem Ro\u00df feldein.", "tokens": ["Er", "braust", "auf", "r\u00fcs\u00b7ti\u00b7gem", "Ro\u00df", "feld\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Aus den Bergen heraus, in's ebene Land,", "tokens": ["Aus", "den", "Ber\u00b7gen", "he\u00b7raus", ",", "in's", "e\u00b7be\u00b7ne", "Land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da sieht er den Schnee sich dehnen, wie Sand.", "tokens": ["Da", "sieht", "er", "den", "Schnee", "sich", "deh\u00b7nen", ",", "wie", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PRF", "PDS", "$,", "PWAV", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Weit hinter ihm schwinden Dorf und Stadt,", "tokens": ["Weit", "hin\u00b7ter", "ihm", "schwin\u00b7den", "Dorf", "und", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Weg wird eben, die Bahn wird glatt.", "tokens": ["Der", "Weg", "wird", "e\u00b7ben", ",", "die", "Bahn", "wird", "glatt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "In weiter Fl\u00e4che kein B\u00fchl, kein Haus,", "tokens": ["In", "wei\u00b7ter", "Fl\u00e4\u00b7che", "kein", "B\u00fchl", ",", "kein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die B\u00e4ume gingen, die Felsen aus;", "tokens": ["Die", "B\u00e4u\u00b7me", "gin\u00b7gen", ",", "die", "Fel\u00b7sen", "aus", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "So flieget er hin eine Meil', und zwei,", "tokens": ["So", "flie\u00b7get", "er", "hin", "ei\u00b7ne", "Meil'", ",", "und", "zwei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "KON", "CARD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er h\u00f6rt in den L\u00fcften der Schneegans Schrei;", "tokens": ["Er", "h\u00f6rt", "in", "den", "L\u00fcf\u00b7ten", "der", "Schnee\u00b7gans", "Schrei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Es flattert das Wasserhuhn empor,", "tokens": ["Es", "flat\u00b7tert", "das", "Was\u00b7ser\u00b7huhn", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nicht anderen Laut vernimmt sein Ohr;", "tokens": ["Nicht", "an\u00b7de\u00b7ren", "Laut", "ver\u00b7nimmt", "sein", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "APPR", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Keinen Wandersmann sein Auge schaut,", "tokens": ["Kei\u00b7nen", "Wan\u00b7ders\u00b7mann", "sein", "Au\u00b7ge", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Der ihm den rechten Pfad vertraut.", "tokens": ["Der", "ihm", "den", "rech\u00b7ten", "Pfad", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Fort geht's, wie auf Sammt, auf dem weichen Schnee,", "tokens": ["Fort", "geht's", ",", "wie", "auf", "Sammt", ",", "auf", "dem", "wei\u00b7chen", "Schnee", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "APPR", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wann rauscht das Wasser, wann gl\u00e4nzt der See?", "tokens": ["Wann", "rauscht", "das", "Was\u00b7ser", ",", "wann", "gl\u00e4nzt", "der", "See", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Da bricht der Abend, der fr\u00fche, herein:", "tokens": ["Da", "bricht", "der", "A\u00b7bend", ",", "der", "fr\u00fc\u00b7he", ",", "her\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von Lichtern blinket ein ferner Schein.", "tokens": ["Von", "Lich\u00b7tern", "blin\u00b7ket", "ein", "fer\u00b7ner", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.13": {"line.1": {"text": "Es hebt aus dem Nebel sich Baum an Baum,", "tokens": ["Es", "hebt", "aus", "dem", "Ne\u00b7bel", "sich", "Baum", "an", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PRF", "NE", "APPR", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und H\u00fcgel schlie\u00dfen den weiten Raum.", "tokens": ["Und", "H\u00fc\u00b7gel", "schlie\u00b7\u00dfen", "den", "wei\u00b7ten", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Er sp\u00fcrt auf dem Boden Stein und Dorn,", "tokens": ["Er", "sp\u00fcrt", "auf", "dem", "Bo\u00b7den", "Stein", "und", "Dorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem Rosse giebt er den scharfen Sporn.", "tokens": ["Dem", "Ros\u00b7se", "giebt", "er", "den", "schar\u00b7fen", "Sporn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.15": {"line.1": {"text": "Und Hunde bellen empor am Pferd,", "tokens": ["Und", "Hun\u00b7de", "bel\u00b7len", "em\u00b7por", "am", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und es winkt im Dorf ihm der warme Herd.", "tokens": ["Und", "es", "winkt", "im", "Dorf", "ihm", "der", "war\u00b7me", "Herd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.16": {"line.1": {"text": "\u00bbwillkommen am Fenster, M\u00e4gdelein,", "tokens": ["\u00bb", "will\u00b7kom\u00b7men", "am", "Fens\u00b7ter", ",", "M\u00e4g\u00b7del\u00b7ein", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADJD", "APPRART", "NN", "$,", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An den See, an den See, wie weit mag's sein?\u00ab", "tokens": ["An", "den", "See", ",", "an", "den", "See", ",", "wie", "weit", "mag's", "sein", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "PWAV", "ADJD", "ADJD", "VAINF", "$.", "$("], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.17": {"line.1": {"text": "Die Maid sie staunet den Reiter an:", "tokens": ["Die", "Maid", "sie", "stau\u00b7net", "den", "Rei\u00b7ter", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbder See liegt hinter dir und der Kahn.", "tokens": ["\u00bb", "der", "See", "liegt", "hin\u00b7ter", "dir", "und", "der", "Kahn", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "APPR", "PPER", "KON", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.18": {"line.1": {"text": "Und deckt' ihn die Rinde von Eis nicht zu,", "tokens": ["Und", "deckt'", "ihn", "die", "Rin\u00b7de", "von", "Eis", "nicht", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ich spr\u00e4ch', aus dem Nachen stiegest du.\u00ab", "tokens": ["Ich", "spr\u00e4ch'", ",", "aus", "dem", "Na\u00b7chen", "stie\u00b7gest", "du", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.19": {"line.1": {"text": "Der Fremde schaudert, er athmet schwer:", "tokens": ["Der", "Frem\u00b7de", "schau\u00b7dert", ",", "er", "ath\u00b7met", "schwer", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbdort hinten die Ebne, die ritt ich her!\u00ab", "tokens": ["\u00bb", "dort", "hin\u00b7ten", "die", "Eb\u00b7ne", ",", "die", "ritt", "ich", "her", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADV", "ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.20": {"line.1": {"text": "Da recket die Magd die Arm' in die H\u00f6h':", "tokens": ["Da", "re\u00b7cket", "die", "Magd", "die", "Arm'", "in", "die", "H\u00f6h'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbherr Gott! so rittest du \u00fcber den See!", "tokens": ["\u00bb", "herr", "Gott", "!", "so", "rit\u00b7test", "du", "\u00fc\u00b7ber", "den", "See", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NN", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.21": {"line.1": {"text": "An den Schlund, an die Tiefe bodenlos,", "tokens": ["An", "den", "Schlund", ",", "an", "die", "Tie\u00b7fe", "bo\u00b7den\u00b7los", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Hat gepocht des rasenden Hufes Sto\u00df!", "tokens": ["Hat", "ge\u00b7pocht", "des", "ra\u00b7sen\u00b7den", "Hu\u00b7fes", "Sto\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.22": {"line.1": {"text": "Und unter dir z\u00fcrnten die Wasser nicht?", "tokens": ["Und", "un\u00b7ter", "dir", "z\u00fcrn\u00b7ten", "die", "Was\u00b7ser", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Nicht krachte hinunter die Rinde dicht?", "tokens": ["Nicht", "krach\u00b7te", "hin\u00b7un\u00b7ter", "die", "Rin\u00b7de", "dicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.23": {"line.1": {"text": "Und du wardst nicht die Speise der stummen Brut?", "tokens": ["Und", "du", "wardst", "nicht", "die", "Spei\u00b7se", "der", "stum\u00b7men", "Brut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Der hungrigen Hecht' in der kalten Flut?\u00ab", "tokens": ["Der", "hung\u00b7ri\u00b7gen", "Hecht'", "in", "der", "kal\u00b7ten", "Flut", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA",