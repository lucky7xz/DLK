{"textgrid.poem.65729": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh in dein Glas! Zum Bild, das es dir weist,", "tokens": ["Sieh", "in", "dein", "Glas", "!", "Zum", "Bild", ",", "das", "es", "dir", "weist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$.", "APPRART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sprich: Bild, nun mu\u00dft du auf dein Abbild denken.", "tokens": ["Sprich", ":", "Bild", ",", "nun", "mu\u00dft", "du", "auf", "dein", "Ab\u00b7bild", "den\u00b7ken", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "NN", "$,", "ADV", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn du dich jetzt auffrischend nicht erneust,", "tokens": ["Wenn", "du", "dich", "jetzt", "auf\u00b7fri\u00b7schend", "nicht", "er\u00b7neust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "H\u00f6hnst du die Welt, wirst Mutterrechte kr\u00e4nken.", "tokens": ["H\u00f6hnst", "du", "die", "Welt", ",", "wirst", "Mut\u00b7ter\u00b7rech\u00b7te", "kr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn welcher Sch\u00f6nen unbestellter Scho\u00df", "tokens": ["Denn", "wel\u00b7cher", "Sch\u00f6\u00b7nen", "un\u00b7be\u00b7stell\u00b7ter", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verschm\u00e4ht den Pflug wohl deiner Feldwirtschaft?", "tokens": ["Ver\u00b7schm\u00e4ht", "den", "Pflug", "wohl", "dei\u00b7ner", "Feld\u00b7wirt\u00b7schaft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wer w\u00e4r in eigner Meinung je so gro\u00df,", "tokens": ["Wer", "w\u00e4r", "in", "eig\u00b7ner", "Mei\u00b7nung", "je", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "ADJA", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Selbstsucht Grab zu sein, der Enkel Haft?", "tokens": ["Der", "Selbst\u00b7sucht", "Grab", "zu", "sein", ",", "der", "En\u00b7kel", "Haft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VAINF", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Du, deiner Mutter Spiegel, zauberst ihr", "tokens": ["Du", ",", "dei\u00b7ner", "Mut\u00b7ter", "Spie\u00b7gel", ",", "zau\u00b7berst", "ihr"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "PPOSAT", "NN", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Jugendtage holden Lenz herbei:", "tokens": ["Der", "Ju\u00b7gend\u00b7ta\u00b7ge", "hol\u00b7den", "Lenz", "her\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "So, trotz der Runzeln auch erscheinet dir", "tokens": ["So", ",", "trotz", "der", "Run\u00b7zeln", "auch", "er\u00b7schei\u00b7net", "dir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Durch deines Alters Fenster einst dein Mai.", "tokens": ["Durch", "dei\u00b7nes", "Al\u00b7ters", "Fens\u00b7ter", "einst", "dein", "Mai", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Doch, lebst du nur Vergessenheit zu erben,", "tokens": ["Doch", ",", "lebst", "du", "nur", "Ver\u00b7ges\u00b7sen\u00b7heit", "zu", "er\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "ADV", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Stirb einsam, und dein Bild wird mit dir sterben.", "tokens": ["Stirb", "ein\u00b7sam", ",", "und", "dein", "Bild", "wird", "mit", "dir", "ster\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "$,", "KON", "PPOSAT", "NN", "VAFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}