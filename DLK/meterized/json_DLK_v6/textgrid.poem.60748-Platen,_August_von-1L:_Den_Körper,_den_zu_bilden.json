{"textgrid.poem.60748": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den K\u00f6rper, den zu bilden", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den K\u00f6rper, den zu bilden", "tokens": ["Den", "K\u00f6r\u00b7per", ",", "den", "zu", "bil\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Natur hat aufgewendet all ihr Lieben,", "tokens": ["Na\u00b7tur", "hat", "auf\u00b7ge\u00b7wen\u00b7det", "all", "ihr", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "PIAT", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den ihre Hand mit milden", "tokens": ["Den", "ih\u00b7re", "Hand", "mit", "mil\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Begrenzungen umschrieben,", "tokens": ["Be\u00b7gren\u00b7zun\u00b7gen", "um\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.5": {"text": "Den aus dem reinsten Golde sie getrieben:", "tokens": ["Den", "aus", "dem", "reins\u00b7ten", "Gol\u00b7de", "sie", "ge\u00b7trie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O woll ihn rein bewahren,", "tokens": ["O", "woll", "ihn", "rein", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und la\u00df dich nicht zum eitlen Spiel verlocken,", "tokens": ["Und", "la\u00df", "dich", "nicht", "zum", "eit\u00b7len", "Spiel", "ver\u00b7lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PTKNEG", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Spiele voll Gefahren,", "tokens": ["Zum", "Spie\u00b7le", "voll", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und weiche weg erschrocken,", "tokens": ["Und", "wei\u00b7che", "weg", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn eine Hand sich naht den goldnen Locken!", "tokens": ["Wenn", "ei\u00b7ne", "Hand", "sich", "naht", "den", "gold\u00b7nen", "Lo\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wiewohl dein ganzes Wesen", "tokens": ["Wie\u00b7wohl", "dein", "gan\u00b7zes", "We\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus leicht entz\u00fcndbarn Stoffen scheint zu stammen,", "tokens": ["Aus", "leicht", "ent\u00b7z\u00fcnd\u00b7barn", "Stof\u00b7fen", "scheint", "zu", "stam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zur Liebe scheint erlesen,", "tokens": ["Zur", "Lie\u00b7be", "scheint", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "La\u00df doch dich nicht entflammen,", "tokens": ["La\u00df", "doch", "dich", "nicht", "ent\u00b7flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sonst schl\u00e4gt die Glut dir \u00fcberm Haupt zusammen!", "tokens": ["Sonst", "schl\u00e4gt", "die", "Glut", "dir", "\u00fc\u00b7berm", "Haupt", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}