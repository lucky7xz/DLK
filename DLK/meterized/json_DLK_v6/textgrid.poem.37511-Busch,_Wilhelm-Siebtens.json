{"textgrid.poem.37511": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Siebtens", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wohnte zu Padua ein Weib,", "tokens": ["Es", "wohn\u00b7te", "zu", "Pa\u00b7dua", "ein", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "B\u00f6s von Seele, gut von Leib,", "tokens": ["B\u00f6s", "von", "See\u00b7le", ",", "gut", "von", "Leib", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Genannt die sch\u00f6ne Monika. \u2013", "tokens": ["Ge\u00b7nannt", "die", "sch\u00f6\u00b7ne", "Mo\u00b7ni\u00b7ka", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als die den frommen Pater sah,", "tokens": ["Als", "die", "den", "from\u00b7men", "Pa\u00b7ter", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Versp\u00fcrte sie ein gro\u00df Verlangen,", "tokens": ["Ver\u00b7sp\u00fcr\u00b7te", "sie", "ein", "gro\u00df", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auch ihn in ihre Netze zu fangen.", "tokens": ["Auch", "ihn", "in", "ih\u00b7re", "Net\u00b7ze", "zu", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "\u00bbgeht, rufet mir den heil'gen Mann\u00ab \u2013", "tokens": ["\u00bb", "geht", ",", "ru\u00b7fet", "mir", "den", "heil'\u00b7gen", "Mann", "\u00ab", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$(", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "So sprach sie \u2013 \u00bbda\u00df ich beichten kann!\u00ab", "tokens": ["So", "sprach", "sie", "\u2013", "\u00bb", "da\u00df", "ich", "beich\u00b7ten", "kann", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "$(", "KOUS", "PPER", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er kam und trat ins Schlafgemach.", "tokens": ["Er", "kam", "und", "trat", "ins", "Schlaf\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sie war so krank, sie war so schwach.", "tokens": ["Sie", "war", "so", "krank", ",", "sie", "war", "so", "schwach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbsei mir gegr\u00fc\u00dft, o heil'ger Mann!", "tokens": ["\u00bb", "sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "o", "heil'\u00b7ger", "Mann", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "VVPP", "$,", "FM", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und h\u00f6re meine Beichte an!\u00ab", "tokens": ["Und", "h\u00f6\u00b7re", "mei\u00b7ne", "Beich\u00b7te", "an", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Antonius sprach mit ernstem Ton:", "tokens": ["An\u00b7to\u00b7ni\u00b7us", "sprach", "mit", "erns\u00b7tem", "Ton", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u00bbfahre fort, meine Tochter, ich h\u00f6re schon!\u00ab", "tokens": ["\u00bb", "fah\u00b7re", "fort", ",", "mei\u00b7ne", "Toch\u00b7ter", ",", "ich", "h\u00f6\u00b7re", "schon", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "\u00bbam Freitag war es, vor acht Tagen \u2013", "tokens": ["\u00bb", "am", "Frei\u00b7tag", "war", "es", ",", "vor", "acht", "Ta\u00b7gen", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "VAFIN", "PPER", "$,", "APPR", "CARD", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ach Gott! Ich wag' es kaum zu sagen! \u2013", "tokens": ["Ach", "Gott", "!", "Ich", "wag'", "es", "kaum", "zu", "sa\u00b7gen", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es war schon sp\u00e4t, ich lag allein \u2013", "tokens": ["Es", "war", "schon", "sp\u00e4t", ",", "ich", "lag", "al\u00b7lein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da trat ein Freund zu mir herein. \u2013", "tokens": ["Da", "trat", "ein", "Freund", "zu", "mir", "her\u00b7ein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u2013 Gewi\u00df, ich konnte nichts daf\u00fcr! \u2013", "tokens": ["\u2013", "Ge\u00b7wi\u00df", ",", "ich", "konn\u00b7te", "nichts", "da\u00b7f\u00fcr", "!", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$,", "PPER", "VMFIN", "PIS", "PAV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er setzte sich ans Bett zu mir... \u2013...", "tokens": ["Er", "setz\u00b7te", "sich", "ans", "Bett", "zu", "mir", "...", "\u2013", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "APPR", "PPER", "$(", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "\u2013 Ach, frommer Vater Antonio!", "tokens": ["\u2013", "Ach", ",", "from\u00b7mer", "Va\u00b7ter", "An\u00b7to\u00b7nio", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wie Ihr da sitzt! Gerade so!\u00ab", "tokens": ["Wie", "Ihr", "da", "sitzt", "!", "Ge\u00b7ra\u00b7de", "so", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$.", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Antonius sprach mit ernstem Ton:", "tokens": ["An\u00b7to\u00b7ni\u00b7us", "sprach", "mit", "erns\u00b7tem", "Ton", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "\u00bbfahre fort, meine Tochter, ich h\u00f6re schon!\u00ab", "tokens": ["\u00bb", "fah\u00b7re", "fort", ",", "mei\u00b7ne", "Toch\u00b7ter", ",", "ich", "h\u00f6\u00b7re", "schon", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "\u00bbso sa\u00df er da und sprach kein Wort", "tokens": ["\u00bb", "so", "sa\u00df", "er", "da", "und", "sprach", "kein", "Wort"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sah mich an in einem fort", "tokens": ["Und", "sah", "mich", "an", "in", "ei\u00b7nem", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "APPR", "ART", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sah so fromm und freundlich drein \u2013", "tokens": ["Und", "sah", "so", "fromm", "und", "freund\u00b7lich", "drein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich konnte ihm nicht b\u00f6se sein!", "tokens": ["Ich", "konn\u00b7te", "ihm", "nicht", "b\u00f6\u00b7se", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u2013 Die Finger waren schlank und zart,", "tokens": ["\u2013", "Die", "Fin\u00b7ger", "wa\u00b7ren", "schlank", "und", "zart", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blau war sein Auge, blond sein Bart...", "tokens": ["Blau", "war", "sein", "Au\u00b7ge", ",", "blond", "sein", "Bart", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "\u2013 Ach, guter Vater Antonio!", "tokens": ["\u2013", "Ach", ",", "gu\u00b7ter", "Va\u00b7ter", "An\u00b7to\u00b7nio", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gerade wie Eurer! Gerade so!\u00ab", "tokens": ["Ge\u00b7ra\u00b7de", "wie", "Eu\u00b7rer", "!", "Ge\u00b7ra\u00b7de", "so", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "KOKOM", "PPOSAT", "$.", "ADV", "ADV", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Antonius sprach mit ernstem Ton:", "tokens": ["An\u00b7to\u00b7ni\u00b7us", "sprach", "mit", "erns\u00b7tem", "Ton", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "\u00bbfahre fort, meine Tochter, ich h\u00f6re schon!\u00ab", "tokens": ["\u00bb", "fah\u00b7re", "fort", ",", "mei\u00b7ne", "Toch\u00b7ter", ",", "ich", "h\u00f6\u00b7re", "schon", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "\u00bbund leise t\u00e4ndelnd, mit der Rechten,", "tokens": ["\u00bb", "und", "lei\u00b7se", "t\u00e4n\u00b7delnd", ",", "mit", "der", "Rech\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADJD", "VVPP", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ber\u00fchrt er meine losen Flechten,", "tokens": ["Be\u00b7r\u00fchrt", "er", "mei\u00b7ne", "lo\u00b7sen", "Flech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zieht meine Hand an seine Lippen,", "tokens": ["Zieht", "mei\u00b7ne", "Hand", "an", "sei\u00b7ne", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gar lieb und kosend dran zu nippen...", "tokens": ["Gar", "lieb", "und", "ko\u00b7send", "dran", "zu", "nip\u00b7pen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "PAV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, bester Vater Antonio!", "tokens": ["Ach", ",", "bes\u00b7ter", "Va\u00b7ter", "An\u00b7to\u00b7nio", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So nippte er! Gerade so!!!\u00ab", "tokens": ["So", "nipp\u00b7te", "er", "!", "Ge\u00b7ra\u00b7de", "so", "!!!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Antonius sprach mit ernstem Ton:", "tokens": ["An\u00b7to\u00b7ni\u00b7us", "sprach", "mit", "erns\u00b7tem", "Ton", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "\u00bbfahre fort, meine Tochter, ich h\u00f6re schon!\u00ab", "tokens": ["\u00bb", "fah\u00b7re", "fort", ",", "mei\u00b7ne", "Toch\u00b7ter", ",", "ich", "h\u00f6\u00b7re", "schon", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}}, "stanza.7": {"line.1": {"text": "\u00bbso nippte er \u2013 und nippt nicht lange \u2013", "tokens": ["\u00bb", "so", "nipp\u00b7te", "er", "\u2013", "und", "nippt", "nicht", "lan\u00b7ge", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$(", "KON", "VVFIN", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er pre\u00dft' den Mund an meine Wange.", "tokens": ["Er", "pre\u00dft'", "den", "Mund", "an", "mei\u00b7ne", "Wan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u203ageliebte\u2039, sprach er, \u203aliebst du mich??\u2039", "tokens": ["\u203a", "ge\u00b7lieb\u00b7te", "\u2039", ",", "sprach", "er", ",", "\u203a", "liebst", "du", "mich", "??", "\u2039"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "VVFIN", "PPER", "PRF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u203aja\u2039, sprach ich, \u203arasend lieb' ich dich!!\u2039", "tokens": ["\u203a", "ja", "\u2039", ",", "sprach", "ich", ",", "\u203a", "ra\u00b7send", "lieb'", "ich", "dich", "!!", "\u2039"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "VVPP", "VVFIN", "PPER", "PRF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ja, liebster, bester Antonio!", "tokens": ["Ja", ",", "liebs\u00b7ter", ",", "bes\u00b7ter", "An\u00b7to\u00b7nio", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "$,", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich lieb dich rasend, gerade so!!!\u00ab", "tokens": ["Ich", "lieb", "dich", "ra\u00b7send", ",", "ge\u00b7ra\u00b7de", "so", "!!!", "\u00ab"], "token_info": ["word", "word", "word"