{"dta.poem.21622": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Kein Ende .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.57", "af:0.42"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Jene Hand, die im verworrnen", "tokens": ["Je\u00b7ne", "Hand", ",", "die", "im", "ver\u00b7worr\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Traume dunkler Schmerzensn\u00e4chte", "tokens": ["Trau\u00b7me", "dunk\u00b7ler", "Schmer\u00b7zens\u00b7n\u00e4ch\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir den Kranz gereicht von Dornen,", "tokens": ["Mir", "den", "Kranz", "ge\u00b7reicht", "von", "Dor\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4lt ein bl\u00fchendes Geflechte", "tokens": ["H\u00e4lt", "ein", "bl\u00fc\u00b7hen\u00b7des", "Ge\u00b7flech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Junger Rosen;", "tokens": ["Jun\u00b7ger", "Ro\u00b7sen", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Und die zarten Lippen, welche", "tokens": ["Und", "die", "zar\u00b7ten", "Lip\u00b7pen", ",", "wel\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Einst gedroht als Todeskelche,", "tokens": ["Einst", "ge\u00b7droht", "als", "To\u00b7des\u00b7kel\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "L\u00e4cheln, kosen.", "tokens": ["L\u00e4\u00b7cheln", ",", "ko\u00b7sen", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Was ich ringend nie erstritten,", "tokens": ["Was", "ich", "rin\u00b7gend", "nie", "er\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Schwebt nun sanften Flugs herbei,", "tokens": ["Schwebt", "nun", "sanf\u00b7ten", "Flugs", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und der Liebsten Augen bitten:", "tokens": ["Und", "der", "Liebs\u00b7ten", "Au\u00b7gen", "bit\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.12": {"text": "Ach, verzeih!", "tokens": ["Ach", ",", "ver\u00b7zeih", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Also la\u00df ich steuerlos", "tokens": ["Al\u00b7so", "la\u00df", "ich", "steu\u00b7er\u00b7los"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Traumw\u00e4rts treiben meinen Nachen;", "tokens": ["Traum\u00b7w\u00e4rts", "trei\u00b7ben", "mei\u00b7nen", "Na\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Denn der Tag ist nackt und blo\u00df,", "tokens": ["Denn", "der", "Tag", "ist", "nackt", "und", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "KON", "ADV", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.16": {"text": "Und ich will nicht wachen. \u2014", "tokens": ["Und", "ich", "will", "nicht", "wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VMFIN", "PTKNEG", "VVINF", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}