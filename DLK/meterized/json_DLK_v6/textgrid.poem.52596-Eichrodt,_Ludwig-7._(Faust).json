{"textgrid.poem.52596": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "7. (Faust)", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In ihren Tr\u00e4umen wird die Tr\u00fcbsal wohnen,", "tokens": ["In", "ih\u00b7ren", "Tr\u00e4u\u00b7men", "wird", "die", "Tr\u00fcb\u00b7sal", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gib Acht, da\u00df dir nicht einmal bangt!", "tokens": ["Gib", "Acht", ",", "da\u00df", "dir", "nicht", "ein\u00b7mal", "bangt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du kannst die stumme Treue ihr nicht lohnen,", "tokens": ["Du", "kannst", "die", "stum\u00b7me", "Treu\u00b7e", "ihr", "nicht", "loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie es dein stolzes Herz verlangt.", "tokens": ["Wie", "es", "dein", "stol\u00b7zes", "Herz", "ver\u00b7langt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du kannst vom Schlaf ihr nicht den Treuen beschw\u00f6ren,", "tokens": ["Du", "kannst", "vom", "Schlaf", "ihr", "nicht", "den", "Treu\u00b7en", "be\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der ihr ein Schreckensende malt,", "tokens": ["Der", "ihr", "ein", "Schre\u00b7cken\u00b7sen\u00b7de", "malt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du hast kein Recht die Thr\u00e4nen ihr zu wehren,", "tokens": ["Du", "hast", "kein", "Recht", "die", "Thr\u00e4\u00b7nen", "ihr", "zu", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hast \u00fcber Thr\u00e4nen nicht Gewalt!", "tokens": ["Hast", "\u00fc\u00b7ber", "Thr\u00e4\u00b7nen", "nicht", "Ge\u00b7walt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und dennoch, willst du feig dich selber narren!", "tokens": ["Und", "den\u00b7noch", ",", "willst", "du", "feig", "dich", "sel\u00b7ber", "nar\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VMFIN", "PPER", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil ewige Treue dir nicht frommt,", "tokens": ["Weil", "e\u00b7wi\u00b7ge", "Treu\u00b7e", "dir", "nicht", "frommt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In jedem Lenze kl\u00fcgelnd weiterharren,", "tokens": ["In", "je\u00b7dem", "Len\u00b7ze", "kl\u00fc\u00b7gelnd", "wei\u00b7ter\u00b7har\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob nicht ein bessrer Fr\u00fchling kommt?", "tokens": ["Ob", "nicht", "ein", "bess\u00b7rer", "Fr\u00fch\u00b7ling", "kommt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Willst du, solange noch die Augen blitzen,", "tokens": ["Willst", "du", ",", "so\u00b7lan\u00b7ge", "noch", "die", "Au\u00b7gen", "blit\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So lang dich Jugend noch durchbraust,", "tokens": ["So", "lang", "dich", "Ju\u00b7gend", "noch", "durch\u00b7braust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein angestaunter Weiser trauernd sitzen,", "tokens": ["Ein", "an\u00b7ge\u00b7staun\u00b7ter", "Wei\u00b7ser", "trau\u00b7ernd", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis endlich vor dir selbst dir graust?", "tokens": ["Bis", "end\u00b7lich", "vor", "dir", "selbst", "dir", "graust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und siehst du nicht der Hoffnung Sterne schimmern", "tokens": ["Und", "siehst", "du", "nicht", "der", "Hoff\u00b7nung", "Ster\u00b7ne", "schim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am Wolkenhimmel fern herauf?", "tokens": ["Am", "Wol\u00b7ken\u00b7him\u00b7mel", "fern", "her\u00b7auf", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entschlossen baue, wenn auch \u00fcber Tr\u00fcmmern,", "tokens": ["Ent\u00b7schlos\u00b7sen", "bau\u00b7e", ",", "wenn", "auch", "\u00fc\u00b7ber", "Tr\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Liebe goldnen Tempel auf!", "tokens": ["Der", "Lie\u00b7be", "gold\u00b7nen", "Tem\u00b7pel", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}