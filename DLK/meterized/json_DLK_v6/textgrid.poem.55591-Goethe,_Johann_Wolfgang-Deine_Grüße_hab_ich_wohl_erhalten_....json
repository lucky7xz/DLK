{"textgrid.poem.55591": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Deine Gr\u00fc\u00dfe hab ich wohl erhalten ...", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deine Gr\u00fc\u00dfe hab ich wohl erhalten.", "tokens": ["Dei\u00b7ne", "Gr\u00fc\u00b7\u00dfe", "hab", "ich", "wohl", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Liebe lebt jetzt in tausend Gestalten,", "tokens": ["Lie\u00b7be", "lebt", "jetzt", "in", "tau\u00b7send", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "CARD", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Gibt der Blume Farb und Duft,", "tokens": ["Gibt", "der", "Blu\u00b7me", "Farb", "und", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeden Morgen durchzieht sie die Luft,", "tokens": ["Je\u00b7den", "Mor\u00b7gen", "durch\u00b7zieht", "sie", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Tag und Nacht spielt sie auf Wiesen, in Hainen", "tokens": ["Tag", "und", "Nacht", "spielt", "sie", "auf", "Wie\u00b7sen", ",", "in", "Hai\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "APPR", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Mir will sie oft zu herrlich erscheinen;", "tokens": ["Mir", "will", "sie", "oft", "zu", "herr\u00b7lich", "er\u00b7schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKA", "ADJD", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Neues bringt sie t\u00e4glich hervor,", "tokens": ["Neu\u00b7es", "bringt", "sie", "t\u00e4g\u00b7lich", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Leben summt uns die Biene ins Ohr.", "tokens": ["Le\u00b7ben", "summt", "uns", "die", "Bie\u00b7ne", "ins", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Bleib, ruf ich oft, Fr\u00fchling! man k\u00fcsset dich kaum,", "tokens": ["Bleib", ",", "ruf", "ich", "oft", ",", "Fr\u00fch\u00b7ling", "!", "man", "k\u00fcs\u00b7set", "dich", "kaum", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "$,", "NN", "$.", "PIS", "VVFIN", "PPER", "ADV", "$,"], "meter": "++--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Engel, so fliehst du wie ein schwankender Traum;", "tokens": ["En\u00b7gel", ",", "so", "fliehst", "du", "wie", "ein", "schwan\u00b7ken\u00b7der", "Traum", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.11": {"text": "Immer wollen wir dich ehren und sch\u00e4tzen,", "tokens": ["Im\u00b7mer", "wol\u00b7len", "wir", "dich", "eh\u00b7ren", "und", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "So uns an dir wie am Himmel erg\u00f6tzen.", "tokens": ["So", "uns", "an", "dir", "wie", "am", "Him\u00b7mel", "er\u00b7g\u00f6t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "PPER", "KOKOM", "APPRART", "NN", "VVINF", "$."], "meter": "----+-+--+-", "measure": "iambic.tri.relaxed"}}}}}