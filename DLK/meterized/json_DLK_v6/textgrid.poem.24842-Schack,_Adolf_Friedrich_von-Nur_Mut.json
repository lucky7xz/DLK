{"textgrid.poem.24842": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Nur Mut", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df das Zagen! Trage mutig", "tokens": ["La\u00df", "das", "Za\u00b7gen", "!", "Tra\u00b7ge", "mu\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "ART", "NN", "$.", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Sorgen, deine Qual!", "tokens": ["Dei\u00b7ne", "Sor\u00b7gen", ",", "dei\u00b7ne", "Qual", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei die Wunde noch so blutig,", "tokens": ["Sei", "die", "Wun\u00b7de", "noch", "so", "blu\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heilen wird sie doch einmal.", "tokens": ["Hei\u00b7len", "wird", "sie", "doch", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Unter tiefer Eisesdecke", "tokens": ["Un\u00b7ter", "tie\u00b7fer", "Ei\u00b7ses\u00b7de\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4umt die junge Knospe schon,", "tokens": ["Tr\u00e4umt", "die", "jun\u00b7ge", "Knos\u00b7pe", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der Fr\u00fchling sie erwecke", "tokens": ["Da\u00df", "der", "Fr\u00fch\u00b7ling", "sie", "er\u00b7we\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der Lieder holdem Ton.", "tokens": ["Mit", "der", "Lie\u00b7der", "hol\u00b7dem", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur empor den Blick gewendet,", "tokens": ["Nur", "em\u00b7por", "den", "Blick", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und durch d\u00fcstres Wolkengrau", "tokens": ["Und", "durch", "d\u00fcst\u00b7res", "Wol\u00b7ken\u00b7grau"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bricht zuletzt, da\u00df es dich blendet,", "tokens": ["Bricht", "zu\u00b7letzt", ",", "da\u00df", "es", "dich", "blen\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glorreich noch des Himmels Blau!", "tokens": ["Glor\u00b7reich", "noch", "des", "Him\u00b7mels", "Blau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber auch die tr\u00fcben Stunden", "tokens": ["A\u00b7ber", "auch", "die", "tr\u00fc\u00b7ben", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Thr\u00e4nen, die du weinst,", "tokens": ["Und", "die", "Thr\u00e4\u00b7nen", ",", "die", "du", "weinst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Glaub, wie Freuden, die entschwunden,", "tokens": ["Glaub", ",", "wie", "Freu\u00b7den", ",", "die", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00df erscheinen sie dir einst.", "tokens": ["S\u00fc\u00df", "er\u00b7schei\u00b7nen", "sie", "dir", "einst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und mit Wehmut, halb nur heiter,", "tokens": ["Und", "mit", "Weh\u00b7mut", ",", "halb", "nur", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "ADJD", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scheidest du f\u00fcr immerdar", "tokens": ["Schei\u00b7dest", "du", "f\u00fcr", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von dem Leiden, dem Begleiter,", "tokens": ["Von", "dem", "Lei\u00b7den", ",", "dem", "Be\u00b7glei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der so lange treu dir war.", "tokens": ["Der", "so", "lan\u00b7ge", "treu", "dir", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}