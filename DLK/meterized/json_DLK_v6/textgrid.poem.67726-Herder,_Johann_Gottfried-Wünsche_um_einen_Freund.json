{"textgrid.poem.67726": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "W\u00fcnsche um einen Freund", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um mich ist W\u00fcst' \u2013 und wo mein Freund?", "tokens": ["Um", "mich", "ist", "W\u00fcst'", "\u2013", "und", "wo", "mein", "Freund", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VAFIN", "NN", "$(", "KON", "PWAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herrscht meine Kainsstirn ihn weit zur\u00fcck?", "tokens": ["Herrscht", "mei\u00b7ne", "Kains\u00b7stirn", "ihn", "weit", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bin ich f\u00fcr ihn blos Mannthier noch?", "tokens": ["Bin", "ich", "f\u00fcr", "ihn", "blos", "Mannt\u00b7hier", "noch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "ADV", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie? oder ist kein Freund?", "tokens": ["Wie", "?", "o\u00b7der", "ist", "kein", "Freund", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KON", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sprich, Freundin Muse! \u2013 Doch bin ich", "tokens": ["Sprich", ",", "Freun\u00b7din", "Mu\u00b7se", "!", "\u2013", "Doch", "bin", "ich"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "NN", "NN", "$.", "$(", "KON", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir nicht selbst Muse? \u2013 Bin ich auch mein Freund? \u2013", "tokens": ["Mir", "nicht", "selbst", "Mu\u00b7se", "?", "\u2013", "Bin", "ich", "auch", "mein", "Freund", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "NN", "$.", "$(", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sprich schwarze Funken, Seelenbrand!", "tokens": ["Sprich", "schwar\u00b7ze", "Fun\u00b7ken", ",", "See\u00b7len\u00b7brand", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nie, nie bin ich mein Freund!", "tokens": ["Nie", ",", "nie", "bin", "ich", "mein", "Freund", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bleib' ich mir treu? \u2013 Nein! Schwur wird Scherz!", "tokens": ["Bleib'", "ich", "mir", "treu", "?", "\u2013", "Nein", "!", "Schwur", "wird", "Scherz", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "$.", "$(", "PTKANT", "$.", "VVFIN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Bollwerk spreng' ich selbst und k\u00fcss' den Feind;", "tokens": ["Mein", "Boll\u00b7werk", "spreng'", "ich", "selbst", "und", "k\u00fcss'", "den", "Feind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch meine Thr\u00e4ne r\u00fchrt mich nicht!", "tokens": ["Auch", "mei\u00b7ne", "Thr\u00e4\u00b7ne", "r\u00fchrt", "mich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht Freund! ich bin mein Feind!", "tokens": ["Nicht", "Freund", "!", "ich", "bin", "mein", "Feind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dort schwebt mein Geist, von mir ermord't \u2013", "tokens": ["Dort", "schwebt", "mein", "Geist", ",", "von", "mir", "er\u00b7mord't", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vertriebne Zeit \u2013 hier modert Jugendblut.", "tokens": ["Ver\u00b7trieb\u00b7ne", "Zeit", "\u2013", "hier", "mo\u00b7dert", "Ju\u00b7gend\u00b7blut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADV", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch modr' ich kr\u00e4chzend selbst im Staub", "tokens": ["Noch", "modr'", "ich", "kr\u00e4ch\u00b7zend", "selbst", "im", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lieg' und hab' mich nicht!", "tokens": ["Und", "lieg'", "und", "hab'", "mich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Gieb, Pluto, einen Schatten mir,", "tokens": ["Gieb", ",", "Plu\u00b7to", ",", "ei\u00b7nen", "Schat\u00b7ten", "mir", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NE", "$,", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der, blos f\u00fcr mich geborn im todten Bild,", "tokens": ["Der", ",", "blos", "f\u00fcr", "mich", "ge\u00b7born", "im", "tod\u00b7ten", "Bild", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "APPR", "PPER", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mich weck' und lehr' und f\u00fcr den Freund", "tokens": ["Mich", "weck'", "und", "lehr'", "und", "f\u00fcr", "den", "Freund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}, "stanza.6": {"line.1": {"text": "Der jetzt vielleicht in Felsennacht", "tokens": ["Der", "jetzt", "viel\u00b7leicht", "in", "Fel\u00b7sen\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Blut und Armuth k\u00e4mpft, der Pallas' Pan-", "tokens": ["Mit", "Blut", "und", "Ar\u00b7muth", "k\u00e4mpft", ",", "der", "Pal\u00b7las'", "Pan"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "zer k\u00fc\u00dft, mich m\u00e4nnlich einst umarmt", "tokens": ["zer", "k\u00fc\u00dft", ",", "mich", "m\u00e4nn\u00b7lich", "einst", "um\u00b7armt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "VVFIN", "$,", "PPER", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und vor mir, Pallas nach,", "tokens": ["Und", "vor", "mir", ",", "Pal\u00b7las", "nach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Den kalten H\u00e4mus bald steil auf,", "tokens": ["Den", "kal\u00b7ten", "H\u00e4\u00b7mus", "bald", "steil", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bald Erymanthus' schwarzen Wald durchjagt", "tokens": ["Bald", "E\u00b7ry\u00b7man\u00b7thus'", "schwar\u00b7zen", "Wald", "durch\u00b7jagt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Und hirschgekr\u00f6nt, wie Hercul, rauh", "tokens": ["Und", "hirschge\u00b7kr\u00f6nt", ",", "wie", "Her\u00b7cul", ",", "rauh"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "NE", "$,", "ADJD"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Zum Himmel dringt und herrscht.", "tokens": ["Zum", "Him\u00b7mel", "dringt", "und", "herrscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}