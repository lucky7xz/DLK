{"textgrid.poem.55394": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "16", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Vatikan bedient man sich", "tokens": ["Im", "Va\u00b7ti\u00b7kan", "be\u00b7dient", "man", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Palmsonntags echter Palmen,", "tokens": ["Palm\u00b7sonn\u00b7tags", "ech\u00b7ter", "Pal\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Kardin\u00e4le beugen sich", "tokens": ["Die", "Kar\u00b7di\u00b7n\u00e4\u00b7le", "beu\u00b7gen", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und singen alte Psalmen.", "tokens": ["Und", "sin\u00b7gen", "al\u00b7te", "Psal\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dieselben Psalmen singt man auch,", "tokens": ["Die\u00b7sel\u00b7ben", "Psal\u00b7men", "singt", "man", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00d6lzweiglein in den H\u00e4nden,", "tokens": ["\u00d6l\u00b7zwei\u00b7glein", "in", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mu\u00df im Gebirg zu diesem Brauch", "tokens": ["Mu\u00df", "im", "Ge\u00b7birg", "zu", "die\u00b7sem", "Brauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPRART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Stechpalmen gar verwenden;", "tokens": ["Stech\u00b7pal\u00b7men", "gar", "ver\u00b7wen\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Zuletzt, man will ein gr\u00fcnes Reis,", "tokens": ["Zu\u00b7letzt", ",", "man", "will", "ein", "gr\u00fc\u00b7nes", "Reis", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PIS", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So nimmt man Weidenzweige,", "tokens": ["So", "nimmt", "man", "Wei\u00b7den\u00b7zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Damit der Fromme Lob und Preis", "tokens": ["Da\u00b7mit", "der", "From\u00b7me", "Lob", "und", "Preis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Auch im Geringsten zeige.", "tokens": ["Auch", "im", "Ge\u00b7rings\u00b7ten", "zei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und habt ihr euch das wohl gemerkt,", "tokens": ["Und", "habt", "ihr", "euch", "das", "wohl", "ge\u00b7merkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "PDS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "G\u00f6nnt man euch das Bequeme,", "tokens": ["G\u00f6nnt", "man", "euch", "das", "Be\u00b7que\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.15": {"text": "Wenn ihr im Glauben euch best\u00e4rkt;", "tokens": ["Wenn", "ihr", "im", "Glau\u00b7ben", "euch", "be\u00b7st\u00e4rkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Das sind Mythologeme.", "tokens": ["Das", "sind", "My\u00b7tho\u00b7lo\u00b7ge\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}