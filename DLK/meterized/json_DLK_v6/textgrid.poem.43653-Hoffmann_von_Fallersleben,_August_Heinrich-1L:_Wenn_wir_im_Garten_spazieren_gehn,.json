{"textgrid.poem.43653": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn wir im Garten spazieren gehn,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir im Garten spazieren gehn,", "tokens": ["Wenn", "wir", "im", "Gar\u00b7ten", "spa\u00b7zie\u00b7ren", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da gibt's zu h\u00f6ren gar viel und zu sehn,", "tokens": ["Da", "gibt's", "zu", "h\u00f6\u00b7ren", "gar", "viel", "und", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKZU", "VVINF", "ADV", "ADV", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da h\u00f6ren wir die Hummeln brummen", "tokens": ["Da", "h\u00f6\u00b7ren", "wir", "die", "Hum\u00b7meln", "brum\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00f6ren die lieben Bienen summen,", "tokens": ["Und", "h\u00f6\u00b7ren", "die", "lie\u00b7ben", "Bie\u00b7nen", "sum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und sehen, wie sie eilig wandern", "tokens": ["Und", "se\u00b7hen", ",", "wie", "sie", "ei\u00b7lig", "wan\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$,", "PWAV", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von einer Blume zu der andern,", "tokens": ["Von", "ei\u00b7ner", "Blu\u00b7me", "zu", "der", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wie sie sich holen den sch\u00f6nsten Raub,", "tokens": ["Und", "wie", "sie", "sich", "ho\u00b7len", "den", "sch\u00f6ns\u00b7ten", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Den Bl\u00fcthenwein und Blumenstaub.", "tokens": ["Den", "Bl\u00fc\u00b7then\u00b7wein", "und", "Blu\u00b7men\u00b7staub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Gott m\u00f6g' euch flei\u00dfigen Bienen verleihn", "tokens": ["Gott", "m\u00f6g'", "euch", "flei\u00b7\u00dfi\u00b7gen", "Bie\u00b7nen", "ver\u00b7leihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADJA", "NN", "VVINF"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.10": {"text": "Stets milde Luft und Sonnenschein", "tokens": ["Stets", "mil\u00b7de", "Luft", "und", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und Bl\u00fcmelein von den sch\u00f6nsten Arten", "tokens": ["Und", "Bl\u00fc\u00b7me\u00b7lein", "von", "den", "sch\u00f6ns\u00b7ten", "Ar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Auf allen euren Wanderfahrten!", "tokens": ["Auf", "al\u00b7len", "eu\u00b7ren", "Wan\u00b7der\u00b7fahr\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}