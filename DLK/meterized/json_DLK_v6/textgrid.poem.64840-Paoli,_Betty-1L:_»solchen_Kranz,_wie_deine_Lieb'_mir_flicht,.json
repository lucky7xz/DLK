{"textgrid.poem.64840": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbsolchen Kranz, wie deine Lieb' mir flicht,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbsolchen Kranz, wie deine Lieb' mir flicht,", "tokens": ["\u00bb", "sol\u00b7chen", "Kranz", ",", "wie", "dei\u00b7ne", "Lieb'", "mir", "flicht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "$,", "PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Glaube mir, ach, ich verdien' ihn nicht;", "tokens": ["Glau\u00b7be", "mir", ",", "ach", ",", "ich", "ver\u00b7dien'", "ihn", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ITJ", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "L\u00e4\u00dft mich doch dein flammend Lied erscheinen", "tokens": ["L\u00e4\u00dft", "mich", "doch", "dein", "flam\u00b7mend", "Lied", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "ADJD", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Als der leuchtend Ueberird'schen Einen,", "tokens": ["Als", "der", "leuch\u00b7tend", "Ue\u00b7ber\u00b7ird'\u00b7schen", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "ART", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Der als G\u00f6tterbild in's Leben ragt,", "tokens": ["Der", "als", "G\u00f6t\u00b7ter\u00b7bild", "in's", "Le\u00b7ben", "ragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Den man betend nur zu lieben wagt.", "tokens": ["Den", "man", "be\u00b7tend", "nur", "zu", "lie\u00b7ben", "wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "O ich wei\u00df, da\u00df deines Lobes Worte", "tokens": ["O", "ich", "wei\u00df", ",", "da\u00df", "dei\u00b7nes", "Lo\u00b7bes", "Wor\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wandeln aus wahrhaft'ger Seele Pforte,", "tokens": ["Wan\u00b7deln", "aus", "wahr\u00b7haft'\u00b7ger", "See\u00b7le", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Da\u00df, was Deine Dichterlippe singt,", "tokens": ["Da\u00df", ",", "was", "Dei\u00b7ne", "Dich\u00b7ter\u00b7lip\u00b7pe", "singt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Wahr und wirklich dein Gem\u00fcth durchdringt:", "tokens": ["Wahr", "und", "wirk\u00b7lich", "dein", "Ge\u00b7m\u00fcth", "durch\u00b7dringt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Aber Sorge mu\u00df mich bang beschleichen,", "tokens": ["A\u00b7ber", "Sor\u00b7ge", "mu\u00df", "mich", "bang", "be\u00b7schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Da\u00df die T\u00e4uschung endlich wird entweichen,", "tokens": ["Da\u00df", "die", "T\u00e4u\u00b7schung", "end\u00b7lich", "wird", "ent\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Da\u00df, wenn rauher Wirklichkeiten Strahl", "tokens": ["Da\u00df", ",", "wenn", "rau\u00b7her", "Wirk\u00b7lich\u00b7kei\u00b7ten", "Strahl"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "Einst entf\u00e4rbt dein tr\u00e4um'risch Ideal,", "tokens": ["Einst", "ent\u00b7f\u00e4rbt", "dein", "tr\u00e4um'\u00b7risch", "I\u00b7deal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Der Erkenntni\u00dfschmerz dich wird durchbeben:", "tokens": ["Der", "Er\u00b7kennt\u00b7ni\u00df\u00b7schmerz", "dich", "wird", "durch\u00b7be\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Rufen wirst du dann, von Qual durchfleischt:", "tokens": ["Ru\u00b7fen", "wirst", "du", "dann", ",", "von", "Qual", "durch\u00b7fleischt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Bitter hab' ich ihn und mich get\u00e4uscht!", "tokens": ["Bit\u00b7ter", "hab'", "ich", "ihn", "und", "mich", "ge\u00b7t\u00e4uscht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "KON", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Und der Mensch wird dir nicht mehr gen\u00fcgen,", "tokens": ["Und", "der", "Mensch", "wird", "dir", "nicht", "mehr", "ge\u00b7n\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "--+--++-+-", "measure": "anapaest.di.plus"}, "line.19": {"text": "Den im Traum du sahst mit Engelz\u00fcgen!\u00ab \u2013", "tokens": ["Den", "im", "Traum", "du", "sahst", "mit", "En\u00b7gel\u00b7z\u00fc\u00b7gen", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "VVFIN", "APPR", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "\u00bbnein, mein Lieb! kein leeres Traumgebild", "tokens": ["\u00bb", "nein", ",", "mein", "Lieb", "!", "kein", "lee\u00b7res", "Traum\u00b7ge\u00b7bild"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PTKANT", "$,", "PPOSAT", "NN", "$.", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Ist's, dem meines Herzens Jubel gilt.", "tokens": ["Ist's", ",", "dem", "mei\u00b7nes", "Her\u00b7zens", "Ju\u00b7bel", "gilt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.22": {"text": "Nein! ich will dich mit phantast'schem Streben", "tokens": ["Nein", "!", "ich", "will", "dich", "mit", "phan\u00b7tast'\u00b7schem", "Stre\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VMFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Zu den Engelch\u00f6ren nicht erheben", "tokens": ["Zu", "den", "En\u00b7gelc\u00b7h\u00f6\u00b7ren", "nicht", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKNEG", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Nicht mit Ideales Sternenband", "tokens": ["Nicht", "mit", "I\u00b7dea\u00b7les", "Ster\u00b7nen\u00b7band"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NE", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.25": {"text": "Kr\u00e4nzen deiner sch\u00f6nen Stirne Rand.", "tokens": ["Kr\u00e4n\u00b7zen", "dei\u00b7ner", "sch\u00f6\u00b7nen", "Stir\u00b7ne", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.26": {"text": "Wenn f\u00fcr dich in tiefer Lieb' ich brenne,", "tokens": ["Wenn", "f\u00fcr", "dich", "in", "tie\u00b7fer", "Lieb'", "ich", "bren\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Ist's, weil ich dein Inn'res ganz erkenne,", "tokens": ["Ist's", ",", "weil", "ich", "dein", "Inn'\u00b7res", "ganz", "er\u00b7ken\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Weil ich treulich folgte jeder Spur", "tokens": ["Weil", "ich", "treu\u00b7lich", "folg\u00b7te", "je\u00b7der", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.29": {"text": "Deiner ", "tokens": ["Dei\u00b7ner"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.30": {"text": "Vieles ist an mir vorbeigegangen,", "tokens": ["Vie\u00b7les", "ist", "an", "mir", "vor\u00b7bei\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.31": {"text": "Sieh's an meinen marmorbleichen Wangen,", "tokens": ["Sieh's", "an", "mei\u00b7nen", "mar\u00b7mor\u00b7blei\u00b7chen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "An des Auges thr\u00e4nenfeuchtem Strahl,", "tokens": ["An", "des", "Au\u00b7ges", "thr\u00e4\u00b7nen\u00b7feuch\u00b7tem", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.33": {"text": "H\u00f6r's an meiner Stimme Seufzerhall.", "tokens": ["H\u00f6r's", "an", "mei\u00b7ner", "Stim\u00b7me", "Seuf\u00b7zer\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.34": {"text": "L\u00e4ngst verraut mit dieses Lebens M\u00fch", "tokens": ["L\u00e4ngst", "ver\u00b7raut", "mit", "die\u00b7ses", "Le\u00b7bens", "M\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PDAT", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.35": {"text": "Lebe ich kein Sein der Phantasie;", "tokens": ["Le\u00b7be", "ich", "kein", "Sein", "der", "Phan\u00b7ta\u00b7sie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.36": {"text": "Darum darfst du meinen Worten trauen,", "tokens": ["Da\u00b7rum", "darfst", "du", "mei\u00b7nen", "Wor\u00b7ten", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.37": {"text": "Wenn mit Augen, die vor R\u00fchrung thauen,", "tokens": ["Wenn", "mit", "Au\u00b7gen", ",", "die", "vor", "R\u00fch\u00b7rung", "thau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.38": {"text": "Dir mein Lied nun innig offenbart,", "tokens": ["Dir", "mein", "Lied", "nun", "in\u00b7nig", "of\u00b7fen\u00b7bart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.39": {"text": "Da\u00df dir darum meine Liebe ward,", "tokens": ["Da\u00df", "dir", "da\u00b7rum", "mei\u00b7ne", "Lie\u00b7be", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.40": {"text": "Weil ich nimmer fand in Lebens Reichen", "tokens": ["Weil", "ich", "nim\u00b7mer", "fand", "in", "Le\u00b7bens", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPR", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.41": {"text": "Einen Menschen, der dir zu vergleichen,", "tokens": ["Ei\u00b7nen", "Men\u00b7schen", ",", "der", "dir", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.42": {"text": "Dessen Herz so rein und unentweiht", "tokens": ["Des\u00b7sen", "Herz", "so", "rein", "und", "un\u00b7ent\u00b7weiht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.43": {"text": "Und so stark blieb in dem Sturm der Zeit;", "tokens": ["Und", "so", "stark", "blieb", "in", "dem", "Sturm", "der", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.44": {"text": "Dessen Geist, so k\u00fchn und stolz im Wagen,", "tokens": ["Des\u00b7sen", "Geist", ",", "so", "k\u00fchn", "und", "stolz", "im", "Wa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.45": {"text": "So verstand gebrochner Seele Klagen;", "tokens": ["So", "ver\u00b7stand", "ge\u00b7broch\u00b7ner", "See\u00b7le", "Kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.46": {"text": "Dessen Huld, ein mild verkl\u00e4rend Licht,", "tokens": ["Des\u00b7sen", "Huld", ",", "ein", "mild", "ver\u00b7kl\u00e4\u00b7rend", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ART", "ADJD", "VVPP", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.47": {"text": "Den, der sie gewonnen, heilig spricht.", "tokens": ["Den", ",", "der", "sie", "ge\u00b7won\u00b7nen", ",", "hei\u00b7lig", "spricht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "VVPP", "$,", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.48": {"text": "K\u00f6nnt' ich nun im Liede Sch\u00f6n'res zeigen,", "tokens": ["K\u00f6nnt'", "ich", "nun", "im", "Lie\u00b7de", "Sch\u00f6n'\u00b7res", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.49": {"text": "Als dir wahr und wirklich ist zu eigen?", "tokens": ["Als", "dir", "wahr", "und", "wirk\u00b7lich", "ist", "zu", "ei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "VAFIN", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.50": {"text": "M\u00f6cht' ich schm\u00fccken dich mit fremdem Schein,", "tokens": ["M\u00f6cht'", "ich", "schm\u00fc\u00b7cken", "dich", "mit", "frem\u00b7dem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.51": {"text": "Da so edle Sonnenstrahlen dein?", "tokens": ["Da", "so", "ed\u00b7le", "Son\u00b7nen\u00b7strah\u00b7len", "dein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "PPOSAT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.52": {"text": "O was k\u00f6nnte Phantasie erfinden,", "tokens": ["O", "was", "k\u00f6nn\u00b7te", "Phan\u00b7ta\u00b7sie", "er\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.53": {"text": "Das nicht herrlicher in Dir zu finden?!", "tokens": ["Das", "nicht", "herr\u00b7li\u00b7cher", "in", "Dir", "zu", "fin\u00b7den", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.54": {"text": "La\u00df mich drum, mein Freund, zu jeder Frist", "tokens": ["La\u00df", "mich", "drum", ",", "mein", "Freund", ",", "zu", "je\u00b7der", "Frist"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PAV", "$,", "PPOSAT", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.55": {"text": "Dich nur treulich schildern, wie du bist!", "tokens": ["Dich", "nur", "treu\u00b7lich", "schil\u00b7dern", ",", "wie", "du", "bist", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVINF", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.56": {"text": "Wozu w\u00e4r' mir auch Gesang gegeben,", "tokens": ["Wo\u00b7zu", "w\u00e4r'", "mir", "auch", "Ge\u00b7sang", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.57": {"text": "Wenn f\u00fcr dich nicht, du mein liebes Leben?\u00ab", "tokens": ["Wenn", "f\u00fcr", "dich", "nicht", ",", "du", "mein", "lie\u00b7bes", "Le\u00b7ben", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "APPR", "PPER", "PTKNEG", "$,", "PPER", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}