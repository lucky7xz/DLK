{"dta.poem.18565": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A n  S elimenen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Kommt ihr wundersch\u00f6nen Blicke/", "tokens": ["Kommt", "ihr", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt und f\u00e4sselt meinen Geist", "tokens": ["Kommt", "und", "f\u00e4s\u00b7selt", "mei\u00b7nen", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch gelinde Seelen Stricke", "tokens": ["Durch", "ge\u00b7lin\u00b7de", "See\u00b7len", "Stri\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die gar keine Macht zerreist/", "tokens": ["Die", "gar", "kei\u00b7ne", "Macht", "zer\u00b7reist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil der Strahl/ so mich betroffen/", "tokens": ["Weil", "der", "Strahl", "/", "so", "mich", "be\u00b7trof\u00b7fen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$(", "ADV", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Endlich l\u00e4st Genade hoffen.", "tokens": ["End\u00b7lich", "l\u00e4st", "Ge\u00b7na\u00b7de", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lieg' ich gleich in Band und Eisen/", "tokens": ["Lieg'", "ich", "gleich", "in", "Band", "und", "Ei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist die Freyheit v\u00f6llig hin/", "tokens": ["Ist", "die", "Frey\u00b7heit", "v\u00f6l\u00b7lig", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll dennoch die That erwiesen/", "tokens": ["Soll", "den\u00b7noch", "die", "That", "er\u00b7wie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich h\u00f6chst vergn\u00fcget bin:", "tokens": ["Da\u00df", "ich", "h\u00f6chst", "ver\u00b7gn\u00fc\u00b7get", "bin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil einst von den schweren Ketten/", "tokens": ["Weil", "einst", "von", "den", "schwe\u00b7ren", "Ket\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Mich ein sch\u00f6nes Kind will retten.", "tokens": ["Mich", "ein", "sch\u00f6\u00b7nes", "Kind", "will", "ret\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was vor ungemeine Schmertzen/", "tokens": ["Was", "vor", "un\u00b7ge\u00b7mei\u00b7ne", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Waren mir nicht vorbewust!", "tokens": ["Wa\u00b7ren", "mir", "nicht", "vor\u00b7be\u00b7wust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als die angebrandten Kertzen/", "tokens": ["Als", "die", "an\u00b7ge\u00b7brand\u00b7ten", "Kert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich verbargen in der Brust.", "tokens": ["Sich", "ver\u00b7bar\u00b7gen", "in", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn es darff bey scharffen Zeugen/", "tokens": ["Denn", "es", "darff", "bey", "scharf\u00b7fen", "Zeu\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Keine Glut ins Auge steigen.", "tokens": ["Kei\u00b7ne", "Glut", "ins", "Au\u00b7ge", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch der Neid mu\u00df nun erliegen/", "tokens": ["Doch", "der", "Neid", "mu\u00df", "nun", "er\u00b7lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Sch\u00e4lsucht wird verlacht:", "tokens": ["Und", "die", "Sch\u00e4l\u00b7sucht", "wird", "ver\u00b7lacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der Schatz soll mich vergn\u00fcgen/", "tokens": ["Denn", "der", "Schatz", "soll", "mich", "ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den ein Argus sonst bewacht.", "tokens": ["Den", "ein", "Ar\u00b7gus", "sonst", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Noht und Leyden wird vers\u00fcsset/", "tokens": ["Noht", "und", "Ley\u00b7den", "wird", "ver\u00b7s\u00fcs\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Obs die Schwester gleich verdr\u00fcsset.", "tokens": ["Obs", "die", "Schwes\u00b7ter", "gleich", "ver\u00b7dr\u00fcs\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Was Verdru\u00df der mu\u00df verschwinden/", "tokens": ["Was", "Ver\u00b7dru\u00df", "der", "mu\u00df", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ART", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer auff was Galantes denckt.", "tokens": ["Wer", "auff", "was", "Ga\u00b7lan\u00b7tes", "denckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PRELS", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bi\u00df man kan den Hafen finden/", "tokens": ["Bi\u00df", "man", "kan", "den", "Ha\u00b7fen", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der uns tausend Freuden schenckt", "tokens": ["Der", "uns", "tau\u00b7send", "Freu\u00b7den", "schenckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "CARD", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn allhier geliebt zu werden/", "tokens": ["Denn", "all\u00b7hier", "ge\u00b7liebt", "zu", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "PTKZU", "VAINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ist ein Paradie\u00df auff Erden.", "tokens": ["Ist", "ein", "Pa\u00b7ra\u00b7die\u00df", "auff", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "M\u00fcssen manchmal Dornen stechen/", "tokens": ["M\u00fcs\u00b7sen", "manch\u00b7mal", "Dor\u00b7nen", "ste\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ey so l\u00e4sst der Himmel mich", "tokens": ["Ey", "so", "l\u00e4sst", "der", "Him\u00b7mel", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "ART", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch dereinsten Rosen brechen/", "tokens": ["Noch", "de\u00b7reins\u00b7ten", "Ro\u00b7sen", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Spiel ver\u00e4ndert sich", "tokens": ["Und", "das", "Spiel", "ver\u00b7\u00e4n\u00b7dert", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bi\u00df man kan nach tr\u00fcben Zeiten/", "tokens": ["Bi\u00df", "man", "kan", "nach", "tr\u00fc\u00b7ben", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das gelobte Land beschreiten.", "tokens": ["Das", "ge\u00b7lob\u00b7te", "Land", "be\u00b7schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nun so lachen mich die Blicke", "tokens": ["Nun", "so", "la\u00b7chen", "mich", "die", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fcsser Hoffnung ferner an/", "tokens": ["S\u00fcs\u00b7ser", "Hoff\u00b7nung", "fer\u00b7ner", "an", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bi\u00df ich eins mein gantzes Gl\u00fccke/", "tokens": ["Bi\u00df", "ich", "eins", "mein", "gant\u00b7zes", "Gl\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Liebe finden kan:", "tokens": ["In", "der", "Lie\u00b7be", "fin\u00b7den", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn das Gl\u00fccke mu\u00df es f\u00fcgen/", "tokens": ["Denn", "das", "Gl\u00fc\u00b7cke", "mu\u00df", "es", "f\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df die Losung heist Vergn\u00fcgen.", "tokens": ["Da\u00df", "die", "Lo\u00b7sung", "heist", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}