{"dta.poem.2528": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein anderer an einen andern.  \n  JESUS .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Eltern liebstes Pfand/ der Musen Lust und Wonne/", "tokens": ["Der", "El\u00b7tern", "liebs\u00b7tes", "Pfand", "/", "der", "Mu\u00b7sen", "Lust", "und", "Won\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gelehrter Tugend Kern/ mein Modrach/ zieht von", "tokens": ["Ge\u00b7lehr\u00b7ter", "Tu\u00b7gend", "Kern", "/", "mein", "Mod\u00b7rach", "/", "zieht", "von"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "NN", "$(", "PPOSAT", "NN", "$(", "VVFIN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Himmel schein\u2019 ihn an/ mit seiner Gnaden-Sonne/", "tokens": ["Der", "Him\u00b7mel", "schein'", "ihn", "an", "/", "mit", "sei\u00b7ner", "Gna\u00b7den\u00b7Son\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$(", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und stell\u2019 ihm einen May der Liebligkeiten f\u00fcr/", "tokens": ["Und", "stell'", "ihm", "ei\u00b7nen", "May", "der", "Lieb\u00b7lig\u00b7kei\u00b7ten", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er aber/ werther Freund/ woll\u2019 ihn geneigt empfangen/", "tokens": ["Er", "a\u00b7ber", "/", "wert\u00b7her", "Freund", "/", "woll'", "ihn", "ge\u00b7neigt", "emp\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "ADJA", "NN", "$(", "VMFIN", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er wird dem Plei\u00df-Athen zu Ruhm und Ehren bl\u00fchn/", "tokens": ["Er", "wird", "dem", "Plei\u00df\u00b7A\u00b7then", "zu", "Ruhm", "und", "Eh\u00b7ren", "bl\u00fchn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Es brennt sein Hertze schon f\u00fcr eyfrigen Verlangen/", "tokens": ["Es", "brennt", "sein", "Hert\u00b7ze", "schon", "f\u00fcr", "ey\u00b7fri\u00b7gen", "Ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In de\u00df Herrn Buhlen Hau\u00df ein: als ein Gast zu ziehn.", "tokens": ["In", "de\u00df", "Herrn", "Buh\u00b7len", "Hau\u00df", "ein", ":", "als", "ein", "Gast", "zu", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "NN", "PTKVZ", "$.", "KOUS", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mit tausend Tr\u00e4hnen hat er unsre Stadt verlassen/", "tokens": ["Mit", "tau\u00b7send", "Tr\u00e4h\u00b7nen", "hat", "er", "uns\u00b7re", "Stadt", "ver\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die ihm der Freunde Treu au\u00df Pflicht geliefert hat/", "tokens": ["Die", "ihm", "der", "Freun\u00b7de", "Treu", "au\u00df", "Pflicht", "ge\u00b7lie\u00b7fert", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "APPR", "NN", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df so viel Engel itzt ihn auf der Rei\u00df umfassen/", "tokens": ["Da\u00df", "so", "viel", "En\u00b7gel", "itzt", "ihn", "auf", "der", "Rei\u00df", "um\u00b7fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "ADV", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bi\u00df er siht wohl begl\u00fcckt die gr\u00fcne Linden-Stadt.", "tokens": ["Bi\u00df", "er", "siht", "wohl", "be\u00b7gl\u00fcckt", "die", "gr\u00fc\u00b7ne", "Lin\u00b7den\u00b7Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Es setzt das Vater-Land auf ihn ein grosses hoffen/", "tokens": ["Es", "setzt", "das", "Va\u00b7ter\u00b7Land", "auf", "ihn", "ein", "gros\u00b7ses", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df er ins k\u00fcnfftige sol Rath und Sch\u00fctzer seyn/", "tokens": ["Da\u00df", "er", "ins", "k\u00fcnff\u00b7ti\u00b7ge", "sol", "Rath", "und", "Sch\u00fct\u00b7zer", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "PIAT", "NN", "KON", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mich d\u00fcnckt ich sehe schon der Themis Tempel offen/", "tokens": ["Mich", "d\u00fcnckt", "ich", "se\u00b7he", "schon", "der", "The\u00b7mis", "Tem\u00b7pel", "of\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die G\u00f6ttin nimmt ihn auch zu ihrem Kleinod ein.", "tokens": ["Die", "G\u00f6t\u00b7tin", "nimmt", "ihn", "auch", "zu", "ih\u00b7rem", "Klei\u00b7nod", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Er hat manch kluges Buch mit Nutzen auffgeschlagen/", "tokens": ["Er", "hat", "manch", "klu\u00b7ges", "Buch", "mit", "Nut\u00b7zen", "auff\u00b7ge\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der Tugend Morgenr\u00f6th im ", "tokens": ["Der", "Tu\u00b7gend", "Mor\u00b7gen\u00b7r\u00f6th", "im"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPRART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Wie solt\u2019 er mit der Zeit nicht reiffe Fr\u00fcchte tragen/", "tokens": ["Wie", "solt'", "er", "mit", "der", "Zeit", "nicht", "reif\u00b7fe", "Fr\u00fcch\u00b7te", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "ART", "NN", "PTKNEG", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da ihn von Wiegen an der Ph\u00f6bus angelacht.", "tokens": ["Da", "ihn", "von", "Wie\u00b7gen", "an", "der", "Ph\u00f6\u00b7bus", "an\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es wird sich ", "tokens": ["Es", "wird", "sich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Der Clarien ihr Hertz Alberti seyn bem\u00fcht/", "tokens": ["Der", "Cla\u00b7ri\u00b7en", "ihr", "Hertz", "Al\u00b7ber\u00b7ti", "seyn", "be\u00b7m\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PPOSAT", "NN", "NE", "VAINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wie diese Pflantze mog in Helikon gedeyen/", "tokens": ["Wie", "die\u00b7se", "Pflant\u00b7ze", "mog", "in", "He\u00b7li\u00b7kon", "ge\u00b7de\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "VVFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.24": {"text": "Als derer Treffligkeit er schon auffsprossen siht.", "tokens": ["Als", "de\u00b7rer", "Tref\u00b7flig\u00b7keit", "er", "schon", "auffs\u00b7pros\u00b7sen", "siht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "NN", "PPER", "ADV", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Mein Buhle wird gewi\u00df sein Hertz auch lassen blicken/", "tokens": ["Mein", "Buh\u00b7le", "wird", "ge\u00b7wi\u00df", "sein", "Hertz", "auch", "las\u00b7sen", "bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Dieweil er vest und tief in unsern Hertzen steht/", "tokens": ["Die\u00b7weil", "er", "vest", "und", "tief", "in", "un\u00b7sern", "Hert\u00b7zen", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und sein Ged\u00e4chtn\u00fc\u00df kan oft unsern Geist erquicken/", "tokens": ["Und", "sein", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "kan", "oft", "un\u00b7sern", "Geist", "er\u00b7qui\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ich seh\u2019 auch di\u00df im Geist/ wie er zu rathe geht/", "tokens": ["Ich", "seh'", "auch", "di\u00df", "im", "Geist", "/", "wie", "er", "zu", "ra\u00b7the", "geht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDS", "APPRART", "NN", "$(", "PWAV", "PPER", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und alle Freundschaffts-Pflicht ist embsig zuerfullen/", "tokens": ["Und", "al\u00b7le", "Freund\u00b7schaffts\u00b7Pflicht", "ist", "emb\u00b7sig", "zu\u00b7er\u00b7ful\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wer kennt die Redlichkeit de\u00df werthen Buhlen nicht?", "tokens": ["Wer", "kennt", "die", "Red\u00b7lich\u00b7keit", "de\u00df", "wert\u00b7hen", "Buh\u00b7len", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der H\u00f6chste schicke di\u00df nach seinem weisen Willen/", "tokens": ["Der", "H\u00f6chs\u00b7te", "schi\u00b7cke", "di\u00df", "nach", "sei\u00b7nem", "wei\u00b7sen", "Wil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDS", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df auch in Ewigkeit nichts dieses B\u00fcndn\u00fc\u00df bricht.", "tokens": ["Da\u00df", "auch", "in", "E\u00b7wig\u00b7keit", "nichts", "die\u00b7ses", "B\u00fcnd\u00b7n\u00fc\u00df", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "PIS", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich w\u00fcnsche nochmahls Heyl und tausendfaches Gl\u00fccke/", "tokens": ["Ich", "w\u00fcn\u00b7sche", "noch\u00b7mahls", "Heyl", "und", "tau\u00b7send\u00b7fa\u00b7ches", "Gl\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der dreymahl grosse GOtt halt \u00fcber ihn die Hand;", "tokens": ["Der", "drey\u00b7mahl", "gros\u00b7se", "Gott", "halt", "\u00fc\u00b7ber", "ihn", "die", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Mein Siegel ist das Hertz/ da\u00df ich dem Brief aufdr\u00fccke/", "tokens": ["Mein", "Sie\u00b7gel", "ist", "das", "Hertz", "/", "da\u00df", "ich", "dem", "Brief", "auf\u00b7dr\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Er \u00f6ffne itzt sein Hau\u00df und k\u00fcsse dieses Pfand.", "tokens": ["Er", "\u00f6ff\u00b7ne", "itzt", "sein", "Hau\u00df", "und", "k\u00fcs\u00b7se", "die\u00b7ses", "Pfand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "KON", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}