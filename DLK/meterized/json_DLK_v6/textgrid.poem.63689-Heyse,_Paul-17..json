{"textgrid.poem.63689": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lieb' ich dies vertr\u00e4umte Waldesschweigen!", "tokens": ["Wie", "lieb'", "ich", "dies", "ver\u00b7tr\u00e4um\u00b7te", "Wal\u00b7des\u00b7schwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur selten t\u00f6nt der Hall von Menschentritten,", "tokens": ["Nur", "sel\u00b7ten", "t\u00f6nt", "der", "Hall", "von", "Men\u00b7schen\u00b7trit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Sommer ist so sacht vorbeigeglitten,", "tokens": ["Der", "Som\u00b7mer", "ist", "so", "sacht", "vor\u00b7bei\u00b7ge\u00b7glit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "L\u00e4ngst sind verstummt die V\u00f6gel in den Zweigen.", "tokens": ["L\u00e4ngst", "sind", "ver\u00b7stummt", "die", "V\u00f6\u00b7gel", "in", "den", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nur Sonntags kommt von allen Bergessteigen", "tokens": ["Nur", "Sonn\u00b7tags", "kommt", "von", "al\u00b7len", "Ber\u00b7gess\u00b7tei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein lustig Wandervolk dahergeschritten,", "tokens": ["Ein", "lus\u00b7tig", "Wan\u00b7der\u00b7volk", "da\u00b7her\u00b7ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und aus dem Talgrund von den Sieben H\u00fctten", "tokens": ["Und", "aus", "dem", "Tal\u00b7grund", "von", "den", "Sie\u00b7ben", "H\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ART", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erschallt der Juhschrei zum Schuhplattlerreigen.", "tokens": ["Er\u00b7schallt", "der", "Juhsc\u00b7hrei", "zum", "Schuh\u00b7platt\u00b7ler\u00b7rei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch erst im Winter \u2013 welch ein hohes Fest", "tokens": ["Doch", "erst", "im", "Win\u00b7ter", "\u2013", "welch", "ein", "ho\u00b7hes", "Fest"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "$(", "PWAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Einsamkeit und Stille, wenn im Schnee", "tokens": ["Der", "Ein\u00b7sam\u00b7keit", "und", "Stil\u00b7le", ",", "wenn", "im", "Schnee"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Waldung schl\u00e4ft und jedes Tier im Nest!", "tokens": ["Die", "Wal\u00b7dung", "schl\u00e4ft", "und", "je\u00b7des", "Tier", "im", "Nest", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann kommen sacht in Rudeln Hirsch' und Rehe", "tokens": ["Dann", "kom\u00b7men", "sacht", "in", "Ru\u00b7deln", "Hirsch'", "und", "Re\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "VVFIN", "APPR", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Futterplatz, indes sich h\u00f6ren l\u00e4\u00dft", "tokens": ["Zum", "Fut\u00b7ter\u00b7platz", ",", "in\u00b7des", "sich", "h\u00f6\u00b7ren", "l\u00e4\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "ADV", "PRF", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Hungerschrei des Habichts aus der H\u00f6he.", "tokens": ["Der", "Hun\u00b7ger\u00b7schrei", "des", "Ha\u00b7bichts", "aus", "der", "H\u00f6\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}