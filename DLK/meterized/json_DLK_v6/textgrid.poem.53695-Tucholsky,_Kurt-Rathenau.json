{"textgrid.poem.53695": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Rathenau", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist doch schon daran gew\u00f6hnt!", "tokens": ["Du", "bist", "doch", "schon", "da\u00b7ran", "ge\u00b7w\u00f6hnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du wei\u00dft doch, wie das ist, wenn deinen jungen", "tokens": ["Du", "wei\u00dft", "doch", ",", "wie", "das", "ist", ",", "wenn", "dei\u00b7nen", "jun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PDS", "VAFIN", "$,", "KOUS", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Deutschnationalen so ein Ding gelungen.", "tokens": ["Deutschna\u00b7ti\u00b7o\u00b7na\u00b7len", "so", "ein", "Ding", "ge\u00b7lun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Sie schie\u00dfen. Karlchen Helfferich, der h\u00f6hnt.", "tokens": ["Sie", "schie\u00b7\u00dfen", ".", "Karl\u00b7chen", "Helf\u00b7fe\u00b7rich", ",", "der", "h\u00f6hnt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "NE", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das ist seit Jahren deine Politik \u2013", "tokens": ["Das", "ist", "seit", "Jah\u00b7ren", "dei\u00b7ne", "Po\u00b7li\u00b7tik", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du Republik!", "tokens": ["Du", "Re\u00b7pub\u00b7lik", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Du hast doch darin \u00dcbung, junge Frau!", "tokens": ["Du", "hast", "doch", "da\u00b7rin", "\u00dc\u00b7bung", ",", "jun\u00b7ge", "Frau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PAV", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Glatt gehn dir von der Hand die Totenfeiern.", "tokens": ["Glatt", "gehn", "dir", "von", "der", "Hand", "die", "To\u00b7ten\u00b7fei\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Proteste gellen. Nekrologe leiern.", "tokens": ["Pro\u00b7tes\u00b7te", "gel\u00b7len", ".", "Nek\u00b7ro\u00b7lo\u00b7ge", "lei\u00b7ern", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$.", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und hinterher bist du genau so schlau.", "tokens": ["Und", "hin\u00b7ter\u00b7her", "bist", "du", "ge\u00b7nau", "so", "schlau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie lange siehst du Helfferich noch zu?", "tokens": ["Wie", "lan\u00b7ge", "siehst", "du", "Helf\u00b7fe\u00b7rich", "noch", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "NE", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Derselbe, der aus Moskau, als man putschte,", "tokens": ["Der\u00b7sel\u00b7be", ",", "der", "aus", "Mos\u00b7kau", ",", "als", "man", "putschte", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "$,", "PRELS", "APPR", "NE", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "mit vollen Hosen in die Heimat rutschte,", "tokens": ["mit", "vol\u00b7len", "Ho\u00b7sen", "in", "die", "Hei\u00b7mat", "rutschte", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "hat jetzt den zweiten Menschen ungerochen", "tokens": ["hat", "jetzt", "den", "zwei\u00b7ten", "Men\u00b7schen", "un\u00b7ge\u00b7ro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "ins Grab gehetzt, geflucht, gesprochen.", "tokens": ["ins", "Grab", "ge\u00b7hetzt", ",", "ge\u00b7flucht", ",", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und w\u00e4hrend eine alte Mutter bebt,", "tokens": ["Und", "w\u00e4h\u00b7rend", "ei\u00b7ne", "al\u00b7te", "Mut\u00b7ter", "bebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "sitzt das im Parlament.", "tokens": ["sitzt", "das", "im", "Par\u00b7la\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Und lebt.", "tokens": ["Und", "lebt", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Das war doch nicht das erste Mal!", "tokens": ["Das", "war", "doch", "nicht", "das", "ers\u00b7te", "Mal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du h\u00f6rst die Bonzen der Partein", "tokens": ["Du", "h\u00f6rst", "die", "Bon\u00b7zen", "der", "Par\u00b7tein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "im Reichstag und im Landtag schrein:", "tokens": ["im", "Reichs\u00b7tag", "und", "im", "Land\u00b7tag", "schrein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbso geht das nicht mehr weiter! Ein Skandal!\u00ab", "tokens": ["\u00bb", "so", "geht", "das", "nicht", "mehr", "wei\u00b7ter", "!", "Ein", "Skan\u00b7dal", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PDS", "PTKNEG", "ADV", "PTKVZ", "$.", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "War es das letzte Mal?", "tokens": ["War", "es", "das", "letz\u00b7te", "Mal", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Steh einmal auf! Schlag mit der Faust darein!", "tokens": ["Steh", "ein\u00b7mal", "auf", "!", "Schlag", "mit", "der", "Faust", "da\u00b7rein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$.", "NN", "APPR", "ART", "NN", "PAV", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Schlaf nicht nach vierzehn Tagen wieder ein!", "tokens": ["Schlaf", "nicht", "nach", "vier\u00b7zehn", "Ta\u00b7gen", "wie\u00b7der", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "CARD", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Heraus mit deinem Monarchistenrichter,", "tokens": ["He\u00b7raus", "mit", "dei\u00b7nem", "Mon\u00b7ar\u00b7chis\u00b7ten\u00b7rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "mit Offizieren \u2013 und mit dem Gelichter,", "tokens": ["mit", "Of\u00b7fi\u00b7zie\u00b7ren", "\u2013", "und", "mit", "dem", "Ge\u00b7lich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "das von dir lebt, und das dich sabotiert,", "tokens": ["das", "von", "dir", "lebt", ",", "und", "das", "dich", "sa\u00b7bo\u00b7tiert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "VVFIN", "$,", "KON", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "an deine H\u00e4user Hakenkreuze schmiert.", "tokens": ["an", "dei\u00b7ne", "H\u00e4u\u00b7ser", "Ha\u00b7ken\u00b7kreu\u00b7ze", "schmiert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Schlag du in St\u00fccke die Geheimverb\u00e4nde!", "tokens": ["Schlag", "du", "in", "St\u00fc\u00b7cke", "die", "Ge\u00b7heim\u00b7ver\u00b7b\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Bind Ludendorff und Escherich die H\u00e4nde!", "tokens": ["Bind", "Lu\u00b7den\u00b7dorff", "und", "E\u00b7sche\u00b7rich", "die", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "La\u00df dich nicht von der Reichswehr h\u00f6hnen!", "tokens": ["La\u00df", "dich", "nicht", "von", "der", "Reichs\u00b7wehr", "h\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sie mu\u00df sich an die Republik gew\u00f6hnen.", "tokens": ["Sie", "mu\u00df", "sich", "an", "die", "Re\u00b7pub\u00b7lik", "ge\u00b7w\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Schlag zu! Schlag zu! Pack sie geh\u00f6rig an!", "tokens": ["Schlag", "zu", "!", "Schlag", "zu", "!", "Pack", "sie", "ge\u00b7h\u00f6\u00b7rig", "an", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "NN", "PTKVZ", "$.", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Sie kneifen alle. Denn da ist kein Mann.", "tokens": ["Sie", "knei\u00b7fen", "al\u00b7le", ".", "Denn", "da", "ist", "kein", "Mann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$.", "KON", "ADV", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Da sind nur Heckensch\u00fctzen. Pack sie fest \u2013", "tokens": ["Da", "sind", "nur", "He\u00b7cken\u00b7sch\u00fct\u00b7zen", ".", "Pack", "sie", "fest", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "$.", "NN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "dein Haus verbrennt, wenn dus jetzt glimmen l\u00e4\u00dft.", "tokens": ["dein", "Haus", "ver\u00b7brennt", ",", "wenn", "dus", "jetzt", "glim\u00b7men", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PIS", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Zerrei\u00df die Paragraphenschlingen.", "tokens": ["Zer\u00b7rei\u00df", "die", "Pa\u00b7ra\u00b7gra\u00b7phen\u00b7schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Fall nicht darein. Es mu\u00df gelingen!", "tokens": ["Fall", "nicht", "da\u00b7rein", ".", "Es", "mu\u00df", "ge\u00b7lin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PAV", "$.", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Vier Jahre Mord \u2013 das sind, wei\u00df Gott, genug.", "tokens": ["Vier", "Jah\u00b7re", "Mord", "\u2013", "das", "sind", ",", "wei\u00df", "Gott", ",", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["CARD", "NN", "NN", "$(", "PDS", "VAFIN", "$,", "VVFIN", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Du stehst vor deinem letzten Atemzug.", "tokens": ["Du", "stehst", "vor", "dei\u00b7nem", "letz\u00b7ten", "A\u00b7tem\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Zeig, was du bist. Halt mit dir selbst Gericht.", "tokens": ["Zeig", ",", "was", "du", "bist", ".", "Halt", "mit", "dir", "selbst", "Ge\u00b7richt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "VAFIN", "$.", "NN", "APPR", "PPER", "ADV", "NN", "$."], "meter": "----+--+-+", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Stirb oder k\u00e4mpfe!", "tokens": ["Stirb", "o\u00b7der", "k\u00e4mp\u00b7fe", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "KON", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "Drittes gibt es nicht.", "tokens": ["Drit\u00b7tes", "gibt", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}