{"dta.poem.1013": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Der abend schw&#252;l der morgen fahl und n&#252;chtern               ", "tokens": ["Der", "a\u00b7bend", "schw", "&#252;", "l", "der", "mor\u00b7gen", "fahl", "und", "n", "&#252;", "ch\u00b7tern"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["ART", "ADV", "ADJD", "$(", "NN", "ART", "ADJA", "NN", "KON", "XY", "XY", "XY"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sind einziger wechsel ihrer tr&#252;ben reise               ", "tokens": ["Sind", "ein\u00b7zi\u00b7ger", "wech\u00b7sel", "ih\u00b7rer", "tr", "&#252;", "ben", "rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["VAFIN", "ADJA", "NN", "PPOSAT", "FM", "FM", "FM", "FM"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie ganz in thr&#228;nen ganz in schmerz und sch&#252;chtern               ", "tokens": ["Sie", "ganz", "in", "thr", "&#228;", "nen", "ganz", "in", "schmerz", "und", "sch", "&#252;", "ch\u00b7tern"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["PPER", "ADV", "APPR", "NE", "$(", "VVFIN", "ADV", "APPR", "NN", "KON", "ADJD", "$(", "VVINF"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.4": {"text": "Bestimmten die gezogenen geleise             ", "tokens": ["Be\u00b7stimm\u00b7ten", "die", "ge\u00b7zo\u00b7ge\u00b7nen", "ge\u00b7lei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "An hohen thoren wo sie eintritt heische               ", "tokens": ["An", "ho\u00b7hen", "tho\u00b7ren", "wo", "sie", "ein\u00b7tritt", "hei\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PWAV", "PPER", "VVFIN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist niemand der f&#252;r ihre treue zeuge               ", "tokens": ["Ist", "nie\u00b7mand", "der", "f", "&#252;", "r", "ih\u00b7re", "treu\u00b7e", "zeu\u00b7ge"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "ART", "NN", "$(", "VVIMP", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und keine hand die fleisch von ihrem fleische               ", "tokens": ["Und", "kei\u00b7ne", "hand", "die", "fleisch", "von", "ih\u00b7rem", "flei\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich bis zu ihr herniederbeuge             ", "tokens": ["Sich", "bis", "zu", "ihr", "her\u00b7nie\u00b7der\u00b7beu\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So wird sie bald ergriffen vom get\u00f6se               ", "tokens": ["So", "wird", "sie", "bald", "er\u00b7grif\u00b7fen", "vom", "ge\u00b7t\u00f6\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald kehrt sie um mit seiner schlimmen beute               ", "tokens": ["Bald", "kehrt", "sie", "um", "mit", "sei\u00b7ner", "schlim\u00b7men", "beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und so wie fr&#252;her murmelt sie noch heute               ", "tokens": ["Und", "so", "wie", "fr", "&#252;", "her", "mur\u00b7melt", "sie", "noch", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "NE", "$(", "APZR", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den spruch der nahend sie erl\u00f6se.             ", "tokens": ["Den", "spruch", "der", "na\u00b7hend", "sie", "er\u00b7l\u00f6\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "PPER", "VVFIN", "$."], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}}}}}