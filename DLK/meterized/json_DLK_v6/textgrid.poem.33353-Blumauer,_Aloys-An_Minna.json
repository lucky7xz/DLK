{"textgrid.poem.33353": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "An Minna", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh, wie der Mond, die Sterne prangen,", "tokens": ["Sieh", ",", "wie", "der", "Mond", ",", "die", "Ster\u00b7ne", "pran\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So klar auf ihrer dunklen Bahn;", "tokens": ["So", "klar", "auf", "ih\u00b7rer", "dunk\u00b7len", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "D'rum komm', und stille mein Verlangen,", "tokens": ["D'\u00b7rum", "komm'", ",", "und", "stil\u00b7le", "mein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und eile bald zu mir heran.", "tokens": ["Und", "ei\u00b7le", "bald", "zu", "mir", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dorthin zu jenen gr\u00fcnen Lauben,", "tokens": ["Dor\u00b7thin", "zu", "je\u00b7nen", "gr\u00fc\u00b7nen", "Lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dorthin la\u00df, Minna, la\u00df uns flieh'n,", "tokens": ["Dor\u00b7thin", "la\u00df", ",", "Min\u00b7na", ",", "la\u00df", "uns", "flieh'n", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "NE", "$,", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Der K\u00fcsse zahllos' Menge rauben,", "tokens": ["Der", "K\u00fcs\u00b7se", "zahl\u00b7los'", "Men\u00b7ge", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Rosen und Jasminen bl\u00fch'n.", "tokens": ["Wo", "Ro\u00b7sen", "und", "Jas\u00b7mi\u00b7nen", "bl\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und dort bei jener Silberquelle", "tokens": ["Und", "dort", "bei", "je\u00b7ner", "Sil\u00b7ber\u00b7quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Soll schmiegen sich die Brust an Brust,", "tokens": ["Soll", "schmie\u00b7gen", "sich", "die", "Brust", "an", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PRF", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und immer neu, wie jede Welle,", "tokens": ["Und", "im\u00b7mer", "neu", ",", "wie", "je\u00b7de", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,", "PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So sey f\u00fcr uns auch jede Lust.", "tokens": ["So", "sey", "f\u00fcr", "uns", "auch", "je\u00b7de", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Auch jeden Augenblick geniessen,", "tokens": ["Auch", "je\u00b7den", "Au\u00b7gen\u00b7blick", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der freundlich sich entgegenbeut,", "tokens": ["Der", "freund\u00b7lich", "sich", "ent\u00b7ge\u00b7gen\u00b7beut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Sorg' und Kummer, alle missen,", "tokens": ["Und", "Sor\u00b7g'", "und", "Kum\u00b7mer", ",", "al\u00b7le", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "PIS", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So lange noch die Jugendzeit,", "tokens": ["So", "lan\u00b7ge", "noch", "die", "Ju\u00b7gend\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die holde, rosige uns blinket,", "tokens": ["Die", "hol\u00b7de", ",", "ro\u00b7si\u00b7ge", "uns", "blin\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihre Gaben reichet dar;", "tokens": ["Und", "ih\u00b7re", "Ga\u00b7ben", "rei\u00b7chet", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So lange Amor uns noch winket", "tokens": ["So", "lan\u00b7ge", "A\u00b7mor", "uns", "noch", "win\u00b7ket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "NE", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bei Hymens festlichem Altar.", "tokens": ["Bei", "Hy\u00b7mens", "fest\u00b7li\u00b7chem", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}