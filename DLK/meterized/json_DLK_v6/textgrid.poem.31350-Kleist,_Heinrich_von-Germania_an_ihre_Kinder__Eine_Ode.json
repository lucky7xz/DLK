{"textgrid.poem.31350": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "Germania an ihre Kinder / Eine Ode", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die des Maines Regionen,", "tokens": ["Die", "des", "Mai\u00b7nes", "Re\u00b7gi\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die der Elbe heitre Aun,", "tokens": ["Die", "der", "El\u00b7be", "heit\u00b7re", "Aun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die der Donau Strand bewohnen,", "tokens": ["Die", "der", "Do\u00b7nau", "Strand", "be\u00b7woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NE", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die das Odertal bebaun,", "tokens": ["Die", "das", "O\u00b7der\u00b7tal", "be\u00b7baun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus des Rheines Laubensitzen,", "tokens": ["Aus", "des", "Rhei\u00b7nes", "Lau\u00b7ben\u00b7sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von dem duftgen Mittelmeer,", "tokens": ["Von", "dem", "duft\u00b7gen", "Mit\u00b7tel\u00b7meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Von der Riesenberge Spitzen,", "tokens": ["Von", "der", "Rie\u00b7sen\u00b7ber\u00b7ge", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von der Ost und Nordsee her!", "tokens": ["Von", "der", "Ost", "und", "Nord\u00b7see", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NE", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}