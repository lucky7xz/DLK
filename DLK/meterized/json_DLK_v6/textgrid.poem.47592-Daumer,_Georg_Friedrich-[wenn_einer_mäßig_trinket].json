{"textgrid.poem.47592": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wenn einer m\u00e4\u00dfig trinket]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn einer m\u00e4\u00dfig trinket,", "tokens": ["Wenn", "ei\u00b7ner", "m\u00e4\u00b7\u00dfig", "trin\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So soll ihm das gedeihlich sein;", "tokens": ["So", "soll", "ihm", "das", "ge\u00b7deih\u00b7lich", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ohne Ma\u00df, so soll es", "tokens": ["Wenn", "oh\u00b7ne", "Ma\u00df", ",", "so", "soll", "es"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "$,", "ADV", "VMFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm ohne Ma\u00df gedeihlich sein.", "tokens": ["Ihm", "oh\u00b7ne", "Ma\u00df", "ge\u00b7deih\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Scheich, der alte Vater,", "tokens": ["Der", "Scheich", ",", "der", "al\u00b7te", "Va\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn er zu Glas und Flasche greift,", "tokens": ["Wenn", "er", "zu", "Glas", "und", "Fla\u00b7sche", "greift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df Himmel ihm die Flasche,", "tokens": ["La\u00df", "Him\u00b7mel", "ihm", "die", "Fla\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "La\u00df ihm das Glas gedeihlich sein!", "tokens": ["La\u00df", "ihm", "das", "Glas", "ge\u00b7deih\u00b7lich", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Den Ku\u00df auf zarte Munde", "tokens": ["Den", "Ku\u00df", "auf", "zar\u00b7te", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "La\u00df in Pallast und H\u00fctten uns,", "tokens": ["La\u00df", "in", "Pal\u00b7last", "und", "H\u00fct\u00b7ten", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "NN", "KON", "NN", "PPER", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Auf Kissen und auf Polstern", "tokens": ["Auf", "Kis\u00b7sen", "und", "auf", "Pols\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Busch und Gras gedeihlich sein!", "tokens": ["In", "Busch", "und", "Gras", "ge\u00b7deih\u00b7lich", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch sperrt die Erde hungernd", "tokens": ["Doch", "sperrt", "die", "Er\u00b7de", "hun\u00b7gernd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zehntausend schwarze Rachen auf,", "tokens": ["Zehn\u00b7tau\u00b7send", "schwar\u00b7ze", "Ra\u00b7chen", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df sie Zeloten fressen", "tokens": ["La\u00df", "sie", "Ze\u00b7lo\u00b7ten", "fres\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ihr den Fra\u00df gedeihlich sein!", "tokens": ["Und", "ihr", "den", "Fra\u00df", "ge\u00b7deih\u00b7lich", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}