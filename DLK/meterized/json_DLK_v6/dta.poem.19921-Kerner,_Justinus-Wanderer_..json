{"dta.poem.19921": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Wanderer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Stra\u00dfen, die ich gehe,", "tokens": ["Die", "Stra\u00b7\u00dfen", ",", "die", "ich", "ge\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So oft ich um mich sehe,", "tokens": ["So", "oft", "ich", "um", "mich", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie bleiben fremd doch mir.", "tokens": ["Sie", "blei\u00b7ben", "fremd", "doch", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Herberg', wo ich m\u00f6cht' weilen,", "tokens": ["Her\u00b7ber\u00b7g'", ",", "wo", "ich", "m\u00f6cht'", "wei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich kann sie nicht ereilen,", "tokens": ["Ich", "kann", "sie", "nicht", "er\u00b7ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Weit, weit ist sie von hier.", "tokens": ["Weit", ",", "weit", "ist", "sie", "von", "hier", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VAFIN", "PPER", "APPR", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So fremd mir anzuschauen", "tokens": ["So", "fremd", "mir", "an\u00b7zu\u00b7schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "VVIZU"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind diese St\u00e4dt' und Auen,", "tokens": ["Sind", "die\u00b7se", "St\u00e4dt'", "und", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Burgen stumm und todt;", "tokens": ["Die", "Bur\u00b7gen", "stumm", "und", "todt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch fern Gebirge ragen,", "tokens": ["Doch", "fern", "Ge\u00b7bir\u00b7ge", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die meine Heimat tragen,", "tokens": ["Die", "mei\u00b7ne", "Hei\u00b7mat", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein ewig Morgenroth.", "tokens": ["Ein", "e\u00b7wig", "Mor\u00b7gen\u00b7roth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}