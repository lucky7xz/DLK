{"dta.poem.2963": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im f\u00fcrstlichen Palast des Festes Schaugepr\u00e4nge,", "tokens": ["Im", "f\u00fcrst\u00b7li\u00b7chen", "Pa\u00b7last", "des", "Fes\u00b7tes", "Schau\u00b7ge\u00b7pr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auf dem Platz davor des Volkes Schaugedr\u00e4nge;", "tokens": ["Und", "auf", "dem", "Platz", "da\u00b7vor", "des", "Vol\u00b7kes", "Schau\u00b7ge\u00b7dr\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PAV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bescheiden nehmen sie, und sind damit zufrieden,", "tokens": ["Be\u00b7schei\u00b7den", "neh\u00b7men", "sie", ",", "und", "sind", "da\u00b7mit", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "KON", "VAFIN", "PAV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Abfall von der Lust, der ihnen ist beschieden,", "tokens": ["Den", "Ab\u00b7fall", "von", "der", "Lust", ",", "der", "ih\u00b7nen", "ist", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Den Glanz der Lichter, der durch Fenster bricht hervor,", "tokens": ["Den", "Glanz", "der", "Lich\u00b7ter", ",", "der", "durch", "Fens\u00b7ter", "bricht", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Instrumente Klang, berauschend Aug' und Ohr:", "tokens": ["Der", "Inst\u00b7ru\u00b7men\u00b7te", "Klang", ",", "be\u00b7rau\u00b7schend", "Aug'", "und", "Ohr", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Bescheiden, wie nur sonst die Gl\u00e4ubigen hienieden,", "tokens": ["Be\u00b7schei\u00b7den", ",", "wie", "nur", "sonst", "die", "Gl\u00e4u\u00b7bi\u00b7gen", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die mit dem Abglanz sind der Seligkeit zufrieden;", "tokens": ["Die", "mit", "dem", "Ab\u00b7glanz", "sind", "der", "Se\u00b7lig\u00b7keit", "zu\u00b7frie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und viel bescheidner noch, weil diese wollen kommen", "tokens": ["Und", "viel", "be\u00b7scheid\u00b7ner", "noch", ",", "weil", "die\u00b7se", "wol\u00b7len", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "ADV", "$,", "KOUS", "PDS", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Himmel, jene nicht beim Fest seyn aufgenommen.", "tokens": ["Zum", "Him\u00b7mel", ",", "je\u00b7ne", "nicht", "beim", "Fest", "seyn", "auf\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PDS", "PTKNEG", "APPRART", "NN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}