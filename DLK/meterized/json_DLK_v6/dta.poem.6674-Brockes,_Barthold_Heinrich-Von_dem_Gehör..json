{"dta.poem.6674": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geh\u00f6r.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein solch Empfinden nun geschicht", "tokens": ["Ein", "solch", "Emp\u00b7fin\u00b7den", "nun", "ge\u00b7schicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In den getroffnen Ohren nicht,", "tokens": ["In", "den", "ge\u00b7troff\u00b7nen", "Oh\u00b7ren", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch nicht in den bewegten Sehnen,", "tokens": ["Auch", "nicht", "in", "den", "be\u00b7weg\u00b7ten", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht im Gehirn, wohin die Regungen sich dehnen.", "tokens": ["Nicht", "im", "Ge\u00b7hirn", ",", "wo\u00b7hin", "die", "Re\u00b7gun\u00b7gen", "sich", "deh\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "$,", "PWAV", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Nein, in der Seele zeigets sich,", "tokens": ["Nein", ",", "in", "der", "See\u00b7le", "zei\u00b7gets", "sich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "ART", "NN", "VVFIN", "PRF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Die sich besch\u00e4fftigt, innerlich,", "tokens": ["Die", "sich", "be\u00b7sch\u00e4ff\u00b7tigt", ",", "in\u00b7ner\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PRF", "VVPP", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch die Bewegungen, die von der Lufft geschehn,", "tokens": ["Durch", "die", "Be\u00b7we\u00b7gun\u00b7gen", ",", "die", "von", "der", "Lufft", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+---+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Und \u00e4usserlich entstehn.", "tokens": ["Und", "\u00e4us\u00b7ser\u00b7lich", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}