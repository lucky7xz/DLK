{"textgrid.poem.31248": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Itzt/ bitte/ basst mahl Acht.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Itzt/ bitte/ basst mahl Acht.", "tokens": ["Itzt", "/", "bit\u00b7te", "/", "basst", "mahl", "Acht", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PTKANT", "$(", "VVFIN", "ADV", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Itzt zupff ich euch ein Z\u00f6tgen/", "tokens": ["Itzt", "zupff", "ich", "euch", "ein", "Z\u00f6t\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und hindter jedem N\u00f6tgen", "tokens": ["und", "hind\u00b7ter", "je\u00b7dem", "N\u00f6t\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "sto\u00dft an/ da\u00df alles kracht!", "tokens": ["sto\u00dft", "an", "/", "da\u00df", "al\u00b7les", "kracht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$(", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich schl\u00e4ndre so die Gracht.", "tokens": ["Ich", "schl\u00e4nd\u00b7re", "so", "die", "Gracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Kaum schw\u00e4nck ich \u00fcmb die Ekke/", "tokens": ["Kaum", "schw\u00e4nck", "ich", "\u00fcmb", "die", "Ek\u00b7ke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "gleich gr\u00fc\u00dft mich eine Kekke", "tokens": ["gleich", "gr\u00fc\u00dft", "mich", "ei\u00b7ne", "Kek\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "in ", "tokens": ["in"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Ich br\u00e4nne wie ein Dacht.", "tokens": ["Ich", "br\u00e4n\u00b7ne", "wie", "ein", "Dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Willstu mein Feuer l\u00f6schen?", "tokens": ["Will\u00b7stu", "mein", "Feu\u00b7er", "l\u00f6\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sih/ hihr sind drey-zehn Gr\u00f6schen/", "tokens": ["Sih", "/", "hihr", "sind", "drey\u00b7zehn", "Gr\u00f6\u00b7schen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADV", "VAFIN", "CARD", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "versprich mir dihse Nacht!", "tokens": ["ver\u00b7sprich", "mir", "dih\u00b7se", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie siht mich an und lacht.", "tokens": ["Sie", "siht", "mich", "an", "und", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "For so vihl Silberlinge", "tokens": ["For", "so", "vihl", "Sil\u00b7ber\u00b7lin\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "PIAT", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "dhat mancher manche Dinge/", "tokens": ["dhat", "man\u00b7cher", "man\u00b7che", "Din\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "ich dhu es schon for acht!", "tokens": ["ich", "dhu", "es", "schon", "for", "acht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Worhin war ich gebracht?", "tokens": ["Wor\u00b7hin", "war", "ich", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein L\u00e4mpgen mit Gefunckel", "tokens": ["Ein", "L\u00e4mp\u00b7gen", "mit", "Ge\u00b7fun\u00b7ckel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "verstreute kaum das Dunckel/", "tokens": ["ver\u00b7streu\u00b7te", "kaum", "das", "Dun\u00b7ckel", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "ihr Bett stund schon gemacht!", "tokens": ["ihr", "Bett", "stund", "schon", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie war zu adorabel", "tokens": ["Sie", "war", "zu", "a\u00b7do\u00b7ra\u00b7bel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "vom Zeh bi\u00df zu dem Nabel", "tokens": ["vom", "Zeh", "bi\u00df", "zu", "dem", "Na\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "ohn alle jede Dracht!", "tokens": ["ohn", "al\u00b7le", "je\u00b7de", "Dracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wir kniepten uns nicht sacht.", "tokens": ["Wir", "kniep\u00b7ten", "uns", "nicht", "sacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sie musste sich mir borgen", "tokens": ["Sie", "muss\u00b7te", "sich", "mir", "bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "des offtern bi\u00df zum Morgen", "tokens": ["des", "off\u00b7tern", "bi\u00df", "zum", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "in ihrem Fehder-Schacht!", "tokens": ["in", "ih\u00b7rem", "Feh\u00b7der\u00b7Schacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Printz ", "tokens": ["Printz"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Itzt sizz ich hihr im Luxen/", "tokens": ["Itzt", "sizz", "ich", "hihr", "im", "Lu\u00b7xen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "den F\u00fcnwer zu verjuxen \u2013", "tokens": ["den", "F\u00fcn\u00b7wer", "zu", "ver\u00b7ju\u00b7xen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "wer h\u00e4tte da\u00df gedacht?", "tokens": ["wer", "h\u00e4t\u00b7te", "da\u00df", "ge\u00b7dacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "KOUS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}