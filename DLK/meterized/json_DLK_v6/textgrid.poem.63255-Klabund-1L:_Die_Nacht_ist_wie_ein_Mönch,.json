{"textgrid.poem.63255": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Nacht ist wie ein M\u00f6nch,", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht ist wie ein M\u00f6nch,", "tokens": ["Die", "Nacht", "ist", "wie", "ein", "M\u00f6nch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sie tr\u00e4gt ein braun Gewand.", "tokens": ["Sie", "tr\u00e4gt", "ein", "braun", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Tag ist wie ein Mensch,", "tokens": ["Der", "Tag", "ist", "wie", "ein", "Mensch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hat Lilien in der Hand.", "tokens": ["Hat", "Li\u00b7li\u00b7en", "in", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Die Nacht ist dunkel ganz", "tokens": ["Die", "Nacht", "ist", "dun\u00b7kel", "ganz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und stummer als ein Grab.", "tokens": ["Und", "stum\u00b7mer", "als", "ein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Tag: Gefunkel ganz,", "tokens": ["Der", "Tag", ":", "Ge\u00b7fun\u00b7kel", "ganz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gel\u00e4chter, Klang und Tanz.", "tokens": ["Ge\u00b7l\u00e4ch\u00b7ter", ",", "Klang", "und", "Tanz", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}