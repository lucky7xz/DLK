{"dta.poem.2935": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "134.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In diesem Spiel des Gl\u00fccks, in welchem keiner kann", "tokens": ["In", "die\u00b7sem", "Spiel", "des", "Gl\u00fccks", ",", "in", "wel\u00b7chem", "kei\u00b7ner", "kann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "$,", "APPR", "PRELS", "PIS", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gewinnen, ohne da\u00df verlor ein Gegenmann;", "tokens": ["Ge\u00b7win\u00b7nen", ",", "oh\u00b7ne", "da\u00df", "ver\u00b7lor", "ein", "Ge\u00b7gen\u00b7mann", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "KOUS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In diesem Spiel des Gl\u00fccks, in dem auch keiner kann", "tokens": ["In", "die\u00b7sem", "Spiel", "des", "Gl\u00fccks", ",", "in", "dem", "auch", "kei\u00b7ner", "kann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "$,", "APPR", "PRELS", "ADV", "PIS", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verlieren, ohne was ein Gegenmann gewann;", "tokens": ["Ver\u00b7lie\u00b7ren", ",", "oh\u00b7ne", "was", "ein", "Ge\u00b7gen\u00b7mann", "ge\u00b7wann", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In diesem Spiel des Gl\u00fccks verliert an ruh'gen Sinnen", "tokens": ["In", "die\u00b7sem", "Spiel", "des", "Gl\u00fccks", "ver\u00b7liert", "an", "ruh'\u00b7gen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Spieler, ob er mag verlieren, ob gewinnen;", "tokens": ["Der", "Spie\u00b7ler", ",", "ob", "er", "mag", "ver\u00b7lie\u00b7ren", ",", "ob", "ge\u00b7win\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$,", "KOUS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und Lust gewinnt allein, wer als Zuschauer steht,", "tokens": ["Und", "Lust", "ge\u00b7winnt", "al\u00b7lein", ",", "wer", "als", "Zu\u00b7schau\u00b7er", "steht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "$,", "PWS", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und siehet da\u00df im Grund hier nichts verloren geht;", "tokens": ["Und", "sie\u00b7het", "da\u00df", "im", "Grund", "hier", "nichts", "ver\u00b7lo\u00b7ren", "geht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "APPRART", "NN", "ADV", "PIS", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da\u00df eines Lebens Tod des andern ist Belebung,", "tokens": ["Da\u00df", "ei\u00b7nes", "Le\u00b7bens", "Tod", "des", "an\u00b7dern", "ist", "Be\u00b7le\u00b7bung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ART", "ADJA", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jedes Sinken hier wird dort zu einer Hebung;", "tokens": ["Und", "je\u00b7des", "Sin\u00b7ken", "hier", "wird", "dort", "zu", "ei\u00b7ner", "He\u00b7bung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Da\u00df dieses Schwanken selbst sich h\u00e4lt im Gleichgewicht:", "tokens": ["Da\u00df", "die\u00b7ses", "Schwan\u00b7ken", "selbst", "sich", "h\u00e4lt", "im", "Gleich\u00b7ge\u00b7wicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADV", "PRF", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer sich im Ganzen f\u00fchlt, der h\u00e4ngt am Einzlen nicht.", "tokens": ["Wer", "sich", "im", "Gan\u00b7zen", "f\u00fchlt", ",", "der", "h\u00e4ngt", "am", "Einz\u00b7len", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPRART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "APPRART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und will das Gl\u00fcck dich selbst in seine Wirbel ziehn,", "tokens": ["Und", "will", "das", "Gl\u00fcck", "dich", "selbst", "in", "sei\u00b7ne", "Wir\u00b7bel", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df nur die ruhige Betrachtung nicht entfliehn:", "tokens": ["La\u00df", "nur", "die", "ru\u00b7hi\u00b7ge", "Be\u00b7trach\u00b7tung", "nicht", "ent\u00b7fliehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Da\u00df nur, was du verlierst, ein andrer hat einstweilen,", "tokens": ["Da\u00df", "nur", ",", "was", "du", "ver\u00b7lierst", ",", "ein", "an\u00b7drer", "hat", "einst\u00b7wei\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,", "ART", "ADJA", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und das was du gewannst, du kannst mit andern theilen.", "tokens": ["Und", "das", "was", "du", "ge\u00b7wannst", ",", "du", "kannst", "mit", "an\u00b7dern", "thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PRELS", "PPER", "VVPP", "$,", "PPER", "VMFIN", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Ungl\u00fccklich ist nur, wer sein Gl\u00fcck mit keinem theilt,", "tokens": ["Un\u00b7gl\u00fcck\u00b7lich", "ist", "nur", ",", "wer", "sein", "Gl\u00fcck", "mit", "kei\u00b7nem", "theilt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "$,", "PWS", "PPOSAT", "NN", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und vor dem Ungl\u00fcck bangt, noch ehr es ihn ereilt.", "tokens": ["Und", "vor", "dem", "Un\u00b7gl\u00fcck", "bangt", ",", "noch", "ehr", "es", "ihn", "er\u00b7eilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,", "ADV", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}