{"dta.poem.8473": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Apfelb\u00e4ume,  \n  wo ich Julien erblickte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Durchzittre deine Wipfel, o Schattengang,", "tokens": ["Durch\u00b7zitt\u00b7re", "dei\u00b7ne", "Wip\u00b7fel", ",", "o", "Schat\u00b7ten\u00b7gang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo bang' und wild der ersten Liebe", "tokens": ["Wo", "bang'", "und", "wild", "der", "ers\u00b7ten", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Selige Taumel mein Herz berauschten.", "tokens": ["Se\u00b7li\u00b7ge", "Tau\u00b7mel", "mein", "Herz", "be\u00b7rauschten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Die Abendsonne bebte wie lichtes Gold", "tokens": ["Die", "A\u00b7bend\u00b7son\u00b7ne", "beb\u00b7te", "wie", "lich\u00b7tes", "Gold"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Durch Purpurbl\u00fcten, bebte wie lichtes Gold", "tokens": ["Durch", "Pur\u00b7pur\u00b7bl\u00fc\u00b7ten", ",", "beb\u00b7te", "wie", "lich\u00b7tes", "Gold"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Um ihres Busens Silberschleier;", "tokens": ["Um", "ih\u00b7res", "Bu\u00b7sens", "Sil\u00b7ber\u00b7schlei\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ich zerfloss in Entz\u00fcckungsschauer.", "tokens": ["Und", "ich", "zer\u00b7floss", "in", "Ent\u00b7z\u00fc\u00b7ckungs\u00b7schau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Nach langer Trennung k\u00fcsse mit Engelkuss", "tokens": ["Nach", "lan\u00b7ger", "Tren\u00b7nung", "k\u00fcs\u00b7se", "mit", "En\u00b7gel\u00b7kuss"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "NE"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ein treuer J\u00fcngling hier das geliebte Weib,", "tokens": ["Ein", "treu\u00b7er", "J\u00fcng\u00b7ling", "hier", "das", "ge\u00b7lieb\u00b7te", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und schw\u00f6r' in diesem Bl\u00fctendunkel", "tokens": ["Und", "schw\u00f6r'", "in", "die\u00b7sem", "Bl\u00fc\u00b7ten\u00b7dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ewige Treue der Auserkornen.", "tokens": ["E\u00b7wi\u00b7ge", "Treu\u00b7e", "der", "Au\u00b7ser\u00b7kor\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Ein Bl\u00fcmchen sprosse, wann wir gestorben sind,", "tokens": ["Ein", "Bl\u00fcm\u00b7chen", "spros\u00b7se", ",", "wann", "wir", "ge\u00b7stor\u00b7ben", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus jedem Rasen, welchen ihr Fuss ber\u00fchrt,", "tokens": ["Aus", "je\u00b7dem", "Ra\u00b7sen", ",", "wel\u00b7chen", "ihr", "Fuss", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PWAT", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und trag' auf jedem seiner Bl\u00e4tter", "tokens": ["Und", "trag'", "auf", "je\u00b7dem", "sei\u00b7ner", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Meines verherlichten M\u00e4dchens Namen.", "tokens": ["Mei\u00b7nes", "ver\u00b7her\u00b7lich\u00b7ten", "M\u00e4d\u00b7chens", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}