{"textgrid.poem.42104": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich seh dich wieder, Fr\u00fchling, steigen ins Ge\u00e4st.", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich seh dich wieder, Fr\u00fchling, steigen ins Ge\u00e4st.", "tokens": ["Ich", "seh", "dich", "wie\u00b7der", ",", "Fr\u00fch\u00b7ling", ",", "stei\u00b7gen", "ins", "Ge\u00b7\u00e4st", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich seh den d\u00fcrren Zweig, von deiner Faust umpre\u00dft,", "tokens": ["Ich", "seh", "den", "d\u00fcr\u00b7ren", "Zweig", ",", "von", "dei\u00b7ner", "Faust", "um\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun wieder gl\u00fchn und bl\u00fchn, ich seh den Wiesenpfad", "tokens": ["Nun", "wie\u00b7der", "gl\u00fchn", "und", "bl\u00fchn", ",", "ich", "seh", "den", "Wie\u00b7sen\u00b7pfad"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVINF", "KON", "VVINF", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bergauf dir unterm Fu\u00df erstehn und wehn wie Saat.", "tokens": ["Ber\u00b7gauf", "dir", "un\u00b7term", "Fu\u00df", "er\u00b7stehn", "und", "wehn", "wie", "Saat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVINF", "KON", "VVFIN", "KOKOM", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Dann wirst du wieder um die Gipfel streichen \u2013", "tokens": ["Dann", "wirst", "du", "wie\u00b7der", "um", "die", "Gip\u00b7fel", "strei\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schnee, der vergeht, o Wei\u00df, das zum Verbleichen", "tokens": ["Schnee", ",", "der", "ver\u00b7geht", ",", "o", "Wei\u00df", ",", "das", "zum", "Ver\u00b7blei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "VVFIN", "$,", "FM", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Sich wieder f\u00fcgt, tal\u00fcber quillt, o Einsamkeit,", "tokens": ["Sich", "wie\u00b7der", "f\u00fcgt", ",", "ta\u00b7l\u00fc\u00b7ber", "quillt", ",", "o", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gel\u00f6st, hinstr\u00f6mend z\u00e4rtlich wild und weit.", "tokens": ["Ge\u00b7l\u00f6st", ",", "hins\u00b7tr\u00f6\u00b7mend", "z\u00e4rt\u00b7lich", "wild", "und", "weit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}