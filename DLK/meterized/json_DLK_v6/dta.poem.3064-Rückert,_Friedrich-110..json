{"dta.poem.3064": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "110.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus zwei Verneinungen wird eine Wortbejahung,", "tokens": ["Aus", "zwei", "Ver\u00b7nei\u00b7nun\u00b7gen", "wird", "ei\u00b7ne", "Wort\u00b7be\u00b7ja\u00b7hung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Aus zwei Entfernungen doch niemals eine Nahung,", "tokens": ["Aus", "zwei", "Ent\u00b7fer\u00b7nun\u00b7gen", "doch", "nie\u00b7mals", "ei\u00b7ne", "Na\u00b7hung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+--++-+-+---", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Aus zwei Abneigungen nie eine Liebumfahung.", "tokens": ["Aus", "zwei", "Ab\u00b7nei\u00b7gun\u00b7gen", "nie", "ei\u00b7ne", "Lieb\u00b7um\u00b7fa\u00b7hung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Aus zwei Bejahungen wird nie im Wort Verneinung,", "tokens": ["Aus", "zwei", "Be\u00b7ja\u00b7hun\u00b7gen", "wird", "nie", "im", "Wort", "Ver\u00b7nei\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VAFIN", "ADV", "APPRART", "NN", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Doch in der Sache wol; wenn bringen in Vereinung", "tokens": ["Doch", "in", "der", "Sa\u00b7che", "wol", ";", "wenn", "brin\u00b7gen", "in", "Ver\u00b7ei\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "$.", "KOUS", "VVINF", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du willst zwei Meinungen, erh\u00e4ltst du keine Meinung.", "tokens": ["Du", "willst", "zwei", "Mei\u00b7nun\u00b7gen", ",", "er\u00b7h\u00e4ltst", "du", "kei\u00b7ne", "Mei\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "CARD", "NN", "$,", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}