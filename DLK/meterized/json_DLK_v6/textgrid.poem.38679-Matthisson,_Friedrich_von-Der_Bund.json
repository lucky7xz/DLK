{"textgrid.poem.38679": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Bund", "genre": "verse", "period": "N.A.", "pub_year": 1792, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast du's in meinem Auge nicht gelesen,", "tokens": ["Hast", "du's", "in", "mei\u00b7nem", "Au\u00b7ge", "nicht", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "PPOSAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was ungest\u00fcm dein Mund seit gestern fragt?", "tokens": ["Was", "un\u00b7ge\u00b7st\u00fcm", "dein", "Mund", "seit", "ge\u00b7stern", "fragt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPOSAT", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich ahnd' in dir das gleichgeschaffne Wesen", "tokens": ["Ich", "ahnd'", "in", "dir", "das", "gleich\u00b7ge\u00b7schaff\u00b7ne", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und meines Daseyns \u00f6de D\u00e4mmrung tagt.", "tokens": ["Und", "mei\u00b7nes", "Da\u00b7seyns", "\u00f6\u00b7de", "D\u00e4mm\u00b7rung", "tagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In dunkler Wolke webt, mit leiser Hand,", "tokens": ["In", "dunk\u00b7ler", "Wol\u00b7ke", "webt", ",", "mit", "lei\u00b7ser", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Sympathie geheimni\u00dfvoll ihr Band.", "tokens": ["Die", "Sym\u00b7pa\u00b7thie", "ge\u00b7heim\u00b7ni\u00df\u00b7voll", "ihr", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Empfang', Ersehnter, diese Freudenz\u00e4hre,", "tokens": ["Emp\u00b7fang'", ",", "Er\u00b7sehn\u00b7ter", ",", "die\u00b7se", "Freu\u00b7den\u00b7z\u00e4h\u00b7re", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Dank, da\u00df du den Himmel mir enth\u00fcllt!", "tokens": ["Zum", "Dank", ",", "da\u00df", "du", "den", "Him\u00b7mel", "mir", "ent\u00b7h\u00fcllt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Erd' entf\u00fchrt ins Thal der Schattench\u00f6re", "tokens": ["Der", "Erd'", "ent\u00b7f\u00fchrt", "ins", "Thal", "der", "Schat\u00b7ten\u00b7ch\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Einst Psyche nur allein dein holdes Bild:", "tokens": ["Einst", "Psy\u00b7che", "nur", "al\u00b7lein", "dein", "hol\u00b7des", "Bild", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So rettete von Tauris wildem Strand", "tokens": ["So", "ret\u00b7te\u00b7te", "von", "Tau\u00b7ris", "wil\u00b7dem", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sein Heiligthum Orest ins be\u00dfre Land.", "tokens": ["Sein", "Hei\u00b7lig\u00b7thum", "O\u00b7rest", "ins", "be\u00df\u00b7re", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "APPRART", "ADJA", "NN", "$."], "meter": "-+-----+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Du den ich k\u00fchn aus Tausenden erw\u00e4hle,", "tokens": ["Du", "den", "ich", "k\u00fchn", "aus", "Tau\u00b7sen\u00b7den", "er\u00b7w\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Sch\u00f6pfer hoffnungsvoller Bl\u00fctenzeit!", "tokens": ["O", "Sch\u00f6p\u00b7fer", "hoff\u00b7nungs\u00b7vol\u00b7ler", "Bl\u00fc\u00b7ten\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In diesem Ku\u00df nimm meine ganze Seele,", "tokens": ["In", "die\u00b7sem", "Ku\u00df", "nimm", "mei\u00b7ne", "gan\u00b7ze", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVIMP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In diesem Ring das Pfand der Ewigkeit.", "tokens": ["In", "die\u00b7sem", "Ring", "das", "Pfand", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Am Sternenhimmel flammt das heilge Wort:", "tokens": ["Am", "Ster\u00b7nen\u00b7him\u00b7mel", "flammt", "das", "heil\u00b7ge", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Geister Einklang t\u00f6nt unendlich fort.", "tokens": ["Der", "Geis\u00b7ter", "Ein\u00b7klang", "t\u00f6nt", "un\u00b7end\u00b7lich", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}