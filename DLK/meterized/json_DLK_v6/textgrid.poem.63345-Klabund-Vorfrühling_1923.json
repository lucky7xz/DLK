{"textgrid.poem.63345": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Vorfr\u00fchling 1923", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heute fing ich \u2013 Krieg ist Krieg \u2013 eine Maus in der Schlinge.", "tokens": ["Heu\u00b7te", "fing", "ich", "\u2013", "Krieg", "ist", "Krieg", "\u2013", "ei\u00b7ne", "Maus", "in", "der", "Schlin\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "NN", "VAFIN", "NN", "$(", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Fr\u00fchlingswolken flattern rosig im Winde.", "tokens": ["Fr\u00fch\u00b7lings\u00b7wol\u00b7ken", "flat\u00b7tern", "ro\u00b7sig", "im", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Emma schrieb mir von unserm gemeinsamen Kinde,", "tokens": ["Em\u00b7ma", "schrieb", "mir", "von", "un\u00b7serm", "ge\u00b7mein\u00b7sa\u00b7men", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Da\u00df es schon in die Schule ginge,", "tokens": ["Da\u00df", "es", "schon", "in", "die", "Schu\u00b7le", "gin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df \u2013 wie erhebend! \u2013 ein Einser Fritzchens Zensur im Rechnen ziere,", "tokens": ["Da\u00df", "\u2013", "wie", "er\u00b7he\u00b7bend", "!", "\u2013", "ein", "Ein\u00b7ser", "Fritz\u00b7chens", "Zen\u00b7sur", "im", "Rech\u00b7nen", "zie\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PWAV", "VVPP", "$.", "$(", "ART", "NN", "NE", "NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Der Idiot...) \u00e0 la hausse der Mark spekuliere...", "tokens": ["Der", "I\u00b7diot", "...", ")", "\u00e0", "la", "haus\u00b7se", "der", "Mark", "spe\u00b7ku\u00b7lie\u00b7re", "..."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "$(", "FM", "FM", "FM", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Heute begegnete ich den ersten Staren.", "tokens": ["Heu\u00b7te", "be\u00b7ge\u00b7gne\u00b7te", "ich", "den", "ers\u00b7ten", "Sta\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Zum erstenmal bin ich auch mit der Nord-S\u00fcd-Bahn gefahren.", "tokens": ["Zum", "ers\u00b7ten\u00b7mal", "bin", "ich", "auch", "mit", "der", "Nord\u00b7S\u00fcd\u00b7Bahn", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+----+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich bildete mir ein, vom Nord- zum S\u00fcdpol zu rasen.", "tokens": ["Ich", "bil\u00b7de\u00b7te", "mir", "ein", ",", "vom", "Nord", "zum", "S\u00fcd\u00b7pol", "zu", "ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "APPRART", "TRUNC", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Am Wedding sah ich Eskimos mit Tran handeln,", "tokens": ["Am", "Wed\u00b7ding", "sah", "ich", "Es\u00b7ki\u00b7mos", "mit", "Tran", "han\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Pinguine durch die Chausseestra\u00dfe wandeln,", "tokens": ["Pin\u00b7gu\u00b7i\u00b7ne", "durch", "die", "Chaus\u00b7see\u00b7stra\u00b7\u00dfe", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Und am Halleschen Tor h\u00f6rte ich die Kaurineger im Jandorfkraal", "tokens": ["Und", "am", "Hal\u00b7le\u00b7schen", "Tor", "h\u00f6r\u00b7te", "ich", "die", "Kau\u00b7ri\u00b7ne\u00b7ger", "im", "Jan\u00b7dorf\u00b7kraal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN"], "meter": "--+--++-+-+--+-+-+", "measure": "anapaest.di.plus"}}}}}