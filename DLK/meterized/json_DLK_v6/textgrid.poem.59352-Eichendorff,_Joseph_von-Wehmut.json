{"textgrid.poem.59352": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Wehmut", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich irr in Tal und Hainen", "tokens": ["Ich", "irr", "in", "Tal", "und", "Hai\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bei k\u00fchler Abendstund,", "tokens": ["Bei", "k\u00fch\u00b7ler", "A\u00b7bends\u00b7tund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach, weinen m\u00f6cht ich, weinen", "tokens": ["Ach", ",", "wei\u00b7nen", "m\u00f6cht", "ich", ",", "wei\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "$,", "VVINF", "VMFIN", "PPER", "$,", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So recht aus Herzensgrund.", "tokens": ["So", "recht", "aus", "Her\u00b7zens\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und alter Zeiten Gr\u00fc\u00dfen", "tokens": ["Und", "al\u00b7ter", "Zei\u00b7ten", "Gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kam da, im Tal erwacht,", "tokens": ["Kam", "da", ",", "im", "Tal", "er\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gleich wie von fernen Fl\u00fcssen", "tokens": ["Gleich", "wie", "von", "fer\u00b7nen", "Fl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Rauschen durch die Nacht.", "tokens": ["Das", "Rau\u00b7schen", "durch", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Sonne ging hinunter,", "tokens": ["Die", "Son\u00b7ne", "ging", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da s\u00e4uselt' kaum die Welt,", "tokens": ["Da", "s\u00e4u\u00b7selt'", "kaum", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich blieb noch lange munter", "tokens": ["Ich", "blieb", "noch", "lan\u00b7ge", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Allein im stillen Feld.", "tokens": ["Al\u00b7lein", "im", "stil\u00b7len", "Feld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}