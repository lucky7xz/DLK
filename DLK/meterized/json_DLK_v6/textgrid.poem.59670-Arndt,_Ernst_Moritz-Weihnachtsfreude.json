{"textgrid.poem.59670": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Weihnachtsfreude", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steh auf! Die Sonn' ist aufgegangen,", "tokens": ["Steh", "auf", "!", "Die", "Sonn'", "ist", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es scheint das Licht der Herrlichkeit \u2013", "tokens": ["Es", "scheint", "das", "Licht", "der", "Herr\u00b7lich\u00b7keit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Seele, klinge dein Verlangen,", "tokens": ["O", "See\u00b7le", ",", "klin\u00b7ge", "dein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hell kling herein die neue Zeit!", "tokens": ["Hell", "kling", "her\u00b7ein", "die", "neu\u00b7e", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df heut die frohe Kunde schallen", "tokens": ["La\u00df", "heut", "die", "fro\u00b7he", "Kun\u00b7de", "schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Weit \u00fcbern Erdenball ringsum!", "tokens": ["Weit", "\u00fc\u00b7bern", "Er\u00b7den\u00b7ball", "ring\u00b7sum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Erklinge, singe, k\u00fcnde allen", "tokens": ["Er\u00b7klin\u00b7ge", ",", "sin\u00b7ge", ",", "k\u00fcn\u00b7de", "al\u00b7len"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Menschheit Evangelium.", "tokens": ["Der", "Menschheit", "E\u00b7van\u00b7ge\u00b7li\u00b7um", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Dies ist das Licht, dies ist der Morgen,", "tokens": ["Dies", "ist", "das", "Licht", ",", "dies", "ist", "der", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Vorwelt d\u00fcnner D\u00e4mmerschein,", "tokens": ["Der", "Vor\u00b7welt", "d\u00fcn\u00b7ner", "D\u00e4m\u00b7mer\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Oft leuchtend auf und oft verborgen,", "tokens": ["Oft", "leuch\u00b7tend", "auf", "und", "oft", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nun scheint er hell zur Welt herein,", "tokens": ["Nun", "scheint", "er", "hell", "zur", "Welt", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Liebesr\u00e4tsel ew'ger G\u00fcte,", "tokens": ["Das", "Lie\u00b7bes\u00b7r\u00e4t\u00b7sel", "ew'\u00b7ger", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Frommen Hort, der Weisen Lust \u2013", "tokens": ["Der", "From\u00b7men", "Hort", ",", "der", "Wei\u00b7sen", "Lust", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Sehnsucht s\u00fc\u00dfe Rosenbl\u00fcte", "tokens": ["Der", "Sehn\u00b7sucht", "s\u00fc\u00b7\u00dfe", "Ro\u00b7sen\u00b7bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erbl\u00fcht nun voll in jeder Brust.", "tokens": ["Er\u00b7bl\u00fcht", "nun", "voll", "in", "je\u00b7der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Drum sollst du, frohe Liebe, klingen,", "tokens": ["Drum", "sollst", "du", ",", "fro\u00b7he", "Lie\u00b7be", ",", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df alle Welt in Wonne sei,", "tokens": ["Da\u00df", "al\u00b7le", "Welt", "in", "Won\u00b7ne", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit allen Himmelsch\u00f6ren singen:", "tokens": ["Mit", "al\u00b7len", "Him\u00b7mel\u00b7sch\u00f6\u00b7ren", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr dunkle Menschen eilt herbei!", "tokens": ["Ihr", "dunk\u00b7le", "Men\u00b7schen", "eilt", "her\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O eilet euch im Licht zu baden!", "tokens": ["O", "ei\u00b7let", "euch", "im", "Licht", "zu", "ba\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Glanz des Himmels strahlt herein,", "tokens": ["Der", "Glanz", "des", "Him\u00b7mels", "strahlt", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und jeder Jammer, jeder Schaden", "tokens": ["Und", "je\u00b7der", "Jam\u00b7mer", ",", "je\u00b7der", "Scha\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Nacht soll weggeleuchtet sein!", "tokens": ["Der", "Nacht", "soll", "weg\u00b7ge\u00b7leuch\u00b7tet", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Kommt alle, die ihr lieft verloren", "tokens": ["Kommt", "al\u00b7le", ",", "die", "ihr", "lieft", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "PRELS", "PPER", "VVFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In freudenvoller Finsternis!", "tokens": ["In", "freu\u00b7den\u00b7vol\u00b7ler", "Fins\u00b7ter\u00b7nis", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn Jesus Christus ist geboren,", "tokens": ["Denn", "Je\u00b7sus", "Chris\u00b7tus", "ist", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es scheint das lichte Heil gewi\u00df.", "tokens": ["Es", "scheint", "das", "lich\u00b7te", "Heil", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Liebesglanz! O Lebensmorgen!", "tokens": ["O", "Lie\u00b7bes\u00b7glanz", "!", "O", "Le\u00b7bens\u00b7mor\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "O wunderbarer Gottesschein!", "tokens": ["O", "wun\u00b7der\u00b7ba\u00b7rer", "Got\u00b7tes\u00b7schein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Weg S\u00fcnden, Schmerzen, Zweifel, Sorgen!", "tokens": ["Weg", "S\u00fcn\u00b7den", ",", "Schmer\u00b7zen", ",", "Zwei\u00b7fel", ",", "Sor\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Denn Jesus Christ will unser sein.", "tokens": ["Denn", "Je\u00b7sus", "Christ", "will", "un\u00b7ser", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VMFIN", "PPOSAT", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}