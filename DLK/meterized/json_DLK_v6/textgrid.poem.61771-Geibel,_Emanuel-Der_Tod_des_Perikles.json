{"textgrid.poem.61771": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod des Perikles", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fchrt mich hinaus! Versinkend blickt der Tag", "tokens": ["F\u00fchrt", "mich", "hin\u00b7aus", "!", "Ver\u00b7sin\u00b7kend", "blickt", "der", "Tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "VVPP", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus goldnen Wimpern \u00fcber Salamis,", "tokens": ["Aus", "gold\u00b7nen", "Wim\u00b7pern", "\u00fc\u00b7ber", "Sa\u00b7la\u00b7mis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und k\u00fchler vom Pir\u00e4us weht's herauf.", "tokens": ["Und", "k\u00fch\u00b7ler", "vom", "Pi\u00b7r\u00e4us", "weht's", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mein Auge will noch einmal, eh' es sich", "tokens": ["Mein", "Au\u00b7ge", "will", "noch", "ein\u00b7mal", ",", "eh'", "es", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auf immer zuschlie\u00dft, ruhn auf dieser Stadt;", "tokens": ["Auf", "im\u00b7mer", "zu\u00b7schlie\u00dft", ",", "ruhn", "auf", "die\u00b7ser", "Stadt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "$,", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Denn \u00fcber alles hab' ich sie geliebt", "tokens": ["Denn", "\u00fc\u00b7ber", "al\u00b7les", "hab'", "ich", "sie", "ge\u00b7liebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "VAFIN", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und liebe sie noch heut in ihrer Not,", "tokens": ["Und", "lie\u00b7be", "sie", "noch", "heut", "in", "ih\u00b7rer", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wiewohl sie mein verga\u00df.", "tokens": ["Wie\u00b7wohl", "sie", "mein", "ver\u00b7ga\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O mein Athen,", "tokens": ["O", "mein", "A\u00b7then", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Juwel von Hellas, stolze Herrscherin", "tokens": ["Ju\u00b7wel", "von", "Hel\u00b7las", ",", "stol\u00b7ze", "Herr\u00b7sche\u00b7rin"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "APPR", "NE", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Meers und aller G\u00f6tter Liebling einst,", "tokens": ["Des", "Meers", "und", "al\u00b7ler", "G\u00f6t\u00b7ter", "Lieb\u00b7ling", "einst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PIAT", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "K\u00f6nnt' ich dich, Kodrus gleich, durch meinen Tod", "tokens": ["K\u00f6nnt'", "ich", "dich", ",", "Ko\u00b7drus", "gleich", ",", "durch", "mei\u00b7nen", "Tod"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "$,", "NE", "ADV", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vom Fluch erretten, der im fahlen Qualm", "tokens": ["Vom", "Fluch", "er\u00b7ret\u00b7ten", ",", "der", "im", "fah\u00b7len", "Qualm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVINF", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dumpfbr\u00fctend \u00fcber deinen Zinnen h\u00e4ngt,", "tokens": ["Dumpf\u00b7br\u00fc\u00b7tend", "\u00fc\u00b7ber", "dei\u00b7nen", "Zin\u00b7nen", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie freudig st\u00fcrb' ich! Doch es ward mir nicht", "tokens": ["Wie", "freu\u00b7dig", "st\u00fcrb'", "ich", "!", "Doch", "es", "ward", "mir", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "$.", "KON", "PPER", "VAFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So sch\u00f6n verg\u00f6nnt; die bleiche Stirne soll", "tokens": ["So", "sch\u00f6n", "ver\u00b7g\u00f6nnt", ";", "die", "blei\u00b7che", "Stir\u00b7ne", "soll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "$.", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Kein Kranz mir schm\u00fccken. Lautlos hingerafft,", "tokens": ["Kein", "Kranz", "mir", "schm\u00fc\u00b7cken", ".", "Laut\u00b7los", "hin\u00b7ge\u00b7rafft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVINF", "$.", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wie eine dunkle Well' im dunkeln Strom,", "tokens": ["Wie", "ei\u00b7ne", "dunk\u00b7le", "Well'", "im", "dun\u00b7keln", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Versink' ich mit im allgemeinen Leid.", "tokens": ["Ver\u00b7sink'", "ich", "mit", "im", "all\u00b7ge\u00b7mei\u00b7nen", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Weint nicht, ihr Treuen! Immer war's mein Stolz,", "tokens": ["Weint", "nicht", ",", "ihr", "Treu\u00b7en", "!", "Im\u00b7mer", "wa\u00b7r's", "mein", "Stolz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Da\u00df keines B\u00fcrgers Tr\u00e4ne jemals flo\u00df", "tokens": ["Da\u00df", "kei\u00b7nes", "B\u00fcr\u00b7gers", "Tr\u00e4\u00b7ne", "je\u00b7mals", "flo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um meinetwillen; la\u00dft mich diesen Ruhm", "tokens": ["Um", "mei\u00b7net\u00b7wil\u00b7len", ";", "la\u00dft", "mich", "die\u00b7sen", "Ruhm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "$.", "VVIMP", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bewahren bis ans Ende! Klagt auch nicht,", "tokens": ["Be\u00b7wah\u00b7ren", "bis", "ans", "En\u00b7de", "!", "Klagt", "auch", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$.", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df dies gest\u00e4hlte Herz, bevor es brach,", "tokens": ["Da\u00df", "dies", "ge\u00b7st\u00e4hl\u00b7te", "Herz", ",", "be\u00b7vor", "es", "brach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADJA", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Noch so viel Leid erfuhr. Es trifft der Gott", "tokens": ["Noch", "so", "viel", "Leid", "er\u00b7fuhr", ".", "Es", "trifft", "der", "Gott"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit sch\u00e4rfstem Pfeile, wen er einst erh\u00f6ht.", "tokens": ["Mit", "sch\u00e4rfs\u00b7tem", "Pfei\u00b7le", ",", "wen", "er", "einst", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und wenn mein Phidias im Kerker starb,", "tokens": ["Und", "wenn", "mein", "Phi\u00b7di\u00b7as", "im", "Ker\u00b7ker", "starb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wenn, der mit Milch der Weisheit mich gen\u00e4hrt,", "tokens": ["Wenn", ",", "der", "mit", "Milch", "der", "Weis\u00b7heit", "mich", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "APPR", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ge\u00e4chtet floh, wenn kleiner Ha\u00df sich frech", "tokens": ["Ge\u00e4ch\u00b7tet", "floh", ",", "wenn", "klei\u00b7ner", "Ha\u00df", "sich", "frech"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "$,", "KOUS", "ADJA", "NN", "PRF", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "An sie gewagt, die meine Muse war,", "tokens": ["An", "sie", "ge\u00b7wagt", ",", "die", "mei\u00b7ne", "Mu\u00b7se", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "So wi\u00dft: ich nehm' es hin als meines Gl\u00fccks", "tokens": ["So", "wi\u00dft", ":", "ich", "nehm'", "es", "hin", "als", "mei\u00b7nes", "Gl\u00fccks"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADV", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ausgleichung, und dafern ich allzu k\u00fchn,", "tokens": ["Aus\u00b7glei\u00b7chung", ",", "und", "da\u00b7fern", "ich", "all\u00b7zu", "k\u00fchn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "KOUS", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Verf\u00fchrt vom Reize des Gelingens, je", "tokens": ["Ver\u00b7f\u00fchrt", "vom", "Rei\u00b7ze", "des", "Ge\u00b7lin\u00b7gens", ",", "je"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVPP", "APPRART", "NN", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Mich \u00fcberhob, als Bu\u00dfe meiner Schuld.", "tokens": ["Mich", "\u00fc\u00b7berh\u00b7ob", ",", "als", "Bu\u00b7\u00dfe", "mei\u00b7ner", "Schuld", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Durch meine Seele dunkel mahnend t\u00f6nt", "tokens": ["Durch", "mei\u00b7ne", "See\u00b7le", "dun\u00b7kel", "mah\u00b7nend", "t\u00f6nt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Lied der Eumeniden, das ich nie", "tokens": ["Das", "Lied", "der", "Eu\u00b7me\u00b7ni\u00b7den", ",", "das", "ich", "nie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vergessen konnte. Z\u00fcrnend sang es mir,", "tokens": ["Ver\u00b7ges\u00b7sen", "konn\u00b7te", ".", "Z\u00fcr\u00b7nend", "sang", "es", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$.", "VVPP", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum Wanderstab schon greifend, \u00c4schylus,", "tokens": ["Zum", "Wan\u00b7der\u00b7stab", "schon", "grei\u00b7fend", ",", "\u00c4\u00b7schy\u00b7lus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "$,", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als ich die Pfleger fromm erstarrten Brauchs,", "tokens": ["Als", "ich", "die", "Pfle\u00b7ger", "fromm", "er\u00b7starr\u00b7ten", "Brauchs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Alten, von den Richterst\u00fchlen warf.", "tokens": ["Die", "Al\u00b7ten", ",", "von", "den", "Rich\u00b7ter\u00b7st\u00fch\u00b7len", "warf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Vielleicht, wenn damals ich mein Herz bez\u00e4hmt,", "tokens": ["Viel\u00b7leicht", ",", "wenn", "da\u00b7mals", "ich", "mein", "Herz", "be\u00b7z\u00e4hmt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Hinausgeschoben h\u00e4tt' ich diesen Tag", "tokens": ["Hin\u00b7aus\u00b7ge\u00b7scho\u00b7ben", "h\u00e4tt'", "ich", "die\u00b7sen", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und seine Not, vielleicht \u2013 vielleicht auch nicht!", "tokens": ["Und", "sei\u00b7ne", "Not", ",", "viel\u00b7leicht", "\u2013", "viel\u00b7leicht", "auch", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADV", "$(", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Denn viel ist Schicksal, was als Tat erscheint,", "tokens": ["Denn", "viel", "ist", "Schick\u00b7sal", ",", "was", "als", "Tat", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "$,", "PRELS", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und wie der Apfel, wenn kein Wind vom Ast", "tokens": ["Und", "wie", "der", "Ap\u00b7fel", ",", "wenn", "kein", "Wind", "vom", "Ast"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "KOUS", "PIAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ihn sch\u00fcttelt oder keine Hand ihn pfl\u00fcckt,", "tokens": ["Ihn", "sch\u00fct\u00b7telt", "o\u00b7der", "kei\u00b7ne", "Hand", "ihn", "pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Unwiderruflich gr\u00fcnt und reift und \u2013 fault,", "tokens": ["Un\u00b7wi\u00b7der\u00b7ruf\u00b7lich", "gr\u00fcnt", "und", "reift", "und", "\u2013", "fault", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "VVFIN", "KON", "$(", "VVFIN", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.14": {"text": "So gr\u00fcnt und reift und fault die Kraft des Volks,", "tokens": ["So", "gr\u00fcnt", "und", "reift", "und", "fault", "die", "Kraft", "des", "Volks", ","], "token_info": ["word", "word", "word", "word", "word", "word", "wor