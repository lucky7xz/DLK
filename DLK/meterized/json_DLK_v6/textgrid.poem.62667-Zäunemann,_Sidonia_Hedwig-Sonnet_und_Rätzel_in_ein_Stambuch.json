{"textgrid.poem.62667": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet und R\u00e4tzel in ein Stambuch", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich diene jedermann, dem B\u00fcrger und dem Bauer,", "tokens": ["Ich", "die\u00b7ne", "je\u00b7der\u00b7mann", ",", "dem", "B\u00fcr\u00b7ger", "und", "dem", "Bau\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich diene gro\u00df und klein, fr\u00fch, mittags und bey Nacht,", "tokens": ["Ich", "die\u00b7ne", "gro\u00df", "und", "klein", ",", "fr\u00fch", ",", "mit\u00b7tags", "und", "bey", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,", "ADJD", "$,", "ADV", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich werd auf jedem Schmau\u00df sehr reichlich dargebracht:", "tokens": ["Ich", "werd", "auf", "je\u00b7dem", "Schmau\u00df", "sehr", "reich\u00b7lich", "dar\u00b7ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich gleich der Bayren Tracht, und schicke mich zur Trauer,", "tokens": ["Ich", "gleich", "der", "Bay\u00b7ren", "Tracht", ",", "und", "schi\u00b7cke", "mich", "zur", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich gl\u00e4nz und bin so fest wie Steine an der Mauer.", "tokens": ["Ich", "gl\u00e4nz", "und", "bin", "so", "fest", "wie", "Stei\u00b7ne", "an", "der", "Mau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "KON", "VAFIN", "ADV", "ADJD", "KOKOM", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mich liebt das Frauen-Volk; man ist zugleich bedacht,", "tokens": ["Mich", "liebt", "das", "Frau\u00b7en\u00b7Volk", ";", "man", "ist", "zu\u00b7gleich", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PIS", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df man mich vor der Hand der Diebe wohl bewacht,", "tokens": ["Da\u00df", "man", "mich", "vor", "der", "Hand", "der", "Die\u00b7be", "wohl", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das Fleisch verfaulet bald; ich bin von be\u00dfrer Dauer.", "tokens": ["Das", "Fleisch", "ver\u00b7fau\u00b7let", "bald", ";", "ich", "bin", "von", "be\u00df\u00b7rer", "Dau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$.", "PPER", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es ist mein Vaterland den wenigsten bewust.", "tokens": ["Es", "ist", "mein", "Va\u00b7ter\u00b7land", "den", "we\u00b7nigs\u00b7ten", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.10": {"text": "Ich reise weit herum, und komm in viele L\u00e4nder,", "tokens": ["Ich", "rei\u00b7se", "weit", "he\u00b7rum", ",", "und", "komm", "in", "vie\u00b7le", "L\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich bin ein guter Arzt, darneben ein Verschwender.", "tokens": ["Ich", "bin", "ein", "gu\u00b7ter", "Arzt", ",", "dar\u00b7ne\u00b7ben", "ein", "Ver\u00b7schwen\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auf! hohlt zum Zeitvertreib, und zur beliebten Lust", "tokens": ["Auf", "!", "hohlt", "zum", "Zeit\u00b7ver\u00b7treib", ",", "und", "zur", "be\u00b7lieb\u00b7ten", "Lust"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$.", "VVFIN", "APPRART", "NN", "$,", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dergleichen jetzt herbey; ja wer mich wird ergr\u00fcnden,", "tokens": ["Derg\u00b7lei\u00b7chen", "jetzt", "her\u00b7bey", ";", "ja", "wer", "mich", "wird", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "PTKVZ", "$.", "ADV", "PWS", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Soll einen Zucker-Hut davor zur Gabe finden.", "tokens": ["Soll", "ei\u00b7nen", "Zu\u00b7cke\u00b7rHut", "da\u00b7vor", "zur", "Ga\u00b7be", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PAV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}