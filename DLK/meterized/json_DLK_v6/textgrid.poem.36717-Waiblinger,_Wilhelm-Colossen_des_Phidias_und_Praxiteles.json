{"textgrid.poem.36717": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Colossen des Phidias und Praxiteles", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Quirinalischer Stolz, Colossen des Monte Cavallo,", "tokens": ["Qui\u00b7ri\u00b7na\u00b7li\u00b7scher", "Stolz", ",", "Co\u00b7los\u00b7sen", "des", "Mon\u00b7te", "Ca\u00b7val\u00b7lo", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "ART", "NN", "NE", "$,"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wie ihr mir t\u00e4glich ersteigt, seid mir im Liede gegr\u00fc\u00dft!", "tokens": ["Wie", "ihr", "mir", "t\u00e4g\u00b7lich", "er\u00b7steigt", ",", "seid", "mir", "im", "Lie\u00b7de", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADJD", "VVPP", "$,", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ewiger Jugend Bilder, der Kraft erhabne Gestalten,", "tokens": ["E\u00b7wi\u00b7ger", "Ju\u00b7gend", "Bil\u00b7der", ",", "der", "Kraft", "er\u00b7hab\u00b7ne", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Blieb euch die Jugend, und blieb's mitten im alterndem Rom.", "tokens": ["Blieb", "euch", "die", "Ju\u00b7gend", ",", "und", "blie\u00b7b's", "mit\u00b7ten", "im", "al\u00b7tern\u00b7dem", "Rom", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "ADV", "APPRART", "ADJA", "NE", "$."], "meter": "+--+--+-+--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Ja, ich glaub's, eure V\u00e4ter sind nicht die griechischen Bildner,", "tokens": ["Ja", ",", "ich", "glaub's", ",", "eu\u00b7re", "V\u00e4\u00b7ter", "sind", "nicht", "die", "grie\u00b7chi\u00b7schen", "Bild\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "NE", "$,", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Sterbliche nicht, doch der Gott hat euch, der Donnrer, gezeugt.", "tokens": ["Sterb\u00b7li\u00b7che", "nicht", ",", "doch", "der", "Gott", "hat", "euch", ",", "der", "Donn\u00b7rer", ",", "ge\u00b7zeugt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "ADV", "ART", "NN", "VAFIN", "PPER", "$,", "ART", "NN", "$,", "VVPP", "$."], "meter": "+---+-+-+-+--+", "measure": "iambic.hexa.chol"}}}}}