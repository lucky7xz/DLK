{"dta.poem.8530": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Das Kind .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "W&#228;r' ich ein Kind, ein Kn&#228;blein klein,", "tokens": ["W", "&#228;", "r'", "ich", "ein", "Kind", ",", "ein", "Kn", "&#228;", "blein", "klein", ","], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "punct", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["XY", "$(", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$(", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ein armes, schwaches, geliebtes;", "tokens": ["Ein", "ar\u00b7mes", ",", "schwa\u00b7ches", ",", "ge\u00b7lieb\u00b7tes", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df noch die Mutter mich wiegte ein", "tokens": ["Da\u00df", "noch", "die", "Mut\u00b7ter", "mich", "wieg\u00b7te", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "ART"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und s\u00fc\u00dfe Lieder mir s\u00e4nge,", "tokens": ["Und", "s\u00fc\u00b7\u00dfe", "Lie\u00b7der", "mir", "s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Blumen br\u00e4chten die Sklavinnen auch,", "tokens": ["Blu\u00b7men", "br\u00e4ch\u00b7ten", "die", "Skla\u00b7vin\u00b7nen", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Mit dem Wedel wehrten die Fliegen,", "tokens": ["Mit", "dem", "We\u00b7del", "wehr\u00b7ten", "die", "Flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Aber Zillah, mich k\u00fcssend, spr\u00e4ch\u2019:", "tokens": ["A\u00b7ber", "Zil\u00b7lah", ",", "mich", "k\u00fcs\u00b7send", ",", "spr\u00e4ch'", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NE", "$,", "PPER", "ADJD", "$,", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "\u201egesegnet, mein s\u00fc\u00dfes Kn\u00e4bchen!\u201c", "tokens": ["\u201e", "ge\u00b7seg\u00b7net", ",", "mein", "s\u00fc\u00b7\u00dfes", "Kn\u00e4b\u00b7chen", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}