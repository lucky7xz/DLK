{"textgrid.poem.27983": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eine Dam ist mir bekant,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Dam ist mir bekant,", "tokens": ["Ei\u00b7ne", "Dam", "ist", "mir", "be\u00b7kant", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deutsch ist Zung und Vaterland;", "tokens": ["Deutsch", "ist", "Zung", "und", "Va\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann sie redet, mu\u00df dazwischen", "tokens": ["Wann", "sie", "re\u00b7det", ",", "mu\u00df", "da\u00b7zwi\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VMFIN", "PAV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Halb Latein sich untermischen;", "tokens": ["Halb", "La\u00b7tein", "sich", "un\u00b7ter\u00b7mi\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Drum ihr Name, solls so seyn,", "tokens": ["Drum", "ihr", "Na\u00b7me", ",", "solls", "so", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "$,", "VMFIN", "ADV", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Halb mu\u00df Deutsch seyn, halb Latein.", "tokens": ["Halb", "mu\u00df", "Deutsch", "seyn", ",", "halb", "La\u00b7tein."], "token_info": ["word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["ADJD", "VMFIN", "NN", "VAINF", "$,", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Da\u00df man mag ihr Art erkennen,", "tokens": ["Da\u00df", "man", "mag", "ihr", "Art", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wil ich sie Man-sueta nennen.", "tokens": ["Wil", "ich", "sie", "Man\u00b7su\u00b7e\u00b7ta", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "NE", "VVINF", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}}}}}