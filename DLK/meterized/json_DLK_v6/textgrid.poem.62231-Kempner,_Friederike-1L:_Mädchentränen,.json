{"textgrid.poem.62231": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: M\u00e4dchentr\u00e4nen,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4dchentr\u00e4nen,", "tokens": ["M\u00e4d\u00b7chen\u00b7tr\u00e4\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Schmerzlich Sehnen,", "tokens": ["Schmerz\u00b7lich", "Seh\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Perlen aus des Himmels Tau,", "tokens": ["Per\u00b7len", "aus", "des", "Him\u00b7mels", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden wei\u00df und werden grau.", "tokens": ["Wer\u00b7den", "wei\u00df", "und", "wer\u00b7den", "grau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "KON", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einer Seele weiches Sehnen,", "tokens": ["Ei\u00b7ner", "See\u00b7le", "wei\u00b7ches", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eines Herzens schmerzlich Dehnen,", "tokens": ["Ei\u00b7nes", "Her\u00b7zens", "schmerz\u00b7lich", "Deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hui, das ist so warm und kalt", "tokens": ["Hui", ",", "das", "ist", "so", "warm", "und", "kalt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "$,", "PDS", "VAFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie gespenstische Gestalt.", "tokens": ["Wie", "ge\u00b7spens\u00b7ti\u00b7sche", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Eisig schm\u00fcckt es alle W\u00e4nde,", "tokens": ["Ei\u00b7sig", "schm\u00fcckt", "es", "al\u00b7le", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Starr und freudlos alle H\u00e4nde,", "tokens": ["Starr", "und", "freud\u00b7los", "al\u00b7le", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles schweigend, alles stumm,", "tokens": ["Al\u00b7les", "schwei\u00b7gend", ",", "al\u00b7les", "stumm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "ADJD", "$,", "PIS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur der B\u00f6se geht herum!", "tokens": ["Nur", "der", "B\u00f6\u00b7se", "geht", "he\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}