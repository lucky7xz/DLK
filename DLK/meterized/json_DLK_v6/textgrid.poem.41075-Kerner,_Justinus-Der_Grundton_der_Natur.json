{"textgrid.poem.41075": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der Grundton der Natur", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Wald im Winde rauscht,", "tokens": ["Wenn", "der", "Wald", "im", "Win\u00b7de", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Blatt mit Blatt die Rede tauscht,", "tokens": ["Blatt", "mit", "Blatt", "die", "Re\u00b7de", "tauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6cht' ich gern die Bl\u00e4tter fragen:", "tokens": ["M\u00f6cht'", "ich", "gern", "die", "Bl\u00e4t\u00b7ter", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00f6nt ihr Wonnen? t\u00f6nt ihr Klagen?", "tokens": ["T\u00f6nt", "ihr", "Won\u00b7nen", "?", "t\u00f6nt", "ihr", "Kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Springt der Waldbach Tal entlang", "tokens": ["Springt", "der", "Wald\u00b7bach", "Tal", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "APPO"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit melodischem Gesang,", "tokens": ["Mit", "me\u00b7lo\u00b7di\u00b7schem", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frag' ich still in meinem Herzen:", "tokens": ["Frag'", "ich", "still", "in", "mei\u00b7nem", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Singt er Wonne? singt er Schmerzen?", "tokens": ["Singt", "er", "Won\u00b7ne", "?", "singt", "er", "Schmer\u00b7zen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$.", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lausch' der \u00c4olsharfe nur!", "tokens": ["Lausch'", "der", "\u00c4\u00b7ols\u00b7har\u00b7fe", "nur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmerz ist Grundton der Natur;", "tokens": ["Schmerz", "ist", "Grund\u00b7ton", "der", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schmerz des Waldes rauschend Singen,", "tokens": ["Schmerz", "des", "Wal\u00b7des", "rau\u00b7schend", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmerz \u2013 des Baches murmelnd Springen,", "tokens": ["Schmerz", "\u2013", "des", "Ba\u00b7ches", "mur\u00b7melnd", "Sprin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "NN", "VVPP", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und am meist aus Menschen Scherz", "tokens": ["Und", "am", "meist", "aus", "Men\u00b7schen", "Scherz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADV", "APPR", "NN", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "T\u00f6nt als Grundton Schmerz, nur Schmerz.", "tokens": ["T\u00f6nt", "als", "Grund\u00b7ton", "Schmerz", ",", "nur", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NE", "NN", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}