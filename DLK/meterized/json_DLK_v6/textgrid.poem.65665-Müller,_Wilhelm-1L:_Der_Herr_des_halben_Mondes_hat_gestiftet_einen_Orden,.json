{"textgrid.poem.65665": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Herr des halben Mondes hat gestiftet einen Orden,", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herr des halben Mondes hat gestiftet einen Orden,", "tokens": ["Der", "Herr", "des", "hal\u00b7ben", "Mon\u00b7des", "hat", "ge\u00b7stif\u00b7tet", "ei\u00b7nen", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Ein Kreuz f\u00fcr alle Christen, die ihm Christen helfen morden,", "tokens": ["Ein", "Kreuz", "f\u00fcr", "al\u00b7le", "Chris\u00b7ten", ",", "die", "ihm", "Chris\u00b7ten", "hel\u00b7fen", "mor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "F\u00fcr alle, die der Freiheit Haupt in's Joch ihm helfen beugen,", "tokens": ["F\u00fcr", "al\u00b7le", ",", "die", "der", "Frei\u00b7heit", "Haupt", "in's", "Joch", "ihm", "hel\u00b7fen", "beu\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ART", "NN", "NN", "APPRART", "NN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Und lehren, da\u00df das heil'ge Kreuz soll vor dem Mond sich neigen.", "tokens": ["Und", "leh\u00b7ren", ",", "da\u00df", "das", "heil'\u00b7ge", "Kreuz", "soll", "vor", "dem", "Mond", "sich", "nei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "KOUS", "ART", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Hervor, ihr Ritter allzumal! Hervor aus allen Ecken!", "tokens": ["Her\u00b7vor", ",", "ihr", "Rit\u00b7ter", "all\u00b7zu\u00b7mal", "!", "Her\u00b7vor", "aus", "al\u00b7len", "E\u00b7cken", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PPOSAT", "NN", "ADV", "$.", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Mein Lied soll eurer Thaten Ruf mit hellem Klang erwecken.", "tokens": ["Mein", "Lied", "soll", "eu\u00b7rer", "Tha\u00b7ten", "Ruf", "mit", "hel\u00b7lem", "Klang", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPOSAT", "NN", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Hervor, der du mit frechem Mund die Freiheit nennst Emp\u00f6rung,", "tokens": ["Her\u00b7vor", ",", "der", "du", "mit", "fre\u00b7chem", "Mund", "die", "Frei\u00b7heit", "nennst", "Em\u00b7p\u00f6\u00b7rung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Und der Hellenen Heldenkampf bejammerst als Beth\u00f6rung!", "tokens": ["Und", "der", "Hel\u00b7le\u00b7nen", "Hel\u00b7den\u00b7kampf", "be\u00b7jam\u00b7merst", "als", "Be\u00b7th\u00f6\u00b7rung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Du, der mit feiner Politik du drechselst die Beweise,", "tokens": ["Du", ",", "der", "mit", "fei\u00b7ner", "Po\u00b7li\u00b7tik", "du", "drech\u00b7selst", "die", "Be\u00b7wei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "ADJA", "NN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Da\u00df man die Menschheit w\u00fcrgen kann auf legitime Weise!", "tokens": ["Da\u00df", "man", "die", "Menschheit", "w\u00fcr\u00b7gen", "kann", "auf", "le\u00b7gi\u00b7ti\u00b7me", "Wei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVINF", "VMFIN", "APPR", "ADJA", "NN", "$."], "meter": "++--+--+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Du auch, der jeden T\u00fcrkensieg verk\u00fcndet mit Posaunen,", "tokens": ["Du", "auch", ",", "der", "je\u00b7den", "T\u00fcr\u00b7ken\u00b7sieg", "ver\u00b7k\u00fcn\u00b7det", "mit", "Po\u00b7sau\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PIAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "Und was der Griechen Schwert vollbracht, befleckt mit leisem Raunen!", "tokens": ["Und", "was", "der", "Grie\u00b7chen", "Schwert", "voll\u00b7bracht", ",", "be\u00b7fleckt", "mit", "lei\u00b7sem", "Rau\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "NE", "VVPP", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.13": {"text": "Ihr alle, die durch Meer und Land die blinden Heiden leiten,", "tokens": ["Ihr", "al\u00b7le", ",", "die", "durch", "Meer", "und", "Land", "die", "blin\u00b7den", "Hei\u00b7den", "lei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PRELS", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "Und ihre Heere christlich klug mit Christen lehren streiten!", "tokens": ["Und", "ih\u00b7re", "Hee\u00b7re", "christ\u00b7lich", "klug", "mit", "Chris\u00b7ten", "leh\u00b7ren", "strei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "ADJD", "APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.15": {"text": "Ihr, die ihr \u00f6ffnet euern Arm den fl\u00fcchtigen Barbaren,", "tokens": ["Ihr", ",", "die", "ihr", "\u00f6ff\u00b7net", "eu\u00b7ern", "Arm", "den", "fl\u00fcch\u00b7ti\u00b7gen", "Bar\u00b7ba\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.16": {"text": "Und unter eurer Flagge Hut sie f\u00fchrt aus den Gefahren,", "tokens": ["Und", "un\u00b7ter", "eu\u00b7rer", "Flag\u00b7ge", "Hut", "sie", "f\u00fchrt", "aus", "den", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.17": {"text": "Und die ihr dann vor\u00fcberschifft, wo an der Mutter Br\u00fcsten", "tokens": ["Und", "die", "ihr", "dann", "vor\u00b7\u00fc\u00b7bers\u00b7chifft", ",", "wo", "an", "der", "Mut\u00b7ter", "Br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "ADV", "VVFIN", "$,", "PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.18": {"text": "Der Islamit den S\u00e4ugling w\u00fcrgt mit wilden Henkerl\u00fcsten!", "tokens": ["Der", "Is\u00b7la\u00b7mit", "den", "S\u00e4ug\u00b7ling", "w\u00fcrgt", "mit", "wil\u00b7den", "Hen\u00b7ker\u00b7l\u00fcs\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.19": {"text": "Hervor, ihr Ritter allzumal! \u2013 Will denn die Schaar nicht enden?", "tokens": ["Her\u00b7vor", ",", "ihr", "Rit\u00b7ter", "all\u00b7zu\u00b7mal", "!", "\u2013", "Will", "denn", "die", "Schaar", "nicht", "en\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PPOSAT", "NN", "ADV", "$.", "$(", "VMFIN", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Das wird einmal ein Kreuzzug sein, wenn ", "tokens": ["Das", "wird", "ein\u00b7mal", "ein", "Kreuz\u00b7zug", "sein", ",", "wenn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "VAINF", "$,", "KOUS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}