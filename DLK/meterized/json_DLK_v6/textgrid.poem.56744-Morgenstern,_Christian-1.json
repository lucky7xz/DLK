{"textgrid.poem.56744": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mond steht da", "tokens": ["Der", "Mond", "steht", "da"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "wie ein alter van Dyck:", "tokens": ["wie", "ein", "al\u00b7ter", "van", "Dyck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NE", "NE", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "ein rundes, gutm\u00fctiges", "tokens": ["ein", "run\u00b7des", ",", "gut\u00b7m\u00fc\u00b7ti\u00b7ges"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "ADJA", "$,", "ADJA"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Holl\u00e4ndergesicht", "tokens": ["Hol\u00b7l\u00e4n\u00b7der\u00b7ge\u00b7sicht"], "token_info": ["word"], "pos": ["NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "mit einer m\u00e4chtigen,", "tokens": ["mit", "ei\u00b7ner", "m\u00e4ch\u00b7ti\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "m\u00fchlsteinartigen,", "tokens": ["m\u00fchl\u00b7stein\u00b7ar\u00b7ti\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "cr\u00eamefarbenen", "tokens": ["cr\u00ea\u00b7me\u00b7fa\u00b7rbe\u00b7nen"], "token_info": ["word"], "pos": ["NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Halskrause.", "tokens": ["Hals\u00b7krau\u00b7se", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Ich m\u00f6cht ihn", "tokens": ["Ich", "m\u00f6cht", "ihn"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "wohl kaufen,", "tokens": ["wohl", "kau\u00b7fen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "den alten van Dyck!", "tokens": ["den", "al\u00b7ten", "van", "Dyck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Aber ich f\u00fcrchte,", "tokens": ["A\u00b7ber", "ich", "f\u00fcrch\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "er ist im Privatbesitz", "tokens": ["er", "ist", "im", "Pri\u00b7vat\u00b7be\u00b7sitz"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.14": {"text": "des Herrn Zebaoth.", "tokens": ["des", "Herrn", "Ze\u00b7bao\u00b7th", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-++-+", "measure": "unknown.measure.tri"}, "line.15": {"text": "Ich m\u00fc\u00dfte den Abla\u00df", "tokens": ["Ich", "m\u00fc\u00df\u00b7te", "den", "Ab\u00b7la\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "wieder in Schwang bringen!", "tokens": ["wie\u00b7der", "in", "Schwang", "brin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Vielleicht lie\u00df er ihn", "tokens": ["Viel\u00b7leicht", "lie\u00df", "er", "ihn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "daf\u00fcr mir ab ...", "tokens": ["da\u00b7f\u00fcr", "mir", "ab", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PTKVZ", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Hm.", "tokens": ["Hm", "."], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "+", "measure": "single.up"}, "line.20": {"text": "Hm.", "tokens": ["Hm", "."], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "+", "measure": "single.up"}}}}}