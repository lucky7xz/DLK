{"textgrid.poem.44178": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Las dich deine S\u00f6hne k\u00fc\u00dfen,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Las dich deine S\u00f6hne k\u00fc\u00dfen,", "tokens": ["Las", "dich", "dei\u00b7ne", "S\u00f6h\u00b7ne", "k\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Atlas unsers Heiligthums,", "tokens": ["At\u00b7las", "un\u00b7sers", "Hei\u00b7lig\u00b7thums", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Obgleich Erd und Himmel wi\u00dfen,", "tokens": ["Ob\u00b7gleich", "Erd", "und", "Him\u00b7mel", "wi\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df die Ehre deines Ruhms", "tokens": ["Da\u00df", "die", "Eh\u00b7re", "dei\u00b7nes", "Ruhms"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch von Kl\u00fcgsten dieser Erden", "tokens": ["Auch", "von", "Kl\u00fcgs\u00b7ten", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht recht kan gepriesen werden.", "tokens": ["Nicht", "recht", "kan", "ge\u00b7prie\u00b7sen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VMFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Salems fest gesezte Mauren,", "tokens": ["Sa\u00b7lems", "fest", "ge\u00b7sez\u00b7te", "Mau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unser Abel, deine Lust,", "tokens": ["Un\u00b7ser", "A\u00b7bel", ",", "dei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fc\u00dfen forthin ewig dauren,", "tokens": ["M\u00fc\u00b7\u00dfen", "for\u00b7thin", "e\u00b7wig", "dau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil Eli\u00e4 Geist und Brust,", "tokens": ["Weil", "E\u00b7li\u00e4", "Geist", "und", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dein so unabl\u00e4slichs Bethen,", "tokens": ["Dein", "so", "un\u00b7ab\u00b7l\u00e4s\u00b7lichs", "Be\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vor die Ri\u00df und Br\u00fcche treten.", "tokens": ["Vor", "die", "Ri\u00df", "und", "Br\u00fc\u00b7che", "tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Deine Zunge lehrt ein Leben,", "tokens": ["Dei\u00b7ne", "Zun\u00b7ge", "lehrt", "ein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Goldner Mund, das ewig w\u00e4hrt;", "tokens": ["Gold\u00b7ner", "Mund", ",", "das", "e\u00b7wig", "w\u00e4hrt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wir sind die zarten Reben,", "tokens": ["Und", "wir", "sind", "die", "zar\u00b7ten", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die dein milder Seegen n\u00e4hrt.", "tokens": ["Die", "dein", "mil\u00b7der", "See\u00b7gen", "n\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die Bl\u00e4tter Fr\u00fcchte bringen,", "tokens": ["Wenn", "die", "Bl\u00e4t\u00b7ter", "Fr\u00fcch\u00b7te", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wollen wir weit be\u00dfer singen.", "tokens": ["Wol\u00b7len", "wir", "weit", "be\u00b7\u00dfer", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADJD", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Jezt schm\u00fcckt Unschuld unsern Willen,", "tokens": ["Jezt", "schm\u00fcckt", "Un\u00b7schuld", "un\u00b7sern", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPOSAT", "NN", "$,"], "meter": "+++-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Unvergleichlich holder Mann,", "tokens": ["Un\u00b7ver\u00b7gleich\u00b7lich", "hol\u00b7der", "Mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn dich kan das Herze stillen,", "tokens": ["Denn", "dich", "kan", "das", "Her\u00b7ze", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PDS", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn man nichts mehr leisten kan.", "tokens": ["Wenn", "man", "nichts", "mehr", "leis\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dein Ruhm baut dir Ehrenbogen,", "tokens": ["Dein", "Ruhm", "baut", "dir", "Eh\u00b7ren\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Bleib nur unserm Wuntsch gewogen.", "tokens": ["Bleib", "nur", "un\u00b7serm", "Wunt\u00b7sch", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Gott hat dich wohl angeschrieben,", "tokens": ["Gott", "hat", "dich", "wohl", "an\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jacobs Heerde folget dir,", "tokens": ["Ja\u00b7cobs", "Heer\u00b7de", "fol\u00b7get", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder mu\u00df dein Thummim lieben,", "tokens": ["Je\u00b7der", "mu\u00df", "dein", "Thum\u00b7mim", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil dein Urim f\u00fcr und f\u00fcr", "tokens": ["Weil", "dein", "U\u00b7rim", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "KON", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf dem frommen Herzen brennet,", "tokens": ["Auf", "dem", "from\u00b7men", "Her\u00b7zen", "bren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das kein w\u00fcstes Babel kennet.", "tokens": ["Das", "kein", "w\u00fcs\u00b7tes", "Ba\u00b7bel", "ken\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Also wachs in tausend \u00c4ste,", "tokens": ["Al\u00b7so", "wachs", "in", "tau\u00b7send", "\u00c4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ceder unsers Libanons,", "tokens": ["Ce\u00b7der", "un\u00b7sers", "Li\u00b7ba\u00b7nons", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Himmel thu das Beste", "tokens": ["Und", "der", "Him\u00b7mel", "thu", "das", "Bes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Nebst dir zu dem Bau Sions:", "tokens": ["Nebst", "dir", "zu", "dem", "Bau", "Si\u00b7ons", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So wird stets dein Nahme gr\u00fcnen", "tokens": ["So", "wird", "stets", "dein", "Nah\u00b7me", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und Perpetua dir dienen.", "tokens": ["Und", "Per\u00b7pe\u00b7tua", "dir", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}