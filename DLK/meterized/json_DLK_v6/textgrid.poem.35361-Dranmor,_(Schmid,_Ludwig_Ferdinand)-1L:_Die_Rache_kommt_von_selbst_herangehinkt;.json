{"textgrid.poem.35361": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Rache kommt von selbst herangehinkt;", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Rache kommt von selbst herangehinkt;", "tokens": ["Die", "Ra\u00b7che", "kommt", "von", "selbst", "her\u00b7an\u00b7ge\u00b7hinkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Fatum w\u00fctet in der Weltgemeinde,", "tokens": ["Ein", "Fa\u00b7tum", "w\u00fc\u00b7tet", "in", "der", "Welt\u00b7ge\u00b7mein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem jeder Sch\u00e4cher in die Arme sinkt.", "tokens": ["Dem", "je\u00b7der", "Sch\u00e4\u00b7cher", "in", "die", "Ar\u00b7me", "sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich k\u00f6nnte auf den Gr\u00e4bern meiner Feinde", "tokens": ["Ich", "k\u00f6nn\u00b7te", "auf", "den", "Gr\u00e4\u00b7bern", "mei\u00b7ner", "Fein\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Hosianna unter Trauerweiden,", "tokens": ["Ein", "Ho\u00b7si\u00b7an\u00b7na", "un\u00b7ter", "Trau\u00b7er\u00b7wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein stolzes Jubellied ert\u00f6nen lassen;", "tokens": ["Ein", "stol\u00b7zes", "Ju\u00b7bel\u00b7lied", "er\u00b7t\u00f6\u00b7nen", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch will ich sonder Jubel mich bescheiden \u2013", "tokens": ["Doch", "will", "ich", "son\u00b7der", "Ju\u00b7bel", "mich", "be\u00b7schei\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJA", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es lohnt sich nur zu lieben, nicht zu hassen.", "tokens": ["Es", "lohnt", "sich", "nur", "zu", "lie\u00b7ben", ",", "nicht", "zu", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "PTKZU", "VVINF", "$,", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}