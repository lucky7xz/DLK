{"textgrid.poem.61707": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Traumleben", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O hast du niemals selbstvergessen", "tokens": ["O", "hast", "du", "nie\u00b7mals", "selbst\u00b7ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf d\u00fcrrem Moos und Farrenkraut", "tokens": ["Auf", "d\u00fcr\u00b7rem", "Moos", "und", "Far\u00b7ren\u00b7kraut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Wald am Wassersturz gesessen", "tokens": ["Im", "Wald", "am", "Was\u00b7ser\u00b7sturz", "ge\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schweigend in die Flut geschaut?", "tokens": ["Und", "schwei\u00b7gend", "in", "die", "Flut", "ge\u00b7schaut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du sahst die Welle nahn und sch\u00e4umen,", "tokens": ["Du", "sahst", "die", "Wel\u00b7le", "nahn", "und", "sch\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du sahst sie schimmernd weiterziehn,", "tokens": ["Du", "sahst", "sie", "schim\u00b7mernd", "wei\u00b7ter\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dich befing ein waches Tr\u00e4umen,", "tokens": ["Und", "dich", "be\u00b7fing", "ein", "wa\u00b7ches", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In dem dir doch kein Bild erschien.", "tokens": ["In", "dem", "dir", "doch", "kein", "Bild", "er\u00b7schien", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und Stunden kamen, Stunden gingen,", "tokens": ["Und", "Stun\u00b7den", "ka\u00b7men", ",", "Stun\u00b7den", "gin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch du vernahmst nicht ihren Schritt,", "tokens": ["Doch", "du", "ver\u00b7nahmst", "nicht", "ih\u00b7ren", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du warst verloren in den Dingen", "tokens": ["Du", "warst", "ver\u00b7lo\u00b7ren", "in", "den", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und webtest, walltest, rauschtest mit.", "tokens": ["Und", "web\u00b7test", ",", "wall\u00b7test", ",", "rauschtest", "mit", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Ja, ganz als ob euch nichts mehr schiede,", "tokens": ["Ja", ",", "ganz", "als", "ob", "euch", "nichts", "mehr", "schie\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "KOKOM", "KOUS", "PPER", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Empfand sich deine Seele nur", "tokens": ["Emp\u00b7fand", "sich", "dei\u00b7ne", "See\u00b7le", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als einen Laut noch in dem Liede", "tokens": ["Als", "ei\u00b7nen", "Laut", "noch", "in", "dem", "Lie\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "APPR", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der allumfangenden Natur;", "tokens": ["Der", "al\u00b7lum\u00b7fan\u00b7gen\u00b7den", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da war kein Drau\u00dfen mehr, kein Drinnen,", "tokens": ["Da", "war", "kein", "Drau\u00b7\u00dfen", "mehr", ",", "kein", "Drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ADV", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du schwebtest, frei vom Bann der Zeit,", "tokens": ["Du", "schweb\u00b7test", ",", "frei", "vom", "Bann", "der", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ausruhend mit gel\u00f6sten Sinnen", "tokens": ["Aus\u00b7ru\u00b7hend", "mit", "ge\u00b7l\u00f6s\u00b7ten", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Scho\u00dfe der Unendlichkeit.", "tokens": ["Im", "Scho\u00b7\u00dfe", "der", "Un\u00b7end\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}