{"dta.poem.4161": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Des Finken Abschied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der war so frisch und bl\u00e4tterreich,", "tokens": ["Der", "war", "so", "frisch", "und", "bl\u00e4t\u00b7ter\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sang wohl Dies und Jenes:", "tokens": ["Und", "sang", "wohl", "Dies", "und", "Je\u00b7nes", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PDS", "KON", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch Lenz und Sommer und Herbst er sang,", "tokens": ["Durch", "Lenz", "und", "Som\u00b7mer", "und", "Herbst", "er", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "H\u00e4tt' da gesungen sein Lebelang,", "tokens": ["H\u00e4tt'", "da", "ge\u00b7sun\u00b7gen", "sein", "Le\u00b7be\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVPP", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "W\u00e4r' nicht der Winter kommen.", "tokens": ["W\u00e4r'", "nicht", "der", "Win\u00b7ter", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Winter kam mit Saus und Braus:", "tokens": ["Der", "Win\u00b7ter", "kam", "mit", "Saus", "und", "Braus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eihr M\u00fc\u00dfigg\u00e4nger, zum Reich heraus,", "tokens": ["\u201e", "ihr", "M\u00fc\u00b7\u00dfig\u00b7g\u00e4n\u00b7ger", ",", "zum", "Reich", "he\u00b7raus", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u201eihr Flattrer und S\u00e4nger und Horcher!", "tokens": ["\u201e", "ihr", "Flatt\u00b7rer", "und", "S\u00e4n\u00b7ger", "und", "Hor\u00b7cher", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "\u201eherab vom Baum, du gr\u00fcnes Blatt!", "tokens": ["\u201e", "her\u00b7ab", "vom", "Baum", ",", "du", "gr\u00fc\u00b7nes", "Blatt", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPRART", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201ezum Bauen und zum Brennen hat", "tokens": ["\u201e", "zum", "Bau\u00b7en", "und", "zum", "Bren\u00b7nen", "hat"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPRART", "NN", "KON", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201eder Herr das Holz erschaffen.\u201c", "tokens": ["\u201e", "der", "Herr", "das", "Holz", "er\u00b7schaf\u00b7fen", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da geht im Hain das Sch\u00fctteln los,", "tokens": ["Da", "geht", "im", "Hain", "das", "Sch\u00fct\u00b7teln", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und flugs steht Alles blank und blo\u00df,", "tokens": ["Und", "flugs", "steht", "Al\u00b7les", "blank", "und", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PTKVZ", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis auf den Zweig des Finken.", "tokens": ["Bis", "auf", "den", "Zweig", "des", "Fin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jetzt, naseweises V\u00f6glein, flieh!", "tokens": ["Jetzt", ",", "na\u00b7se\u00b7wei\u00b7ses", "V\u00f6\u00b7glein", ",", "flieh", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit solcher Staats\u00f6konomie", "tokens": ["Mit", "sol\u00b7cher", "Staats\u00b7\u00f6\u00b7ko\u00b7no\u00b7mie"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da ist nicht viel zu spa\u00dfen.", "tokens": ["Da", "ist", "nicht", "viel", "zu", "spa\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und's V\u00f6glein flog und sang: Ade!", "tokens": ["Un\u00b7d's", "V\u00f6\u00b7glein", "flog", "und", "sang", ":", "A\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "KON", "VVFIN", "$.", "NN", "$."], "meter": "+-+-+-++-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Da warf der Winter Reif und Schnee", "tokens": ["Da", "warf", "der", "Win\u00b7ter", "Reif", "und", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm hinterdrein, und traf's nicht.", "tokens": ["Ihm", "hin\u00b7ter\u00b7drein", ",", "und", "traf's", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Finke lacht aus voller Kehl':", "tokens": ["Der", "Fin\u00b7ke", "lacht", "aus", "vol\u00b7ler", "Kehl'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bewahre Gott jede Christenseel'", "tokens": ["Be\u00b7wah\u00b7re", "Gott", "je\u00b7de", "Chris\u00b7ten\u00b7seel'"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "PIAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Vor diesem Landesvater!", "tokens": ["Vor", "die\u00b7sem", "Lan\u00b7des\u00b7va\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und als ich mal nach Welschland zog,", "tokens": ["Und", "als", "ich", "mal", "nach", "Wel\u00b7schland", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Manch V\u00f6glein mit dem Wandrer flog,", "tokens": ["Manch", "V\u00f6\u00b7glein", "mit", "dem", "Wand\u00b7rer", "flog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da war auch jenes drunter:", "tokens": ["Da", "war", "auch", "je\u00b7nes", "drun\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PDAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und w\u00e4r's gewest eine Nachtigall,", "tokens": ["Und", "w\u00e4r's", "ge\u00b7west", "ei\u00b7ne", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So h\u00e4tt' mein Lied einen bessern Schall,", "tokens": ["So", "h\u00e4tt'", "mein", "Lied", "ei\u00b7nen", "bes\u00b7sern", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ich hab's ihm nachgesungen.", "tokens": ["Ich", "hab's", "ihm", "nach\u00b7ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}