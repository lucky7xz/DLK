{"textgrid.poem.44412": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Sinnpflanze", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh, wie sich die Blumen freun!", "tokens": ["Sieh", ",", "wie", "sich", "die", "Blu\u00b7men", "freun", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle \u00f6ffnen ihre Bl\u00e4tter", "tokens": ["Al\u00b7le", "\u00f6ff\u00b7nen", "ih\u00b7re", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Sonne warmem Strahl;", "tokens": ["In", "der", "Son\u00b7ne", "war\u00b7mem", "Strahl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Du allein nur bleibst verschlossen?", "tokens": ["Du", "al\u00b7lein", "nur", "bleibst", "ver\u00b7schlos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bist du f\u00fchllos? freust dich nicht? \u2013", "tokens": ["Bist", "du", "f\u00fchl\u00b7los", "?", "freust", "dich", "nicht", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$.", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00bbf\u00fchllos nun gerade nicht!", "tokens": ["\u00bb", "f\u00fchl\u00b7los", "nun", "ge\u00b7ra\u00b7de", "nicht", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ADV", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Will mich auch wohl wieder \u00f6ffnen,", "tokens": ["Will", "mich", "auch", "wohl", "wie\u00b7der", "\u00f6ff\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur hat mich, eh du gekommen,", "tokens": ["Nur", "hat", "mich", ",", "eh", "du", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Tastend eine Hand ber\u00fchrt.\u00ab", "tokens": ["Tas\u00b7tend", "ei\u00b7ne", "Hand", "be\u00b7r\u00fchrt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}