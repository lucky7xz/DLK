{"textgrid.poem.54637": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "51. Stephani Tod", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Jacob auff dem Stein entschliff; erschin ihm Gott/", "tokens": ["Als", "Ja\u00b7cob", "auff", "dem", "Stein", "ent\u00b7schliff", ";", "er\u00b7schin", "ihm", "Gott", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "VVFIN", "$.", "ADV", "PPER", "NN", "$("], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Umbgeben mit dem Heer der lichten Seraphinen.", "tokens": ["U\u00b7mbge\u00b7ben", "mit", "dem", "Heer", "der", "lich\u00b7ten", "Se\u00b7ra\u00b7phi\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Nun du auff Steinen ligst erleichtert Tod und Spott", "tokens": ["Nun", "du", "auff", "Stei\u00b7nen", "ligst", "er\u00b7leich\u00b7tert", "Tod", "und", "Spott"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPR", "NN", "VVFIN", "VVPP", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gott und sein Jesus der dir wachend ist erschinen.", "tokens": ["Gott", "und", "sein", "Je\u00b7sus", "der", "dir", "wa\u00b7chend", "ist", "er\u00b7schi\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPOSAT", "NN", "ART", "PPER", "VVPP", "VAFIN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Er salbete den Stein zum Denckmal und Altar/", "tokens": ["Er", "sal\u00b7be\u00b7te", "den", "Stein", "zum", "Denck\u00b7mal", "und", "Al\u00b7tar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du salbst mit Blut und gibst dich selbst zum Opfer dar.", "tokens": ["Du", "salbst", "mit", "Blut", "und", "gibst", "dich", "selbst", "zum", "Op\u00b7fer", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}