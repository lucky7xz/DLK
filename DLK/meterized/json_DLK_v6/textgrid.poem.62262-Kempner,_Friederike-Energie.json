{"textgrid.poem.62262": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Energie", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weine nicht, klage nicht,", "tokens": ["Wei\u00b7ne", "nicht", ",", "kla\u00b7ge", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Weinen, klagen hilft ja nicht.", "tokens": ["Wei\u00b7nen", ",", "kla\u00b7gen", "hilft", "ja", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVINF", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Legt die geist'gen Kr\u00e4fte an,", "tokens": ["Legt", "die", "geist'\u00b7gen", "Kr\u00e4f\u00b7te", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wag' die ganze Seele dran.", "tokens": ["Wag'", "die", "gan\u00b7ze", "See\u00b7le", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weichen wird, was zwischen liegt:", "tokens": ["Wei\u00b7chen", "wird", ",", "was", "zwi\u00b7schen", "liegt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PRELS", "APPR", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur der Tapfere, er siegt!", "tokens": ["Nur", "der", "Tap\u00b7fe\u00b7re", ",", "er", "siegt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}