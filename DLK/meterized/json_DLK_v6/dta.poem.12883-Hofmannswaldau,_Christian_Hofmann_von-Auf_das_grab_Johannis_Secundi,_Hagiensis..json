{"dta.poem.12883": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf das grab   Johannis Secundi,  \n Hagiensis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer liegt die kluge faust, so Julien geschnitzt:", "tokens": ["Hjer", "liegt", "die", "klu\u00b7ge", "faust", ",", "so", "Ju\u00b7li\u00b7en", "ge\u00b7schnitzt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das, was Ne\u00e4rens ku\u00df erqvicket und erhitzt,", "tokens": ["Das", ",", "was", "Ne\u00e4\u00b7rens", "ku\u00df", "er\u00b7qvic\u00b7ket", "und", "er\u00b7hitzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "NE", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Mu\u00df hier den kalten stanb der schweren erde k\u00fc\u00dfen.", "tokens": ["Mu\u00df", "hier", "den", "kal\u00b7ten", "stanb", "der", "schwe\u00b7ren", "er\u00b7de", "k\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach, Leser! weine doch, und k\u00fc\u00dfe dessen grab,", "tokens": ["Ach", ",", "Le\u00b7ser", "!", "wei\u00b7ne", "doch", ",", "und", "k\u00fc\u00b7\u00dfe", "des\u00b7sen", "grab", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$.", "VVFIN", "ADV", "$,", "KON", "VVFIN", "PDS", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der lauter honig lie\u00df aus seiner feder flie\u00dfen,", "tokens": ["Der", "lau\u00b7ter", "ho\u00b7nig", "lie\u00df", "aus", "sei\u00b7ner", "fe\u00b7der", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und der gelehrten welt so s\u00fc\u00dfe k\u00fc\u00dfe gab.", "tokens": ["Und", "der", "ge\u00b7lehr\u00b7ten", "welt", "so", "s\u00fc\u00b7\u00dfe", "k\u00fc\u00b7\u00dfe", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}