{"textgrid.poem.43369": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gesperret ist das gro\u00dfe Reich des Zaren", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gesperret ist das gro\u00dfe Reich des Zaren", "tokens": ["Ge\u00b7sper\u00b7ret", "ist", "das", "gro\u00b7\u00dfe", "Reich", "des", "Za\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An allen Enden allen fremden Waaren;", "tokens": ["An", "al\u00b7len", "En\u00b7den", "al\u00b7len", "frem\u00b7den", "Waa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein unbestechbar gro\u00df Kosackenheer", "tokens": ["Ein", "un\u00b7be\u00b7stech\u00b7bar", "gro\u00df", "Ko\u00b7sa\u00b7cken\u00b7heer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "H\u00e4lt ab den fremden Handel und Verkehr.", "tokens": ["H\u00e4lt", "ab", "den", "frem\u00b7den", "Han\u00b7del", "und", "Ver\u00b7kehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch gn\u00e4digst hat der wei\u00dfe Zar befohlen:", "tokens": ["Doch", "gn\u00e4\u00b7digst", "hat", "der", "wei\u00b7\u00dfe", "Zar", "be\u00b7foh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbihr k\u00f6nnet euch aus Ru\u00dfland alles holen!", "tokens": ["\u00bb", "ihr", "k\u00f6n\u00b7net", "euch", "aus", "Ru\u00df\u00b7land", "al\u00b7les", "ho\u00b7len", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "APPR", "NE", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur Esel, Lumpen, Knut' und Kantschu nicht,", "tokens": ["Nur", "E\u00b7sel", ",", "Lum\u00b7pen", ",", "Knut'", "und", "Kant\u00b7schu", "nicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weil's uns an solchen Dingen noch gebricht.\u00ab", "tokens": ["Weil's", "uns", "an", "sol\u00b7chen", "Din\u00b7gen", "noch", "ge\u00b7bricht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PPER", "APPR", "PIAT", "NN", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}