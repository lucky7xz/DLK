{"dta.poem.7376": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Geyerpfiff.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, lieblich ist des Berges Maid", "tokens": ["Ja", ",", "lieb\u00b7lich", "ist", "des", "Ber\u00b7ges", "Maid"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADJD", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ihrer festen Glieder Pracht,", "tokens": ["In", "ih\u00b7rer", "fes\u00b7ten", "Glie\u00b7der", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In ihrer blanken Fr\u00f6hlichkeit", "tokens": ["In", "ih\u00b7rer", "blan\u00b7ken", "Fr\u00f6h\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihrer Z\u00f6pfe Rabennacht;", "tokens": ["Und", "ih\u00b7rer", "Z\u00f6p\u00b7fe", "Ra\u00b7ben\u00b7nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Siehst du sie brechen durch's Genist", "tokens": ["Siehst", "du", "sie", "bre\u00b7chen", "durch's", "Ge\u00b7nist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Brombeerranken, frisch, gedrungen,", "tokens": ["Der", "Brom\u00b7beer\u00b7ran\u00b7ken", ",", "frisch", ",", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du denkst, die Centifolie ist", "tokens": ["Du", "denkst", ",", "die", "Cen\u00b7ti\u00b7fo\u00b7lie", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vor Uebermuth vom Stiel gesprungen.", "tokens": ["Vor", "Ue\u00b7ber\u00b7muth", "vom", "Stiel", "ge\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun steht sie still und schaut sich um \u2014", "tokens": ["Nun", "steht", "sie", "still", "und", "schaut", "sich", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PRF", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "All \u00fcberall nur Baum an Baum;", "tokens": ["All", "\u00fc\u00b7be\u00b7rall", "nur", "Baum", "an", "Baum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja, irre zieht im Walde um", "tokens": ["Ja", ",", "ir\u00b7re", "zieht", "im", "Wal\u00b7de", "um"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "VVFIN", "APPRART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Berges Maid und glaubt es kaum;", "tokens": ["Des", "Ber\u00b7ges", "Maid", "und", "glaubt", "es", "kaum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch zwei Minuten, wo sie sann,", "tokens": ["Noch", "zwei", "Mi\u00b7nu\u00b7ten", ",", "wo", "sie", "sann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Pulsiren lie\u00df die hei\u00dfen Glieder, \u2014", "tokens": ["Pul\u00b7si\u00b7ren", "lie\u00df", "die", "hei\u00b7\u00dfen", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Behende wie ein Marder dann", "tokens": ["Be\u00b7hen\u00b7de", "wie", "ein", "Mar\u00b7der", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schl\u00fcpft keck sie in den Steinbruch nieder.", "tokens": ["Schl\u00fcpft", "keck", "sie", "in", "den", "Stein\u00b7bruch", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Am Eingang steht ein Felsenblock,", "tokens": ["Am", "Ein\u00b7gang", "steht", "ein", "Fel\u00b7sen\u00b7block", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo das Geschiebe \u00fcberh\u00e4ngt;", "tokens": ["Wo", "das", "Ge\u00b7schie\u00b7be", "\u00fc\u00b7ber\u00b7h\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Epheu sch\u00fcttelt sein Gelock,", "tokens": ["Der", "E\u00b7pheu", "sch\u00fct\u00b7telt", "sein", "Ge\u00b7lock", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur gr\u00fcnen Laube vorgedr\u00e4ngt:", "tokens": ["Zur", "gr\u00fc\u00b7nen", "Lau\u00b7be", "vor\u00b7ge\u00b7dr\u00e4ngt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da unter'm Dache lagert sie,", "tokens": ["Da", "un\u00b7ter'm", "Da\u00b7che", "la\u00b7gert", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Behaglich lehnend an dem Steine,", "tokens": ["Be\u00b7hag\u00b7lich", "leh\u00b7nend", "an", "dem", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und denkt: ich sitze wahrlich wie", "tokens": ["Und", "denkt", ":", "ich", "sit\u00b7ze", "wahr\u00b7lich", "wie"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "KOKOM"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Heil'genbildchen in dem Schreine!", "tokens": ["Ein", "Heil'\u00b7gen\u00b7bild\u00b7chen", "in", "dem", "Schrei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr ist so warm, der Z\u00f6pfe Paar", "tokens": ["Ihr", "ist", "so", "warm", ",", "der", "Z\u00f6p\u00b7fe", "Paar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie l\u00f6set mit der runden Hand,", "tokens": ["Sie", "l\u00f6\u00b7set", "mit", "der", "run\u00b7den", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nieder rauscht ihr schwarzes Haar", "tokens": ["Und", "nie\u00b7der", "rauscht", "ihr", "schwar\u00b7zes", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Rabensittiges Gewand.", "tokens": ["Wie", "Ra\u00b7ben\u00b7sit\u00b7ti\u00b7ges", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ei! denkt sie, bin ich doch allein!", "tokens": ["Ei", "!", "denkt", "sie", ",", "bin", "ich", "doch", "al\u00b7lein", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf springt das Spangenpaar am Mieder;", "tokens": ["Auf", "springt", "das", "Span\u00b7gen\u00b7paar", "am", "Mie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch unbeweglich gleich dem Stein", "tokens": ["Doch", "un\u00b7be\u00b7weg\u00b7lich", "gleich", "dem", "Stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Steht hinter'm Block der wilde Rieder:", "tokens": ["Steht", "hin\u00b7ter'm", "Block", "der", "wil\u00b7de", "Rie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er sieht sie nicht, nur ihren Fu\u00df,", "tokens": ["Er", "sieht", "sie", "nicht", ",", "nur", "ih\u00b7ren", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der t\u00e4ndelnd schaukelt wie ein Schiff,", "tokens": ["Der", "t\u00e4n\u00b7delnd", "schau\u00b7kelt", "wie", "ein", "Schiff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zuweilen treibt des Windes Gru\u00df", "tokens": ["Zu\u00b7wei\u00b7len", "treibt", "des", "Win\u00b7des", "Gru\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch eine Locke um das Riff,", "tokens": ["Auch", "ei\u00b7ne", "Lo\u00b7cke", "um", "das", "Riff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch ihres hei\u00dfen Odems Zug,", "tokens": ["Doch", "ih\u00b7res", "hei\u00b7\u00dfen", "O\u00b7dems", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Samumes Hauch, glaubt er zu f\u00fchlen,", "tokens": ["Sa\u00b7mu\u00b7mes", "Hauch", ",", "glaubt", "er", "zu", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Verlorne Laute, wie im Flug", "tokens": ["Ver\u00b7lor\u00b7ne", "Lau\u00b7te", ",", "wie", "im", "Flug"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Lockv\u00f6gel, um das Ohr ihm spielen.", "tokens": ["Lock\u00b7v\u00f6\u00b7gel", ",", "um", "das", "Ohr", "ihm", "spie\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So weich die Luft und badewarm,", "tokens": ["So", "weich", "die", "Luft", "und", "ba\u00b7de\u00b7warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Berauschend Thimianes Duft,", "tokens": ["Be\u00b7rau\u00b7schend", "Thi\u00b7mi\u00b7a\u00b7nes", "Duft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie lehnt sich, dehnt sich, ihren Arm,", "tokens": ["Sie", "lehnt", "sich", ",", "dehnt", "sich", ",", "ih\u00b7ren", "Arm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "VVFIN", "PRF", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den vollen, streckt sie aus der Kluft,", "tokens": ["Den", "vol\u00b7len", ",", "streckt", "sie", "aus", "der", "Kluft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schlie\u00dft dann ihr gl\u00e4nzend Augenpaar \u2014", "tokens": ["Schlie\u00dft", "dann", "ihr", "gl\u00e4n\u00b7zend", "Au\u00b7gen\u00b7paar"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADJD", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht schlafen, ruhn nur eine Stunde \u2014", "tokens": ["Nicht", "schla\u00b7fen", ",", "ruhn", "nur", "ei\u00b7ne", "Stun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So d\u00e4mmert sie und die Gefahr", "tokens": ["So", "d\u00e4m\u00b7mert", "sie", "und", "die", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "W\u00e4chst von Sekunde zu Sekunde.", "tokens": ["W\u00e4chst", "von", "Se\u00b7kun\u00b7de", "zu", "Se\u00b7kun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nun Alles still \u2014 sie ", "tokens": ["Nun", "Al\u00b7les", "still", "sie"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "PIS", "ADJD", "$(", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Doch hinter'm Steine wird's belebt", "tokens": ["Doch", "hin\u00b7ter'm", "Stei\u00b7ne", "wird's", "be\u00b7lebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seine B\u00fcchse sachte, sacht,", "tokens": ["Und", "sei\u00b7ne", "B\u00fcch\u00b7se", "sach\u00b7te", ",", "sacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Rieder von der Schulter hebt,", "tokens": ["Der", "Rie\u00b7der", "von", "der", "Schul\u00b7ter", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lehnt an die Klippe ihren Lauf,", "tokens": ["Lehnt", "an", "die", "Klip\u00b7pe", "ih\u00b7ren", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dann lockert er der Messer Klingen,", "tokens": ["Dann", "lo\u00b7ckert", "er", "der", "Mes\u00b7ser", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hebt nun den Fu\u00df \u2014 was h\u00e4lt ihn auf?", "tokens": ["Hebt", "nun", "den", "Fu\u00df", "was", "h\u00e4lt", "ihn", "auf", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$(", "PWS"