{"textgrid.poem.67189": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer Gutes ehrt, er macht sich keinen Schaden,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer Gutes ehrt, er macht sich keinen Schaden,", "tokens": ["Wer", "Gu\u00b7tes", "ehrt", ",", "er", "macht", "sich", "kei\u00b7nen", "Scha\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er h\u00e4lt sich hoch, er lebt den Menschen nicht vergebens,", "tokens": ["Er", "h\u00e4lt", "sich", "hoch", ",", "er", "lebt", "den", "Men\u00b7schen", "nicht", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "$,", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er kennt den Wert, den Nutzen solchen Lebens,", "tokens": ["Er", "kennt", "den", "Wert", ",", "den", "Nut\u00b7zen", "sol\u00b7chen", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er traut dem Bessern sich, er geht auf Segenspfaden.", "tokens": ["Er", "traut", "dem", "Bes\u00b7sern", "sich", ",", "er", "geht", "auf", "Se\u00b7gen\u00b7spfa\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "$,", "PPER