{"textgrid.poem.63912": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Wissenschaft", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sobald die K\u00fcnste verbl\u00fchn,", "tokens": ["So\u00b7bald", "die", "K\u00fcns\u00b7te", "ver\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Kommt Wissenschaft in Gunst.", "tokens": ["Kommt", "Wis\u00b7sen\u00b7schaft", "in", "Gunst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie lohnt auch Handwerksm\u00fchn,", "tokens": ["Sie", "lohnt", "auch", "Hand\u00b7werks\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Denn Wissen ist keine Kunst.", "tokens": ["Denn", "Wis\u00b7sen", "ist", "kei\u00b7ne", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbwer nicht in der Wissenschaft Kleines ehrt,", "tokens": ["\u00bb", "wer", "nicht", "in", "der", "Wis\u00b7sen\u00b7schaft", "Klei\u00b7nes", "ehrt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PTKNEG", "APPR", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ist auch des gro\u00dfen Gewinns nicht wert.\u00ab \u2013", "tokens": ["Ist", "auch", "des", "gro\u00b7\u00dfen", "Ge\u00b7winns", "nicht", "wert", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "PTKNEG", "ADJD", "$.", "$(", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das werd' ich niemals euch bestreiten,", "tokens": ["Das", "werd'", "ich", "nie\u00b7mals", "euch", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur euer Gro\u00dftun mit Kleinigkeiten.", "tokens": ["Nur", "eu\u00b7er", "Gro\u00df\u00b7tun", "mit", "Klei\u00b7nig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Ein Haus zu bauen ist stets beschwerlich,", "tokens": ["Ein", "Haus", "zu", "bau\u00b7en", "ist", "stets", "be\u00b7schwer\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Viel Gewerke reichen sich da die Hand,", "tokens": ["Viel", "Ge\u00b7wer\u00b7ke", "rei\u00b7chen", "sich", "da", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auch Handlanger sind unentbehrlich,", "tokens": ["Auch", "Hand\u00b7lan\u00b7ger", "sind", "un\u00b7ent\u00b7behr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihren Lohn verdienen sie ehrlich,", "tokens": ["Und", "ih\u00b7ren", "Lohn", "ver\u00b7die\u00b7nen", "sie", "ehr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nur werden sie nicht Architekten genannt.", "tokens": ["Nur", "wer\u00b7den", "sie", "nicht", "Ar\u00b7chi\u00b7tek\u00b7ten", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}