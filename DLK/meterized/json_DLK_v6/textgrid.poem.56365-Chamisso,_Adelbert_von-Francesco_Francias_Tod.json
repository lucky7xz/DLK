{"textgrid.poem.56365": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Francesco Francias Tod", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Francesco Francia war zu seiner Zeit", "tokens": ["Fran\u00b7ce\u00b7sco", "Fran\u00b7cia", "war", "zu", "sei\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Italiens Stolz, ger\u00fchmt von allen Zungen", "tokens": ["I\u00b7ta\u00b7li\u00b7ens", "Stolz", ",", "ge\u00b7r\u00fchmt", "von", "al\u00b7len", "Zun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVPP", "APPR", "PIAT", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als Aurifex und Maler weit und breit.", "tokens": ["Als", "Au\u00b7ri\u00b7fex", "und", "Ma\u00b7ler", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu ihm, dem Alten, ist der Ruf gedrungen", "tokens": ["Zu", "ihm", ",", "dem", "Al\u00b7ten", ",", "ist", "der", "Ruf", "ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "ART", "NN", "$,", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vom jungen R\u00f6mer, welcher sonder Gleichen", "tokens": ["Vom", "jun\u00b7gen", "R\u00f6\u00b7mer", ",", "wel\u00b7cher", "son\u00b7der", "Glei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sich fr\u00fch gar hohen K\u00fcnstlerruhm errungen.", "tokens": ["Sich", "fr\u00fch", "gar", "ho\u00b7hen", "K\u00fcnst\u00b7ler\u00b7ruhm", "er\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zwar konnt er noch zu sehen nicht erreichen", "tokens": ["Zwar", "konnt", "er", "noch", "zu", "se\u00b7hen", "nicht", "er\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKZU", "VVINF", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Werk von ihm, doch haben sie geehret", "tokens": ["Ein", "Werk", "von", "ihm", ",", "doch", "ha\u00b7ben", "sie", "ge\u00b7eh\u00b7ret"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Einander und gewechselt Freundschaftszeichen.", "tokens": ["Ein\u00b7an\u00b7der", "und", "ge\u00b7wech\u00b7selt", "Freund\u00b7schafts\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ihm wird die Freude jetzt, die er begehret;", "tokens": ["Ihm", "wird", "die", "Freu\u00b7de", "jetzt", ",", "die", "er", "be\u00b7ge\u00b7hret", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Sieh! jener schreibt: \u00bbMein Bitten werde mir", "tokens": ["Sieh", "!", "je\u00b7ner", "schreibt", ":", "\u00bb", "Mein", "Bit\u00b7ten", "wer\u00b7de", "mir"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PDS", "VVFIN", "$.", "$(", "PPOSAT", "NN", "VAFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Von meinem v\u00e4terlichen Freund gew\u00e4hret.", "tokens": ["Von", "mei\u00b7nem", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "Freund", "ge\u00b7w\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ich k\u00e4me selbst, doch andres h\u00e4lt mich hier;", "tokens": ["Ich", "k\u00e4\u00b7me", "selbst", ",", "doch", "and\u00b7res", "h\u00e4lt", "mich", "hier", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "PIS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Mein Bild f\u00fcr die San Giovanni Kapelle,", "tokens": ["Mein", "Bild", "f\u00fcr", "die", "San", "Gio\u00b7van\u00b7ni", "Ka\u00b7pel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NE", "NE", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Die heilige C\u00e4cilie, send ich dir.", "tokens": ["Die", "hei\u00b7li\u00b7ge", "C\u00e4\u00b7ci\u00b7lie", ",", "send", "ich", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "KOUS", "PPER", "PPER", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Vertritt, mein lieber Meister, meine Stelle,", "tokens": ["Ver\u00b7tritt", ",", "mein", "lie\u00b7ber", "Meis\u00b7ter", ",", "mei\u00b7ne", "Stel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Sieh helfend nach, ob Schaden es bekommen,", "tokens": ["Sieh", "hel\u00b7fend", "nach", ",", "ob", "Scha\u00b7den", "es", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKVZ", "$,", "KOUS", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Ein Ri\u00df, ein Fleck das zarte Werk entstelle;", "tokens": ["Ein", "Ri\u00df", ",", "ein", "Fleck", "das", "zar\u00b7te", "Werk", "ent\u00b7stel\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und hast den Pinsel du zur Hand genommen,", "tokens": ["Und", "hast", "den", "Pin\u00b7sel", "du", "zur", "Hand", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Verbe\u00dfre du zugleich auch liebevoll,", "tokens": ["Ver\u00b7be\u00df\u00b7re", "du", "zu\u00b7gleich", "auch", "lie\u00b7be\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Wo selber meine Kunst zu kurz gekommen.", "tokens": ["Wo", "sel\u00b7ber", "mei\u00b7ne", "Kunst", "zu", "kurz", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Dann stell es auf, das Bild, da wo es soll,", "tokens": ["Dann", "stell", "es", "auf", ",", "das", "Bild", ",", "da", "wo", "es", "soll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PTKVZ", "$,", "ART", "NN", "$,", "ADV", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Mit Liebe sorgend f\u00fcr das beste Licht,", "tokens": ["Mit", "Lie\u00b7be", "sor\u00b7gend", "f\u00fcr", "das", "bes\u00b7te", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und nimm entgegen meines Dankes Zoll!", "tokens": ["Und", "nimm", "ent\u00b7ge\u00b7gen", "mei\u00b7nes", "Dan\u00b7kes", "Zoll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Dein Raffael.\u00ab \u2013 Der Meister schnell erbricht", "tokens": ["Dein", "Raf\u00b7fael", ".", "\u00ab", "\u2013", "Der", "Meis\u00b7ter", "schnell", "er\u00b7bricht"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "$(", "$(", "ART", "NN", "ADJD", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Die Kiste, zieht das Bild hervor und r\u00fcckt", "tokens": ["Die", "Kis\u00b7te", ",", "zieht", "das", "Bild", "her\u00b7vor", "und", "r\u00fcckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Es sich ins Licht und sieht, und glaubt es nicht.", "tokens": ["Es", "sich", "ins", "Licht", "und", "sieht", ",", "und", "glaubt", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPRART", "NN", "KON", "VVFIN", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Er steht davor erschrocken und entz\u00fcckt,", "tokens": ["Er", "steht", "da\u00b7vor", "er\u00b7schro\u00b7cken", "und", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "VVINF", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Erf\u00fcllet ist, was seine Tr\u00e4ume waren,", "tokens": ["Er\u00b7f\u00fcl\u00b7let", "ist", ",", "was", "sei\u00b7ne", "Tr\u00e4u\u00b7me", "wa\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Er f\u00fchlt sich selbst vernichtet und begl\u00fcckt.", "tokens": ["Er", "f\u00fchlt", "sich", "selbst", "ver\u00b7nich\u00b7tet", "und", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "\u00bbheil mir! und Preis dir, Herr! der offenbaren", "tokens": ["\u00bb", "heil", "mir", "!", "und", "Preis", "dir", ",", "Herr", "!", "der", "of\u00b7fen\u00b7ba\u00b7ren"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["$(", "KOUS", "PPER", "$.", "KON", "NN", "PPER", "$,", "NN", "$.", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Du solches noch gewollt in meinen Tagen;", "tokens": ["Du", "sol\u00b7ches", "noch", "ge\u00b7wollt", "in", "mei\u00b7nen", "Ta\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADV", "VMPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Nun la\u00df in Frieden deinen Diener fahren.\u00ab", "tokens": ["Nun", "la\u00df", "in", "Frie\u00b7den", "dei\u00b7nen", "Die\u00b7ner", "fah\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVIMP", "APPR", "NN", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Die J\u00fcnger h\u00f6rten ihn die Worte sagen,", "tokens": ["Die", "J\u00fcn\u00b7ger", "h\u00f6r\u00b7ten", "ihn", "die", "Wor\u00b7te", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Den letzten Laut aus seinem frommen Munde;", "tokens": ["Den", "letz\u00b7ten", "Laut", "aus", "sei\u00b7nem", "from\u00b7men", "Mun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Nicht Antwort gab er mehr auf ihre Fragen:", "tokens": ["Nicht", "Ant\u00b7wort", "gab", "er", "mehr", "auf", "ih\u00b7re", "Fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Es war des alten Francias Sterbestunde.", "tokens": ["Es", "war", "des", "al\u00b7ten", "Fran\u00b7ci\u00b7as", "Ster\u00b7be\u00b7stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}