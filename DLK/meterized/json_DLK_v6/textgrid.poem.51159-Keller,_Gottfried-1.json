{"textgrid.poem.51159": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie kamen von der Tr\u00e4nke,", "tokens": ["Sie", "ka\u00b7men", "von", "der", "Tr\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie wankten aus der Schenke", "tokens": ["Sie", "wank\u00b7ten", "aus", "der", "Schen\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit einer Zecherschar,", "tokens": ["Mit", "ei\u00b7ner", "Ze\u00b7cher\u00b7schar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als es Karfreitag morgen", "tokens": ["Als", "es", "Kar\u00b7frei\u00b7tag", "mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "ADV"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und grabesstille war.", "tokens": ["Und", "gra\u00b7bess\u00b7til\u00b7le", "war", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Von hei\u00dfen Stirnen nicken", "tokens": ["Von", "hei\u00b7\u00dfen", "Stir\u00b7nen", "ni\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und st\u00e4uben die Per\u00fccken,", "tokens": ["Und", "st\u00e4u\u00b7ben", "die", "Pe\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie Wolke birgt den Blitz;", "tokens": ["Wie", "Wol\u00b7ke", "birgt", "den", "Blitz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die spitze Kling am Degen", "tokens": ["Die", "spit\u00b7ze", "Kling", "am", "De\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zuckt wie geschliffner Witz.", "tokens": ["Zuckt", "wie", "ge\u00b7schliff\u00b7ner", "Witz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Sie taumelten und sangen,", "tokens": ["Sie", "tau\u00b7mel\u00b7ten", "und", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Vom Mund wie St\u00f6psel sprangen", "tokens": ["Vom", "Mund", "wie", "St\u00f6p\u00b7sel", "spran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KOKOM", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Verse, Schlag auf Schlag;", "tokens": ["Die", "Ver\u00b7se", ",", "Schlag", "auf", "Schlag", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da schrie Panard: \u00bbO f\u00fchlet", "tokens": ["Da", "schrie", "Pa\u00b7nard", ":", "\u00bb", "O", "f\u00fch\u00b7let"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "$.", "$(", "NE", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den furchtbar gro\u00dfen Tag!", "tokens": ["Den", "furcht\u00b7bar", "gro\u00b7\u00dfen", "Tag", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Universum trauert,", "tokens": ["Das", "U\u00b7ni\u00b7ver\u00b7sum", "trau\u00b7ert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die dunkle Sonne schauert,", "tokens": ["Die", "dunk\u00b7le", "Son\u00b7ne", "schau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Erde wankt und bebt,", "tokens": ["Die", "Er\u00b7de", "wankt", "und", "bebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df unter unsern F\u00fc\u00dfen", "tokens": ["Da\u00df", "un\u00b7ter", "un\u00b7sern", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der hohle Boden schwebt!", "tokens": ["Der", "hoh\u00b7le", "Bo\u00b7den", "schwebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Unsicher ist's, zu stehen,", "tokens": ["Un\u00b7si\u00b7cher", "ist's", ",", "zu", "ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Und ratsam nicht, zu gehen!", "tokens": ["Und", "rat\u00b7sam", "nicht", ",", "zu", "ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKNEG", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kehrt um zu unsrem Wirt!\u00ab \u2013", "tokens": ["Kehrt", "um", "zu", "uns\u00b7rem", "Wirt", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "KOUI", "APPR", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und alsbald kroch die Herde", "tokens": ["Und", "als\u00b7bald", "kroch", "die", "Her\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zur\u00fcck zu ihrem Hirt.", "tokens": ["Zu\u00b7r\u00fcck", "zu", "ih\u00b7rem", "Hirt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dort blieben sie verborgen", "tokens": ["Dort", "blie\u00b7ben", "sie", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bis an den dritten Morgen", "tokens": ["Bis", "an", "den", "drit\u00b7ten", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Tief und geheimnisvoll,", "tokens": ["Tief", "und", "ge\u00b7heim\u00b7nis\u00b7voll", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Bis in der goldnen Fr\u00fche", "tokens": ["Bis", "in", "der", "gold\u00b7nen", "Fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Osterglocke scholl.", "tokens": ["Die", "Os\u00b7ter\u00b7glo\u00b7cke", "scholl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Als die verj\u00fcngte Sonne", "tokens": ["Als", "die", "ver\u00b7j\u00fcng\u00b7te", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Auferstehungswonne", "tokens": ["In", "Auf\u00b7er\u00b7ste\u00b7hungs\u00b7won\u00b7ne"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durchschritt des Fr\u00fchlings Tor,", "tokens": ["Durch\u00b7schritt", "des", "Fr\u00fch\u00b7lings", "Tor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da stiegen aus der H\u00f6hle", "tokens": ["Da", "stie\u00b7gen", "aus", "der", "H\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Weinselig sie hervor.", "tokens": ["Wein\u00b7se\u00b7lig", "sie", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}