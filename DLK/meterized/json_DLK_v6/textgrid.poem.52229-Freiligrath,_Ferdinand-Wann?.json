{"textgrid.poem.52229": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Wann?", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Zeitung schreibt von braven Henkern,", "tokens": ["Die", "Zei\u00b7tung", "schreibt", "von", "bra\u00b7ven", "Hen\u00b7kern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schwert und Augentuch", "tokens": ["Die", "Schwert", "und", "Au\u00b7gen\u00b7tuch"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Voll Zorns in einen Winkel schlenkern,", "tokens": ["Voll", "Zorns", "in", "ei\u00b7nen", "Win\u00b7kel", "schlen\u00b7kern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sprechend: \u00bbEs ist genug!", "tokens": ["Spre\u00b7chend", ":", "\u00bb", "Es", "ist", "ge\u00b7nug", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "$(", "PPER", "VAFIN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "In unsrer Seele schreit es Zeter \u2013", "tokens": ["In", "uns\u00b7rer", "See\u00b7le", "schreit", "es", "Ze\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wir geben ihr Geh\u00f6r!", "tokens": ["Wir", "ge\u00b7ben", "ihr", "Ge\u00b7h\u00f6r", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "K\u00f6pft selber eure Misseth\u00e4ter \u2013", "tokens": ["K\u00f6pft", "sel\u00b7ber", "eu\u00b7re", "Mis\u00b7set\u00b7h\u00e4\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wir k\u00f6pfen keinen mehr!\u00ab", "tokens": ["Wir", "k\u00f6p\u00b7fen", "kei\u00b7nen", "mehr", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "PIS", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u2013 Wann fallen so erst Deutschlands Karten,", "tokens": ["\u2013", "Wann", "fal\u00b7len", "so", "erst", "Deutschlands", "Kar\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ihr Mund, die jetzo seiner warten,", "tokens": ["Ihr", "Mund", ",", "die", "jet\u00b7zo", "sei\u00b7ner", "war\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Offenheit verdammt?", "tokens": ["Mit", "Of\u00b7fen\u00b7heit", "ver\u00b7dammt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da sie ihr Mordzeug von sich schmei\u00dfen,", "tokens": ["Da", "sie", "ihr", "Mord\u00b7zeug", "von", "sich", "schmei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ausrufend: \u00bbNimmermehr!", "tokens": ["Aus\u00b7ru\u00b7fend", ":", "\u00bb", "Nim\u00b7mer\u00b7mehr", "!"], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["VVPP", "$.", "$(", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wir lassen lieber uns zerrei\u00dfen!", "tokens": ["Wir", "las\u00b7sen", "lie\u00b7ber", "uns", "zer\u00b7rei\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nur das \u2013 nur das nicht mehr!", "tokens": ["Nur", "das", "\u2013", "nur", "das", "nicht", "mehr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$(", "ADV", "PDS", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nein, nimmer! \u2013 Und f\u00fcr ehrlos gelte", "tokens": ["Nein", ",", "nim\u00b7mer", "!", "\u2013", "Und", "f\u00fcr", "ehr\u00b7los", "gel\u00b7te"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "$.", "$(", "KON", "APPR", "ADJD", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der deutsche Mann hinfort,", "tokens": ["Der", "deut\u00b7sche", "Mann", "hin\u00b7fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der st\u00fcmmelnd niederhaut mit K\u00e4lte", "tokens": ["Der", "st\u00fcm\u00b7melnd", "nie\u00b7der\u00b7haut", "mit", "K\u00e4l\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das unbeschirmte Wort!", "tokens": ["Das", "un\u00b7be\u00b7schirm\u00b7te", "Wort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Hand legt an das Allerfreiste", "tokens": ["Der", "Hand", "legt", "an", "das", "Al\u00b7ler\u00b7freis\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von allem, was da frei!", "tokens": ["Von", "al\u00b7lem", ",", "was", "da", "frei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der an dem Gott in uns, dem Geiste,", "tokens": ["Der", "an", "dem", "Gott", "in", "uns", ",", "dem", "Geis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Aus\u00fcbt Scharfrichterei!", "tokens": ["Aus\u00b7\u00fcbt", "Schar\u00b7frich\u00b7te\u00b7rei", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Ist euch der Geist ein armer S\u00fcnder,", "tokens": ["Ist", "euch", "der", "Geist", "ein", "ar\u00b7mer", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl \u2013 tut ihn selber ab!", "tokens": ["Wohl", "\u2013", "tut", "ihn", "sel\u00b7ber", "ab", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drauf eure Vierundzwanzigpf\u00fcnder!", "tokens": ["Drauf", "eu\u00b7re", "Vie\u00b7rund\u00b7zwan\u00b7zig\u00b7pf\u00fcn\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Drauf \u2013 in Galopp und Trab!", "tokens": ["Drauf", "\u2013", "in", "Ga\u00b7lopp", "und", "Trab", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "APPR", "NE", "KON", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Doch wir: \u2013 ins Weltmeer unsre Schere!", "tokens": ["Doch", "wir", ":", "\u2013", "ins", "Welt\u00b7meer", "uns\u00b7re", "Sche\u00b7re", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$.", "$(", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hinschwemme sie der Rhein!", "tokens": ["Hin\u00b7schwem\u00b7me", "sie", "der", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kein deutscher Mann, kein Mann von Ehre", "tokens": ["Kein", "deut\u00b7scher", "Mann", ",", "kein", "Mann", "von", "Eh\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "PIAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Will Zensor f\u00fcrder sein!\u00ab", "tokens": ["Will", "Zen\u00b7sor", "f\u00fcr\u00b7der", "sein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "NN", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}