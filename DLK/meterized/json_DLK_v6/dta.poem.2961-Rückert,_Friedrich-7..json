{"dta.poem.2961": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was einen Dichter macht? das hohe Selbstgef\u00fchl", "tokens": ["Was", "ei\u00b7nen", "Dich\u00b7ter", "macht", "?", "das", "ho\u00b7he", "Selbst\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fr\u00f6liche Vertraun im bunten Weltgew\u00fchl.", "tokens": ["Und", "fr\u00f6\u00b7li\u00b7che", "Ver\u00b7traun", "im", "bun\u00b7ten", "Welt\u00b7ge\u00b7w\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O Freund, mir aber kam allbeides fast abhanden,", "tokens": ["O", "Freund", ",", "mir", "a\u00b7ber", "kam", "all\u00b7bei\u00b7des", "fast", "ab\u00b7han\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADV", "VVFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht durch Unbilden, die ich reichlich selbst bestanden;", "tokens": ["Nicht", "durch", "Un\u00b7bil\u00b7den", ",", "die", "ich", "reich\u00b7lich", "selbst", "be\u00b7stan\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "PRELS", "PPER", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was einem widerfuhr, der gr\u00f6\u00dfer ist als ich,", "tokens": ["Was", "ei\u00b7nem", "wi\u00b7der\u00b7fuhr", ",", "der", "gr\u00f6\u00b7\u00dfer", "ist", "als", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "VVFIN", "$,", "PRELS", "ADJD", "VAFIN", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ohne den ich selbst nicht w\u00e4re, kr\u00e4nket mich:", "tokens": ["Und", "oh\u00b7ne", "den", "ich", "selbst", "nicht", "w\u00e4\u00b7re", ",", "kr\u00e4n\u00b7ket", "mich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "PPER", "ADV", "PTKNEG", "VAFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da\u00df Goethe werden darf mishandelt ungerochen,", "tokens": ["Da\u00df", "Goe\u00b7the", "wer\u00b7den", "darf", "mis\u00b7han\u00b7delt", "un\u00b7ge\u00b7ro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VAINF", "VMFIN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das hat mein Selbstgef\u00fchl und Weltvertraun gebrochen.", "tokens": ["Das", "hat", "mein", "Selbst\u00b7ge\u00b7f\u00fchl", "und", "Welt\u00b7ver\u00b7traun", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}