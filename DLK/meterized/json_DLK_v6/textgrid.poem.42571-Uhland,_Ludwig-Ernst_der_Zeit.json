{"textgrid.poem.42571": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Ernst der Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann ward der erste Kranz gewunden?", "tokens": ["Wann", "ward", "der", "ers\u00b7te", "Kranz", "ge\u00b7wun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wann flog der erste Ball ans Ziel?", "tokens": ["Wann", "flog", "der", "ers\u00b7te", "Ball", "ans", "Ziel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann ward der heitre Tanz erfunden?", "tokens": ["Wann", "ward", "der", "heit\u00b7re", "Tanz", "er\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wann das lose Pf\u00e4nderspiel?", "tokens": ["Und", "wann", "das", "lo\u00b7se", "Pf\u00e4n\u00b7der\u00b7spiel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach! wohl in fernen, fernen Tagen,", "tokens": ["Ach", "!", "wohl", "in", "fer\u00b7nen", ",", "fer\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die unsern h\u00e4tten's nie erdacht,", "tokens": ["Die", "un\u00b7sern", "h\u00e4t\u00b7ten's", "nie", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo bald im Feld die V\u00f6lker schlagen,", "tokens": ["Wo", "bald", "im", "Feld", "die", "V\u00f6l\u00b7ker", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bald der innre Zank erwacht.", "tokens": ["Und", "bald", "der", "inn\u00b7re", "Zank", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}