{"textgrid.poem.39575": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, weiser Goethe! Du hast wahrlich Recht!", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, weiser Goethe! Du hast wahrlich Recht!", "tokens": ["Ja", ",", "wei\u00b7ser", "Goe\u00b7the", "!", "Du", "hast", "wahr\u00b7lich", "Recht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$.", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Caviar mu\u00df man bei'm Frost versenden.", "tokens": ["Den", "Ca\u00b7vi\u00b7ar", "mu\u00df", "man", "bei'm", "Frost", "ver\u00b7sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom feuchten Wetter wird das Salz geschw\u00e4cht;", "tokens": ["Vom", "feuch\u00b7ten", "Wet\u00b7ter", "wird", "das", "Salz", "ge\u00b7schw\u00e4cht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Eier faulen dann, und schmecken schlecht.", "tokens": ["Die", "Ei\u00b7er", "fau\u00b7len", "dann", ",", "und", "schme\u00b7cken", "schlecht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch d\u00fcnngesalz'ne Brief' in sechs ger\u00e4um'gen B\u00e4nden,", "tokens": ["Doch", "d\u00fcnn\u00b7ge\u00b7sa\u00b7lz'\u00b7ne", "Brie\u00b7f'", "in", "sechs", "ge\u00b7r\u00e4um'\u00b7gen", "B\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Die la\u00dfen sich in jeder Jahrszeit spenden", "tokens": ["Die", "la\u00b7\u00dfen", "sich", "in", "je\u00b7der", "Jahrs\u00b7zeit", "spen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und sind dem stumpfen Leser immer recht.", "tokens": ["Und", "sind", "dem", "stump\u00b7fen", "Le\u00b7ser", "im\u00b7mer", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}