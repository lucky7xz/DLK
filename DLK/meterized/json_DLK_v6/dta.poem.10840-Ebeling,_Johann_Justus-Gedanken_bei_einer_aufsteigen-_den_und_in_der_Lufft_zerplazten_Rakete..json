{"dta.poem.10840": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken bei einer aufsteigen-  \n den und in der Lufft zerplazten  \n Rakete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sah im Feuerwerk ein sch\u00f6nes Lust-\nspiel an;", "tokens": ["Ich", "sah", "im", "Feu\u00b7er\u00b7werk", "ein", "sch\u00f6\u00b7nes", "Lust", "spiel", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "TRUNC", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es stieg in schnellen Flug, auf", "tokens": ["Es", "stieg", "in", "schnel\u00b7len", "Flug", ",", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die feurige Raket; sie plazte in der Ferne,", "tokens": ["Die", "feu\u00b7ri\u00b7ge", "Ra\u00b7ket", ";", "sie", "plaz\u00b7te", "in", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und spr\u00fczte noch hervor ein schimmernd Heer der", "tokens": ["Und", "spr\u00fcz\u00b7te", "noch", "her\u00b7vor", "ein", "schim\u00b7mernd", "Heer", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "ADJD", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Damit war alles aus; Es flatterte der Schein", "tokens": ["Da\u00b7mit", "war", "al\u00b7les", "aus", ";", "Es", "flat\u00b7ter\u00b7te", "der", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PIS", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In der erhabnen Lufft zwar pr\u00e4chtig, ungemein,", "tokens": ["In", "der", "er\u00b7hab\u00b7nen", "Lufft", "zwar", "pr\u00e4ch\u00b7tig", ",", "un\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "ADJD", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jedoch der Untergang war auch damit verbunden,", "tokens": ["Je\u00b7doch", "der", "Un\u00b7ter\u00b7gang", "war", "auch", "da\u00b7mit", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So bald sie war zerplazt, war gleich ihr Blik ver-", "tokens": ["So", "bald", "sie", "war", "zer\u00b7plazt", ",", "war", "gleich", "ihr", "Blik", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VAFIN", "VVPP", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So geht es in der Welt den eitlen Menschen auch,", "tokens": ["So", "geht", "es", "in", "der", "Welt", "den", "eit\u00b7len", "Men\u00b7schen", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die hoch erhaben sind; die Ehr ist wie ein", "tokens": ["Die", "hoch", "er\u00b7ha\u00b7ben", "sind", ";", "die", "Ehr", "ist", "wie", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "VAFIN", "$.", "ART", "NN", "VAFIN", "KOKOM", "ART"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Der pl\u00f6zlich wird verweht. Man solte Wunder", "tokens": ["Der", "pl\u00f6z\u00b7lich", "wird", "ver\u00b7weht", ".", "Man", "sol\u00b7te", "Wun\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "VAFIN", "VVFIN", "$.", "PIS", "VMFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Was solche Menschen sind, die in der H\u00f6he scheinen:", "tokens": ["Was", "sol\u00b7che", "Men\u00b7schen", "sind", ",", "die", "in", "der", "H\u00f6\u00b7he", "schei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Allein so bald der Todt des Leibes Bau zerst\u00fckt,", "tokens": ["Al\u00b7lein", "so", "bald", "der", "Todt", "des", "Lei\u00b7bes", "Bau", "zer\u00b7st\u00fckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wird bei dem Grabe noch der lezte Glanz erblikt,", "tokens": ["Wird", "bei", "dem", "Gra\u00b7be", "noch", "der", "lez\u00b7te", "Glanz", "er\u00b7blikt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Kaum ist der Rest versenkt zum d\u00fcstren Grabes Bo-", "tokens": ["Kaum", "ist", "der", "Rest", "ver\u00b7senkt", "zum", "d\u00fcst\u00b7ren", "Gra\u00b7bes", "Bo"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So ist ihr Glanz und Schein gar bald auch mit", "tokens": ["So", "ist", "ihr", "Glanz", "und", "Schein", "gar", "bald", "auch", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "KON", "NN", "ADV", "ADV", "ADV", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Wenn die Raket zerplazt; giebt sie noch einen Klang,", "tokens": ["Wenn", "die", "Ra\u00b7ket", "zer\u00b7plazt", ";", "giebt", "sie", "noch", "ei\u00b7nen", "Klang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$.", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.18": {"text": "Der Schein verdampfet gleich; so riecht man den", "tokens": ["Der", "Schein", "ver\u00b7damp\u00b7fet", "gleich", ";", "so", "riecht", "man", "den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$.", "ADV", "VVFIN", "PIS", "ART"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.19": {"text": "Des Pulvers noch zulezt, wenn durch der L\u00fcffte", "tokens": ["Des", "Pul\u00b7vers", "noch", "zu\u00b7lezt", ",", "wenn", "durch", "der", "L\u00fcff\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Der \u00fcberbliebne Dunst durchkreucht das Sieb der", "tokens": ["Der", "\u00fc\u00b7berb\u00b7lieb\u00b7ne", "Dunst", "durch\u00b7kreucht", "das", "Sieb", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Die Hohen dieser Welt die herrlich untergehn,", "tokens": ["Die", "Ho\u00b7hen", "die\u00b7ser", "Welt", "die", "herr\u00b7lich", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "ART", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die lassen \u00f6ffters auch, wenn wir sie nicht mehr", "tokens": ["Die", "las\u00b7sen", "\u00f6ff\u00b7ters", "auch", ",", "wenn", "wir", "sie", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "ADV"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Ein stinkendes Ger\u00fccht auf dieser Welt zur\u00fckke:", "tokens": ["Ein", "stin\u00b7ken\u00b7des", "Ge\u00b7r\u00fccht", "auf", "die\u00b7ser", "Welt", "zu\u00b7r\u00fck\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Das ist der Ueberrest von ihrem stolzen Gl\u00fckke.", "tokens": ["Das", "ist", "der", "Ue\u00b7ber\u00b7rest", "von", "ih\u00b7rem", "stol\u00b7zen", "Gl\u00fck\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}