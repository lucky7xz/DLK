{"textgrid.poem.28072": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "87.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wolte Bavius sein Weib Lateinisch lehren,", "tokens": ["Es", "wol\u00b7te", "Ba\u00b7vius", "sein", "Weib", "La\u00b7tei\u00b7nisch", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch wolt er Cornu nicht beym decliniren h\u00f6ren;", "tokens": ["Doch", "wolt", "er", "Cor\u00b7nu", "nicht", "beym", "de\u00b7cli\u00b7ni\u00b7ren", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NE", "PTKNEG", "APPRART", "VVINF", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auff Amo da es kam, gestund er, Ego, Tu;", "tokens": ["Auff", "A\u00b7mo", "da", "es", "kam", ",", "ge\u00b7stund", "er", ",", "E\u00b7go", ",", "Tu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "KOUS", "PPER", "VVFIN", "$,", "KON", "PPER", "$,", "NE", "$,", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Ille strich er au\u00df und lie\u00df es ihr nicht zu.", "tokens": ["Das", "Il\u00b7le", "strich", "er", "au\u00df", "und", "lie\u00df", "es", "ihr", "nicht", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+