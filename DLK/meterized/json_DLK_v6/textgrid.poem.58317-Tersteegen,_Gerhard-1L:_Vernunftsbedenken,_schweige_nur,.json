{"textgrid.poem.58317": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vernunftsbedenken, schweige nur,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vernunftsbedenken, schweige nur,", "tokens": ["Ver\u00b7nunfts\u00b7be\u00b7den\u00b7ken", ",", "schwei\u00b7ge", "nur", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich folge frei dem Herren ohne Sehen!", "tokens": ["Ich", "fol\u00b7ge", "frei", "dem", "Her\u00b7ren", "oh\u00b7ne", "Se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der blinde Glaube zeigt die Spur,", "tokens": ["Der", "blin\u00b7de", "Glau\u00b7be", "zeigt", "die", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie man zum Tod der Eigenheit mu\u00df gehen.", "tokens": ["Wie", "man", "zum", "Tod", "der", "Ei\u00b7gen\u00b7heit", "mu\u00df", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPRART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}