{"dta.poem.3287": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Ermunterung zum Vergn\u00fcgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Wird stets dein Stolz der falschen Hoffnung trauen,", "tokens": ["Wird", "stets", "dein", "Stolz", "der", "fal\u00b7schen", "Hoff\u00b7nung", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ihn mit Tr\u00e4umen unterh\u00e4lt;", "tokens": ["Die", "ihn", "mit", "Tr\u00e4u\u00b7men", "un\u00b7ter\u00b7h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in der Luft manch gl\u00e4nzend Schlo\u00df erbauen,", "tokens": ["Und", "in", "der", "Luft", "manch", "gl\u00e4n\u00b7zend", "Schlo\u00df", "er\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PIAT", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das pl\u00f6tzlich ohne Spur zerf\u00e4llt?", "tokens": ["Das", "pl\u00f6tz\u00b7lich", "oh\u00b7ne", "Spur", "zer\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Hoffnung tr\u00e4umt, was \u00f6fters nie geschiehet,", "tokens": ["Die", "Hoff\u00b7nung", "tr\u00e4umt", ",", "was", "\u00f6f\u00b7ters", "nie", "ge\u00b7schie\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So hitzig wir ihm nachgestrebt:", "tokens": ["So", "hit\u00b7zig", "wir", "ihm", "nach\u00b7ge\u00b7strebt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Indessen flieht und ungekannt entfliehet", "tokens": ["In\u00b7des\u00b7sen", "flieht", "und", "un\u00b7ge\u00b7kannt", "ent\u00b7flie\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Freude, die uns nahe schwebt.", "tokens": ["Die", "Freu\u00b7de", ",", "die", "uns", "na\u00b7he", "schwebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Rasen hier, die weiches Gras bedecket,", "tokens": ["Die", "Ra\u00b7sen", "hier", ",", "die", "wei\u00b7ches", "Gras", "be\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und \u00fcber die zu freyer Lust", "tokens": ["Und", "\u00fc\u00b7ber", "die", "zu", "frey\u00b7er", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich, schattenreich, die breite Linde strecket,", "tokens": ["Sich", ",", "schat\u00b7ten\u00b7reich", ",", "die", "brei\u00b7te", "Lin\u00b7de", "stre\u00b7cket", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "ADJD", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erwarten dich an meiner Brust.", "tokens": ["Er\u00b7war\u00b7ten", "dich", "an", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hier la\u00df uns, Freund! bey Wein und Liedern liegen:", "tokens": ["Hier", "la\u00df", "uns", ",", "Freund", "!", "bey", "Wein", "und", "Lie\u00b7dern", "lie\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$,", "NN", "$.", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie s\u00fc\u00df ists, von Ly\u00e4en gl\u00fchn!", "tokens": ["Wie", "s\u00fc\u00df", "ists", ",", "von", "Ly\u00b7\u00e4\u00b7en", "gl\u00fchn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "$,", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf! hohl\u2019 ihn her! ihm folge das Vergn\u00fcgen,", "tokens": ["Auf", "!", "hohl'", "ihn", "her", "!", "ihm", "fol\u00b7ge", "das", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und eitle Sorge m\u00fcsse fliehn!", "tokens": ["Und", "eit\u00b7le", "Sor\u00b7ge", "m\u00fcs\u00b7se", "fliehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Denn tiefe Nacht deckt vor uns her die Tage,", "tokens": ["Denn", "tie\u00b7fe", "Nacht", "deckt", "vor", "uns", "her", "die", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "PRF", "APZR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ieder noch durchwandern wird.", "tokens": ["Die", "ie\u00b7der", "noch", "durch\u00b7wan\u00b7dern", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich schleiche fort, bereit zu Lust und Plage,", "tokens": ["Ich", "schlei\u00b7che", "fort", ",", "be\u00b7reit", "zu", "Lust", "und", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gleich einem, der im Nebel irrt.", "tokens": ["Gleich", "ei\u00b7nem", ",", "der", "im", "Ne\u00b7bel", "irrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie Schritt vor Schritt die schwarze Wolke flie-", "tokens": ["Wie", "Schritt", "vor", "Schritt", "die", "schwar\u00b7ze", "Wol\u00b7ke", "flie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "het,", "tokens": ["het", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Entdeckt sich ihm bald \u00f6der Sand,", "tokens": ["Ent\u00b7deckt", "sich", "ihm", "bald", "\u00f6\u00b7der", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der, unerfrischt von kalten Quellen, gl\u00fchet,", "tokens": ["Der", ",", "un\u00b7er\u00b7frischt", "von", "kal\u00b7ten", "Quel\u00b7len", ",", "gl\u00fc\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "$,", "ADJD", "APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein rauhes und unwirthbars Land.", "tokens": ["Ein", "rau\u00b7hes", "und", "un\u00b7wirth\u00b7bars", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Bald aber wird sein frohes Lied erschallen,", "tokens": ["Bald", "a\u00b7ber", "wird", "sein", "fro\u00b7hes", "Lied", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wann, auf so viel Beschwerlichkeit,", "tokens": ["Wann", ",", "auf", "so", "viel", "Be\u00b7schwer\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am k\u00fchlen Bach, ein Wald voll Nachtigallen", "tokens": ["Am", "k\u00fch\u00b7len", "Bach", ",", "ein", "Wald", "voll", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jhm angenehme Schatten beut.", "tokens": ["Jhm", "an\u00b7ge\u00b7neh\u00b7me", "Schat\u00b7ten", "beut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}