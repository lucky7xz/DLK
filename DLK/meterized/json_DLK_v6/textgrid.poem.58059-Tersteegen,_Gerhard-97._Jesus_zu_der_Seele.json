{"textgrid.poem.58059": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "97. Jesus zu der Seele", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kind, schlie\u00df die Augen zu vor diesem ganzen Rund,", "tokens": ["Kind", ",", "schlie\u00df", "die", "Au\u00b7gen", "zu", "vor", "die\u00b7sem", "gan\u00b7zen", "Rund", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "PTKZU", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Welt la\u00df von der Welt und ihren Puppen handeln,", "tokens": ["Die", "Welt", "la\u00df", "von", "der", "Welt", "und", "ih\u00b7ren", "Pup\u00b7pen", "han\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bleib du stets nah bei mir in deiner Seele Grund!", "tokens": ["Bleib", "du", "stets", "nah", "bei", "mir", "in", "dei\u00b7ner", "See\u00b7le", "Grund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJD", "APPR", "PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So innig wollen wir hier miteinander wandeln;", "tokens": ["So", "in\u00b7nig", "wol\u00b7len", "wir", "hier", "mi\u00b7tein\u00b7an\u00b7der", "wan\u00b7deln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was geht die Welt dich an? Bald gehest du hinaus;", "tokens": ["Was", "geht", "die", "Welt", "dich", "an", "?", "Bald", "ge\u00b7hest", "du", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dann sind wir stets beisamm'n in meines Vaters Haus.", "tokens": ["Dann", "sind", "wir", "stets", "bei\u00b7sam\u00b7m'n", "in", "mei\u00b7nes", "Va\u00b7ters", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}