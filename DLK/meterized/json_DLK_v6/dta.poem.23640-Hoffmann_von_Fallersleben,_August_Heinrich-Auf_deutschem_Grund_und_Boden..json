{"dta.poem.23640": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf deutschem Grund und Boden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O da\u00df ich Hoffnung wieder habe,", "tokens": ["O", "da\u00df", "ich", "Hoff\u00b7nung", "wie\u00b7der", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dies lang' entbehrte s\u00fc\u00dfe Gl\u00fcck!", "tokens": ["Dies", "lang'", "ent\u00b7behr\u00b7te", "s\u00fc\u00b7\u00dfe", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich kehre neu wie aus dem Grabe", "tokens": ["Ich", "keh\u00b7re", "neu", "wie", "aus", "dem", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur sch\u00f6nen Erde jetzt zur\u00fcck.", "tokens": ["Zur", "sch\u00f6\u00b7nen", "Er\u00b7de", "jetzt", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gel\u00f6st ist meines Herzens Blindheit,", "tokens": ["Ge\u00b7l\u00f6st", "ist", "mei\u00b7nes", "Her\u00b7zens", "Blind\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sehe wieder Tagesschein,", "tokens": ["Ich", "se\u00b7he", "wie\u00b7der", "Ta\u00b7ges\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lebe wie in fr\u00fcher Kindheit,", "tokens": ["Ich", "le\u00b7be", "wie", "in", "fr\u00fc\u00b7her", "Kind\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ganze Welt ist wieder mein.", "tokens": ["Die", "gan\u00b7ze", "Welt", "ist", "wie\u00b7der", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und allem was da lebt und webet", "tokens": ["Und", "al\u00b7lem", "was", "da", "lebt", "und", "we\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "PWS", "ADV", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df ich mich froh und liebend nahn,", "tokens": ["Mu\u00df", "ich", "mich", "froh", "und", "lie\u00b7bend", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADJD", "KON", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie der Lenz die Erd' umschwebet,", "tokens": ["Und", "wie", "der", "Lenz", "die", "Erd'", "um\u00b7schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Will auch mein Herz die Welt umfahn.", "tokens": ["Will", "auch", "mein", "Herz", "die", "Welt", "um\u00b7fahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}