{"textgrid.poem.49999": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "31. Auf die Klagen \u00fcber den Unbestand des Gl\u00fcckes", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil man auf dieser Welt mehr ", "tokens": ["Weil", "man", "auf", "die\u00b7ser", "Welt", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PDAT", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und immer in der Irre schwebt,", "tokens": ["Und", "im\u00b7mer", "in", "der", "Ir\u00b7re", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So denckt ihr wenig nach, indem ihr so verzagt", "tokens": ["So", "denckt", "ihr", "we\u00b7nig", "nach", ",", "in\u00b7dem", "ihr", "so", "ver\u00b7zagt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Danckt vor ", "tokens": ["Danckt", "vor"], "token_info": ["word", "word"], "pos": ["VVFIN", "APPR"], "meter": "-+", "measure": "iambic.single"}}}}}