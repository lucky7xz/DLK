{"dta.poem.3829": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Trauer-Zeilen,  \n Bey Absterben eines Hoffnungs-vollen  Studiosi Theologiae,  \n als selbiger j\u00e4hling und unvermuthet Todes verblichen.  \n Jm Jahre 1742, verfertiget.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Blasser Freund, was vor ein Schrecken jagt", "tokens": ["Blas\u00b7ser", "Freund", ",", "was", "vor", "ein", "Schre\u00b7cken", "jagt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "uns dein Erbleichen ein!", "tokens": ["uns", "dein", "Er\u00b7blei\u00b7chen", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jeder kan ein wahres Bildni\u00df deiner blassen Leiche", "tokens": ["Je\u00b7der", "kan", "ein", "wah\u00b7res", "Bild\u00b7ni\u00df", "dei\u00b7ner", "blas\u00b7sen", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "seyn.", "tokens": ["seyn", "."], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Ach! ist di\u00df der Wei\u00dfheit Lohn, den sie ihren Kin-", "tokens": ["Ach", "!", "ist", "di\u00df", "der", "Wei\u00df\u00b7heit", "Lohn", ",", "den", "sie", "ih\u00b7ren", "Kin"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VAFIN", "PDS", "ART", "NN", "NN", "$,", "PRELS", "PPER", "PPOSAT", "TRUNC"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "dern zollet,", "tokens": ["dern", "zol\u00b7let", ","], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Da\u00df die b\u00e4ngste Z\u00e4hren-Fluth \u00fcber ihre Wan-", "tokens": ["Da\u00df", "die", "b\u00e4ngs\u00b7te", "Z\u00e4h\u00b7ren\u00b7Fluth", "\u00fc\u00b7ber", "ih\u00b7re", "Wan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "TRUNC"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "gen rollet?", "tokens": ["gen", "rol\u00b7let", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Heist di\u00df den Altar betreten? wird die Cantzel dir", "tokens": ["Heist", "di\u00df", "den", "Al\u00b7tar", "be\u00b7tre\u00b7ten", "?", "wird", "die", "Cant\u00b7zel", "dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "ART", "NN", "VVPP", "$.", "VAFIN", "ART", "NN", "PPER"], "meter": "+---+-+-+-+-+", "measure": "dactylic.init"}, "line.10": {"text": "zum Sarg?", "tokens": ["zum", "Sarg", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Wem wirst du erstorben n\u00fctzen? dein Fall kr\u00e4nckt", "tokens": ["Wem", "wirst", "du", "ers\u00b7tor\u00b7ben", "n\u00fct\u00b7zen", "?", "dein", "Fall", "kr\u00e4nckt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "VVINF", "VVINF", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "uns gar zu arg.", "tokens": ["uns", "gar", "zu", "arg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Kan denn Wei\u00dfheit und Verdienst keinen bessern", "tokens": ["Kan", "denn", "Wei\u00df\u00b7heit", "und", "Ver\u00b7dienst", "kei\u00b7nen", "bes\u00b7sern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "KON", "NN", "KON", "NN", "PIAT", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Lohn erwerben?", "tokens": ["Lohn", "er\u00b7wer\u00b7ben", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Hilfft gelehrt seyn weiter nichts, als nur zeitiger zu", "tokens": ["Hilfft", "ge\u00b7lehrt", "seyn", "wei\u00b7ter", "nichts", ",", "als", "nur", "zei\u00b7ti\u00b7ger", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "VAINF", "ADV", "PIS", "$,", "KOUS", "ADV", "ADJD", "PTKZU"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "sterben?", "tokens": ["ster\u00b7ben", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Hier, ", "tokens": ["Hier", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+", "measure": "single.up"}, "line.18": {"text": "stumme Seuffzer Platz,", "tokens": ["stum\u00b7me", "Seuff\u00b7zer", "Platz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.19": {"text": "Freund! bey deiner Todten-Baare hemmet Weh-", "tokens": ["Freund", "!", "bey", "dei\u00b7ner", "Tod\u00b7ten\u00b7Baa\u00b7re", "hem\u00b7met", "Weh"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "PPOSAT", "NN", "VVFIN", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.20": {"text": "muth Kiel und Satz.", "tokens": ["muth", "Kiel", "und", "Satz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Ob gleich Zung und Ader starrt, soll mein Geist", "tokens": ["Ob", "gleich", "Zung", "und", "A\u00b7der", "starrt", ",", "soll", "mein", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "KON", "NN", "VVFIN", "$,", "VMFIN", "PPOSAT", "NN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.22": {"text": "doch seine Pflichten,", "tokens": ["doch", "sei\u00b7ne", "Pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Soll doch mein bethr\u00e4ntes Blatt dir den letzten", "tokens": ["Soll", "doch", "mein", "be\u00b7thr\u00e4n\u00b7tes", "Blatt", "dir", "den", "letz\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJA", "NN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.24": {"text": "Dienst entrichten.", "tokens": ["Dienst", "ent\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.25": {"text": "Du hast Jacob gleich gek\u00e4mpffet, o! mehr als", "tokens": ["Du", "hast", "Ja\u00b7cob", "gleich", "ge\u00b7k\u00e4mpf\u00b7fet", ",", "o", "!", "mehr", "als"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "NE", "ADV", "VVPP", "$,", "FM", "$.", "PIAT", "KOKOM"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "gepr\u00fcfter Geist!", "tokens": ["ge\u00b7pr\u00fcf\u00b7ter", "Geist", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.27": {"text": "Der nun in dem stillen Zion seinen Heiland sieht", "tokens": ["Der", "nun", "in", "dem", "stil\u00b7len", "Zi\u00b7on", "sei\u00b7nen", "Hei\u00b7land", "sieht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "----+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.28": {"text": "und prei\u00dft.", "tokens": ["und", "prei\u00dft", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.29": {"text": "Kan ich dich zu guter letzt gleich nicht auf der Baare", "tokens": ["Kan", "ich", "dich", "zu", "gu\u00b7ter", "letzt", "gleich", "nicht", "auf", "der", "Baa\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "ADJA", "ADV", "ADV", "PTKNEG", "APPR", "ART", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.30": {"text": "schauen,", "tokens": ["schau\u00b7en", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.31": {"text": "So will ich doch deine Grufft dir in meinem Her-", "tokens": ["So", "will", "ich", "doch", "dei\u00b7ne", "Grufft", "dir", "in", "mei\u00b7nem", "Her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "PPER", "APPR", "PPOSAT", "TRUNC"], "meter": "-+--+----+-+", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "tzen bauen.", "tokens": ["tzen", "bau\u00b7en", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.33": {"text": "Freund! ich g\u00f6nne dir dein Sterben, das dich in", "tokens": ["Freund", "!", "ich", "g\u00f6n\u00b7ne", "dir", "dein", "Ster\u00b7ben", ",", "das", "dich", "in"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PRELS", "PRF", "APPR"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.34": {"text": "der h\u00f6chsten Pracht", "tokens": ["der", "h\u00f6chs\u00b7ten", "Pracht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.35": {"text": "Zu dem w\u00fcrdigsten ", "tokens": ["Zu", "dem", "w\u00fcr\u00b7digs\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.36": {"text": "Engel macht.", "tokens": ["En\u00b7gel", "macht", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.37": {"text": "Nunmehr bist du eingeschrieben in ein Buch das", "tokens": ["Nun\u00b7mehr", "bist", "du", "ein\u00b7ge\u00b7schrie\u00b7ben", "in", "ein", "Buch", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "APPR", "ART", "NN", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.38": {"text": "ewig w\u00e4hrt,", "tokens": ["e\u00b7wig", "w\u00e4hrt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.39": {"text": "Welches seine Auserwehlten mit der Kost der", "tokens": ["Wel\u00b7ches", "sei\u00b7ne", "Au\u00b7ser\u00b7wehl\u00b7ten", "mit", "der", "Kost", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "ART", "NN", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.40": {"text": "Wei\u00dfheit n\u00e4hrt.", "tokens": ["Wei\u00df\u00b7heit", "n\u00e4hrt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.41": {"text": "Nun so geh nach Ararat, geh in den gew\u00f6lbten", "tokens": ["Nun", "so", "geh", "nach", "Ar\u00b7a\u00b7rat", ",", "geh", "in", "den", "ge\u00b7w\u00f6lb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "NE", "$,", "VVFIN", "APPR", "ART", "ADJA"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "Bogen;", "tokens": ["Bo\u00b7gen", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.43": {"text": "Siehe nun der Wei\u00dfheit Frucht, die dich als ein", "tokens": ["Sie\u00b7he", "nun", "der", "Wei\u00df\u00b7heit", "Frucht", ",", "die", "dich", "als", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ART", "NN", "NN", "$,", "PRELS", "PPER", "KOUS", "ART"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.44": {"text": "Kind gesogen.", "tokens": ["Kind", "ge\u00b7so\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.45": {"text": "Jesus rufft dich in die Schule, und so schl\u00e4gt", "tokens": ["Je\u00b7sus", "rufft", "dich", "in", "die", "Schu\u00b7le", ",", "und", "so", "schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "KON", "ADV", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.46": {"text": "dein Wunsch nicht fehl,", "tokens": ["dein", "Wunsch", "nicht", "fehl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.47": {"text": "Nun rufft dir der Mund der Wei\u00dfheit; ja du folgst,", "tokens": ["Nun", "rufft", "dir", "der", "Mund", "der", "Wei\u00df\u00b7heit", ";", "ja", "du", "folgst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$.", "PTKANT", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.48": {"text": "mein Samuel,", "tokens": ["mein", "Sa\u00b7muel", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.49": {"text": "Ja du folgst, du setzest dich zu des grossen Meisters", "tokens": ["Ja", "du", "folgst", ",", "du", "set\u00b7zest", "dich", "zu", "des", "gros\u00b7sen", "Meis\u00b7ters"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.50": {"text": "F\u00fcssen,", "tokens": ["F\u00fcs\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.51": {"text": "Nun wird deiner Einsichts Krafft weit mehr, als wir", "tokens": ["Nun", "wird", "dei\u00b7ner", "Ein\u00b7sichts", "Krafft", "weit", "mehr", ",", "als", "wir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "PIAT", "NN", "ADJD", "ADV", "$,", "KOUS", "PPER"], "meter": "----+-++--+", "measure": "iambic.tetra.chol"}, "line.52": {"text": "alle wissen.", "tokens": ["al\u00b7le", "wis\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.53": {"text": "Du bist nun ein grosses Mitglied bey der gr\u00f6sten", "tokens": ["Du", "bist", "nun", "ein", "gros\u00b7ses", "Mit\u00b7glied", "bey", "der", "gr\u00f6s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.54": {"text": "Hohen Schul,", "tokens": ["Ho\u00b7hen", "Schul", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.55": {"text": "Da du ewig triumphirest vor des theuren Lam-", "tokens": ["Da", "du", "e\u00b7wig", "tri\u00b7um\u00b7phi\u00b7rest", "vor", "des", "theu\u00b7ren", "Lam"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.56": {"text": "mes Stuhl.", "tokens": ["mes", "Stuhl", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}