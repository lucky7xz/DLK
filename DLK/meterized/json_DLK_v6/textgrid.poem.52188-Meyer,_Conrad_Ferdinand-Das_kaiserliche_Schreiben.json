{"textgrid.poem.52188": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Das kaiserliche Schreiben", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Petrus, schreib \u2013 zu seinem Kanzler", "tokens": ["Pet\u00b7rus", ",", "schreib", "\u2013", "zu", "sei\u00b7nem", "Kanz\u00b7ler"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["FM", "$,", "VVFIN", "$(", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "sprach's der gramverst\u00f6rte Staufen \u2013", "tokens": ["sprach's", "der", "gram\u00b7ver\u00b7st\u00f6r\u00b7te", "Stau\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Satteln sollen meine Boten,", "tokens": ["Sat\u00b7teln", "sol\u00b7len", "mei\u00b7ne", "Bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "hundert Rosse sollen laufen!", "tokens": ["hun\u00b7dert", "Ros\u00b7se", "sol\u00b7len", "lau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Meinen Eignen, meinen St\u00e4dtern,", "tokens": ["Mei\u00b7nen", "Eig\u00b7nen", ",", "mei\u00b7nen", "St\u00e4d\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "meinen Pfaffen und Baronen!", "tokens": ["mei\u00b7nen", "Pfaf\u00b7fen", "und", "Ba\u00b7ro\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dem Geringsten wie dem H\u00f6chsten!", "tokens": ["Dem", "Ge\u00b7rings\u00b7ten", "wie", "dem", "H\u00f6chs\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "allen, die das Reich bewohnen!", "tokens": ["al\u00b7len", ",", "die", "das", "Reich", "be\u00b7woh\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Klage! Klage! Totenklage!", "tokens": ["Kla\u00b7ge", "!", "Kla\u00b7ge", "!", "To\u00b7ten\u00b7kla\u00b7ge", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Meinen Sohn hab ich verloren...", "tokens": ["Mei\u00b7nen", "Sohn", "hab", "ich", "ver\u00b7lo\u00b7ren", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Heinrich mit den finstern Locken...", "tokens": ["Hein\u00b7rich", "mit", "den", "fins\u00b7tern", "Lo\u00b7cken", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "den Konstanze mir geboren", "tokens": ["den", "Kons\u00b7tan\u00b7ze", "mir", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVPP"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.13": {"text": "Der das Reich verriet... dem eignen", "tokens": ["Der", "das", "Reich", "ver\u00b7riet", "...", "dem", "eig\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "$(", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Vater brach das Lehnsversprechen...", "tokens": ["Va\u00b7ter", "brach", "das", "Lehns\u00b7ver\u00b7spre\u00b7chen", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Den ich beugen, beugen mu\u00dfte,", "tokens": ["Den", "ich", "beu\u00b7gen", ",", "beu\u00b7gen", "mu\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "VVINF", "$,", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "dessen Trotz ich mu\u00dfte brechen...", "tokens": ["des\u00b7sen", "Trotz", "ich", "mu\u00df\u00b7te", "bre\u00b7chen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Lange br\u00fctet' er im Kerker \u2013", "tokens": ["Lan\u00b7ge", "br\u00fc\u00b7tet'", "er", "im", "Ker\u00b7ker", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "endlich hat er mich gerufen \u2013", "tokens": ["end\u00b7lich", "hat", "er", "mich", "ge\u00b7ru\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Da ich kam, flog er vor\u00fcber,", "tokens": ["Da", "ich", "kam", ",", "flog", "er", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--++--+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "flog empor die Wendelstufen \u2013", "tokens": ["flog", "em\u00b7por", "die", "Wen\u00b7del\u00b7stu\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Wieder war's, als ob, verzweifelnd,", "tokens": ["Wie\u00b7der", "wa\u00b7r's", ",", "als", "ob", ",", "ver\u00b7zwei\u00b7felnd", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOKOM", "KOUS", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "er vom h\u00f6chsten S\u00f6ller riefe \u2013", "tokens": ["er", "vom", "h\u00f6chs\u00b7ten", "S\u00f6l\u00b7ler", "rie\u00b7fe", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Da! Der Knabe springt vor meinen", "tokens": ["Da", "!", "Der", "Kna\u00b7be", "springt", "vor", "mei\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "ART", "NN", "VVFIN", "APPR", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Augen in die Todestiefe!", "tokens": ["Au\u00b7gen", "in", "die", "To\u00b7des\u00b7tie\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Jammeranblick ohnegleichen!", "tokens": ["Jam\u00b7me\u00b7ran\u00b7blick", "oh\u00b7ne\u00b7glei\u00b7chen", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Kommt, da\u00df wir zusammen klagen!", "tokens": ["Kommt", ",", "da\u00df", "wir", "zu\u00b7sam\u00b7men", "kla\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Helft mir meine schlimmen Tr\u00e4ume,", "tokens": ["Helft", "mir", "mei\u00b7ne", "schlim\u00b7men", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "meine Nachtgedanken tragen! \u2013", "tokens": ["mei\u00b7ne", "Nacht\u00b7ge\u00b7dan\u00b7ken", "tra\u00b7gen", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "K\u00f6nnt ich ihn erwecken, nimmer", "tokens": ["K\u00f6nnt", "ich", "ihn", "er\u00b7we\u00b7cken", ",", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "w\u00fcrd ich aus dem Arm ihn lassen!", "tokens": ["w\u00fcrd", "ich", "aus", "dem", "Arm", "ihn", "las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Saget, ist es nicht entsetzlich,", "tokens": ["Sa\u00b7get", ",", "ist", "es", "nicht", "ent\u00b7setz\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "da\u00df mein Kind mich mu\u00dfte hassen?", "tokens": ["da\u00df", "mein", "Kind", "mich", "mu\u00df\u00b7te", "has\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Petrus, zeig mir was du schreibest!", "tokens": ["Pet\u00b7rus", ",", "zeig", "mir", "was", "du", "schrei\u00b7best", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "$,", "VVFIN", "PPER", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Willst du mir den Mund verhalten?", "tokens": ["Willst", "du", "mir", "den", "Mund", "ver\u00b7hal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "\u00dcber meine Qualen wirfst du", "tokens": ["\u00dc\u00b7ber", "mei\u00b7ne", "Qua\u00b7len", "wirfst", "du"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "w\u00fcrdevolle Purpurfalten?", "tokens": ["w\u00fcr\u00b7de\u00b7vol\u00b7le", "Pur\u00b7pur\u00b7fal\u00b7ten", "?"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Meines Knaben Schrei erstickst du?", "tokens": ["Mei\u00b7nes", "Kna\u00b7ben", "Schrei", "er\u00b7stickst", "du", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.38": {"text": "Meine Tr\u00e4nen sind verboten?", "tokens": ["Mei\u00b7ne", "Tr\u00e4\u00b7nen", "sind", "ver\u00b7bo\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Kanzler Petrus, schreibe Wahrheit", "tokens": ["Kanz\u00b7ler", "Pet\u00b7rus", ",", "schrei\u00b7be", "Wahr\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["FM", "FM", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "\u00fcber mich und meinen Toten!", "tokens": ["\u00fc\u00b7ber", "mich", "und", "mei\u00b7nen", "To\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Reden will ich zu den V\u00e4tern:", "tokens": ["Re\u00b7den", "will", "ich", "zu", "den", "V\u00e4\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Sagt mir, w\u00fcrdet ihr nicht einen", "tokens": ["Sagt", "mir", ",", "w\u00fcr\u00b7det", "ihr", "nicht", "ei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "VAFIN", "PPER", "PTKNEG", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Knaben, der euch Not und dunkeln", "tokens": ["Kna\u00b7ben", ",", "der", "euch", "Not", "und", "dun\u00b7keln"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "NN", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Kummer brachte, doch beweinen?", "tokens": ["Kum\u00b7mer", "brach\u00b7te", ",", "doch", "be\u00b7wei\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Den ihr in der Wiege k\u00fc\u00dftet \u2013", "tokens": ["Den", "ihr", "in", "der", "Wie\u00b7ge", "k\u00fc\u00df\u00b7tet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "ob er auch ein Arger w\u00e4re \u2013", "tokens": ["ob", "er", "auch", "ein", "Ar\u00b7ger", "w\u00e4\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Wenn er ginge zu den Schatten,", "tokens": ["Wenn", "er", "gin\u00b7ge", "zu", "den", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "weigertet ihr ihm die Z\u00e4hre?", "tokens": ["wei\u00b7ger\u00b7tet", "ihr", "ihm", "die", "Z\u00e4h\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.49": {"text": "Pr\u00fcfet eure Herzen, V\u00e4ter!", "tokens": ["Pr\u00fc\u00b7fet", "eu\u00b7re", "Her\u00b7zen", ",", "V\u00e4\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Was wir von den Kindern dulden,", "tokens": ["Was", "wir", "von", "den", "Kin\u00b7dern", "dul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Ist es nicht gerechte S\u00fchne,", "tokens": ["Ist", "es", "nicht", "ge\u00b7rech\u00b7te", "S\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "nicht das eigene Verschulden?...", "tokens": ["nicht", "das", "ei\u00b7ge\u00b7ne", "Ver\u00b7schul\u00b7den", "?", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.53": {"text": "Petrus, du erschrickst, so ende!", "tokens": ["Pet\u00b7rus", ",", "du", "er\u00b7schrickst", ",", "so", "en\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM", "$,", "PPER", "VVFIN", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Ende mit dem kurzgefa\u00dften", "tokens": ["En\u00b7de", "mit", "dem", "kurz\u00b7ge\u00b7fa\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Reichsbefehl: Wir ordnen Trauer", "tokens": ["Reichs\u00b7be\u00b7fehl", ":", "Wir", "ord\u00b7nen", "Trau\u00b7er"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "an f\u00fcr diesen Fr\u00fchverbla\u00dften.", "tokens": ["an", "f\u00fcr", "die\u00b7sen", "Fr\u00fch\u00b7ver\u00b7bla\u00df\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}