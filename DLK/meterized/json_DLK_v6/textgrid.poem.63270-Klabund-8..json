, "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Tr\u00e4um ich die Tanne?", "tokens": ["Tr\u00e4um", "ich", "die", "Tan\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Tr\u00e4umt die Tanne mich?", "tokens": ["Tr\u00e4umt", "die", "Tan\u00b7ne", "mich", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Es lebt und stirbt", "tokens": ["Es", "lebt", "und", "stirbt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Sich leicht.", "tokens": ["Sich", "leicht", "."], "token_info": ["word", "word", "punct"], "pos": ["PRF", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}}}}