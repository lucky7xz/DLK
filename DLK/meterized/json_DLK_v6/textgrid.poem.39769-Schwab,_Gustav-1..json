{"textgrid.poem.39769": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eilf Jahre sind's, du wardst geboren,", "tokens": ["Eilf", "Jah\u00b7re", "sin\u00b7d's", ",", "du", "wardst", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du s\u00fc\u00dfer Sp\u00e4tling, j\u00fcngster Sohn!", "tokens": ["Du", "s\u00fc\u00b7\u00dfer", "Sp\u00e4t\u00b7ling", ",", "j\u00fcngs\u00b7ter", "Sohn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Eltern Blick, in dich verloren,", "tokens": ["Der", "El\u00b7tern", "Blick", ",", "in", "dich", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Las eine goldne Zukunft schon.", "tokens": ["Las", "ei\u00b7ne", "gold\u00b7ne", "Zu\u00b7kunft", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So freudig bl\u00fchten deine Wangen,", "tokens": ["So", "freu\u00b7dig", "bl\u00fch\u00b7ten", "dei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So \u00fcppig wuchs dein gelbes Haar;", "tokens": ["So", "\u00fcp\u00b7pig", "wuchs", "dein", "gel\u00b7bes", "Haar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Lebensstrom flo\u00df ausgegangen", "tokens": ["Ein", "Le\u00b7bens\u00b7strom", "flo\u00df", "aus\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von deinem hellen Augenpaar.", "tokens": ["Von", "dei\u00b7nem", "hel\u00b7len", "Au\u00b7gen\u00b7paar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wohl nahm die Scheere jene Locken,", "tokens": ["Wohl", "nahm", "die", "Schee\u00b7re", "je\u00b7ne", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Knabenantlitz trat hervor.", "tokens": ["Das", "Kna\u00b7ben\u00b7ant\u00b7litz", "trat", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, ernstes Schulkind, streng und trocken,", "tokens": ["Und", ",", "erns\u00b7tes", "Schul\u00b7kind", ",", "streng", "und", "tro\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Standst du vor der Erkenntni\u00df Thor.", "tokens": ["Standst", "du", "vor", "der", "Er\u00b7kennt\u00b7ni\u00df", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Bald aber ward der Trieb zum Spiele,", "tokens": ["Bald", "a\u00b7ber", "ward", "der", "Trieb", "zum", "Spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Geistchen flog durch Raum und Zeit,", "tokens": ["Dein", "Geist\u00b7chen", "flog", "durch", "Raum", "und", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die junge Hand zwang mit dem Kiele", "tokens": ["Die", "jun\u00b7ge", "Hand", "zwang", "mit", "dem", "Kie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der R\u00f6mersprache Herbigkeit.", "tokens": ["Der", "R\u00f6\u00b7mer\u00b7spra\u00b7che", "Her\u00b7big\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Durch die gew\u00f6lbte Stirne zogen", "tokens": ["Durch", "die", "ge\u00b7w\u00f6lb\u00b7te", "Stir\u00b7ne", "zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schon die Gedanken aus und ein,", "tokens": ["Schon", "die", "Ge\u00b7dan\u00b7ken", "aus", "und", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch kindliche Gebete flogen", "tokens": ["Doch", "kind\u00b7li\u00b7che", "Ge\u00b7be\u00b7te", "flo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch von den Lippen, zweifelsrein.", "tokens": ["Noch", "von", "den", "Lip\u00b7pen", ",", "zwei\u00b7fel\u00b7srein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du gr\u00fcntest, stark an Leib und Seele,", "tokens": ["Du", "gr\u00fcn\u00b7test", ",", "stark", "an", "Leib", "und", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du Mutterwonne, Vaterstolz,", "tokens": ["Du", "Mut\u00b7ter\u00b7won\u00b7ne", ",", "Va\u00b7ter\u00b7stolz", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geschwisterlust, recht ohne Fehle,", "tokens": ["Ge\u00b7schwis\u00b7ter\u00b7lust", ",", "recht", "oh\u00b7ne", "Feh\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du, B\u00e4umchen, hinter \u00e4ltrem Holz.", "tokens": ["Du", ",", "B\u00e4um\u00b7chen", ",", "hin\u00b7ter", "\u00e4lt\u00b7rem", "Holz", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Froh sang ich: \u00bbJ\u00fcngster Knabe, funkle", "tokens": ["Froh", "sang", "ich", ":", "\u00bb", "J\u00fcngs\u00b7ter", "Kna\u00b7be", ",", "funk\u00b7le"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$.", "$(", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich keck mit schwarzen Augen an;", "tokens": ["Mich", "keck", "mit", "schwar\u00b7zen", "Au\u00b7gen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie auch das Erdenleben dunkle,", "tokens": ["Wie", "auch", "das", "Er\u00b7den\u00b7le\u00b7ben", "dunk\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch bricht sich solch ein Stral die Bahn!\u00ab", "tokens": ["Doch", "bricht", "sich", "solch", "ein", "Stral", "die", "Bahn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PRF", "PIAT", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}