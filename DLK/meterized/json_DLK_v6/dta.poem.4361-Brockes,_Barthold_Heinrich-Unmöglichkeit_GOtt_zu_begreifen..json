{"dta.poem.4361": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unm\u00f6glichkeit GOtt zu begreifen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Buch der Creaturen zeiget von GOtt uns eine", "tokens": ["Das", "Buch", "der", "Crea\u00b7tu\u00b7ren", "zei\u00b7get", "von", "Gott", "uns", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "PPER", "ART"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "klare Spur", "tokens": ["kla\u00b7re", "Spur"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "In Seinen Wundern, da\u00df Er sey. Doch, was Er sey,", "tokens": ["In", "Sei\u00b7nen", "Wun\u00b7dern", ",", "da\u00df", "Er", "sey", ".", "Doch", ",", "was", "Er", "sey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VAFIN", "$.", "KON", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "kann die Natur", "tokens": ["kann", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Dem Geiste nicht begreiflich machen. Die Offenbahrung", "tokens": ["Dem", "Geis\u00b7te", "nicht", "be\u00b7greif\u00b7lich", "ma\u00b7chen", ".", "Die", "Of\u00b7fen\u00b7bah\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "VVINF", "$.", "ART", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "selber lehrt:", "tokens": ["sel\u00b7ber", "lehrt", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Der Gottheit Angesicht zu sehen sey allen Sterblichen", "tokens": ["Der", "Got\u00b7theit", "An\u00b7ge\u00b7sicht", "zu", "se\u00b7hen", "sey", "al\u00b7len", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+--+-+--", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "verwehrt.", "tokens": ["ver\u00b7wehrt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Die\u00df wird noch mehr, da\u00df man Jhn hier im dunklen", "tokens": ["Die\u00df", "wird", "noch", "mehr", ",", "da\u00df", "man", "Jhn", "hier", "im", "dunk\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "$,", "KOUS", "PIS", "PPER", "ADV", "APPRART", "ADJA"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Wort nur sieht, erkl\u00e4rt.", "tokens": ["Wort", "nur", "sieht", ",", "er\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "$,", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Hieraus erhellt unwidersprechlich, die Gottheit, so un-", "tokens": ["Hier\u00b7aus", "er\u00b7hellt", "un\u00b7wi\u00b7der\u00b7sprech\u00b7lich", ",", "die", "Got\u00b7theit", ",", "so", "un"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "ADJD", "$,", "ART", "NN", "$,", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "endlich, wolle,", "tokens": ["end\u00b7lich", ",", "wol\u00b7le", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "VMFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Da\u00df man Sie nicht, so wie wir wollen, nein, wie Sie will,", "tokens": ["Da\u00df", "man", "Sie", "nicht", ",", "so", "wie", "wir", "wol\u00b7len", ",", "nein", ",", "wie", "Sie", "will", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "PTKNEG", "$,", "ADV", "KOKOM", "PPER", "VMFIN", "$,", "PTKANT", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "begreifen solle.", "tokens": ["be\u00b7grei\u00b7fen", "sol\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Giebt denn, bey so bestallten Sachen, diejenige Philosophie", "tokens": ["Giebt", "denn", ",", "bey", "so", "be\u00b7stall\u00b7ten", "Sa\u00b7chen", ",", "die\u00b7je\u00b7ni\u00b7ge", "Phi\u00b7lo\u00b7so\u00b7phie"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "APPR", "ADV", "ADJA", "NN", "$,", "PDAT", "NN"], "meter": "--+--+-+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "(die, was der Sch\u00f6pfer kann, und nicht kann, was Er ge-", "tokens": ["(", "die", ",", "was", "der", "Sch\u00f6p\u00b7fer", "kann", ",", "und", "nicht", "kann", ",", "was", "Er", "ge"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "$,", "PRELS", "ART", "NN", "VMFIN", "$,", "KON", "PTKNEG", "VMFIN", "$,", "PRELS", "PPER", "TRUNC"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "dacht, und nicht gedacht,", "tokens": ["dacht", ",", "und", "nicht", "ge\u00b7dacht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.18": {"text": "Was Er gewollt, und nicht gewollt, nach einer Schnur, die", "tokens": ["Was", "Er", "ge\u00b7wollt", ",", "und", "nicht", "ge\u00b7wollt", ",", "nach", "ei\u00b7ner", "Schnur", ",", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWS", "PPER", "VMPP", "$,", "KON", "PTKNEG", "VMPP", "$,", "APPR", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "sie gemacht,", "tokens": ["sie", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "Untrieglich abzumessen sucht) sich folglich nicht vergebne", "tokens": ["Un\u00b7trieg\u00b7lich", "ab\u00b7zu\u00b7mes\u00b7sen", "sucht", ")", "sich", "folg\u00b7lich", "nicht", "ver\u00b7geb\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVIZU", "VVFIN", "$(", "PRF", "ADV", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.21": {"text": "M\u00fch?", "tokens": ["M\u00fch", "?"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}}}}}