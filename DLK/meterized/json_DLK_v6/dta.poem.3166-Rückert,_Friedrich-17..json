{"dta.poem.3166": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du mu\u00dft zuviel nur von den Freunden nicht verlangen,", "tokens": ["Du", "mu\u00dft", "zu\u00b7viel", "nur", "von", "den", "Freun\u00b7den", "nicht", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ADV", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie m\u00f6gen gerne Dank f\u00fcr Weniges erlangen.", "tokens": ["Sie", "m\u00f6\u00b7gen", "ger\u00b7ne", "Dank", "f\u00fcr", "We\u00b7ni\u00b7ges", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NN", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht helfen wollen sie, doch wollen sie dir rathen;", "tokens": ["Nicht", "hel\u00b7fen", "wol\u00b7len", "sie", ",", "doch", "wol\u00b7len", "sie", "dir", "ra\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "$,", "ADV", "VMFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lohn' ihnen Gott, was sie um Gotteswillen thaten.", "tokens": ["Lohn'", "ih\u00b7nen", "Gott", ",", "was", "sie", "um", "Got\u00b7tes\u00b7wil\u00b7len", "tha\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "PRELS", "PPER", "APPR