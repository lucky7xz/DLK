{"dta.poem.13109": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201egenug: ich dichte stets, wie man es nennen will;", "tokens": ["\u201e", "ge\u00b7nug", ":", "ich", "dich\u00b7te", "stets", ",", "wie", "man", "es", "nen\u00b7nen", "will", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$.", "PPER", "VVFIN", "ADV", "$,", "PWAV", "PIS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201edie Wahrheit und die Lust seynd meiner Arbeit Ziel.", "tokens": ["\u201e", "die", "Wahr\u00b7heit", "und", "die", "Lust", "seynd", "mei\u00b7ner", "Ar\u00b7beit", "Ziel", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "KON", "ART", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "435\u201dEs k\u00f6nnen solcher Macht sich keine K\u00fcnste r\u00fchmen;", "tokens": ["\"", "Es", "k\u00f6n\u00b7nen", "sol\u00b7cher", "Macht", "sich", "kei\u00b7ne", "K\u00fcns\u00b7te", "r\u00fch\u00b7men", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PIAT", "NN", "PRF", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eso wird gewi\u00df nur mir der Bau des Wercks geziemen.", "tokens": ["\u201e", "so", "wird", "ge\u00b7wi\u00df", "nur", "mir", "der", "Bau", "des", "Wercks", "ge\u00b7zie\u00b7men", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ADV", "ADV", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eje mehr Unwissenheit und Einfalt mich verschm\u00e4ht,", "tokens": ["\u201e", "je", "mehr", "Un\u00b7wis\u00b7sen\u00b7heit", "und", "Ein\u00b7falt", "mich", "ver\u00b7schm\u00e4ht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PIAT", "NN", "KON", "NN", "PPER", "VVPP", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "\u201eje mehr wird meine Kunst von der Vernunft erh\u00f6ht.", "tokens": ["\u201e", "je", "mehr", "wird", "mei\u00b7ne", "Kunst", "von", "der", "Ver\u00b7nunft", "er\u00b7h\u00f6ht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "\u201ewer um des P\u00f6bels Lob und dessen Beyfall th\u00f6net,", "tokens": ["\u201e", "wer", "um", "des", "P\u00f6\u00b7bels", "Lob", "und", "des\u00b7sen", "Bey\u00b7fall", "th\u00f6\u00b7net", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "APPR", "ART", "NN", "NN", "KON", "PRELAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "440\u201dIst von versuchtem Wiz verachtet und verh\u00f6hnet;", "tokens": ["\"", "Ist", "von", "ver\u00b7such\u00b7tem", "Wiz", "ver\u00b7ach\u00b7tet", "und", "ver\u00b7h\u00f6h\u00b7net", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "APPR", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eweil jener Niedrigkeit und eitlen Schimmer liebt,", "tokens": ["\u201e", "weil", "je\u00b7ner", "Nied\u00b7rig\u00b7keit", "und", "eit\u00b7len", "Schim\u00b7mer", "liebt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PDAT", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eder aber dem den Ruhm, der ihn verdienet, gibt.", "tokens": ["\u201e", "der", "a\u00b7ber", "dem", "den", "Ruhm", ",", "der", "ihn", "ver\u00b7die\u00b7net", ",", "gibt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ART", "ADV", "ART", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eja selbst sich nach der Kunst, der K\u00fcnste Mutter, s\u00e4hnet,", "tokens": ["\u201e", "ja", "selbst", "sich", "nach", "der", "Kunst", ",", "der", "K\u00fcns\u00b7te", "Mut\u00b7ter", ",", "s\u00e4h\u00b7net", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PRF", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eund mich, weil er mich ehrt, mit Lorber-Zweigen kr\u00f6net.", "tokens": ["\u201e", "und", "mich", ",", "weil", "er", "mich", "ehrt", ",", "mit", "Lor\u00b7ber\u00b7Zwei\u00b7gen", "kr\u00f6\u00b7net", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}