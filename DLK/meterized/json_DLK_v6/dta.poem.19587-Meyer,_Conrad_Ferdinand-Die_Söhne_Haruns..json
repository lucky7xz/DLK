{"dta.poem.19587": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die S\u00f6hne Haruns.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Harun sprach zu seinen Kindern Assur, Assad, Scheherban:", "tokens": ["Ha\u00b7run", "sprach", "zu", "sei\u00b7nen", "Kin\u00b7dern", "As\u00b7sur", ",", "As\u00b7sad", ",", "Sche\u00b7her\u00b7ban", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,", "NE", "$,", "NE", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "\u201es\u00f6hne, werdet ihr vollenden, was ich k\u00fchnen Muths begann?", "tokens": ["\u201e", "s\u00f6h\u00b7ne", ",", "wer\u00b7det", "ihr", "voll\u00b7en\u00b7den", ",", "was", "ich", "k\u00fch\u00b7nen", "Muths", "be\u00b7gann", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VAFIN", "PPER", "VVINF", "$,", "PWS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Seit ich Bagdads Thron bestiegen, bin von Feinden ich umgeben!", "tokens": ["Seit", "ich", "Bag\u00b7dads", "Thron", "be\u00b7stie\u00b7gen", ",", "bin", "von", "Fein\u00b7den", "ich", "um\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "NN", "VVFIN", "$,", "VAFIN", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wie befestigt ihr die Herrschaft? Wie vertheidigt ihr mein Leben?\u201c", "tokens": ["Wie", "be\u00b7fes\u00b7tigt", "ihr", "die", "Herr\u00b7schaft", "?", "Wie", "ver\u00b7thei\u00b7digt", "ihr", "mein", "Le\u00b7ben", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$.", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Assur ruft, der feurig schlanke: \u201eSchleunig werb' ich Dir ein Heer!", "tokens": ["As\u00b7sur", "ruft", ",", "der", "feu\u00b7rig", "schlan\u00b7ke", ":", "\u201e", "Schleu\u00b7nig", "werb'", "ich", "Dir", "ein", "Heer", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "ADJD", "VVFIN", "$.", "$(", "ADJD", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Zimmre Masten, webe Segel! Ich bev\u00f6lkre Dir das Meer!", "tokens": ["Zimm\u00b7re", "Mas\u00b7ten", ",", "we\u00b7be", "Se\u00b7gel", "!", "Ich", "be\u00b7v\u00f6l\u00b7kre", "Dir", "das", "Meer", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "NN", "$.", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Rosse schul' ich. S\u00e4bel schmied' ich. Ich erbaue Dir Castelle.", "tokens": ["Ros\u00b7se", "schul'", "ich", ".", "S\u00e4\u00b7bel", "schmied'", "ich", ".", "Ich", "er\u00b7bau\u00b7e", "Dir", "Cas\u00b7tel\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$.", "NN", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+-+--+--", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Dir geh\u00f6ren Stadt und W\u00fcste! Dir gehorchen Strand und", "tokens": ["Dir", "ge\u00b7h\u00f6\u00b7ren", "Stadt", "und", "W\u00fcs\u00b7te", "!", "Dir", "ge\u00b7hor\u00b7chen", "Strand", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$.", "PPER", "ADJA", "NN", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.3": {"line.1": {"text": "Assad mit der schlauen Miene sinnt und \u00e4u\u00dfert sich bed\u00e4chtig:", "tokens": ["As\u00b7sad", "mit", "der", "schlau\u00b7en", "Mie\u00b7ne", "sinnt", "und", "\u00e4u\u00b7\u00dfert", "sich", "be\u00b7d\u00e4ch\u00b7tig", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "\u201esicher schaff' ich Deinen Schlummer, Sorgen machen \u00fcbern\u00e4chtig.", "tokens": ["\u201e", "si\u00b7cher", "schaff'", "ich", "Dei\u00b7nen", "Schlum\u00b7mer", ",", "Sor\u00b7gen", "ma\u00b7chen", "\u00fc\u00b7bern\u00b7\u00e4ch\u00b7tig", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "NN", "VVINF", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Traue Deinem Assad! W\u00e4hle mich zum Polizeiminister!", "tokens": ["Trau\u00b7e", "Dei\u00b7nem", "As\u00b7sad", "!", "W\u00e4h\u00b7le", "mich", "zum", "Po\u00b7li\u00b7zei\u00b7mi\u00b7nis\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Jeden Athemzug belausch' ich, jedes heimliche Gefl\u00fcster.", "tokens": ["Je\u00b7den", "A\u00b7them\u00b7zug", "be\u00b7lausch'", "ich", ",", "je\u00b7des", "heim\u00b7li\u00b7che", "Ge\u00b7fl\u00fcs\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Wirthe, Kuppler und Barbiere, jedem setz' ich einen Sold,", "tokens": ["Wirt\u00b7he", ",", "Kupp\u00b7ler", "und", "Bar\u00b7bie\u00b7re", ",", "je\u00b7dem", "setz'", "ich", "ei\u00b7nen", "Sold", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,", "PIS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df ein jeder mir berichte, ", "tokens": ["Da\u00df", "ein", "je\u00b7der", "mir", "be\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Harun l\u00e4chelt. Zu dem j\u00fcngsten, seinem Liebling, sagt er:", "tokens": ["Ha\u00b7run", "l\u00e4\u00b7chelt", ".", "Zu", "dem", "j\u00fcng\u00b7sten", ",", "sei\u00b7nem", "Lieb\u00b7ling", ",", "sagt", "er", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "APPR", "ART", "ADJA", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Wie besch\u00e4mst du deine Br\u00fcder? Zarter Scheherban, was", "tokens": ["Wie", "be\u00b7sch\u00e4mst", "du", "dei\u00b7ne", "Br\u00fc\u00b7der", "?", "Zar\u00b7ter", "Sche\u00b7her\u00b7ban", ",", "was"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "NN", "NE", "$,", "PWS"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.5": {"line.1": {"text": "\u201evater\u201c, redet jetzt der J\u00fcngste keusch err\u00f6thend, \u201ees ist gut,", "tokens": ["\u201e", "va\u00b7ter", "\u201c", ",", "re\u00b7det", "jetzt", "der", "J\u00fcngs\u00b7te", "keusch", "er\u00b7r\u00f6\u00b7thend", ",", "\u201e", "es", "ist", "gut", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$(", "$,", "VVFIN", "ADV", "ART", "NN", "ADJD", "VVPP", "$,", "$(", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-+---+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Da\u00df ein Tropfen rinne nieder warm ins Volk aus Deinem Blut!", "tokens": ["Da\u00df", "ein", "Trop\u00b7fen", "rin\u00b7ne", "nie\u00b7der", "warm", "ins", "Volk", "aus", "Dei\u00b7nem", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "PTKVZ", "ADJD", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ueber ungez\u00e4hlte Loose bist allm\u00e4chtig Du auf Erden,", "tokens": ["Ue\u00b7ber", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Loo\u00b7se", "bist", "all\u00b7m\u00e4ch\u00b7tig", "Du", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ADJD", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Das ist Raub an Deinen Br\u00fcdern \u2014 und Du wirst gerichtet werden!", "tokens": ["Das", "ist", "Raub", "an", "Dei\u00b7nen", "Br\u00fc\u00b7dern", "und", "Du", "wirst", "ge\u00b7rich\u00b7tet", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$(", "KON", "PPER", "VAFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "Dein erhaben Loos zu s\u00fchnen, das sich th\u00fcrmt den Blitzen zu,", "tokens": ["Dein", "er\u00b7ha\u00b7ben", "Loos", "zu", "s\u00fch\u00b7nen", ",", "das", "sich", "th\u00fcrmt", "den", "Blit\u00b7zen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,", "PRELS", "PRF", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "La\u00df mich in des Lebens dunkle Tiefe niedertauchen Du!", "tokens": ["La\u00df", "mich", "in", "des", "Le\u00b7bens", "dunk\u00b7le", "Tie\u00b7fe", "nie\u00b7der\u00b7tau\u00b7chen", "Du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Such mich nicht! Ich ging verloren! Sende weder Kleid noch", "tokens": ["Such", "mich", "nicht", "!", "Ich", "ging", "ver\u00b7lo\u00b7ren", "!", "Sen\u00b7de", "we\u00b7der", "Kleid", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "VVPP", "$.", "NN", "KON", "NN", "ADV"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Wie der Aermste will ich leben von der Arbeit meiner H\u00e4nde!", "tokens": ["Wie", "der", "A\u00b7erms\u00b7te", "will", "ich", "le\u00b7ben", "von", "der", "Ar\u00b7beit", "mei\u00b7ner", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Mit dem Hammer, mit der Kelle la\u00df mich, Herr, ein Maurer sein!", "tokens": ["Mit", "dem", "Ham\u00b7mer", ",", "mit", "der", "Kel\u00b7le", "la\u00df", "mich", ",", "Herr", ",", "ein", "Mau\u00b7rer", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "VVIMP", "PPER", "$,", "NN", "$,", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Selber maur' ich mich in Deines Gl\u00fcckes Grund und Boden ein!", "tokens": ["Sel\u00b7ber", "maur'", "ich", "mich", "in", "Dei\u00b7nes", "Gl\u00fc\u00b7ckes", "Grund", "und", "Bo\u00b7den", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Jedem Hause wird ein Zauber, da\u00df es unzerst\u00f6rlich dauert,", "tokens": ["Je\u00b7dem", "Hau\u00b7se", "wird", "ein", "Zau\u00b7ber", ",", "da\u00df", "es", "un\u00b7zer\u00b7st\u00f6r\u00b7lich", "dau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Etwas Liebes, etwas Theures in den Grundstein eingemauert!", "tokens": ["Et\u00b7was", "Lie\u00b7bes", ",", "et\u00b7was", "Theu\u00b7res", "in", "den", "Grund\u00b7stein", "ein\u00b7ge\u00b7mau\u00b7ert", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "PIAT", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "H\u00f6rest Du die Stra\u00dfe rauschen unter Deinem Marmorschlo\u00df?", "tokens": ["H\u00f6\u00b7rest", "Du", "die", "Stra\u00b7\u00dfe", "rau\u00b7schen", "un\u00b7ter", "Dei\u00b7nem", "Mar\u00b7mor\u00b7schlo\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Morgen bin ich dieser Menge namenloser Tischgeno\u00df \u2014", "tokens": ["Mor\u00b7gen", "bin", "ich", "die\u00b7ser", "Men\u00b7ge", "na\u00b7men\u00b7lo\u00b7ser", "Tischge\u00b7no\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PDAT", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "Wenn Dich die Beherrschten l\u00e4stern, segnet Einer, Herr, Dich", "tokens": ["Wenn", "Dich", "die", "Be\u00b7herrschten", "l\u00e4s\u00b7tern", ",", "seg\u00b7net", "Ei\u00b7ner", ",", "Herr", ",", "Dich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$,", "VVFIN", "PIS", "$,", "NN", "$,", "PPER"], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wenn Dich die Enterbten hassen, Einer, Vater, liebt Dich kindlich!\u201c", "tokens": ["Wenn", "Dich", "die", "En\u00b7terb\u00b7ten", "has\u00b7sen", ",", "Ei\u00b7ner", ",", "Va\u00b7ter", ",", "liebt", "Dich", "kind\u00b7lich", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$,", "PIS", "$,", "NN", "$,", "VVFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}}}}}