{"textgrid.poem.36541": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "13. Der Esel in der L\u00f6wenhaut", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In eines L\u00f6wen Haut verbarg ein Esel sich,", "tokens": ["In", "ei\u00b7nes", "L\u00f6\u00b7wen", "Haut", "ver\u00b7barg", "ein", "E\u00b7sel", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ging auf den M\u00fchlenhof, und wer ihn sah, entwich,", "tokens": ["Ging", "auf", "den", "M\u00fch\u00b7len\u00b7hof", ",", "und", "wer", "ihn", "sah", ",", "ent\u00b7wich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "KON", "PWS", "PPER", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sagt' es in dem Dorf umher,", "tokens": ["Und", "sagt'", "es", "in", "dem", "Dorf", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df auf dem Hof' eine L\u00f6we w\u00e4r'.", "tokens": ["Da\u00df", "auf", "dem", "Hof'", "ei\u00b7ne", "L\u00f6\u00b7we", "w\u00e4r'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NE", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein L\u00f6w'? eine L\u00f6we \u2013 Ja! man siehet ihn, erschrickt,", "tokens": ["Ein", "L\u00f6\u00b7w'", "?", "ei\u00b7ne", "L\u00f6\u00b7we", "\u2013", "Ja", "!", "man", "sie\u00b7het", "ihn", ",", "er\u00b7schrickt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NE", "$(", "PTKANT", "$.", "PIS", "VVFIN", "PPER", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Entflieht, so weit man kann! bald aber, bald erblickt", "tokens": ["Ent\u00b7flieht", ",", "so", "weit", "man", "kann", "!", "bald", "a\u00b7ber", ",", "bald", "er\u00b7blickt"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "ADJD", "PIS", "VMFIN", "$.", "ADV", "ADV", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des M\u00fcllers gro\u00dfer Hund ein Zipfelchen vom Ohr:", "tokens": ["Des", "M\u00fcl\u00b7lers", "gro\u00b7\u00dfer", "Hund", "ein", "Zip\u00b7fel\u00b7chen", "vom", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Held ruft er, gro\u00dfer Held! aus deiner Haut hervor!", "tokens": ["Held", "ruft", "er", ",", "gro\u00b7\u00dfer", "Held", "!", "aus", "dei\u00b7ner", "Haut", "her\u00b7vor", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$.", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Springt mutig auf ihn zu, tr\u00e4nkt den Betrug ihm ein!", "tokens": ["Springt", "mu\u00b7tig", "auf", "ihn", "zu", ",", "tr\u00e4nkt", "den", "Be\u00b7trug", "ihm", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$,", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.6": {"text": "Der Esel schreit, und will keine L\u00f6we wieder sein!", "tokens": ["Der", "E\u00b7sel", "schreit", ",", "und", "will", "kei\u00b7ne", "L\u00f6\u00b7we", "wie\u00b7der", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "VMFIN", "PIAT", "NE", "ADV", "VAINF", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}