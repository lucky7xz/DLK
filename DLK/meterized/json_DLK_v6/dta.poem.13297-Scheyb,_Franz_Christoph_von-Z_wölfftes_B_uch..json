{"dta.poem.13297": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ejhr wi\u00dft die Schifffart noch?\u201e so sprachen sie zusam\u0303en,", "tokens": ["\u201e", "jhr", "wi\u00dft", "die", "Schiff\u00b7fart", "noch", "?", "\u201e", "so", "spra\u00b7chen", "sie", "zu\u00b7sam\u0303en", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "ADV", "$.", "$(", "ADV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201evon welcher dieses Throns Gl\u00fcckseeligkeiten stammen?", "tokens": ["\u201e", "von", "wel\u00b7cher", "die\u00b7ses", "Throns", "Gl\u00fcck\u00b7see\u00b7lig\u00b7kei\u00b7ten", "stam\u00b7men", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PDAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Reise nacher Franckfurt\nwar den 15. Septembr. 1745. ange-\ntretten.", "tokens": ["Die", "Rei\u00b7se", "na\u00b7cher", "Fran\u00b7ck\u00b7furt", "war", "den", "15.", "Sep\u00b7tembr", ".", "1745", ".", "an\u00b7ge", "tret\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "ordinal", "word", "punct", "number", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NE", "VAFIN", "ART", "ADJA", "NN", "$.", "CARD", "$.", "TRUNC", "VVINF", "$."], "meter": "-+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.4": {"text": "Jhro K\u00f6nigl. Maj. Zuruckkunft\nvon Prag und Linz/ welche den 4 Julij\n1743. Nachmittag um 5. Uhr geschahe.", "tokens": ["Ih\u00b7ro", "K\u00f6\u00b7nigl", ".", "Maj", ".", "Zu\u00b7ruck\u00b7kunft", "von", "Prag", "und", "Linz", "/", "wel\u00b7che", "den", "4", "Ju\u00b7lij", "1743", ".", "Nach\u00b7mit\u00b7tag", "um", "5.", "Uhr", "ge\u00b7scha\u00b7he", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "number", "word", "number", "punct", "word", "word", "ordinal", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "NE", "$.", "NN", "APPR", "NE", "KON", "NE", "$(", "PRELS", "ART", "CARD", "NN", "CARD", "$.", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+---+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "\u201ewie fr\u00f6hlich hatte nicht das Land damahls erth\u00f6nt?", "tokens": ["\u201e", "wie", "fr\u00f6h\u00b7lich", "hat\u00b7te", "nicht", "das", "Land", "da\u00b7mahls", "er\u00b7th\u00f6nt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJD", "VAFIN", "PTKNEG", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ehat sich nicht Alt und Jung nach unserm Schiff ges\u00e4hnt?", "tokens": ["\u201e", "hat", "sich", "nicht", "Alt", "und", "Jung", "nach", "un\u00b7serm", "Schiff", "ge\u00b7s\u00e4hnt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PRF", "PTKNEG", "ADJD", "KON", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.7": {"text": "175\u201dWir schwammen ja daher, als m\u00fc\u00dften Flutt und Wellen", "tokens": ["\"", "Wir", "schwam\u00b7men", "ja", "da\u00b7her", ",", "als", "m\u00fc\u00df\u00b7ten", "Flutt", "und", "Wel\u00b7len"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ADV", "PAV", "$,", "KOUS", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eauf unsers Haupts Befehl sich zum Gehorsam stellen.", "tokens": ["\u201e", "auf", "un\u00b7sers", "Haupts", "Be\u00b7fehl", "sich", "zum", "Ge\u00b7hor\u00b7sam", "stel\u00b7len", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "NN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201esie rollt- und schwelleten und welzten sich so sch\u00f6n,", "tokens": ["\u201e", "sie", "roll\u00b7t", "und", "schwel\u00b7le\u00b7ten", "und", "welz\u00b7ten", "sich", "so", "sch\u00f6n", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "TRUNC", "KON", "VVFIN", "KON", "VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "\u201eals freuten sie sich selbst die Last des Schiffs zu sehn.", "tokens": ["\u201e", "als", "freu\u00b7ten", "sie", "sich", "selbst", "die", "Last", "des", "Schiffs", "zu", "sehn", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "VVFIN", "PPER", "PRF", "ADV", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201esie trugen uns so sanft, als h\u00e4tt der Flu\u00df auch H\u00e4nde.", "tokens": ["\u201e", "sie", "tru\u00b7gen", "uns", "so", "sanft", ",", "als", "h\u00e4tt", "der", "Flu\u00df", "auch", "H\u00e4n\u00b7de", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,", "KOKOM", "VAFIN", "ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}