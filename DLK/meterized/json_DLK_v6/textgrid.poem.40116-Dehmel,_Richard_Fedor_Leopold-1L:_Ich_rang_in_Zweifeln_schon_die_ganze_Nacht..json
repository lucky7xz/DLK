{"textgrid.poem.40116": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich rang in Zweifeln schon die ganze Nacht.", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich rang in Zweifeln schon die ganze Nacht.", "tokens": ["Ich", "rang", "in", "Zwei\u00b7feln", "schon", "die", "gan\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mich treibt ein Geist, und folgen mu\u00df ich ihm;", "tokens": ["Mich", "treibt", "ein", "Geist", ",", "und", "fol\u00b7gen", "mu\u00df", "ich", "ihm", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "VVINF", "VMFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "doch ", "tokens": ["doch"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "ist's Eitelmut? so zagte meine Seele.", "tokens": ["ist's", "Ei\u00b7tel\u00b7mut", "?", "so", "zag\u00b7te", "mei\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und Furcht ergriff mich vor dem unverstandnen", "tokens": ["Und", "Furcht", "er\u00b7griff", "mich", "vor", "dem", "un\u00b7ver\u00b7stand\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gebet der Kindheit: Nicht wie Ich will, Vater \u2013", "tokens": ["Ge\u00b7bet", "der", "Kind\u00b7heit", ":", "Nicht", "wie", "Ich", "will", ",", "Va\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "PTKNEG", "PWAV", "PPER", "VMFIN", "$,", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "in ", "tokens": ["in"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Und heft'ger rang ich, wie einst Jesus rang ...", "tokens": ["Und", "heft'\u00b7ger", "rang", "ich", ",", "wie", "einst", "Je\u00b7sus", "rang", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "PWAV", "ADV", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Da f\u00fchrte mich der Geist hinweg. Ich stand", "tokens": ["Da", "f\u00fchr\u00b7te", "mich", "der", "Geist", "hin\u00b7weg", ".", "Ich", "stand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "an eines Weltmeers sturmgeworfner Fl\u00e4che.", "tokens": ["an", "ei\u00b7nes", "Welt\u00b7meers", "sturm\u00b7ge\u00b7worf\u00b7ner", "Fl\u00e4\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Sehr finster war's. Doch fernher sah ich ragen,", "tokens": ["Sehr", "fins\u00b7ter", "wa\u00b7r'", "s.", "Doch", "fern\u00b7her", "sah", "ich", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "NE", "KON", "ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "im d\u00fcstern Graulicht d\u00fcstrer noch get\u00fcrmt,", "tokens": ["im", "d\u00fcs\u00b7tern", "Grau\u00b7licht", "d\u00fcst\u00b7rer", "noch", "ge\u00b7t\u00fcrmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "ein starr Gebilde wie ein Felseneiland.", "tokens": ["ein", "starr", "Ge\u00b7bil\u00b7de", "wie", "ein", "Fel\u00b7sen\u00b7ei\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die Wogen rollten und die Tiefe br\u00fcllte,", "tokens": ["Die", "Wo\u00b7gen", "roll\u00b7ten", "und", "die", "Tie\u00b7fe", "br\u00fcll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "und ich erkannte: eine Sintflut war's,", "tokens": ["und", "ich", "er\u00b7kann\u00b7te", ":", "ei\u00b7ne", "Sint\u00b7flut", "wa\u00b7r's", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "die eine alte Welt hinunterschlang.", "tokens": ["die", "ei\u00b7ne", "al\u00b7te", "Welt", "hin\u00b7un\u00b7ter\u00b7schlang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "In grauenhafter Ohnmacht mit den Wellen", "tokens": ["In", "grau\u00b7en\u00b7haf\u00b7ter", "Ohn\u00b7macht", "mit", "den", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.18": {"text": "zwei letzte Menschen rangen, Mann und Weib.", "tokens": ["zwei", "letz\u00b7te", "Men\u00b7schen", "ran\u00b7gen", ",", "Mann", "und", "Weib", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ich sah sie sinken. Doch noch einmal tauchten", "tokens": ["Ich", "sah", "sie", "sin\u00b7ken", ".", "Doch", "noch", "ein\u00b7mal", "tauch\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$.", "KON", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "des Weibes Glieder krampfig zuckend hoch,", "tokens": ["des", "Wei\u00b7bes", "Glie\u00b7der", "kramp\u00b7fig", "zu\u00b7ckend", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "noch einmal \u00e4chzte sie: und ihrem Schoo\u00df", "tokens": ["noch", "ein\u00b7mal", "\u00e4chz\u00b7te", "sie", ":", "und", "ih\u00b7rem", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$.", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "entwand im Schaume sich ein bl\u00fchend Kind.", "tokens": ["ent\u00b7wand", "im", "Schau\u00b7me", "sich", "ein", "bl\u00fc\u00b7hend", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PRF", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Aus Wolken pl\u00f6tzlich quoll der volle Mond,", "tokens": ["Aus", "Wol\u00b7ken", "pl\u00f6tz\u00b7lich", "quoll", "der", "vol\u00b7le", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "die Fluten schwiegen und die Wellen h\u00fcpften,", "tokens": ["die", "Flu\u00b7ten", "schwie\u00b7gen", "und", "die", "Wel\u00b7len", "h\u00fcpf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "und wiegend trugen sie das neue Leben", "tokens": ["und", "wie\u00b7gend", "tru\u00b7gen", "sie", "das", "neu\u00b7e", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "auf sanften Armen an das Felsgestade.", "tokens": ["auf", "sanf\u00b7ten", "Ar\u00b7men", "an", "das", "Fels\u00b7ge\u00b7sta\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Und nun gewahrt' ich auf dem schroffen Gipfel", "tokens": ["Und", "nun", "ge\u00b7wahrt'", "ich", "auf", "dem", "schrof\u00b7fen", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "ein ", "tokens": ["ein"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.29": {"text": "in regungsloser Starrheit thronte sie;", "tokens": ["in", "re\u00b7gungs\u00b7lo\u00b7ser", "Star\u00b7rheit", "thron\u00b7te", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "sie sa\u00df, als ob ihr Haupt den Himmel r\u00fchrte,", "tokens": ["sie", "sa\u00df", ",", "als", "ob", "ihr", "Haupt", "den", "Him\u00b7mel", "r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "und Scheu befiel mich vor der Wundersamen.", "tokens": ["und", "Scheu", "be\u00b7fiel", "mich", "vor", "der", "Wun\u00b7der\u00b7sa\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Doch l\u00e4chelnd langte nach ihr auf das ", "tokens": ["Doch", "l\u00e4\u00b7chelnd", "lang\u00b7te", "nach", "ihr", "auf", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Und nieder zu ihm neigte sich die Hohe", "tokens": ["Und", "nie\u00b7der", "zu", "ihm", "neig\u00b7te", "sich", "die", "Ho\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "APPR", "PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "und nahm es mit gelassner Hand ans Herz", "tokens": ["und", "nahm", "es", "mit", "ge\u00b7lass\u00b7ner", "Hand", "ans", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "und s\u00e4ugte es \u2013 und k\u00fc\u00dfte es \u2013 und schaute", "tokens": ["und", "s\u00e4ug\u00b7te", "es", "\u2013", "und", "k\u00fc\u00df\u00b7te", "es", "\u2013", "und", "schau\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$(", "KON", "VVFIN", "PPER", "$(", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "ihm lang' ins Auge, und mit mildem Glanz", "tokens": ["ihm", "lang'", "ins", "Au\u00b7ge", ",", "und", "mit", "mil\u00b7dem", "Glanz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPRART", "NN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "umfing ihr Blick des Kindes Angesicht;", "tokens": ["um\u00b7fing", "ihr", "Blick", "des", "Kin\u00b7des", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "es war, als wachte drin die Seele auf.", "tokens": ["es", "war", ",", "als", "wach\u00b7te", "drin", "die", "See\u00b7le", "auf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Und in dem Arm der G\u00f6ttin wuchs das Kind", "tokens": ["Und", "in", "dem", "Arm", "der", "G\u00f6t\u00b7tin", "wuchs", "das", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "und wuchs und wuchs und \u2013 sprach das erste Wort.", "tokens": ["und", "wuchs", "und", "wuchs", "und", "\u2013", "sprach", "das", "ers\u00b7te", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "$(", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Da nahm es von der Brust die R\u00e4tselhafte", "tokens": ["Da", "nahm", "es", "von", "der", "Brust", "die", "R\u00e4t\u00b7sel\u00b7haf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "und setzte mit gelassner Hand es wieder", "tokens": ["und", "setz\u00b7te", "mit", "ge\u00b7lass\u00b7ner", "Hand", "es", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "hinab ans Ufer, wo ein ", "tokens": ["hin\u00b7ab", "ans", "U\u00b7fer", ",", "wo", "ein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "PWAV", "ART"], "meter": "-+-+-++", "measure": "unknown.measure.tetra"}, "line.44": {"text": "sich aus den Fluten hob, und \u2013 hie\u00df es gehen;", "tokens": ["sich", "aus", "den", "Flu\u00b7ten", "hob", ",", "und", "\u2013", "hie\u00df", "es", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$,", "KON", "$(", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "mit stummem Wink wies in die Ferne sie,", "tokens": ["mit", "stum\u00b7mem", "Wink", "wies", "in", "die", "Fer\u00b7ne", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "dann sa\u00df sie ehern thronend wieder da.", "tokens": ["dann", "sa\u00df", "sie", "e\u00b7hern", "thro\u00b7nend", "wie\u00b7der", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Auf stand der Knabe, Scheu befiel auch ihn,", "tokens": ["Auf", "stand", "der", "Kna\u00b7be", ",", "Scheu", "be\u00b7fiel", "auch", "ihn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$,", "NN", "VVFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "der erste Schmerz schlich \u00fcber seine Stirne;", "tokens": ["der", "ers\u00b7te", "Schmerz", "schlich", "\u00fc\u00b7ber", "sei\u00b7ne", "Stir\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "doch still gehorchend ging er, schritt und wuchs,", "tokens": ["doch", "still", "ge\u00b7hor\u00b7chend", "ging", "er", ",", "schritt", "und", "wuchs", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VVFIN", "PPER", "$,", "VVFIN", "KON", "VVFIN"