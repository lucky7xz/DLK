{"dta.poem.3379": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Unsterblichkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist unsterblich!", "tokens": ["Du", "bist", "uns\u00b7terb\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Freue dich, Sohn des Staubes;", "tokens": ["Freu\u00b7e", "dich", ",", "Sohn", "des", "Stau\u00b7bes", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der du schmachtest nach Ewigkeit, jauchze", "tokens": ["Der", "du", "schmach\u00b7test", "nach", "E\u00b7wig\u00b7keit", ",", "jauch\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NN", "$,", "VVFIN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "laut:", "tokens": ["laut", ":"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Du bist unsterblich!", "tokens": ["Du", "bist", "uns\u00b7terb\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Was du ersehnet, erdurstet hast", "tokens": ["Was", "du", "er\u00b7seh\u00b7net", ",", "er\u00b7durs\u00b7tet", "hast"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVPP", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit dem Durste des J\u00fcnglings nach dem ersten", "tokens": ["Mit", "dem", "Durs\u00b7te", "des", "J\u00fcng\u00b7lings", "nach", "dem", "ers\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Kusse der Einen;", "tokens": ["Kus\u00b7se", "der", "Ei\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Siehe, das ist dir erschienen,", "tokens": ["Sie\u00b7he", ",", "das", "ist", "dir", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PDS", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und Anschaun worden das Ahnen.", "tokens": ["Und", "An\u00b7schaun", "wor\u00b7den", "das", "Ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAPP", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Was du erflehet, erbetet dir hast,", "tokens": ["Was", "du", "er\u00b7fle\u00b7het", ",", "er\u00b7be\u00b7tet", "dir", "hast", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ergreifet dich, wie den J\u00fcngling die Wonne,", "tokens": ["Er\u00b7grei\u00b7fet", "dich", ",", "wie", "den", "J\u00fcng\u00b7ling", "die", "Won\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-++-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die schmerzliche, s\u00fcsse, ersch\u00fctternd ergreift,", "tokens": ["Die", "schmerz\u00b7li\u00b7che", ",", "s\u00fcs\u00b7se", ",", "er\u00b7sch\u00fct\u00b7ternd", "er\u00b7greift", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "VVPP", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Dass die Geliebte ihn liebe.", "tokens": ["Dass", "die", "Ge\u00b7lieb\u00b7te", "ihn", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Wie den Wiedergebornen der Gnade Gef\u00fchl", "tokens": ["Wie", "den", "Wie\u00b7der\u00b7ge\u00b7bor\u00b7nen", "der", "Gna\u00b7de", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Erfasst mit Schauerentz\u00fccken;", "tokens": ["Er\u00b7fasst", "mit", "Schau\u00b7e\u00b7rent\u00b7z\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So erfasst mich mit Schauern Gottes,", "tokens": ["So", "er\u00b7fasst", "mich", "mit", "Schau\u00b7ern", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NN", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Unsterblichkeit, dein grosses Gef\u00fchl!", "tokens": ["U\u00b7nsterb\u00b7lich\u00b7keit", ",", "dein", "gros\u00b7ses", "Ge\u00b7f\u00fchl", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "L\u00e4ngst ahnet', l\u00e4ngst hofft' ichs; itzt glaub' ich,", "tokens": ["L\u00e4ngst", "ah\u00b7net'", ",", "l\u00e4ngst", "hofft'", "ichs", ";", "itzt", "glaub'", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "PIS", "$.", "ADV", "VVFIN", "PPER", "$,"], "meter": "----+-+++", "measure": "unknown.measure.tetra"}, "line.2": {"text": "dass ich bin!", "tokens": ["dass", "ich", "bin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ich glaub' und f\u00fchle, dass ich ewig bin. \u2014", "tokens": ["Ich", "glaub'", "und", "f\u00fch\u00b7le", ",", "dass", "ich", "e\u00b7wig", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Neige deine Wipfel, Eiche!", "tokens": ["Nei\u00b7ge", "dei\u00b7ne", "Wip\u00b7fel", ",", "Ei\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ein Unsterblicher wandelt unter dir.", "tokens": ["Ein", "U\u00b7nsterb\u00b7li\u00b7cher", "wan\u00b7delt", "un\u00b7ter", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "R\u00fcnde die silberne Scheibe, Mond!", "tokens": ["R\u00fcn\u00b7de", "die", "sil\u00b7ber\u00b7ne", "Schei\u00b7be", ",", "Mond", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Entblinket dem Nachtged\u00fcft, schimmer\u00e4ugige", "tokens": ["Ent\u00b7blin\u00b7ket", "dem", "Nacht\u00b7ge\u00b7d\u00fcft", ",", "schim\u00b7mer\u00b7\u00e4u\u00b7gi\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJA"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sterne!", "tokens": ["Ster\u00b7ne", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Sirius, w\u00e4lze dein Flammenrad! Glanzge-", "tokens": ["Si\u00b7rius", ",", "w\u00e4l\u00b7ze", "dein", "Flam\u00b7men\u00b7rad", "!", "Glanz\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "VVFIN", "PPOSAT", "NN", "$.", "TRUNC"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "g\u00fcrteter Orion,", "tokens": ["g\u00fcr\u00b7te\u00b7ter", "O\u00b7rion", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wandle stattlich den Riesengang!", "tokens": ["Wand\u00b7le", "statt\u00b7lich", "den", "Rie\u00b7sen\u00b7gang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Wonne! Stolz! Entz\u00fccken!", "tokens": ["Won\u00b7ne", "!", "Stolz", "!", "Ent\u00b7z\u00fc\u00b7cken", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ich bin unsterblich!", "tokens": ["Ich", "bin", "uns\u00b7terb\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Mehr als die Eich' und der Mond, mehr als", "tokens": ["Mehr", "als", "die", "Eich'", "und", "der", "Mond", ",", "mehr", "als"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "KOKOM", "ART", "NN", "KON", "ART", "NN", "$,", "PIAT", "KOKOM"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Orion und Sirius", "tokens": ["O\u00b7rion", "und", "Si\u00b7rius"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Bin ich \u2014 ich bin unsterblich!", "tokens": ["Bin", "ich", "ich", "bin", "uns\u00b7terb\u00b7lich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Himmel und Erde vergehn!", "tokens": ["Him\u00b7mel", "und", "Er\u00b7de", "ver\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Ich vergehe nicht. \u2014 \u2014", "tokens": ["Ich", "ver\u00b7ge\u00b7he", "nicht", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "$(", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ach, wenn ich verginge \u2014", "tokens": ["Ach", ",", "wenn", "ich", "ver\u00b7gin\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ewige Liebe, wer w\u00e4r' ich dann?", "tokens": ["E\u00b7wi\u00b7ge", "Lie\u00b7be", ",", "wer", "w\u00e4r'", "ich", "dann", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWS", "VAFIN", "PPER", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Staub, Schatten, Traum,", "tokens": ["Staub", ",", "Schat\u00b7ten", ",", "Traum", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Gestern gerufen aus dem Nichts,", "tokens": ["Ge\u00b7stern", "ge\u00b7ru\u00b7fen", "aus", "dem", "Nichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Heute zur\u00fcckgeworfen ins \u00f6de Nichts \u2014", "tokens": ["Heu\u00b7te", "zu\u00b7r\u00fcck\u00b7ge\u00b7wor\u00b7fen", "ins", "\u00f6\u00b7de", "Nichts"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Der w\u00e4r' ich!", "tokens": ["Der", "w\u00e4r'", "ich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.10": {"line.1": {"text": "\u00c4rmer noch w\u00e4r' ich, als der Halm und das Gras;", "tokens": ["\u00c4r\u00b7mer", "noch", "w\u00e4r'", "ich", ",", "als", "der", "Halm", "und", "das", "Gras", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ver\u00e4chtlicher noch, als der Kiesel der Gasse.", "tokens": ["Ver\u00b7\u00e4cht\u00b7li\u00b7cher", "noch", ",", "als", "der", "Kie\u00b7sel", "der", "Gas\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Des Daseyns Entz\u00fccken empfanden sie nicht;", "tokens": ["Des", "Da\u00b7seyns", "Ent\u00b7z\u00fc\u00b7cken", "emp\u00b7fan\u00b7den", "sie", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dein Grauen, Vernichtung, empfinden", "tokens": ["Dein", "Grau\u00b7en", ",", "Ver\u00b7nich\u00b7tung", ",", "emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "sie nimmer, nimmer.", "tokens": ["sie", "nim\u00b7mer", ",", "nim\u00b7mer", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Ach, wenn ich ewig nicht w\u00e4re,", "tokens": ["Ach", ",", "wenn", "ich", "e\u00b7wig", "nicht", "w\u00e4\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADJD", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So \u00e4chzt' ich dem kommenden Tag'", "tokens": ["So", "\u00e4chzt'", "ich", "dem", "kom\u00b7men\u00b7den", "Tag'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Entgegen, so \u00e4chzt' ich, k\u00e4me die Nacht,", "tokens": ["Ent\u00b7ge\u00b7gen", ",", "so", "\u00e4chzt'", "ich", ",", "k\u00e4\u00b7me", "die", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und verh\u00fcllte mich, und schwiege", "tokens": ["Und", "ver\u00b7h\u00fcll\u00b7te", "mich", ",", "und", "schwie\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "verzweifelnd.", "tokens": ["ver\u00b7zwei\u00b7felnd", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.12": {"line.1": {"text": "So w\u00fcrd' ich unter die Blumen des Fr\u00fchlings", "tokens": ["So", "w\u00fcrd'", "ich", "un\u00b7ter", "die", "Blu\u00b7men", "des", "Fr\u00fch\u00b7lings"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mich schmiegen, mich kr\u00fcmmen, und die Blume", "tokens": ["Mich", "schmie\u00b7gen", ",", "mich", "kr\u00fcm\u00b7men", ",", "und", "die", "Blu\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$,", "KON", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "beneiden.", "tokens": ["be\u00b7nei\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Du, o bl\u00fchende Erde, d\u00e4uchtest mir ein", "tokens": ["Du", ",", "o", "bl\u00fc\u00b7hen\u00b7de", "Er\u00b7de", ",", "d\u00e4uch\u00b7test", "mir", "ein"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"],