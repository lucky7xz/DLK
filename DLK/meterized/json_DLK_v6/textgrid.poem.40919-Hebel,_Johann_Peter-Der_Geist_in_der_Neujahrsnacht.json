{"textgrid.poem.40919": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Geist in der Neujahrsnacht", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tochter, such e Strumpf, und stopfen do hinten ins Fenster,", "tokens": ["Toch\u00b7ter", ",", "such", "e", "Strumpf", ",", "und", "stop\u00b7fen", "do", "hin\u00b7ten", "ins", "Fens\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "NE", "NN", "$,", "KON", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "wo h\u00fct 's B\u00fcebli mittem Stecke d'Schibe verheit het.", "tokens": ["wo", "h\u00fct", "'s", "B\u00fceb\u00b7li", "mit\u00b7tem", "Ste\u00b7cke", "d'\u00b7Schi\u00b7be", "ver\u00b7heit", "het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "ADJA", "NN", "PDAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Gschicht ich im neue Johr kei gr\u00f6\u00dfer Ungl\u00fcck, as das isch,", "tokens": ["Gschicht", "ich", "im", "neu\u00b7e", "Johr", "kei", "gr\u00f6\u00b7\u00dfer", "Un\u00b7gl\u00fcck", ",", "as", "das", "isch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "ADJA", "NN", "PIAT", "ADJA", "NN", "$,", "PRELS", "ART", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "ch\u00f6nneter z'friede si. Doch weiht's mer so frostig im \u00c4cke,", "tokens": ["ch\u00f6n\u00b7ne\u00b7ter", "z'\u00b7frie\u00b7de", "si", ".", "Doch", "weiht's", "mer", "so", "fros\u00b7tig", "im", "\u00c4\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "KON", "VVFIN", "ADV", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.5": {"text": "und i bi die letzti Nacht e wengeli z'jung gsi", "tokens": ["und", "i", "bi", "die", "letz\u00b7ti", "Nacht", "e", "wen\u00b7ge\u00b7li", "z'\u00b7jung", "gsi"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "FM", "FM", "ART", "ADJA", "NN", "NE", "PIAT", "NN", "NE"], "meter": "-+--+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "f\u00fcr mi Alter, doch mit Zucht, und eimol isch keimol.", "tokens": ["f\u00fcr", "mi", "Al\u00b7ter", ",", "doch", "mit", "Zucht", ",", "und", "ei\u00b7mol", "isch", "kei\u00b7mol", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "ADV", "APPR", "NN", "$,", "KON", "ADV", "ADJD", "PIAT", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "Will me Geister erblicken und heimligi Sachen erfahre,", "tokens": ["Will", "me", "Geis\u00b7ter", "er\u00b7bli\u00b7cken", "und", "heim\u00b7li\u00b7gi", "Sa\u00b7chen", "er\u00b7fah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "VVINF", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "mue\u00df me, wenn's Zw\u00f6lfi schlacht, nit in de Federe liege.", "tokens": ["mu\u00b7e\u00df", "me", ",", "wenn's", "Zw\u00f6l\u00b7fi", "schlacht", ",", "nit", "in", "de", "Fe\u00b7de\u00b7re", "lie\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "$,", "KOUS", "NN", "VVFIN", "$,", "PTKNEG", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+--+-++--+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Nu mer hen is versp\u00f6tet mit allerhand fr\u00fcndlige Gspr\u00f6che", "tokens": ["Nu", "mer", "hen", "is", "ver\u00b7sp\u00f6\u00b7tet", "mit", "al\u00b7ler\u00b7hand", "fr\u00fcnd\u00b7li\u00b7ge", "Gs\u00b7pr\u00f6\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "FM", "FM", "FM", "VVFIN", "APPR", "PIAT", "ADJA", "NN"], "meter": "+--+-+--+-++-+-+-", "measure": "iambic.octa.plus.invert"}, "line.10": {"text": "z'Heiterschen an der Stro\u00df, und Uhr und Zeiger isch gstande;", "tokens": ["z'\u00b7Heiter\u00b7schen", "an", "der", "Stro\u00df", ",", "und", "Uhr", "und", "Zei\u00b7ger", "isch", "gstan\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "KON", "NN", "KON", "NN", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "d'Uhr het im alte Johr no welle ne wengeli Frist lo,", "tokens": ["d'\u00b7Uhr", "het", "im", "al\u00b7te", "Johr", "no", "wel\u00b7le", "ne", "wen\u00b7ge\u00b7li", "Frist", "lo", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+--+-+-+--+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "oder hani's verh\u00f6rt. \u2013 \u00bbGuet Nacht, ihr Nochbere\u00ab, sagi,", "tokens": ["o\u00b7der", "ha\u00b7ni's", "ver\u00b7h\u00f6rt", ".", "\u2013", "\u00bb", "Guet", "Nacht", ",", "ihr", "Noch\u00b7be\u00b7re", "\u00ab", ",", "sa\u00b7gi", ","], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "NE", "VVPP", "$.", "$(", "$(", "NE", "NN", "$,", "PPOSAT", "NN", "$(", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "\u00bbmi Weg wird am witschte si go Chrotzige\u00ab, sagi,", "tokens": ["\u00bb", "mi", "Weg", "wird", "am", "witschte", "si", "go", "Chrot\u00b7zi\u00b7ge", "\u00ab", ",", "sa\u00b7gi", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["$(", "NE", "NN", "VAFIN", "APPRART", "ADJA", "FM", "FM", "FM", "$(", "$,", "VVFIN", "$,"], "meter": "-+--+--++-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "\u00bbgebis Gott e gl\u00fccklich Johr und freudigi Sinne!\u00ab", "tokens": ["\u00bb", "ge\u00b7bis", "Gott", "e", "gl\u00fcck\u00b7lich", "Johr", "und", "freu\u00b7di\u00b7gi", "Sin\u00b7ne", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "NE", "ADJD", "NE", "KON", "ADJA", "NN", "$.", "$("], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "\u00bbdas geb Gott der Her\u00ab, so sage die andre, \u00bbund schick di,", "tokens": ["\u00bb", "das", "geb", "Gott", "der", "Her", "\u00ab", ",", "so", "sa\u00b7ge", "die", "and\u00b7re", ",", "\u00bb", "und", "schick", "di", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "NN", "ART", "NN", "$(", "$,", "ADV", "VVFIN", "ART", "ADJA", "$,", "$(", "KON", "ADJD", "NE", "$,"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "sust trappiert di der Geist no n\u00e4umen, eb de deheim bisch,", "tokens": ["sust", "trap\u00b7piert", "di", "der", "Geist", "no", "n\u00e4u\u00b7men", ",", "eb", "de", "de\u00b7heim", "bisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ART", "NN", "NE", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "wo mit sim Chind im Arm am lezte Dezember an d'Stro\u00df stoht.", "tokens": ["wo", "mit", "sim", "Chind", "im", "Arm", "am", "lez\u00b7te", "De\u00b7zem\u00b7ber", "an", "d'\u00b7St\u00b7ro\u00df", "stoht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "APPRART", "NN", "APPRART", "NN", "APPRART", "ADJA", "NN", "APPR", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.18": {"text": "d'Postknecht wisse's alli, und rite lieber im Feldweg.\u00ab", "tokens": ["d'\u00b7Post\u00b7knecht", "wis\u00b7se's", "al\u00b7li", ",", "und", "ri\u00b7te", "lie\u00b7ber", "im", "Feld\u00b7weg", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "KON", "VVFIN", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "'s isch so cho, und zmitts im Dorf, und woni ums Eck gang,", "tokens": ["'s", "isch", "so", "cho", ",", "und", "zmitts", "im", "Dorf", ",", "und", "wo\u00b7ni", "ums", "Eck", "gang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "NE", "$,", "KON", "VVFIN", "APPRART", "NN", "$,", "KON", "PWAV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "nebe 's Xaveris Huus, bim Bluest! do stoht er am Brunne,", "tokens": ["ne\u00b7be", "'s", "Xa\u00b7ve\u00b7ris", "Huus", ",", "bim", "Blu\u00b7est", "!", "do", "stoht", "er", "am", "Brun\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "NE", "$,", "APPRART", "NN", "$.", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "---+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "gro\u00df bis fast ans Dach und inneme duftige Mantel,", "tokens": ["gro\u00df", "bis", "fast", "ans", "Dach", "und", "in\u00b7ne\u00b7me", "duf\u00b7ti\u00b7ge", "Man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADV", "APPRART", "NN", "KON", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.22": {"text": "gwoben us Wulken und Liecht, und mitteme Bendel im Chnopfloch,", "tokens": ["gwo\u00b7ben", "us", "Wul\u00b7ken", "und", "Liecht", ",", "und", "mit\u00b7te\u00b7me", "Ben\u00b7del", "im", "Chno\u00b7pfloch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "KON", "NN", "$,", "KON", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.23": {"text": "und het in den Armen und halber im Mantel verborge", "tokens": ["und", "het", "in", "den", "Ar\u00b7men", "und", "hal\u00b7ber", "im", "Man\u00b7tel", "ver\u00b7bor\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "KON", "APPO", "APPRART", "NN", "VVFIN"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.24": {"text": "wundersch\u00f6n e B\u00fcebli gha mit fr\u00fcndligen Auge,", "tokens": ["wun\u00b7der\u00b7sch\u00f6n", "e", "B\u00fceb\u00b7li", "gha", "mit", "fr\u00fcnd\u00b7li\u00b7gen", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NE", "NE", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.25": {"text": "ch\u00fc\u00dft's und l\u00e4chlet's a us sinen ernstlige Miene,", "tokens": ["ch\u00fc\u00dft's", "und", "l\u00e4chlet's", "a", "us", "si\u00b7nen", "ernst\u00b7li\u00b7ge", "Mie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "FM", "FM", "FM", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "wie us n\u00e4chtligem Gw\u00fclch der Vollmond lieblig in d'Welt luegt.", "tokens": ["wie", "us", "n\u00e4cht\u00b7li\u00b7gem", "Gw\u00fclch", "der", "Voll\u00b7mond", "lieb\u00b7lig", "in", "d'\u00b7Welt", "luegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADJA", "NN", "ART", "NN", "ADJD", "APPR", "NE", "VVFIN", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.27": {"text": "\u00bbsiehsch mi nit, so tuesch mer n\u00fct\u00ab, \u2013 so denki und weih mi", "tokens": ["\u00bb", "sieh\u00b7sch", "mi", "nit", ",", "so", "tu\u00b7esch", "mer", "n\u00fct", "\u00ab", ",", "\u2013", "so", "den\u00b7ki", "und", "weih", "mi"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "NE", "PTKNEG", "$,", "ADV", "ADJD", "ADV", "ADJD", "$(", "$,", "$(", "ADV", "ADV", "KON", "VVIMP", "NE"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "mit em heilige Chr\u00fctz, und stell mi hinter de Brunnstock,", "tokens": ["mit", "em", "hei\u00b7li\u00b7ge", "Chr\u00fctz", ",", "und", "stell", "mi", "hin\u00b7ter", "de", "Brunn\u00b7stock", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,", "KON", "ADJD", "NE", "APPR", "NE", "NE", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.29": {"text": "und will lose, was er seit, und wienerem zuspricht.", "tokens": ["und", "will", "lo\u00b7se", ",", "was", "er", "seit", ",", "und", "wie\u00b7ne\u00b7rem", "zu\u00b7spricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NE", "$,", "PWS", "PPER", "ADV", "$,", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.30": {"text": "Wenig hani z'erst verstande; 's Wasser het bruuschet", "tokens": ["We\u00b7nig", "ha\u00b7ni", "z'\u00b7erst", "ver\u00b7stan\u00b7de", ";", "'s", "Was\u00b7ser", "het", "bruu\u00b7schet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "VVFIN", "$.", "PPER", "NN", "VAFIN", "VVFIN"], "meter": "+---+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "us de R\u00f6hren in Trog und us em Brunntrog ins Gr\u00e4bli.", "tokens": ["us", "de", "R\u00f6h\u00b7ren", "in", "Trog", "und", "us", "em", "Brunn\u00b7trog", "ins", "Gr\u00e4b\u00b7li", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.32": {"text": "\u00bbchilchhof\u00ab \u2013 hani verstande, und \u2013 \u00bbN\u00fct darf ewige Bstand ha.\u00ab", "tokens": ["\u00bb", "chilch\u00b7hof", "\u00ab", "\u2013", "ha\u00b7ni", "ver\u00b7stan\u00b7de", ",", "und", "\u2013", "\u00bb", "N\u00fct", "darf", "e\u00b7wi\u00b7ge", "Bstand", "ha", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "$(", "$(", "FM.la", "FM.la", "$,", "KON", "$(", "$(", "NN", "VMFIN", "ADJA", "NN", "NE", "$.", "$("], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "Und \u2013 \u00bbJez gohsch in d'Welt mit dine Schmerzen und Freude.", "tokens": ["Und", "\u2013", "\u00bb", "Jez", "goh\u00b7sch", "in", "d'\u00b7Welt", "mit", "di\u00b7ne", "Schmer\u00b7zen", "und", "Freu\u00b7de", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "$(", "ADV", "ADJD", "APPR", "NE", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Teil sie verst\u00e4ndig us, und was ich n\u00fcmme cha schlichte,", "tokens": ["Teil", "sie", "ver\u00b7st\u00e4n\u00b7dig", "us", ",", "und", "was", "ich", "n\u00fcm\u00b7me", "cha", "schlich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PTKVZ", "$,", "KON", "PWS", "PPER", "VVFIN", "NE", "VVFIN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.35": {"text": "bring zum gueten End. Sie hen e freudige Herbst gha.", "tokens": ["bring", "zum", "gue\u00b7ten", "End", ".", "Sie", "hen", "e", "freu\u00b7di\u00b7ge", "Herbst", "gha", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$.", "PPER", "VVFIN", "NE", "ADJA", "NN", "NE", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.36": {"text": "Trinkt ein z'viel, und sizt er lang im n\u00e4chtlige Wirtshuus,", "tokens": ["Trinkt", "ein", "z'\u00b7viel", ",", "und", "sizt", "er", "lang", "im", "n\u00e4cht\u00b7li\u00b7ge", "Wirts\u00b7huus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.37": {"text": "gang, und bietem heim und f\u00fchr en, a\u00df er kei Bei bricht!", "tokens": ["gang", ",", "und", "bie\u00b7tem", "heim", "und", "f\u00fchr", "en", ",", "a\u00df", "er", "kei", "Bei", "bricht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "NE", "PTKVZ", "KON", "VVFIN", "FM", "$,", "VVFIN", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.38": {"text": "Nimm di der Armet a, und sorg mer f\u00fcr Witwen und Waise,", "tokens": ["Nimm", "di", "der", "Ar\u00b7met", "a", ",", "und", "sorg", "mer", "f\u00fcr", "Wit\u00b7wen", "und", "Wai\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "NE", "$,", "KON", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.39": {"text": "mach mer die Chranke gsund. \u2013 Die brave Saldate han ich no", "tokens": ["mach", "mer", "die", "Chran\u00b7ke", "gsund", ".", "\u2013", "Die", "bra\u00b7ve", "Sal\u00b7da\u00b7te", "han", "ich", "no"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVFIN", "$.", "$(", "ART", "ADJA", "NN", "VAFIN", "PPER", "NE"], "meter": "+--+-+-+-+--+-+", "measure": "iambic.septa.invert"}, "line.40": {"text": "mit Trumpeten