{"textgrid.poem.54173": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Schnipsel", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6flich wie ein Engl\u00e4nder zu Hause. Unh\u00f6flich wie ein Engl\u00e4nder auf Reisen.", "tokens": ["H\u00f6f\u00b7lich", "wie", "ein", "En\u00b7gl\u00e4n\u00b7der", "zu", "Hau\u00b7se", ".", "Un\u00b7h\u00f6f\u00b7lich", "wie", "ein", "En\u00b7gl\u00e4n\u00b7der", "auf", "Rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPR", "NN", "$.", "ADJD", "KOKOM", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-++--+-++-+-++--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Man sollte gar nicht glauben, wie gut man auch ohne die Erfindungen des Jahres 2500 auskommen kann!", "tokens": ["Man", "soll\u00b7te", "gar", "nicht", "glau\u00b7ben", ",", "wie", "gut", "man", "auch", "oh\u00b7ne", "die", "Er\u00b7fin\u00b7dun\u00b7gen", "des", "Jah\u00b7res", "2500", "aus\u00b7kom\u00b7men", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "number", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,", "PWAV", "ADJD", "PIS", "ADV", "APPR", "ART", "NN", "ART", "NN", "CARD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+--+--+-+--+--+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Kriminalroman im Bett ist schwer. Ein Bett ist doch keine Eisenbahn!", "tokens": ["Kri\u00b7mi\u00b7nal\u00b7ro\u00b7man", "im", "Bett", "ist", "schwer", ".", "Ein", "Bett", "ist", "doch", "kei\u00b7ne", "Ei\u00b7sen\u00b7bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VAFIN", "ADJD", "$.", "ART", "NN", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-----+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Rein hippologisch betrachtet ist er vom Pferd gefallen.", "tokens": ["Rein", "hip\u00b7po\u00b7lo\u00b7gisch", "be\u00b7trach\u00b7tet", "ist", "er", "vom", "Pferd", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Eine allen Deutschen gemeinsame Literatur gibt es nicht. Bei uns liest jeder nur seins.", "tokens": ["Ei\u00b7ne", "al\u00b7len", "Deut\u00b7schen", "ge\u00b7mein\u00b7sa\u00b7me", "Li\u00b7te\u00b7ra\u00b7tur", "gibt", "es", "nicht", ".", "Bei", "uns", "liest", "je\u00b7der", "nur", "seins", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "APPR", "PPER", "VVFIN", "PIS", "ADV", "CARD", "$."], "meter": "+-+-+--+--+-+-+--+--+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Die sch\u00f6nste Geschichte \u00fcber Masochismus, die ich wei\u00df:", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Ge\u00b7schich\u00b7te", "\u00fc\u00b7ber", "Ma\u00b7so\u00b7chis\u00b7mus", ",", "die", "ich", "wei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}}}}}