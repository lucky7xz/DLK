{"textgrid.poem.66520": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sterne schon winken und steigen", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sterne schon winken und steigen", "tokens": ["Ster\u00b7ne", "schon", "win\u00b7ken", "und", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVINF", "KON", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Nieder zur spiegelnden Fluth.", "tokens": ["Nie\u00b7der", "zur", "spie\u00b7geln\u00b7den", "Fluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Komm, in des Abends Schweigen", "tokens": ["Komm", ",", "in", "des", "A\u00b7bends", "Schwei\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wandelt's am Strome sich gut!", "tokens": ["Wan\u00b7delt's", "am", "Stro\u00b7me", "sich", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PRF", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "K\u00fchlung weht von den Wellen,", "tokens": ["K\u00fch\u00b7lung", "weht", "von", "den", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "D\u00fcfte, den Bl\u00fcthen entfacht,", "tokens": ["D\u00fcf\u00b7te", ",", "den", "Bl\u00fc\u00b7then", "ent\u00b7facht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Hoffen und W\u00fcnsche gesellen", "tokens": ["Hof\u00b7fen", "und", "W\u00fcn\u00b7sche", "ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVPP"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Gern sich der d\u00e4mmernden Nacht.", "tokens": ["Gern", "sich", "der", "d\u00e4m\u00b7mern\u00b7den", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ART", "ADJA", "NN", "$."], "meter": "---+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "\u00bbgru\u00df euch, Jungfraun, ihr feinen!", "tokens": ["\u00bb", "gru\u00df", "euch", ",", "Jung\u00b7fraun", ",", "ihr", "fei\u00b7nen", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "$,", "NN", "$,", "PPOSAT", "ADJA", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Nehmet zu Diensten uns an!", "tokens": ["Neh\u00b7met", "zu", "Diens\u00b7ten", "uns", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Lasset den Weg uns vereinen,", "tokens": ["Las\u00b7set", "den", "Weg", "uns", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Der uns so lieblich begann!", "tokens": ["Der", "uns", "so", "lieb\u00b7lich", "be\u00b7gann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Kr\u00e4ftig die Ruder zu r\u00fchren", "tokens": ["Kr\u00e4f\u00b7tig", "die", "Ru\u00b7der", "zu", "r\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "PTKZU", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Harrten wir dort in dem Kahn,", "tokens": ["Harr\u00b7ten", "wir", "dort", "in", "dem", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "La\u00dft auf die Wellen euch f\u00fchren,", "tokens": ["La\u00dft", "auf", "die", "Wel\u00b7len", "euch", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ueber die funkelnde Bahn!\u00ab \u2013", "tokens": ["Ue\u00b7ber", "die", "fun\u00b7keln\u00b7de", "Bahn", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Weit schon klingt auf den Wogen", "tokens": ["Weit", "schon", "klingt", "auf", "den", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Ruder und fr\u00f6hlicher Sang,", "tokens": ["Ru\u00b7der", "und", "fr\u00f6h\u00b7li\u00b7cher", "Sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ufer und Himmelsbogen", "tokens": ["U\u00b7fer", "und", "Him\u00b7mels\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Lauschen verhallendem Klang.", "tokens": ["Lau\u00b7schen", "ver\u00b7hal\u00b7len\u00b7dem", "Klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Und von den T\u00f6nen getragen,", "tokens": ["Und", "von", "den", "T\u00f6\u00b7nen", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Und von den Liedern verh\u00fcllt,", "tokens": ["Und", "von", "den", "Lie\u00b7dern", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Wagen die Herzen zu sagen", "tokens": ["Wa\u00b7gen", "die", "Her\u00b7zen", "zu", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Was sie beseligt erf\u00fcllt.", "tokens": ["Was", "sie", "be\u00b7se\u00b7ligt", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "So auf den wiegenden Gleisen", "tokens": ["So", "auf", "den", "wie\u00b7gen\u00b7den", "Glei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ueber die rauschende Fluth", "tokens": ["Ue\u00b7ber", "die", "rau\u00b7schen\u00b7de", "Fluth"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Weit durch die Lande zu reisen", "tokens": ["Weit", "durch", "die", "Lan\u00b7de", "zu", "rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "W\u00e4ren wir wohlgemuth!", "tokens": ["W\u00e4\u00b7ren", "wir", "wohl\u00b7ge\u00b7muth", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Liederklang auf den Wellen,", "tokens": ["Lie\u00b7der\u00b7klang", "auf", "den", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Droben der Sterne Pracht!", "tokens": ["Dro\u00b7ben", "der", "Ster\u00b7ne", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Hoffen und W\u00fcnsche gesellen", "tokens": ["Hof\u00b7fen", "und", "W\u00fcn\u00b7sche", "ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVPP"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Gern sich der d\u00e4mmernden Nacht.", "tokens": ["Gern", "sich", "der", "d\u00e4m\u00b7mern\u00b7den", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ART", "ADJA", "NN", "$."], "meter": "---+--+", "measure": "iambic.di.chol"}}}}}