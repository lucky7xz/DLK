{"textgrid.poem.24293": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ick hee\u00dfe Ede Petermann!", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ick hee\u00dfe Ede Petermann!", "tokens": ["Ick", "hee\u00b7\u00dfe", "E\u00b7de", "Pe\u00b7ter\u00b7mann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Rixdorf kennt mir jedermann", "tokens": ["In", "Rix\u00b7dorf", "kennt", "mir", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "PPER", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Kulickes Destille.", "tokens": ["In", "Ku\u00b7li\u00b7ckes", "Des\u00b7til\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier rees ick man inkognito,", "tokens": ["Hier", "rees", "ick", "man", "in\u00b7kog\u00b7ni\u00b7to", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als Meyer, Lehmann oder so,", "tokens": ["Als", "Me\u00b7yer", ",", "Leh\u00b7mann", "o\u00b7der", "so", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "NE", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Denn Namens jibt et ville.", "tokens": ["Denn", "Na\u00b7mens", "jibt", "et", "vil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "NE", "NE", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Warum? Det jeht Sie jarnischt an!", "tokens": ["Wa\u00b7rum", "?", "Det", "jeht", "Sie", "jar\u00b7nischt", "an", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "NE", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Halts Maul, sagt Ede Petermann,", "tokens": ["Halts", "Maul", ",", "sagt", "E\u00b7de", "Pe\u00b7ter\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nu sin Se, bitte, stille!", "tokens": ["Nu", "sin", "Se", ",", "bit\u00b7te", ",", "stil\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "NE", "NE", "$,", "PTKANT", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sonst werde ick am Ende roh,", "tokens": ["Sonst", "wer\u00b7de", "ick", "am", "En\u00b7de", "roh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie dazumal im Falle wo,", "tokens": ["Wie", "da\u00b7zu\u00b7mal", "im", "Fal\u00b7le", "wo", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "PWAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Kulickes Destille.", "tokens": ["In", "Ku\u00b7li\u00b7ckes", "Des\u00b7til\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der wunderscheene Nutten-Franz,", "tokens": ["Der", "wun\u00b7der\u00b7schee\u00b7ne", "Nut\u00b7ten\u00b7Franz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der is seitdem nich mehr janz janz,", "tokens": ["Der", "is", "seit\u00b7dem", "nich", "mehr", "janz", "janz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "FM", "FM", "PTKNEG", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Det kommt vont ville Reden.", "tokens": ["Det", "kommt", "vont", "vil\u00b7le", "Re\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Blo\u00df schade, det ick fortjemu\u00dft!", "tokens": ["Blo\u00df", "scha\u00b7de", ",", "det", "ick", "fort\u00b7je\u00b7mu\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Rixdorf, meine Mutterbrust,", "tokens": ["O", "Rix\u00b7dorf", ",", "mei\u00b7ne", "Mut\u00b7ter\u00b7brust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wat machste ohne Eden?", "tokens": ["Wat", "machs\u00b7te", "oh\u00b7ne", "E\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Im zweeten Jarderejiment", "tokens": ["Im", "zwee\u00b7ten", "Jar\u00b7de\u00b7re\u00b7ji\u00b7ment"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hab ickn Unteroffzier jekennt.", "tokens": ["Hab", "ickn", "Un\u00b7te\u00b7roff\u00b7zier", "je\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ick sage blo\u00df: Noblesse!", "tokens": ["Ick", "sa\u00b7ge", "blo\u00df", ":", "Nob\u00b7les\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "NN", "$."], "meter": "+---+--", "measure": "dactylic.init"}, "line.4": {"text": "Denn ieberhaupt: Ick bin n Aas,", "tokens": ["Denn", "ie\u00b7ber\u00b7haupt", ":", "Ick", "bin", "n", "Aas", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PPER", "VAFIN", "XY", "XY", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wer mir Jauche jie\u00dft int Ilas,", "tokens": ["Und", "wer", "mir", "Jau\u00b7che", "jie\u00dft", "int", "I\u00b7las", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "NN", "VVFIN", "NE", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Den hau ick in die Fresse.", "tokens": ["Den", "hau", "ick", "in", "die", "Fres\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Det kommt von meinem Handgelenk,", "tokens": ["Det", "kommt", "von", "mei\u00b7nem", "Hand\u00b7ge\u00b7lenk", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ick hab et so in der Lamenk.", "tokens": ["Ick", "hab", "et", "so", "in", "der", "La\u00b7menk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst bin ick sanft wie Sahne.", "tokens": ["Sonst", "bin", "ick", "sanft", "wie", "Sah\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KOKOM", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jott! \u2013 Rieke! Schonsten wird mir schwach.", "tokens": ["Jott", "!", "\u2013", "Rie\u00b7ke", "!", "Schons\u00b7ten", "wird", "mir", "schwach", "."], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "NE", "$.", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Blo\u00df: Keene Zicken! Sonst jibts Krach!", "tokens": ["Blo\u00df", ":", "Kee\u00b7ne", "Zi\u00b7cken", "!", "Sonst", "jibts", "Krach", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADJA", "NN", "$.", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Denn hau ck ins Porzellane!", "tokens": ["Denn", "hau", "ck", "ins", "Por\u00b7zel\u00b7la\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}