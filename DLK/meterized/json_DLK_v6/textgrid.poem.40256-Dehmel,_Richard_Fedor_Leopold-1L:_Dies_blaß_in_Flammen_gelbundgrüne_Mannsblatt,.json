{"textgrid.poem.40256": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dies bla\u00df in Flammen gelbundgr\u00fcne Mannsblatt,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies bla\u00df in Flammen gelbundgr\u00fcne Mannsblatt,", "tokens": ["Dies", "bla\u00df", "in", "Flam\u00b7men", "gel\u00b7bund\u00b7gr\u00fc\u00b7ne", "Manns\u00b7blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "knaben\u00fcppig, und dies zarte", "tokens": ["kna\u00b7be\u00b7n\u00fcp\u00b7pig", ",", "und", "dies", "zar\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "KON", "PDS", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schneegl\u00f6ckchen, eben aufgebl\u00fcht,", "tokens": ["Schnee\u00b7gl\u00f6ck\u00b7chen", ",", "e\u00b7ben", "auf\u00b7ge\u00b7bl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "ganz furchtsam wei\u00df,", "tokens": ["ganz", "furcht\u00b7sam", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "im irdnen Topf \u2013", "tokens": ["im", "ird\u00b7nen", "Topf", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "die beiden Kinder wuchsen so allein", "tokens": ["die", "bei\u00b7den", "Kin\u00b7der", "wuch\u00b7sen", "so", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und hatten niemals einen Duft genossen,", "tokens": ["und", "hat\u00b7ten", "nie\u00b7mals", "ei\u00b7nen", "Duft", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "da pflanzt'ich sie zusammen", "tokens": ["da", "pflanzt'\u00b7ich", "sie", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "und brachte sie zu Dir ...", "tokens": ["und", "brach\u00b7te", "sie", "zu", "Dir", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}