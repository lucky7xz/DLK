{"textgrid.poem.40342": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Venus Sapiens", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun, du Eine, tritt heran,", "tokens": ["Nun", ",", "du", "Ei\u00b7ne", ",", "tritt", "he\u00b7ran", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "ART", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "h\u00f6re meine wahrsten Laute;", "tokens": ["h\u00f6\u00b7re", "mei\u00b7ne", "wahrs\u00b7ten", "Lau\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "h\u00f6re zu wie Jonathan,", "tokens": ["h\u00f6\u00b7re", "zu", "wie", "Jo\u00b7na\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KOKOM", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "als sich David ihm vertraute.", "tokens": ["als", "sich", "Da\u00b7vid", "ihm", "ver\u00b7trau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NE", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwer vom Hohn und \u00dcbermute", "tokens": ["Schwer", "vom", "Hohn", "und", "\u00dc\u00b7ber\u00b7mu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Goliaths herabgem\u00e4chtigt,", "tokens": ["Go\u00b7li\u00b7aths", "her\u00b7ab\u00b7ge\u00b7m\u00e4ch\u00b7tigt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "hat bis heut in meinem Blute", "tokens": ["hat", "bis", "heut", "in", "mei\u00b7nem", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "noch der greise Saul gen\u00e4chtigt.", "tokens": ["noch", "der", "grei\u00b7se", "Saul", "ge\u00b7n\u00e4ch\u00b7tigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwielicht. Sterbend h\u00e4ngt die scharfe", "tokens": ["Zwie\u00b7licht", ".", "Ster\u00b7bend", "h\u00e4ngt", "die", "schar\u00b7fe"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zunge aus dem L\u00e4stermaul.", "tokens": ["Zun\u00b7ge", "aus", "dem", "L\u00e4s\u00b7ter\u00b7maul", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh, nun weint dein K\u00f6nig Saul,", "tokens": ["Sieh", ",", "nun", "weint", "dein", "K\u00f6\u00b7nig", "Saul", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "denn dein David singt zur Harfe.", "tokens": ["denn", "dein", "Da\u00b7vid", "singt", "zur", "Har\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Kleider sind zerrissen,", "tokens": ["Al\u00b7le", "Klei\u00b7der", "sind", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "die den alten K\u00f6nig schm\u00fcckten;", "tokens": ["die", "den", "al\u00b7ten", "K\u00f6\u00b7nig", "schm\u00fcck\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "br\u00fctend h\u00f6rt er den Entz\u00fcckten", "tokens": ["br\u00fc\u00b7tend", "h\u00f6rt", "er", "den", "Ent\u00b7z\u00fcck\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "nahen aus den Finsternissen.", "tokens": ["na\u00b7hen", "aus", "den", "Fins\u00b7ter\u00b7nis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Goliath tot! den K\u00f6nig schauert;", "tokens": ["Go\u00b7li\u00b7ath", "tot", "!", "den", "K\u00f6\u00b7nig", "schau\u00b7ert", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "seine Schwermut ahnt das Ende.", "tokens": ["sei\u00b7ne", "Schwer\u00b7mut", "ahnt", "das", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dein S\u00e4nger steht und trauert:", "tokens": ["Und", "dein", "S\u00e4n\u00b7ger", "steht", "und", "trau\u00b7ert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "blutbefleckt sind seine H\u00e4nde.", "tokens": ["blut\u00b7be\u00b7fleckt", "sind", "sei\u00b7ne", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber weiter mu\u00df er schreiten,", "tokens": ["A\u00b7ber", "wei\u00b7ter", "mu\u00df", "er", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "seine T\u00f6ne sind ein Bann,", "tokens": ["sei\u00b7ne", "T\u00f6\u00b7ne", "sind", "ein", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "selig greift er in die Saiten:", "tokens": ["se\u00b7lig", "greift", "er", "in", "die", "Sai\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Komm, o komm, mein Jonathan!", "tokens": ["Komm", ",", "o", "komm", ",", "mein", "Jo\u00b7na\u00b7than", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Traure nicht um den gebeugten", "tokens": ["Trau\u00b7re", "nicht", "um", "den", "ge\u00b7beug\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vater, dem vor morgen graut;", "tokens": ["Va\u00b7ter", ",", "dem", "vor", "mor\u00b7gen", "graut", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ADV", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "denn die Tr\u00fcbsal ist die Braut", "tokens": ["denn", "die", "Tr\u00fcb\u00b7sal", "ist", "die", "Braut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "aller nicht vom Geist Gezeugten.", "tokens": ["al\u00b7ler", "nicht", "vom", "Geist", "Ge\u00b7zeug\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PTKNEG", "APPRART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jonathan, du sahst ihn sitzen,", "tokens": ["Jo\u00b7na\u00b7than", ",", "du", "sahst", "ihn", "sit\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "den Berater deiner Reife,", "tokens": ["den", "Be\u00b7ra\u00b7ter", "dei\u00b7ner", "Rei\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "nackt und schamlos, und das steife", "tokens": ["nackt", "und", "scham\u00b7los", ",", "und", "das", "stei\u00b7fe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$,", "KON", "PDS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Haupt umstarrt von Lanzenspitzen.", "tokens": ["Haupt", "um\u00b7starrt", "von", "Lan\u00b7zens\u00b7pit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und du sahst vor seinem Zelt", "tokens": ["Und", "du", "sahst", "vor", "sei\u00b7nem", "Zelt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "sterben den Philisterf\u00fcrsten;", "tokens": ["ster\u00b7ben", "den", "Phi\u00b7lis\u00b7ter\u00b7f\u00fcrs\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "aber Leben braucht die Welt,", "tokens": ["a\u00b7ber", "Le\u00b7ben", "braucht", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "la\u00df uns nach dem Geiste d\u00fcrsten!", "tokens": ["la\u00df", "uns", "nach", "dem", "Geis\u00b7te", "d\u00fcrs\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn es weht von allen H\u00fcgeln", "tokens": ["Denn", "es", "weht", "von", "al\u00b7len", "H\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "immer neu sein ewiger Segen;", "tokens": ["im\u00b7mer", "neu", "sein", "e\u00b7wi\u00b7ger", "Se\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "lerne nur dein Herz befl\u00fcgeln,", "tokens": ["ler\u00b7ne", "nur", "dein", "Herz", "be\u00b7fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "und er wird auch Dich bewegen!", "tokens": ["und", "er", "wird", "auch", "Dich", "be\u00b7we\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Jonathan, zu jeder Frist", "tokens": ["Jo\u00b7na\u00b7than", ",", "zu", "je\u00b7der", "Frist"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "sei nun meiner Liebe sicher;", "tokens": ["sei", "nun", "mei\u00b7ner", "Lie\u00b7be", "si\u00b7cher", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und sie ist viel sonderlicher,", "tokens": ["und", "sie", "ist", "viel", "son\u00b7der\u00b7li\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "als mir Frauenliebe ist.", "tokens": ["als", "mir", "Frau\u00b7en\u00b7lie\u00b7be", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Glutwind droht den jungen Saaten;", "tokens": ["Glut\u00b7wind", "droht", "den", "jun\u00b7gen", "Saa\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "nimm den Bogen in die H\u00e4nde,", "tokens": ["nimm", "den", "Bo\u00b7gen", "in", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "da\u00df dein Pfeil mir Warnung sende,", "tokens": ["da\u00df", "dein", "Pfeil", "mir", "War\u00b7nung", "sen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "sinnt der Vater Wahnsinnstaten.", "tokens": ["sinnt", "der", "Va\u00b7ter", "Wahn\u00b7sinns\u00b7ta\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Jonathan, hier steh ich nackt;", "tokens": ["Jo\u00b7na\u00b7than", ",", "hier", "steh", "ich", "nackt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "du mein Bruder, Freund, Berater,", "tokens": ["du", "mein", "Bru\u00b7der", ",", "Freund", ",", "Be\u00b7ra\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "hilf mir, wenn die Glut mich packt!", "tokens": ["hilf", "mir", ",", "wenn", "die", "Glut", "mich", "packt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jona! Weib! noch giert der Vater!", "tokens": ["Jo\u00b7na", "!", "Weib", "!", "noch", "giert", "der", "Va\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jona, Schwester! unsre Kinder \u2013", "tokens": ["Jo\u00b7na", ",", "Schwes\u00b7ter", "!", "uns\u00b7re", "Kin\u00b7der", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gattin! weinen meine Saiten \u2013 \u2013", "tokens": ["Gat\u00b7tin", "!", "wei\u00b7nen", "mei\u00b7ne", "Sai\u00b7ten", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "VVINF", "PPOSAT", "NN", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbdavid, komm! du \u00dcberwinder", "tokens": ["\u00bb", "da\u00b7vid", ",", "komm", "!", "du", "\u00dc\u00b7berw\u00b7in\u00b7der"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["$(", "PAV", "$,", "VVFIN", "$.", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "unsrer Unwillk\u00fcrlichkeiten\u00ab ...", "tokens": ["uns\u00b7rer", "Un\u00b7will\u00b7k\u00fcr\u00b7lich\u00b7kei\u00b7ten", "\u00ab", "..."], "token_info": ["word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punc