{"textgrid.poem.25724": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Als Wilhelmine verm\u00e4hlt wurde", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Prophezein wird dir der Schmeichler Haufen", "tokens": ["Pro\u00b7phe\u00b7zein", "wird", "dir", "der", "Schmeich\u00b7ler", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit dem Trauungsringe, Freud' und Gl\u00fcck!", "tokens": ["Mit", "dem", "Trau\u00b7ungs\u00b7rin\u00b7ge", ",", "Freud'", "und", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aber, einem Ringe gleich, entlaufen", "tokens": ["A\u00b7ber", ",", "ei\u00b7nem", "Rin\u00b7ge", "gleich", ",", "ent\u00b7lau\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "ART", "NN", "ADV", "$,", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gl\u00fcck und Freud' in einem Augenblick',", "tokens": ["Gl\u00fcck", "und", "Freud'", "in", "ei\u00b7nem", "Au\u00b7gen\u00b7blick'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Hast du, beide liebzukosen,", "tokens": ["Hast", "du", ",", "bei\u00b7de", "lieb\u00b7zu\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mit einer Schnur von Rosen", "tokens": ["Und", "mit", "ei\u00b7ner", "Schnur", "von", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Fest zu halten, kein Geschick.", "tokens": ["Fest", "zu", "hal\u00b7ten", ",", "kein", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00e4tte deine Wang' auch nicht das Gr\u00fcbchen,", "tokens": ["H\u00e4t\u00b7te", "dei\u00b7ne", "Wang'", "auch", "nicht", "das", "Gr\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Das so gern wir alle l\u00e4cheln sehn,", "tokens": ["Das", "so", "gern", "wir", "al\u00b7le", "l\u00e4\u00b7cheln", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "PPER", "PIS", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Dennoch w\u00fcrdest du als Damis Liebchen", "tokens": ["Den\u00b7noch", "w\u00fcr\u00b7dest", "du", "als", "Da\u00b7mis", "Lieb\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "KOUS", "NE", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Vor dem Traualtare heute stehn;", "tokens": ["Vor", "dem", "Trau\u00b7al\u00b7ta\u00b7re", "heu\u00b7te", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Denn der J\u00fcnglinge Verlangen", "tokens": ["Denn", "der", "J\u00fcng\u00b7lin\u00b7ge", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "In dem Netz' der Liebe fangen,", "tokens": ["In", "dem", "Netz'", "der", "Lie\u00b7be", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Das ist wahrlich leicht geschehn!", "tokens": ["Das", "ist", "wahr\u00b7lich", "leicht", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Lieblich, gleich dem klugen Vogelsteller,", "tokens": ["Lieb\u00b7lich", ",", "gleich", "dem", "klu\u00b7gen", "Vo\u00b7gel\u00b7stel\u00b7ler", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Mu\u00dft du nur dein Netz auch \u00fcberstreun;", "tokens": ["Mu\u00dft", "du", "nur", "dein", "Netz", "auch", "\u00fc\u00b7bers\u00b7treun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Denn aus einem Lamm' verwandelt schneller", "tokens": ["Denn", "aus", "ei\u00b7nem", "Lamm'", "ver\u00b7wan\u00b7delt", "schnel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Keine Wolke sich in einen Leun,", "tokens": ["Kei\u00b7ne", "Wol\u00b7ke", "sich", "in", "ei\u00b7nen", "Leun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Als die Gatten in Tyrannen,", "tokens": ["Als", "die", "Gat\u00b7ten", "in", "Ty\u00b7ran\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Wenn zu straff sie Netz' umspannen,", "tokens": ["Wenn", "zu", "straff", "sie", "Netz'", "um\u00b7span\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKZU", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Die nicht sichtbar sollten seyn.", "tokens": ["Die", "nicht", "sicht\u00b7bar", "soll\u00b7ten", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADJD", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "La\u00df du deinem Liebling' niemals f\u00fchlen,", "tokens": ["La\u00df", "du", "dei\u00b7nem", "Lieb\u00b7ling'", "nie\u00b7mals", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Da\u00df der Ring, der itzt so viel verspricht,", "tokens": ["Da\u00df", "der", "Ring", ",", "der", "itzt", "so", "viel", "ver\u00b7spricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "Und so leicht ist, um damit zu spielen,", "tokens": ["Und", "so", "leicht", "ist", ",", "um", "da\u00b7mit", "zu", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "$,", "KOUI", "PAV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Jeden Tag vermehret sein Gewicht.", "tokens": ["Je\u00b7den", "Tag", "ver\u00b7meh\u00b7ret", "sein", "Ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.26": {"text": "Damis wird die B\u00fcrde tragen,", "tokens": ["Da\u00b7mis", "wird", "die", "B\u00fcr\u00b7de", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.27": {"text": "Nur verbirg auch deine Klagen", "tokens": ["Nur", "ver\u00b7birg", "auch", "dei\u00b7ne", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Im gelassenen Gesicht'!", "tokens": ["Im", "ge\u00b7las\u00b7se\u00b7nen", "Ge\u00b7sicht'", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "O vielleicht ist kaum der Strau\u00df von Rosen,", "tokens": ["O", "viel\u00b7leicht", "ist", "kaum", "der", "Strau\u00df", "von", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "ADV", "ART", "NE", "APPR", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Der itzt deinen Busen schm\u00fcckt, verbl\u00fcht,", "tokens": ["Der", "itzt", "dei\u00b7nen", "Bu\u00b7sen", "schm\u00fcckt", ",", "ver\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.31": {"text": "Da\u00df, dich so wie sonst noch liebzukosen,", "tokens": ["Da\u00df", ",", "dich", "so", "wie", "sonst", "noch", "lieb\u00b7zu\u00b7ko\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PPER", "ADV", "KOKOM", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "Damis ganze Stunden schon verzieht,", "tokens": ["Da\u00b7mis", "gan\u00b7ze", "Stun\u00b7den", "schon", "ver\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.33": {"text": "Und mit Grillen im Gehirne,", "tokens": ["Und", "mit", "Gril\u00b7len", "im", "Ge\u00b7hir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Und mit Falten vor der Stirne,", "tokens": ["Und", "mit", "Fal\u00b7ten", "vor", "der", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Stumm auf seine N\u00e4gel sieht.", "tokens": ["Stumm", "auf", "sei\u00b7ne", "N\u00e4\u00b7gel", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Sicher, M\u00e4dchen, bist du halb verloren,", "tokens": ["Si\u00b7cher", ",", "M\u00e4d\u00b7chen", ",", "bist", "du", "halb", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.37": {"text": "Wenn du w\u00e4hnst, da\u00df ihn der Trauring dr\u00fcckt.", "tokens": ["Wenn", "du", "w\u00e4hnst", ",", "da\u00df", "ihn", "der", "Trau\u00b7ring", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.38": {"text": "Nein! der Mann ist nur noch nicht geboren,", "tokens": ["Nein", "!", "der", "Mann", "ist", "nur", "noch", "nicht", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "NN", "VAFIN", "ADV", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.39": {"text": "Den die Laune nicht schon dann bestrickt,", "tokens": ["Den", "die", "Lau\u00b7ne", "nicht", "schon", "dann", "be\u00b7strickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKNEG", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.40": {"text": "Wenn er kaum die letzte Blume", "tokens": ["Wenn", "er", "kaum", "die", "letz\u00b7te", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "In der Liebe Heiligthume,", "tokens": ["In", "der", "Lie\u00b7be", "Hei\u00b7lig\u00b7thu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "S\u00fc\u00df berauscht, hat abgepfl\u00fcckt.", "tokens": ["S\u00fc\u00df", "be\u00b7rauscht", ",", "hat", "ab\u00b7ge\u00b7pfl\u00fcckt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Darum bitt' ich, Freundin, dich, erstaune", "tokens": ["Da\u00b7rum", "bitt'", "ich", ",", "Freun\u00b7din", ",", "dich", ",", "er\u00b7stau\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PAV", "VVFIN", "PPER", "$,", "NN", "$,", "PPER", "$,", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.44": {"text": "Ueber die Verwandelung nur nicht.", "tokens": ["Ue\u00b7ber", "die", "Ver\u00b7wan\u00b7de\u00b7lung", "nur", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.45": {"text": "Zeige dann ihm zwiefach gute Laune,", "tokens": ["Zei\u00b7ge", "dann", "ihm", "zwie\u00b7fach", "gu\u00b7te", "Lau\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.46": {"text": "(das zu thun, ist deine sch\u00f6nste Pflicht!)", "tokens": ["(", "das", "zu", "thun", ",", "ist", "dei\u00b7ne", "sch\u00f6ns\u00b7te", "Pflicht", "!", ")"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "PTKZU", "VVINF", "$,", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.47": {"text": "Treib ihn, da\u00df er reit' und jage,", "tokens": ["Treib", "ihn", ",", "da\u00df", "er", "reit'", "und", "ja\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Wie vordem, weil das die Tage", "tokens": ["Wie", "vor\u00b7dem", ",", "weil", "das", "die", "Ta\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PDS", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.49": {"text": "Alter Freiheit, ihm verspricht.", "tokens": ["Al\u00b7ter", "Frei\u00b7heit", ",", "ihm", "ver\u00b7spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "La\u00df ihn nie empfinden, o du Holde,", "tokens": ["La\u00df", "ihn", "nie", "emp\u00b7fin\u00b7den", ",", "o", "du", "Hol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVFIN", "$,", "FM", "PPER", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.51": {"text": "Da\u00df sein Herz f\u00fcr M\u00e4dchen nicht mehr frei,", "tokens": ["Da\u00df", "sein", "Herz", "f\u00fcr", "M\u00e4d\u00b7chen", "nicht", "mehr", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.52": {"text": "Da\u00df er nicht allein von seinem Golde", "tokens": ["Da\u00df", "er", "nicht", "al\u00b7lein", "von", "sei\u00b7nem", "Gol\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", 