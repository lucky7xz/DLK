{"textgrid.poem.34090": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Verwunderung und Aufl\u00f6sung", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "G\u00e4be es lauter Genies, ich w\u00fcrde mich gar nicht verwundern,", "tokens": ["G\u00e4\u00b7be", "es", "lau\u00b7ter", "Ge\u00b7nies", ",", "ich", "w\u00fcr\u00b7de", "mich", "gar", "nicht", "ver\u00b7wun\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Aber ich staunte schon oft, da\u00df es so wenige giebt.", "tokens": ["A\u00b7ber", "ich", "staun\u00b7te", "schon", "oft", ",", "da\u00df", "es", "so", "we\u00b7ni\u00b7ge", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "ADV", "PIS", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Dennoch ist es nat\u00fcrlich! Wie viel ist Muskel am Menschen", "tokens": ["Den\u00b7noch", "ist", "es", "na\u00b7t\u00fcr\u00b7lich", "!", "Wie", "viel", "ist", "Mus\u00b7kel", "am", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$.", "PWAV", "PIS", "VAFIN", "NN", "APPRART", "NN"], "meter": "+-+-+---+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und wie wenig Gehirn! So auch am Menschengeschlecht.", "tokens": ["Und", "wie", "we\u00b7nig", "Ge\u00b7hirn", "!", "So", "auch", "am", "Men\u00b7schen\u00b7ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIAT", "NN", "$.", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}}}}