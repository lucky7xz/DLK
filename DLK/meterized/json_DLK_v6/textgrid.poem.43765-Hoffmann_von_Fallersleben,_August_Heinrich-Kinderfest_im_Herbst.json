{"textgrid.poem.43765": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Kinderfest im Herbst", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch ehe der Herbst uns ganz verl\u00e4\u00dft,", "tokens": ["Doch", "e\u00b7he", "der", "Herbst", "uns", "ganz", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So bringt er uns noch ein Kinderfest:", "tokens": ["So", "bringt", "er", "uns", "noch", "ein", "Kin\u00b7der\u00b7fest", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sobald es Abend, zieh'n wir aus", "tokens": ["So\u00b7bald", "es", "A\u00b7bend", ",", "zieh'n", "wir", "aus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "$,", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wandern singend von Haus zu Haus,", "tokens": ["Und", "wan\u00b7dern", "sin\u00b7gend", "von", "Haus", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und bitten dem heiligen Martin zu Ehren", "tokens": ["Und", "bit\u00b7ten", "dem", "hei\u00b7li\u00b7gen", "Mar\u00b7tin", "zu", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NE", "APPR", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Uns kleinen Kindern was zu bescheeren.", "tokens": ["Uns", "klei\u00b7nen", "Kin\u00b7dern", "was", "zu", "be\u00b7schee\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da reicht man uns Aepfel und N\u00fcsse dar,", "tokens": ["Da", "reicht", "man", "uns", "A\u00b7e\u00b7pfel", "und", "N\u00fcs\u00b7se", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zuweilen auch Honigkuchen sogar.", "tokens": ["Zu\u00b7wei\u00b7len", "auch", "Ho\u00b7nig\u00b7ku\u00b7chen", "so\u00b7gar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "ADV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wir sprechen unsern Dank daf\u00fcr aus", "tokens": ["Wir", "spre\u00b7chen", "un\u00b7sern", "Dank", "da\u00b7f\u00fcr", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PAV", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und wandern dann in ein anderes Haus.", "tokens": ["Und", "wan\u00b7dern", "dann", "in", "ein", "an\u00b7de\u00b7res", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nun la\u00dft uns heute singen auch", "tokens": ["Nun", "la\u00dft", "uns", "heu\u00b7te", "sin\u00b7gen", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie's ist am Martinstag der Brauch!", "tokens": ["Wie's", "ist", "am", "Mar\u00b7tins\u00b7tag", "der", "Brauch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}