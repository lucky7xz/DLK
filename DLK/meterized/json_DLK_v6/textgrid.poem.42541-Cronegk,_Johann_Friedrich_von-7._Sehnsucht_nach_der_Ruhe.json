{"textgrid.poem.42541": {"metadata": {"author": {"name": "Cronegk, Johann Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "7. Sehnsucht nach der Ruhe", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann werd' ich wiederum, ihr unschuldsvollen Haiden,", "tokens": ["Wann", "werd'", "ich", "wie\u00b7de\u00b7rum", ",", "ihr", "un\u00b7schulds\u00b7vol\u00b7len", "Hai\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann werd' ich wiederum euch bl\u00fchend l\u00e4cheln sehn?", "tokens": ["Wann", "werd'", "ich", "wie\u00b7de\u00b7rum", "euch", "bl\u00fc\u00b7hend", "l\u00e4\u00b7cheln", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PPER", "ADJD", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In euch nur wohnen noch des Lebens stille Freuden,", "tokens": ["In", "euch", "nur", "woh\u00b7nen", "noch", "des", "Le\u00b7bens", "stil\u00b7le", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die dem betrognen Blick der Sterblichen entgehn;", "tokens": ["Die", "dem", "be\u00b7trog\u00b7nen", "Blick", "der", "Sterb\u00b7li\u00b7chen", "ent\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die man vergebens sucht, wo sie zu wohnen scheinen,", "tokens": ["Die", "man", "ver\u00b7ge\u00b7bens", "sucht", ",", "wo", "sie", "zu", "woh\u00b7nen", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVFIN", "$,", "PWAV", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In l\u00e4rmendem Gedr\u00e4ng, in unruhvoller Pracht;", "tokens": ["In", "l\u00e4r\u00b7men\u00b7dem", "Ge\u00b7dr\u00e4ng", ",", "in", "un\u00b7ruh\u00b7vol\u00b7ler", "Pracht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die nur der Weise f\u00fchlt in unbewohnten Hainen,", "tokens": ["Die", "nur", "der", "Wei\u00b7se", "f\u00fchlt", "in", "un\u00b7be\u00b7wohn\u00b7ten", "Hai\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der ganzen Welt versteckt, zur Zeit der ruh'gen Nacht:", "tokens": ["Der", "gan\u00b7zen", "Welt", "ver\u00b7steckt", ",", "zur", "Zeit", "der", "ruh'\u00b7gen", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn er, in sich versenkt, sich und die Welt betrachtet,", "tokens": ["Wenn", "er", ",", "in", "sich", "ver\u00b7senkt", ",", "sich", "und", "die", "Welt", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PRF", "VVPP", "$,", "PRF", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die pr\u00e4cht'gen Schmerzen sieht, die Stolz und Torheit liebt,", "tokens": ["Die", "pr\u00e4cht'\u00b7gen", "Schmer\u00b7zen", "sieht", ",", "die", "Stolz", "und", "Tor\u00b7heit", "liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und, mit sich selbst vergn\u00fcgt, des P\u00f6bels Wahn verachtet,", "tokens": ["Und", ",", "mit", "sich", "selbst", "ver\u00b7gn\u00fcgt", ",", "des", "P\u00f6\u00b7bels", "Wahn", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PRF", "ADV", "VVPP", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zufrieden mit dem Gl\u00fcck, das Ruh' und Tugend gibt.", "tokens": ["Zu\u00b7frie\u00b7den", "mit", "dem", "Gl\u00fcck", ",", "das", "Ruh'", "und", "Tu\u00b7gend", "gibt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Man eilt der Freude nach und wei\u00df sie nie zu finden.", "tokens": ["Man", "eilt", "der", "Freu\u00b7de", "nach", "und", "wei\u00df", "sie", "nie", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "KON", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Man sucht sie fern von uns, und allzeit ist sie nah.", "tokens": ["Man", "sucht", "sie", "fern", "von", "uns", ",", "und", "all\u00b7zeit", "ist", "sie", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "$,", "KON", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wann einst der Eitelkeit Verblendungen verschwinden,", "tokens": ["Wann", "einst", "der", "Ei\u00b7tel\u00b7keit", "Ver\u00b7blen\u00b7dun\u00b7gen", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Erstaunt man, da\u00df man sonst sein gr\u00f6\u00dftes Gl\u00fcck nicht sah.", "tokens": ["Er\u00b7staunt", "man", ",", "da\u00df", "man", "sonst", "sein", "gr\u00f6\u00df\u00b7tes", "Gl\u00fcck", "nicht", "sah", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PIS", "ADV", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was braucht man mehr zur Lust als eine stille Seele?", "tokens": ["Was", "braucht", "man", "mehr", "zur", "Lust", "als", "ei\u00b7ne", "stil\u00b7le", "See\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "ADV", "APPRART", "NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Was braucht man mehr zum Gl\u00fcck als ein zufriednes Herz?", "tokens": ["Was", "braucht", "man", "mehr", "zum", "Gl\u00fcck", "als", "ein", "zu\u00b7fried\u00b7nes", "Herz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "ADV", "APPRART", "NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Im pr\u00e4chtigsten Palast und in der tiefsten H\u00f6hle", "tokens": ["Im", "pr\u00e4ch\u00b7tigs\u00b7ten", "Pa\u00b7last", "und", "in", "der", "tiefs\u00b7ten", "H\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Kann wahre Freude sein so gut als wahrer Schmerz.", "tokens": ["Kann", "wah\u00b7re", "Freu\u00b7de", "sein", "so", "gut", "als", "wah\u00b7rer", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "PPOSAT", "ADV", "ADJD", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der Seelen heil'ge Ruh', von wenigen gefunden,", "tokens": ["Der", "See\u00b7len", "heil'\u00b7ge", "Ruh'", ",", "von", "we\u00b7ni\u00b7gen", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Von vielen nicht gesucht, den meisten unbekannt,", "tokens": ["Von", "vie\u00b7len", "nicht", "ge\u00b7sucht", ",", "den", "meis\u00b7ten", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PTKNEG", "VVPP", "$,", "ART", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ist nicht an einen Stand, an einen Ort gebunden;", "tokens": ["Ist", "nicht", "an", "ei\u00b7nen", "Stand", ",", "an", "ei\u00b7nen", "Ort", "ge\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Nein, jede Gegend ist des Weisen Vaterland.", "tokens": ["Nein", ",", "je\u00b7de", "Ge\u00b7gend", "ist", "des", "Wei\u00b7sen", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIAT", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der \u00e4u\u00dfre Schein ist nichts; das Herz mu\u00df gl\u00fccklich machen,", "tokens": ["Der", "\u00e4u\u00df\u00b7re", "Schein", "ist", "nichts", ";", "das", "Herz", "mu\u00df", "gl\u00fcck\u00b7lich", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIS", "$.", "ART", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und jeder bildet sich sein eigenes Geschick:", "tokens": ["Und", "je\u00b7der", "bil\u00b7det", "sich", "sein", "ei\u00b7ge\u00b7nes", "Ge\u00b7schick", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Das Kind ist schon vergn\u00fcgt mit den geringsten Sachen:", "tokens": ["Das", "Kind", "ist", "schon", "ver\u00b7gn\u00fcgt", "mit", "den", "ge\u00b7rings\u00b7ten", "Sa\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Den abgelebten Greis erfreut kein wirklich Gl\u00fcck.", "tokens": ["Den", "ab\u00b7ge\u00b7leb\u00b7ten", "Greis", "er\u00b7freut", "kein", "wirk\u00b7lich", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "O st\u00f6rt nicht unsre Ruh' durch schwermutsvolle S\u00e4tze,", "tokens": ["O", "st\u00f6rt", "nicht", "uns\u00b7re", "Ruh'", "durch", "schwer\u00b7muts\u00b7vol\u00b7le", "S\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ihr Weisen! \u00fcberla\u00dft die Menschen der Natur!", "tokens": ["Ihr", "Wei\u00b7sen", "!", "\u00fc\u00b7berl\u00b7a\u00dft", "die", "Men\u00b7schen", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Erlaubt dem Kind sein Spiel, den Greisen tote Sch\u00e4tze,", "tokens": ["Er\u00b7laubt", "dem", "Kind", "sein", "Spiel", ",", "den", "Grei\u00b7sen", "to\u00b7te", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Dem stolzen Manne Ruhm, und mir la\u00dft Chloen nur.", "tokens": ["Dem", "stol\u00b7zen", "Man\u00b7ne", "Ruhm", ",", "und", "mir", "la\u00dft", "Chloen", "nur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "KON", "PPER", "VVFIN", "NE", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Mein ruh'ges Leben soll in ihrem Arm verflie\u00dfen:", "tokens": ["Mein", "ruh'\u00b7ges", "Le\u00b7ben", "soll", "in", "ih\u00b7rem", "Arm", "ver\u00b7flie\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "In einer sichern Flur, in einem stillen Hain,", "tokens": ["In", "ei\u00b7ner", "si\u00b7chern", "Flur", ",", "in", "ei\u00b7nem", "stil\u00b7len", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Will ich, mit ihr vergn\u00fcgt, des Lebens Lust genie\u00dfen;", "tokens": ["Will", "ich", ",", "mit", "ihr", "ver\u00b7gn\u00fcgt", ",", "des", "Le\u00b7bens", "Lust", "ge\u00b7nie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "APPR", "PPOSAT", "VVPP", "$,", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Nicht witzig, nicht ber\u00fchmt, nein, gl\u00fccklich will ich sein.", "tokens": ["Nicht", "wit\u00b7zig", ",", "nicht", "be\u00b7r\u00fchmt", ",", "nein", ",", "gl\u00fcck\u00b7lich", "will", "ich", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "$,", "PTKANT", "$,", "ADJD", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Vergn\u00fcgt durch die Natur, will ich bei heiterm Morgen", "tokens": ["Ver\u00b7gn\u00fcgt", "durch", "die", "Na\u00b7tur", ",", "will", "ich", "bei", "hei\u00b7term", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Dem Vater der Natur vergn\u00fcgte Lieder weihn,", "tokens": ["Dem", "Va\u00b7ter", "der", "Na\u00b7tur", "ver\u00b7gn\u00fcg\u00b7te", "Lie\u00b7der", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der B\u00fcsche heil'ge Nacht, in die ich mich verborgen,", "tokens": ["Der", "B\u00fc\u00b7sche", "heil'\u00b7ge", "Nacht", ",", "in", "die", "ich", "mich", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wird ihm gef\u00e4lliger als Marmortempel sein.", "tokens": ["Wird", "ihm", "ge\u00b7f\u00e4l\u00b7li\u00b7ger", "als", "Mar\u00b7mor\u00b7tem\u00b7pel", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KOKOM", "NE", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wenn gleich zum Himmel nicht entweihter Weihrauch steiget;", "tokens": ["Wenn", "gleich", "zum", "Him\u00b7mel", "nicht", "ent\u00b7weih\u00b7ter", "Weih\u00b7rauch", "stei\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "PTKNEG", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Er h\u00f6rt der Seelen Wunsch und stille Seufzer an.", "tokens": ["Er", "h\u00f6rt", "der", "See\u00b7len", "Wunsch", "und", "stil\u00b7le", "Seuf\u00b7zer", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Es prangt die Redekunst, nur wenn das Herze schweiget,", "tokens": ["Es", "prangt", "die", "Re\u00b7de\u00b7kunst", ",", "nur", "wenn", "das", "Her\u00b7ze", "schwei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "KOUS", "PDS", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und dieses f\u00fchlet mehr, als es beschreiben kann.", "tokens": ["Und", "die\u00b7ses", "f\u00fch\u00b7let", "mehr", ",", "als", "es", "be\u00b7schrei\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-