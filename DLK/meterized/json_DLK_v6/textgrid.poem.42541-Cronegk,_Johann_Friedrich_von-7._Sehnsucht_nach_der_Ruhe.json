, "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,", "APPR", "ADJD", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Wann dann der Tod sich naht, will ich nicht unruhvoll", "tokens": ["Wann", "dann", "der", "Tod", "sich", "naht", ",", "will", "ich", "nicht", "un\u00b7ruh\u00b7voll"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "PRF", "VVFIN", "$,", "VMFIN", "PPER", "PTKNEG", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Den Himmel wiederum um meine Jugend bitten;", "tokens": ["Den", "Him\u00b7mel", "wie\u00b7de\u00b7rum", "um", "mei\u00b7ne", "Ju\u00b7gend", "bit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Ich habe g'nug gelebt. Ich lebte, wie man soll.", "tokens": ["Ich", "ha\u00b7be", "g'\u00b7nug", "ge\u00b7lebt", ".", "Ich", "leb\u00b7te", ",", "wie", "man", "soll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$.", "PPER", "VVFIN", "$,", "PWAV", "PIS", "VMFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.53": {"text": "Es dr\u00fccke Chloe selbst mit schwach gewordnen H\u00e4nden", "tokens": ["Es", "dr\u00fc\u00b7cke", "Chloe", "selbst", "mit", "schwach", "ge\u00b7word\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "ADV", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+----+-+-+-", "measure": "dactylic.init"}, "line.54": {"text": "Mir einst die Augen zu, die sterbend nach ihr sehn,", "tokens": ["Mir", "einst", "die", "Au\u00b7gen", "zu", ",", "die", "ster\u00b7bend", "nach", "ihr", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Und die sich noch vergn\u00fcgt zum g\u00fct'gen Himmel wenden,", "tokens": ["Und", "die", "sich", "noch", "ver\u00b7gn\u00fcgt", "zum", "g\u00fct'\u00b7gen", "Him\u00b7mel", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRF", "ADV", "VVPP", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Um ihm zu danken nur, nicht um ihn anzuflehn.", "tokens": ["Um", "ihm", "zu", "dan\u00b7ken", "nur", ",", "nicht", "um", "ihn", "an\u00b7zu\u00b7flehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKZU", "VVINF", "ADV", "$,", "PTKNEG", "APPR", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Mein Tod bleib' unbekannt und ruhig wie mein Leben;", "tokens": ["Mein", "Tod", "bleib'", "un\u00b7be\u00b7kannt", "und", "ru\u00b7hig", "wie", "mein", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Die Welt soll meinem Ruhm kein pr\u00e4cht'ges Denkmal weihn.", "tokens": ["Die", "Welt", "soll", "mei\u00b7nem", "Ruhm", "kein", "pr\u00e4cht'\u00b7ges", "Denk\u00b7mal", "