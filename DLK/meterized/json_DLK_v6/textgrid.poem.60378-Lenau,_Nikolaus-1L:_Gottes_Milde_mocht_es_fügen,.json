{"textgrid.poem.60378": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gottes Milde mocht es f\u00fcgen,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gottes Milde mocht es f\u00fcgen,", "tokens": ["Got\u00b7tes", "Mil\u00b7de", "mocht", "es", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt ein Mensch in letzten Z\u00fcgen,", "tokens": ["Liegt", "ein", "Mensch", "in", "letz\u00b7ten", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stehn am Sterbepf\u00fchl die Seinen,", "tokens": ["Stehn", "am", "Ster\u00b7be\u00b7pf\u00fchl", "die", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "PPOSS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie m\u00fcssen weinen, weinen;", "tokens": ["Da\u00df", "sie", "m\u00fcs\u00b7sen", "wei\u00b7nen", ",", "wei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df sie nicht vor Tr\u00e4nen schauen", "tokens": ["Da\u00df", "sie", "nicht", "vor", "Tr\u00e4\u00b7nen", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das unnennbar bange Grauen,", "tokens": ["Das", "un\u00b7nenn\u00b7bar", "ban\u00b7ge", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie der Geist verl\u00e4\u00dft die H\u00fclle,", "tokens": ["Wie", "der", "Geist", "ver\u00b7l\u00e4\u00dft", "die", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Letztes Zucken, tiefe Stille.", "tokens": ["Letz\u00b7tes", "Zu\u00b7cken", ",", "tie\u00b7fe", "Stil\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Weh dem Tr\u00e4nenlosen, wehe,", "tokens": ["Weh", "dem", "Tr\u00e4\u00b7nen\u00b7lo\u00b7sen", ",", "we\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sich wagt in Sterbens N\u00e4he,", "tokens": ["Der", "sich", "wagt", "in", "Ster\u00b7bens", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ihm kann durchs ganze Leben", "tokens": ["Denn", "ihm", "kann", "durchs", "gan\u00b7ze", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jenes Grauen heimlich beben.", "tokens": ["Je\u00b7nes", "Grau\u00b7en", "heim\u00b7lich", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch ein Anblick tiefrer Trauer,", "tokens": ["Doch", "ein", "An\u00b7blick", "tief\u00b7rer", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "B\u00e4nger als des Sterbens Schauer,", "tokens": ["B\u00e4n\u00b7ger", "als", "des", "Ster\u00b7bens", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "War es, k\u00f6nnt ein Aug es fassen,", "tokens": ["War", "es", ",", "k\u00f6nnt", "ein", "Aug", "es", "fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "VVFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie zwei Herzen sich verlassen.", "tokens": ["Wie", "zwei", "Her\u00b7zen", "sich", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}