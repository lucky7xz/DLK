{"textgrid.poem.63658": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "In such a night ...", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Veranda. Durch den Garten weht", "tokens": ["In", "der", "Ver\u00b7an\u00b7da", ".", "Durch", "den", "Gar\u00b7ten", "weht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Sommernacht. Der Vollmond gl\u00e4nzt im Blau,", "tokens": ["Die", "Som\u00b7mer\u00b7nacht", ".", "Der", "Voll\u00b7mond", "gl\u00e4nzt", "im", "Blau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df jeder Busch und Baum versilbert steht.", "tokens": ["Da\u00df", "je\u00b7der", "Busch", "und", "Baum", "ver\u00b7sil\u00b7bert", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "KON", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit leisem Rieseln spr\u00fcht herab der Tau,", "tokens": ["Mit", "lei\u00b7sem", "Rie\u00b7seln", "spr\u00fcht", "her\u00b7ab", "der", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Nachtviolen duften. Tr\u00e4umend ruht,", "tokens": ["Die", "Nacht\u00b7vi\u00b7o\u00b7len", "duf\u00b7ten", ".", "Tr\u00e4u\u00b7mend", "ruht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Haupt zur Brust geneigt, die holde Frau.", "tokens": ["Das", "Haupt", "zur", "Brust", "ge\u00b7neigt", ",", "die", "hol\u00b7de", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie sp\u00e4ht, wie der Johannisk\u00e4fer Brut", "tokens": ["Sie", "sp\u00e4ht", ",", "wie", "der", "Jo\u00b7han\u00b7nis\u00b7k\u00e4\u00b7fer", "Brut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NE", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Grase funkelt, das Gesicht, das blasse,", "tokens": ["Im", "Gra\u00b7se", "fun\u00b7kelt", ",", "das", "Ge\u00b7sicht", ",", "das", "blas\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sanft \u00fcberflogen von der roten Glut", "tokens": ["Sanft", "\u00fc\u00b7berf\u00b7lo\u00b7gen", "von", "der", "ro\u00b7ten", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Des L\u00e4mpchens auf den Tisch. Drau\u00df' auf der Gasse", "tokens": ["Des", "L\u00e4mp\u00b7chens", "auf", "den", "Tisch", ".", "Drau\u00df'", "auf", "der", "Gas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verhallt der L\u00e4rm. Ihr gegen\u00fcber sitzt", "tokens": ["Ver\u00b7hallt", "der", "L\u00e4rm", ".", "Ihr", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "sitzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Mann, versonnen heut im \u00dcberma\u00dfe,", "tokens": ["Ihr", "Mann", ",", "ver\u00b7son\u00b7nen", "heut", "im", "\u00dc\u00b7berm\u00b7a\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die Stirn nachdenklich in die Hand gest\u00fctzt.", "tokens": ["Die", "Stirn", "nach\u00b7denk\u00b7lich", "in", "die", "Hand", "ge\u00b7st\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Zeitung, die er las, liegt auf den Knieen,", "tokens": ["Die", "Zei\u00b7tung", ",", "die", "er", "las", ",", "liegt", "auf", "den", "Kni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.3": {"text": "Der Funke, der an der Zigarre blitzt',", "tokens": ["Der", "Fun\u00b7ke", ",", "der", "an", "der", "Zi\u00b7gar\u00b7re", "blitzt'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Erlosch. Als lausch' er fernen Melodieen,", "tokens": ["Er\u00b7losch", ".", "Als", "lausch'", "er", "fer\u00b7nen", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "KOUS", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hat er den Blick zu Boden still gesenkt,", "tokens": ["Hat", "er", "den", "Blick", "zu", "Bo\u00b7den", "still", "ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie aus magischer Laterne ziehen", "tokens": ["Und", "wie", "aus", "ma\u00b7gi\u00b7scher", "La\u00b7ter\u00b7ne", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Bilder an ihm vorbei. Da endlich f\u00e4ngt", "tokens": ["Bil\u00b7der", "an", "ihm", "vor\u00b7bei", ".", "Da", "end\u00b7lich", "f\u00e4ngt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$.", "ADV", "ADV", "VVFIN"], "meter": "+----+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Sie an: Was ist's, das dich so schweigsam macht?", "tokens": ["Sie", "an", ":", "Was", "ist's", ",", "das", "dich", "so", "schweig\u00b7sam", "macht", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$.", "PWS", "VAFIN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich m\u00f6chte wissen, was mein Liebster denkt.", "tokens": ["Ich", "m\u00f6ch\u00b7te", "wis\u00b7sen", ",", "was", "mein", "Liebs\u00b7ter", "denkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und er: Ja, Kind, woran ich jetzt gedacht \u2013", "tokens": ["Und", "er", ":", "Ja", ",", "Kind", ",", "wo\u00b7ran", "ich", "jetzt", "ge\u00b7dacht", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$.", "PTKANT", "$,", "NN", "$,", "PWAV", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Lang, lang ist's her. Im Fr\u00fchling war's und eben", "tokens": ["Lang", ",", "lang", "ist's", "her", ".", "Im", "Fr\u00fch\u00b7ling", "wa\u00b7r's", "und", "e\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "VAFIN", "PTKVZ", "$.", "APPRART", "NN", "VAFIN", "KON", "ADV"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So mondhell wie in dieser Sommernacht.", "tokens": ["So", "mond\u00b7hell", "wie", "in", "die\u00b7ser", "Som\u00b7mer\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Du hattst an jenem Abend dich f\u00fcrs Leben", "tokens": ["Du", "hattst", "an", "je\u00b7nem", "A\u00b7bend", "dich", "f\u00fcrs", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir angelobt und, da ich von euch ging,", "tokens": ["Mir", "an\u00b7ge\u00b7lobt", "und", ",", "da", "ich", "von", "euch", "ging", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "KON", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zur Treppe noch mir das Geleit gegeben,", "tokens": ["Zur", "Trep\u00b7pe", "noch", "mir", "das", "Ge\u00b7leit", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wo scheidend ich noch einmal dich umfing.", "tokens": ["Wo", "schei\u00b7dend", "ich", "noch", "ein\u00b7mal", "dich", "um\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dann, wie im Rausch, schwankt' ich hinab die Stiege,", "tokens": ["Dann", ",", "wie", "im", "Rausch", ",", "schwankt'", "ich", "hin\u00b7ab", "die", "Stie\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "APPRART", "NN", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Kopf zur\u00fcckgewandt. Mein Auge hing", "tokens": ["Den", "Kopf", "zu\u00b7r\u00fcck\u00b7ge\u00b7wandt", ".", "Mein", "Au\u00b7ge", "hing"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Entz\u00fcckt an deinem Antlitz, dessen Z\u00fcge", "tokens": ["Ent\u00b7z\u00fcckt", "an", "dei\u00b7nem", "Ant\u00b7litz", ",", "des\u00b7sen", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Kerze Schein mir zeigte wie verkl\u00e4rt,", "tokens": ["Der", "Ker\u00b7ze", "Schein", "mir", "zeig\u00b7te", "wie", "ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ganz so wie jetzt. Und in mir war's, als fr\u00fcge", "tokens": ["Ganz", "so", "wie", "jetzt", ".", "Und", "in", "mir", "wa\u00b7r's", ",", "als", "fr\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "KOKOM", "ADV", "$.", "KON", "APPR", "PPER", "VAFIN", "$,", "KOUS", "NN"], "meter": "+-+---+-+-+-", "measure": "unknown.measure.penta"}}, "stanza.12": {"line.1": {"text": "Mich mein Gewissen: bist du's denn auch wert,", "tokens": ["Mich", "mein", "Ge\u00b7wis\u00b7sen", ":", "bist", "du's", "denn", "auch", "wert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$.", "VAFIN", "PIS", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df dies holdsel'ge Wesen dir zu eigen", "tokens": ["Da\u00df", "dies", "hold\u00b7sel'\u00b7ge", "We\u00b7sen", "dir", "zu", "ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "ADJA", "NN", "PPER", "PTKA", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich geben will, dir, dem das Herz beschwert", "tokens": ["Sich", "ge\u00b7ben", "will", ",", "dir", ",", "dem", "das", "Herz", "be\u00b7schwert"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "VVINF", "VMFIN", "$,", "PPER", "$,", "PRELS", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "So manch Erinnern? Wenn die Schatten steigen", "tokens": ["So", "manch", "E\u00b7rin\u00b7nern", "?", "Wenn", "die", "Schat\u00b7ten", "stei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$.", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus ihrer Gruft, wird sie's nicht traurig machen?", "tokens": ["Aus", "ih\u00b7rer", "Gruft", ",", "wird", "sie's", "nicht", "trau\u00b7rig", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da sah ich gr\u00fc\u00dfend dein Gesicht sich neigen,", "tokens": ["Da", "sah", "ich", "gr\u00fc\u00b7\u00dfend", "dein", "Ge\u00b7sicht", "sich", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Die Augen strahlend mir entgegenlachen", "tokens": ["Die", "Au\u00b7gen", "strah\u00b7lend", "mir", "ent\u00b7ge\u00b7gen\u00b7la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und h\u00f6rt' ein letztes s\u00fc\u00dfes Liebeswort", "tokens": ["Und", "h\u00f6rt'", "ein", "letz\u00b7tes", "s\u00fc\u00b7\u00dfes", "Lie\u00b7bes\u00b7wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An deinem jungen roten Mund erwachen,", "tokens": ["An", "dei\u00b7nem", "jun\u00b7gen", "ro\u00b7ten", "Mund", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Ein letztes \u00bbgute Nacht!\u00ab \u2013 Dann st\u00fcrmt ich fort,", "tokens": ["Ein", "letz\u00b7tes", "\u00bb", "gu\u00b7te", "Nacht", "!", "\u00ab", "\u2013", "Dann", "st\u00fcrmt", "ich", "fort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "ADJA", "NN", "$.", "$(", "$(", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein heiliges Gel\u00fcbd' in mir, auf H\u00e4nden", "tokens": ["Ein", "hei\u00b7li\u00b7ges", "Ge\u00b7l\u00fcbd'", "in", "mir", ",", "auf", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu tragen das geliebte Leben dort.", "tokens": ["Zu", "tra\u00b7gen", "das", "ge\u00b7lieb\u00b7te", "Le\u00b7ben", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Und jetzt \u2013 auf einmal war es mir, als f\u00e4nden", "tokens": ["Und", "jetz