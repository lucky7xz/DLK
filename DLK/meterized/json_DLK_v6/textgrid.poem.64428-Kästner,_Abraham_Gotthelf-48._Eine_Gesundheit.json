{"textgrid.poem.64428": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "48. Eine Gesundheit", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Voll Feuer, wie die Frucht der Reben,", "tokens": ["Voll", "Feu\u00b7er", ",", "wie", "die", "Frucht", "der", "Re\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verlangt mein Wunsch, da\u00df alle Dichter leben,", "tokens": ["Ver\u00b7langt", "mein", "Wunsch", ",", "da\u00df", "al\u00b7le", "Dich\u00b7ter", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die ganze lange Reih', die noch den Reim nicht schm\u00e4ht,", "tokens": ["Die", "gan\u00b7ze", "lan\u00b7ge", "Reih'", ",", "die", "noch", "den", "Reim", "nicht", "schm\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom ", "tokens": ["Vom"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Doch, da\u00df wir, ein Opfer der Mode zu bringen,", "tokens": ["Doch", ",", "da\u00df", "wir", ",", "ein", "Op\u00b7fer", "der", "Mo\u00b7de", "zu", "brin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "$,", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Nicht g\u00e4nzlich versehn,", "tokens": ["Nicht", "g\u00e4nz\u00b7lich", "ver\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "So leben auch alle nicht reimende Dichter,", "tokens": ["So", "le\u00b7ben", "auch", "al\u00b7le", "nicht", "rei\u00b7men\u00b7de", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIS", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Vom betenden ", "tokens": ["Vom", "be\u00b7ten\u00b7den"], "token_info": ["word", "word"], "pos": ["APPRART", "ADJA"], "meter": "-+--", "measure": "dactylic.init"}}}}}