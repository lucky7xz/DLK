{"textgrid.poem.47224": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die beiden Lenen", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der Kindheit fr\u00fchsten Szenen", "tokens": ["Auf", "der", "Kind\u00b7heit", "fr\u00fchs\u00b7ten", "Sze\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Erinnerungsd\u00e4mmerschein", "tokens": ["Im", "E\u00b7rin\u00b7ne\u00b7rungs\u00b7d\u00e4m\u00b7mer\u00b7schein"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Seh' ich um mich stehn zwo Lenen,", "tokens": ["Seh'", "ich", "um", "mich", "stehn", "zwo", "Le\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "VVFIN", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Beide meine Schwesterlein;", "tokens": ["Bei\u00b7de", "mei\u00b7ne", "Schwes\u00b7ter\u00b7lein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles kam von denen,", "tokens": ["Al\u00b7les", "kam", "von", "de\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PRELS", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Was von zarter Poesie ist mein.", "tokens": ["Was", "von", "zar\u00b7ter", "Poe\u00b7sie", "ist", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "VAFIN", "PPOSAT", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Mir, dem Knaben, nachgeboren,", "tokens": ["Mir", ",", "dem", "Kna\u00b7ben", ",", "nach\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sahn sie selbst einander nicht;", "tokens": ["Sahn", "sie", "selbst", "ein\u00b7an\u00b7der", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schon der einen war verloren,", "tokens": ["Schon", "der", "ei\u00b7nen", "war", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PIS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eh' die andr' es sah, das Licht;", "tokens": ["Eh'", "die", "andr'", "es", "sah", ",", "das", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIS", "PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ich war erkoren,", "tokens": ["Und", "ich", "war", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Beide zu vereinen im Gedicht.", "tokens": ["Bei\u00b7de", "zu", "ver\u00b7ei\u00b7nen", "im", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Nach der ersten ging die zweite,", "tokens": ["Nach", "der", "ers\u00b7ten", "ging", "die", "zwei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als sie mir gel\u00e4chelt kaum;", "tokens": ["Als", "sie", "mir", "ge\u00b7l\u00e4\u00b7chelt", "kaum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Standen mir darauf zur Seite", "tokens": ["Stan\u00b7den", "mir", "da\u00b7rauf", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PAV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur, wie noch sie stehn, im Traum,", "tokens": ["Nur", ",", "wie", "noch", "sie", "stehn", ",", "im", "Traum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ADV", "PPER", "VVINF", "$,", "APPRART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Blickten zum Geleite", "tokens": ["Blick\u00b7ten", "zum", "Ge\u00b7lei\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Mir hernieder aus des Himmels Raum.", "tokens": ["Mir", "her\u00b7nie\u00b7der", "aus", "des", "Him\u00b7mels", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ob sie je den Blick entzogen,", "tokens": ["Ob", "sie", "je", "den", "Blick", "ent\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ich sein nicht w\u00fcrdig war?", "tokens": ["Wenn", "ich", "sein", "nicht", "w\u00fcr\u00b7dig", "war", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAINF", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber niemals mir entflogen", "tokens": ["A\u00b7ber", "nie\u00b7mals", "mir", "ent\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Engel ganz und gar;", "tokens": ["Mei\u00b7ne", "En\u00b7gel", "ganz", "und", "gar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus den tiefsten Wogen", "tokens": ["Aus", "den", "tiefs\u00b7ten", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Sah ich hoch mein Sterne-Schwester-Paar.", "tokens": ["Sah", "ich", "hoch", "mein", "Ster\u00b7ne\u00b7Schwes\u00b7ter\u00b7Paar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Jede von euch hei\u00dfet Lene,", "tokens": ["Je\u00b7de", "von", "euch", "hei\u00b7\u00dfet", "Le\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "PPER", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch der Nam' ist ganz nicht gleich.", "tokens": ["Doch", "der", "Nam'", "ist", "ganz", "nicht", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Voller hei\u00df die ein' Helene,", "tokens": ["Vol\u00b7ler", "hei\u00df", "die", "ein'", "He\u00b7le\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bild aus griech'schem Sch\u00f6nheitsreich,", "tokens": ["Bild", "aus", "griech'\u00b7schem", "Sch\u00f6n\u00b7heits\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber Magdalene", "tokens": ["A\u00b7ber", "Mag\u00b7da\u00b7le\u00b7ne"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Hie\u00df die andre herzempfindungsweich.", "tokens": ["Hie\u00df", "die", "and\u00b7re", "her\u00b7zemp\u00b7fin\u00b7dungs\u00b7weich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Ob Helen', ob Magdalene", "tokens": ["Ob", "He\u00b7len'", ",", "ob", "Mag\u00b7da\u00b7le\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KOUS", "NE", "$,", "KOUS", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Erste oder zweite war,", "tokens": ["Ers\u00b7te", "o\u00b7der", "zwei\u00b7te", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "KON", "ADJA", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00df ich nicht, es ist Helene", "tokens": ["Wei\u00df", "ich", "nicht", ",", "es", "ist", "He\u00b7le\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur mit Magdalen' ein Paar,", "tokens": ["Nur", "mit", "Mag\u00b7da\u00b7len'", "ein", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die nicht ohne jene,", "tokens": ["Die", "nicht", "oh\u00b7ne", "je\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "PDAT", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Beide miteinander immerdar.", "tokens": ["Bei\u00b7de", "mi\u00b7tein\u00b7an\u00b7der", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "O Helene, der Hellenen", "tokens": ["O", "He\u00b7le\u00b7ne", ",", "der", "Hel\u00b7le\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ne Klarheit strahle du!", "tokens": ["Sch\u00f6\u00b7ne", "Klar\u00b7heit", "strah\u00b7le", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Magdalene, Magd in Thr\u00e4nen,", "tokens": ["Mag\u00b7da\u00b7le\u00b7ne", ",", "Magd", "in", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sehnen gib und Friedensruh'", "tokens": ["Seh\u00b7nen", "gib", "und", "Frie\u00b7dens\u00b7ruh'"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVIMP", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir und allen denen,", "tokens": ["Mir", "und", "al\u00b7len", "de\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PIAT", "PDS", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Die noch h\u00f6ren meinen Liedern zu!", "tokens": ["Die", "noch", "h\u00f6\u00b7ren", "mei\u00b7nen", "Lie\u00b7dern", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}