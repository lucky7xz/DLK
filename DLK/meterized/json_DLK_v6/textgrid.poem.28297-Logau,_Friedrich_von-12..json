{"textgrid.poem.28297": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auff einem Fusse stehn und hundert Verse schmieden", "tokens": ["Auff", "ei\u00b7nem", "Fus\u00b7se", "stehn", "und", "hun\u00b7dert", "Ver\u00b7se", "schmie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF", "KON", "CARD", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das hab ich nie gekunt und bins auch wol zufrieden,", "tokens": ["Das", "hab", "ich", "nie", "ge\u00b7kunt", "und", "bins", "auch", "wol", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "KON", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ich es noch nicht kan. Ein Piltz w\u00e4chst eine Nacht,", "tokens": ["Da\u00df", "ich", "es", "noch", "nicht", "kan", ".", "Ein", "Piltz", "w\u00e4chst", "ei\u00b7ne", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PTKNEG", "VMFIN", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die andre f\u00e4llt er hin; drum wird er schlecht geacht.", "tokens": ["Die", "and\u00b7re", "f\u00e4llt", "er", "hin", ";", "drum", "wird", "er", "schlecht", "ge\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "PTKVZ", "$.", "PAV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "De\u00df Bacchus s\u00fcsser Safft, darauff Poeten pochen,", "tokens": ["De\u00df", "Bac\u00b7chus", "s\u00fcs\u00b7ser", "Safft", ",", "dar\u00b7auff", "Po\u00b7et\u00b7en", "po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "$,", "PAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mu\u00df werden zam durch Sonn und Zeit und mu\u00df wol kochen;", "tokens": ["Mu\u00df", "wer\u00b7den", "zam", "durch", "Sonn", "und", "Zeit", "und", "mu\u00df", "wol", "ko\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VAFIN", "ADJD", "APPR", "NN", "KON", "NN", "KON", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das Wa\u00dfer, das mit Macht da, dort herausser quillt,", "tokens": ["Das", "Wa\u00b7\u00dfer", ",", "das", "mit", "Macht", "da", ",", "dort", "her\u00b7aus\u00b7ser", "quillt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "PTKVZ", "$,", "ADV", "APZR", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hat seinen Nutz zwar auch, nur da\u00df es wenig gilt.", "tokens": ["Hat", "sei\u00b7nen", "Nutz", "zwar", "auch", ",", "nur", "da\u00df", "es", "we\u00b7nig", "gilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADV", "$,", "ADV", "KOUS", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}