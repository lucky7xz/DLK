{"textgrid.poem.33942": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Sonnen-J\u00fcngling", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sonnen-J\u00fcngling blickt zum ersten Mal", "tokens": ["Der", "Son\u00b7nen\u00b7J\u00fcng\u00b7ling", "blickt", "zum", "ers\u00b7ten", "Mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hernieder auf die Erde mit Verlangen,", "tokens": ["Her\u00b7nie\u00b7der", "auf", "die", "Er\u00b7de", "mit", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er kehrt sich gl\u00fchend ab in s\u00fc\u00dfem Bangen,", "tokens": ["Er", "kehrt", "sich", "gl\u00fc\u00b7hend", "ab", "in", "s\u00fc\u00b7\u00dfem", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch bl\u00fch'n schon Veilchen auf vor seinem Stral.", "tokens": ["Doch", "bl\u00fch'n", "schon", "Veil\u00b7chen", "auf", "vor", "sei\u00b7nem", "Stral", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er blickt noch einmal, und zu seiner Qual", "tokens": ["Er", "blickt", "noch", "ein\u00b7mal", ",", "und", "zu", "sei\u00b7ner", "Qual"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist schnell die erste Lilje aufgegangen;", "tokens": ["Ist", "schnell", "die", "ers\u00b7te", "Lil\u00b7je", "auf\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bei'm dritten Mal sieht er die Rose prangen,", "tokens": ["Bei'm", "drit\u00b7ten", "Mal", "sieht", "er", "die", "Ro\u00b7se", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nun mu\u00df er rastlos blicken, ohne Wahl.", "tokens": ["Nun", "mu\u00df", "er", "rast\u00b7los", "bli\u00b7cken", ",", "oh\u00b7ne", "Wahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,", "KOUI", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und ach, je l\u00e4nger er sie nun betrachtet,", "tokens": ["Und", "ach", ",", "je", "l\u00e4n\u00b7ger", "er", "sie", "nun", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ADV", "ADJD", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Je gr\u00f6\u00dfer wird in seiner Brust das Sehnen,", "tokens": ["Je", "gr\u00f6\u00b7\u00dfer", "wird", "in", "sei\u00b7ner", "Brust", "das", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil sie sich immer lieblicher gestaltet.", "tokens": ["Weil", "sie", "sich", "im\u00b7mer", "lieb\u00b7li\u00b7cher", "ge\u00b7stal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er aber, der sich neben ihr verachtet,", "tokens": ["Er", "a\u00b7ber", ",", "der", "sich", "ne\u00b7ben", "ihr", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ahnt nicht in seinem Weh und seinen Thr\u00e4nen,", "tokens": ["Ahnt", "nicht", "in", "sei\u00b7nem", "Weh", "und", "sei\u00b7nen", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df all' die Sch\u00f6nheit nur sein Blick entfaltet.", "tokens": ["Da\u00df", "all'", "die", "Sch\u00f6n\u00b7heit", "nur", "sein", "Blick", "ent\u00b7fal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}