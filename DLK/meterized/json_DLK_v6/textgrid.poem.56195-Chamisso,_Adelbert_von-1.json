{"textgrid.poem.56195": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat die Zeit gegeben,", "tokens": ["Es", "hat", "die", "Zeit", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo hinaus mein Auge mich trug,", "tokens": ["Wo", "hin\u00b7aus", "mein", "Au\u00b7ge", "mich", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APZR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Zu folgen im tiefen Lichtmeer", "tokens": ["Zu", "fol\u00b7gen", "im", "tie\u00b7fen", "Licht\u00b7meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der fl\u00fcchtigen Wolken Zug;", "tokens": ["Der", "fl\u00fcch\u00b7ti\u00b7gen", "Wol\u00b7ken", "Zug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Zu streifen \u00fcber die Ebne", "tokens": ["Zu", "strei\u00b7fen", "\u00fc\u00b7ber", "die", "Eb\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nach jenem verschwindenden Saum,", "tokens": ["Nach", "je\u00b7nem", "ver\u00b7schwin\u00b7den\u00b7den", "Saum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mich unbegrenzt zu verlieren", "tokens": ["Mich", "un\u00b7be\u00b7grenzt", "zu", "ver\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im lichten unendlichen Raum.", "tokens": ["Im", "lich\u00b7ten", "un\u00b7end\u00b7li\u00b7chen", "Raum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Die Zeit ist abgeflossen,", "tokens": ["Die", "Zeit", "ist", "ab\u00b7ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lebwohl, du heiterer Schein!", "tokens": ["Leb\u00b7wohl", ",", "du", "hei\u00b7te\u00b7rer", "Schein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es schlie\u00dfet die Nacht der Blindheit", "tokens": ["Es", "schlie\u00b7\u00dfet", "die", "Nacht", "der", "Blind\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In engere Schranken mich ein.", "tokens": ["In", "en\u00b7ge\u00b7re", "Schran\u00b7ken", "mich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "O trauert nicht, ihr Schwestern,", "tokens": ["O", "trau\u00b7ert", "nicht", ",", "ihr", "Schwes\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich dem Licht erstarb;", "tokens": ["Da\u00df", "ich", "dem", "Licht", "er\u00b7starb", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr wi\u00dft nur, was ich verloren,", "tokens": ["Ihr", "wi\u00dft", "nur", ",", "was", "ich", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr wi\u00dft nicht, was ich erwarb.", "tokens": ["Ihr", "wi\u00dft", "nicht", ",", "was", "ich", "er\u00b7warb", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich bin aus irren Fernen", "tokens": ["Ich", "bin", "aus", "ir\u00b7ren", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In mich zur\u00fccke gekehrt,", "tokens": ["In", "mich", "zu\u00b7r\u00fc\u00b7cke", "ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Die Welt in des Busens Tiefe", "tokens": ["Die", "Welt", "in", "des", "Bu\u00b7sens", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ist wohl die verlorene wert.", "tokens": ["Ist", "wohl", "die", "ver\u00b7lo\u00b7re\u00b7ne", "wert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Was au\u00dfen t\u00f6net, das steiget", "tokens": ["Was", "au\u00b7\u00dfen", "t\u00f6\u00b7net", ",", "das", "stei\u00b7get"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "$,", "PDS", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Herein in mein Heiligtum;", "tokens": ["Her\u00b7ein", "in", "mein", "Hei\u00b7lig\u00b7tum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und was die Brust mir beweget,", "tokens": ["Und", "was", "die", "Brust", "mir", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das ist mein Eigentum.", "tokens": ["Das", "ist", "mein", "Ei\u00b7gen\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}