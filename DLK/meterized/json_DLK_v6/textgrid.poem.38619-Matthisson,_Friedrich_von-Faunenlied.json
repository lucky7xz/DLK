{"textgrid.poem.38619": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Faunenlied", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann schl\u00e4frig die Lippen", "tokens": ["Wann", "schl\u00e4f\u00b7rig", "die", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Beim G\u00f6ttermahl nippen,", "tokens": ["Beim", "G\u00f6t\u00b7ter\u00b7mahl", "nip\u00b7pen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Umtanzen wir Faunen", "tokens": ["Um\u00b7tan\u00b7zen", "wir", "Fau\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Im Walde den Schlauch", "tokens": ["Im", "Wal\u00b7de", "den", "Schlauch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Nach altem Gebrauch,", "tokens": ["Nach", "al\u00b7tem", "Ge\u00b7brauch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Mit Blonden und Braunen.", "tokens": ["Mit", "Blon\u00b7den", "und", "Brau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Wir tauchen die Sorgen", "tokens": ["Wir", "tau\u00b7chen", "die", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Von gestern und morgen", "tokens": ["Von", "ge\u00b7stern", "und", "mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "KON", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "In sch\u00e4umende Becher,", "tokens": ["In", "sch\u00e4u\u00b7men\u00b7de", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Bacchantisch das Haupt", "tokens": ["Bac\u00b7chan\u00b7tisch", "das", "Haupt"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Mit Eppich umlaubt,", "tokens": ["Mit", "Ep\u00b7pich", "um\u00b7laubt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Dem Lorbeer der Zecher.", "tokens": ["Dem", "Lor\u00b7beer", "der", "Ze\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Wir schlummern in Grotten,", "tokens": ["Wir", "schlum\u00b7mern", "in", "Grot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Umkr\u00e4uselt von Zotten", "tokens": ["Um\u00b7kr\u00e4u\u00b7selt", "von", "Zot\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sicilischer Vliesse;", "tokens": ["Si\u00b7ci\u00b7li\u00b7scher", "Vlies\u00b7se", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gar treffend und sch\u00f6n", "tokens": ["Gar", "tref\u00b7fend", "und", "sch\u00f6n"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sagt Vater Silen:", "tokens": ["Sagt", "Va\u00b7ter", "Si\u00b7len", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Entbehr' und geniesse!", "tokens": ["Ent\u00b7behr'", "und", "ge\u00b7nies\u00b7se", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Wir wissen in Ch\u00f6ren,", "tokens": ["Wir", "wis\u00b7sen", "in", "Ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dir, Bacchus! zu Ehren,", "tokens": ["Dir", ",", "Bac\u00b7chus", "!", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$.", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Arkadisch zu pfeifen.", "tokens": ["Ar\u00b7ka\u00b7disch", "zu", "pfei\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das geht bis ins Mark!", "tokens": ["Das", "geht", "bis", "ins", "Mark", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Nur Pan ist so stark", "tokens": ["Nur", "Pan", "ist", "so", "stark"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VAFIN", "ADV", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "In Trillern und L\u00e4ufen.", "tokens": ["In", "Tril\u00b7lern", "und", "L\u00e4u\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Die F\u00e4unlinge sonnen,", "tokens": ["Die", "F\u00e4un\u00b7lin\u00b7ge", "son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Bei ledigen Tonnen,", "tokens": ["Bei", "le\u00b7di\u00b7gen", "Ton\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sich krauend auf Rasen,", "tokens": ["Sich", "krau\u00b7end", "auf", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und \u00fcben sich schon,", "tokens": ["Und", "\u00fc\u00b7ben", "sich", "schon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Mit schnarrendem Ton", "tokens": ["Mit", "schnar\u00b7ren\u00b7dem", "Ton"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Ein St\u00fcckchen zu blasen.", "tokens": ["Ein", "St\u00fcck\u00b7chen", "zu", "bla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Eur W\u00fcnschen entfliege", "tokens": ["Eur", "W\u00fcn\u00b7schen", "ent\u00b7flie\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nie jenseit der Kr\u00fcge,", "tokens": ["Nie", "jen\u00b7seit", "der", "Kr\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nach menschlicher Weise!", "tokens": ["Nach", "menschli\u00b7cher", "Wei\u00b7se", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "O Schlauch, unsre Welt,", "tokens": ["O", "Schlauch", ",", "uns\u00b7re", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Bist du nur geschwellt,", "tokens": ["Bist", "du", "nur", "ge\u00b7schwellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ist alles im Gleise!", "tokens": ["Ist", "al\u00b7les", "im", "Glei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Die Ohren zu recken,", "tokens": ["Die", "Oh\u00b7ren", "zu", "re\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wo Nymphen im Becken", "tokens": ["Wo", "Nym\u00b7phen", "im", "Be\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Quelle sich waschen,", "tokens": ["Der", "Quel\u00b7le", "sich", "wa\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und r\u00fcstig bergauf,", "tokens": ["Und", "r\u00fcs\u00b7tig", "ber\u00b7gauf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Bergnieder im Lauf", "tokens": ["Berg\u00b7nie\u00b7der", "im", "Lauf"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Die Spr\u00f6den zu haschen;", "tokens": ["Die", "Spr\u00f6\u00b7den", "zu", "ha\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Das ziemet in W\u00e4ldern,", "tokens": ["Das", "zie\u00b7met", "in", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In Grotten und Feldern,", "tokens": ["In", "Grot\u00b7ten", "und", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Dem w\u00e4hligen Volke,", "tokens": ["Dem", "w\u00e4h\u00b7li\u00b7gen", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Bocks\u00f6hrig und leicht!", "tokens": ["Bock\u00b7s\u00f6h\u00b7rig", "und", "leicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Gelegenheit fleucht,", "tokens": ["Ge\u00b7le\u00b7gen\u00b7heit", "fleucht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Wie Wasser und Wolke!", "tokens": ["Wie", "Was\u00b7ser", "und", "Wol\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}