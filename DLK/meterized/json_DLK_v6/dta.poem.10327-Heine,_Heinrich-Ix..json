{"dta.poem.10327": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ix.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich lag und schlief, und schlief recht mild,", "tokens": ["Ich", "lag", "und", "schlief", ",", "und", "schlief", "recht", "mild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verscheucht war Gram und Leid;", "tokens": ["Ver\u00b7scheucht", "war", "Gram", "und", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kam zu mir ein Traumgebild,", "tokens": ["Da", "kam", "zu", "mir", "ein", "Traum\u00b7ge\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die allersch\u00f6nste Maid.", "tokens": ["Die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Maid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie war wie Marmelstein so bleich,", "tokens": ["Sie", "war", "wie", "Mar\u00b7mel\u00b7stein", "so", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und heimlich wunderbar;", "tokens": ["Und", "heim\u00b7lich", "wun\u00b7der\u00b7bar", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Auge schwamm es perlengleich,", "tokens": ["Im", "Au\u00b7ge", "schwamm", "es", "per\u00b7len\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar seltsam wallt' ihr Haar.", "tokens": ["Gar", "selt\u00b7sam", "wallt'", "ihr", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und leise, leise sich bewegt", "tokens": ["Und", "lei\u00b7se", ",", "lei\u00b7se", "sich", "be\u00b7wegt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "ADJD", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die marmorblasse Maid,", "tokens": ["Die", "mar\u00b7mor\u00b7blas\u00b7se", "Maid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und an mein Herz sich niederlegt", "tokens": ["Und", "an", "mein", "Herz", "sich", "nie\u00b7der\u00b7legt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die marmorblasse Maid.", "tokens": ["Die", "mar\u00b7mor\u00b7blas\u00b7se", "Maid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie bebt und pocht vor Weh und Lust,", "tokens": ["Wie", "bebt", "und", "pocht", "vor", "Weh", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "KON", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz, und brennet hei\u00df!", "tokens": ["Mein", "Herz", ",", "und", "bren\u00b7net", "hei\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht bebt, nicht pocht der Sch\u00f6nen Brust,", "tokens": ["Nicht", "bebt", ",", "nicht", "pocht", "der", "Sch\u00f6\u00b7nen", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ist so kalt wie Eis.", "tokens": ["Die", "ist", "so", "kalt", "wie", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u201enicht bebt, nicht pocht wohl meine Brust,", "tokens": ["\u201e", "nicht", "bebt", ",", "nicht", "pocht", "wohl", "mei\u00b7ne", "Brust", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "VVFIN", "$,", "PTKNEG", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ist wie Eis so kalt;", "tokens": ["Die", "ist", "wie", "Eis", "so", "kalt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "KOKOM", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch kenn' auch ich der Liebe Lust,", "tokens": ["Doch", "kenn'", "auch", "ich", "der", "Lie\u00b7be", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Liebe Allgewalt.", "tokens": ["Der", "Lie\u00b7be", "All\u00b7ge\u00b7walt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mir bl\u00fcht kein Roth auf Mund und Wang,", "tokens": ["Mir", "bl\u00fcht", "kein", "Roth", "auf", "Mund", "und", "Wang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz durchstr\u00f6mt kein Blut;", "tokens": ["Mein", "Herz", "durch\u00b7str\u00f6mt", "kein", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch str\u00e4ube dich nicht schauernd bang,", "tokens": ["Doch", "str\u00e4u\u00b7be", "dich", "nicht", "schau\u00b7ernd", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bin dir hold und gut.\u201c", "tokens": ["Ich", "bin", "dir", "hold", "und", "gut", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und wilder noch umschlang sie mich,", "tokens": ["Und", "wil\u00b7der", "noch", "um\u00b7schlang", "sie", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und that mir bald ein Leid;", "tokens": ["Und", "that", "mir", "bald", "ein", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kr\u00e4ht der Hahn \u2014 und stumm entwich", "tokens": ["Da", "kr\u00e4ht", "der", "Hahn", "und", "stumm", "ent\u00b7wich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "KON", "ADJD", "ADJD"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Die marmorblasse Maid.", "tokens": ["Die", "mar\u00b7mor\u00b7blas\u00b7se", "Maid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}