{"dta.poem.10400": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Wir haben viel f\u00fcr einander gef\u00fchlt,", "tokens": ["Wir", "ha\u00b7ben", "viel", "f\u00fcr", "ein\u00b7an\u00b7der", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und dennoch uns gar vortrefflich vertragen.", "tokens": ["Und", "den\u00b7noch", "uns", "gar", "vor\u00b7treff\u00b7lich", "ver\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wir haben oft \u201eMann und Frau\u201c gespielt", "tokens": ["Wir", "ha\u00b7ben", "oft", "\u201e", "Mann", "und", "Frau", "\u201c", "ge\u00b7spielt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ADV", "$(", "NN", "KON", "NN", "$(", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und dennoch uns nicht gerauft und geschlagen.", "tokens": ["Und", "den\u00b7noch", "uns", "nicht", "ger\u00b7auft", "und", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "PTKNEG", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wir haben zusammen gejauchzt und gescherzt,", "tokens": ["Wir", "ha\u00b7ben", "zu\u00b7sam\u00b7men", "ge\u00b7jauchzt", "und", "ge\u00b7scherzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und z\u00e4rtlich uns gek\u00fc\u00dft und geherzt.", "tokens": ["Und", "z\u00e4rt\u00b7lich", "uns", "ge\u00b7k\u00fc\u00dft", "und", "ge\u00b7herzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Wir haben am Ende, aus kindischer Lust,", "tokens": ["Wir", "ha\u00b7ben", "am", "En\u00b7de", ",", "aus", "kin\u00b7di\u00b7scher", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "\u201everstecken\u201c gespielt in W\u00e4ldern und Gr\u00fcnden,", "tokens": ["\u201e", "ver\u00b7ste\u00b7cken", "\u201c", "ge\u00b7spielt", "in", "W\u00e4l\u00b7dern", "und", "Gr\u00fcn\u00b7den", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$(", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und haben uns so zu verstecken gewu\u00dft,", "tokens": ["Und", "ha\u00b7ben", "uns", "so", "zu", "ver\u00b7ste\u00b7cken", "ge\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Da\u00df wir uns nimmermehr wiederfinden.", "tokens": ["Da\u00df", "wir", "uns", "nim\u00b7mer\u00b7mehr", "wie\u00b7der\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "+-----+-+-", "measure": "dactylic.init"}}}}}