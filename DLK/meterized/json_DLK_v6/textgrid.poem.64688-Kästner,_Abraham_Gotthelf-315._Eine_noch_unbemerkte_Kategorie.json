{"textgrid.poem.64688": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "315. Eine noch unbemerkte Kategorie", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ihre Kategorieen z\u00e4hlt,", "tokens": ["Die", "ih\u00b7re", "Ka\u00b7te\u00b7go\u00b7ri\u00b7een", "z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wi\u00dft, da\u00df euch doch noch eine fehlt,", "tokens": ["Wi\u00dft", ",", "da\u00df", "euch", "doch", "noch", "ei\u00b7ne", "fehlt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADV", "ADV", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In die von euch wohl Manche selbst geh\u00f6ren;", "tokens": ["In", "die", "von", "euch", "wohl", "Man\u00b7che", "selbst", "ge\u00b7h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "PPER", "ADV", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Philosoph ", "tokens": ["Der", "Phi\u00b7lo\u00b7soph"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}}}}}