{"dta.poem.14599": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcck auf .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Gar viel hab' ich versucht, gek\u00e4mpft, ertragen;", "tokens": ["Gar", "viel", "hab'", "ich", "ver\u00b7sucht", ",", "ge\u00b7k\u00e4mpft", ",", "er\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "VVPP", "$,", "VVPP", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ist der tiefen Sehnsucht Lebenslauf,", "tokens": ["Das", "ist", "der", "tie\u00b7fen", "Sehn\u00b7sucht", "Le\u00b7bens\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df br\u00fcnstig sie an jeden Fels mu\u00df schlagen,", "tokens": ["Da\u00df", "br\u00fcns\u00b7tig", "sie", "an", "je\u00b7den", "Fels", "mu\u00df", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "APPR", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob sich des Lichtes Gnadenth\u00fcr th\u00e4t' auf,", "tokens": ["Ob", "sich", "des", "Lich\u00b7tes", "Gna\u00b7dent\u00b7h\u00fcr", "th\u00e4t'", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie ein versch\u00fctt'ter Bergmann in den Kl\u00fcften", "tokens": ["Wie", "ein", "ver\u00b7sch\u00fctt'\u00b7ter", "Berg\u00b7mann", "in", "den", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Heraus sich hauet zu den heitern L\u00fcften.", "tokens": ["He\u00b7raus", "sich", "hau\u00b7et", "zu", "den", "hei\u00b7tern", "L\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}