{"textgrid.poem.61290": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Menetekel", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hei, wie die Tafeln sind geschm\u00fcckt,", "tokens": ["Hei", ",", "wie", "die", "Ta\u00b7feln", "sind", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie klar die Kerzen erglommen!", "tokens": ["Wie", "klar", "die", "Ker\u00b7zen", "er\u00b7glom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wer singt und lacht und Rosen pfl\u00fcckt,", "tokens": ["Wer", "singt", "und", "lacht", "und", "Ro\u00b7sen", "pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der ist zum Fest willkommen.", "tokens": ["Der", "ist", "zum", "Fest", "will\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Musik erklingt den Saal herauf,", "tokens": ["Mu\u00b7sik", "er\u00b7klingt", "den", "Saal", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sch\u00f6ne M\u00e4dchen warten auf", "tokens": ["Sch\u00f6\u00b7ne", "M\u00e4d\u00b7chen", "war\u00b7ten", "auf"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In leichten, losen Gewanden.", "tokens": ["In", "leich\u00b7ten", ",", "lo\u00b7sen", "Ge\u00b7wan\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie tanzen um das goldne Kalb,", "tokens": ["Sie", "tan\u00b7zen", "um", "das", "gold\u00b7ne", "Kalb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie fallen ihm gar zu F\u00fc\u00dfen;", "tokens": ["Sie", "fal\u00b7len", "ihm", "gar", "zu", "F\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie rufen: \u00bbEhe das Laub wird falb,", "tokens": ["Sie", "ru\u00b7fen", ":", "\u00bb", "E\u00b7he", "das", "Laub", "wird", "falb", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hilf du die Lust uns b\u00fc\u00dfen!\u00ab", "tokens": ["Hilf", "du", "die", "Lust", "uns", "b\u00fc\u00b7\u00dfen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PPER", "ART", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00dcbersch\u00e4umt im Kelch der Wein.", "tokens": ["\u00dc\u00b7bersc\u00b7h\u00e4umt", "im", "Kelch", "der", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich dr\u00fccke mich stumm in den Winkel hinein;", "tokens": ["Ich", "dr\u00fc\u00b7cke", "mich", "stumm", "in", "den", "Win\u00b7kel", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Mir schaudert das Herz im Leibe.", "tokens": ["Mir", "schau\u00b7dert", "das", "Herz", "im", "Lei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Mir ist's, durchsichtig wird die Wand,", "tokens": ["Mir", "ist's", ",", "durch\u00b7sich\u00b7tig", "wird", "die", "Wand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und drau\u00dfen, dicht und dichter,", "tokens": ["Und", "drau\u00b7\u00dfen", ",", "dicht", "und", "dich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da dr\u00e4ngen sich bei Fackelbrand", "tokens": ["Da", "dr\u00e4n\u00b7gen", "sich", "bei", "Fa\u00b7ckel\u00b7brand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Viel tausend Hungergesichter.", "tokens": ["Viel", "tau\u00b7send", "Hun\u00b7ger\u00b7ge\u00b7sich\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Durchs Gew\u00fchl mit ries'gem Leib", "tokens": ["Durchs", "Ge\u00b7w\u00fchl", "mit", "ries'\u00b7gem", "Leib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Herschreitet kampfgesch\u00fcrzt ein Weib", "tokens": ["Her\u00b7schrei\u00b7tet", "kampf\u00b7ge\u00b7sch\u00fcrzt", "ein", "Weib"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit blutrot flatternder Fahne.", "tokens": ["Mit", "blut\u00b7rot", "flat\u00b7tern\u00b7der", "Fah\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und sieh, der Boden wird zu Glas,", "tokens": ["Und", "sieh", ",", "der", "Bo\u00b7den", "wird", "zu", "Glas", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und drunten seh' ich sitzen", "tokens": ["Und", "drun\u00b7ten", "seh'", "ich", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Tod mit Augen hohl und gra\u00df", "tokens": ["Den", "Tod", "mit", "Au\u00b7gen", "hohl", "und", "gra\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit der Sense blitzen;", "tokens": ["Und", "mit", "der", "Sen\u00b7se", "blit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "S\u00e4rg' auf S\u00e4rgen rings get\u00fcrmt \u2013", "tokens": ["S\u00e4r\u00b7g'", "auf", "S\u00e4r\u00b7gen", "rings", "ge\u00b7t\u00fcrmt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "VVPP", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Doch dr\u00fcberhin wie rasend st\u00fcrmt", "tokens": ["Doch", "dr\u00fc\u00b7berh\u00b7in", "wie", "ra\u00b7send", "st\u00fcrmt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Tanz mit Pfeifen und Geigen.", "tokens": ["Der", "Tanz", "mit", "Pfei\u00b7fen", "und", "Gei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Sie haben Augen und sehen's nicht,", "tokens": ["Sie", "ha\u00b7ben", "Au\u00b7gen", "und", "se\u00b7hen's", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "ADV", "PTKNEG", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie prassen fort und lachen,", "tokens": ["Sie", "pras\u00b7sen", "fort", "und", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie h\u00f6ren's nicht, wie zum Gericht", "tokens": ["Sie", "h\u00f6\u00b7ren's", "nicht", ",", "wie", "zum", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Schon Balk' und S\u00e4ule krachen:", "tokens": ["Schon", "Bal\u00b7k'", "und", "S\u00e4u\u00b7le", "kra\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Lauter jauchzt der Geige Ton \u2013", "tokens": ["Lau\u00b7ter", "jauchzt", "der", "Gei\u00b7ge", "Ton", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihr M\u00e4nner, ihr Weiber von Babylon,", "tokens": ["Ihr", "M\u00e4n\u00b7ner", ",", "ihr", "Wei\u00b7ber", "von", "Ba\u00b7by\u00b7lon", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Mene, Tekel, Upharsin!", "tokens": ["Me\u00b7ne", ",", "Te\u00b7kel", ",", "U\u00b7phar\u00b7sin", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}