{"textgrid.poem.37697": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn ich zur\u00fcck im Fenster w\u00e4re!", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich zur\u00fcck im Fenster w\u00e4re!", "tokens": ["Wenn", "ich", "zu\u00b7r\u00fcck", "im", "Fens\u00b7ter", "w\u00e4\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ja w\u00e4re!", "tokens": ["Ja", "w\u00e4\u00b7re", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "VAFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Hier unten ziehet Wind und Regen;", "tokens": ["Hier", "un\u00b7ten", "zie\u00b7het", "Wind", "und", "Re\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mach auf, mach auf! und sprich den Segen,", "tokens": ["Mach", "auf", ",", "mach", "auf", "!", "und", "sprich", "den", "Se\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$.", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bin drau\u00dfen bei der Windm\u00fchl,", "tokens": ["Bin", "drau\u00b7\u00dfen", "bei", "der", "Wind\u00b7m\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo der M\u00fcller mahlt,", "tokens": ["Wo", "der", "M\u00fcl\u00b7ler", "mahlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wenn der Wind geht.", "tokens": ["Wenn", "der", "Wind", "geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Ach w\u00e4r' ich heut nur klug gewesen,", "tokens": ["Ach", "w\u00e4r'", "ich", "heut", "nur", "klug", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gewesen.", "tokens": ["Ge\u00b7we\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ich h\u00e4tte dich in Arm genommen,", "tokens": ["Ich", "h\u00e4t\u00b7te", "dich", "in", "Arm", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So st\u00e4nd' ich nicht so ganz verklommen,", "tokens": ["So", "st\u00e4nd'", "ich", "nicht", "so", "ganz", "ver\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hier drau\u00dfen bei der Windm\u00fchl,", "tokens": ["Hier", "drau\u00b7\u00dfen", "bei", "der", "Wind\u00b7m\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo der M\u00fcller mahlt,", "tokens": ["Wo", "der", "M\u00fcl\u00b7ler", "mahlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wenn der Wind geht.", "tokens": ["Wenn", "der", "Wind", "geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Wenn ich in deinem Herzen st\u00e4nde,", "tokens": ["Wenn", "ich", "in", "dei\u00b7nem", "Her\u00b7zen", "st\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Elende!", "tokens": ["E\u00b7len\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Du w\u00fcrdest nicht das Licht ausmachen,", "tokens": ["Du", "w\u00fcr\u00b7dest", "nicht", "das", "Licht", "aus\u00b7ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und durch die Fensterladen lachen,", "tokens": ["Und", "durch", "die", "Fens\u00b7ter\u00b7la\u00b7den", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und mich hier stehen lassen,", "tokens": ["Und", "mich", "hier", "ste\u00b7hen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo die Zither springt,", "tokens": ["Wo", "die", "Zi\u00b7ther", "springt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und die Z\u00e4hne klappern.", "tokens": ["Und", "die", "Z\u00e4h\u00b7ne", "klap\u00b7pern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "--+---", "measure": "anapaest.init"}}}}}