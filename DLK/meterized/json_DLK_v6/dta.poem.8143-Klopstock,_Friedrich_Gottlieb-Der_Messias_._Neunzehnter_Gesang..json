ord", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVPP", "$,", "APPR", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+--+--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.34": {"text": "Habt der herzlichen, edlen, der frommen Menschlichkeit sanfte", "tokens": ["Habt", "der", "herz\u00b7li\u00b7chen", ",", "ed\u00b7len", ",", "der", "from\u00b7men", "Menschlich\u00b7keit", "sanf\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "$,", "ADJA", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.35": {"text": "Liedest\u00f6ne so oft mit rauhem Klange vermischet!", "tokens": ["Lie\u00b7des\u00b7t\u00f6\u00b7ne", "so", "oft", "mit", "rau\u00b7hem", "Klan\u00b7ge", "ver\u00b7mi\u00b7schet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.36": {"text": "So schien zwar nicht die That, des Gedankens Misbild; so war", "tokens": ["So", "schien", "zwar", "nicht", "die", "That", ",", "des", "Ge\u00b7dan\u00b7kens", "Mis\u00b7bild", ";", "so", "war"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "$,", "ART", "NN", "NN", "$.", "ADV", "VAFIN"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.37": {"text": "Aber das Herz in Verborgnem. Dort war es euch dunkel, der Friede", "tokens": ["A\u00b7ber", "das", "Herz", "in", "Ver\u00b7borg\u00b7nem", ".", "Dort", "war", "es", "euch", "dun\u00b7kel", ",", "der", "Frie\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN", "$.", "ADV", "VAFIN", "PPER", "PPER", "ADJD", "$,", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.38": {"text": "Kam nicht in euer Herz, dem Feinde ganz zu verzeihen,", "tokens": ["Kam", "nicht", "in", "eu\u00b7er", "Herz", ",", "dem", "Fein\u00b7de", "ganz", "zu", "ver\u00b7zei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.39": {"text": "Jhn in Stillem zu segnen!. O durft\u2019 auf die Krone denn hoffen,", "tokens": ["Jhn", "in", "Stil\u00b7lem", "zu", "seg\u00b7nen", "!", ".", "O", "durft'", "auf", "die", "Kro\u00b7ne", "denn", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKZU", "VVINF", "$.", "$.", "NE", "VMFIN", "APPR", "ART", "NN", "KON", "VVINF", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.40": {"text": "Wer nicht rein war vor Gott? so gar vor dem eignen Gef\u00fchl nicht", "tokens": ["Wer", "nicht", "rein", "war", "vor", "Gott", "?", "so", "gar", "vor", "dem", "eig\u00b7nen", "Ge\u00b7f\u00fchl", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "ADJD", "VAFIN", "APPR", "NN", "$.", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "PTKNEG"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.41": {"text": "Rein in der Stunde der Angst; traf\u2019s m\u00e4chtiger ihn, da\u00df er Mensch sey?", "tokens": ["Rein", "in", "der", "Stun\u00b7de", "der", "Angst", ";", "traf's", "m\u00e4ch\u00b7ti\u00b7ger", "ihn", ",", "da\u00df", "er", "Mensch", "sey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$.", "ADJA", "ADJA", "PPER", "$,", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.42": {"text": "Wer sich selber nicht mehr entrann; und dennoch um Gnade", "tokens": ["Wer", "sich", "sel\u00b7ber", "nicht", "mehr", "ent\u00b7rann", ";", "und", "den\u00b7noch", "um", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "ADV", "PTKNEG", "ADV", "VVFIN", "$.", "KON", "ADV", "APPR", "NN"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "Zu dem erhabnen Vers\u00f6hner nicht rief? und dennoch zum Stolze", "tokens": ["Zu", "dem", "er\u00b7hab\u00b7nen", "Ver\u00b7s\u00f6h\u00b7ner", "nicht", "rief", "?", "und", "den\u00b7noch", "zum", "Stol\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKNEG", "VVFIN", "$.", "KON", "ADV", "APPRART", "NN"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.44": {"text": "Wiederkehrte, zur eigenen Gr\u00f6\u00dfe? sich selber vers\u00f6hnte?", "tokens": ["Wie\u00b7der\u00b7kehr\u00b7te", ",", "zur", "ei\u00b7ge\u00b7nen", "Gr\u00f6\u00b7\u00dfe", "?", "sich", "sel\u00b7ber", "ver\u00b7s\u00f6hn\u00b7te", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPRART", "ADJA", "NN", "$.", "PRF", "ADV", "VVFIN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.45": {"text": "Arme Ruhige! S\u00fcnder von S\u00fcndern! der letzte der Tage", "tokens": ["Ar\u00b7me", "Ru\u00b7hi\u00b7ge", "!", "S\u00f