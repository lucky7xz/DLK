, "wer", "Gott", "sey", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "VAFIN", "PRF", "PTKNEG", "ADJD", "$,", "KON", "VVFIN", "$,", "PWS", "NN", "VAFIN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Aber ihr waret euch rein! kaum, da\u00df ihr die gro\u00dfe Vers\u00f6hnung", "tokens": ["A\u00b7ber", "ihr", "wa\u00b7ret", "euch", "rein", "!", "kaum", ",", "da\u00df", "ihr", "die", "gro\u00b7\u00dfe", "Ver\u00b7s\u00f6h\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "$.", "ADV", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "+--+--+-++-+--+-", "measure": "dactylic.di.plus"}, "line.19": {"text": "Auch annahmet. Und dennoch habt ihr die edle Begierde,", "tokens": ["Auch", "an\u00b7nah\u00b7met", ".", "Und", "den\u00b7noch", "habt", "ihr", "die", "ed\u00b7le", "Be\u00b7gier\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "KON", "ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Welche zur Ehr\u2019 euch rief, zum Stolz herunter erniedert!", "tokens": ["Wel\u00b7che", "zur", "Ehr'", "euch", "rief", ",", "zum", "Stolz", "her\u00b7un\u00b7ter", "er\u00b7nie\u00b7dert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPRART", "NN", "PPER", "VVFIN", "$,", "APPRART", "NN", "APZR", "VVPP", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.21": {"text": "Habt es gewagt, wer besser als ihr war, mit Strenge zu richten,", "tokens": ["Habt", "es", "ge\u00b7wagt", ",", "wer", "bes\u00b7ser", "als", "ihr", "war", ",", "mit", "Stren\u00b7ge", "zu", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PWS", "ADJD", "KOKOM", "PPER", "VAFIN", "$,", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.22": {"text": "Wer einf\u00e4ltiger, weiser; und tiefer drang in die Jrre", "tokens": ["Wer", "ein\u00b7f\u00e4l\u00b7ti\u00b7ger", ",", "wei\u00b7ser", ";", "und", "tie\u00b7fer", "drang", "in", "die", "Jr\u00b7re"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "$,", "ADJA", "$.", "KON", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.23": {"text": "Schwerer Pflichten, in sich gesch\u00e4rfter Gef\u00fchl des Guten", "tokens": ["Schwe\u00b7rer", "Pflich\u00b7ten", ",", "in", "sich", "ge\u00b7sch\u00e4rf\u00b7ter", "Ge\u00b7f\u00fchl", "des", "Gu\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "PRF", "ADJA", "NN", "ART", "NN"], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Weckte, die\u00df Feuer n\u00e4hrte; mit Streng\u2019 und Wahne zu richten!", "tokens": ["Weck\u00b7te", ",", "die\u00df", "Feu\u00b7er", "n\u00e4hr\u00b7te", ";", "mit", "Streng'", "und", "Wah\u00b7ne", "zu", "rich\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PDS", "NN", "VVFIN", "$.", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.25": {"text": "Euch unheilig erk\u00fchnt, die Tugend in Staube dem Schalle", "tokens": ["Euch", "un\u00b7hei\u00b7lig", "er\u00b7k\u00fchnt", ",", "die", "Tu\u00b7gend", "in", "Stau\u00b7be", "dem", "Schal\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "VVPP", "$,", "ART", "NN", "APPR", "NN", "ART", "NN"], "meter": "-++--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Jhres Namens, dem Schimmer von ihr in der K\u00f6nige H\u00fctten,", "tokens": ["Ih\u00b7res", "Na\u00b7mens", ",", "dem", "Schim\u00b7mer", "von", "ihr", "in", "der", "K\u00f6\u00b7ni\u00b7ge", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "APPR", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.27": {"text": "Oder auf anderer H\u00f6h der Schattengr\u00f6\u00dfe des Menschen,", "tokens": ["O\u00b7der", "auf", "an\u00b7de\u00b7rer", "H\u00f6h", "der", "Schat\u00b7ten\u00b7gr\u00f6\u00b7\u00dfe", "des", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Gleich zu halten! Jhr bautet euch selbst Gl\u00fcckseligkeiten,", "tokens": ["Gleich", "zu", "hal\u00b7ten", "!", "Ihr", "bau\u00b7tet", "euch", "selbst", "Gl\u00fcck\u00b7se\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$.", "PPER", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "+-+--+---+--+-", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "Tempel eurer Erfindung, auf schmeichelnder Ruhe gegr\u00fcndet,", "tokens": ["Tem\u00b7pel", "eu\u00b7rer", "Er\u00b7fin\u00b7dung", ",", "auf", "schmei\u00b7cheln\u00b7der", "Ru\u00b7he", "ge\u00b7gr\u00fcn\