r", "!", "Er\u00b7hebt", "euch", ",", "und", "seht", "die", "Ru\u00b7hi\u00b7gern", "al\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$.", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ART", "NN", "PIAT", "$."], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.50": {"text": "Schaut nun, welches Ziel ihr verfehltet! Ein anderer Weg gieng", "tokens": ["Schaut", "nun", ",", "wel\u00b7ches", "Ziel", "ihr", "ver\u00b7fehl\u00b7tet", "!", "Ein", "an\u00b7de\u00b7rer", "Weg", "gieng"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "PWAT", "NN", "PPER", "VVPP", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.51": {"text": "Nach dem Ziele. Demuth, mehr Menschlichkeit, hei\u00dfre Gebete", "tokens": ["Nach", "dem", "Zie\u00b7le", ".", "De\u00b7muth", ",", "mehr", "Menschlich\u00b7keit", ",", "hei\u00df\u00b7re", "Ge\u00b7be\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "NN", "$,", "PIAT", "NN", "$,", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.52": {"text": "Haben bis hin zu der Krone den Schritt der Sieger geleitet!", "tokens": ["Ha\u00b7ben", "bis", "hin", "zu", "der", "Kro\u00b7ne", "den", "Schritt", "der", "Sie\u00b7ger", "ge\u00b7lei\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.53": {"text": "Jhr habt niemals, wie sie, in Stunden wacherer N\u00e4chte,", "tokens": ["Ihr", "habt", "nie\u00b7mals", ",", "wie", "sie", ",", "in", "Stun\u00b7den", "wa\u00b7che\u00b7rer", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PWAV", "PPER", "$,", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.54": {"text": "Weinend gerungen in tiefem Gebet! Jhr habet euch niemals", "tokens": ["Wei\u00b7nend", "ge\u00b7run\u00b7gen", "in", "tie\u00b7fem", "Ge\u00b7bet", "!", "Ihr", "ha\u00b7bet", "euch", "nie\u00b7mals"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "APPR", "ADJA", "NN", "$.", "PPER", "VAFIN", "PPER", "ADV"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.55": {"text": "Ganz des Elends erbarmt! Jhr habt die h\u00f6chste der Freuden", "tokens": ["Ganz", "des", "E\u00b7lends", "er\u00b7barmt", "!", "Ihr", "habt", "die", "h\u00f6chs\u00b7te", "der", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVPP", "$.", "PPER", "VAFIN", "ART", "ADJA", "ART", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.56": {"text": "Unter den Freuden der Menschen und Engel niemals empfunden,", "tokens": ["Un\u00b7ter", "den", "Freu\u00b7den", "der", "Men\u00b7schen", "und", "En\u00b7gel", "nie\u00b7mals", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.57": {"text": "Jene Freude, den Seher des Himmels allein zum Zeugen", "tokens": ["Je\u00b7ne", "Freu\u00b7de", ",", "den", "Se\u00b7her", "des", "Him\u00b7mels", "al\u00b7lein", "zum", "Zeu\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "ART", "NN", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.58": {"text": "Unsrer Thaten zu haben, nur Jhn! uns fr\u00f6mmer zu achten,", "tokens": ["Uns\u00b7rer", "Tha\u00b7ten", "zu", "ha\u00b7ben", ",", "nur", "Jhn", "!", "uns", "fr\u00f6m\u00b7mer", "zu", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VAINF", "$,", "ADV", "PPER", "$.", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.59": {"text": "Seliger, wenn den Menschen die That, die wir thaten, verh\u00fcllt war!", "tokens": ["Se\u00b7li\u00b7ger", ",", "wenn", "den", "Men\u00b7schen", "die", "That", ",", "die", "wir", "tha\u00b7ten", ",", "ver\u00b7h\u00fcllt", "war", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVPP", "VAFIN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.60": {"text": "Niemals habt ihr genug des Hocherhabnen, des Ersten,", "tokens": ["Nie\u00b7mals", "habt", "ihr", "ge\u00b7nug", "des", "Ho\u00b7cher\u00b7hab\u00b7nen", ",", "des", "Ers\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.61": {"text": "Gottes Gr\u00f6\u00dfe gekannt! Das ist es, da\u00df ihr von Ruhe", "tokens": ["Got\u00b7tes", "Gr\u00f6\u00b7\u00dfe", "ge\u00b7kannt", "!", "Das", "ist", "es", ",", "da\u00df", "ihr", "von", "Ru\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVPP", "$.", "PDS", "VAFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.62": {"text": "L\u00e4chelnd tr\u00e4umtet; allein bis zu jenem Frieden nicht kamet,", "tokens": ["L\u00e4\u00b7chelnd", "tr\u00e4um\u00b7tet", ";", "al\u00b7lein", "bis", "zu", "je\u00b7nem", "Frie\u00b7den", "nicht", "ka\u00b7met", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$.", "ADV", "ADV", "APPR", "PDAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.63": {"text": "Der in der Thr\u00e4ne des B\u00fcssenden rann, die um Gnade nur flehte,", "tokens": ["Der", "in", "der", "Thr\u00e4\u00b7ne", "des", "B\u00fcs\u00b7sen\u00b7den", "rann", ",", "die", "um", "Gna\u00b7de", "nur", "fleh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,", "PRELS", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.64": {"text": "Nur um Gnade, durch Thr\u00e4nen und Blut des Mittlers erworben!", "tokens": ["Nur", "um", "Gna\u00b7de", ",", "durch", "Thr\u00e4\u00b7nen", "und", "Blut", "des", "Mitt\u00b7lers", "er\u00b7wor\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}}}}}