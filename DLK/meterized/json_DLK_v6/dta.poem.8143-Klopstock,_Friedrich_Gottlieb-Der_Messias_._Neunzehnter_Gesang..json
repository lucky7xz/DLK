{"dta.poem.8143": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Der Messias .  \n Neunzehnter Gesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Einen Anblick des ernsten Gerichts verh\u00fcllte der Menschen", "tokens": ["Ei\u00b7nen", "An\u00b7blick", "des", "erns\u00b7ten", "Ge\u00b7richts", "ver\u00b7h\u00fcll\u00b7te", "der", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Vater durch Schweigen. Er sah, in der Mitte des gro\u00dfen,", "tokens": ["Va\u00b7ter", "durch", "Schwei\u00b7gen", ".", "Er", "sah", ",", "in", "der", "Mit\u00b7te", "des", "gro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "PPER", "VVFIN", "$,", "APPR", "ART", "NN", "ART", "ADJA", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.3": {"text": "gedr\u00e4ngten,", "tokens": ["ge\u00b7dr\u00e4ng\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Unabsehlichen Heers der auferstandenen Todten,", "tokens": ["Un\u00b7ab\u00b7seh\u00b7li\u00b7chen", "Heers", "der", "auf\u00b7er\u00b7stan\u00b7de\u00b7nen", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-++--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Eva auf einem H\u00fcgel stehn, und mit fliegenden Haaren,", "tokens": ["E\u00b7va", "auf", "ei\u00b7nem", "H\u00fc\u00b7gel", "stehn", ",", "und", "mit", "flie\u00b7gen\u00b7den", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Ausgebreiteten Armen, mit gl\u00fchender Wange, mit vollen", "tokens": ["Aus\u00b7ge\u00b7brei\u00b7te\u00b7ten", "Ar\u00b7men", ",", "mit", "gl\u00fc\u00b7hen\u00b7der", "Wan\u00b7ge", ",", "mit", "vol\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.7": {"text": "Innigen T\u00f6nen der Mutterstimme, wie nie noch ein Mensch sie,", "tokens": ["In\u00b7ni\u00b7gen", "T\u00f6\u00b7nen", "der", "Mut\u00b7ter\u00b7stim\u00b7me", ",", "wie", "nie", "noch", "ein", "Mensch", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "$,", "PWAV", "ADV", "ADV", "ART", "NN", "PPER", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Oder ein Engel vernahm, um Gnade! sie l\u00e4chelte weinend,", "tokens": ["O\u00b7der", "ein", "En\u00b7gel", "ver\u00b7nahm", ",", "um", "Gna\u00b7de", "!", "sie", "l\u00e4\u00b7chel\u00b7te", "wei\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "KOUI", "NN", "$.", "PPER", "VVFIN", "VVPP", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.9": {"text": "Flehen f\u00fcr ihre Kinder, um Gnade! zum Richter, um Gnade!", "tokens": ["Fle\u00b7hen", "f\u00fcr", "ih\u00b7re", "Kin\u00b7der", ",", "um", "Gna\u00b7de", "!", "zum", "Rich\u00b7ter", ",", "um", "Gna\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "KOUI", "NN", "$.", "APPRART", "NN", "$,", "KOUI", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Aber auf Einmal verschwand ihm der Schaueranblick; er h\u00f6rte", "tokens": ["A\u00b7ber", "auf", "Ein\u00b7mal", "ver\u00b7schwand", "ihm", "der", "Schau\u00b7e\u00b7ran\u00b7blick", ";", "er", "h\u00f6r\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ADV", "VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "+--+--+--+--+-+-", "measure": "dactylic.tetra.plus"}, "line.11": {"text": "Einigemale nur noch sanft Lispeln der himmlischen Harfen.", "tokens": ["Ei\u00b7ni\u00b7ge\u00b7ma\u00b7le", "nur", "noch", "sanft", "Lis\u00b7peln", "der", "himm\u00b7li\u00b7schen", "Har\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.12": {"text": "Mitleid d\u00e4ucht es ihm bald, bald d\u00e4ucht es ihm Freude, doch jez", "tokens": ["Mit\u00b7leid", "d\u00e4ucht", "es", "ihm", "bald", ",", "bald", "d\u00e4ucht", "es", "ihm", "Freu\u00b7de", ",", "doch", "jez"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "PPER", "NN", "$,", "ADV", "NE"], "meter": "--+--+-+--+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Hatt\u2019 auch die\u00df sich verloren. Er sahe wieder Gesichte.", "tokens": ["Hatt'", "auch", "die\u00df", "sich", "ver\u00b7lo\u00b7ren", ".", "Er", "sa\u00b7he", "wie\u00b7der", "Ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PDS", "PRF", "VVPP", "$.", "PPER", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Als erwach\u2019 er aus tiefen Gedanken, beginnt er von neuem:", "tokens": ["Als", "er\u00b7wach'", "er", "aus", "tie\u00b7fen", "Ge\u00b7dan\u00b7ken", ",", "be\u00b7ginnt", "er", "von", "neu\u00b7em", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,", "VVFIN", "PPER", "APPR", "ADJA", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}}, "stanza.2": {"line.1": {"text": "Jezo sah ich die Schnitter der Erndte die Schaaren hinauf gehn,", "tokens": ["Je\u00b7zo", "sah", "ich", "die", "Schnit\u00b7ter", "der", "Ernd\u00b7te", "die", "Schaa\u00b7ren", "hin\u00b7auf", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Und hinunter. Sie giengen mit scharfer Forschung Gebehrden", "tokens": ["Und", "hin\u00b7un\u00b7ter", ".", "Sie", "gien\u00b7gen", "mit", "schar\u00b7fer", "For\u00b7schung", "Ge\u00b7behr\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Langsam vor\u00fcber, und schauten voll Ernst in die Schaaren und riefen:", "tokens": ["Lang\u00b7sam", "vor\u00b7\u00fc\u00b7ber", ",", "und", "schau\u00b7ten", "voll", "Ernst", "in", "die", "Schaa\u00b7ren", "und", "rie\u00b7fen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "KON", "VVFIN", "ADJD", "NN", "APPR", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Komm!.. Dann f\u00fchreten sie die Gerufnen, wie tr\u00fcbe Gedanken", "tokens": ["Komm", "!", "..", "Dann", "f\u00fch\u00b7re\u00b7ten", "sie", "die", "Ge\u00b7ruf\u00b7nen", ",", "wie", "tr\u00fc\u00b7be", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "ADJA", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Stumm sie alle, wie Bilder an Gr\u00e4bern, als Gr\u00e4ber noch waren,", "tokens": ["Stumm", "sie", "al\u00b7le", ",", "wie", "Bil\u00b7der", "an", "Gr\u00e4\u00b7bern", ",", "als", "Gr\u00e4\u00b7ber", "noch", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PIS", "$,", "PWAV", "NN", "APPR", "NN", "$,", "KOUS", "NN", "ADV", "VAFIN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Auf den Gerichtsplatz hin. Da ward ein Seraph gesendet;", "tokens": ["Auf", "den", "Ge\u00b7richts\u00b7platz", "hin", ".", "Da", "ward", "ein", "Se\u00b7raph", "ge\u00b7sen\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$.", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Der trat langsam hervor, und brachte den hohen Befehl mit:", "tokens": ["Der", "trat", "lang\u00b7sam", "her\u00b7vor", ",", "und", "brach\u00b7te", "den", "ho\u00b7hen", "Be\u00b7fehl", "mit", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Fallt auf das Angesicht nieder, und h\u00f6rt euer Urtheil, das vormals", "tokens": ["Fallt", "auf", "das", "An\u00b7ge\u00b7sicht", "nie\u00b7der", ",", "und", "h\u00f6rt", "eu\u00b7er", "Ur\u00b7theil", ",", "das", "vor\u00b7mals"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In dem Leben der Stunden, allein f\u00fcr sich nur der Fromme", "tokens": ["In", "dem", "Le\u00b7ben", "der", "Stun\u00b7den", ",", "al\u00b7lein", "f\u00fcr", "sich", "nur", "der", "From\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "ADV", "APPR", "PRF", "ADV", "ART", "NN"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Ueber euch sprach; und zitternd sich lehrte, selbst selig zu werden!", "tokens": ["Ue\u00b7ber", "euch", "sprach", ";", "und", "zit\u00b7ternd", "sich", "lehr\u00b7te", ",", "selbst", "se\u00b7lig", "zu", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$.", "KON", "VVFIN", "PRF", "VVFIN", "$,", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+--+-+--+-+-+-+-", "measure": "iambic.septa.invert"}}, "stanza.4": {"line.1": {"text": "Und ich sah sie erblassen, und niederfallen zur Erde!", "tokens": ["Und", "ich", "sah", "sie", "er\u00b7blas\u00b7sen", ",", "und", "nie\u00b7der\u00b7fal\u00b7len", "zur", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVINF", "$,", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und sie lagen, und hielten zertr\u00fcmmerte Felsen. Der Seraph", "tokens": ["Und", "sie", "la\u00b7gen", ",", "und", "hiel\u00b7ten", "zer\u00b7tr\u00fcm\u00b7mer\u00b7te", "Fel\u00b7sen", ".", "Der", "Se\u00b7raph"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KON", "VVFIN", "ADJA", "NN", "$.", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Trat stillschweigend zur\u00fcck. Jm Glanze der reineren Tugend,", "tokens": ["Trat", "still\u00b7schwei\u00b7gend", "zu\u00b7r\u00fcck", ".", "Jm", "Glan\u00b7ze", "der", "rei\u00b7ne\u00b7ren", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$.", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mit der Hoheit der Religion, die er dr\u00fcben am Grabe", "tokens": ["Mit", "der", "Ho\u00b7heit", "der", "Re\u00b7li\u00b7gi\u00b7on", ",", "die", "er", "dr\u00fc\u00b7ben", "am", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "APPRART", "NN"], "meter": "--+--+-+-+-+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Schon in ihrer G\u00f6ttlichkeit sah, erhub sich der beste,", "tokens": ["Schon", "in", "ih\u00b7rer", "G\u00f6tt\u00b7lich\u00b7keit", "sah", ",", "er\u00b7hub", "sich", "der", "bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PRF", "ART", "ADJA", "$,"], "meter": "--+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und der liebensw\u00fcrdigste J\u00fcnger, der fromme Johannes.", "tokens": ["Und", "der", "lie\u00b7bens\u00b7w\u00fcr\u00b7digs\u00b7te", "J\u00fcn\u00b7ger", ",", "der", "from\u00b7me", "Jo\u00b7han\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NE", "$."], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und die Aeltesten standen um ihn. Er erhub sich, die Stolzen,", "tokens": ["Und", "die", "A\u00b7el\u00b7tes\u00b7ten", "stan\u00b7den", "um", "ihn", ".", "Er", "er\u00b7hub", "sich", ",", "die", "Stol\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPER", "$.", "PPER", "VVFIN", "PRF", "$,", "ART", "NN", "$,"], "meter": "---+--+---+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Welche zur Erde niedergesunken auf dem Gerichtsplatz", "tokens": ["Wel\u00b7che", "zur", "Er\u00b7de", "nie\u00b7der\u00b7ge\u00b7sun\u00b7ken", "auf", "dem", "Ge\u00b7richts\u00b7platz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "APPRART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.9": {"text": "Lagen, die zu enth\u00fcllen; ihr Thun dem Tage zu zeigen!", "tokens": ["La\u00b7gen", ",", "die", "zu", "ent\u00b7h\u00fcl\u00b7len", ";", "ihr", "Thun", "dem", "Ta\u00b7ge", "zu", "zei\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PTKZU", "VVINF", "$.", "PPOSAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Gleich dem Wetter des M\u00e4chtigen, traf er nicht jede der Tiefen,", "tokens": ["Gleich", "dem", "Wet\u00b7ter", "des", "M\u00e4ch\u00b7ti\u00b7gen", ",", "traf", "er", "nicht", "je\u00b7de", "der", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "PIAT", "ART", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.11": {"text": "Jede H\u00f6h nicht; ber\u00fchrete nur hier Gipfel, dort Abgrund;", "tokens": ["Je\u00b7de", "H\u00f6h", "nicht", ";", "be\u00b7r\u00fch\u00b7re\u00b7te", "nur", "hier", "Gip\u00b7fel", ",", "dort", "Ab\u00b7grund", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKNEG", "$.", "VVFIN", "ADV", "ADV", "NN", "$,", "ADV", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Lie\u00df dann schweigen die schreckende Wolke. So sprach er: Jhr schuft euch", "tokens": ["Lie\u00df", "dann", "schwei\u00b7gen", "die", "schre\u00b7cken\u00b7de", "Wol\u00b7ke", ".", "So", "sprach", "er", ":", "Ihr", "schuft", "euch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.13": {"text": "Eigne Tugend, und stelltet dem Abgott \u00fcber den Thron hin,", "tokens": ["Eig\u00b7ne", "Tu\u00b7gend", ",", "und", "stell\u00b7tet", "dem", "Ab\u0