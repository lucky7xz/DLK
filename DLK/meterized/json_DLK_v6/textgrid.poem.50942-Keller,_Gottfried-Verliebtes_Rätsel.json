{"textgrid.poem.50942": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Verliebtes R\u00e4tsel", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gef\u00e4chelt von der L\u00fcfte Schwingen,", "tokens": ["Ge\u00b7f\u00e4\u00b7chelt", "von", "der", "L\u00fcf\u00b7te", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zeigt's deiner Lippen hohe Rosenglut", "tokens": ["Zeigt's", "dei\u00b7ner", "Lip\u00b7pen", "ho\u00b7he", "Ro\u00b7seng\u00b7lut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und knistert leis, wie deine Lippen singen,", "tokens": ["Und", "knis\u00b7tert", "leis", ",", "wie", "dei\u00b7ne", "Lip\u00b7pen", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn ein geheimer Traum bewegt dein Blut.", "tokens": ["Wenn", "ein", "ge\u00b7hei\u00b7mer", "Traum", "be\u00b7wegt", "dein", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun schweigt das Knistern, stirbt die R\u00f6te,", "tokens": ["Nun", "schweigt", "das", "Knis\u00b7tern", ",", "stirbt", "die", "R\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In tiefe Nacht versinkt der F\u00fcnklein Tanz;", "tokens": ["In", "tie\u00b7fe", "Nacht", "ver\u00b7sinkt", "der", "F\u00fcn\u00b7klein", "Tanz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nun ist es tot und schwarz; was \u00fcberb\u00f6te", "tokens": ["Nun", "ist", "es", "tot", "und", "schwarz", ";", "was", "\u00fc\u00b7ber\u00b7b\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$.", "PWS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Schw\u00e4rze als dein Haar im Morgenglanz?", "tokens": ["Die", "Schw\u00e4r\u00b7ze", "als", "dein", "Haar", "im", "Mor\u00b7gen\u00b7glanz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Noch warm, nehm ich die zarte Leiche", "tokens": ["Noch", "warm", ",", "nehm", "ich", "die", "zar\u00b7te", "Lei\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schreib auf deines Flurs besonnten Stein", "tokens": ["Und", "schreib", "auf", "dei\u00b7nes", "Flurs", "be\u00b7sonn\u00b7ten", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr art'ges Leben, dem das deine gleiche,", "tokens": ["Ihr", "art'\u00b7ges", "Le\u00b7ben", ",", "dem", "das", "dei\u00b7ne", "glei\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "ART", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So hoch ergl\u00fchend und so schlicht und rein:", "tokens": ["So", "hoch", "er\u00b7gl\u00fc\u00b7hend", "und", "so", "schlicht", "und", "rein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "KON", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbich war ein B\u00e4umlein auf den Rainen,", "tokens": ["\u00bb", "ich", "war", "ein", "B\u00e4um\u00b7lein", "auf", "den", "Rai\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Mark war weich und wei\u00df, die Bl\u00e4ttlein gr\u00fcn,", "tokens": ["Mein", "Mark", "war", "weich", "und", "wei\u00df", ",", "die", "Bl\u00e4tt\u00b7lein", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich sah die Sonne feurig niederscheinen,", "tokens": ["Ich", "sah", "die", "Son\u00b7ne", "feu\u00b7rig", "nie\u00b7der\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann brannt ich selber, selig im Vergl\u00fchn.", "tokens": ["Dann", "brannt", "ich", "sel\u00b7ber", ",", "se\u00b7lig", "im", "Ver\u00b7gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was von mir blieb, zeigt noch die Triebe", "tokens": ["Was", "von", "mir", "blieb", ",", "zeigt", "noch", "die", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Adern und der Jahresringe Lauf;", "tokens": ["Der", "A\u00b7dern", "und", "der", "Jah\u00b7res\u00b7rin\u00b7ge", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schreib froh mit mir, Poet! den Preis der Liebe", "tokens": ["Schreib", "froh", "mit", "mir", ",", "Po\u00b7et", "!", "den", "Preis", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "PPER", "$,", "NN", "$.", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und brauch mich ganz zu deinem Liede auf!\u00ab", "tokens": ["Und", "brauch", "mich", "ganz", "zu", "dei\u00b7nem", "Lie\u00b7de", "auf", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}