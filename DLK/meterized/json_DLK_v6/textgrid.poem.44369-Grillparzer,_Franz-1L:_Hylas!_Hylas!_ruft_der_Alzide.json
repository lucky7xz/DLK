{"textgrid.poem.44369": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hylas! Hylas! ruft der Alzide", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hylas! Hylas! ruft der Alzide", "tokens": ["Hy\u00b7las", "!", "Hy\u00b7las", "!", "ruft", "der", "Al\u00b7zi\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "NE", "$.", "VVFIN", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Laut an Mysias Felsengestad,", "tokens": ["Laut", "an", "My\u00b7si\u00b7as", "Fel\u00b7sen\u00b7ge\u00b7stad", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NE", "NE", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ob schon wankend und wegesm\u00fcde,", "tokens": ["Ob", "schon", "wan\u00b7kend", "und", "we\u00b7ges\u00b7m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "KON", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Klimmt er hinan den steinigten Pfad.", "tokens": ["Klimmt", "er", "hi\u00b7nan", "den", "stei\u00b7nig\u00b7ten", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Den seine Brust zum Liebling erkoren,", "tokens": ["Den", "sei\u00b7ne", "Brust", "zum", "Lieb\u00b7ling", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Hylas, den sch\u00f6nen, hat er verloren;", "tokens": ["Hy\u00b7las", ",", "den", "sch\u00f6\u00b7nen", ",", "hat", "er", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und schon die Nacht, die verh\u00fcllende, naht.", "tokens": ["Und", "schon", "die", "Nacht", ",", "die", "ver\u00b7h\u00fcl\u00b7len\u00b7de", ",", "naht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Suchend nach Wasser, ging er, der Knabe,", "tokens": ["Su\u00b7chend", "nach", "Was\u00b7ser", ",", "ging", "er", ",", "der", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mit dem Krug auf dem lockigen Haupt,", "tokens": ["Mit", "dem", "Krug", "auf", "dem", "lo\u00b7cki\u00b7gen", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Sich und dem durstenden Freund zur Labe.", "tokens": ["Sich", "und", "dem", "durs\u00b7ten\u00b7den", "Freund", "zur", "La\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KON", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch durch die Pfade, waldigt umlaubt,", "tokens": ["Doch", "durch", "die", "Pfa\u00b7de", ",", "wal\u00b7digt", "um\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "War er gegangen und nicht mehr gekommen,", "tokens": ["War", "er", "ge\u00b7gan\u00b7gen", "und", "nicht", "mehr", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "KON", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dunkel nur ward die Sage vernommen,", "tokens": ["Dun\u00b7kel", "nur", "ward", "die", "Sa\u00b7ge", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-++-+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Da\u00df ihn die Nymphen, den Knaben, geraubt.", "tokens": ["Da\u00df", "ihn", "die", "Nym\u00b7phen", ",", "den", "Kna\u00b7ben", ",", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Denn, als den Krug in emsigen Handen,", "tokens": ["Denn", ",", "als", "den", "Krug", "in", "em\u00b7si\u00b7gen", "Han\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "\u00dcbergebeugt in den spiegelnden See,", "tokens": ["\u00dc\u00b7ber\u00b7ge\u00b7beugt", "in", "den", "spie\u00b7geln\u00b7den", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Er am Ufer sch\u00f6pfend gestanden,", "tokens": ["Er", "am", "U\u00b7fer", "sch\u00f6p\u00b7fend", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hab es gequollen vom Grund in die H\u00f6h \u2013", "tokens": ["Hab", "es", "ge\u00b7quol\u00b7len", "vom", "Grund", "in", "die", "H\u00f6h", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "APPRART", "NN", "APPR", "ART", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Gl\u00e4nzende Stirn und Augen und Wangen", "tokens": ["Gl\u00e4n\u00b7zen\u00b7de", "Stirn", "und", "Au\u00b7gen", "und", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "KON", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und zwei H\u00e4nde, von denen umfangen,", "tokens": ["Und", "zwei", "H\u00e4n\u00b7de", ",", "von", "de\u00b7nen", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "$,", "APPR", "PRELS", "VVPP", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Hylas versank in dem wallenden See.", "tokens": ["Hy\u00b7las", "ver\u00b7sank", "in", "dem", "wal\u00b7len\u00b7den", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Solches, von zagenden Hirten erz\u00e4hlet,", "tokens": ["Sol\u00b7ches", ",", "von", "za\u00b7gen\u00b7den", "Hir\u00b7ten", "er\u00b7z\u00e4h\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "H\u00f6rt des Herakles heilige Macht,", "tokens": ["H\u00f6rt", "des", "Her\u00b7ak\u00b7les", "hei\u00b7li\u00b7ge", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und, von Zorn die Sehnen gest\u00e4hlet,", "tokens": ["Und", ",", "von", "Zorn", "die", "Seh\u00b7nen", "ge\u00b7st\u00e4h\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dringt er durch Klippen und Waldesnacht.", "tokens": ["Dringt", "er", "durch", "Klip\u00b7pen", "und", "Wal\u00b7des\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Recht hat die schwankende Kunde geleitet,", "tokens": ["Recht", "hat", "die", "schwan\u00b7ken\u00b7de", "Kun\u00b7de", "ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Siehe, schon liegt weithin verbreitet", "tokens": ["Sie\u00b7he", ",", "schon", "liegt", "weit\u00b7hin", "ver\u00b7brei\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vor ihm der See in ruhiger Pracht.", "tokens": ["Vor", "ihm", "der", "See", "in", "ru\u00b7hi\u00b7ger", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-++-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Hin ans Ufer tritt er im Grimme", "tokens": ["Hin", "ans", "U\u00b7fer", "tritt", "er", "im", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und schreit hinaus in die neblichte Luft:", "tokens": ["Und", "schreit", "hin\u00b7aus", "in", "die", "neb\u00b7lich\u00b7te", "Luft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APZR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbhylas! H\u00f6re des Freundes Stimme!", "tokens": ["\u00bb", "hy\u00b7las", "!", "H\u00f6\u00b7re", "des", "Freun\u00b7des", "Stim\u00b7me", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "NE", "ART", "NN", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Komm! \u2013 Und auch ihr, die in felsiger Kluft", "tokens": ["Komm", "!", "\u2013", "Und", "auch", "ihr", ",", "die", "in", "fel\u00b7si\u00b7ger", "Kluft"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "$(", "KON", "ADV", "PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Ihr euch verme\u00dft, den Geliebten zu halten,", "tokens": ["Ihr", "euch", "ver\u00b7me\u00dft", ",", "den", "Ge\u00b7lieb\u00b7ten", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "F\u00fcrchtet des Donnerers h\u00f6chste Gewalten,", "tokens": ["F\u00fcrch\u00b7tet", "des", "Don\u00b7ne\u00b7rers", "h\u00f6chs\u00b7te", "Ge\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "Denn sein Erzeugter ists, der zu euch ruft!\u00ab", "tokens": ["Denn", "sein", "Er\u00b7zeug\u00b7ter", "ists", ",", "der", "zu", "euch", "ruft", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}