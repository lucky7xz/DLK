{"textgrid.poem.58786": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "5. Nichts", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nichts haben, als nur Gott in allen,", "tokens": ["Nichts", "ha\u00b7ben", ",", "als", "nur", "Gott", "in", "al\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "KOUS", "ADV", "NN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nichts wollen, als nur ihm gefallen,", "tokens": ["Nichts", "wol\u00b7len", ",", "als", "nur", "ihm", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "$,", "KOUS", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts k\u00f6nnen in dir selber mehr,", "tokens": ["Nichts", "k\u00f6n\u00b7nen", "in", "dir", "sel\u00b7ber", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts sein: dies ist die h\u00f6chste Lehr.", "tokens": ["Nichts", "sein", ":", "dies", "ist", "die", "h\u00f6chs\u00b7te", "Lehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAINF", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}