{"dta.poem.19637": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Abendreihen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie steht ihr allhie und wartet mein,               ", "tokens": ["Wie", "steht", "ihr", "all\u00b7hie", "und", "war\u00b7tet", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "PPOSAT", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und meint, ich soll eure Vorsingerin seyn,", "tokens": ["Und", "meint", ",", "ich", "soll", "eu\u00b7re", "Vor\u00b7sin\u00b7ge\u00b7rin", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Soll ich denn nun singen, so h\u00f6ret mir zu,", "tokens": ["Soll", "ich", "denn", "nun", "sin\u00b7gen", ",", "so", "h\u00f6\u00b7ret", "mir", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Im Gesetz ist weder Rast noch Ruh.", "tokens": ["Im", "Ge\u00b7setz", "ist", "we\u00b7der", "Rast", "noch", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "KON", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Das Gesetz richtet nichts denn Zoren an,", "tokens": ["Das", "Ge\u00b7setz", "rich\u00b7tet", "nichts", "denn", "Zo\u00b7ren", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und kein Mensch lebet, der es halten kann,", "tokens": ["Und", "kein", "Mensch", "le\u00b7bet", ",", "der", "es", "hal\u00b7ten", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Nun mu\u00df es dennoch erf\u00fcllet seyn,", "tokens": ["Nun", "mu\u00df", "es", "den\u00b7noch", "er\u00b7f\u00fcl\u00b7let", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Darum schickt Gott seinen Sohn herein.", "tokens": ["Da\u00b7rum", "schickt", "Gott", "sei\u00b7nen", "Sohn", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Derselbig ist worden unser Schild,", "tokens": ["Der\u00b7sel\u00b7big", "ist", "wor\u00b7den", "un\u00b7ser", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VAPP", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er hat des Vaters gro\u00dfen Zorn gestillt,", "tokens": ["Er", "hat", "des", "Va\u00b7ters", "gro\u00b7\u00dfen", "Zorn", "ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn er hat dem Gesetz genug gethan,", "tokens": ["Denn", "er", "hat", "dem", "Ge\u00b7setz", "ge\u00b7nug", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "F\u00fcr jedermann, der nur glauben kann.", "tokens": ["F\u00fcr", "je\u00b7der\u00b7mann", ",", "der", "nur", "glau\u00b7ben", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Es hat ihn kostet sein rosenfarbig Blut,", "tokens": ["Es", "hat", "ihn", "kos\u00b7tet", "sein", "ro\u00b7sen\u00b7far\u00b7big", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Am Kreutz trug er alles uns zu gut,", "tokens": ["Am", "Kreutz", "trug", "er", "al\u00b7les", "uns", "zu", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PIS", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Des saget Lob und Dank in Ewigkeit,", "tokens": ["Des", "sa\u00b7get", "Lob", "und", "Dank", "in", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df er uns beh\u00fct vor allem Leid. Amen.", "tokens": ["Da\u00df", "er", "uns", "be\u00b7h\u00fct", "vor", "al\u00b7lem", "Leid", ".", "A\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "PIS", "NN", "$.", "NN", "$."], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}}}}}