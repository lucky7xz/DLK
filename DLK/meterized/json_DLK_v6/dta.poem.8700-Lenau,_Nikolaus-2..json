{"dta.poem.8700": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Schon zerflie\u00dft das ferne Gebirg mit Wolken", "tokens": ["Schon", "zer\u00b7flie\u00dft", "das", "fer\u00b7ne", "Ge\u00b7birg", "mit", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "In ein Meer; den Wogen entsteigt der Mond, er", "tokens": ["In", "ein", "Meer", ";", "den", "Wo\u00b7gen", "ent\u00b7steigt", "der", "Mond", ",", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$,", "PPER"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Gr\u00fc\u00dft die Flur, entgegen ihm gr\u00fc\u00dft das sch\u00f6nste", "tokens": ["Gr\u00fc\u00dft", "die", "Flur", ",", "ent\u00b7ge\u00b7gen", "ihm", "gr\u00fc\u00dft", "das", "sch\u00f6ns\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "APPR", "PPER", "VVFIN", "ART", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Lied Filomelens", "tokens": ["Lied", "Fi\u00b7lo\u00b7me\u00b7lens"], "token_info": ["word", "word"], "pos": ["NN", "NE"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Aus dem Bl\u00fcthenstrauche, der um das Pl\u00e4tzchen", "tokens": ["Aus", "dem", "Bl\u00fc\u00b7then\u00b7strau\u00b7che", ",", "der", "um", "das", "Pl\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "--+-+----+-", "measure": "anapaest.init"}, "line.2": {"text": "Zarter Liebe heimlichend sich verschlinget,", "tokens": ["Zar\u00b7ter", "Lie\u00b7be", "heim\u00b7li\u00b7chend", "sich", "ver\u00b7schlin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PRF", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Mirzi horcht am Busen des J\u00fcnglings ihrem", "tokens": ["Mir\u00b7zi", "horcht", "am", "Bu\u00b7sen", "des", "J\u00fcng\u00b7lings", "ih\u00b7rem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN", "ART", "NN", "PPOSAT"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Zaubergefl\u00f6te.", "tokens": ["Zau\u00b7ber\u00b7ge\u00b7fl\u00f6\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Dort am H\u00fcgel weiden die Schafe Beider", "tokens": ["Dort", "am", "H\u00fc\u00b7gel", "wei\u00b7den", "die", "Scha\u00b7fe", "Bei\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Traulichen Gemenges in ", "tokens": ["Trau\u00b7li\u00b7chen", "Ge\u00b7men\u00b7ges", "in"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Gl\u00f6cklein stimmen so lieblich ein zu", "tokens": ["Ih\u00b7re", "Gl\u00f6c\u00b7klein", "stim\u00b7men", "so", "lieb\u00b7lich", "ein", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "ART", "PTKZU"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Frohen Akkorden.", "tokens": ["Fro\u00b7hen", "Ak\u00b7kor\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}