{"textgrid.poem.40887": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Der Schatz zu Burghausen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herzog J\u00fcrg von Baiern lag auf der Todtenbahr',", "tokens": ["Der", "Her\u00b7zog", "J\u00fcrg", "von", "Bai\u00b7ern", "lag", "auf", "der", "Tod\u00b7ten\u00b7bahr'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "APPR", "NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Kein F\u00fcrstenhut lag h\u00f6hnend auf seinem greisen Haar,", "tokens": ["Kein", "F\u00fcrs\u00b7ten\u00b7hut", "lag", "h\u00f6h\u00b7nend", "auf", "sei\u00b7nem", "grei\u00b7sen", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Kein Sohn hat segenflehend dem Kranken ins Aug' geblickt,", "tokens": ["Kein", "Sohn", "hat", "se\u00b7gen\u00b7fle\u00b7hend", "dem", "Kran\u00b7ken", "ins", "Aug'", "ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Kein treues Weib dem Todten die Wimpern zugedr\u00fcckt.", "tokens": ["Kein", "treu\u00b7es", "Weib", "dem", "Tod\u00b7ten", "die", "Wim\u00b7pern", "zu\u00b7ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Wem sollen nun die Lande die Huldigung erneun?", "tokens": ["Wem", "sol\u00b7len", "nun", "die", "Lan\u00b7de", "die", "Hul\u00b7di\u00b7gung", "er\u00b7neun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Wer wird sich zu Burghausen des F\u00fcrstenschatzes freun?", "tokens": ["Wer", "wird", "sich", "zu", "Burg\u00b7hau\u00b7sen", "des", "F\u00fcrs\u00b7ten\u00b7schat\u00b7zes", "freun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PRF", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Horch, Schild und Schwerter rasseln! Ist das sein Todtensang?", "tokens": ["Horch", ",", "Schild", "und", "Schwer\u00b7ter", "ras\u00b7seln", "!", "Ist", "das", "sein", "Tod\u00b7ten\u00b7sang", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "VVINF", "$.", "VAFIN", "PDS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Seht, erzgewappnete Schaaren! Ist's der Leidtr\u00e4ger Drang?", "tokens": ["Seht", ",", "erz\u00b7ge\u00b7wapp\u00b7ne\u00b7te", "Schaa\u00b7ren", "!", "Ist's", "der", "Leid\u00b7tr\u00e4\u00b7ger", "Drang", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$.", "NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-++-+", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Albrecht von Baiern fa\u00dfte des Todten F\u00fcrstenhut:", "tokens": ["Al\u00b7brecht", "von", "Bai\u00b7ern", "fa\u00df\u00b7te", "des", "Tod\u00b7ten", "F\u00fcrs\u00b7ten\u00b7hut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbso war's wohl auch sein Wille! Wem st\u00fcnd' er auch so gut?\u00ab", "tokens": ["\u00bb", "so", "wa\u00b7r's", "wohl", "auch", "sein", "Wil\u00b7le", "!", "Wem", "st\u00fcnd'", "er", "auch", "so", "gut", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$.", "PWS", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$.", "$("], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Rupprecht der junge Pfalzgraf st\u00fcrmt gen Burghausen an:", "tokens": ["Rup\u00b7precht", "der", "jun\u00b7ge", "Pfalz\u00b7graf", "st\u00fcrmt", "gen", "Burg\u00b7hau\u00b7sen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "APPR", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "\u00bbund hab' ich nur den Pelz erst, \u2013 hol' ich den Hut auch dann.\u00ab", "tokens": ["\u00bb", "und", "hab'", "ich", "nur", "den", "Pelz", "erst", ",", "\u2013", "hol'", "ich", "den", "Hut", "auch", "dann", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "$,", "$(", "VVIMP", "PPER", "ART", "NN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch K\u00f6nig Maxens Herold, der rief den Streitern zu:", "tokens": ["Doch", "K\u00f6\u00b7nig", "Ma\u00b7xens", "He\u00b7rold", ",", "der", "rief", "den", "Strei\u00b7tern", "zu", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "NE", "$,", "PRELS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bblegt nieder eure Waffen! St\u00f6rt nicht des Todten Ruh!", "tokens": ["\u00bb", "legt", "nie\u00b7der", "eu\u00b7re", "Waf\u00b7fen", "!", "St\u00f6rt", "nicht", "des", "Tod\u00b7ten", "Ruh", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "PPOSAT", "NN", "$.", "NN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nicht gab die Friedenssatzung zum Spielball ich dem Reich,", "tokens": ["Nicht", "gab", "die", "Frie\u00b7dens\u00b7sat\u00b7zung", "zum", "Spiel\u00b7ball", "ich", "dem", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "APPRART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Drum ruf' ich vor den Thron euch zum friedlichen Vergleich!\u00ab", "tokens": ["Drum", "ruf'", "ich", "vor", "den", "Thron", "euch", "zum", "fried\u00b7li\u00b7chen", "Ver\u00b7gleich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "NN", "PPER", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Zu Maxens F\u00fc\u00dfen senkte Albrecht den F\u00fcrstenhut:", "tokens": ["Zu", "Ma\u00b7xens", "F\u00fc\u00b7\u00dfen", "senk\u00b7te", "Al\u00b7brecht", "den", "F\u00fcrs\u00b7ten\u00b7hut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "NE", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbvor euren Thron, mein Richter, leg' ich mein Recht und Gut.\u00ab", "tokens": ["\u00bb", "vor", "eu\u00b7ren", "Thron", ",", "mein", "Rich\u00b7ter", ",", "leg'", "ich", "mein", "Recht", "und", "Gut", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch Rupprecht zu Burghausen lacht in den Bart hinein:", "tokens": ["Doch", "Rup\u00b7precht", "zu", "Burg\u00b7hau\u00b7sen", "lacht", "in", "den", "Bart", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--++-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "\u00bbla\u00df doch die eitlen Schw\u00e4nke, du armes K\u00f6niglein!\u00ab", "tokens": ["\u00bb", "la\u00df", "doch", "die", "eit\u00b7len", "Schw\u00e4n\u00b7ke", ",", "du", "ar\u00b7mes", "K\u00f6\u00b7nig\u00b7lein", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "ADV", "ART", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$.", "$("], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Im Schatze zu Burghausen steht g\u00fclden Schrein an Schrein,", "tokens": ["Im", "Schat\u00b7ze", "zu", "Burg\u00b7hau\u00b7sen", "steht", "g\u00fcl\u00b7den", "Schrein", "an", "Schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVFIN", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+--++-++-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Drin blitzen Goldgeschmeide und farbig Edelgestein,", "tokens": ["Drin", "blit\u00b7zen", "Gold\u00b7ge\u00b7schmei\u00b7de", "und", "far\u00b7big", "E\u00b7del\u00b7ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und rings aus lautrem Silber steht, gleichsam wie zur Wacht,", "tokens": ["Und", "rings", "aus", "laut\u00b7rem", "Sil\u00b7ber", "steht", ",", "gleich\u00b7sam", "wie", "zur", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,", "ADJD", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Chor der zw\u00f6lf Apostel in riesenhafter Pracht.", "tokens": ["Der", "Chor", "der", "zw\u00f6lf", "A\u00b7pos\u00b7tel", "in", "rie\u00b7sen\u00b7haf\u00b7ter", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "CARD", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbwillkommen, ihr edlen Herren!\u00ab sprach zu dem Chor Rupprecht,", "tokens": ["\u00bb", "will\u00b7kom\u00b7men", ",", "ihr", "ed\u00b7len", "Her\u00b7ren", "!", "\u00ab", "sprach", "zu", "dem", "Chor", "Rup\u00b7precht", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$.", "$(", "VVFIN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+--+-+----+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u00bbdoch d\u00e4ucht mir, eure Sendung erf\u00fcllt ihr ziemlich schlecht;", "tokens": ["\u00bb", "doch", "d\u00e4ucht", "mir", ",", "eu\u00b7re", "Sen\u00b7dung", "er\u00b7f\u00fcllt", "ihr", "ziem\u00b7lich", "schlecht", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Herr gebot euch: Ziehet in alle Welt hinaus!", "tokens": ["Der", "Herr", "ge\u00b7bot", "euch", ":", "Zie\u00b7het", "in", "al\u00b7le", "Welt", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ihr aber h\u00fctet seit Jahren gem\u00e4chlich schon das Haus.", "tokens": ["Ihr", "a\u00b7ber", "h\u00fc\u00b7tet", "seit", "Jah\u00b7ren", "ge\u00b7m\u00e4ch\u00b7lich", "schon", "das", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "NN", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Drum will ich jetzt euch senden, treu eures Meisters Wort,", "tokens": ["Drum", "will", "ich", "jetzt", "euch", "sen\u00b7den", ",", "treu", "eu\u00b7res", "Meis\u00b7ters", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,", "ADJD", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.2": {"text": "Zu pilgern und zu pred'gen hinaus nach S\u00fcd und Nord!\u00ab", "tokens": ["Zu", "pil\u00b7gern", "und", "zu", "pre\u00b7d'\u00b7gen", "hin\u00b7aus", "nach", "S\u00fcd", "und", "Nord", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "KON", "APPR", "NN", "APZR", "APPR", "NN", "KON", "NE", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Zum zweiten M\u00e4rtyrtode, in flackernd Flammengebraus", "tokens": ["Zum", "zwei\u00b7ten", "M\u00e4r\u00b7tyr\u00b7to\u00b7de", ",", "in", "fla\u00b7ckernd", "Flam\u00b7men\u00b7ge\u00b7braus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ADJD", "NN"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Lie\u00df er die Zw\u00f6lfe werfen und pr\u00e4gte M\u00fcnzen draus.", "tokens": ["Lie\u00df", "er", "die", "Zw\u00f6l\u00b7fe", "wer\u00b7fen", "und", "pr\u00e4g\u00b7te", "M\u00fcn\u00b7zen", "draus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Er sandte dann die Blanken hinaus in alle Ferne,", "tokens": ["Er", "sand\u00b7te", "dann", "die", "Blan\u00b7ken", "hin\u00b7aus", "in", "al\u00b7le", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APZR", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ei, wie sie kr\u00e4ftig pred'gen! Wie h\u00f6rt man sie so gerne!", "tokens": ["Ei", ",", "wie", "sie", "kr\u00e4f\u00b7tig", "pre\u00b7d'\u00b7gen", "!", "Wie", "h\u00f6rt", "man", "sie", "so", "ger\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$.", "PWAV", "VVFIN", "PIS", "PPER", "ADV", "ADV", "$."], "meter": "+--+-++--+-+-+-", "measure": "iambic.septa.invert"}, "line.3": {"text": "Als sie an Kuffsteins Pforte nur leise pochten an,", "tokens": ["Als", "sie", "an", "Kuffs\u00b7teins", "Pfor\u00b7te", "nur", "lei\u00b7se", "poch\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "NN", "ADV", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Gleich hatte Pinzenauer sie gastlich aufgethan.", "tokens": ["Gleich", "hat\u00b7te", "Pin\u00b7ze\u00b7nau\u00b7er", "sie", "gast\u00b7lich", "auf\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Der Veste goldne Schl\u00fcssel sandt' er Herrn Rupprecht dar:", "tokens": ["Der", "Ves\u00b7te", "gold\u00b7ne", "Schl\u00fcs\u00b7sel", "sandt'", "er", "Herrn", "Rup\u00b7precht", "dar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "PPER", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbmein F\u00fcrst, ihr seid mir wahrlich ein Schl\u00f6sser wunderbar!\u00ab", "tokens": ["\u00bb", "mein", "F\u00fcrst", ",", "ihr", "seid", "mir", "wahr\u00b7