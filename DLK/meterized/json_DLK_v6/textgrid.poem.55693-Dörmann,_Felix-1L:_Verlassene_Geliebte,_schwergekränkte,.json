{"textgrid.poem.55693": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verlassene Geliebte, schwergekr\u00e4nkte,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verlassene Geliebte, schwergekr\u00e4nkte,", "tokens": ["Ver\u00b7las\u00b7se\u00b7ne", "Ge\u00b7lieb\u00b7te", ",", "schwer\u00b7ge\u00b7kr\u00e4nk\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch einmal gib mir freundlich Deine Hand,", "tokens": ["Noch", "ein\u00b7mal", "gib", "mir", "freund\u00b7lich", "Dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVIMP", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die mir Dein gro\u00dfes Herz in Tagen schenkte,", "tokens": ["Die", "mir", "Dein", "gro\u00b7\u00dfes", "Herz", "in", "Ta\u00b7gen", "schenk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Du noch viel zu wenig mich gekannt.", "tokens": ["Wo", "Du", "noch", "viel", "zu", "we\u00b7nig", "mich", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "PTKA", "PIS", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein hei\u00dfes Lieben und ein hei\u00dfes Leiden", "tokens": ["Ein", "hei\u00b7\u00dfes", "Lie\u00b7ben", "und", "ein", "hei\u00b7\u00dfes", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat unser beider Herzen durchgew\u00fchlt,", "tokens": ["Hat", "un\u00b7ser", "bei\u00b7der", "Her\u00b7zen", "durch\u00b7ge\u00b7w\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sekundenlanges Finden, bittres Scheiden,", "tokens": ["Se\u00b7kun\u00b7den\u00b7lan\u00b7ges", "Fin\u00b7den", ",", "bitt\u00b7res", "Schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Reue dann, von Starrsinn unterk\u00fchlt.", "tokens": ["Und", "Reu\u00b7e", "dann", ",", "von", "Starr\u00b7sinn", "un\u00b7ter\u00b7k\u00fchlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Erschienen bist Du mir in stillen Stunden,", "tokens": ["Er\u00b7schie\u00b7nen", "bist", "Du", "mir", "in", "stil\u00b7len", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo klar und ruhig flo\u00df mein wildes Blut;", "tokens": ["Wo", "klar", "und", "ru\u00b7hig", "flo\u00df", "mein", "wil\u00b7des", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich w\u00e4hnte damals, da\u00df ich heimgefunden,", "tokens": ["Ich", "w\u00e4hn\u00b7te", "da\u00b7mals", ",", "da\u00df", "ich", "heim\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Alles, Alles sei nun endlich gut.", "tokens": ["Und", "Al\u00b7les", ",", "Al\u00b7les", "sei", "nun", "end\u00b7lich", "gut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PIS", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dir aber ist ein b\u00f6ses Wort entglitten,", "tokens": ["Dir", "a\u00b7ber", "ist", "ein", "b\u00f6\u00b7ses", "Wort", "ent\u00b7glit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein ungewolltes, doch ein b\u00f6ses Wort;", "tokens": ["Ein", "un\u00b7ge\u00b7woll\u00b7tes", ",", "doch", "ein", "b\u00f6\u00b7ses", "Wort", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Umsonst war Deiner Augen scheues Bitten,", "tokens": ["Um\u00b7sonst", "war", "Dei\u00b7ner", "Au\u00b7gen", "scheu\u00b7es", "Bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es peitschte mich aus Deinem Bannkreis fort.", "tokens": ["Es", "peitschte", "mich", "aus", "Dei\u00b7nem", "Bann\u00b7kreis", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Zerschmettert fallen traulich enge Schranken,", "tokens": ["Zer\u00b7schmet\u00b7tert", "fal\u00b7len", "trau\u00b7lich", "en\u00b7ge", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In trunknem Toben geht es abgrundw\u00e4rts;", "tokens": ["In", "trunk\u00b7nem", "To\u00b7ben", "geht", "es", "ab\u00b7grund\u00b7w\u00e4rts", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie tolle Rosse rasen die Gedanken", "tokens": ["Wie", "tol\u00b7le", "Ros\u00b7se", "ra\u00b7sen", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sie zerstampfen Dir und mir das Herz ...", "tokens": ["Und", "sie", "zer\u00b7stamp\u00b7fen", "Dir", "und", "mir", "das", "Herz", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "KON", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Du hast bereut in Trauer und in Tr\u00e4nen,", "tokens": ["Du", "hast", "be\u00b7reut", "in", "Trau\u00b7er", "und", "in", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich wei\u00df es nun, da\u00df Du es gut gemeint;", "tokens": ["Ich", "wei\u00df", "es", "nun", ",", "da\u00df", "Du", "es", "gut", "ge\u00b7meint", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch einmal la\u00df an Deine Brust mich lehnen,", "tokens": ["Noch", "ein\u00b7mal", "la\u00df", "an", "Dei\u00b7ne", "Brust", "mich", "leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVIMP", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch ich hab' die Vergangenheit beweint.", "tokens": ["Auch", "ich", "hab'", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", "be\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}