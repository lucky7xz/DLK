{"textgrid.poem.49854": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Lebewohl", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bblebe wohl\u00ab \u2013 Du f\u00fchlest nicht,", "tokens": ["\u00bb", "le\u00b7be", "wohl", "\u00ab", "\u2013", "Du", "f\u00fch\u00b7lest", "nicht", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$(", "$(", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was es hei\u00dft, dies Wort der Schmerzen;", "tokens": ["Was", "es", "hei\u00dft", ",", "dies", "Wort", "der", "Schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PDS", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit getrostem Angesicht", "tokens": ["Mit", "ge\u00b7tros\u00b7tem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagtest du's und leichtem Herzen.", "tokens": ["Sag\u00b7test", "du's", "und", "leich\u00b7tem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lebe wohl! \u2013 Ach tausendmal", "tokens": ["Le\u00b7be", "wohl", "!", "\u2013", "Ach", "tau\u00b7send\u00b7mal"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["VVIMP", "ADV", "$.", "$(", "ITJ", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab ich mir es vorgesprochen,", "tokens": ["Hab", "ich", "mir", "es", "vor\u00b7ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in nimmersatter Qual", "tokens": ["Und", "in", "nim\u00b7mer\u00b7sat\u00b7ter", "Qual"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir das Herz damit gebrochen!", "tokens": ["Mir", "das", "Herz", "da\u00b7mit", "ge\u00b7bro\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PAV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}