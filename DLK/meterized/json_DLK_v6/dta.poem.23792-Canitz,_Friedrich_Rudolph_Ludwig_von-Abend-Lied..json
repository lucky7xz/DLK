{"dta.poem.23792": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Abend-Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist/ O Mensch/ heut abermahl", "tokens": ["Es", "ist", "/", "O", "Mensch", "/", "heut", "a\u00b7ber\u00b7mahl"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "$(", "NE", "NN", "$(", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Tag von deiner Jahre-Zahl", "tokens": ["Ein", "Tag", "von", "dei\u00b7ner", "Jah\u00b7re\u00b7Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verflogen/ und in nichts verwandelt.", "tokens": ["Ver\u00b7flo\u00b7gen", "/", "und", "in", "nichts", "ver\u00b7wan\u00b7delt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KON", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du n\u00e4herst dich zu deiner Grufft;", "tokens": ["Du", "n\u00e4\u00b7herst", "dich", "zu", "dei\u00b7ner", "Grufft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und zu der Stimme die dich rufft;", "tokens": ["Und", "zu", "der", "Stim\u00b7me", "die", "dich", "rufft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Thu Rechnung wie du hast gehandelt.", "tokens": ["Thu", "Rech\u00b7nung", "wie", "du", "hast", "ge\u00b7han\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KOKOM", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer aber giebt die Sicherheit/", "tokens": ["Wer", "a\u00b7ber", "giebt", "die", "Si\u00b7cher\u00b7heit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df morgen noch um diese Zeit", "tokens": ["Da\u00df", "mor\u00b7gen", "noch", "um", "die\u00b7se", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du dieses Leben wirst geniessen/", "tokens": ["Du", "die\u00b7ses", "Le\u00b7ben", "wirst", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott ", "tokens": ["Gott"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Vielleicht ist man alsdenn bem\u00fch\u2019t/", "tokens": ["Viel\u00b7leicht", "ist", "man", "als\u00b7denn", "be\u00b7m\u00fch't", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich in vier Bretter einzuschliessen.", "tokens": ["Dich", "in", "vier", "Bret\u00b7ter", "ein\u00b7zu\u00b7schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "CARD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "R\u00fcckt unvermerckt die Zeit heran/", "tokens": ["R\u00fcckt", "un\u00b7ver\u00b7merckt", "die", "Zeit", "he\u00b7ran", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In der dein Nachbar sagen kan", "tokens": ["In", "der", "dein", "Nach\u00b7bar", "sa\u00b7gen", "kan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PPOSAT", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von dir: auch dieser ist verschieden.", "tokens": ["Von", "dir", ":", "auch", "die\u00b7ser", "ist", "ver\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "ADV", "PDS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil du nun nicht die Stunde weist/", "tokens": ["Weil", "du", "nun", "nicht", "die", "Stun\u00b7de", "weist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wolan/ so r\u00fcste deinen Geist/", "tokens": ["Wo\u00b7lan", "/", "so", "r\u00fcs\u00b7te", "dei\u00b7nen", "Geist", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df er hinfahren mag in Frieden/", "tokens": ["Da\u00df", "er", "hin\u00b7fah\u00b7ren", "mag", "in", "Frie\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du hast dich in die Welt vergafft/", "tokens": ["Du", "hast", "dich", "in", "die", "Welt", "ver\u00b7gafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was aber hat sie dir geschafft?", "tokens": ["Was", "a\u00b7ber", "hat", "sie", "dir", "ge\u00b7schafft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Viel tr\u00fcbe/ wenig frohe Stunden.", "tokens": ["Viel", "tr\u00fc\u00b7be", "/", "we\u00b7nig", "fro\u00b7he", "Stun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$(", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch hast du ihr aus eitlem Sinn", "tokens": ["Doch", "hast", "du", "ihr", "aus", "eit\u00b7lem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Lebens-Kern gegeben hin/", "tokens": ["Den", "Le\u00b7bens\u00b7Kern", "ge\u00b7ge\u00b7ben", "hin", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und GOtt mit H\u00fclsen abgefunden.", "tokens": ["Und", "Gott", "mit", "H\u00fcl\u00b7sen", "ab\u00b7ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Rei\u0