ens": ["Lieb'", "Ihn", "/", "weil", "du", "Ihn", "eh\u00b7ren", "must", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und la\u00df dich nicht Gewalt noch Lust", "tokens": ["Und", "la\u00df", "dich", "nicht", "Ge\u00b7walt", "noch", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "PTKNEG", "NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von diesem heilgen Vorsatz trennen.", "tokens": ["Von", "die\u00b7sem", "heil\u00b7gen", "Vor\u00b7satz", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nimm das mit frohem Hertzen auff/", "tokens": ["Nimm", "das", "mit", "fro\u00b7hem", "Hert\u00b7zen", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "APPR", "ADJA", "NN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was Er in deinem Lebens-Lauff/", "tokens": ["Was", "Er", "in", "dei\u00b7nem", "Le\u00b7bens\u00b7Lauff", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dir zu gebrauchen wil verg\u00f6nnen.", "tokens": ["Dir", "zu", "ge\u00b7brau\u00b7chen", "wil", "ver\u00b7g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sein Wort sey deiner Augen Zweck;", "tokens": ["Sein", "Wort", "sey", "dei\