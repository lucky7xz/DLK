{"textgrid.poem.34385": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Philomele,", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Philomele,", "tokens": ["O", "Phi\u00b7lo\u00b7me\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sing immer zu!", "tokens": ["Sing", "im\u00b7mer", "zu", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Du siehst, ich qu\u00e4le", "tokens": ["Du", "siehst", ",", "ich", "qu\u00e4\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mich mehr als du.", "tokens": ["Mich", "mehr", "als", "du", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KOUS", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Es floh der meine", "tokens": ["Es", "floh", "der", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wie deiner floh,", "tokens": ["Wie", "dei\u00b7ner", "floh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und wie der deine,", "tokens": ["Und", "wie", "der", "dei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "PPOSAT", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "So liebt' er, \u2013 so! \u2013", "tokens": ["So", "liebt'", "er", ",", "\u2013", "so", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "$(", "ADV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Nur wenig Tage", "tokens": ["Nur", "we\u00b7nig", "Ta\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "W\u00e4hrt dein Gesang,", "tokens": ["W\u00e4hrt", "dein", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Doch meine Klage", "tokens": ["Doch", "mei\u00b7ne", "Kla\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "W\u00e4hrt lebenslang.", "tokens": ["W\u00e4hrt", "le\u00b7bens\u00b7lang", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Nach kurzer Weile", "tokens": ["Nach", "kur\u00b7zer", "Wei\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Suchst fremde Luft:", "tokens": ["Suchst", "frem\u00b7de", "Luft", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und ich \u2013 ich eile", "tokens": ["Und", "ich", "\u2013", "ich", "ei\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$(", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Zur dunkeln Gruft.", "tokens": ["Zur", "dun\u00b7keln", "Gruft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Im sch\u00f6nen Lenze", "tokens": ["Im", "sch\u00f6\u00b7nen", "Len\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Kommt wieder dir", "tokens": ["Kommt", "wie\u00b7der", "dir"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "ADV", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Von ferner Grenze", "tokens": ["Von", "fer\u00b7ner", "Gren\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dein Liebchen hier.", "tokens": ["Dein", "Lieb\u00b7chen", "hier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und neue Freude", "tokens": ["Und", "neu\u00b7e", "Freu\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Bringt dir das Jahr,", "tokens": ["Bringt", "dir", "das", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich lieb' \u2013 und leide", "tokens": ["Ich", "lieb'", "\u2013", "und", "lei\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ach immerdar!", "tokens": ["Ach", "im\u00b7mer\u00b7dar", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}