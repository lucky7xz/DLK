{"textgrid.poem.65745": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "19", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stumpfe, du gierige Zeit! des L\u00f6wen Krallen,", "tokens": ["Stump\u00b7fe", ",", "du", "gie\u00b7ri\u00b7ge", "Zeit", "!", "des", "L\u00f6\u00b7wen", "Kral\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$.", "ART", "NN", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Der Erde gib zum Fra\u00df die eigne Kinderbrut;", "tokens": ["Der", "Er\u00b7de", "gib", "zum", "Fra\u00df", "die", "eig\u00b7ne", "Kin\u00b7der\u00b7brut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "La\u00df wildem Tigerschlund die scharfen Z\u00e4hn' entfallen,", "tokens": ["La\u00df", "wil\u00b7dem", "Ti\u00b7ger\u00b7schlund", "die", "schar\u00b7fen", "Z\u00e4hn'", "ent\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Flamm' auf den Ph\u00f6nix im uralten Blut:", "tokens": ["Flamm'", "auf", "den", "Ph\u00f6\u00b7nix", "im", "ur\u00b7al\u00b7ten", "Blut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Gib froh und bang auf deiner Flucht die Stunden", "tokens": ["Gib", "froh", "und", "bang", "auf", "dei\u00b7ner", "Flucht", "die", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der weiten Welt, tu was du willst mit ihr", "tokens": ["Der", "wei\u00b7ten", "Welt", ",", "tu", "was", "du", "willst", "mit", "ihr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PWS", "PPER", "VMFIN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und allem Sch\u00f6nen drin entstanden wie verschwunden;", "tokens": ["Und", "al\u00b7lem", "Sch\u00f6\u00b7nen", "drin", "ent\u00b7stan\u00b7den", "wie", "ver\u00b7schwun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "NN", "ADV", "VVPP", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nur einen \u00e4rgsten Frevel wehr' ich dir:", "tokens": ["Nur", "ei\u00b7nen", "\u00e4rgs\u00b7ten", "Fre\u00b7vel", "wehr'", "ich", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "O furche nicht die sch\u00f6ne Stirn des Lieben!", "tokens": ["O", "fur\u00b7che", "nicht", "die", "sch\u00f6\u00b7ne", "Stirn", "des", "Lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "PTKNEG", "ART", "ADJA", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Mit deinem grauen Kiel zieh keine Linien dort:", "tokens": ["Mit", "dei\u00b7nem", "grau\u00b7en", "Kiel", "zieh", "kei\u00b7ne", "Li\u00b7ni\u00b7en", "dort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Ihn wolle nicht in deinem Laufe tr\u00fcben!", "tokens": ["Ihn", "wol\u00b7le", "nicht", "in", "dei\u00b7nem", "Lau\u00b7fe", "tr\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Der Sch\u00f6nheit Muster leucht' er k\u00fcnft'gen Tagen fort.", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "Mus\u00b7ter", "leucht'", "er", "k\u00fcnft'\u00b7gen", "Ta\u00b7gen", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch tu dein \u00e4rgstes, alte Zeit! es bl\u00fcht", "tokens": ["Doch", "tu", "dein", "\u00e4rgs\u00b7tes", ",", "al\u00b7te", "Zeit", "!", "es", "bl\u00fcht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Trotz deiner Wut mein Lieb doch jung in meinem Lied.", "tokens": ["Trotz", "dei\u00b7ner", "Wut", "mein", "Lieb", "doch", "jung", "in", "mei\u00b7nem", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}