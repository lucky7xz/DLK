{"dta.poem.10528": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxXXV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.57", "da:0.42"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "D\u00e4mmernd liegt der Sommerabend", "tokens": ["D\u00e4m\u00b7mernd", "liegt", "der", "Som\u00b7mer\u00b7a\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber Wald und gr\u00fcnen Wiesen;", "tokens": ["Ue\u00b7ber", "Wald", "und", "gr\u00fc\u00b7nen", "Wie\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Goldner Mond, am blauen Himmel,", "tokens": ["Gold\u00b7ner", "Mond", ",", "am", "blau\u00b7en", "Him\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Strahlt herunter, duftig labend.", "tokens": ["Strahlt", "her\u00b7un\u00b7ter", ",", "duf\u00b7tig", "la\u00b7bend", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "An dem Bache zirpt die Grille,", "tokens": ["An", "dem", "Ba\u00b7che", "zirpt", "die", "Gril\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es regt sich in dem Wasser,", "tokens": ["Und", "es", "regt", "sich", "in", "dem", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Wandrer h\u00f6rt ein Pl\u00e4tschern,", "tokens": ["Und", "der", "Wand\u00b7rer", "h\u00f6rt", "ein", "Pl\u00e4t\u00b7schern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Und ein Athmen in der Stille.", "tokens": ["Und", "ein", "Ath\u00b7men", "in", "der", "Stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dorten, an dem Bach alleine,", "tokens": ["Dor\u00b7ten", ",", "an", "dem", "Bach", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Badet sich die sch\u00f6ne Elfe;", "tokens": ["Ba\u00b7det", "sich", "die", "sch\u00f6\u00b7ne", "El\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Arm und Nacken, wei\u00df und lieblich,", "tokens": ["Arm", "und", "Na\u00b7cken", ",", "wei\u00df", "und", "lieb\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schimmern in dem Mondenscheine.", "tokens": ["Schim\u00b7mern", "in", "dem", "Mon\u00b7den\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}