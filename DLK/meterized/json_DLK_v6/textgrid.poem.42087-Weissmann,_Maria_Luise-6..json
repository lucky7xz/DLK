{"textgrid.poem.42087": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch dann zuletzt bist Du das gute Gleiten", "tokens": ["Doch", "dann", "zu\u00b7letzt", "bist", "Du", "das", "gu\u00b7te", "Glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ins Schlafende, das ohne Sprache ist", "tokens": ["Ins", "Schla\u00b7fen\u00b7de", ",", "das", "oh\u00b7ne", "Spra\u00b7che", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPR", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie ohne Traum. Das sich so tief vergi\u00dft,", "tokens": ["Wie", "oh\u00b7ne", "Traum", ".", "Das", "sich", "so", "tief", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$.", "PDS", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df Namen schon es mit sich selbst entzweiten.", "tokens": ["Da\u00df", "Na\u00b7men", "schon", "es", "mit", "sich", "selbst", "ent\u00b7zwei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PPER", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie stehen wieder stumm im Topf aus Ton,", "tokens": ["Sie", "ste\u00b7hen", "wie\u00b7der", "stumm", "im", "Topf", "aus", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und was sie sprachen wurde nie gesagt,", "tokens": ["Und", "was", "sie", "spra\u00b7chen", "wur\u00b7de", "nie", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und was sie klagten wurde nie geklagt:", "tokens": ["Und", "was", "sie", "klag\u00b7ten", "wur\u00b7de", "nie", "ge\u00b7klagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ganz pflanzenhaft in einer dunklen Fron", "tokens": ["Ganz", "pflan\u00b7zen\u00b7haft", "in", "ei\u00b7ner", "dunk\u00b7len", "Fron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Von Wuchs und Trieb sind sie zur\u00fcckgewandt", "tokens": ["Von", "Wuchs", "und", "Trieb", "sind", "sie", "zu\u00b7r\u00fcck\u00b7ge\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zum Schweigenden. Und Du darfst nichts erwarten", "tokens": ["Zum", "Schwei\u00b7gen\u00b7den", ".", "Und", "Du", "darfst", "nichts", "er\u00b7war\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "KON", "PPER", "VMFIN", "PIS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als Dieses nur: da\u00df sie einmal, besehn", "tokens": ["Als", "Die\u00b7ses", "nur", ":", "da\u00df", "sie", "ein\u00b7mal", ",", "be\u00b7sehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PDS", "ADV", "$.", "KOUS", "PPER", "ADV", "$,", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Von Deinem Blick, ber\u00fchrt von Deiner Hand,", "tokens": ["Von", "Dei\u00b7nem", "Blick", ",", "be\u00b7r\u00fchrt", "von", "Dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So wie ein pl\u00f6tzlich \u00fcbersonnter Garten,", "tokens": ["So", "wie", "ein", "pl\u00f6tz\u00b7lich", "\u00fc\u00b7ber\u00b7sonn\u00b7ter", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aufbrechen und in j\u00e4her Bl\u00fcte stehn.", "tokens": ["Auf\u00b7bre\u00b7chen", "und", "in", "j\u00e4\u00b7her", "Bl\u00fc\u00b7te", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}