{"textgrid.poem.66185": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ha, die gro\u00dfe Freiheitsstunde", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ha, die gro\u00dfe Freiheitsstunde", "tokens": ["Ha", ",", "die", "gro\u00b7\u00dfe", "Frei\u00b7heits\u00b7stun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt einmal, mein Freund, f\u00fcr dich!", "tokens": ["Kommt", "ein\u00b7mal", ",", "mein", "Freund", ",", "f\u00fcr", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mit dem Jubel aus dem Munde", "tokens": ["Mit", "dem", "Ju\u00b7bel", "aus", "dem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwebt sie! Br\u00e4utlich zeigt sie sich!", "tokens": ["Schwebt", "sie", "!", "Br\u00e4ut\u00b7lich", "zeigt", "sie", "sich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "NE", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von des Engels Hauch zerschmelzen", "tokens": ["Von", "des", "En\u00b7gels", "Hauch", "zer\u00b7schmel\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwere Fesseln, deren Last", "tokens": ["Schwe\u00b7re", "Fes\u00b7seln", ",", "de\u00b7ren", "Last"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Du, gekettet an den Felsen", "tokens": ["Du", ",", "ge\u00b7ket\u00b7tet", "an", "den", "Fel\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deiner Wand, getragen hast.", "tokens": ["Dei\u00b7ner", "Wand", ",", "ge\u00b7tra\u00b7gen", "hast", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach! sie f\u00fchrt mit Lilienh\u00e4nden", "tokens": ["Ach", "!", "sie", "f\u00fchrt", "mit", "Li\u00b7li\u00b7en\u00b7h\u00e4n\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Dich vom Thr\u00e4nenberg herab,", "tokens": ["Dich", "vom", "Thr\u00e4\u00b7nen\u00b7berg", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem Gefangenen, Elenden,", "tokens": ["Dem", "Ge\u00b7fan\u00b7ge\u00b7nen", ",", "E\u00b7len\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Schauervoller als das Grab.", "tokens": ["Schau\u00b7er\u00b7vol\u00b7ler", "als", "das", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kerkerstaub entfliegt dem Kleide,", "tokens": ["Ker\u00b7ker\u00b7staub", "ent\u00b7fliegt", "dem", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der goldnen Freiheit Licht", "tokens": ["Und", "der", "gold\u00b7nen", "Frei\u00b7heit", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "H\u00e4ngt an seinem Saum; die Freude", "tokens": ["H\u00e4ngt", "an", "sei\u00b7nem", "Saum", ";", "die", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "R\u00f6thet wieder dein Gesicht.", "tokens": ["R\u00f6\u00b7thet", "wie\u00b7der", "dein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Weggeschwunden, o du Lieber!", "tokens": ["Weg\u00b7ge\u00b7schwun\u00b7den", ",", "o", "du", "Lie\u00b7ber", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weggeschwunden ist die Nacht,", "tokens": ["Weg\u00b7ge\u00b7schwun\u00b7den", "ist", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die dir oft die Seele tr\u00fcber,", "tokens": ["Die", "dir", "oft", "die", "See\u00b7le", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als der N\u00e4chte Schau'r gemacht.", "tokens": ["Als", "der", "N\u00e4ch\u00b7te", "Schau'r", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine Thr\u00e4nen sind verflossen,", "tokens": ["Dei\u00b7ne", "Thr\u00e4\u00b7nen", "sind", "ver\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die du oft im Kerkergrab", "tokens": ["Die", "du", "oft", "im", "Ker\u00b7ker\u00b7grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Vor dem Engel hingegossen,", "tokens": ["Vor", "dem", "En\u00b7gel", "hin\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Den dir Gott zum Schutze gab.", "tokens": ["Den", "dir", "Gott", "zum", "Schut\u00b7ze", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Keine Schl\u00f6sser, keine Riegel", "tokens": ["Kei\u00b7ne", "Schl\u00f6s\u00b7ser", ",", "kei\u00b7ne", "Rie\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rasseln mehr vor deiner Th\u00fcr';", "tokens": ["Ras\u00b7seln", "mehr", "vor", "dei\u00b7ner", "Th\u00fcr'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Schwermuth Rabenfl\u00fcgel", "tokens": ["Und", "der", "Schwer\u00b7muth", "Ra\u00b7ben\u00b7fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Schattet nimmer \u00fcber dir.", "tokens": ["Schat\u00b7tet", "nim\u00b7mer", "\u00fc\u00b7ber", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimmer steigt durchs Eisengitter", "tokens": ["Nim\u00b7mer", "steigt", "durchs", "Ei\u00b7sen\u00b7git\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dein Ge\u00e4chz': O Vater, nimm", "tokens": ["Dein", "Ge\u00b7\u00e4ch\u00b7z'", ":", "O", "Va\u00b7ter", ",", "nimm"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$.", "NE", "NN", "$,", "VVIMP"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Diesen Kelch, so schwer, so bitter,", "tokens": ["Die\u00b7sen", "Kelch", ",", "so", "schwer", ",", "so", "bit\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "So gef\u00fcllt mit deinem Grimm.", "tokens": ["So", "ge\u00b7f\u00fcllt", "mit", "dei\u00b7nem", "Grimm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "PPOSAT", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Deine Br\u00fcder siehst du nimmer", "tokens": ["Dei\u00b7ne", "Br\u00fc\u00b7der", "siehst", "du", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schleppen ihrer Ketten Last;", "tokens": ["Schlep\u00b7pen", "ih\u00b7rer", "Ket\u00b7ten", "Last", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rst nicht mehr ihr Angstgewimmer", "tokens": ["H\u00f6rst", "nicht", "mehr", "ihr", "Angst\u00b7ge\u00b7wim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den N\u00e4chten ohne Rast.", "tokens": ["In", "den", "N\u00e4ch\u00b7ten", "oh\u00b7ne", "Rast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Siehst nicht mehr die Weidengerte", "tokens": ["Siehst", "nicht", "mehr", "die", "Wei\u00b7den\u00b7ger\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In des Kriegers Blut getaucht;", "tokens": ["In", "des", "Krie\u00b7gers", "Blut", "ge\u00b7taucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nimmer siehst du, wie die Erde", "tokens": ["Nim\u00b7mer", "siehst", "du", ",", "wie", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von Verzweiflungsz\u00e4hren raucht.", "tokens": ["Von", "Ver\u00b7zwei\u00b7flungs\u00b7z\u00e4h\u00b7ren", "raucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Freiheit! Freiheit! h\u00f6rst du t\u00f6nen", "tokens": ["Frei\u00b7heit", "!", "Frei\u00b7heit", "!", "h\u00f6rst", "du", "t\u00f6\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem alten Eichenhain,", "tokens": ["Aus", "dem", "al\u00b7ten", "Ei\u00b7chen\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandelst bald mit Deutschlands S\u00f6hnen", "tokens": ["Wan\u00b7delst", "bald", "mit", "Deutschlands", "S\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NE", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wieder an dem freien Main.", "tokens": ["Wie\u00b7der", "an", "dem", "frei\u00b7en", "Main", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Freiheit! Gottes gr\u00f6\u00dfter Segen!", "tokens": ["Frei\u00b7heit", "!", "Got\u00b7tes", "gr\u00f6\u00df\u00b7ter", "Se\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freiheit, ach, wann wandelst du", "tokens": ["Frei\u00b7heit", ",", "ach", ",", "wann", "wan\u00b7delst", "du"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ITJ", "$,", "PWAV", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mir Best\u00fcrmten auch entgegen?", "tokens": ["Mir", "Be\u00b7st\u00fcrm\u00b7ten", "auch", "ent\u00b7ge\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bringst mir wieder Seelenruh'?", "tokens": ["Bringst", "mir", "wie\u00b7der", "See\u00b7len\u00b7ruh'", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "R ..., Trauter, sieh mich weinen", "tokens": ["R", "...", ",", "Trau\u00b7ter", ",", "sieh", "mich", "wei\u00b7nen"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$(", "$,", "NN", "$,", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit verh\u00fclltem Angesicht.", "tokens": ["Mit", "ver\u00b7h\u00fcll\u00b7tem", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geh, umarme nun die Deinen;", "tokens": ["Geh", ",", "um\u00b7ar\u00b7me", "nun", "die", "Dei\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "ART", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber Freund, vergi\u00df mich nicht!", "tokens": ["A\u00b7ber", "Freund", ",", "ver\u00b7gi\u00df", "mich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sprich zu deinen Lieben: droben", "tokens": ["Sprich", "zu", "dei\u00b7nen", "Lie\u00b7ben", ":", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "APPR", "PPOSAT", "ADJA", "$.", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fault in seinem Kerkergrab", "tokens": ["Fault", "in", "sei\u00b7nem", "Ker\u00b7ker\u00b7grab"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schubart, der mir manche Proben", "tokens": ["Schu\u00b7bart", ",", "der", "mir", "man\u00b7che", "Pro\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "PPER", "PIAT", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Seiner Lieb' und Freundschaft gab.", "tokens": ["Sei\u00b7ner", "Lieb'", "und", "Freund\u00b7schaft", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "R ..., nicht mehr auf dieser Erde,", "tokens": ["R", "...", ",", "nicht", "mehr", "auf", "die\u00b7ser", "Er\u00b7de", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "$,", "PTKNEG", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst im Himmel seh' ich dich!", "tokens": ["Einst", "im", "Him\u00b7mel", "seh'", "ich", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O dann bleibst du mein Gef\u00e4hrte,", "tokens": ["O", "dann", "bleibst", "du", "m