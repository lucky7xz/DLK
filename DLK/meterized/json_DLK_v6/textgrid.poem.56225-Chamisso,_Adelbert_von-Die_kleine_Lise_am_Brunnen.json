{"textgrid.poem.56225": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Die kleine Lise am Brunnen", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In den Grund des Brunnens schaut", "tokens": ["In", "den", "Grund", "des", "Brun\u00b7nens", "schaut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lischen gar gedankenvoll;", "tokens": ["Li\u00b7schen", "gar", "ge\u00b7dan\u00b7ken\u00b7voll", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was hier dieser Brunnen soll,", "tokens": ["Was", "hier", "die\u00b7ser", "Brun\u00b7nen", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PDAT", "NN", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat die Mutter ihr vertraut.", "tokens": ["Hat", "die", "Mut\u00b7ter", "ihr", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbmeine Schwester sagte zwar", "tokens": ["\u00bb", "mei\u00b7ne", "Schwes\u00b7ter", "sag\u00b7te", "zwar"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df der Storch die Kinder bringt;", "tokens": ["Da\u00df", "der", "Storch", "die", "Kin\u00b7der", "bringt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie verst\u00e4ndig es auch klingt,", "tokens": ["Wie", "ver\u00b7st\u00e4n\u00b7dig", "es", "auch", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist es aber doch nicht wahr.", "tokens": ["Ist", "es", "a\u00b7ber", "doch", "nicht", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nein, das macht sie mir nicht weis.", "tokens": ["Nein", ",", "das", "macht", "sie", "mir", "nicht", "weis", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mutter, wie ich sie gefragt,", "tokens": ["Mut\u00b7ter", ",", "wie", "ich", "sie", "ge\u00b7fragt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat es anders mir gesagt,", "tokens": ["Hat", "es", "an\u00b7ders", "mir", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mutter, die es besser wei\u00df.", "tokens": ["Mut\u00b7ter", ",", "die", "es", "bes\u00b7ser", "wei\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aus dem Brunnen holt bei Nacht", "tokens": ["Aus", "dem", "Brun\u00b7nen", "holt", "bei", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie die weise Frau allein,", "tokens": ["Sie", "die", "wei\u00b7se", "Frau", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die hat j\u00fcngst das Br\u00fcderlein", "tokens": ["Die", "hat", "j\u00fcngst", "das", "Br\u00fc\u00b7derl\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Aus dem Brunnen uns gebracht.", "tokens": ["Aus", "dem", "Brun\u00b7nen", "uns", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Vor f\u00fcnf Jahren schlief ich auch", "tokens": ["Vor", "f\u00fcnf", "Jah\u00b7ren", "schlief", "ich", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier im Brunnen, wundersam,", "tokens": ["Hier", "im", "Brun\u00b7nen", ",", "wun\u00b7der\u00b7sam", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis sie mich zu holen kam", "tokens": ["Bis", "sie", "mich", "zu", "ho\u00b7len", "kam"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PTKZU", "VVINF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach dem hergebrachten Brauch.", "tokens": ["Nach", "dem", "her\u00b7ge\u00b7brach\u00b7ten", "Brauch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "K\u00f6nnt ich nur die Kleinen sehn!", "tokens": ["K\u00f6nnt", "ich", "nur", "die", "Klei\u00b7nen", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, ich s\u00e4h sie gar zu gern!", "tokens": ["Ach", ",", "ich", "s\u00e4h", "sie", "gar", "zu", "gern", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKA", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch sie schlafen tief und fern,", "tokens": ["Doch", "sie", "schla\u00b7fen", "tief", "und", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Keines l\u00e4\u00dft sich heut ersp\u00e4hn.", "tokens": ["Kei\u00b7nes", "l\u00e4\u00dft", "sich", "heut", "er\u00b7sp\u00e4hn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "W\u00fc\u00dft ich, wie die Frau es macht,", "tokens": ["W\u00fc\u00dft", "ich", ",", "wie", "die", "Frau", "es", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Holt ich eines mir geschwind.", "tokens": ["Holt", "ich", "ei\u00b7nes", "mir", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So ein himmlisch kleines Kind,", "tokens": ["So", "ein", "himm\u00b7lisch", "klei\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ei, das w\u00e4r auch eine Pracht!", "tokens": ["Ei", ",", "das", "w\u00e4r", "auch", "ei\u00b7ne", "Pracht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "O was g\u00e4b ich nicht darum!", "tokens": ["O", "was", "g\u00e4b", "ich", "nicht", "da\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VVFIN", "PPER", "PTKNEG", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seit es durch den Sinn mir f\u00e4hrt,", "tokens": ["Seit", "es", "durch", "den", "Sinn", "mir", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bist mir gar nichts, gar nichts wert,", "tokens": ["Bist", "mir", "gar", "nichts", ",", "gar", "nichts", "wert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIS", "$,", "ADV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Garst'ge Puppe, stumm und dumm!\u00ab", "tokens": ["Gar\u00b7st'\u00b7ge", "Pup\u00b7pe", ",", "stumm", "und", "dumm", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}