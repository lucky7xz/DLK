{"dta.poem.10305": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Ein Geheimnis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Vier edle F&#252;chse nicken mit den K\u00f6pfen,             ", "tokens": ["Vier", "ed\u00b7le", "F", "&#252;", "chse", "ni\u00b7cken", "mit", "den", "K\u00f6p\u00b7fen", ","], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$(", "NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Brust und Hals und M\u00e4hnen, Zaum und Z\u00fcgel,", "tokens": ["Da\u00df", "Brust", "und", "Hals", "und", "M\u00e4h\u00b7nen", ",", "Zaum", "und", "Z\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit wei\u00dfem Schaumgeflock getigert sind.", "tokens": ["Mit", "wei\u00b7\u00dfem", "Schaum\u00b7ge\u00b7flock", "ge\u00b7ti\u00b7gert", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die feinen Hufe scharren ungeduldig,", "tokens": ["Die", "fei\u00b7nen", "Hu\u00b7fe", "schar\u00b7ren", "un\u00b7ge\u00b7dul\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den leichten Wagen, dem sie vorgespannt,", "tokens": ["Den", "leich\u00b7ten", "Wa\u00b7gen", ",", "dem", "sie", "vor\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Durch weite Strecken m\u00fchlos fortzurei\u00dfen.", "tokens": ["Durch", "wei\u00b7te", "Stre\u00b7cken", "m\u00fch\u00b7los", "fort\u00b7zu\u00b7rei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Am offnen Schlage steht der Groom und wartet.", "tokens": ["Am", "off\u00b7nen", "Schla\u00b7ge", "steht", "der", "Groom", "und", "war\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Th\u00fcr des Schlosses \u00f6ffnet ihre Fl\u00fcgel.", "tokens": ["Die", "Th\u00fcr", "des", "Schlos\u00b7ses", "\u00f6ff\u00b7net", "ih\u00b7re", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und tiefgebeugter Dienerschaft vor\u00fcber", "tokens": ["Und", "tief\u00b7ge\u00b7beug\u00b7ter", "Die\u00b7ner\u00b7schaft", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Betritt, des linken Handschuh Kn\u00f6pfe schlie\u00dfend,", "tokens": ["Be\u00b7tritt", ",", "des", "lin\u00b7ken", "Hand\u00b7schuh", "Kn\u00f6p\u00b7fe", "schlie\u00b7\u00dfend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein gro\u00dfer Mann mit kurzem, braunem Vollbart,", "tokens": ["Ein", "gro\u00b7\u00dfer", "Mann", "mit", "kur\u00b7zem", ",", "brau\u00b7nem", "Voll\u00b7bart", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Die Marmortreppe, steht, und steigt hinunter.", "tokens": ["Die", "Mar\u00b7mor\u00b7trep\u00b7pe", ",", "steht", ",", "und", "steigt", "hin\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Haare deckt ein alter grauer Filz,", "tokens": ["Die", "Haa\u00b7re", "deckt", "ein", "al\u00b7ter", "grau\u00b7er", "Filz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Geschm\u00fcckt mit unscheinbarer Sperberfeder.", "tokens": ["Ge\u00b7schm\u00fcckt", "mit", "un\u00b7schein\u00b7ba\u00b7rer", "Sper\u00b7ber\u00b7fe\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Gewehr und Tasche liegen schon im Sitz.", "tokens": ["Ge\u00b7wehr", "und", "Ta\u00b7sche", "lie\u00b7gen", "schon", "im", "Sitz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der H\u00fchnerhund springt bellend auf die Polster.", "tokens": ["Der", "H\u00fch\u00b7ner\u00b7hund", "springt", "bel\u00b7lend", "auf", "die", "Pols\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und fort, als g\u00e4lt\u2019 es eine Siegesbotschaft,", "tokens": ["Und", "fort", ",", "als", "g\u00e4lt'", "es", "ei\u00b7ne", "Sie\u00b7ges\u00b7bot\u00b7schaft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$,", "KOUS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Entst\u00fcrmt dem Halt in Hast der Viererzug.", "tokens": ["Ent\u00b7st\u00fcrmt", "dem", "Halt", "in", "Hast", "der", "Vier\u00b7er\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dem J\u00e4ger schaut vom hohen Fenster nach", "tokens": ["Dem", "J\u00e4\u00b7ger", "schaut", "vom", "ho\u00b7hen", "Fens\u00b7ter", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein stolzes, blasses, \u00fcppig gro\u00dfes Weib:", "tokens": ["Ein", "stol\u00b7zes", ",", "blas\u00b7ses", ",", "\u00fcp\u00b7pig", "gro\u00b7\u00dfes", "Weib", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u201ewenn ich nur w\u00fc\u00dfte, was ihn immer dr\u00e4ngt,", "tokens": ["\u201e", "wenn", "ich", "nur", "w\u00fc\u00df\u00b7te", ",", "was", "ihn", "im\u00b7mer", "dr\u00e4ngt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "VVFIN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf jener magern Heidewelt zu jagen.", "tokens": ["Auf", "je\u00b7ner", "ma\u00b7gern", "Hei\u00b7de\u00b7welt", "zu", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn einmal nur er fragte: Willst du mit?\u201c", "tokens": ["Wenn", "ein\u00b7mal", "nur", "er", "frag\u00b7te", ":", "Willst", "du", "mit", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "ADV", "PPER", "VVFIN", "$.", "VMFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und traurig l\u00e4\u00dft sie sich im Sessel nieder,", "tokens": ["Und", "trau\u00b7rig", "l\u00e4\u00dft", "sie", "sich", "im", "Ses\u00b7sel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die stillen Augen mit den H\u00e4nden deckend.", "tokens": ["Die", "stil\u00b7len", "Au\u00b7gen", "mit", "den", "H\u00e4n\u00b7den", "de\u00b7ckend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch keine Thr\u00e4ne tropft ihr von der Wimper.", "tokens": ["Doch", "kei\u00b7ne", "Thr\u00e4\u00b7ne", "tropft", "ihr", "von", "der", "Wim\u00b7per", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Indessen rollt der Wagen seinen Weg,", "tokens": ["In\u00b7des\u00b7sen", "rollt", "der", "Wa\u00b7gen", "sei\u00b7nen", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und rollt und rollt drei Stunden durch die Felder", "tokens": ["Und", "rollt", "und", "rollt", "drei", "Stun\u00b7den", "durch", "die", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "CARD", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im immer gleichen, schlanken, schnellen Trab.", "tokens": ["Im", "im\u00b7mer", "glei\u00b7chen", ",", "schlan\u00b7ken", ",", "schnel\u00b7len", "Trab", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADV", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Nord und S\u00fcd, so weit das Auge reicht,", "tokens": ["Und", "Nord", "und", "S\u00fcd", ",", "so", "weit", "das", "Au\u00b7ge", "reicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "NN", "$,", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und West und Ost in unbegrenzter Ferne,", "tokens": ["Und", "West", "und", "Ost", "in", "un\u00b7be\u00b7grenz\u00b7ter", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Geh\u00f6rt dem J\u00e4ger, der im Wagen sitzt,", "tokens": ["Ge\u00b7h\u00f6rt", "dem", "J\u00e4\u00b7ger", ",", "der", "im", "Wa\u00b7gen", "sitzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und freundlich rechts und links den Bauern dankt,", "tokens": ["Und", "freund\u00b7lich", "rechts", "und", "links", "den", "Bau\u00b7ern", "dankt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wenn ehrerbietig sie die M\u00fctzen r\u00fccken.", "tokens": ["Wenn", "ehr\u00b7er\u00b7bie\u00b7tig", "sie", "die", "M\u00fct\u00b7zen", "r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Vor einem Heidkrug h\u00e4lt das Viergespann.", "tokens": ["Vor", "ei\u00b7nem", "Heid\u00b7krug", "h\u00e4lt", "das", "Vier\u00b7ge\u00b7spann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die B\u00fcchse umgehangen, schlendert nun", "tokens": ["Die", "B\u00fcch\u00b7se", "um\u00b7ge\u00b7han\u00b7gen", ",", "schlen\u00b7dert", "nun"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Allein der J\u00e4ger durch das braune Kraut.", "tokens": ["Al\u00b7lein", "der", "J\u00e4\u00b7ger", "durch", "das", "brau\u00b7ne", "Kraut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Feldmann hat H\u00fchner in der Nase, steht.", "tokens": ["Feld\u00b7mann", "hat", "H\u00fch\u00b7ner", "in", "der", "Na\u00b7se", ",", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "APPR", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Doch hinter ihm blitzt kein Gewehr heran.", "tokens": ["Doch", "hin\u00b7ter", "ihm", "blitzt", "kein", "Ge\u00b7wehr", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Am Waldrand weilt der Mann vor einem H\u00e4uschen,", "tokens": ["Am", "Wald\u00b7rand", "weilt", "der", "Mann", "vor", "ei\u00b7nem", "H\u00e4usc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bei dessen Th\u00fcr ein kleiner Knabe spielt.", "tokens": ["Bei", "des\u00b7sen", "Th\u00fcr", "ein", "klei\u00b7ner", "Kna\u00b7be", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und in die Arme nimmt er rasch den Jungen,", "tokens": ["Und", "in", "die", "Ar\u00b7me", "nimmt", "er", "rasch", "den", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und k\u00fc\u00dft die Lippen ihm, die gro\u00dfen Augen,", "tokens": ["Und", "k\u00fc\u00dft", "die", "Lip\u00b7pen", "ihm", ",", "die", "gro\u00b7\u00dfen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die wunderbaren, dunkelblauen Augen,", "tokens": ["Die", "wun\u00b7der\u00b7ba\u00b7ren", ",", "dun\u00b7kel\u00b7blau\u00b7en", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von langen, schwarzen Wimpern scharf besch\u00fctzt.", "tokens": ["Von", "lan\u00b7gen", ",", "schwar\u00b7zen", "Wim\u00b7pern", "scharf", "be\u00b7sch\u00fctzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und tr\u00e4gt ihn dann in\u2019s Haus.", "tokens": ["Und", "tr\u00e4gt", "ihn", "dann", "in's", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Tritt ihm ent