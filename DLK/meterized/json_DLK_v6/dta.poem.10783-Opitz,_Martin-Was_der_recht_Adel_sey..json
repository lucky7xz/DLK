{"dta.poem.10783": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Was der recht Adel sey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ein vnzeitige Frucht/ bewart in Mutter Leibe/", "tokens": ["Ein", "vn\u00b7zei\u00b7ti\u00b7ge", "Frucht", "/", "be\u00b7wart", "in", "Mut\u00b7ter", "Lei\u00b7be", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADJD", "APPR", "NN", "NN", "$("], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Lebt halber nur bi\u00df sie zum gantzen Menschen wirdt:", "tokens": ["Lebt", "hal\u00b7ber", "nur", "bi\u00df", "sie", "zum", "gant\u00b7zen", "Men\u00b7schen", "wirdt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPO", "ADV", "APPR", "PPER", "APPRART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So auch ein Kindt erzeugt von einem Edeln Weibe;", "tokens": ["So", "auch", "ein", "Kindt", "er\u00b7zeugt", "von", "ei\u00b7nem", "E\u00b7deln", "Wei\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit halbem Adel nur von der Natur geziert.", "tokens": ["Mit", "hal\u00b7bem", "A\u00b7del", "nur", "von", "der", "Na\u00b7tur", "ge\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der ist ein halber Mensch/ der sein vnarth verbl\u00fcmet", "tokens": ["Der", "ist", "ein", "hal\u00b7ber", "Mensch", "/", "der", "sein", "vn\u00b7arth", "ver\u00b7bl\u00fc\u00b7met"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "PPOSAT", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit seiner Eltern Rhum/ den er zur schande lebt/", "tokens": ["Mit", "sei\u00b7ner", "El\u00b7tern", "Rhum", "/", "den", "er", "zur", "schan\u00b7de", "lebt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$(", "PRELS", "PPER", "APPRART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der ist ein rechter Mensch den eigne Tugendt r\u00fchmet/", "tokens": ["Der", "ist", "ein", "rech\u00b7ter", "Mensch", "den", "eig\u00b7ne", "Tu\u00b7gendt", "r\u00fch\u00b7met", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den sein selbst Raht vndt that zu Ehren hoch erhebt.", "tokens": ["Den", "sein", "selbst", "Raht", "vndt", "that", "zu", "Eh\u00b7ren", "hoch", "er\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADV", "NN", "KON", "VVFIN", "APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die aber so zugleich von Edlem stamm geboren/", "tokens": ["Die", "a\u00b7ber", "so", "zu\u00b7gleich", "von", "Ed\u00b7lem", "stamm", "ge\u00b7bo\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADV", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Durch Tugendt noch darzu vollkommen sein gemacht/", "tokens": ["Durch", "Tu\u00b7gendt", "noch", "dar\u00b7zu", "voll\u00b7kom\u00b7men", "sein", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PAV", "ADJD", "VAINF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die sein/ die sein allein die Edlen au\u00dferkoren/", "tokens": ["Die", "sein", "/", "die", "sein", "al\u00b7lein", "die", "Ed\u00b7len", "au\u00b7\u00dfer\u00b7ko\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAINF", "$(", "ART", "PPOSAT", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die man vor G\u00f6tter hie vnder den Menschen acht.", "tokens": ["Die", "man", "vor", "G\u00f6t\u00b7ter", "hie", "vn\u00b7der", "den", "Men\u00b7schen", "acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "ADV", "APPR", "ART", "NN", "CARD", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}