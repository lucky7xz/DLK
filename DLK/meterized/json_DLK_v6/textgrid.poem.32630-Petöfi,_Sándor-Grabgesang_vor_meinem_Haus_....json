{"textgrid.poem.32630": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "Grabgesang vor meinem Haus ...", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grabgesang vor meinem Haus erklingt,", "tokens": ["Grab\u00b7ge\u00b7sang", "vor", "mei\u00b7nem", "Haus", "er\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wer ist's, den man zur letzten Ruhe bringt?", "tokens": ["Wer", "ist's", ",", "den", "man", "zur", "letz\u00b7ten", "Ru\u00b7he", "bringt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PIS", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer immer, 's ist kein Erdensklave mehr,", "tokens": ["Wer", "im\u00b7mer", ",", "'s", "ist", "kein", "Er\u00b7dens\u00b7kla\u00b7ve", "mehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "PPER", "VAFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und zehnmal gl\u00fccklicher als ich ist er!", "tokens": ["Und", "zehn\u00b7mal", "gl\u00fcck\u00b7li\u00b7cher", "als", "ich", "ist", "er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "KOKOM", "PPER", "VAFIN", "PPER", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Vor meinem Fenster tragen sie ihn fort,", "tokens": ["Vor", "mei\u00b7nem", "Fens\u00b7ter", "tra\u00b7gen", "sie", "ihn", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Gott, wie viele Leute weinen dort!", "tokens": ["Mein", "Gott", ",", "wie", "vie\u00b7le", "Leu\u00b7te", "wei\u00b7nen", "dort", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "PIAT", "NN", "VVINF", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O, warum tr\u00e4gt man mich denn nicht hinaus?", "tokens": ["O", ",", "wa\u00b7rum", "tr\u00e4gt", "man", "mich", "denn", "nicht", "hin\u00b7aus", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PIS", "PRF", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es weinte niemand sich die Augen aus ...", "tokens": ["Es", "wein\u00b7te", "nie\u00b7mand", "sich", "die", "Au\u00b7gen", "aus", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}