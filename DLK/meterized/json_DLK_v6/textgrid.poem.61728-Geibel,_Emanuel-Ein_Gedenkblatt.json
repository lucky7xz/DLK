{"textgrid.poem.61728": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Ein Gedenkblatt", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Samstagmorgen vor Palmarum war's", "tokens": ["Am", "Sams\u00b7tag\u00b7mor\u00b7gen", "vor", "Pal\u00b7ma\u00b7rum", "wa\u00b7r's"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NE", "VAFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Jahre, da man Neunundvierzig schrieb,", "tokens": ["Im", "Jah\u00b7re", ",", "da", "man", "Neun\u00b7und\u00b7vier\u00b7zig", "schrieb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PIS", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df mich die goldne Sonne des Aprils", "tokens": ["Da\u00df", "mich", "die", "gold\u00b7ne", "Son\u00b7ne", "des", "Ap\u00b7rils"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus meinem alten Nest am Hafendamm", "tokens": ["Aus", "mei\u00b7nem", "al\u00b7ten", "Nest", "am", "Ha\u00b7fen\u00b7damm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hinab ins Freie lockte. Drau\u00dfen zog", "tokens": ["Hin\u00b7ab", "ins", "Frei\u00b7e", "lock\u00b7te", ".", "Drau\u00b7\u00dfen", "zog"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Flu\u00df, von m\u00e4cht'gen Segeln schon belebt,", "tokens": ["Der", "Flu\u00df", ",", "von", "m\u00e4cht'\u00b7gen", "Se\u00b7geln", "schon", "be\u00b7lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Blaugl\u00e4nzend hin, und in den L\u00fcften schwamm", "tokens": ["Blau\u00b7gl\u00e4n\u00b7zend", "hin", ",", "und", "in", "den", "L\u00fcf\u00b7ten", "schwamm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Des Fr\u00fchlings ahnungsvolles Hoffnungslied.", "tokens": ["Des", "Fr\u00fch\u00b7lings", "ah\u00b7nungs\u00b7vol\u00b7les", "Hoff\u00b7nungs\u00b7lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Mir aber wuchs das Herz bei diesem Ton,", "tokens": ["Mir", "a\u00b7ber", "wuchs", "das", "Herz", "bei", "die\u00b7sem", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Als m\u00fc\u00dft' er Gl\u00fcck verk\u00fcnden. Ruhiger", "tokens": ["Als", "m\u00fc\u00dft'", "er", "Gl\u00fcck", "ver\u00b7k\u00fcn\u00b7den", ".", "Ru\u00b7hi\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "VMFIN", "PPER", "NN", "VVINF", "$.", "NN"], "meter": "-+-+-+----", "measure": "unknown.measure.tri"}, "line.11": {"text": "Gedacht' ich an der Zeit verworrnen Kampf", "tokens": ["Ge\u00b7dacht'", "ich", "an", "der", "Zeit", "ver\u00b7worr\u00b7nen", "Kampf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und an die Zukunft, deren Los vielleicht", "tokens": ["Und", "an", "die", "Zu\u00b7kunft", ",", "de\u00b7ren", "Los", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "In diesem Augenblick geworfen ward.", "tokens": ["In", "die\u00b7sem", "Au\u00b7gen\u00b7blick", "ge\u00b7wor\u00b7fen", "ward", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Da, wie ich so am Damm des Ufers noch", "tokens": ["Da", ",", "wie", "ich", "so", "am", "Damm", "des", "U\u00b7fers", "noch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Vertieft hinabschritt, kam mein Jugendfreund,", "tokens": ["Ver\u00b7tieft", "hin\u00b7ab\u00b7schritt", ",", "kam", "mein", "Ju\u00b7gend\u00b7freund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Der blonde Maler, hastig und erregt,", "tokens": ["Der", "blon\u00b7de", "Ma\u00b7ler", ",", "has\u00b7tig", "und", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Da\u00df Bart und Haar ihm flog, des Wegs daher,", "tokens": ["Da\u00df", "Bart", "und", "Haar", "ihm", "flog", ",", "des", "Wegs", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVFIN", "$,", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und sein des L\u00e4chelns ungewohnt Gesicht", "tokens": ["Und", "sein", "des", "L\u00e4\u00b7chelns", "un\u00b7ge\u00b7wohnt", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ergl\u00e4nzte wie vom Fr\u00fchrot \u00fcbersonnt.", "tokens": ["Er\u00b7gl\u00e4nz\u00b7te", "wie", "vom", "Fr\u00fch\u00b7rot", "\u00fc\u00b7ber\u00b7sonnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "So rief er mir entgegen: \u00bbWei\u00dft du's schon?\u00ab", "tokens": ["So", "rief", "er", "mir", "ent\u00b7ge\u00b7gen", ":", "\u00bb", "Wei\u00dft", "du's", "schon", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$.", "$(", "VVFIN", "PIS", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Und da mein Blick ihn fragte, quollen ihm", "tokens": ["Und", "da", "mein", "Blick", "ihn", "frag\u00b7te", ",", "quol\u00b7len", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Aus tiefster Brust die Worte: \u00bbFreue dich!", "tokens": ["Aus", "tiefs\u00b7ter", "Brust", "die", "Wor\u00b7te", ":", "\u00bb", "Freu\u00b7e", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$.", "$(", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "(und seine Stimme zittert', als er sprach)", "tokens": ["(", "und", "sei\u00b7ne", "Stim\u00b7me", "zit\u00b7tert'", ",", "als", "er", "sprach", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Ein Deutscher Kaiser ist gew\u00e4hlt am Main,", "tokens": ["Ein", "Deut\u00b7scher", "Kai\u00b7ser", "ist", "ge\u00b7w\u00e4hlt", "am", "Main", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und seine Boten sendet ihm das Reich.\u00ab", "tokens": ["Und", "sei\u00b7ne", "Bo\u00b7ten", "sen\u00b7det", "ihm", "das", "Reich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und w\u00e4hrend er von allem, wie's geschah,", "tokens": ["Und", "w\u00e4h\u00b7rend", "er", "von", "al\u00b7lem", ",", "wie's", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PIS", "$,", "PWAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mir nun Bericht gab, sieh, da schm\u00fcckten sich", "tokens": ["Mir", "nun", "Be\u00b7richt", "gab", ",", "sieh", ",", "da", "schm\u00fcck\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "NN", "VVFIN", "$,", "VVFIN", "$,", "ADV", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die alten Zackengiebel l\u00e4ngs dem Flu\u00df", "tokens": ["Die", "al\u00b7ten", "Za\u00b7cken\u00b7gie\u00b7bel", "l\u00e4ngs", "dem", "Flu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit frohen Fahnen schon, und gr\u00fc\u00dfend flog", "tokens": ["Mit", "fro\u00b7hen", "Fah\u00b7nen", "schon", ",", "und", "gr\u00fc\u00b7\u00dfend", "flog"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An manchem Schiff ein deutscher Wimpel auf", "tokens": ["An", "man\u00b7chem", "Schiff", "ein", "deut\u00b7scher", "Wim\u00b7pel", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und wallte breitentrollt im Morgenwind.", "tokens": ["Und", "wall\u00b7te", "brei\u00b7tent\u00b7rollt", "im", "Mor\u00b7gen\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und jetzt, von Turm zu Turm einfallend, scholl", "tokens": ["Und", "jetzt", ",", "von", "Turm", "zu", "Turm", "ein\u00b7fal\u00b7lend", ",", "scholl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "$,", "APPR", "NN", "APPR", "NN", "VVPP", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Glocken Chorgesang und k\u00fcndigte", "tokens": ["Der", "Glo\u00b7cken", "Chor\u00b7ge\u00b7sang", "und", "k\u00fcn\u00b7dig\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Das Fest der Palmen an. Mir aber war's,", "tokens": ["Das", "Fest", "der", "Pal\u00b7men", "an", ".", "Mir", "a\u00b7ber", "wa\u00b7r's", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$.", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Als l\u00e4utete man ein das Deutsche Reich,", "tokens": ["Als", "l\u00e4u\u00b7te\u00b7te", "man", "ein", "das", "Deut\u00b7sche", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PIS", "ART", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und das Hosanna, das in meiner Brust", "tokens": ["Und", "das", "Ho\u00b7san\u00b7na", ",", "das", "in", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "And\u00e4chtig widerklang, zwei K\u00f6nigen,", "tokens": ["An\u00b7d\u00e4ch\u00b7tig", "wi\u00b7der\u00b7klang", ",", "zwei", "K\u00f6\u00b7ni\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die ihren Einzug hielten, galt's zumal,", "tokens": ["Die", "ih\u00b7ren", "Ein\u00b7zug", "hiel\u00b7ten", ",", "galt's", "zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Dem himmlischen und dem von dieser Welt.", "tokens": ["Dem", "himm\u00b7li\u00b7schen", "und", "dem", "von", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auf Windesschwingen flog von Haus zu Haus", "tokens": ["Auf", "Win\u00b7des\u00b7schwin\u00b7gen", "flog", "von", "Haus", "zu", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Kunde weiter, da begann im Glanz", "tokens": ["Die", "Kun\u00b7de", "wei\u00b7ter", ",", "da", "be\u00b7gann", "im", "Glanz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Fr\u00fchlingssonne durch die Gassen hin", "tokens": ["Der", "Fr\u00fch\u00b7lings\u00b7son\u00b7ne", "durch", "die", "Gas\u00b7sen", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein festlich Wogen. Freunde tauschten rings", "tokens": ["Ein", "fest\u00b7lich", "Wo\u00b7gen", ".", "Freun\u00b7de", "tauschten", "rings"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$.", "NN", "VVFIN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Bewegten Handschlag, Feinde gr\u00fc\u00dften sich,", "tokens": ["Be\u00b7weg\u00b7ten", "Hand\u00b7schlag", ",", "Fein\u00b7de", "gr\u00fc\u00df\u00b7ten", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als w\u00e4re pl\u00f6tzlich aller Zwist ges\u00fchnt,", "tokens": ["Als", "w\u00e4\u00b7re", "pl\u00f6tz\u00b7lich", "al\u00b7ler", "Zwist", "ge\u00b7s\u00fchnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ADJD", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und manches Auge, das ich l\u00e4ngst im Staub", "tokens": ["Und", "man\u00b7ches", "Au\u00b7ge", ",", "das", "ich", "l\u00e4ngst", "im", "Staub"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Akten oder \u00fcberm Rechnungsbuch", "tokens": ["Der", "Ak\u00b7ten", "o\u00b7der", "\u00fc\u00b7berm", "Rech\u00b7nungs\u00b7buch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Verh\u00e4rtet glaubte, sah ich freudenfeucht.", "tokens": ["Ver\u00b7h\u00e4r\u00b7tet", "glaub\u00b7te", ",", "sah", "ich", "freu\u00b7den\u00b7feucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Denn was wir alle, sei's mit klarem Geist,", "tokens": ["Denn", "was", "wir", "al\u00b7le", ",", "sei's", "mit", "kla\u00b7rem