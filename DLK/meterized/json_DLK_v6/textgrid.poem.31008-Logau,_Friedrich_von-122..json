{"textgrid.poem.31008": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "122.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrstin, Euer Lob zu schreiben, mu\u00df ich einmal stille schweigen;", "tokens": ["F\u00fcrs\u00b7tin", ",", "Eu\u00b7er", "Lob", "zu", "schrei\u00b7ben", ",", "mu\u00df", "ich", "ein\u00b7mal", "stil\u00b7le", "schwei\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "VMFIN", "PPER", "ADV", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "So ich dem gleich nach stets steige, desto mehr ist noch zu steigen.", "tokens": ["So", "ich", "dem", "gleich", "nach", "stets", "stei\u00b7ge", ",", "des\u00b7to", "mehr", "ist", "noch", "zu", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADV", "APPR", "ADV", "VVFIN", "$,", "ADV", "ADV", "VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}}}}}