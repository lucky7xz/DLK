{"textgrid.poem.39757": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: O blau Gebirg, dort winkst du ja", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O blau Gebirg, dort winkst du ja", "tokens": ["O", "blau", "Ge\u00b7birg", ",", "dort", "winkst", "du", "ja"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit frischer J\u00fcnglingsmahnung;", "tokens": ["Mit", "fri\u00b7scher", "J\u00fcng\u00b7lings\u00b7mah\u00b7nung", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit allen Nebeln bist du da,", "tokens": ["Mit", "al\u00b7len", "Ne\u00b7beln", "bist", "du", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit aller Sonnenahnung.", "tokens": ["Mit", "al\u00b7ler", "Son\u00b7ne\u00b7nah\u00b7nung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Geheimni\u00dfreich senkt sich dein Hang", "tokens": ["Ge\u00b7heim\u00b7ni\u00df\u00b7reich", "senkt", "sich", "dein", "Hang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Voll unentdeckter Falten.", "tokens": ["Voll", "un\u00b7ent\u00b7deck\u00b7ter", "Fal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und doch \u2013 wie oft hat sie mein Gang", "tokens": ["Und", "doch", "\u2013", "wie", "oft", "hat", "sie", "mein", "Gang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "KOKOM", "ADV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit raschem Schritt gespalten!", "tokens": ["Mit", "ra\u00b7schem", "Schritt", "ge\u00b7spal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Kein Wald senkt sich in Thalesschoos,", "tokens": ["Kein", "Wald", "senkt", "sich", "in", "Tha\u00b7les\u00b7schoos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der mir nicht schon gerauschet,", "tokens": ["Der", "mir", "nicht", "schon", "ge\u00b7rau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein B\u00e4chlein springt aus Fels und Moos,", "tokens": ["Kein", "B\u00e4ch\u00b7lein", "springt", "aus", "Fels", "und", "Moos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ich nicht einst belauschet.", "tokens": ["Das", "ich", "nicht", "einst", "be\u00b7lau\u00b7schet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Kein Steg ist, der nicht unterm Tritt", "tokens": ["Kein", "Steg", "ist", ",", "der", "nicht", "un\u00b7term", "Tritt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "$,", "PRELS", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir schon gezittert h\u00e4tte,", "tokens": ["Mir", "schon", "ge\u00b7zit\u00b7tert", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein Bergpfad, den ich nicht beschritt,", "tokens": ["Kein", "Berg\u00b7pfad", ",", "den", "ich", "nicht", "be\u00b7schritt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Gipfel in der Kette.", "tokens": ["Kein", "Gip\u00b7fel", "in", "der", "Ket\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Den Zauber hab' ich l\u00e4ngst gest\u00f6rt:", "tokens": ["Den", "Zau\u00b7ber", "hab'", "ich", "l\u00e4ngst", "ge\u00b7st\u00f6rt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hab' ich dich doch beschrieben!", "tokens": ["Hab'", "ich", "dich", "doch", "be\u00b7schrie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein jedes Pl\u00e4tzchen mir geh\u00f6rt. \u2013", "tokens": ["Ein", "je\u00b7des", "Pl\u00e4tz\u00b7chen", "mir", "ge\u00b7h\u00f6rt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PIAT", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ist's nun mit dem Lieben?", "tokens": ["Wie", "ist's", "nun", "mit", "dem", "Lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich habe selbst den Jungfernkranz", "tokens": ["Ich", "ha\u00b7be", "selbst", "den", "Jung\u00b7fern\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir von dem Haupt genommen;", "tokens": ["Dir", "von", "dem", "Haupt", "ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein eh'lich Weib, das bist du ganz,", "tokens": ["Mein", "eh'\u00b7lich", "Weib", ",", "das", "bist", "du", "ganz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PDS", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun, sei auch so willkommen!", "tokens": ["Nun", ",", "sei", "auch", "so", "will\u00b7kom\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}