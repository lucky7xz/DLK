{"textgrid.poem.53545": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Schwere Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Jungfrau in der Nebenstuben \u2013", "tokens": ["Die", "Jung\u00b7frau", "in", "der", "Ne\u00b7bens\u00b7tu\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ich frage mich, was tut sie nur?", "tokens": ["ich", "fra\u00b7ge", "mich", ",", "was", "tut", "sie", "nur", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich h\u00f6r die Stimme eines Buben \u2013", "tokens": ["Ich", "h\u00f6r", "die", "Stim\u00b7me", "ei\u00b7nes", "Bu\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "so sp\u00e4t am Abend? Um elf Uhr?", "tokens": ["so", "sp\u00e4t", "am", "A\u00b7bend", "?", "Um", "elf", "Uhr", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$.", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie er mutiert! Und ihre Stimmen", "tokens": ["Wie", "er", "mu\u00b7tiert", "!", "Und", "ih\u00b7re", "Stim\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "verklingen sacht \u2013 sie murmeln leis.", "tokens": ["ver\u00b7klin\u00b7gen", "sacht", "\u2013", "sie", "mur\u00b7meln", "leis", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$(", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bin ich der Zeuge einer schlimmen", "tokens": ["Bin", "ich", "der", "Zeu\u00b7ge", "ei\u00b7ner", "schlim\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verbrechertat? Wer wei\u00df! wer wei\u00df!", "tokens": ["Ver\u00b7bre\u00b7cher\u00b7tat", "?", "Wer", "wei\u00df", "!", "wer", "wei\u00df", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VVFIN", "$.", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie spricht ihm g\u00fctig zu. Belehrend", "tokens": ["Sie", "spricht", "ihm", "g\u00fc\u00b7tig", "zu", ".", "Be\u00b7leh\u00b7rend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ert\u00f6nt ihr lieblicher Sopran.", "tokens": ["er\u00b7t\u00f6nt", "ihr", "lieb\u00b7li\u00b7cher", "Sop\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er lacht: \u00bbJawohl!\u00ab Dies ist erschwerend!", "tokens": ["Er", "lacht", ":", "\u00bb", "Ja\u00b7wohl", "!", "\u00ab", "Dies", "ist", "er\u00b7schwe\u00b7rend", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ITJ", "$.", "$(", "PDS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was wird dem Knaben nur getan?", "tokens": ["Was", "wird", "dem", "Kna\u00b7ben", "nur", "ge\u00b7tan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sind das nicht halberstickte K\u00fcsse?", "tokens": ["Sind", "das", "nicht", "hal\u00b7ber\u00b7stick\u00b7te", "K\u00fcs\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich frag sie sp\u00e4ter, was sie treibt . . .", "tokens": ["Ich", "frag", "sie", "sp\u00e4\u00b7ter", ",", "was", "sie", "treibt", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "PRELS", "PPER", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sagt: \u00bbDie geistigen Gen\u00fcsse,", "tokens": ["Sie", "sagt", ":", "\u00bb", "Die", "geis\u00b7ti\u00b7gen", "Ge\u00b7n\u00fcs\u00b7se", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "sie bringen nichts als K\u00fcmmernisse.", "tokens": ["sie", "brin\u00b7gen", "nichts", "als", "K\u00fcm\u00b7mer\u00b7nis\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist das einzige, was mir bleibt!\u00ab", "tokens": ["Es", "ist", "das", "ein\u00b7zi\u00b7ge", ",", "was", "mir", "bleibt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "PWS", "PPER", "VVFIN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}