{"textgrid.poem.37268": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[es flog einmal ein muntres Fliegel]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es flog einmal ein muntres Fliegel", "tokens": ["Es", "flog", "ein\u00b7mal", "ein", "mun\u00b7tres", "Flie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu einem vollen Honigtiegel.", "tokens": ["Zu", "ei\u00b7nem", "vol\u00b7len", "Ho\u00b7nig\u00b7tie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da tunkt es mit Zufriedenheit", "tokens": ["Da", "tunkt", "es", "mit", "Zu\u00b7frie\u00b7den\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den R\u00fcssel in die S\u00fc\u00dfigkeit.", "tokens": ["Den", "R\u00fcs\u00b7sel", "in", "die", "S\u00fc\u00b7\u00dfig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nachdem es dann genug geschleckt,", "tokens": ["Nach\u00b7dem", "es", "dann", "ge\u00b7nug", "ge\u00b7schleckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat es die Fl\u00fcglein ausgereckt", "tokens": ["Hat", "es", "die", "Fl\u00fcg\u00b7lein", "aus\u00b7ge\u00b7reckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und m\u00f6chte sich nach oben schwingen.", "tokens": ["Und", "m\u00f6ch\u00b7te", "sich", "nach", "o\u00b7ben", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Allein das Bein im Honigseim", "tokens": ["Al\u00b7lein", "das", "Bein", "im", "Ho\u00b7ni\u00b7gseim"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sitzt fest als wie in Vogelleim.", "tokens": ["Sitzt", "fest", "als", "wie", "in", "Vo\u00b7gel\u00b7leim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KOKOM", "KOKOM", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nun f\u00e4ngt das Fliegel an zu singen:", "tokens": ["Nun", "f\u00e4ngt", "das", "Flie\u00b7gel", "an", "zu", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ach, lieber Himmel, mach mich frei", "tokens": ["Ach", ",", "lie\u00b7ber", "Him\u00b7mel", ",", "mach", "mich", "frei"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "NN", "$,", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Aus dieser s\u00fc\u00dfen Sklaverei!", "tokens": ["Aus", "die\u00b7ser", "s\u00fc\u00b7\u00dfen", "Skla\u00b7ve\u00b7rei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Freund von mir, der dieses sah,", "tokens": ["Ein", "Freund", "von", "mir", ",", "der", "die\u00b7ses", "sah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "PRELS", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der seufzte tief und rief: Ja, ja!", "tokens": ["Der", "seufz\u00b7te", "tief", "und", "rief", ":", "Ja", ",", "ja", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "VVFIN", "$.", "PTKANT", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}