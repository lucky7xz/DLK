{"dta.poem.7681": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast, o schwacher Mensch, alswie an jedem Tage,", "tokens": ["Du", "hast", ",", "o", "schwa\u00b7cher", "Mensch", ",", "als\u00b7wie", "an", "je\u00b7dem", "Ta\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "FM", "ADJA", "NN", "$,", "KON", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein anderes Gem\u00fcth in jeder andern Lage.", "tokens": ["Ein", "an\u00b7de\u00b7res", "Ge\u00b7m\u00fcth", "in", "je\u00b7der", "an\u00b7dern", "La\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das hab' ich an mir selbst auf mancher Reis' erfahren,", "tokens": ["Das", "hab'", "ich", "an", "mir", "selbst", "auf", "man\u00b7cher", "Reis'", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "PPER", "ADV", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df anders mir zu Muth im Gehn war als im Fahren.", "tokens": ["Da\u00df", "an\u00b7ders", "mir", "zu", "Muth", "im", "Gehn", "war", "als", "im", "Fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "APPR", "NN", "APPRART", "NN", "VAFIN", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Im Fahren war ich stolz, geneigt herabzusehn,", "tokens": ["Im", "Fah\u00b7ren", "war", "ich", "stolz", ",", "ge\u00b7neigt", "her\u00b7ab\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verachtend alle die ich sah zu Fu\u00dfe gehn.", "tokens": ["Ver\u00b7ach\u00b7tend", "al\u00b7le", "die", "ich", "sah", "zu", "Fu\u00b7\u00dfe", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PIAT", "PRELS", "PPER", "VVFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Im Gehen war ich stolz, verachtend, doch nach oben,", "tokens": ["Im", "Ge\u00b7hen", "war", "ich", "stolz", ",", "ver\u00b7ach\u00b7tend", ",", "doch", "nach", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "$,", "VVPP", "$,", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die \u00fcber mich zu Ro\u00df und Wagen sich erhoben.", "tokens": ["Die", "\u00fc\u00b7ber", "mich", "zu", "Ro\u00df", "und", "Wa\u00b7gen", "sich", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "APPR", "NN", "KON", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und wenn es besser gieng, so trat als Weggeselle", "tokens": ["Und", "wenn", "es", "bes\u00b7ser", "gieng", ",", "so", "trat", "als", "Weg\u00b7ge\u00b7sel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "$,", "ADV", "VVFIN", "KOUS", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dort Gro\u00dfmuth, Demuth hier, an Hoch- und Unmuths Stelle.", "tokens": ["Dort", "Gro\u00df\u00b7muth", ",", "De\u00b7muth", "hier", ",", "an", "Hoch", "und", "Un\u00b7muths", "Stel\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "ADV", "$,", "APPR", "TRUNC", "KON", "NN", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Das h\u00f6chste doch, wozu wir dort und hier es brachten,", "tokens": ["Das", "h\u00f6chs\u00b7te", "doch", ",", "wo\u00b7zu", "wir", "dort", "und", "hier", "es", "brach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,", "PWAV", "PPER", "ADV", "KON", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "War Selbzufriedenheit ohn' andre zu verachten.", "tokens": ["War", "Selb\u00b7zu\u00b7frie\u00b7den\u00b7heit", "ohn'", "and\u00b7re", "zu", "ver\u00b7ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PIS", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}