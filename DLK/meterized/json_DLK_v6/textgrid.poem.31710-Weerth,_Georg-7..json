{"textgrid.poem.31710": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Werthchen, das gr\u00fcne Eiland,", "tokens": ["Das", "Werth\u00b7chen", ",", "das", "gr\u00fc\u00b7ne", "Ei\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das liegt im Rhein, bei der Stadt,", "tokens": ["Das", "liegt", "im", "Rhein", ",", "bei", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NE", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das kennt wohl jeder, der weiland", "tokens": ["Das", "kennt", "wohl", "je\u00b7der", ",", "der", "wei\u00b7land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "PIS", "$,", "PRELS", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu K\u00f6ln geliebet hat.", "tokens": ["Zu", "K\u00f6ln", "ge\u00b7lie\u00b7bet", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dort sa\u00dfen wir oft und lallten", "tokens": ["Dort", "sa\u00b7\u00dfen", "wir", "oft", "und", "lall\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Viel fromme Abendges\u00e4ng;", "tokens": ["Viel", "from\u00b7me", "A\u00b7bend\u00b7ge\u00b7s\u00e4ng", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Domesglocken schallten", "tokens": ["Die", "Do\u00b7mes\u00b7glo\u00b7cken", "schall\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Her\u00fcber mit ernstem Gekl\u00e4ng.", "tokens": ["Her\u00b7\u00fc\u00b7ber", "mit", "erns\u00b7tem", "Ge\u00b7kl\u00e4ng", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Die vollen R\u00f6mer blickten", "tokens": ["Die", "vol\u00b7len", "R\u00f6\u00b7mer", "blick\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Smaragdenen Augs uns an;", "tokens": ["Sma\u00b7rag\u00b7de\u00b7nen", "Augs", "uns", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die k\u00f6lnischen Banner nickten", "tokens": ["Die", "k\u00f6l\u00b7ni\u00b7schen", "Ban\u00b7ner", "nick\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von T\u00fcrmen und Altan.", "tokens": ["Von", "T\u00fcr\u00b7men", "und", "Al\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die k\u00f6lnischen Banner winken", "tokens": ["Die", "k\u00f6l\u00b7ni\u00b7schen", "Ban\u00b7ner", "win\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit rot und wei\u00dfem Schein,", "tokens": ["Mit", "rot", "und", "wei\u00b7\u00dfem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und die Leute in K\u00f6ln, die trinken", "tokens": ["Und", "die", "Leu\u00b7te", "in", "K\u00f6ln", ",", "die", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NE", "$,", "PRELS", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Viel roten und wei\u00dfen Wein.", "tokens": ["Viel", "ro\u00b7ten", "und", "wei\u00b7\u00dfen", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}