{"textgrid.poem.65157": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als d\u00e4mmernd noch das Leben vor mir lag,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als d\u00e4mmernd noch das Leben vor mir lag,", "tokens": ["Als", "d\u00e4m\u00b7mernd", "noch", "das", "Le\u00b7ben", "vor", "mir", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Herz noch nichts errungen, nichts verloren,", "tokens": ["Mein", "Herz", "noch", "nichts", "er\u00b7run\u00b7gen", ",", "nichts", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PIS", "VVPP", "$,", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht ahnt' ich da, da\u00df mir an diesem Tag", "tokens": ["Nicht", "ahnt'", "ich", "da", ",", "da\u00df", "mir", "an", "die\u00b7sem", "Tag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein bestes Kleinod ward zur Welt geboren!", "tokens": ["Mein", "bes\u00b7tes", "Klei\u00b7nod", "ward", "zur", "Welt", "ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nicht ahnte ich, da\u00df heut' der hellste Stern", "tokens": ["Nicht", "ahn\u00b7te", "ich", ",", "da\u00df", "heut'", "der", "hells\u00b7te", "Stern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "An meinem Horizonte aufgegangen,", "tokens": ["An", "mei\u00b7nem", "Ho\u00b7ri\u00b7zon\u00b7te", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df meines Wesens innerlichster Kern", "tokens": ["Da\u00df", "mei\u00b7nes", "We\u00b7sens", "in\u00b7ner\u00b7lichs\u00b7ter", "Kern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den vollen Abschlu\u00df heute erst empfangen.", "tokens": ["Den", "vol\u00b7len", "Ab\u00b7schlu\u00df", "heu\u00b7te", "erst", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich ahnt' es nicht; erst jetzt erkenn' ich's ganz!", "tokens": ["Ich", "ahnt'", "es", "nicht", ";", "erst", "jetzt", "er\u00b7kenn'", "ich's", "ganz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "ADV", "ADV", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur eines kann ich auch noch jetzt nicht fassen:", "tokens": ["Nur", "ei\u00b7nes", "kann", "ich", "auch", "noch", "jetzt", "nicht", "fas\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "PPER", "ADV", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df deiner Liebe heller Strahlenkranz", "tokens": ["Da\u00df", "dei\u00b7ner", "Lie\u00b7be", "hel\u00b7ler", "Strah\u00b7len\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf meine Stirn sich mochte niederlassen.", "tokens": ["Auf", "mei\u00b7ne", "Stirn", "sich", "moch\u00b7te", "nie\u00b7der\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es hei\u00dft ja doch, da\u00df nur um Gleich und Gleich", "tokens": ["Es", "hei\u00dft", "ja", "doch", ",", "da\u00df", "nur", "um", "Gleich", "und", "Gleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KOUS", "ADV", "APPR", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Bande sich wahrhaft'ger Freundschaft weben.", "tokens": ["Die", "Ban\u00b7de", "sich", "wahr\u00b7haft'\u00b7ger", "Freund\u00b7schaft", "we\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du aber bist so reich, so \u00fcberreich,", "tokens": ["Du", "a\u00b7ber", "bist", "so", "reich", ",", "so", "\u00fc\u00b7berr\u00b7eich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und ich, \u2013 \u2013 was hab' ich Arme dir zu geben?", "tokens": ["Und", "ich", ",", "\u2013", "\u2013", "was", "hab'", "ich", "Ar\u00b7me", "dir", "zu", "ge\u00b7ben", "?"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "$(", "$(", "PWS", "VAFIN", "PPER", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nichts als mich selbst! doch diese Gabe schafft", "tokens": ["Nichts", "als", "mich", "selbst", "!", "doch", "die\u00b7se", "Ga\u00b7be", "schafft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "PPER", "ADV", "$.", "ADV", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dir Sorgen nur und immer neue M\u00fchen!", "tokens": ["Dir", "Sor\u00b7gen", "nur", "und", "im\u00b7mer", "neu\u00b7e", "M\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "KON", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn st\u00fctzen mu\u00dft du mich mit deiner Kraft,", "tokens": ["Denn", "st\u00fct\u00b7zen", "mu\u00dft", "du", "mich", "mit", "dei\u00b7ner", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein b\u00f6ses altes Kind zum Guten ziehen.", "tokens": ["Dein", "b\u00f6\u00b7ses", "al\u00b7tes", "Kind", "zum", "Gu\u00b7ten", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du mu\u00dft, bald ernst und streng, und bald gelind,", "tokens": ["Du", "mu\u00dft", ",", "bald", "ernst", "und", "streng", ",", "und", "bald", "ge\u00b7lind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ADV", "ADJD", "KON", "VVFIN", "$,", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Hier raten, tr\u00f6sten, strafen dort und wehren,", "tokens": ["Hier", "ra\u00b7ten", ",", "tr\u00f6s\u00b7ten", ",", "stra\u00b7fen", "dort", "und", "weh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ADV", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und die Gedanken, die das Leben sind,", "tokens": ["Und", "die", "Ge\u00b7dan\u00b7ken", ",", "die", "das", "Le\u00b7ben", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den erdgebund'nen Geist erst denken lehren.", "tokens": ["Den", "erd\u00b7ge\u00b7bun\u00b7d'\u00b7nen", "Geist", "erst", "den\u00b7ken", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Tief schmerzlich \u00fcberkommt mich's manchesmal:", "tokens": ["Tief", "schmerz\u00b7lich", "\u00fc\u00b7ber\u00b7kommt", "mich's", "man\u00b7ches\u00b7mal", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O da\u00df ich fr\u00fcher, fr\u00fcher dich gefunden,", "tokens": ["O", "da\u00df", "ich", "fr\u00fc\u00b7her", ",", "fr\u00fc\u00b7her", "dich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ADJD", "$,", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als ungetr\u00fcbt noch meines Auges Strahl,", "tokens": ["Als", "un\u00b7ge\u00b7tr\u00fcbt", "noch", "mei\u00b7nes", "Au\u00b7ges", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und meine Brust noch rein von Schuld und Wunden!", "tokens": ["Und", "mei\u00b7ne", "Brust", "noch", "rein", "von", "Schuld", "und", "Wun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann w\u00e4re nie des Samums gl\u00fcher Hauch", "tokens": ["Dann", "w\u00e4\u00b7re", "nie", "des", "Sa\u00b7mums", "gl\u00fc\u00b7her", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vergiftend \u00fcber mich hinweggegangen!", "tokens": ["Ver\u00b7gif\u00b7tend", "\u00fc\u00b7ber", "mich", "hin\u00b7weg\u00b7ge\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich gliche nicht dem blitzversengten Strauch,", "tokens": ["Ich", "gli\u00b7che", "nicht", "dem", "blitz\u00b7ver\u00b7seng\u00b7ten", "Strauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und k\u00f6nnte geben, statt nur zu empfangen!", "tokens": ["Und", "k\u00f6nn\u00b7te", "ge\u00b7ben", ",", "statt", "nur", "zu", "emp\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "$,", "KOUI", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch, hat voreinst nicht aus des Heilands Mund", "tokens": ["Doch", ",", "hat", "vor\u00b7e\u00b7inst", "nicht", "aus", "des", "Hei\u00b7lands", "Mund"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "VAFIN", "ADV", "PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die schmerzenm\u00fcde Welt dies Wort vernommen:", "tokens": ["Die", "schmer\u00b7zen\u00b7m\u00fc\u00b7de", "Welt", "dies", "Wort", "ver\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbf\u00fcr jene nicht, die kr\u00e4ftig und gesund,", "tokens": ["\u00bb", "f\u00fcr", "je\u00b7ne", "nicht", ",", "die", "kr\u00e4f\u00b7tig", "und", "ge\u00b7sund", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PDS", "PTKNEG", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nein! f\u00fcr die Kranken ist der Arzt gekommen\u00ab?", "tokens": ["Nein", "!", "f\u00fcr", "die", "Kran\u00b7ken", "ist", "der", "Arzt", "ge\u00b7kom\u00b7men", "\u00ab", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$.", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du treuer Arzt! so hast, als, w\u00fcst und wirr,", "tokens": ["Du", "treu\u00b7er", "Arzt", "!", "so", "hast", ",", "als", ",", "w\u00fcst", "und", "wirr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "ADV", "VAFIN", "$,", "KOUS", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Fieber mich der Leidenschaft bezwungen,", "tokens": ["Das", "Fie\u00b7ber", "mich", "der", "Lei\u00b7den\u00b7schaft", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du mich gepflegt, und liebest nun in mir", "tokens": ["Du", "mich", "ge\u00b7pflegt", ",", "und", "lie\u00b7best", "nun", "in", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PRF", "VVPP", "$,", "KON", "VVFIN", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Beute, die dem Tod du abgerungen!", "tokens": ["Die", "Beu\u00b7te", ",", "die", "dem", "Tod", "du", "ab\u00b7ge\u00b7run\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}