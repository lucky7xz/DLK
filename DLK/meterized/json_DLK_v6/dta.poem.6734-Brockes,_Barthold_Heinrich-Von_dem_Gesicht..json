{"dta.poem.6734": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein, wie fuss't man sich auf solchen eitlen Grillen?", "tokens": ["Al\u00b7lein", ",", "wie", "fuss't", "man", "sich", "auf", "sol\u00b7chen", "eit\u00b7len", "Gril\u00b7len", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "VVFIN", "PIS", "PRF", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum wil man das Gantz der L\u00fcffte", "tokens": ["Wa\u00b7rum", "wil", "man", "das", "Gantz", "der", "L\u00fcff\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PIS", "ART", "NN", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mit fliegenden Chymeren f\u00fcllen?", "tokens": ["Mit", "flie\u00b7gen\u00b7den", "Chy\u00b7me\u00b7ren", "f\u00fcl\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wer solte doch die leichten D\u00fcffte", "tokens": ["Wer", "sol\u00b7te", "doch", "die", "leich\u00b7ten", "D\u00fcff\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der unf\u00fchlbahren Bilder f\u00fchren?", "tokens": ["Der", "un\u00b7f\u00fchl\u00b7bah\u00b7ren", "Bil\u00b7der", "f\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wer sie, so in der N\u00e4h\u2019 als in der Fern, regieren?", "tokens": ["Wer", "sie", ",", "so", "in", "der", "N\u00e4h'", "als", "in", "der", "Fern", ",", "re\u00b7gie\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "$,", "ADV", "APPR", "ART", "NN", "KOKOM", "APPR", "ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und wie kan doch ein Werck, das voll Zerbrechlichkeiten,", "tokens": ["Und", "wie", "kan", "doch", "ein", "Werck", ",", "das", "voll", "Zer\u00b7brech\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "ADV", "ART", "NN", "$,", "PRELS", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bis ins Gehirn sich einen Weg bereiten,", "tokens": ["Bis", "ins", "Ge\u00b7hirn", "sich", "ei\u00b7nen", "Weg", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ohn\u2019 aus einander sich zu setzen", "tokens": ["Ohn'", "aus", "ein\u00b7an\u00b7der", "sich", "zu", "set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PRF", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und seine Theile zu verletzen?", "tokens": ["Und", "sei\u00b7ne", "Thei\u00b7le", "zu", "ver\u00b7let\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}