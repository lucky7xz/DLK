{"textgrid.poem.45166": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[die Staatsvertr\u00e4ge sind Papier]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Staatsvertr\u00e4ge sind Papier,", "tokens": ["Die", "Staats\u00b7ver\u00b7tr\u00e4\u00b7ge", "sind", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum ist auch ein Kurs ihnen eigen,", "tokens": ["Drum", "ist", "auch", "ein", "Kurs", "ih\u00b7nen", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Krakauer gehen sehr mit Verlust,", "tokens": ["Die", "Kra\u00b7kau\u00b7er", "ge\u00b7hen", "sehr", "mit", "Ver\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Doch die von Ferrara steigen.", "tokens": ["Doch", "die", "von", "Fer\u00b7ra\u00b7ra", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NE", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}