{"textgrid.poem.36842": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Erstes Lied", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tage verstreichen an Tage; noch immer im heiteren Zankle", "tokens": ["Ta\u00b7ge", "ver\u00b7strei\u00b7chen", "an", "Ta\u00b7ge", ";", "noch", "im\u00b7mer", "im", "hei\u00b7te\u00b7ren", "Zank\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN", "$.", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "H\u00e4lt mich die Sonne, die Lust, h\u00e4lt mich die Laune zur\u00fcck.", "tokens": ["H\u00e4lt", "mich", "die", "Son\u00b7ne", ",", "die", "Lust", ",", "h\u00e4lt", "mich", "die", "Lau\u00b7ne", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Runzle die Stirn, dich ergreife der Ernst altgriechischer Vorwelt,", "tokens": ["Runz\u00b7le", "die", "Stirn", ",", "dich", "er\u00b7grei\u00b7fe", "der", "Ernst", "alt\u00b7grie\u00b7chi\u00b7scher", "Vor\u00b7welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-++-+-", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Dank es dem Gl\u00fcck, dich umgl\u00e4nzt endlich trinakrische Luft.", "tokens": ["Dank", "es", "dem", "Gl\u00fcck", ",", "dich", "um\u00b7gl\u00e4nzt", "end\u00b7lich", "tri\u00b7nak\u00b7ri\u00b7sche", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Der Pelorias hier und die sandige Zunge des Faro,", "tokens": ["Der", "Pe\u00b7lo\u00b7ri\u00b7as", "hier", "und", "die", "san\u00b7di\u00b7ge", "Zun\u00b7ge", "des", "Fa\u00b7ro", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Ueber des Meeres Azur lockt dich Kalabrien dort.", "tokens": ["Ue\u00b7ber", "des", "Mee\u00b7res", "A\u00b7zur", "lockt", "dich", "Ka\u00b7la\u00b7bri\u00b7en", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "VVFIN", "PPER", "NE", "ADV", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.7": {"text": "Hier am Horne des Stiers durchschwamm Herakles die Wogen,", "tokens": ["Hier", "am", "Hor\u00b7ne", "des", "Stiers", "durch\u00b7schwamm", "Her\u00b7ak\u00b7les", "die", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "--+--+---+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Hier mit Strudel und Tod k\u00e4mpfte der Dulder Uly\u00df.", "tokens": ["Hier", "mit", "Stru\u00b7del", "und", "Tod", "k\u00e4mpf\u00b7te", "der", "Dul\u00b7der", "U\u00b7ly\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "NE", "$."], "meter": "--+--+---+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Still, o nordischer Freund, und z\u00fcrne mir nicht, ich gestehe,", "tokens": ["Still", ",", "o", "nor\u00b7di\u00b7scher", "Freund", ",", "und", "z\u00fcr\u00b7ne", "mir", "nicht", ",", "ich", "ge\u00b7ste\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "FM", "ADJA", "NN", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Manche Scylla hat mir, manche Charybdis gedroht.", "tokens": ["Man\u00b7che", "Scyl\u00b7la", "hat", "mir", ",", "man\u00b7che", "Cha\u00b7ryb\u00b7dis", "ge\u00b7droht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "PPER", "$,", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.11": {"text": "Zwinge zum Ernst mich nicht, dem Gem\u00fcth verg\u00f6nne die Freiheit,", "tokens": ["Zwin\u00b7ge", "zum", "Ernst", "mich", "nicht", ",", "dem", "Ge\u00b7m\u00fcth", "ver\u00b7g\u00f6n\u00b7ne", "die", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "PTKNEG", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Bleibe dem denkenden Ernst, bleibe dem Scherze sein Recht.", "tokens": ["Blei\u00b7be", "dem", "den\u00b7ken\u00b7den", "Ernst", ",", "blei\u00b7be", "dem", "Scher\u00b7ze", "sein", "Recht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.13": {"text": "Sie gehorchen dir nicht, du bist ihr Diener, ihr Priester,", "tokens": ["Sie", "ge\u00b7hor\u00b7chen", "dir", "nicht", ",", "du", "bist", "ihr", "Die\u00b7ner", ",", "ihr", "Pries\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "H\u00f6heren folgst du, sie sind dir der begeisternde Gott.", "tokens": ["H\u00f6\u00b7he\u00b7ren", "folgst", "du", ",", "sie", "sind", "dir", "der", "be\u00b7geis\u00b7tern\u00b7de", "Gott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-----+-+-+--+", "measure": "iambic.penta.chol"}, "line.15": {"text": "So das Heiterste nur, das Fr\u00f6hlichste l\u00e4chelt mir heute,", "tokens": ["So", "das", "Hei\u00b7ters\u00b7te", "nur", ",", "das", "Fr\u00f6h\u00b7lichs\u00b7te", "l\u00e4\u00b7chelt", "mir", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADV", "$,", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Und ein seltenes Gl\u00fcck wagt nur ein Thor zu verschm\u00e4hn", "tokens": ["Und", "ein", "sel\u00b7te\u00b7nes", "Gl\u00fcck", "wagt", "nur", "ein", "Thor", "zu", "ver\u00b7schm\u00e4hn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "PTKZU", "VVINF"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Hat ja den nordischen Gast am Strand schon Amor empfangen,", "tokens": ["Hat", "ja", "den", "nor\u00b7di\u00b7schen", "Gast", "am", "Strand", "schon", "A\u00b7mor", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "ADV", "NE", "VVPP", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Als er zum erstenmal Sikulerboden betrat.", "tokens": ["Als", "er", "zum", "ers\u00b7ten\u00b7mal", "Si\u00b7ku\u00b7ler\u00b7bo\u00b7den", "be\u00b7trat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.19": {"text": "Ist's ein Wunder, das ich der Chalcidenser und Samer,", "tokens": ["Ist's", "ein", "Wun\u00b7der", ",", "das", "ich", "der", "Chal\u00b7ci\u00b7den\u00b7ser", "und", "Sa\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Spartas wenig und Roms oder Karthagos gedacht?", "tokens": ["Spar\u00b7tas", "we\u00b7nig", "und", "Roms", "o\u00b7der", "Kar\u00b7tha\u00b7gos", "ge\u00b7dacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KON", "NE", "KON", "NE", "VVPP", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Denn ans Fenster f\u00fchrt mir der Schalk ein liebliches M\u00e4dchen;", "tokens": ["Denn", "ans", "Fens\u00b7ter", "f\u00fchrt", "mir", "der", "Schalk", "ein", "lieb\u00b7li\u00b7ches", "M\u00e4d\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Erst nur Blicke, doch bald folgt der verstohlene Gru\u00df.", "tokens": ["Erst", "nur", "Bli\u00b7cke", ",", "doch", "bald", "folgt", "der", "ver\u00b7stoh\u00b7le\u00b7ne", "Gru\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$,", "ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Und man redet mit Zeichen, man redet mit Augen und H\u00e4nden;", "tokens": ["Und", "man", "re\u00b7det", "mit", "Zei\u00b7chen", ",", "man", "re\u00b7det", "mit", "Au\u00b7gen", "und", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "NN", "$,", "PIS", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.24": {"text": "Andere Sprache verg\u00f6nnt lauschende Nachbarschaft nicht.", "tokens": ["An\u00b7de\u00b7re", "Spra\u00b7che", "ver\u00b7g\u00f6nnt", "lau\u00b7schen\u00b7de", "Nach\u00b7bar\u00b7schaft", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJA", "NN", "PTKNEG", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Kannst du lesen, mein holdestes Kind? so frag' ich mit Zeichen", "tokens": ["Kannst", "du", "le\u00b7sen", ",", "mein", "hol\u00b7des\u00b7tes", "Kind", "?", "so", "frag'", "ich", "mit", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "PPOSAT", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.26": {"text": "\u00bbja\u00ab, ist die Antwort. Im Nu liegt auch ein Briefchen bereit.", "tokens": ["\u00bb", "ja", "\u00ab", ",", "ist", "die", "Ant\u00b7wort", ".", "Im", "Nu", "liegt", "auch", "ein", "Brief\u00b7chen", "be\u00b7reit", "."], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$(", "$,", "VAFIN", "ART", "NN", "$.", "APPRART", "ADV", "VVFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Und beschwert mit dem Kiesel, damit es der Wind nicht entf\u00fchre,", "tokens": ["Und", "be\u00b7schwert", "mit", "dem", "Kie\u00b7sel", ",", "da\u00b7mit", "es", "der", "Wind", "nicht", "ent\u00b7f\u00fch\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.28": {"text": "Fliegt es ins Fenster und schnell hebt sie err\u00f6thend es auf.", "tokens": ["Fliegt", "es", "ins", "Fens\u00b7ter", "und", "schnell", "hebt", "sie", "er\u00b7r\u00f6\u00b7thend", "es", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "KON", "ADJD", "VVFIN", "PPER", "VVPP", "PPER", "PTKVZ", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.29": {"text": "Goldne Minuten erwartender Angst, und die z\u00e4rtliche Antwort", "tokens": ["Gold\u00b7ne", "Mi\u00b7nu\u00b7ten", "er\u00b7war\u00b7ten\u00b7der", "Angst", ",", "und", "die", "z\u00e4rt\u00b7li\u00b7che", "Ant\u00b7wort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Eilt den gef\u00e4hrlichen Pfad schon in mein Fenster herein.", "tokens": ["Eilt", "den", "ge\u00b7f\u00e4hr\u00b7li\u00b7chen", "Pfad", "schon", "in", "mein", "Fens\u00b7ter", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.31": {"text": "Worte der Liebe. Wie bin ich dir gut, doch im Hause, mein Liebling,", "tokens": ["Wor\u00b7te", "der", "Lie\u00b7be", ".", "Wie", "bin", "ich", "dir", "gut", ",", "doch", "im", "Hau\u00b7se", ",", "mein", "Lieb\u00b7ling", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "PWAV", "VAFIN", "PPER", "PPER", "ADJD", "$,", "ADV", "APPRART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+-+-+--+-", "measure": "dactylic.di.plus"}, "line.32": {"text": "Darf ich dich jetzt noch nicht sehn, aber heut Abend gewi\u00df.", "tokens": ["Darf", "ich", "dich", "jetzt", "noch", "nicht", "sehn", ",", "a\u00b7ber", "heut", "A\u00b7bend", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADV", "PTKNEG", "VVINF", "$,", "ADV", "ADV", "NN", "ADV", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.33": {"text": "Zweimal noch durch die L\u00fcfte geleitet Amor die Briefe;", "tokens": ["Zwei\u00b7mal", "noch", "durch", "die", "L\u00fcf\u00b7te", "ge\u00b7lei\u00b7tet", "A\u00b7mor", "die", "Brie\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVPP", "NE", "ART", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.34": {"text": "Und der Abend, er naht schon mit begl\u00fcckendem Schritt.", "tokens": ["Und", "der", "A\u00b7bend", ",", "er", "naht", "schon", "mit", "be\u00b7gl\u00fc\u00b7cken\u00b7dem", "Schritt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.35": {"text": "Bl\u00e4uer rollet des Meeres Krystall und in gl\u00fchendem Dufte", "tokens": ["Bl\u00e4u\u00b7er", "rol\u00b7let", "des", "Mee\u00b7res", "Krys\u00b7tall", "und", "in", "gl\u00fc\u00b7hen\u00b7dem", "Duf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "KON", "APPR", "ADJA", "NN"], "meter": "+-+--+-++--+--+-", "measure": "trochaic.septa.relaxed"}, "line.36": {"text": "Schimmert das holde Gebirg, schimmert Kalabrien schon.", "tokens": ["Schim\u00b7mert", "das", "hol\u00b7de", "Ge\u00b7birg", ",", "schim\u00b7mert", "Ka\u00b7la\u00b7bri\u00b7en", "schon", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "NE", "ADV", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.37": {"text": "Und die d\u00e4mmernde Nacht, bald deckt ihr Schleier die Erde,", "tokens": ["Und", "die", "d\u00e4m\u00b7mern\u00b7de", "Nacht", ",", "bald", "deckt", "ihr", "Schlei\u00b7er", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "ana