{"dta.poem.1681": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Dergleichen Ahnen Zahl/ von denen sie entsprissen/", "tokens": ["Derg\u00b7lei\u00b7chen", "Ah\u00b7nen", "Zahl", "/", "von", "de\u00b7nen", "sie", "ent\u00b7spris\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "NN", "$(", "APPR", "PRELS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Eltern gleiche Gunst/ das gleiche Vaterland/", "tokens": ["Der", "El\u00b7tern", "glei\u00b7che", "Gunst", "/", "das", "glei\u00b7che", "Va\u00b7ter\u00b7land", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In welchem sie gezeugt/ dergleichen Neigung Band", "tokens": ["In", "wel\u00b7chem", "sie", "ge\u00b7zeugt", "/", "derg\u00b7lei\u00b7chen", "Nei\u00b7gung", "Band"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "$(", "PIS", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von Jugend auff verkn\u00fcpfft/ und niemahls abgerissen/", "tokens": ["Von", "Ju\u00b7gend", "auff", "ver\u00b7kn\u00fcpfft", "/", "und", "nie\u00b7mahls", "ab\u00b7ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "VVFIN", "$(", "KON", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der gleich-gearte Sinn/ die Gleichheit der Gewissen/", "tokens": ["Der", "gleich\u00b7ge\u00b7ar\u00b7te", "Sinn", "/", "die", "Gleich\u00b7heit", "der", "Ge\u00b7wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der gleichen Tugend-Zier/ die sich in Beyden fand/", "tokens": ["Der", "glei\u00b7chen", "Tu\u00b7gen\u00b7dZier", "/", "die", "sich", "in", "Bey\u00b7den", "fand", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Versezt euch/ Edles Paar/ numehr in gleichen Stand.", "tokens": ["Ver\u00b7sezt", "euch", "/", "Ed\u00b7les", "Paar", "/", "nu\u00b7mehr", "in", "glei\u00b7chen", "Stand", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ADJA", "NN", "$(", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was wolte denn hieraus/ als lauter Segen/ fliessen?", "tokens": ["Was", "wol\u00b7te", "denn", "hier\u00b7aus", "/", "als", "lau\u00b7ter", "Se\u00b7gen", "/", "flies\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "KON", "PAV", "$(", "KOKOM", "PIAT", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Schaut wie die Venus dort am Himmel blinckt und lacht!", "tokens": ["Schaut", "wie", "die", "Ve\u00b7nus", "dort", "am", "Him\u00b7mel", "blinckt", "und", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "ADV", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie spielet gegen euch mit Blicken in die Wette/", "tokens": ["Sie", "spie\u00b7let", "ge\u00b7gen", "euch", "mit", "Bli\u00b7cken", "in", "die", "Wet\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie eilet nach der Ruh/ und leuchtet euch zu Bette.", "tokens": ["Sie", "ei\u00b7let", "nach", "der", "Ruh", "/", "und", "leuch\u00b7tet", "euch", "zu", "Bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Geht/ gebet eurem Stand in Freuden gutte Nacht/", "tokens": ["Geht", "/", "ge\u00b7bet", "eu\u00b7rem", "Stand", "in", "Freu\u00b7den", "gut\u00b7te", "Nacht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich w\u00fcnsche/ da\u00df sich bald m\u00f6g eures gleichen finden/", "tokens": ["Ich", "w\u00fcn\u00b7sche", "/", "da\u00df", "sich", "bald", "m\u00f6g", "eu\u00b7res", "glei\u00b7chen", "fin\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PRF", "ADV", "VMFIN", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und eure gleiche Glutt ie mehr und mehr entz\u00fcnden.", "tokens": ["Und", "eu\u00b7re", "glei\u00b7che", "Glutt", "ie", "mehr", "und", "mehr", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}