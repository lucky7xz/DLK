{"textgrid.poem.40754": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbaufbrechen wieder Ungarns alte Wunden!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbaufbrechen wieder Ungarns alte Wunden!", "tokens": ["\u00bb", "auf\u00b7bre\u00b7chen", "wie\u00b7der", "Un\u00b7garns", "al\u00b7te", "Wun\u00b7den", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein Mittel als das Eisen macht gesunden.\u00ab", "tokens": ["Kein", "Mit\u00b7tel", "als", "das", "Ei\u00b7sen", "macht", "ge\u00b7sun\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "KOKOM", "ART", "NN", "VVFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein J\u00fcngling hat aus fast unb\u00e4rt'gem Munde", "tokens": ["Ein", "J\u00fcng\u00b7ling", "hat", "aus", "fast", "un\u00b7b\u00e4rt'\u00b7gem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die\u00df Wort geschleudert in die F\u00fcrstenrunde;", "tokens": ["Die\u00df", "Wort", "ge\u00b7schleu\u00b7dert", "in", "die", "F\u00fcrs\u00b7ten\u00b7run\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zu seinem Volk doch sprach in weh'nden Zungen", "tokens": ["Zu", "sei\u00b7nem", "Volk", "doch", "sprach", "in", "weh'n\u00b7den", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sein Bannerspruch: \u00bbGott, Freiheit, Vaterland!\u00ab", "tokens": ["Sein", "Ban\u00b7ner\u00b7spruch", ":", "\u00bb", "Gott", ",", "Frei\u00b7heit", ",", "Va\u00b7ter\u00b7land", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$.", "$(", "NN", "$,", "NN", "$,", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Von den Karpathen zu den Niederungen", "tokens": ["Von", "den", "Kar\u00b7pa\u00b7then", "zu", "den", "Nie\u00b7de\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ha, wie das warb und freudig Kriegsvolk fand!", "tokens": ["Ha", ",", "wie", "das", "warb", "und", "freu\u00b7dig", "Kriegs\u00b7volk", "fand", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "PDS", "VVFIN", "KON", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "L\u00e4ngst ist's vorbei, \u2013 das dunkle Haar ward wei\u00df,", "tokens": ["L\u00e4ngst", "ist's", "vor\u00b7bei", ",", "\u2013", "das", "dunk\u00b7le", "Haar", "ward", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKVZ", "$,", "$(", "ART", "ADJA", "NN", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der J\u00fcngling vor der Zeit ein kranker Greis!", "tokens": ["Der", "J\u00fcng\u00b7ling", "vor", "der", "Zeit", "ein", "kran\u00b7ker", "Greis", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Kerkerluft t\u00fcncht keine Wange roth", "tokens": ["Die", "Ker\u00b7ker\u00b7luft", "t\u00fcncht", "kei\u00b7ne", "Wan\u00b7ge", "roth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Muskeln st\u00e4hlt nicht der Verbannung Brod;", "tokens": ["Und", "Mus\u00b7keln", "st\u00e4hlt", "nicht", "der", "Ver\u00b7ban\u00b7nung", "Brod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die H\u00e4nde, die zu schwach ein Schwert zu halten,", "tokens": ["Die", "H\u00e4n\u00b7de", ",", "die", "zu", "schwach", "ein", "Schwert", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKZU", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie lernen willig zum Gebet sich falten.", "tokens": ["Sie", "ler\u00b7nen", "wil\u00b7lig", "zum", "Ge\u00b7bet", "sich", "fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die Heimat ach wie fern! Nicht wecken ihn", "tokens": ["Die", "Hei\u00b7mat", "ach", "wie", "fern", "!", "Nicht", "we\u00b7cken", "ihn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "KOKOM", "ADJD", "$.", "PTKNEG", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Fr\u00fchglocken mehr; doch thut's der Muezzin,", "tokens": ["Fr\u00fch\u00b7glo\u00b7cken", "mehr", ";", "doch", "thut's", "der", "Muez\u00b7zin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "++-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Der zum Gebet Rodostos Gl\u00e4ub'ge ruft.", "tokens": ["Der", "zum", "Ge\u00b7bet", "Ro\u00b7dos\u00b7tos", "Gl\u00e4ub'\u00b7ge", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "NE", "NN", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Kein Vesperklang! Ein Ave tr\u00e4gt bisweilen", "tokens": ["Kein", "Ves\u00b7per\u00b7klang", "!", "Ein", "A\u00b7ve", "tr\u00e4gt", "bis\u00b7wei\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$.", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Von Griechenschiffen her die Abendluft,", "tokens": ["Von", "Grie\u00b7chen\u00b7schif\u00b7fen", "her", "die", "A\u00b7ben\u00b7dluft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APZR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die scheu durch der Propontis Wellen eilen.", "tokens": ["Die", "scheu", "durch", "der", "Pro\u00b7pon\u00b7tis", "Wel\u00b7len", "ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Das T\u00fcrkenweib, das Kr\u00fcg' am Brunnen f\u00fcllt,", "tokens": ["Das", "T\u00fcr\u00b7ken\u00b7weib", ",", "das", "Kr\u00fcg'", "am", "Brun\u00b7nen", "f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Der Freiheit mahnt's, die sich in Schleier h\u00fcllt:", "tokens": ["Der", "Frei\u00b7heit", "mahnt's", ",", "die", "sich", "in", "Schlei\u00b7er", "h\u00fcllt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "\u00bbo st\u00fcrme, Sturm! Den Schleierflor verwehe,", "tokens": ["\u00bb", "o", "st\u00fcr\u00b7me", ",", "Sturm", "!", "Den", "Schlei\u00b7er\u00b7flor", "ver\u00b7we\u00b7he", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "$,", "NN", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Da\u00df schleierlos mein Volk ihr Antlitz sehe!\u00ab", "tokens": ["Da\u00df", "schlei\u00b7er\u00b7los", "mein", "Volk", "ihr", "Ant\u00b7litz", "se\u00b7he", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Umsonst! Das Heer der Tapfern hat verblutet, \u2013", "tokens": ["Um\u00b7sonst", "!", "Das", "Heer", "der", "Tap\u00b7fern", "hat", "ver\u00b7blu\u00b7tet", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$.", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der F\u00fchrer selbst gebeugt, ergreist, entmuthet,", "tokens": ["Der", "F\u00fch\u00b7rer", "selbst", "ge\u00b7beugt", ",", "er\u00b7greist", ",", "ent\u00b7mu\u00b7thet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In fremdem Land auf seinem Sterbebette!", "tokens": ["In", "frem\u00b7dem", "Land", "auf", "sei\u00b7nem", "Ster\u00b7be\u00b7bet\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor seinem Geist entrollt sich nun die Kette", "tokens": ["Vor", "sei\u00b7nem", "Geist", "ent\u00b7rollt", "sich", "nun", "die", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von Schlachten, Siegesz\u00fcgen, Niederlagen", "tokens": ["Von", "Schlach\u00b7ten", ",", "Sie\u00b7ges\u00b7z\u00fc\u00b7gen", ",", "Nie\u00b7der\u00b7la\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bis hin zu der Verbannung dunklen Tagen.", "tokens": ["Bis", "hin", "zu", "der", "Ver\u00b7ban\u00b7nung", "dunk\u00b7len", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die letzten Tr\u00fcmmer, des Exils Genossen,", "tokens": ["Die", "letz\u00b7ten", "Tr\u00fcm\u00b7mer", ",", "des", "E\u00b7xils", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Greis Bercseny, treuester Achates,", "tokens": ["Der", "Greis", "Ber\u00b7cse\u00b7ny", ",", "treu\u00b7es\u00b7ter", "A\u00b7cha\u00b7tes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Und Esterhaz', der Mann des Schwerts und Rathes,", "tokens": ["Und", "Es\u00b7ter\u00b7ha\u00b7z'", ",", "der", "Mann", "des", "Schwerts", "und", "Ra\u00b7thes", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.10": {"text": "Sie ruhn, von fremder Erde l\u00e4ngst umschlossen.", "tokens": ["Sie", "ruhn", ",", "von", "frem\u00b7der", "Er\u00b7de", "l\u00e4ngst", "um\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "APPR", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ein T\u00fcrkensklave hieb in ihren Stein", "tokens": ["Ein", "T\u00fcr\u00b7kens\u00b7kla\u00b7ve", "hieb", "in", "ih\u00b7ren", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ihm unbekannte Zeichen, wie zum Hohne,", "tokens": ["Ihm", "un\u00b7be\u00b7kann\u00b7te", "Zei\u00b7chen", ",", "wie", "zum", "Hoh\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "\u00bbvertheidiger von Ungarns Freiheit\u00ab ein.", "tokens": ["\u00bb", "ver\u00b7thei\u00b7di\u00b7ger", "von", "Un\u00b7garns", "Frei\u00b7heit", "\u00ab", "ein", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADJD", "APPR", "NN", "NN", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Gefahrlos Wort auf der Propontis Gr\u00fcnden!", "tokens": ["Ge\u00b7fahr\u00b7los", "Wort", "auf", "der", "Pro\u00b7pon\u00b7tis", "Gr\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Du wirst in Herzen nicht bei Osmans Sohne", "tokens": ["Du", "wirst", "in", "Her\u00b7zen", "nicht", "bei", "Os\u00b7mans", "Soh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "PTKNEG", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und nicht bei Pera's Diplomaten z\u00fcnden.", "tokens": ["Und", "nicht", "bei", "Pe\u00b7ra's", "Dip\u00b7lo\u00b7ma\u00b7ten", "z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Kranke h\u00e4lt in seiner d\u00fcrren Hand", "tokens": ["Der", "Kran\u00b7ke", "h\u00e4lt", "in", "sei\u00b7ner", "d\u00fcr\u00b7ren", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein schweres Buch, in schwarzen Sammt gebunden,", "tokens": ["Ein", "schwe\u00b7res", "Buch", ",", "in", "schwar\u00b7zen", "Sammt", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein golden Kreuz prangt auf der Deckelwand,", "tokens": ["Ein", "gol\u00b7den", "Kreuz", "prangt", "auf", "der", "De\u00b7ckel\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Inschrift sagt: \u00bbRakoczy's Andachtstunden\u00ab.", "tokens": ["Die", "In\u00b7schrift", "sagt", ":", "\u00bb", "Ra\u00b7koc\u00b7zy's", "An\u00b7dachts\u00b7tun\u00b7den", "\u00ab", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NE", "NE", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gebete sind es, Uebungen im B\u00fc\u00dfen,", "tokens": ["Ge\u00b7be\u00b7te", "sind", "es", ",", "Ue\u00b7bun\u00b7gen", "im", "B\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Die selbst er schrieb und die ihn oft erbaut;", "tokens": ["Die", "selbst", "er", "schrieb", "und", "die", "ihn", "oft", "er\u00b7baut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "KON", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.