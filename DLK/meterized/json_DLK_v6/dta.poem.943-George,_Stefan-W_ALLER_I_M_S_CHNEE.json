{"dta.poem.943": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "W ALLER   I M   S CHNEE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Ich darf nicht dankend an dir niedersinken               ", "tokens": ["Ich", "darf", "nicht", "dan\u00b7kend", "an", "dir", "nie\u00b7der\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du bist vom geist der flur aus der wir stiegen               ", "tokens": ["Du", "bist", "vom", "geist", "der", "flur", "aus", "der", "wir", "stie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ART", "NN", "APPR", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Will sich mein trost an deine wehmut schmiegen               ", "tokens": ["Will", "sich", "mein", "trost", "an", "dei\u00b7ne", "weh\u00b7mut", "schmie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So wird sie zucken um ihm abzuwinken             ", "tokens": ["So", "wird", "sie", "zu\u00b7cken", "um", "ihm", "ab\u00b7zu\u00b7win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "APPR", "PPER", "VVIZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verharrst du bei dem qu&#228;lenden beschlusse               ", "tokens": ["Ver\u00b7harrst", "du", "bei", "dem", "qu", "&#228;", "len\u00b7den", "be\u00b7schlus\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NE", "$(", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nie deines leides n&#228;he zu gestehen               ", "tokens": ["Nie", "dei\u00b7nes", "lei\u00b7des", "n", "&#228;", "he", "zu", "ge\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "FM", "FM", "FM", "FM", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nur mit ihm und mir dich zu ergehen               ", "tokens": ["Und", "nur", "mit", "ihm", "und", "mir", "dich", "zu", "er\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPER", "KON", "PPER", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am eisigklaren tief-entschlafnen flusse?             ", "tokens": ["Am", "ei\u00b7sig\u00b7kla\u00b7ren", "tie\u00b7fent\u00b7schlaf\u00b7nen", "flus\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}