{"textgrid.poem.64783": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "5. An Hrn. Gottlob Benj. Straube, als er an seinem Geburtstage Leipzig verlie\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, den der Tag mir raubt, der dich der Welt geschenket,", "tokens": ["Freund", ",", "den", "der", "Tag", "mir", "raubt", ",", "der", "dich", "der", "Welt", "ge\u00b7schen\u00b7ket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,", "PRELS", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Sieh, was hier meinen Wunsch f\u00fcr dich und mich umschr\u00e4nket:", "tokens": ["Sieh", ",", "was", "hier", "mei\u00b7nen", "Wunsch", "f\u00fcr", "dich", "und", "mich", "um\u00b7schr\u00e4n\u00b7ket", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "PPOSAT", "NN", "APPR", "PPER", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich bringe heute nicht, wie sonst, ein scherzend Lied,", "tokens": ["Ich", "brin\u00b7ge", "heu\u00b7te", "nicht", ",", "wie", "sonst", ",", "ein", "scher\u00b7zend", "Lied", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "ADV", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir, den vielleicht mein Blick zum letztenmale sieht;", "tokens": ["Dir", ",", "den", "viel\u00b7leicht", "mein", "Blick", "zum", "letz\u00b7ten\u00b7ma\u00b7le", "sieht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "PPOSAT", "NN", "APPRART", "ADV", "VVFIN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Doch mein gesetzter Sinn bringt dir auch keine Klagen,", "tokens": ["Doch", "mein", "ge\u00b7setz\u00b7ter", "Sinn", "bringt", "dir", "auch", "kei\u00b7ne", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur W\u00fcnsche von der Art, wie Philosophen wagen:", "tokens": ["Nur", "W\u00fcn\u00b7sche", "von", "der", "Art", ",", "wie", "Phi\u00b7lo\u00b7so\u00b7phen", "wa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "$,", "PWAV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Des Sch\u00f6pfers Gr\u00f6\u00dfe sehn, in Wahrheit beyder Art,", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Gr\u00f6\u00b7\u00dfe", "sehn", ",", "in", "Wahr\u00b7heit", "bey\u00b7der", "Art", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In der, die Er uns lehrt, und die Er offenbart;", "tokens": ["In", "der", ",", "die", "Er", "uns", "lehrt", ",", "und", "die", "Er", "of\u00b7fen\u00b7bart", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "KON", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht dumm ein irdisch Gl\u00fcck verscherzen und verachten,", "tokens": ["Nicht", "dumm", "ein", "ir\u00b7disch", "Gl\u00fcck", "ver\u00b7scher\u00b7zen", "und", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ART", "ADJD", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht mit vergebner Qual nach eitlen G\u00fctern schmachten.", "tokens": ["Nicht", "mit", "ver\u00b7geb\u00b7ner", "Qual", "nach", "eit\u00b7len", "G\u00fc\u00b7tern", "schmach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Kr\u00e4fte gr\u00f6\u00dften Theil den Wissenschaften weyhn,", "tokens": ["Der", "Kr\u00e4f\u00b7te", "gr\u00f6\u00df\u00b7ten", "Theil", "den", "Wis\u00b7sen\u00b7schaf\u00b7ten", "weyhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch f\u00fcr des K\u00f6rpers Wohl nicht unempfindlich seyn;", "tokens": ["Doch", "f\u00fcr", "des", "K\u00f6r\u00b7pers", "Wohl", "nicht", "un\u00b7emp\u00b7find\u00b7lich", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sich n\u00fctzlich und beliebt durch Witz und Sitten machen;", "tokens": ["Sich", "n\u00fctz\u00b7lich", "und", "be\u00b7liebt", "durch", "Witz", "und", "Sit\u00b7ten", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "ADJD", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ha\u00dft man uns unverdient, der Thoren Zorn verlachen:", "tokens": ["Ha\u00dft", "man", "uns", "un\u00b7ver\u00b7di\u00b7ent", ",", "der", "Tho\u00b7ren", "Zorn", "ver\u00b7la\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ADJD", "$,", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Das w\u00fcnsch' ich dir und mir. Dich hab' ich Freund genannt,", "tokens": ["Das", "w\u00fcn\u00b7sch'", "ich", "dir", "und", "mir", ".", "Dich", "hab'", "ich", "Freund", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "KON", "PPER", "$.", "PPER", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.10": {"text": "Seit ich der Me\u00dfkunst Werth, der Dichtkunst Reiz gekannt.", "tokens": ["Seit", "ich", "der", "Me\u00df\u00b7kunst", "Werth", ",", "der", "Dicht\u00b7kunst", "Reiz", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Freundschaft daure stets, die wir sechs Jahre n\u00e4hren,", "tokens": ["Die", "Freund\u00b7schaft", "dau\u00b7re", "stets", ",", "die", "wir", "sechs", "Jah\u00b7re", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PRELS", "PPER", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So stark, als ", "tokens": ["So", "stark", ",", "als"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS"], "meter": "-+-", "measure": "amphibrach.single"}}}}}