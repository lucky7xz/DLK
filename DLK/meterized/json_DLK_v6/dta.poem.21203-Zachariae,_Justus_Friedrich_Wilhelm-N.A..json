{"dta.poem.21203": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch eine s\u00fc\u00dfe Harmonie!", "tokens": ["Welch", "ei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Har\u00b7mo\u00b7nie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So klangen Sterblicher Ges\u00e4nge nie.", "tokens": ["So", "klan\u00b7gen", "Sterb\u00b7li\u00b7cher", "Ge\u00b7s\u00e4n\u00b7ge", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom Berge steiget dort", "tokens": ["Vom", "Ber\u00b7ge", "stei\u00b7get", "dort"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein holder Wanderer herab;", "tokens": ["Ein", "hol\u00b7der", "Wan\u00b7de\u00b7rer", "her\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es stralt in seiner Hand der helle Pilgerstab;", "tokens": ["Es", "stralt", "in", "sei\u00b7ner", "Hand", "der", "hel\u00b7le", "Pil\u00b7ger\u00b7stab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sein jugendliches Angesicht", "tokens": ["Sein", "ju\u00b7gend\u00b7li\u00b7ches", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gleicht dem Gesicht der Erdenb\u00fcrger nicht.", "tokens": ["Gleicht", "dem", "Ge\u00b7sicht", "der", "Er\u00b7den\u00b7b\u00fcr\u00b7ger", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "O dies ist einer von des Himmels Ch\u00f6ren,", "tokens": ["O", "dies", "ist", "ei\u00b7ner", "von", "des", "Him\u00b7mels", "Ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "VAFIN", "ART", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die wir ietzt \u00fcber uns erschallen h\u00f6ren.", "tokens": ["Die", "wir", "ietzt", "\u00fc\u00b7ber", "uns", "er\u00b7schal\u00b7len", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wir neigen uns vor dir", "tokens": ["Wir", "nei\u00b7gen", "uns", "vor", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Mit Ehrfurcht, hoher Wanderer des Himmels.", "tokens": ["Mit", "Ehr\u00b7furcht", ",", "ho\u00b7her", "Wan\u00b7de\u00b7rer", "des", "Him\u00b7mels", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}