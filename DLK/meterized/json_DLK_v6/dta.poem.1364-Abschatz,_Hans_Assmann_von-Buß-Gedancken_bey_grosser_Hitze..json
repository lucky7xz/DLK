{"dta.poem.1364": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Bu\u00df-Gedancken bey grosser Hitze.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wo soll ich fliehen hin/ da\u00df ich im Schatten sitze?", "tokens": ["Wo", "soll", "ich", "flie\u00b7hen", "hin", "/", "da\u00df", "ich", "im", "Schat\u00b7ten", "sit\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$(", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Es brennt des H\u00f6chsten Zorn mit angeflammter Hitze", "tokens": ["Es", "brennt", "des", "H\u00f6chs\u00b7ten", "Zorn", "mit", "an\u00b7ge\u00b7flamm\u00b7ter", "Hit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den von Gerechtigkeit und Unschuld-blossen Geist/", "tokens": ["Den", "von", "Ge\u00b7rech\u00b7tig\u00b7keit", "und", "Un\u00b7schuld\u00b7\u00b7blos\u00b7sen", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Thr\u00e4nen aus dem Aug/ und Blutt vom Hertzen schweist.", "tokens": ["Der", "Thr\u00e4\u00b7nen", "aus", "dem", "Aug", "/", "und", "Blutt", "vom", "Hert\u00b7zen", "schweist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "KON", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kein K\u00fcrbi\u00df-Blat beschirmt mich nicht/", "tokens": ["Kein", "K\u00fcr\u00b7bi\u00df\u00b7Blat", "be\u00b7schirmt", "mich", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn dieser Sonne Feuer sticht/", "tokens": ["Wenn", "die\u00b7ser", "Son\u00b7ne", "Feu\u00b7er", "sticht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein dunckler Wald noch d\u00fcstre H\u00f6le", "tokens": ["Kein", "dunck\u00b7ler", "Wald", "noch", "d\u00fcst\u00b7re", "H\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "K\u00fchlt oder birgt die matte Seele.", "tokens": ["K\u00fchlt", "o\u00b7der", "birgt", "die", "mat\u00b7te", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Unschuld reines Kleid/ zu dem ich war erkohren/", "tokens": ["Der", "Un\u00b7schuld", "rei\u00b7nes", "Kleid", "/", "zu", "dem", "ich", "war", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "APPR", "PRELS", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hab ich durch Evens Lust und Adams Bi\u00df verlohren/", "tokens": ["Hab", "ich", "durch", "E\u00b7vens", "Lust", "und", "A\u00b7dams", "Bi\u00df", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NE", "NN", "KON", "NN", "APPR", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Wahnwitz reist mir selbst den Rock des Heiles ab/", "tokens": ["Mein", "Wahn\u00b7witz", "reist", "mir", "selbst", "den", "Rock", "des", "Hei\u00b7les", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den mir der Tauffe Bund doch zu gebrauchen gab.", "tokens": ["Den", "mir", "der", "Tauf\u00b7fe", "Bund", "doch", "zu", "ge\u00b7brau\u00b7chen", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die mit viel Schuld beschw\u00e4rzte Scho\u00df", "tokens": ["Die", "mit", "viel", "Schuld", "be\u00b7schw\u00e4rz\u00b7te", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist leider aller Zierde blo\u00df/", "tokens": ["Ist", "lei\u00b7der", "al\u00b7ler", "Zier\u00b7de", "blo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nichts hab ich mehr mit Furcht und Zagen/", "tokens": ["Nichts", "hab", "ich", "mehr", "mit", "Furcht", "und", "Za\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als nackte D\u00fcrfftigkeit zu klagen.", "tokens": ["Als", "nack\u00b7te", "D\u00fcr\u00b7ff\u00b7tig\u00b7keit", "zu", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wo soll ich fliehen hin? der Tag will k\u00fchle werden/", "tokens": ["Wo", "soll", "ich", "flie\u00b7hen", "hin", "?", "der", "Tag", "will", "k\u00fch\u00b7le", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VMFIN", "ADJA", "VAINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die Gnaden-Sonne neigt sich weit von mir zur Erden/", "tokens": ["Die", "Gna\u00b7den\u00b7Son\u00b7ne", "neigt", "sich", "weit", "von", "mir", "zur", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "APPR", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von fernen dr\u00e4uet mir Zahnklappern finstrer Grufft/", "tokens": ["Von", "fer\u00b7nen", "dr\u00e4u\u00b7et", "mir", "Zahn\u00b7klap\u00b7pern", "finst\u00b7rer", "Grufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "PPER", "NN", "ADJA", "NN", "$("], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Von Hinten schrecket mich das St\u00fcrmen schwartzer Lufft:", "tokens": ["Von", "Hin\u00b7ten", "schre\u00b7cket", "mich", "das", "St\u00fcr\u00b7men", "schwart\u00b7zer", "Lufft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie sich ein Aespen-Laub bewegt/", "tokens": ["Wie", "sich", "ein", "A\u00b7e\u00b7spen\u00b7Laub", "be\u00b7wegt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "$("], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wenn Eurus Zweig an Zweige schl\u00e4gt/", "tokens": ["Wenn", "Eu\u00b7rus", "Zweig", "an", "Zwei\u00b7ge", "schl\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So sieht man unter solchem Wittern", "tokens": ["So", "sieht", "man", "un\u00b7ter", "sol\u00b7chem", "Wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein h\u00f6chsterschrocknes Hertze zittern.", "tokens": ["Mein", "h\u00f6chs\u00b7ter\u00b7schrock\u00b7nes", "Hert\u00b7ze", "zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wohin verberg ich mich f\u00fcr GOttes Angesichte?", "tokens": ["Wo\u00b7hin", "ver\u00b7berg", "ich", "mich", "f\u00fcr", "Got\u00b7tes", "An\u00b7ge\u00b7sich\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der tieffsten Berge Klufft ist seinen Augen lichte!", "tokens": ["Der", "tieffs\u00b7ten", "Ber\u00b7ge", "Klufft", "ist", "sei\u00b7nen", "Au\u00b7gen", "lich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sezt ich dem R\u00fccken gleich Matutens Fl\u00fcgel an/", "tokens": ["Sezt", "ich", "dem", "R\u00fc\u00b7cken", "gleich", "Ma\u00b7tu\u00b7tens", "Fl\u00fc\u00b7gel", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "NE", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wei\u00df ich/ da\u00df sein Blick mich doch ereilen kan.", "tokens": ["So", "wei\u00df", "ich", "/", "da\u00df", "sein", "Blick", "mich", "doch", "er\u00b7ei\u00b7len", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPOSAT", "NN", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des abgelegnen Meeres Grund", "tokens": ["Des", "ab\u00b7ge\u00b7leg\u00b7nen", "Mee\u00b7res", "Grund"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist ihm durch alle Flutten kund/", "tokens": ["Ist", "ihm", "durch", "al\u00b7le", "Flut\u00b7ten", "kund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PIAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wolt ich mir in die H\u00f6lle betten/", "tokens": ["Wolt", "ich", "mir", "in", "die", "H\u00f6l\u00b7le", "bet\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So findt sich da auch kein Erretten.", "tokens": ["So", "findt", "sich", "da", "auch", "kein", "Er\u00b7ret\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Last\u2019 Decken Babylons mit stoltzem Ruhme sticken/", "tokens": ["Last'", "De\u00b7cken", "Ba\u00b7by\u00b7lons", "mit", "stolt\u00b7zem", "Ruh\u00b7me", "sti\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NE", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich kan kein fremder Zeug bey eignem Mangel schm\u00fc-", "tokens": ["Mich", "kan", "kein", "frem\u00b7der", "Zeug", "bey", "eig\u00b7nem", "Man\u00b7gel", "schm\u00fc"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "ADJA", "NN", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich poche nur umsonst auff Arbeit meiner Hand/", "tokens": ["Ich", "po\u00b7che", "nur", "um\u00b7sonst", "auff", "Ar\u00b7beit", "mei\u00b7ner", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und w\u00fcrcke nichts als M\u00fch und Frevel zum Gewand.", "tokens": ["Und", "w\u00fcr\u00b7cke", "nichts", "als", "M\u00fch", "und", "Fre\u00b7vel", "zum", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "KOKOM", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit Adams welckem Feigen-Blat", "tokens": ["Mit", "A\u00b7dams", "wel\u00b7ckem", "Fei\u00b7gen\u00b7Blat"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "PWAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bedeck ich meine Missethat/", "tokens": ["Be\u00b7deck", "ich", "mei\u00b7ne", "Mis\u00b7se\u00b7that", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Thun gleicht leichten Spinnenweben/", "tokens": ["Mein", "Thun", "gleicht", "leich\u00b7ten", "Spin\u00b7nen\u00b7we\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und kan mir keine Kleidung geben.", "tokens": ["Und", "kan", "mir", "kei\u00b7ne", "Klei\u00b7dung", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Weg mit geborgtem Schmuck und eigner Flecken Kleide/", "tokens": ["Weg", "mit", "ge\u00b7borg\u00b7tem", "Schmuck", "und", "eig\u00b7ner", "Fle\u00b7cken", "Klei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein JEsus beut mir an die Rosin-rothe Seide/", "tokens": ["Mein", "Je\u00b7sus", "beut", "mir", "an", "die", "Ro\u00b7sin\u00b7ro\u00b7the", "Sei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VVFIN", "PPER", "APPR", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch sein selbst eigen Blutt gef\u00e4rbt ans Creutzes Stamm.", "tokens": ["Durch", "sein", "selbst", "ei\u00b7gen", "Blutt", "ge\u00b7f\u00e4rbt", "ans", "Creut\u00b7zes", "Stamm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "ADJD", "ADJD", "VVPP", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ward nicht das erste Kleid/ (er ist das reine Lamm", "tokens": ["Ward", "nicht", "das", "ers\u00b7te", "Kleid", "/", "(", "er", "ist", "das", "rei\u00b7ne", "Lamm"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "ADJA", "NN", "$(", "$(", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "F\u00fcr mich von Anbeginn geschlacht:)", "tokens": ["F\u00fcr", "mich", "von", "An\u00b7be\u00b7ginn", "ge\u00b7schlacht", ":)"], "token_info": ["word", "word", "word", "word", "word", "emoticon"], "pos": ["APPR", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch GOtt von Fellen selbst gemacht?", "tokens": ["Durch", "Gott", "von", "Fel\u00b7len", "selbst", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In sein Verdienst will ich mich kleiden/", "tokens": ["In", "sein", "Ver\u00b7dienst", "will", "ich", "mich", "klei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und so getrost von hinnen scheiden.", "tokens": ["Und", "so", "ge\u00b7trost", "von", "hin\u00b7nen", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}