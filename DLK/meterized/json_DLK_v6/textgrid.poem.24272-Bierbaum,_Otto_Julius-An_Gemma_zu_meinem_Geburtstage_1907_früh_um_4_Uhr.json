{"textgrid.poem.24272": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "An Gemma zu meinem Geburtstage 1907 fr\u00fch um 4 Uhr", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch hat der Mond sein silbernes Szepter nicht", "tokens": ["Noch", "hat", "der", "Mond", "sein", "sil\u00b7ber\u00b7nes", "Szep\u00b7ter", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der goldenen Tagesk\u00f6nigin \u00fcberreicht,", "tokens": ["Der", "gol\u00b7de\u00b7nen", "Ta\u00b7ges\u00b7k\u00f6\u00b7ni\u00b7gin", "\u00fc\u00b7berr\u00b7eicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Doch tausend V\u00f6gel singen schon ihren Gru\u00df", "tokens": ["Doch", "tau\u00b7send", "V\u00f6\u00b7gel", "sin\u00b7gen", "schon", "ih\u00b7ren", "Gru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dankbarer Liebe dem Licht, das wieder kommt.", "tokens": ["Dank\u00b7ba\u00b7rer", "Lie\u00b7be", "dem", "Licht", ",", "das", "wie\u00b7der", "kommt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Auch ich begr\u00fc\u00dfe die himmlische K\u00f6nigin", "tokens": ["Auch", "ich", "be\u00b7gr\u00fc\u00b7\u00dfe", "die", "himm\u00b7li\u00b7sche", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bei ihrem Lever; dann komm ich zu dir ans Bett", "tokens": ["Bei", "ih\u00b7rem", "Le\u00b7ver", ";", "dann", "komm", "ich", "zu", "dir", "ans", "Bett"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und freue mich \u00fcber dein L\u00e4cheln mehr noch,", "tokens": ["Und", "freu\u00b7e", "mich", "\u00fc\u00b7ber", "dein", "L\u00e4\u00b7cheln", "mehr", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Als \u00fcber alle Strahlen der Ewigkeit.", "tokens": ["Als", "\u00fc\u00b7ber", "al\u00b7le", "Strah\u00b7len", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "So wollen wirs, hoff ich, lange noch halten und", "tokens": ["So", "wol\u00b7len", "wirs", ",", "hoff", "ich", ",", "lan\u00b7ge", "noch", "hal\u00b7ten", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "$,", "VVFIN", "PPER", "$,", "ADV", "ADV", "VVINF", "KON"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bei Mond und Sonne immer gedenken, da\u00df", "tokens": ["Bei", "Mond", "und", "Son\u00b7ne", "im\u00b7mer", "ge\u00b7den\u00b7ken", ",", "da\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVINF", "$,", "KOUS"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Tag wie Nacht dunkel und schaurig sind,", "tokens": ["Tag", "wie", "Nacht", "dun\u00b7kel", "und", "schau\u00b7rig", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wenn unser Herz sie nicht helle und heiter macht.", "tokens": ["Wenn", "un\u00b7ser", "Herz", "sie", "nicht", "hel\u00b7le", "und", "hei\u00b7ter", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PTKNEG", "ADJA", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}