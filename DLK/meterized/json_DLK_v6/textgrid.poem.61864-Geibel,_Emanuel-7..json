{"textgrid.poem.61864": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herz, was willst du? Warum schwillst du?", "tokens": ["Herz", ",", "was", "willst", "du", "?", "Wa\u00b7rum", "schwillst", "du", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "PPER", "$.", "PWAV", "VVFIN", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Was bewegt dich so mit Macht?", "tokens": ["Was", "be\u00b7wegt", "dich", "so", "mit", "Macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "War dies Bangen und Verlangen", "tokens": ["War", "dies", "Ban\u00b7gen", "und", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn nicht l\u00e4ngst zur Ruh' gebracht?", "tokens": ["Denn", "nicht", "l\u00e4ngst", "zur", "Ruh'", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was vor Jahren du erfahren,", "tokens": ["Was", "vor", "Jah\u00b7ren", "du", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Jugend reinstes Gl\u00fcck,", "tokens": ["Dei\u00b7ner", "Ju\u00b7gend", "reins\u00b7tes", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Erstes Leiden, schwerstes Scheiden,", "tokens": ["Ers\u00b7tes", "Lei\u00b7den", ",", "schwers\u00b7tes", "Schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer beschwor es dir zur\u00fcck?", "tokens": ["Wer", "be\u00b7schwor", "es", "dir", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Herz, was willst du? Warum schwillst du?", "tokens": ["Herz", ",", "was", "willst", "du", "?", "Wa\u00b7rum", "schwillst", "du", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "PPER", "$.", "PWAV", "VVFIN", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ach, du wei\u00dft, was dir geschehn:", "tokens": ["Ach", ",", "du", "wei\u00dft", ",", "was", "dir", "ge\u00b7schehn", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Erkorne, Fr\u00fchverlorne", "tokens": ["Die", "Er\u00b7kor\u00b7ne", ",", "Fr\u00fch\u00b7ver\u00b7lor\u00b7ne"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollst du heute wiedersehn.", "tokens": ["Sollst", "du", "heu\u00b7te", "wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}