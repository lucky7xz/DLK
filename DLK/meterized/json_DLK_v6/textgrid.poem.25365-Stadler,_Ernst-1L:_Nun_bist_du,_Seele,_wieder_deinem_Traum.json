{"textgrid.poem.25365": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun bist du, Seele, wieder deinem Traum", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun bist du, Seele, wieder deinem Traum", "tokens": ["Nun", "bist", "du", ",", "See\u00b7le", ",", "wie\u00b7der", "dei\u00b7nem", "Traum"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "$,", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und deiner Sehnsucht selig hingegeben.", "tokens": ["Und", "dei\u00b7ner", "Sehn\u00b7sucht", "se\u00b7lig", "hin\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In holdem Feuer gl\u00fchend f\u00fchlst du kaum,", "tokens": ["In", "hol\u00b7dem", "Feu\u00b7er", "gl\u00fc\u00b7hend", "f\u00fchlst", "du", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df Schatten alle Bilder sind, die um dich leben.", "tokens": ["Da\u00df", "Schat\u00b7ten", "al\u00b7le", "Bil\u00b7der", "sind", ",", "die", "um", "dich", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PIAT", "NN", "VAFIN", "$,", "PRELS", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn n\u00e4chtelang war deine Kammer leer.", "tokens": ["Denn", "n\u00e4ch\u00b7tel\u00b7ang", "war", "dei\u00b7ne", "Kam\u00b7mer", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nun gr\u00fc\u00dfen dich, wie \u00fcber Nacht die Zeichen", "tokens": ["Nun", "gr\u00fc\u00b7\u00dfen", "dich", ",", "wie", "\u00fc\u00b7ber", "Nacht", "die", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des jungen Fr\u00fchlings durch die Fenster her,", "tokens": ["Des", "jun\u00b7gen", "Fr\u00fch\u00b7lings", "durch", "die", "Fens\u00b7ter", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die neuen Schauer, die durch deine Seele streichen.", "tokens": ["Die", "neu\u00b7en", "Schau\u00b7er", ",", "die", "durch", "dei\u00b7ne", "See\u00b7le", "strei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wei\u00dft doch: niemals wird Erf\u00fcllung sein", "tokens": ["Und", "wei\u00dft", "doch", ":", "nie\u00b7mals", "wird", "Er\u00b7f\u00fcl\u00b7lung", "sein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$.", "ADV", "VAFIN", "NN", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Schwachen, die ihr Blut dem Traum verpf\u00e4nden,", "tokens": ["Den", "Schwa\u00b7chen", ",", "die", "ihr", "Blut", "dem", "Traum", "ver\u00b7pf\u00e4n\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und h\u00f6hnend schl\u00e4gt das Schicksal Krug und Wein", "tokens": ["Und", "h\u00f6h\u00b7nend", "schl\u00e4gt", "das", "Schick\u00b7sal", "Krug", "und", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den ewig D\u00fcrstenden aus hochgehobnen H\u00e4nden.", "tokens": ["Den", "e\u00b7wig", "D\u00fcrs\u00b7ten\u00b7den", "aus", "hoch\u00b7ge\u00b7hob\u00b7nen", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}