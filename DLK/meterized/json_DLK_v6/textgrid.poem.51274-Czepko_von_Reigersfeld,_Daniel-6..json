{"textgrid.poem.51274": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe mein Gem\u00fcth' auff manche Kunst gewandt,", "tokens": ["Ich", "ha\u00b7be", "mein", "Ge\u00b7m\u00fcth'", "auff", "man\u00b7che", "Kunst", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff manches Thun gelegt mein Arbeit und Verstand:", "tokens": ["Auff", "man\u00b7ches", "Thun", "ge\u00b7legt", "mein", "Ar\u00b7beit", "und", "Ver\u00b7stand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Licht der Welt hat mir die B\u00fccher, wenn es kommen,", "tokens": ["Das", "Licht", "der", "Welt", "hat", "mir", "die", "B\u00fc\u00b7cher", ",", "wenn", "es", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gegeben in die Hand und wieder draus genommen.", "tokens": ["Ge\u00b7ge\u00b7ben", "in", "die", "Hand", "und", "wie\u00b7der", "draus", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KON", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kein Flei\u00df hat mir gefehlt: zu Rom und zu Athen,", "tokens": ["Kein", "Flei\u00df", "hat", "mir", "ge\u00b7fehlt", ":", "zu", "Rom", "und", "zu", "A\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVPP", "$.", "APPR", "NE", "KON", "APPR", "NE", "$,"], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kont' als ihr B\u00fcrger ich, selbst hin und wieder gehn:", "tokens": ["Kont'", "als", "ihr", "B\u00fcr\u00b7ger", "ich", ",", "selbst", "hin", "und", "wie\u00b7der", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KOKOM", "PPOSAT", "NN", "PPER", "$,", "ADV", "PTKVZ", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich hab' aus gro\u00dfer Huld des Himmels viel geschrieben,", "tokens": ["Ich", "hab'", "aus", "gro\u00b7\u00dfer", "Huld", "des", "Him\u00b7mels", "viel", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mich in der Heimlichkeit der Wei\u00dfheit k\u00f6nnen \u00fcben:", "tokens": ["Mich", "in", "der", "Heim\u00b7lich\u00b7keit", "der", "Wei\u00df\u00b7heit", "k\u00f6n\u00b7nen", "\u00fc\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Getreten zum Gestirn, erforscht, was Tag und Nacht", "tokens": ["Ge\u00b7tre\u00b7ten", "zum", "Ge\u00b7stirn", ",", "er\u00b7forscht", ",", "was", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "VVFIN", "$,", "PWS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In C\u00f6rpern \u00fcber uns zu ihrem Wechsel bracht.", "tokens": ["In", "C\u00f6r\u00b7pern", "\u00fc\u00b7ber", "uns", "zu", "ih\u00b7rem", "Wech\u00b7sel", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Th\u00fcr' stund mir zu Kriegs- und Friedens Diensten offen,", "tokens": ["Die", "Th\u00fcr'", "stund", "mir", "zu", "Kriegs", "und", "Frie\u00b7dens", "Diens\u00b7ten", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "TRUNC", "KON", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Fragt ihr, was ich zuletzt in diesen angetroffen?", "tokens": ["Fragt", "ihr", ",", "was", "ich", "zu\u00b7letzt", "in", "die\u00b7sen", "an\u00b7ge\u00b7trof\u00b7fen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "ADV", "APPR", "PDAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich hab', o gro\u00dfes Heil! Den Irrthum so erkannt,", "tokens": ["Ich", "hab'", ",", "o", "gro\u00b7\u00dfes", "Heil", "!", "Den", "Irr\u00b7thum", "so", "er\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "FM", "ADJA", "NN", "$.", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wist aber, da\u00df zunechst bey ihm die Wahrheit stand.", "tokens": ["Wist", "a\u00b7ber", ",", "da\u00df", "zu\u00b7nechst", "bey", "ihm", "die", "Wahr\u00b7heit", "stand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "KOUS", "ADV", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}