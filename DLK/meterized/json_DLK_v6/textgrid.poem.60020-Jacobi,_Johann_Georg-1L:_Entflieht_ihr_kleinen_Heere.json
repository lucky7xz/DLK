{"textgrid.poem.60020": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Entflieht ihr kleinen Heere", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Entflieht ihr kleinen Heere", "tokens": ["Ent\u00b7flieht", "ihr", "klei\u00b7nen", "Hee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der l\u00e4chelnden Cythere!", "tokens": ["Der", "l\u00e4\u00b7cheln\u00b7den", "Cy\u00b7the\u00b7re", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Thal ist freudenleer;", "tokens": ["Das", "Thal", "ist", "freu\u00b7den\u00b7leer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bereift sind eure Fl\u00fcgel;", "tokens": ["Be\u00b7reift", "sind", "eu\u00b7re", "Fl\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem nackten, \u00f6den H\u00fcgel", "tokens": ["Dem", "nack\u00b7ten", ",", "\u00f6\u00b7den", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "T\u00f6nt keine Leyer mehr.", "tokens": ["T\u00f6nt", "kei\u00b7ne", "Le\u00b7yer", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Seht! wilde J\u00e4ger w\u00fcrgen", "tokens": ["Seht", "!", "wil\u00b7de", "J\u00e4\u00b7ger", "w\u00fcr\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf hallenden Geb\u00fcrgen,", "tokens": ["Auf", "hal\u00b7len\u00b7den", "Ge\u00b7b\u00fcr\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie spotten eurer Macht;", "tokens": ["Sie", "spot\u00b7ten", "eu\u00b7rer", "Macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von spr\u00f6den Amazonen,", "tokens": ["Von", "spr\u00f6\u00b7den", "A\u00b7maz\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die nur in W\u00e4ldern wohnen,", "tokens": ["Die", "nur", "in", "W\u00e4l\u00b7dern", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wird Paphia verlacht.", "tokens": ["Wird", "Pa\u00b7phia", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wollt ihr vielleicht beym Jagen", "tokens": ["Wollt", "ihr", "viel\u00b7leicht", "beym", "Ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Mordgewehre tragen,", "tokens": ["Die", "Mord\u00b7ge\u00b7weh\u00b7re", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Netze H\u00fcter seyn;", "tokens": ["Der", "Net\u00b7ze", "H\u00fc\u00b7ter", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gedungen von Centauren,", "tokens": ["Ge\u00b7dun\u00b7gen", "von", "Cen\u00b7tau\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf hohen Aesten lauren,", "tokens": ["Auf", "ho\u00b7hen", "A\u00b7es\u00b7ten", "lau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Zum Klang der H\u00f6rner schreyn?", "tokens": ["Zum", "Klang", "der", "H\u00f6r\u00b7ner", "schreyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wenn die St\u00fcrme wehen,", "tokens": ["Und", "wenn", "die", "St\u00fcr\u00b7me", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Soll dann auf kalten H\u00f6hen,", "tokens": ["Soll", "dann", "auf", "kal\u00b7ten", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo Sonnenstrahl gebricht,", "tokens": ["Wo", "Son\u00b7nen\u00b7strahl", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Euch eure Fackel w\u00e4rmen?", "tokens": ["Euch", "eu\u00b7re", "Fa\u00b7ckel", "w\u00e4r\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem Wilde nachzuschw\u00e4rmen,", "tokens": ["Dem", "Wil\u00b7de", "nach\u00b7zu\u00b7schw\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gab sie Cythere nicht.", "tokens": ["Gab", "sie", "Cy\u00b7the\u00b7re", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "PTKNEG", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Das Laub, dem Hain entrissen,", "tokens": ["Das", "Laub", ",", "dem", "Hain", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stirbt unter euren F\u00fc\u00dfen:", "tokens": ["Stirbt", "un\u00b7ter", "eu\u00b7ren", "F\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Flieht! alles ist verheert.", "tokens": ["Flieht", "!", "al\u00b7les", "ist", "ver\u00b7heert", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "O tragt die d\u00fcrren Bl\u00e4tter,", "tokens": ["O", "tragt", "die", "d\u00fcr\u00b7ren", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr artigsten der G\u00f6tter,", "tokens": ["Ihr", "ar\u00b7tigs\u00b7ten", "der", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Auf eines Dichters Herd!", "tokens": ["Auf", "ei\u00b7nes", "Dich\u00b7ters", "Herd", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}