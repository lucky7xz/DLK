{"textgrid.poem.51181": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Parteileben", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer ", "tokens": ["Wer"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Der steht zumeist vielmehr betr\u00e4chtlich ", "tokens": ["Der", "steht", "zu\u00b7meist", "viel\u00b7mehr", "be\u00b7tr\u00e4cht\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Trau keinem, der nie Partei genommen", "tokens": ["Trau", "kei\u00b7nem", ",", "der", "nie", "Par\u00b7tei", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PIS", "$,", "PRELS", "ADV", "NN", "VVPP"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und immer im tr\u00fcben ist geschwommen!", "tokens": ["Und", "im\u00b7mer", "im", "tr\u00fc\u00b7ben", "ist", "ge\u00b7schwom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch wird dir jener auch nicht frommen,", "tokens": ["Doch", "wird", "dir", "je\u00b7ner", "auch", "nicht", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PDS", "ADV", "PTKNEG", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der nie dar\u00fcber hinaus will kommen.", "tokens": ["Der", "nie", "da\u00b7r\u00fc\u00b7ber", "hin\u00b7aus", "will", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PAV", "APZR", "VMFIN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "F\u00e4llt einer ab von eurer Schar,", "tokens": ["F\u00e4llt", "ei\u00b7ner", "ab", "von", "eu\u00b7rer", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So la\u00dft ihn laufen und richtet nicht;", "tokens": ["So", "la\u00dft", "ihn", "lau\u00b7fen", "und", "rich\u00b7tet", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVINF", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch dem, der zu euch sto\u00dfen will", "tokens": ["Doch", "dem", ",", "der", "zu", "euch", "sto\u00b7\u00dfen", "will"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "$,", "PRELS", "APPR", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von dort, dem schauet ins Gesicht!", "tokens": ["Von", "dort", ",", "dem", "schau\u00b7et", "ins", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "PRELS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbwas du nicht willst, da\u00df man dir tu,", "tokens": ["\u00bb", "was", "du", "nicht", "willst", ",", "da\u00df", "man", "dir", "tu", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "PTKNEG", "VMFIN", "$,", "KOUS", "PIS", "PPER", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Das f\u00fcg auch keinem andern zu!\u00ab", "tokens": ["Das", "f\u00fcg", "auch", "kei\u00b7nem", "an\u00b7dern", "zu", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PIAT", "PIS", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df ", "tokens": ["La\u00df"], "token_info": ["word"], "pos": ["VVIMP"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "So ist der halbe Sieg schon dein.", "tokens": ["So", "ist", "der", "hal\u00b7be", "Sieg", "schon", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu diesem Wort lacht manch ein Schuft,", "tokens": ["Zu", "die\u00b7sem", "Wort", "lacht", "manch", "ein", "Schuft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der sich auf den Erfolg beruft;", "tokens": ["Der", "sich", "auf", "den", "Er\u00b7folg", "be\u00b7ruft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Doch du erlebst, da\u00df er wird wandern,", "tokens": ["Doch", "du", "er\u00b7lebst", ",", "da\u00df", "er", "wird", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "'s trifft eben einen nach dem andern!", "tokens": ["'s", "trifft", "e\u00b7ben", "ei\u00b7nen", "nach", "dem", "an\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Halte fest an der Partei, wenn du ein Parteimann bist,", "tokens": ["Hal\u00b7te", "fest", "an", "der", "Par\u00b7tei", ",", "wenn", "du", "ein", "Par\u00b7tei\u00b7mann", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "+--+-++-++-+-+", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Aber unbewegt verleugne jeden L\u00fcgner und Sophist!", "tokens": ["A\u00b7ber", "un\u00b7be\u00b7wegt", "ver\u00b7leug\u00b7ne", "je\u00b7den", "L\u00fcg\u00b7ner", "und", "So\u00b7phist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Betrachtet eurer Gegner Schw\u00e4chen", "tokens": ["Be\u00b7trach\u00b7tet", "eu\u00b7rer", "Geg\u00b7ner", "Schw\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lernt, am besten euch zu r\u00e4chen,", "tokens": ["Und", "lernt", ",", "am", "bes\u00b7ten", "euch", "zu", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PTKA", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das eigne Unkraut auszustechen!", "tokens": ["Das", "eig\u00b7ne", "Un\u00b7kraut", "aus\u00b7zu\u00b7ste\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn schlechte Leute zanken, riecht's \u00fcbel um sie her;", "tokens": ["Wenn", "schlech\u00b7te", "Leu\u00b7te", "zan\u00b7ken", ",", "riecht's", "\u00fc\u00b7bel", "um", "sie", "her", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "$,", "VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Doch wenn sie sich vers\u00f6hnen, so stinkt es noch viel mehr!", "tokens": ["Doch", "wenn", "sie", "sich", "ver\u00b7s\u00f6h\u00b7nen", ",", "so", "stinkt", "es", "noch", "viel", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVINF", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Als Gegner achte, wer es sei!", "tokens": ["Als", "Geg\u00b7ner", "ach\u00b7te", ",", "wer", "es", "sei", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Strauchdiebe aber sind keine Partei!", "tokens": ["Strauch\u00b7die\u00b7be", "a\u00b7ber", "sind", "kei\u00b7ne", "Par\u00b7tei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VAFIN", "PIAT", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}