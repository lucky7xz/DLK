{"textgrid.poem.36267": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Klage der Mutter", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja in des Herzens Glut werd' ich vergehen.", "tokens": ["Ja", "in", "des", "Her\u00b7zens", "Glut", "werd'", "ich", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ART", "NN", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seit mir die Welt verschwunden,", "tokens": ["Seit", "mir", "die", "Welt", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die holden Kindlein mir der Tod entwunden,", "tokens": ["Die", "hol\u00b7den", "Kin\u00b7dlein", "mir", "der", "Tod", "ent\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Will nirgends K\u00fchlung wehen;", "tokens": ["Will", "nir\u00b7gends", "K\u00fch\u00b7lung", "we\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von wo aus freudig str\u00f6mten alle Flammen,", "tokens": ["Von", "wo", "aus", "freu\u00b7dig", "str\u00f6m\u00b7ten", "al\u00b7le", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "APPR", "ADJD", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da dringen nun die Schmerzen hin zusammen.", "tokens": ["Da", "drin\u00b7gen", "nun", "die", "Schmer\u00b7zen", "hin", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zur\u00fcckgetreten sind ins Herz die Fluten,", "tokens": ["Zu\u00b7r\u00fcck\u00b7ge\u00b7tre\u00b7ten", "sind", "ins", "Herz", "die", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und will die Freundin lindern,", "tokens": ["Und", "will", "die", "Freun\u00b7din", "lin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erregt ihr sanfter Hauch nur wild're Gluten,", "tokens": ["Er\u00b7regt", "ihr", "sanf\u00b7ter", "Hauch", "nur", "wild'\u00b7re", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und kann das Leid nicht mindern.", "tokens": ["Und", "kann", "das", "Leid", "nicht", "min\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach, d\u00fcrft' es einmal str\u00f6men frei ins Freie,", "tokens": ["Ach", ",", "d\u00fcrft'", "es", "ein\u00b7mal", "str\u00f6\u00b7men", "frei", "ins", "Frei\u00b7e", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "ADV", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So ruht' ich bald im Scho\u00df der ew'gen Treue.", "tokens": ["So", "ruht'", "ich", "bald", "im", "Scho\u00df", "der", "ew'\u00b7gen", "Treu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}