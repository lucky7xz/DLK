{"textgrid.poem.56876": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Tage der Gl\u00e4ubigen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Tage der Gl\u00e4ubigen", "tokens": ["Die", "Ta\u00b7ge", "der", "Gl\u00e4u\u00b7bi\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "uralten Wahns", "tokens": ["ur\u00b7al\u00b7ten", "Wahns"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "sind dahin!", "tokens": ["sind", "da\u00b7hin", "!"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "PAV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Unausl\u00f6schlich Gel\u00e4chter", "tokens": ["Un\u00b7aus\u00b7l\u00f6schlich", "Ge\u00b7l\u00e4ch\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "gr\u00fc\u00dft,", "tokens": ["gr\u00fc\u00dft", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "was sie lassen und tun.", "tokens": ["was", "sie", "las\u00b7sen", "und", "tun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Am Sonnenhimmel", "tokens": ["Am", "Son\u00b7nen\u00b7him\u00b7mel"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "schaun sie noch immer", "tokens": ["schaun", "sie", "noch", "im\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "schw\u00e4rzliche Punkte", "tokens": ["schw\u00e4rz\u00b7li\u00b7che", "Punk\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "und sprechen: \u00bbSeht!", "tokens": ["und", "spre\u00b7chen", ":", "\u00bb", "Seht", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "VVINF", "$.", "$(", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Gottes Finger", "tokens": ["Got\u00b7tes", "Fin\u00b7ger"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "deuten auf uns!\u00ab", "tokens": ["deu\u00b7ten", "auf", "uns", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Wissen sie nicht,", "tokens": ["Wis\u00b7sen", "sie", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "da\u00df sie Flecken des eigenen Augs", "tokens": ["da\u00df", "sie", "Fle\u00b7cken", "des", "ei\u00b7ge\u00b7nen", "Augs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "ART", "ADJA", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.9": {"text": "anbeten?", "tokens": ["an\u00b7be\u00b7ten", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "R\u00fchrendem Schauspiel", "tokens": ["R\u00fch\u00b7ren\u00b7dem", "Schau\u00b7spiel"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "lohnt", "tokens": ["lohnt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "unausl\u00f6schlich Gel\u00e4chter.", "tokens": ["un\u00b7aus\u00b7l\u00f6schlich", "Ge\u00b7l\u00e4ch\u00b7ter", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "B\u00e4ndigen", "tokens": ["B\u00e4n\u00b7di\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "wolln sie den Huf der Zeit,", "tokens": ["wolln", "sie", "den", "Huf", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "mit Spruch und Fluch", "tokens": ["mit", "Spruch", "und", "Fluch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "bannen das steigende Ro\u00df,", "tokens": ["ban\u00b7nen", "das", "stei\u00b7gen\u00b7de", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "drauf fr\u00fchlingsgewaltig", "tokens": ["drauf", "fr\u00fch\u00b7lings\u00b7ge\u00b7wal\u00b7tig"], "token_info": ["word", "word"], "pos": ["PAV", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "der freie Geist,", "tokens": ["der", "frei\u00b7e", "Geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "der Zukunft K\u00f6nig,", "tokens": ["der", "Zu\u00b7kunft", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "einherbraust!", "tokens": ["ein\u00b7her\u00b7braust", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Weh den Zermalmten!", "tokens": ["Weh", "den", "Zer\u00b7malm\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Ihr Ende", "tokens": ["Ihr", "En\u00b7de"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "umschallt", "tokens": ["um\u00b7schallt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "unausl\u00f6schlich Gel\u00e4chter.", "tokens": ["un\u00b7aus\u00b7l\u00f6schlich", "Ge\u00b7l\u00e4ch\u00b7ter", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "H\u00f6ren sie nichts?", "tokens": ["H\u00f6\u00b7ren", "sie", "nichts", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Vom Aufgang zum Niedergang", "tokens": ["Vom", "Auf\u00b7gang", "zum", "Nie\u00b7der\u00b7gang"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "lacht es ja unabl\u00e4ssig,", "tokens": ["lacht", "es", "ja", "un\u00b7ab\u00b7l\u00e4s\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "gr\u00fc\u00dft,", "tokens": ["gr\u00fc\u00dft", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "was sie lassen und tun,", "tokens": ["was", "sie", "las\u00b7sen", "und", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "unausl\u00f6schlich Gel\u00e4chter.", "tokens": ["un\u00b7aus\u00b7l\u00f6schlich", "Ge\u00b7l\u00e4ch\u00b7ter", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}