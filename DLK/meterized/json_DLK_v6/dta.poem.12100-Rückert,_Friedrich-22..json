{"dta.poem.12100": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was sucht ihr, Reisende, in des Gebirges Schanzen?", "tokens": ["Was", "sucht", "ihr", ",", "Rei\u00b7sen\u00b7de", ",", "in", "des", "Ge\u00b7bir\u00b7ges", "Schan\u00b7zen", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was, erster, suchest du? \u201eIch suche Stein' und Pflanzen.\u201c", "tokens": ["Was", ",", "ers\u00b7ter", ",", "su\u00b7chest", "du", "?", "\u201e", "Ich", "su\u00b7che", "Stein'", "und", "Pflan\u00b7zen", ".", "\u201c"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "$,", "ADJA", "$,", "VVFIN", "PPER", "$.", "$(", "PPER", "ADJA", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und reichlich findest du. Was suchest du, o zweiter?", "tokens": ["Und", "reich\u00b7lich", "fin\u00b7dest", "du", ".", "Was", "su\u00b7chest", "du", ",", "o", "zwei\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$.", "PWS", "VVFIN", "PPER", "$,", "FM", "FM", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eansichten, Landschaften.\u201c Hier sind sie ernst und heiter.", "tokens": ["\u201e", "an\u00b7sich\u00b7ten", ",", "Land\u00b7schaf\u00b7ten", ".", "\u201c", "Hier", "sind", "sie", "ernst", "und", "hei\u00b7ter", "."], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$.", "$(", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Was, dritter, reisest du? \u201eDie Reise zu beschreiben.\u201c", "tokens": ["Was", ",", "drit\u00b7ter", ",", "rei\u00b7sest", "du", "?", "\u201e", "Die", "Rei\u00b7se", "zu", "be\u00b7schrei\u00b7ben", ".", "\u201c"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "$,", "ADJA", "$,", "VVFIN", "PPER", "$.", "$(", "ART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch gut, doch k\u00f6nntest du wol etwas be\u00dfres treiben.", "tokens": ["Auch", "gut", ",", "doch", "k\u00f6nn\u00b7test", "du", "wol", "et\u00b7was", "be\u00df\u00b7res", "trei\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "VMFIN", "PPER", "ADV", "PIAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und endlich, vierter, du ? \u201eIch reise zum Vergn\u00fcgen.\u201c", "tokens": ["Und", "end\u00b7lich", ",", "vier\u00b7ter", ",", "du", "?", "\u201e", "Ich", "rei\u00b7se", "zum", "Ver\u00b7gn\u00fc\u00b7gen", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$,", "ADJA", "$,", "PPER", "$.", "$(", "PPER", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum doch sagst du das mit misvergn\u00fcgten Z\u00fcgen?", "tokens": ["Wa\u00b7rum", "doch", "sagst", "du", "das", "mit", "mis\u00b7ver\u00b7gn\u00fcg\u00b7ten", "Z\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PDS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}