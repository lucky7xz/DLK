{"dta.poem.12981": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "* *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Milo br\u00fcstet sich, denn er kan ochsen tragen:", "tokens": ["Ein", "Mi\u00b7lo", "br\u00fcs\u00b7tet", "sich", ",", "denn", "er", "kan", "och\u00b7sen", "tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PRF", "$,", "KON", "PPER", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein schwacher David st\u00fcrtzt den st\u00e4rcksten Goliath.", "tokens": ["Ein", "schwa\u00b7cher", "Da\u00b7vid", "st\u00fcrtzt", "den", "st\u00e4rcks\u00b7ten", "Go\u00b7li\u00b7ath", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dort geht ein elephant, der riesen auf sich hat.", "tokens": ["Dort", "geht", "ein", "e\u00b7le\u00b7phant", ",", "der", "rie\u00b7sen", "auf", "sich", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "$,", "PRELS", "VVFIN", "APPR", "PRF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wellen k\u00f6nnen auch Colossen niederschlagen.", "tokens": ["Und", "wel\u00b7len", "k\u00f6n\u00b7nen", "auch", "Co\u00b7los\u00b7sen", "nie\u00b7der\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was well\u2019 und wind nicht kan, das thut der zeiten lauf.", "tokens": ["Was", "well'", "und", "wind", "nicht", "kan", ",", "das", "thut", "der", "zei\u00b7ten", "lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKVZ", "KON", "VAFIN", "PTKNEG", "VMFIN", "$,", "PDS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die weide, so sich biegt, kan harte kiefern trutzen;", "tokens": ["Die", "wei\u00b7de", ",", "so", "sich", "biegt", ",", "kan", "har\u00b7te", "kie\u00b7fern", "trut\u00b7zen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADV", "PRF", "VVFIN", "$,", "VMFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch wenn die weiden schon mit schlancken zweigen stutzen,", "tokens": ["Doch", "wenn", "die", "wei\u00b7den", "schon", "mit", "schlan\u00b7cken", "zwei\u00b7gen", "stut\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "ADV", "APPR", "VVINF", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So reibt sie doch die glut so wol, als kiefern, auf.", "tokens": ["So", "reibt", "sie", "doch", "die", "glut", "so", "wol", ",", "als", "kie\u00b7fern", ",", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "ADV", "$,", "KOUS", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}