{"textgrid.poem.32402": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "80. Die blaue Hand", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Richter war, der sah nicht wohl:", "tokens": ["Ein", "Rich\u00b7ter", "war", ",", "der", "sah", "nicht", "wohl", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein F\u00e4rber k\u00f6mmt, der schw\u00f6ren soll.", "tokens": ["Ein", "F\u00e4r\u00b7ber", "k\u00f6mmt", ",", "der", "schw\u00f6\u00b7ren", "soll", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der F\u00e4rber hebt die blaue Hand;", "tokens": ["Der", "F\u00e4r\u00b7ber", "hebt", "die", "blau\u00b7e", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ruft der Richter: Unverstand!", "tokens": ["Da", "ruft", "der", "Rich\u00b7ter", ":", "Un\u00b7ver\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer schw\u00f6rt im Handschuh? Handschuh aus!", "tokens": ["Wer", "schw\u00f6rt", "im", "Hand\u00b7schuh", "?", "Hand\u00b7schuh", "aus", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "$.", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nein! ruft der F\u00e4rber; Brill' heraus!", "tokens": ["Nein", "!", "ruft", "der", "F\u00e4r\u00b7ber", ";", "Brill'", "he\u00b7raus", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVFIN", "ART", "NN", "$.", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}