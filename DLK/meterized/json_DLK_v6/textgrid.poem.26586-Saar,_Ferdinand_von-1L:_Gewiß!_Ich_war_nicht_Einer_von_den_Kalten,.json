{"textgrid.poem.26586": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gewi\u00df! Ich war nicht Einer von den Kalten,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gewi\u00df! Ich war nicht Einer von den Kalten,", "tokens": ["Ge\u00b7wi\u00df", "!", "Ich", "war", "nicht", "Ei\u00b7ner", "von", "den", "Kal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "PTKNEG", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die vor den Reizen steh'n der holden Frauen", "tokens": ["Die", "vor", "den", "Rei\u00b7zen", "steh'n", "der", "hol\u00b7den", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gekniff'nen Aug's, mit Worten, halben, lauen \u2013", "tokens": ["Ge\u00b7kniff'\u00b7nen", "Aug's", ",", "mit", "Wor\u00b7ten", ",", "hal\u00b7ben", ",", "lau\u00b7en", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "NE", "$,", "APPR", "NN", "$,", "ADJA", "$,", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und stets sich wissen klug im Zaum zu halten.", "tokens": ["Und", "stets", "sich", "wis\u00b7sen", "klug", "im", "Zaum", "zu", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVFIN", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nein! Ich geh\u00f6rte zu den Raschdurchwallten,", "tokens": ["Nein", "!", "Ich", "ge\u00b7h\u00f6r\u00b7te", "zu", "den", "Raschdurch\u00b7wall\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die, l\u00e4\u00dft die Sch\u00f6nheit ihrem Blick sich schauen,", "tokens": ["Die", ",", "l\u00e4\u00dft", "die", "Sch\u00f6n\u00b7heit", "ih\u00b7rem", "Blick", "sich", "schau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVFIN", "ART", "NN", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu tiefst empfinden jenes heil'ge Grauen,", "tokens": ["Zu", "tiefst", "emp\u00b7fin\u00b7den", "je\u00b7nes", "heil'\u00b7ge", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das da entstammt der Liebe Urgewalten.", "tokens": ["Das", "da", "ent\u00b7stammt", "der", "Lie\u00b7be", "Ur\u00b7ge\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vom Trank war ich durchgl\u00fcht, der Faust getrieben,", "tokens": ["Vom", "Trank", "war", "ich", "durch\u00b7gl\u00fcht", ",", "der", "Faust", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df er den Kram des Wissens schlug in Scherben,", "tokens": ["Da\u00df", "er", "den", "Kram", "des", "Wis\u00b7sens", "schlug", "in", "Scher\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um Gretchen und um Helenen zu lieben \u2013", "tokens": ["Um", "Gret\u00b7chen", "und", "um", "He\u00b7le\u00b7nen", "zu", "lie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NE", "KON", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und doch, wie jener Spanier, dessen Werben", "tokens": ["Und", "doch", ",", "wie", "je\u00b7ner", "Spa\u00b7nier", ",", "des\u00b7sen", "Wer\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "PWAV", "PDAT", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ganz ohne Zahl und Grenzen einst geblieben:", "tokens": ["Ganz", "oh\u00b7ne", "Zahl", "und", "Gren\u00b7zen", "einst", "ge\u00b7blie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An unerf\u00fcllter Sehnsucht hinzusterben.", "tokens": ["An", "un\u00b7er\u00b7f\u00fcll\u00b7ter", "Sehn\u00b7sucht", "hin\u00b7zu\u00b7ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}