{"textgrid.poem.57738": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die sieben Steinh\u00e4user", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Frauen vom Stamme der roten Hunde", "tokens": ["Die", "Frau\u00b7en", "vom", "Stam\u00b7me", "der", "ro\u00b7ten", "Hun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die schrien auf wie aus einem Munde,", "tokens": ["Die", "schri\u00b7en", "auf", "wie", "aus", "ei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die weite braune Heide erklang", "tokens": ["Die", "wei\u00b7te", "brau\u00b7ne", "Hei\u00b7de", "er\u00b7klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Von ihrem gellenden Jammergesang.", "tokens": ["Von", "ih\u00b7rem", "gel\u00b7len\u00b7den", "Jam\u00b7mer\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die W\u00f6lfe stoben entsetzt von dannen,", "tokens": ["Die", "W\u00f6l\u00b7fe", "sto\u00b7ben", "ent\u00b7setzt", "von", "dan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Rotwild polterte in die Tannen,", "tokens": ["Das", "Rot\u00b7wild", "pol\u00b7ter\u00b7te", "in", "die", "Tan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Adler ruderte hastig vorbei,", "tokens": ["Der", "Ad\u00b7ler", "ru\u00b7der\u00b7te", "has\u00b7tig", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vergr\u00e4mt von dem gellenden Jammergeschrei.", "tokens": ["Ver\u00b7gr\u00e4mt", "von", "dem", "gel\u00b7len\u00b7den", "Jam\u00b7mer\u00b7ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Die Krieger umstanden die Bahren im Kreise,", "tokens": ["Die", "Krie\u00b7ger", "um\u00b7stan\u00b7den", "die", "Bah\u00b7ren", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie sangen die dunkele Racheweise,", "tokens": ["Sie", "san\u00b7gen", "die", "dun\u00b7ke\u00b7le", "Ra\u00b7che\u00b7wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sie sangen das alte b\u00f6se Lied", "tokens": ["Sie", "san\u00b7gen", "das", "al\u00b7te", "b\u00f6\u00b7se", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von dem roten Hund, der auf Raub auszieht.", "tokens": ["Von", "dem", "ro\u00b7ten", "Hund", ",", "der", "auf", "Raub", "aus\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Der H\u00e4uptling stand zu F\u00fc\u00dfen der Bahren,", "tokens": ["Der", "H\u00e4upt\u00b7ling", "stand", "zu", "F\u00fc\u00b7\u00dfen", "der", "Bah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die rote Fellkappe auf den Haaren,", "tokens": ["Die", "ro\u00b7te", "Fell\u00b7kap\u00b7pe", "auf", "den", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die rechte Hand umschlo\u00df das Beil,", "tokens": ["Die", "rech\u00b7te", "Hand", "um\u00b7schlo\u00df", "das", "Beil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die andere lag auf dem Feuersteinkeil.", "tokens": ["Die", "an\u00b7de\u00b7re", "lag", "auf", "dem", "Feu\u00b7ers\u00b7tein\u00b7keil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Sah nicht zur Rechten, sah nicht zur Linken,", "tokens": ["Sah", "nicht", "zur", "Rech\u00b7ten", ",", "sah", "nicht", "zur", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPRART", "NN", "$,", "VVFIN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Seine Augen starrten, ohne zu blinken,", "tokens": ["Sei\u00b7ne", "Au\u00b7gen", "starr\u00b7ten", ",", "oh\u00b7ne", "zu", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Auf seiner toten S\u00f6hne Gesicht,", "tokens": ["Auf", "sei\u00b7ner", "to\u00b7ten", "S\u00f6h\u00b7ne", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Seine schmalen Lippen die zuckten nicht.", "tokens": ["Sei\u00b7ne", "schma\u00b7len", "Lip\u00b7pen", "die", "zuck\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Sein Antlitz f\u00e4rbten die Zeichen der Trauer,", "tokens": ["Sein", "Ant\u00b7litz", "f\u00e4rb\u00b7ten", "die", "Zei\u00b7chen", "der", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sieben wei\u00dfe Striche und ein blauer,", "tokens": ["Sie\u00b7ben", "wei\u00b7\u00dfe", "Stri\u00b7che", "und", "ein", "blau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "KON", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die Brust war siebenmal aufgeschlitzt,", "tokens": ["Die", "Brust", "war", "sie\u00b7ben\u00b7mal", "auf\u00b7ge\u00b7schlitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Stirne siebenfach blutig geritzt.", "tokens": ["Die", "Stir\u00b7ne", "sie\u00b7ben\u00b7fach", "blu\u00b7tig", "ge\u00b7ritzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Er hob das Beil, der Kreis sich teilte,", "tokens": ["Er", "hob", "das", "Beil", ",", "der", "Kreis", "sich", "teil\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Lager her der Zauberer eilte,", "tokens": ["Vom", "La\u00b7ger", "her", "der", "Zau\u00b7be\u00b7rer", "eil\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APZR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er f\u00fchrte herbei ein wei\u00dfes Ro\u00df,", "tokens": ["Er", "f\u00fchr\u00b7te", "her\u00b7bei", "ein", "wei\u00b7\u00dfes", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von dessen Maule der Geifer flo\u00df.", "tokens": ["Von", "des\u00b7sen", "Mau\u00b7le", "der", "Gei\u00b7fer", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Die Frauen schrien, die M\u00e4nner sangen,", "tokens": ["Die", "Frau\u00b7en", "schri\u00b7en", ",", "die", "M\u00e4n\u00b7ner", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Schilde dr\u00f6hnten, die H\u00f6rner klangen,", "tokens": ["Die", "Schil\u00b7de", "dr\u00f6hn\u00b7ten", ",", "die", "H\u00f6r\u00b7ner", "klan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Zauberer ri\u00df mit dem heiligen Stein", "tokens": ["Der", "Zau\u00b7be\u00b7rer", "ri\u00df", "mit", "dem", "hei\u00b7li\u00b7gen", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Dem Schimmel die Opfermarken ein.", "tokens": ["Dem", "Schim\u00b7mel", "die", "Op\u00b7fer\u00b7mar\u00b7ken", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Und warf ihm Staub in die wei\u00dfe M\u00e4hne,", "tokens": ["Und", "warf", "ihm", "Staub", "in", "die", "wei\u00b7\u00dfe", "M\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und rieb ihm Asche zwischen die Z\u00e4hne,", "tokens": ["Und", "rieb", "ihm", "A\u00b7sche", "zwi\u00b7schen", "die", "Z\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und sengte die Locke auf seiner Stirn", "tokens": ["Und", "seng\u00b7te", "die", "Lo\u00b7cke", "auf", "sei\u00b7ner", "Stirn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und trieb ihm das Messer in das Gehirn.", "tokens": ["Und", "trieb", "ihm", "das", "Mes\u00b7ser", "in", "das", "Ge\u00b7hirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Die H\u00f6rner heulten, die Schilde klangen,", "tokens": ["Die", "H\u00f6r\u00b7ner", "heul\u00b7ten", ",", "die", "Schil\u00b7de", "klan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Weiber schrien, die Krieger sangen,", "tokens": ["Die", "Wei\u00b7ber", "schri\u00b7en", ",", "die", "Krie\u00b7ger", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Zauberer schwang das brennende Scheit", "tokens": ["Der", "Zau\u00b7be\u00b7rer", "schwang", "das", "bren\u00b7nen\u00b7de", "Scheit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und weihte die Toten der Dunkelheit.", "tokens": ["Und", "weih\u00b7te", "die", "To\u00b7ten", "der", "Dun\u00b7kel\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.11": {"line.1": {"text": "Aus sieben Bahren schlugen die Flammen", "tokens": ["Aus", "sie\u00b7ben", "Bah\u00b7ren", "schlu\u00b7gen", "die", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und kamen in einem Rauche zusammen,", "tokens": ["Und", "ka\u00b7men", "in", "ei\u00b7nem", "Rau\u00b7che", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Des brennenden Wacholders Duft", "tokens": ["Des", "bren\u00b7nen\u00b7den", "Wa\u00b7chol\u00b7ders", "Duft"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Erf\u00fcllte weit und breit die Luft.", "tokens": ["Er\u00b7f\u00fcll\u00b7te", "weit", "und", "breit", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Die Flammen flackerten auf und nieder", "tokens": ["Die", "Flam\u00b7men", "fla\u00b7cker\u00b7ten", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und fra\u00dfen der H\u00e4uptlingss\u00f6hne Glieder,", "tokens": ["Und", "fra\u00b7\u00dfen", "der", "H\u00e4upt\u00b7lings\u00b7s\u00f6h\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Krieger sangen das b\u00f6se Lied", "tokens": ["Die", "Krie\u00b7ger", "san\u00b7gen", "das", "b\u00f6\u00b7se", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von dem roten Hund, der auf Raub auszieht.", "tokens": ["Von", "dem", "ro\u00b7ten", "Hund", ",", "der", "auf", "Raub", "aus\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "Und sangen das Lied wohl sieben Jahre,", "tokens": ["Und", "san\u00b7gen", "das", "Lied", "wohl", "sie\u00b7ben", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "CARD", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und lie\u00dfen wachsen am Kinne die Haare,", "tokens": ["Und", "lie\u00b7\u00dfen", "wach\u00b7sen", "am", "Kin\u00b7ne", "die", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "