{"dta.poem.112": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Das Bild redet das Frauen-Zimmer an.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Annehmliches Geschlecht/ ihr K\u00f6nige der Hertzen", "tokens": ["An\u00b7nehm\u00b7li\u00b7ches", "Ge\u00b7schlecht", "/", "ihr", "K\u00f6\u00b7ni\u00b7ge", "der", "Hert\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schaut dieses schlechte Bild mit keinen Eyfer an/", "tokens": ["Schaut", "die\u00b7ses", "schlech\u00b7te", "Bild", "mit", "kei\u00b7nen", "Ey\u00b7fer", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "La\u00dft euch die wen\u2019ge Pracht desselben nur nicht schmertzen/", "tokens": ["La\u00dft", "euch", "die", "wen'\u00b7ge", "Pracht", "des\u00b7sel\u00b7ben", "nur", "nicht", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "PIAT", "NN", "PDS", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil gegen die Natur die Kunst nicht z\u00fcrnen kan.", "tokens": ["Weil", "ge\u00b7gen", "die", "Na\u00b7tur", "die", "Kunst", "nicht", "z\u00fcr\u00b7nen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja st\u00fcnd\u2019s in meiner Macht ich wolte euch so schm\u00fccken/", "tokens": ["Ja", "st\u00fcn\u00b7d's", "in", "mei\u00b7ner", "Macht", "ich", "wol\u00b7te", "euch", "so", "schm\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Da\u00df dieses gantze Rund euch Weyrauch solte streun/", "tokens": ["Da\u00df", "die\u00b7ses", "gant\u00b7ze", "Rund", "euch", "Wey\u00b7rauch", "sol\u00b7te", "streun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "PPER", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch geb ich diesen Raht: spielt stets mit holden Blicken/", "tokens": ["Doch", "geb", "ich", "die\u00b7sen", "Raht", ":", "spielt", "stets", "mit", "hol\u00b7den", "Bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDAT", "NN", "$.", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So werdet ihr so sch\u00f6n bisweilen sch\u00f6ner seyn.", "tokens": ["So", "wer\u00b7det", "ihr", "so", "sch\u00f6n", "bis\u00b7wei\u00b7len", "sch\u00f6\u00b7ner", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}