{"textgrid.poem.46253": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Scheiden und lieb unsterblich", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mu\u00df es gescheiden sein! ist dises dan die stund!", "tokens": ["Mu\u00df", "es", "ge\u00b7schei\u00b7den", "sein", "!", "ist", "di\u00b7ses", "dan", "die", "stund", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "VAINF", "$.", "VAFIN", "PDS", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die stund, ach nein, die wund, die uns will haben scheiden!", "tokens": ["die", "stund", ",", "ach", "nein", ",", "die", "wund", ",", "die", "uns", "will", "ha\u00b7ben", "schei\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "XY", "PTKANT", "$,", "PRELS", "ADJD", "$,", "PRELS", "PPER", "VMFIN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wie scheiden mu\u00df ich dan! ach nein, ich mu\u00df verscheiden,", "tokens": ["wie", "schei\u00b7den", "mu\u00df", "ich", "dan", "!", "ach", "nein", ",", "ich", "mu\u00df", "ver\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVINF", "VMFIN", "PPER", "ADV", "$.", "XY", "PTKANT", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "dan ja zu gro\u00df mein schmerz und zu tief meine wund.", "tokens": ["dan", "ja", "zu", "gro\u00df", "mein", "schmerz", "und", "zu", "tief", "mei\u00b7ne", "wund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKA", "ADJD", "PPOSAT", "NN", "KON", "PTKA", "ADJD", "PPOSAT", "NN", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Zwar nicht mein eigen lied, sondern, mein rosenmund,", "tokens": ["Zwar", "nicht", "mein", "ei\u00b7gen", "lied", ",", "son\u00b7dern", ",", "mein", "ro\u00b7sen\u00b7mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "PPOSAT", "ADJD", "VVFIN", "$,", "KON", "$,", "PPOSAT", "NN", "$,"], "meter": "---+--+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "(mund, dessen s\u00fc\u00dfe k\u00fc\u00df mein herz ganz g\u00f6tlich weiden)", "tokens": ["(", "mund", ",", "des\u00b7sen", "s\u00fc\u00b7\u00dfe", "k\u00fc\u00df", "mein", "herz", "ganz", "g\u00f6t\u00b7lich", "wei\u00b7den", ")"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "PRELAT", "ADJA", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "dein seufzen, weinen, klag mich zu dem tod bescheiden", "tokens": ["dein", "seuf\u00b7zen", ",", "wei\u00b7nen", ",", "klag", "mich", "zu", "dem", "tod", "be\u00b7schei\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVINF", "$,", "VVINF", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und machen deinen tod mir, meinen tod dir kund.", "tokens": ["und", "ma\u00b7chen", "dei\u00b7nen", "tod", "mir", ",", "mei\u00b7nen", "tod", "dir", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPER", "$,", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So la\u00df mich nu von dir, thu du von mir empfangen", "tokens": ["So", "la\u00df", "mich", "nu", "von", "dir", ",", "thu", "du", "von", "mir", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "APPR", "PPER", "$,", "VVFIN", "PPER", "APPR", "PPER", "VVPP"], "meter": "-+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "den letzten letzinku\u00df! o s\u00fc\u00dfer tod, ach nein,", "tokens": ["den", "letz\u00b7ten", "let\u00b7zin\u00b7ku\u00df", "!", "o", "s\u00fc\u00b7\u00dfer", "tod", ",", "ach", "nein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "FM", "ADJA", "NN", "$,", "XY", "PTKANT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "o neue lebenskraft, die wir zugleich erlangen!", "tokens": ["o", "neu\u00b7e", "le\u00b7bens\u00b7kraft", ",", "die", "wir", "zu\u00b7gleich", "er\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dan meine seel in dich, in mich dein seelelein", "tokens": ["Dan", "mei\u00b7ne", "seel", "in", "dich", ",", "in", "mich", "dein", "see\u00b7lel\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PPER", "$,", "APPR", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "(verwechslend) haben sich durch disen ku\u00df vergangen,", "tokens": ["(", "ver\u00b7wechs\u00b7lend", ")", "ha\u00b7ben", "sich", "durch", "di\u00b7sen", "ku\u00df", "ver\u00b7gan\u00b7gen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$(", "VAFIN", "PRF", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "da\u00df unser tod und lieb nu mu\u00df unsterblich sein.", "tokens": ["da\u00df", "un\u00b7ser", "tod", "und", "lieb", "nu", "mu\u00df", "uns\u00b7terb\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "ADJD", "ADV", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}