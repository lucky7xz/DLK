{"textgrid.poem.48037": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "In Tr\u00e4nen", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Fliederbl\u00fcten fallen.", "tokens": ["Die", "Flie\u00b7der\u00b7bl\u00fc\u00b7ten", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wieder ist ein Lenz dahin", "tokens": ["Und", "wie\u00b7der", "ist", "ein", "Lenz", "da\u00b7hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "mit seinen Tr\u00e4umen allen.", "tokens": ["mit", "sei\u00b7nen", "Tr\u00e4u\u00b7men", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vom Meere wehr ein sanfter Wind", "tokens": ["Vom", "Mee\u00b7re", "wehr", "ein", "sanf\u00b7ter", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und singt die Schlummerlieder", "tokens": ["und", "singt", "die", "Schlum\u00b7mer\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "den Freuden, die entschlafen sind.", "tokens": ["den", "Freu\u00b7den", ",", "die", "ent\u00b7schla\u00b7fen", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun bl\u00fchn ja wohl die Rosen \u2013", "tokens": ["Nun", "bl\u00fchn", "ja", "wohl", "die", "Ro\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und unterm dichten Laubendach", "tokens": ["und", "un\u00b7term", "dich\u00b7ten", "Lau\u00b7ben\u00b7dach"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die Turteltauben kosen.", "tokens": ["die", "Tur\u00b7tel\u00b7tau\u00b7ben", "ko\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich seh es nicht, ich wei\u00df es kaum:", "tokens": ["Ich", "seh", "es", "nicht", ",", "ich", "wei\u00df", "es", "kaum", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "vor meinem Blick, ein Schleier,", "tokens": ["vor", "mei\u00b7nem", "Blick", ",", "ein", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "liegt ein gestorbner Traum.", "tokens": ["liegt", "ein", "ge\u00b7storb\u00b7ner", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein feuchter Tr\u00e4nenschleier", "tokens": ["Ein", "feuch\u00b7ter", "Tr\u00e4\u00b7nen\u00b7schlei\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "h\u00e4ngt zitternd \u00fcberm Rosenhag", "tokens": ["h\u00e4ngt", "zit\u00b7ternd", "\u00fc\u00b7berm", "Ro\u00b7sen\u00b7hag"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und wandelt mir den Sommertag", "tokens": ["und", "wan\u00b7delt", "mir", "den", "Som\u00b7mer\u00b7tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "zur d\u00fcstern Totenfeier.", "tokens": ["zur", "d\u00fcs\u00b7tern", "To\u00b7ten\u00b7fei\u00b7er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}