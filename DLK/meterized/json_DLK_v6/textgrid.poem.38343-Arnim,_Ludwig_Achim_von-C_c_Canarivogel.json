{"textgrid.poem.38343": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "C c Canarivogel", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das lieb Canariv\u00f6gelein k\u00f6mmt her aus fremdem Land,", "tokens": ["Das", "lieb", "Ca\u00b7na\u00b7ri\u00b7v\u00f6\u00b7ge\u00b7lein", "k\u00f6mmt", "her", "aus", "frem\u00b7dem", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Es singt gar sch\u00f6n, zart, hell und rein, wie allen ist bekannt,", "tokens": ["Es", "singt", "gar", "sch\u00f6n", ",", "zart", ",", "hell", "und", "rein", ",", "wie", "al\u00b7len", "ist", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,", "PWAV", "PIS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Den Zucker fri\u00dft es gerne, doch nimmt es auch vorlieb,", "tokens": ["Den", "Zu\u00b7cker", "fri\u00dft", "es", "ger\u00b7ne", ",", "doch", "nimmt", "es", "auch", "vor\u00b7lieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn man ihm Hanfsaamkerne, und R\u00fcbesaamen giebt.", "tokens": ["Wenn", "man", "ihm", "Hanf\u00b7saam\u00b7ker\u00b7ne", ",", "und", "R\u00fc\u00b7be\u00b7saa\u00b7men", "giebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "NN", "$,", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}