{"textgrid.poem.61247": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Herbstklage", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O weh, wie ist so rasch dahin", "tokens": ["O", "weh", ",", "wie", "ist", "so", "rasch", "da\u00b7hin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "PWAV", "VAFIN", "ADV", "ADJD", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der gr\u00fcne Sommer gegangen", "tokens": ["Der", "gr\u00fc\u00b7ne", "Som\u00b7mer", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und hat mir doch den tr\u00fcben Sinn", "tokens": ["Und", "hat", "mir", "doch", "den", "tr\u00fc\u00b7ben", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Freuden nicht umfangen!", "tokens": ["Mit", "Freu\u00b7den", "nicht", "um\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem Maien wollt' ich bieten Gru\u00df,", "tokens": ["Dem", "Mai\u00b7en", "wollt'", "ich", "bie\u00b7ten", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da h\u00f6r' ich schon um meinen Fu\u00df", "tokens": ["Da", "h\u00f6r'", "ich", "schon", "um", "mei\u00b7nen", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die fallenden Bl\u00e4tter rauschen.", "tokens": ["Die", "fal\u00b7len\u00b7den", "Bl\u00e4t\u00b7ter", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "O weh, nun hab' ich wieder ein Jahr", "tokens": ["O", "weh", ",", "nun", "hab'", "ich", "wie\u00b7der", "ein", "Jahr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Geharrt auf Gl\u00fcck und Frommen,", "tokens": ["Ge\u00b7harrt", "auf", "Gl\u00fcck", "und", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ist das Gl\u00fcck doch nimmerdar", "tokens": ["Und", "ist", "das", "Gl\u00fcck", "doch", "nim\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An meine T\u00fcr gekommen;", "tokens": ["An", "mei\u00b7ne", "T\u00fcr", "ge\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Oder es kam in N\u00e4chten tief,", "tokens": ["O\u00b7der", "es", "kam", "in", "N\u00e4ch\u00b7ten", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da ich festen Schlummer schlief,", "tokens": ["Da", "ich", "fes\u00b7ten", "Schlum\u00b7mer", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ist vor\u00fcbergezogen.", "tokens": ["Und", "ist", "vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Mein Leben deucht' mir als ein Traum,", "tokens": ["Mein", "Le\u00b7ben", "deucht'", "mir", "als", "ein", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den ich getr\u00e4umet habe;", "tokens": ["Den", "ich", "ge\u00b7tr\u00e4u\u00b7met", "ha\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Rechter Freude denk' ich kaum,", "tokens": ["Rech\u00b7ter", "Freu\u00b7de", "denk'", "ich", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seitdem ich war ein Knabe.", "tokens": ["Seit\u00b7dem", "ich", "war", "ein", "Kna\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Tanz und Sang zergeht mit Gram,", "tokens": ["Tanz", "und", "Sang", "zer\u00b7geht", "mit", "Gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und wenn die Liebe Abschied nahm,", "tokens": ["Und", "wenn", "die", "Lie\u00b7be", "Ab\u00b7schied", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wohl nimmer kehret sie wieder.", "tokens": ["Wohl", "nim\u00b7mer", "keh\u00b7ret", "sie", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Welt ward falsch und eitel Schein,", "tokens": ["Die", "Welt", "ward", "falsch", "und", "ei\u00b7tel", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie soll sie mir gefallen?", "tokens": ["Wie", "soll", "sie", "mir", "ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "An Bechers Rande blinkt der Wein,", "tokens": ["An", "Be\u00b7chers", "Ran\u00b7de", "blinkt", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Doch drunten schwimmen die Gallen.", "tokens": ["Doch", "drun\u00b7ten", "schwim\u00b7men", "die", "Gal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Was ich redlich focht, mi\u00dflang,", "tokens": ["Was", "ich", "red\u00b7lich", "focht", ",", "mi\u00df\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was ich fr\u00f6hlich sang, verklang", "tokens": ["Was", "ich", "fr\u00f6h\u00b7lich", "sang", ",", "ver\u00b7klang"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "PPER", "ADJD", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie Herbstwind \u00fcber den Stoppeln.", "tokens": ["Wie", "Herbst\u00b7wind", "\u00fc\u00b7ber", "den", "Stop\u00b7peln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "O weh, nun bin ich gar allein", "tokens": ["O", "weh", ",", "nun", "bin", "ich", "gar", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit meinem Harm geblieben.", "tokens": ["Mit", "mei\u00b7nem", "Harm", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dahin mein Jugendsonnenschein!", "tokens": ["Da\u00b7hin", "mein", "Ju\u00b7gend\u00b7son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dahin mein Singen und Lieben!", "tokens": ["Da\u00b7hin", "mein", "Sin\u00b7gen", "und", "Lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "KON", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der Abend graut, die Luft geht kalt -", "tokens": ["Der", "A\u00b7bend", "graut", ",", "die", "Luft", "geht", "kalt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Winter, Winter, kommst du bald,", "tokens": ["Win\u00b7ter", ",", "Win\u00b7ter", ",", "kommst", "du", "bald", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf meinen H\u00fcgel zu schneien?", "tokens": ["Auf", "mei\u00b7nen", "H\u00fc\u00b7gel", "zu", "schnei\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}