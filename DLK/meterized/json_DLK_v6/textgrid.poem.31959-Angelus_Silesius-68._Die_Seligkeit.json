{"textgrid.poem.31959": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "68. Die Seligkeit", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist die Seligkeit? Ein Zuflu\u00df aller Freuden,", "tokens": ["Was", "ist", "die", "Se\u00b7lig\u00b7keit", "?", "Ein", "Zu\u00b7flu\u00df", "al\u00b7ler", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein stetes Anschaun Gotts, ein Lieben ohn Verdru\u00df,", "tokens": ["Ein", "ste\u00b7tes", "An\u00b7schaun", "Gotts", ",", "ein", "Lie\u00b7ben", "ohn", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,", "ART", "ADJA", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Leben ohne Tod, ein s\u00fc\u00dfer Jesus Ku\u00df,", "tokens": ["Ein", "Le\u00b7ben", "oh\u00b7ne", "Tod", ",", "ein", "s\u00fc\u00b7\u00dfer", "Je\u00b7sus", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ART", "ADJA", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht einen Augenblick vom Br\u00e4utgam sein gescheiden. ", "tokens": ["Nicht", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", "vom", "Br\u00e4ut\u00b7gam", "sein", "ge\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPRART", "NE", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}