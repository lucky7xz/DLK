{"textgrid.poem.49555": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die Wiederkunft", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie kommt, sie kommt zur\u00fcck, f\u00fcr die ich stets gebrennet;", "tokens": ["Sie", "kommt", ",", "sie", "kommt", "zu\u00b7r\u00fcck", ",", "f\u00fcr", "die", "ich", "stets", "ge\u00b7bren\u00b7net", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$,", "APPR", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schon morgen wird durch sie die Gegend wieder sch\u00f6n.", "tokens": ["Schon", "mor\u00b7gen", "wird", "durch", "sie", "die", "Ge\u00b7gend", "wie\u00b7der", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "APPR", "PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich will bi\u00df an den Baum, an dem wir uns getrennet,", "tokens": ["Ich", "will", "bi\u00df", "an", "den", "Baum", ",", "an", "dem", "wir", "uns", "ge\u00b7tren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr froh entgegen gehn.", "tokens": ["Ihr", "froh", "ent\u00b7ge\u00b7gen", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vollkommen ist mein Gl\u00fcck, und ihr' und eure Huld,", "tokens": ["Voll\u00b7kom\u00b7men", "ist", "mein", "Gl\u00fcck", ",", "und", "ih\u00b7r'", "und", "eu\u00b7re", "Huld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Ihr G\u00f6tter! wann sie mich von weitem schon erkennet", "tokens": ["Ihr", "G\u00f6t\u00b7ter", "!", "wann", "sie", "mich", "von", "wei\u00b7tem", "schon", "er\u00b7ken\u00b7net"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "PPER", "PRF", "APPR", "PIS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "An meiner Ungedult!", "tokens": ["An", "mei\u00b7ner", "Un\u00b7ge\u00b7dult", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}