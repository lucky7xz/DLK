{"textgrid.poem.57544": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Akkorde", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie kommt es, da\u00df die Saiten widerklingen,", "tokens": ["Wie", "kommt", "es", ",", "da\u00df", "die", "Sai\u00b7ten", "wi\u00b7der\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sie ber\u00fchrt ein gleichgestimmter Ton,", "tokens": ["Wenn", "sie", "be\u00b7r\u00fchrt", "ein", "gleich\u00b7ges\u00b7timm\u00b7ter", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df alte Tr\u00e4ume aus dem Nebel dringen,", "tokens": ["Da\u00df", "al\u00b7te", "Tr\u00e4u\u00b7me", "aus", "dem", "Ne\u00b7bel", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der sie umgraute lange Jahre schon,", "tokens": ["Der", "sie", "um\u00b7grau\u00b7te", "lan\u00b7ge", "Jah\u00b7re", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie kommt's, da\u00df unser Herz ersch\u00fcttert", "tokens": ["Wie", "kommt's", ",", "da\u00df", "un\u00b7ser", "Herz", "er\u00b7sch\u00fct\u00b7tert"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zuweilen ein allt\u00e4glich Wort,", "tokens": ["Zu\u00b7wei\u00b7len", "ein", "all\u00b7t\u00e4g\u00b7lich", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df alte Tr\u00e4ume neu erstehen,", "tokens": ["Da\u00df", "al\u00b7te", "Tr\u00e4u\u00b7me", "neu", "er\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Verschollne Lieder uns umwehen,", "tokens": ["Ver\u00b7scholl\u00b7ne", "Lie\u00b7der", "uns", "um\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sprecht, warum dann das Herz erzittert? \u2013", "tokens": ["Sprecht", ",", "wa\u00b7rum", "dann", "das", "Herz", "er\u00b7zit\u00b7tert", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Weil angeschlagen ein Akkord!", "tokens": ["Weil", "an\u00b7ge\u00b7schla\u00b7gen", "ein", "Ak\u00b7kord", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr alle habt es doch schon oft empfunden,", "tokens": ["Ihr", "al\u00b7le", "habt", "es", "doch", "schon", "oft", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "PPER", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ungerufen, durch ein fremdes Wort", "tokens": ["Da\u00df", "un\u00b7ge\u00b7ru\u00b7fen", ",", "durch", "ein", "frem\u00b7des", "Wort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geweckt, aus lange schon verge\u00dfnen Stunden", "tokens": ["Ge\u00b7weckt", ",", "aus", "lan\u00b7ge", "schon", "ver\u00b7ge\u00df\u00b7nen", "Stun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "APPR", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Ton erklang im tiefsten Herzenshort;", "tokens": ["Ein", "Ton", "er\u00b7klang", "im", "tiefs\u00b7ten", "Her\u00b7zens\u00b7hort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein andrer Klang reiht sich dann leise", "tokens": ["Ein", "an\u00b7drer", "Klang", "reiht", "sich", "dann", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem ersten an, man wei\u00df nicht wie;", "tokens": ["Dem", "ers\u00b7ten", "an", ",", "man", "wei\u00df", "nicht", "wie", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKVZ", "$,", "PIS", "VVFIN", "PTKNEG", "KOKOM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die angeschlagnen Saiten klingen,", "tokens": ["Die", "an\u00b7ge\u00b7schlag\u00b7nen", "Sai\u00b7ten", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die wirren T\u00f6ne sich verschlingen", "tokens": ["Die", "wir\u00b7ren", "T\u00f6\u00b7ne", "sich", "ver\u00b7schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Zu einer altbekannten Weise", "tokens": ["Zu", "ei\u00b7ner", "alt\u00b7be\u00b7kann\u00b7ten", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und l\u00e4ngst geliebten Melodie.", "tokens": ["Und", "l\u00e4ngst", "ge\u00b7lieb\u00b7ten", "Me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwar \u00fcbert\u00e4ubt die sch\u00fcchtern leisen Kl\u00e4nge", "tokens": ["Zwar", "\u00fc\u00b7bert\u00b7\u00e4ubt", "die", "sch\u00fcch\u00b7tern", "lei\u00b7sen", "Kl\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am hellen Tag das bunte Einerlei", "tokens": ["Am", "hel\u00b7len", "Tag", "das", "bun\u00b7te", "Ei\u00b7ner\u00b7lei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Sterbeseufzer und Triumphges\u00e4nge,", "tokens": ["Der", "Ster\u00b7be\u00b7seuf\u00b7zer", "und", "Tri\u00b7umph\u00b7ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verzweifelt Lachen und des Schmerzes Schrei \u2013", "tokens": ["Ver\u00b7zwei\u00b7felt", "La\u00b7chen", "und", "des", "Schmer\u00b7zes", "Schrei", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im Herzen klingt's, du fragst erschrocken:", "tokens": ["Im", "Her\u00b7zen", "klingt's", ",", "du", "fragst", "er\u00b7schro\u00b7cken", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbwas wollet ihr, wo kommt ihr her?\u00ab", "tokens": ["\u00bb", "was", "wol\u00b7let", "ihr", ",", "wo", "kommt", "ihr", "her", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VMFIN", "PPER", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da schallt ein fremder Laut dazwischen,", "tokens": ["Da", "schallt", "ein", "frem\u00b7der", "Laut", "da\u00b7zwi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "APPR", "PAV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie im Konzert ein rohes Zischen,", "tokens": ["Wie", "im", "Kon\u00b7zert", "ein", "ro\u00b7hes", "Zi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die zarten Herzenst\u00f6ne stocken,", "tokens": ["Die", "zar\u00b7ten", "Her\u00b7zen\u00b7st\u00f6\u00b7ne", "sto\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Du lauschst, doch h\u00f6rst du jetzt nichts mehr.", "tokens": ["Du", "lauschst", ",", "doch", "h\u00f6rst", "du", "jetzt", "nichts", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch wenn du abends m\u00fcde dich gefl\u00fcchtet", "tokens": ["Doch", "wenn", "du", "a\u00b7bends", "m\u00fc\u00b7de", "dich", "ge\u00b7fl\u00fcch\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJD", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fort aus des Alltagslebens \u00f6dem Plan", "tokens": ["Fort", "aus", "des", "All\u00b7tags\u00b7le\u00b7bens", "\u00f6\u00b7dem", "Plan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und der Verstand tyrannisch nicht mehr richtet", "tokens": ["Und", "der", "Ver\u00b7stand", "ty\u00b7ran\u00b7nisch", "nicht", "mehr", "rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "PTKNEG", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Seele durch die plattgetretne Bahn,", "tokens": ["Die", "See\u00b7le", "durch", "die", "platt\u00b7ge\u00b7tret\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In solcher Zeit der D\u00e4mmerungen,", "tokens": ["In", "sol\u00b7cher", "Zeit", "der", "D\u00e4m\u00b7me\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da bl\u00fcht und gr\u00fcnt das Tote fort,", "tokens": ["Da", "bl\u00fcht", "und", "gr\u00fcnt", "das", "To\u00b7te", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Kinderzeit vertr\u00e4umte Freuden", "tokens": ["Der", "Kin\u00b7der\u00b7zeit", "ver\u00b7tr\u00e4um\u00b7te", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Verkl\u00e4rn des Augenblickes Leiden,", "tokens": ["Ver\u00b7kl\u00e4rn", "des", "Au\u00b7gen\u00b7bli\u00b7ckes", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und bilden in dir engverschlungen", "tokens": ["Und", "bil\u00b7den", "in", "dir", "eng\u00b7ver\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Den herzbegl\u00fcckendsten Akkord.", "tokens": ["Den", "herz\u00b7be\u00b7gl\u00fc\u00b7ckends\u00b7ten", "Ak\u00b7kord", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}