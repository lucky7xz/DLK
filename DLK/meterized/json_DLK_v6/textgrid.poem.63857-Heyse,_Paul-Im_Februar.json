{"textgrid.poem.63857": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Im Februar", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leise knospt es in den G\u00e4rten,", "tokens": ["Lei\u00b7se", "knospt", "es", "in", "den", "G\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zarte Sonne blitzt im Hain.", "tokens": ["Zar\u00b7te", "Son\u00b7ne", "blitzt", "im", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Freunde, die Lazerten,", "tokens": ["Mei\u00b7ne", "Freun\u00b7de", ",", "die", "La\u00b7zer\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00c4ugeln vor aus dem Gestein;", "tokens": ["\u00c4u\u00b7geln", "vor", "aus", "dem", "Ge\u00b7stein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wittern mit den feinen N\u00e4schen,", "tokens": ["Wit\u00b7tern", "mit", "den", "fei\u00b7nen", "N\u00e4\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob es nun schon wirklich lenzt,", "tokens": ["Ob", "es", "nun", "schon", "wirk\u00b7lich", "lenzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie zwischen jungen Gr\u00e4schen", "tokens": ["Da\u00df", "sie", "zwi\u00b7schen", "jun\u00b7gen", "Gr\u00e4sc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tanzen k\u00f6nnten leichtgeschw\u00e4nzt.", "tokens": ["Tan\u00b7zen", "k\u00f6nn\u00b7ten", "leicht\u00b7ge\u00b7schw\u00e4nzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, ihr unbedachten Kinder,", "tokens": ["Ach", ",", "ihr", "un\u00b7be\u00b7dach\u00b7ten", "Kin\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00fcpft zur\u00fcck nur unges\u00e4umt", "tokens": ["Schl\u00fcpft", "zu\u00b7r\u00fcck", "nur", "un\u00b7ge\u00b7s\u00e4umt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In die Ritzen, wo in blinder", "tokens": ["In", "die", "Rit\u00b7zen", ",", "wo", "in", "blin\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frostnacht ihr die Zeit vertr\u00e4umt.", "tokens": ["Frost\u00b7nacht", "ihr", "die", "Zeit", "ver\u00b7tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schlaft nur weiter noch ein Weilchen,", "tokens": ["Schlaft", "nur", "wei\u00b7ter", "noch", "ein", "Weil\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis der Fr\u00fchling naht mit Macht.", "tokens": ["Bis", "der", "Fr\u00fch\u00b7ling", "naht", "mit", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht, es sind ja auch die Veilchen", "tokens": ["Seht", ",", "es", "sind", "ja", "auch", "die", "Veil\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "VAFIN", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch am Bach nicht aufgewacht,", "tokens": ["Noch", "am", "Bach", "nicht", "auf\u00b7ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und ihr m\u00fc\u00dftet Hungers sterben,", "tokens": ["Und", "ihr", "m\u00fc\u00df\u00b7tet", "Hun\u00b7gers", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nirgends bietet sich ein Schmaus,", "tokens": ["Nir\u00b7gends", "bie\u00b7tet", "sich", "ein", "Schmaus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn in dieser Luft, der herben,", "tokens": ["Denn", "in", "die\u00b7ser", "Luft", ",", "der", "her\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wagt kein M\u00fcckchen sich hinaus.", "tokens": ["Wagt", "kein", "M\u00fcck\u00b7chen", "sich", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}