{"textgrid.poem.33132": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "67. Auf Herrn Johann Andreas Rothens 40sten Jahrs-Tag", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Du der Herzen K\u00f6nig bist,", "tokens": ["Der", "Du", "der", "Her\u00b7zen", "K\u00f6\u00b7nig", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und aller Kr\u00e4fte jener Welten,", "tokens": ["Und", "al\u00b7ler", "Kr\u00e4f\u00b7te", "je\u00b7ner", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem unser Herz gereget ist;", "tokens": ["Dem", "un\u00b7ser", "Herz", "ge\u00b7re\u00b7get", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df seine Regung vor Dir gelten:", "tokens": ["La\u00df", "sei\u00b7ne", "Re\u00b7gung", "vor", "Dir", "gel\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dir opfert auf der Herrenhut", "tokens": ["Dir", "op\u00b7fert", "auf", "der", "Her\u00b7ren\u00b7hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Hauffe Deiner Unterthanen", "tokens": ["Ein", "Hauf\u00b7fe", "Dei\u00b7ner", "Un\u00b7ter\u00b7tha\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den zehnten Theil von seinem Gut,", "tokens": ["Den", "zehn\u00b7ten", "Theil", "von", "sei\u00b7nem", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und die vom Feind erk\u00e4mpfte Fahnen.", "tokens": ["Und", "die", "vom", "Feind", "er\u00b7k\u00e4mpf\u00b7te", "Fah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Uns ist zwar wohl bekant,", "tokens": ["Uns", "ist", "zwar", "wohl", "be\u00b7kant", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Wie dis Geschenk bewandt:", "tokens": ["Wie", "dis", "Ge\u00b7schenk", "be\u00b7wandt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Du pflegst nichts halbes anzunehmen.", "tokens": ["Du", "pflegst", "nichts", "hal\u00b7bes", "an\u00b7zu\u00b7neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJA", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Bis da\u00df wir alle nun", "tokens": ["Bis", "da\u00df", "wir", "al\u00b7le", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PIS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Die theure Wahrheit thun,", "tokens": ["Die", "theu\u00b7re", "Wahr\u00b7heit", "thun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}