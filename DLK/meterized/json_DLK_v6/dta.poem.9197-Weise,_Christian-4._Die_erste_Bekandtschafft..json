{"dta.poem.9197": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "4.  Die erste Bekandtschafft.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Lustig/ mein liebgen das lernet mich kennen/", "tokens": ["Lus\u00b7tig", "/", "mein", "lieb\u00b7gen", "das", "ler\u00b7net", "mich", "ken\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPOSAT", "VVFIN", "PDS", "VVFIN", "PPER", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Sie lacht mich schon ein b\u00fc\u00dfgen an:", "tokens": ["Sie", "lacht", "mich", "schon", "ein", "b\u00fc\u00df\u00b7gen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lustig/ mein hertze mag braten und brennen.", "tokens": ["Lus\u00b7tig", "/", "mein", "hert\u00b7ze", "mag", "bra\u00b7ten", "und", "bren\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPOSAT", "NN", "VMFIN", "VVINF", "KON", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Wann ich mich hier erqvicken kan:", "tokens": ["Wann", "ich", "mich", "hier", "er\u00b7qvi\u00b7cken", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lustig mein kindgen/ mein engel/ mein liecht", "tokens": ["Lus\u00b7tig", "mein", "kind\u00b7gen", "/", "mein", "en\u00b7gel", "/", "mein", "liecht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "L\u00e4sset sich k\u00fcssen und wehret sich nicht.", "tokens": ["L\u00e4s\u00b7set", "sich", "k\u00fcs\u00b7sen", "und", "weh\u00b7ret", "sich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVINF", "KON", "VVFIN", "PRF", "PTKNEG", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}