{"textgrid.poem.53648": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Silvester", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was fange ich Silvester an?", "tokens": ["Was", "fan\u00b7ge", "ich", "Sil\u00b7ves\u00b7ter", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geh ich in Frack und meinen kessen", "tokens": ["Geh", "ich", "in", "Frack", "und", "mei\u00b7nen", "kes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "blausanen Str\u00fcmpfen zu dem Essen,", "tokens": ["blau\u00b7sa\u00b7nen", "Str\u00fcmp\u00b7fen", "zu", "dem", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "das Herr Generaldirektor gibt?", "tokens": ["das", "Herr", "Ge\u00b7ne\u00b7ral\u00b7di\u00b7rek\u00b7tor", "gibt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wo man heut nur beim Tanzen schiebt?", "tokens": ["Wo", "man", "heut", "nur", "beim", "Tan\u00b7zen", "schiebt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Hausfrau dehnt sich wild im Sessel \u2013", "tokens": ["Die", "Haus\u00b7frau", "dehnt", "sich", "wild", "im", "Ses\u00b7sel", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "der Hausherr tut das sonst bei Dressel \u2013,", "tokens": ["der", "Haus\u00b7herr", "tut", "das", "sonst", "bei", "Dres\u00b7sel", "\u2013", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PDS", "ADV", "APPR", "NE", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "das junge Volk verdr\u00fcckt sich bald.", "tokens": ["das", "jun\u00b7ge", "Volk", "ver\u00b7dr\u00fcckt", "sich", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Sekt ist warm. Der Kaffee kalt \u2013", "tokens": ["Der", "Sekt", "ist", "warm", ".", "Der", "Kaf\u00b7fee", "kalt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Prost Neujahr!", "tokens": ["Prost", "Neu\u00b7jahr", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Ach, ich armer Mann!", "tokens": ["Ach", ",", "ich", "ar\u00b7mer", "Mann", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Was fange ich Silvester an?", "tokens": ["Was", "fan\u00b7ge", "ich", "Sil\u00b7ves\u00b7ter", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "W\u00e4lz ich mich im Familienscho\u00dfe?", "tokens": ["W\u00e4lz", "ich", "mich", "im", "Fa\u00b7mi\u00b7li\u00b7en\u00b7scho\u00b7\u00dfe", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Erst gibt es Hecht mit s\u00fc\u00dfer Sauce,", "tokens": ["Erst", "gibt", "es", "Hecht", "mit", "s\u00fc\u00b7\u00dfer", "Sau\u00b7ce", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "dann gibts Gelee. Dann gibt es Krach.", "tokens": ["dann", "gibts", "Ge\u00b7lee", ".", "Dann", "gibt", "es", "Krach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$.", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der greise Manne selbst wird schwach.", "tokens": ["Der", "grei\u00b7se", "Man\u00b7ne", "selbst", "wird", "schwach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aufsteigen \u00fcble Knatschger\u00fcche.", "tokens": ["Auf\u00b7stei\u00b7gen", "\u00fcb\u00b7le", "Knatschge\u00b7r\u00fc\u00b7che", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der Hans knutscht Minna in der K\u00fcche.", "tokens": ["Der", "Hans", "knutscht", "Min\u00b7na", "in", "der", "K\u00fc\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "NE", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Um zw\u00f6lf steht R\u00fchrung auf der Uhr.", "tokens": ["Um", "zw\u00f6lf", "steht", "R\u00fch\u00b7rung", "auf", "der", "Uhr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Bowle \u2013! (\u203aLeichter Mosel\u2039 nur \u2013).", "tokens": ["Die", "Bow\u00b7le", "\u2013", "!", "(", "\u203a", "Leich\u00b7ter", "Mo\u00b7sel", "\u2039", "nur", "\u2013", ")", "."], "token_info": ["word", "word", "punct", "punct", "punct", "punct", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "$(", "$.", "$(", "CARD", "NN", "NE", "$(", "ADV", "$(", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Prost Neujahr!", "tokens": ["Prost", "Neu\u00b7jahr", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Ach, ich armer Mann!", "tokens": ["Ach", ",", "ich", "ar\u00b7mer", "Mann", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Was fange ich Silvester an?", "tokens": ["Was", "fan\u00b7ge", "ich", "Sil\u00b7ves\u00b7ter", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mach ich ins Am\u00fcsiervergn\u00fcgen?", "tokens": ["Mach", "ich", "ins", "A\u00b7m\u00fc\u00b7sier\u00b7ver\u00b7gn\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dr\u00fcck ich mich in den Stadtbahnz\u00fcgen?", "tokens": ["Dr\u00fcck", "ich", "mich", "in", "den", "Stadt\u00b7bahn\u00b7z\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schrei ich in einer schwulen Bar:", "tokens": ["Schrei", "ich", "in", "ei\u00b7ner", "schwu\u00b7len", "Bar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbhuch, Schneeballbl\u00fcte! Prost Neujahr \u2013!\u00ab", "tokens": ["\u00bb", "huch", ",", "Schnee\u00b7ball\u00b7bl\u00fc\u00b7te", "!", "Prost", "Neu\u00b7jahr", "\u2013", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "$.", "NN", "NN", "$(", "$.", "$("], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Geh ich zur Firma Sklarz Geschwister \u2013", "tokens": ["Geh", "ich", "zur", "Fir\u00b7ma", "Sklarz", "Ge\u00b7schwis\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "NE", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "(nein, nein \u2013 ich bin ja kein Minister!)", "tokens": ["(", "nein", ",", "nein", "\u2013", "ich", "bin", "ja", "kein", "Mi\u00b7nis\u00b7ter", "!", ")"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$,", "PTKANT", "$(", "PPER", "VAFIN", "ADV", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Bleigie\u00dfen? Ists ein Fladen klein:", "tokens": ["Blei\u00b7gie\u00b7\u00dfen", "?", "Ists", "ein", "Fla\u00b7den", "klein", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dies wird wohl Deutschlands Zukunft sein . . .", "tokens": ["Dies", "wird", "wohl", "Deutschlands", "Zu\u00b7kunft", "sein", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJA", "NN", "VAINF", "$.", "$.", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.9": {"text": "Prost Neujahr!", "tokens": ["Prost", "Neu\u00b7jahr", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Helft mir armem Mann!", "tokens": ["Helft", "mir", "ar\u00b7mem", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Was fang ich blo\u00df Silvester an \u2013?", "tokens": ["Was", "fang", "ich", "blo\u00df", "Sil\u00b7ves\u00b7ter", "an", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "NN", "PTKVZ", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}