{"textgrid.poem.35363": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den trifft f\u00fcrwahr ein unverdientes Los,", "tokens": ["Den", "trifft", "f\u00fcr\u00b7wahr", "ein", "un\u00b7ver\u00b7dien\u00b7tes", "Los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der, als ein wackrer Sohn des fernen Strandes,", "tokens": ["Der", ",", "als", "ein", "wack\u00b7rer", "Sohn", "des", "fer\u00b7nen", "Stran\u00b7des", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Trotz aller Mannheit, allen Widerstandes,", "tokens": ["Trotz", "al\u00b7ler", "Mann\u00b7heit", ",", "al\u00b7len", "Wi\u00b7der\u00b7stan\u00b7des", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Einsam veratmet in der Wellen Scho\u00df;", "tokens": ["Ein\u00b7sam", "ver\u00b7at\u00b7met", "in", "der", "Wel\u00b7len", "Scho\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Nach tausend St\u00fcrmen, K\u00e4mpfen, Abenteuern", "tokens": ["Nach", "tau\u00b7send", "St\u00fcr\u00b7men", ",", "K\u00e4mp\u00b7fen", ",", "A\u00b7bent\u00b7eu\u00b7ern"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "CARD", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bezwungen von der Elemente Wut", "tokens": ["Be\u00b7zwun\u00b7gen", "von", "der", "E\u00b7le\u00b7men\u00b7te", "Wut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Spurlos verschwindet in der grausen Flut,", "tokens": ["Spur\u00b7los", "ver\u00b7schwin\u00b7det", "in", "der", "grau\u00b7sen", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Umzingelt von des Abgrunds Ungeheuern!", "tokens": ["Um\u00b7zin\u00b7gelt", "von", "des", "Ab\u00b7grunds", "Un\u00b7ge\u00b7heu\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}