{"textgrid.poem.37425": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Auch er", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "R\u00fchrend sch\u00f6ne Herzgeschichten,", "tokens": ["R\u00fch\u00b7rend", "sch\u00f6\u00b7ne", "Herz\u00b7ge\u00b7schich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihm vor der Seele schweben,", "tokens": ["Die", "ihm", "vor", "der", "See\u00b7le", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00df der Dichter zu berichten.", "tokens": ["Wei\u00df", "der", "Dich\u00b7ter", "zu", "be\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wovon aber soll er leben?", "tokens": ["Wo\u00b7von", "a\u00b7ber", "soll", "er", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was er fein zusammenharkte,", "tokens": ["Was", "er", "fein", "zu\u00b7sam\u00b7men\u00b7hark\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sauber eingebundne Werklein,", "tokens": ["Sau\u00b7ber", "ein\u00b7ge\u00b7bund\u00b7ne", "Wer\u00b7klein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "F\u00fchrt er eben auch zum Markte,", "tokens": ["F\u00fchrt", "er", "e\u00b7ben", "auch", "zum", "Mark\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Bauer seine Ferklein.", "tokens": ["Wie", "der", "Bau\u00b7er", "sei\u00b7ne", "Fer\u00b7klein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}