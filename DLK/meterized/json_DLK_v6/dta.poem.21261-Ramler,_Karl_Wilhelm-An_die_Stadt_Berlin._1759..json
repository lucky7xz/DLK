{"dta.poem.21261": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An die Stadt Berlin.  \n 1759.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20476-3", "language": ["de:0.99"], "booktitle": "Ramler, Karl Wilhelm: Oden. Berlin, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sahe sie! (mir zittern die Gebeine!)", "tokens": ["Ich", "sa\u00b7he", "sie", "!", "(", "mir", "zit\u00b7tern", "die", "Ge\u00b7bei\u00b7ne", "!", ")"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich sah, gl\u00fcckseliges Berlin,", "tokens": ["Ich", "sah", ",", "gl\u00fcck\u00b7se\u00b7li\u00b7ges", "Ber\u00b7lin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NE", "$,"], "meter": "-+-+-+++", "measure": "unknown.measure.penta"}, "line.3": {"text": "Die G\u00f6ttinn deines Stroms vor deinem", "tokens": ["Die", "G\u00f6t\u00b7tinn", "dei\u00b7nes", "Stroms", "vor", "dei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihren Schw\u00e4nen ziehn!", "tokens": ["Mit", "ih\u00b7ren", "Schw\u00e4\u00b7nen", "ziehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Verg\u00f6nne mir, Najade, nachzulallen,", "tokens": ["Ver\u00b7g\u00f6n\u00b7ne", "mir", ",", "Na\u00b7ja\u00b7de", ",", "nach\u00b7zu\u00b7lal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was mein erstauntes Ohr durchdrang,", "tokens": ["Was", "mein", "er\u00b7staun\u00b7tes", "Ohr", "durch\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was dein G\u00f6ttermund den Faunen", "tokens": ["Und", "was", "dein", "G\u00f6t\u00b7ter\u00b7mund", "den", "Fau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hamadryaden sang. \u2014 \u2014", "tokens": ["Ha\u00b7mad\u00b7ry\u00b7a\u00b7den", "sang", "."], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["NE", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sey mir gegr\u00fcsst, Augusta, meine", "tokens": ["Sey", "mir", "ge\u00b7gr\u00fcs\u00b7st", ",", "Au\u00b7gus\u00b7ta", ",", "mei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "NE", "$,", "PPOSAT"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die St\u00e4dte Deutschlands b\u00fccken sich!", "tokens": ["Die", "St\u00e4d\u00b7te", "Deutschlands", "b\u00fc\u00b7cken", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "PRF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es h\u00f6ren meinen Stolz Belt, Donau,", "tokens": ["Es", "h\u00f6\u00b7ren", "mei\u00b7nen", "Stolz", "Belt", ",", "Do\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVPP", "$,", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und weichen hinter mich!", "tokens": ["Und", "wei\u00b7chen", "hin\u00b7ter", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was f\u00fcrchten wir, ist gleich die Zahl", "tokens": ["Was", "f\u00fcrch\u00b7ten", "wir", ",", "ist", "gleich", "die", "Zahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie dieser beiden Ufer Sand?", "tokens": ["Wie", "die\u00b7ser", "bei\u00b7den", "U\u00b7fer", "Sand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Tochter! hast du nicht zur Seite meines", "tokens": ["O", "Toch\u00b7ter", "!", "hast", "du", "nicht", "zur", "Sei\u00b7te", "mei\u00b7nes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "VAFIN", "PPER", "PTKNEG", "APPRART", "NN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stets einen Gott erkannt?", "tokens": ["Stets", "ei\u00b7nen", "Gott", "er\u00b7kannt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Stritt Jupiter nicht selbst mit Friedrichs", "tokens": ["Stritt", "Ju\u00b7pi\u00b7ter", "nicht", "selbst", "mit", "Fried\u00b7richs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "PTKNEG", "ADV", "APPR", "NE"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und donnerte den Feind zur\u00fcck?", "tokens": ["Und", "don\u00b7ner\u00b7te", "den", "Feind", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warf nicht der Kriegesgott einst pl\u00f6tzlich", "tokens": ["Warf", "nicht", "der", "Krie\u00b7ges\u00b7gott", "einst", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor seines M\u00f6rders Blick?", "tokens": ["Vor", "sei\u00b7nes", "M\u00f6r\u00b7ders", "Blick", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sah ich nicht j\u00fcngst, (als er vom fernen", "tokens": ["Sah", "ich", "nicht", "j\u00fcngst", ",", "(", "als", "er", "vom", "fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$,", "$(", "KOUS", "PPER", "APPRART", "ADJA"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Den Riesen aus der Mitternacht", "tokens": ["Den", "Rie\u00b7sen", "aus", "der", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Heer entgegen riss, ein kleines Heer", "tokens": ["Sein", "Heer", "ent\u00b7ge\u00b7gen", "riss", ",", "ein", "klei\u00b7nes", "Heer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKVZ", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bereit zur zehnten Schlacht,)", "tokens": ["Be\u00b7reit", "zur", "zehn\u00b7ten", "Schlacht", ",", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wie das Panier, von seiner Hand", "tokens": ["Wie", "das", "Pa\u00b7nier", ",", "von", "sei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur drohenden Aegide ward?", "tokens": ["Zur", "dro\u00b7hen\u00b7den", "A\u00b7e\u00b7gi\u00b7de", "ward", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Feinde sahn den Schild der Pallas,", "tokens": ["Die", "Fein\u00b7de", "sahn", "den", "Schild", "der", "Pal\u00b7las", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und hafteten erstarrt", "tokens": ["Und", "haf\u00b7te\u00b7ten", "er\u00b7starrt"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Am Boden; \u2014 bis sie durch sein Heer", "tokens": ["Am", "Bo\u00b7den", ";", "bis", "sie", "durch", "sein", "Heer"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "$(", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das unaufhaltsam weiter drang,", "tokens": ["Das", "un\u00b7auf\u00b7halt\u00b7sam", "wei\u00b7ter", "drang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Halmen von des Himmels Schlossen", "tokens": ["Wie", "Hal\u00b7men", "von", "des", "Him\u00b7mels", "Schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dreyhundert Hufen lang.", "tokens": ["Drey\u00b7hun\u00b7dert", "Hu\u00b7fen", "lang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ja, dinget nur die halbe Welt zu-", "tokens": ["Ja", ",", "din\u00b7get", "nur", "die", "hal\u00b7be", "Welt", "zu"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "ADV", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und raset wider Einen Mann,", "tokens": ["Und", "ra\u00b7set", "wi\u00b7der", "Ei\u00b7nen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wendet wider ihn Verrath, Nacht,", "tokens": ["Und", "wen\u00b7det", "wi\u00b7der", "ihn", "Ver\u00b7rath", ",", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "NN", "$,", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Den ganzen Orkus an:", "tokens": ["Den", "gan\u00b7zen", "Or\u00b7kus", "an", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Borussiens gerechter Held soll sie-", "tokens": ["Bo\u00b7rus\u00b7siens", "ge\u00b7rech\u00b7ter", "Held", "soll", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "NN", "VMFIN", "TRUNC"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Die G\u00f6tter sch\u00fctzen ihren Sohn.", "tokens": ["Die", "G\u00f6t\u00b7ter", "sch\u00fct\u00b7zen", "ih\u00b7ren", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald wird er im Triumph zu seinen Kin-", "tokens": ["Bald", "wird", "er", "im", "Tri\u00b7umph", "zu", "sei\u00b7nen", "Kin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er k\u00f6mmt, ich seh ihn schon!", "tokens": ["Er", "k\u00f6mmt", ",", "ich", "seh", "ihn", "schon", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Er k\u00f6mmt, das Haupt mit Stralen", "tokens": ["Er", "k\u00f6mmt", ",", "das", "Haupt", "mit", "Stra\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie Delius Apollo kam,", "tokens": ["Wie", "De\u00b7lius", "A\u00b7pol\u00b7lo", "kam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NE", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als er den Python schlug und ihm mit", "tokens": ["Als", "er", "den", "Py\u00b7thon", "schlug", "und", "ihm", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "KON", "PPER", "APPR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die schwarze Seele nahm.", "tokens": ["Die", "schwar\u00b7ze", "See\u00b7le", "nahm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Eilt, ihn in Erz den Enkeln auf-", "tokens": ["Eilt", ",", "ihn", "in", "Erz", "den", "En\u00b7keln", "auf"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "APPR", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eilt, einen Tempel ihm zu weihn", "tokens": ["Eilt", ",", "ei\u00b7nen", "Tem\u00b7pel", "ihm", "zu", "weihn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Rande meines Stroms! ich brenne,", "tokens": ["Am", "Ran\u00b7de", "mei\u00b7nes", "Stroms", "!", "ich", "bren\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Bluhmen zu bestreun.", "tokens": ["Mit", "Bluh\u00b7men", "zu", "be\u00b7streun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}