{"dta.poem.9610": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Adonis grab ist hier; mehr sagt die liebe nicht/", "tokens": ["A\u00b7do\u00b7nis", "grab", "ist", "hier", ";", "mehr", "sagt", "die", "lie\u00b7be", "nicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "ADV", "$.", "ADV", "VVFIN", "ART", "VVFIN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Venus seel entschl\u00e4ft bey diesem leichen-steine.", "tokens": ["Und", "Ve\u00b7nus", "seel", "ent\u00b7schl\u00e4ft", "bey", "die\u00b7sem", "lei\u00b7chen\u00b7stei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVFIN", "APPR", "PDAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach hochgeliebter leib! ach werthste todten-beine!", "tokens": ["Ach", "hoch\u00b7ge\u00b7lieb\u00b7ter", "leib", "!", "ach", "werths\u00b7te", "tod\u00b7ten\u00b7bei\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$.", "XY", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach himmlischer Adon! mein mattes hertze bricht", "tokens": ["Ach", "himm\u00b7li\u00b7scher", "A\u00b7don", "!", "mein", "mat\u00b7tes", "hert\u00b7ze", "bricht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "ADJA", "NN", "$.", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In lieb und thr\u00e4nen aus: die thr\u00e4nen sollen zeugen/", "tokens": ["In", "lieb", "und", "thr\u00e4\u00b7nen", "aus", ":", "die", "thr\u00e4\u00b7nen", "sol\u00b7len", "zeu\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "NN", "PTKVZ", "$.", "ART", "NN", "VMFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df meine liebe wird zu keinen zeiten schweigen.", "tokens": ["Da\u00df", "mei\u00b7ne", "lie\u00b7be", "wird", "zu", "kei\u00b7nen", "zei\u00b7ten", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "VAFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wo ist Adonis sarg? wo ist Adonis grab?", "tokens": ["Wo", "ist", "A\u00b7do\u00b7nis", "sarg", "?", "wo", "ist", "A\u00b7do\u00b7nis", "grab", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NE", "VVFIN", "$.", "PWAV", "VAFIN", "NE", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df Venus nicht zugleich sich auf die baare leget/", "tokens": ["Da\u00df", "Ve\u00b7nus", "nicht", "zu\u00b7gleich", "sich", "auf", "die", "baa\u00b7re", "le\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "ADV", "PRF", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie wenn ein rauher wind die blumen niederschl\u00e4get/", "tokens": ["Wie", "wenn", "ein", "rau\u00b7her", "wind", "die", "blu\u00b7men", "nie\u00b7der\u00b7schl\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "ADJA", "VAFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Schl\u00e4gt tulp und nelck entzwey/ und bricht die blumen ab.", "tokens": ["Schl\u00e4gt", "tulp", "und", "nelck", "ent\u00b7zwey", "/", "und", "bricht", "die", "blu\u00b7men", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "NN", "PTKVZ", "$(", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So war mein lebens-geist von hertz und seel entri\u00dfen/", "tokens": ["So", "war", "mein", "le\u00b7bens\u00b7geist", "von", "hertz", "und", "seel", "ent\u00b7ri\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als meinen lieben schatz ein wildes schwein gebi\u00dfen.", "tokens": ["Als", "mei\u00b7nen", "lie\u00b7ben", "schatz", "ein", "wil\u00b7des", "schwein", "ge\u00b7bi\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ach ewiger verlust! unwiederruflich fall!", "tokens": ["Ach", "e\u00b7wi\u00b7ger", "ver\u00b7lust", "!", "un\u00b7wie\u00b7der\u00b7ruf\u00b7lich", "fall", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$.", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich habe deine schoos dem himmel vorgezogen/", "tokens": ["Ich", "ha\u00b7be", "dei\u00b7ne", "schoos", "dem", "him\u00b7mel", "vor\u00b7ge\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Holdseeliger Adon! nun seel und geist verflogen/", "tokens": ["Hold\u00b7see\u00b7li\u00b7ger", "A\u00b7don", "!", "nun", "seel", "und", "geist", "ver\u00b7flo\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So stirbt die Venus auch. Ich h\u00f6rte fast den schall", "tokens": ["So", "stirbt", "die", "Ve\u00b7nus", "auch", ".", "Ich", "h\u00f6r\u00b7te", "fast", "den", "schall"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$.", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und wie du mich zuletzt/ mein tausend lieb/ gesegnet/", "tokens": ["Und", "wie", "du", "mich", "zu\u00b7letzt", "/", "mein", "tau\u00b7send", "lieb", "/", "ge\u00b7seg\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "ADV", "$(", "PPOSAT", "CARD", "ADJD", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Als dir di\u00df ungeheur im finstern wald begegnet.", "tokens": ["Als", "dir", "di\u00df", "un\u00b7ge\u00b7heur", "im", "fins\u00b7tern", "wald", "be\u00b7geg\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADJD", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich ging und suchte drauf mein leben in dem h\u00e4yn/", "tokens": ["Ich", "ging", "und", "such\u00b7te", "drauf", "mein", "le\u00b7ben", "in", "dem", "h\u00e4yn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PAV", "PPOSAT", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und fand da meinen tod/ Adonis sternen glieder", "tokens": ["Und", "fand", "da", "mei\u00b7nen", "tod", "/", "A\u00b7do\u00b7nis", "ster\u00b7nen", "glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "$(", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sind durch des wildes bi\u00df bespr\u00fctzet hin und wieder", "tokens": ["Sind", "durch", "des", "wil\u00b7des", "bi\u00df", "be\u00b7spr\u00fct\u00b7zet", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "KON", "VVFIN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Vom schaum des rothen bluts. Ich bracht ihm himmel-wein", "tokens": ["Vom", "schaum", "des", "ro\u00b7then", "bluts", ".", "Ich", "bracht", "ihm", "him\u00b7mel\u00b7wein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und edlen perlen-tranck/ hertzst\u00e4rckende muscaten/", "tokens": ["Und", "ed\u00b7len", "per\u00b7len\u00b7tranck", "/", "hertz\u00b7st\u00e4r\u00b7cken\u00b7de", "mus\u00b7ca\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "In hoffnung meinem sohn und besten schatz zu rathen/", "tokens": ["In", "hoff\u00b7nung", "mei\u00b7nem", "sohn", "und", "bes\u00b7ten", "schatz", "zu", "ra\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "KON", "VVFIN", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Vergebens ob ich schon den weichen mund gek\u00fcst/", "tokens": ["Ver\u00b7ge\u00b7bens", "ob", "ich", "schon", "den", "wei\u00b7chen", "mund", "ge\u00b7k\u00fcst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und tausend mahl geschryn: erwache meine seele!", "tokens": ["Und", "tau\u00b7send", "mahl", "ge\u00b7schryn", ":", "er\u00b7wa\u00b7che", "mei\u00b7ne", "see\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "ADV", "VVPP", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So regte sich kein glied/ ja was ich nicht verheele/", "tokens": ["So", "reg\u00b7te", "sich", "kein", "glied", "/", "ja", "was", "ich", "nicht", "ver\u00b7hee\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PIAT", "NN", "$(", "ADV", "PWS", "PPER", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ich habe selbst zuletzt krafft/ seel und geist vermist.", "tokens": ["Ich", "ha\u00b7be", "selbst", "zu\u00b7letzt", "krafft", "/", "seel", "und", "geist", "ver\u00b7mist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "NN", "$(", "XY", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich werd auch nimmer sch\u00f6n/ mein\u2019 anmuth ist gestorben/", "tokens": ["Ich", "werd", "auch", "nim\u00b7mer", "sch\u00f6n", "/", "mein'", "an\u00b7muth", "ist", "ge\u00b7stor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und mit Adonis pracht der Venus glantz verdorben.", "tokens": ["Und", "mit", "A\u00b7do\u00b7nis", "pracht", "der", "Ve\u00b7nus", "glantz", "ver\u00b7dor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "VVFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Bedenck ich jene lust und gegenw\u00e4rtig leid/", "tokens": ["Be\u00b7denck", "ich", "je\u00b7ne", "lust", "und", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "leid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDS", "VVFIN", "KON", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ja wenn der himmel gleich in lauter rosen lebte/", "tokens": ["Ja", "wenn", "der", "him\u00b7mel", "gleich", "in", "lau\u00b7ter", "ro\u00b7sen", "leb\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "ART", "NN", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wenn h\u00f6chst\u2019 erg\u00f6tzlichkeit um meine scheitel schwebte/", "tokens": ["Wenn", "h\u00f6chst'", "er\u00b7g\u00f6tz\u00b7lich\u00b7keit", "um", "mei\u00b7ne", "schei\u00b7tel", "schweb\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So blieb ich unbewegt/ bi\u00df da\u00df die s\u00fcsse zeit", "tokens": ["So", "blieb", "ich", "un\u00b7be\u00b7wegt", "/", "bi\u00df", "da\u00df", "die", "s\u00fcs\u00b7se", "zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$(", "APPR", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Mich gab Adonis gunst/ den ich verschwendrisch k\u00fcste/", "tokens": ["Mich", "gab", "A\u00b7do\u00b7nis", "gunst", "/", "den", "ich", "ver\u00b7schwend\u00b7risch", "k\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NE", "$(", "PRELS", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Sein alabaster arm umschr\u00e4nckte meine br\u00fcste;", "tokens": ["Sein", "a\u00b7la\u00b7bas\u00b7ter", "arm", "um\u00b7schr\u00e4nck\u00b7te", "mei\u00b7ne", "br\u00fcs\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJD", "VVFIN", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So hat niemand geliebt/ und niemand wei\u00df es so/", "tokens": ["So", "hat", "nie\u00b7mand", "ge\u00b7liebt", "/", "und", "nie\u00b7mand", "wei\u00df", "es", "so", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVPP", "$(", "KON", "PIS", "VVFIN", "PPER", "ADV", "$("], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.38": {"text": "Die seelen nur allein beschlo\u00dfen was geschehen/", "tokens": ["Die", "see\u00b7len", "nur", "al\u00b7lein", "be\u00b7schlo\u00b7\u00dfen", "was", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "VVFIN", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der monde hat uns oft gantz holdreich zugesehen/", "tokens": ["Der", "mon\u00b7de", "hat", "uns", "oft", "gantz", "hold\u00b7reich", "zu\u00b7ge\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Er ward an meiner brust/ und ich an seiner froh;", "tokens": ["Er", "ward", "an", "mei\u00b7ner", "brust", "/", "und", "ich", "an", "sei\u00b7ner", "froh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$(", "KON", "PPER", "APPR", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Sein mund hie\u00df mein rubin/ ich schenckt ihm himmels-fl\u00fcsse", "tokens": ["Sein", "mund", "hie\u00df", "mein", "ru\u00b7bin", "/", "ich", "schenckt", "ihm", "him\u00b7mels\u00b7fl\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NE", "$(", "PPER", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und selbte macht ich noch mit liebes-zucker s\u00fcsse.", "tokens": ["Und", "selb\u00b7te", "macht", "ich", "noch", "mit", "lie\u00b7bes\u00b7zu\u00b7cker", "s\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Nun seh ich nichts als noth/ und dein verblichner leib/", "tokens": ["Nun", "seh", "ich", "nichts", "als", "noth", "/", "und", "dein", "ver\u00b7blich\u00b7ner", "leib", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "KOKOM", "NN", "$(", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Mein eintzig liebes kind/ entseelt mein kranckes hertze:", "tokens": ["Mein", "eint\u00b7zig", "lie\u00b7bes", "kind", "/", "ent\u00b7seelt", "mein", "kran\u00b7ckes", "hert\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$(", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Doch da\u00df ein denckmal sey/ wie hoch ich dich beschmertze/", "tokens": ["Doch", "da\u00df", "ein", "denck\u00b7mal", "sey", "/", "wie", "hoch", "ich", "dich", "be\u00b7schmert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADV", "VAFIN", "$(", "PWAV", "ADJD", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "So bau ich hier dein grab/ das keine zeit zerreib\u2019/", "tokens": ["So", "bau", "ich", "hier", "dein", "grab", "/", "das", "kei\u00b7ne", "zeit", "zer\u00b7reib'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$(", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Und in vergessenheit die lange n\u00e4chte st\u00fcrtze/", "tokens": ["Und", "in", "ver\u00b7ges\u00b7sen\u00b7heit", "die", "lan\u00b7ge", "n\u00e4ch\u00b7te", "st\u00fcrt\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "ADJA", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Mit thr\u00e4nen salb ich dich statt weit-geholter w\u00fcrtze.", "tokens": ["Mit", "thr\u00e4\u00b7nen", "salb", "ich", "dich", "statt", "weit\u00b7ge\u00b7hol\u00b7ter", "w\u00fcrt\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Hier ist Adonis grab und auch mein heiligthum.", "tokens": ["Hier", "ist", "A\u00b7do\u00b7nis", "grab", "und", "auch", "mein", "hei\u00b7lig\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "NN", "KON", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Ein mensch mag bahr und grust mit g\u00f6ldnen ampeln zieren/", "tokens": ["Ein", "mensch", "mag", "bahr", "und", "grust", "mit", "g\u00f6ld\u00b7nen", "am\u00b7peln", "zie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "KON", "VVFIN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Ich g\u00f6ttin will um dich die stern als fackeln f\u00fchren.", "tokens": ["Ich", "g\u00f6t\u00b7tin", "will", "um", "dich", "die", "stern", "als", "fa\u00b7ckeln", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "VMFIN", "APPR", "PRF", "ART", "ADJA", "KOKOM", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Und wie die leichen sonst schm\u00fcckt eine sch\u00f6ne blum/", "tokens": ["Und", "wie", "die", "lei\u00b7chen", "sonst", "schm\u00fcckt", "ei\u00b7ne", "sch\u00f6\u00b7ne", "blum", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "ADV", "VVFIN", "ART", "ADJA", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "So soll das sch\u00f6ne blut in au\u00e4monen sincken/", "tokens": ["So", "soll", "das", "sch\u00f6\u00b7ne", "blut", "in", "au\u00b7\u00e4\u00b7mo\u00b7nen", "sin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Und bey dem rosen-lentz in purpur-kleidern blincken.", "tokens": ["Und", "bey", "dem", "ro\u00b7sen\u00b7\u00b7lentz", "in", "pur\u00b7pur\u00b7klei\u00b7dern", "blin\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Was mehr? den leichgesang/ das bittre todten-lied", "tokens": ["Was", "mehr", "?", "den", "leich\u00b7ge\u00b7sang", "/", "das", "bitt\u00b7re", "tod\u00b7ten\u00b7lied"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADV", "$.", "ART", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Stimmt Venus ewig an/ der himmel hilfft mir klagen/", "tokens": ["Stimmt", "Ve\u00b7nus", "e\u00b7wig", "an", "/", "der", "him\u00b7mel", "hilfft", "mir", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADJD", "PTKVZ", "$(", "ART", "NN", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Die l\u00fcfte seuftzen mit/ der westenwind soll sagen/", "tokens": ["Die", "l\u00fcf\u00b7te", "seuft\u00b7zen", "mit", "/", "der", "wes\u00b7ten\u00b7wind", "soll", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "$(", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Wie tief ich traurig sey: Ich bin nicht gro\u00df bem\u00fcht/", "tokens": ["Wie", "tief", "ich", "trau\u00b7rig", "sey", ":", "Ich", "bin", "nicht", "gro\u00df", "be\u00b7m\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADJD", "VAFIN", "$.", "PPER", "VAFIN", "PTKNEG", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Um das beliebte grab viel seulen aufzuf\u00fchren/", "tokens": ["Um", "das", "be\u00b7lieb\u00b7te", "grab", "viel", "seu\u00b7len", "auf\u00b7zu\u00b7f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "ADV", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Die liebe soll es mehr mit ihren wundern zieren.", "tokens": ["Die", "lie\u00b7be", "soll", "es", "mehr", "mit", "ih\u00b7ren", "wun\u00b7dern", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Da\u00df Artemisja dort des ehmanns asche tranck/", "tokens": ["Da\u00df", "Ar\u00b7te\u00b7mis\u00b7ja", "dort", "des", "eh\u00b7manns", "asc\u00b7he", "tranck", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Ist viel und liebens werth; Ich opffre meine seele/", "tokens": ["Ist", "viel", "und", "lie\u00b7bens", "werth", ";", "Ich", "opff\u00b7re", "mei\u00b7ne", "see\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KON", "ADV", "ADJD", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Die zwar nicht sichtbar ist/ der lieben grabes-h\u00f6le;", "tokens": ["Die", "zwar", "nicht", "sicht\u00b7bar", "ist", "/", "der", "lie\u00b7ben", "gra\u00b7bes\u00b7h\u00f6le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "ADJD", "VAFIN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Und saget nun iemand/ da\u00df Venus bleich und kranck/", "tokens": ["Und", "sa\u00b7get", "nun", "ie\u00b7mand", "/", "da\u00df", "Ve\u00b7nus", "bleich", "und", "kranck", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIS", "$(", "KOUS", "NN", "ADJD", "KON", "ADJD", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.65": {"text": "Der wisse/ da Adon mein trost und lieb erblichen/", "tokens": ["Der", "wis\u00b7se", "/", "da", "A\u00b7don", "mein", "trost", "und", "lieb", "er\u00b7bli\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "NE", "PPOSAT", "NN", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Da\u00df ich zugleich mit ihm bin aus der welt gewichen.", "tokens": ["Da\u00df", "ich", "zu\u00b7gleich", "mit", "ihm", "bin", "aus", "der", "welt", "ge\u00b7wi\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Die \u00fcberschrifft wird sonst dem marmol einverleibt/", "tokens": ["Die", "\u00fc\u00b7bersc\u00b7hrifft", "wird", "sonst", "dem", "mar\u00b7mol", "ein\u00b7ver\u00b7leibt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Ich will sie ins gem\u00fcth der sp\u00e4ten nachwelt graben/", "tokens": ["Ich", "will", "sie", "ins", "ge\u00b7m\u00fcth", "der", "sp\u00e4\u00b7ten", "nach\u00b7welt", "gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Dran soll der buler volck den sch\u00f6nsten spiegel haben/", "tokens": ["Dran", "soll", "der", "bu\u00b7ler", "volck", "den", "sch\u00f6ns\u00b7ten", "spie\u00b7gel", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Wo nicht der gro\u00dfe schmertz die lieb ins elend treibt:", "tokens": ["Wo", "nicht", "der", "gro\u00b7\u00dfe", "schmertz", "die", "lieb", "ins", "e\u00b7lend", "treibt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "ADJA", "NN", "ART", "ADJD", "APPRART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Hier ruht der sch\u00f6nheit schatz und Venus holde zierden/", "tokens": ["Hier", "ruht", "der", "sch\u00f6n\u00b7heit", "schatz", "und", "Ve\u00b7nus", "hol\u00b7de", "zier\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KON", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Tritt nicht zu nah hinzu! der stein macht die begierden.", "tokens": ["Tritt", "nicht", "zu", "nah", "hin\u00b7zu", "!", "der", "stein", "macht", "die", "be\u00b7gier\u00b7den", "."], "token_info":