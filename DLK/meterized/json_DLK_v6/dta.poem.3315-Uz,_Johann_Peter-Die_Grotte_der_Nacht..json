{"dta.poem.3315": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Grotte der Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Wohin wird mein Gesang verschlagen?", "tokens": ["Wo\u00b7hin", "wird", "mein", "Ge\u00b7sang", "ver\u00b7schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Ocean ist voller Glut:", "tokens": ["Der", "O\u00b7cean", "ist", "vol\u00b7ler", "Glut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Denn Titan kommt; sein strahlenreicher Wagen", "tokens": ["Denn", "Ti\u00b7tan", "kommt", ";", "sein", "strah\u00b7len\u00b7rei\u00b7cher", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schwebt feurig \u00fcber blauer Fluth:", "tokens": ["Schwebt", "feu\u00b7rig", "\u00fc\u00b7ber", "blau\u00b7er", "Fluth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Indessen auf bethauten Schwingen", "tokens": ["In\u00b7des\u00b7sen", "auf", "be\u00b7thau\u00b7ten", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die braune Nacht entlassen flieht,", "tokens": ["Die", "brau\u00b7ne", "Nacht", "ent\u00b7las\u00b7sen", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Nymphen sie zu ihrer Grotte bringen,", "tokens": ["Und", "Nym\u00b7phen", "sie", "zu", "ih\u00b7rer", "Grot\u00b7te", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die kein unheilig Auge sieht.", "tokens": ["Die", "kein", "un\u00b7hei\u00b7lig", "Au\u00b7ge", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wird meinem Blick im tiefsten Meere", "tokens": ["Wird", "mei\u00b7nem", "Blick", "im", "tiefs\u00b7ten", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dort ihre Herrschaft aufgethan?", "tokens": ["Dort", "ih\u00b7re", "Herr\u00b7schaft", "auf\u00b7ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es trennen sich erschrockner Schatten Heere;", "tokens": ["Es", "tren\u00b7nen", "sich", "er\u00b7schrock\u00b7ner", "Schat\u00b7ten", "Hee\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie machen mir entfliehend Bahn.", "tokens": ["Sie", "ma\u00b7chen", "mir", "ent\u00b7flie\u00b7hend", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O Ruh! o welch ein heilig Schweigen", "tokens": ["O", "Ruh", "!", "o", "welch", "ein", "hei\u00b7lig", "Schwei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "FM", "PWAT", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beherrscht ihr schattigtes Revier!", "tokens": ["Be\u00b7herrscht", "ihr", "schat\u00b7tig\u00b7tes", "Re\u00b7vier", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Vogel schwatzt auf d\u00fcstrer Ulmen Zweigen;", "tokens": ["Kein", "Vo\u00b7gel", "schwatzt", "auf", "d\u00fcst\u00b7rer", "Ul\u00b7men", "Zwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der muntre West entschlummert hier.", "tokens": ["Der", "mun\u00b7tre", "West", "ent\u00b7schlum\u00b7mert", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein zitternd Schimmern bleicher Kerzen", "tokens": ["Ein", "zit\u00b7ternd", "Schim\u00b7mern", "blei\u00b7cher", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erleuchtet ihren dunkeln Sitz,", "tokens": ["Er\u00b7leuch\u00b7tet", "ih\u00b7ren", "dun\u00b7keln", "Sitz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo rings umher die leichten Tr\u00e4ume scherzen,", "tokens": ["Wo", "rings", "um\u00b7her", "die", "leich\u00b7ten", "Tr\u00e4u\u00b7me", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PTKVZ", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gefl\u00fcgelt, wie der schnelle Blitz.", "tokens": ["Ge\u00b7fl\u00fc\u00b7gelt", ",", "wie", "der", "schnel\u00b7le", "Blitz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Von welchem angenehmen Kinde", "tokens": ["Von", "wel\u00b7chem", "an\u00b7ge\u00b7neh\u00b7men", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kommt hier der sch\u00f6ne Morgentraum?", "tokens": ["Kommt", "hier", "der", "sch\u00f6\u00b7ne", "Mor\u00b7gen\u00b7traum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seht! Phantasus h\u00fcllt sich in rauhe Rinde", "tokens": ["Seht", "!", "Phan\u00b7ta\u00b7sus", "h\u00fcllt", "sich", "in", "rau\u00b7he", "Rin\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "NE", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und gr\u00fcnt, bebl\u00e4ttert, als ein Baum.", "tokens": ["Und", "gr\u00fcnt", ",", "be\u00b7bl\u00e4t\u00b7tert", ",", "als", "ein", "Baum", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nun, da in junger Nymphen H\u00e4nden", "tokens": ["Nun", ",", "da", "in", "jun\u00b7ger", "Nym\u00b7phen", "H\u00e4n\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ged\u00e4mpfter Saiten Scherz erklingt:", "tokens": ["Ge\u00b7d\u00e4mpf\u00b7ter", "Sai\u00b7ten", "Scherz", "er\u00b7klingt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ert\u00f6nt ein Lied von muschelreichen W\u00e4nden,", "tokens": ["Er\u00b7t\u00f6nt", "ein", "Lied", "von", "mu\u00b7schel\u00b7rei\u00b7chen", "W\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das eine der Najaden singt.", "tokens": ["Das", "ei\u00b7ne", "der", "Na\u00b7ja\u00b7den", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Geneu\u00df die Ruhe, die du zeugest,", "tokens": ["Ge\u00b7neu\u00df", "die", "Ru\u00b7he", ",", "die", "du", "zeu\u00b7gest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O G\u00f6ttinn! singt sie; holde Nacht!", "tokens": ["O", "G\u00f6t\u00b7tinn", "!", "singt", "sie", ";", "hol\u00b7de", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "PPER", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der L\u00e4rm entschl\u00e4ft, wenn du zum Himmel steigest;", "tokens": ["Der", "L\u00e4rm", "ent\u00b7schl\u00e4ft", ",", "wenn", "du", "zum", "Him\u00b7mel", "stei\u00b7gest", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nur der Progne Schwester wacht.", "tokens": ["Und", "nur", "der", "Prog\u00b7ne", "Schwes\u00b7ter", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wie leise gehn in feuchten B\u00fcschen", "tokens": ["Wie", "lei\u00b7se", "gehn", "in", "feuch\u00b7ten", "B\u00fc\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Winde durch den finstern Hayn!", "tokens": ["Die", "Win\u00b7de", "durch", "den", "fins\u00b7tern", "Hayn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Ruhe will, was Odem sch\u00f6pft, erfrischen:", "tokens": ["Die", "Ru\u00b7he", "will", ",", "was", "O\u00b7dem", "sch\u00f6pft", ",", "er\u00b7fri\u00b7schen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "PRELS", "NE", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch k\u00f6nnen Menschen ruhig seyn?", "tokens": ["Doch", "k\u00f6n\u00b7nen", "Men\u00b7schen", "ru\u00b7hig", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Umsonst sind ihre m\u00fcden Glieder", "tokens": ["Um\u00b7sonst", "sind", "ih\u00b7re", "m\u00fc\u00b7den", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Sidons Purpur hingestreckt,", "tokens": ["Auf", "Si\u00b7dons", "Pur\u00b7pur", "hin\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn Mitternacht mit schweigendem Gefieder", "tokens": ["Wenn", "Mit\u00b7ter\u00b7nacht", "mit", "schwei\u00b7gen\u00b7dem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Marmor der Pal\u00e4ste deckt:", "tokens": ["Den", "Mar\u00b7mor", "der", "Pa\u00b7l\u00e4s\u00b7te", "deckt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Umsonst sind schwanenweiche Betten,", "tokens": ["Um\u00b7sonst", "sind", "schwa\u00b7nen\u00b7wei\u00b7che", "Bet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bey st\u00fcrmischer Begierden Wuth:", "tokens": ["Bey", "st\u00fcr\u00b7mi\u00b7scher", "Be\u00b7gier\u00b7den", "Wuth", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der kranke Geist schleppt seine Sklaven-Ketten,", "tokens": ["Der", "kran\u00b7ke", "Geist", "schleppt", "sei\u00b7ne", "Skla\u00b7ven\u00b7Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stets ohne Ruh, wann alles ruht.", "tokens": ["Stets", "oh\u00b7ne", "Ruh", ",", "wann", "al\u00b7les", "ruht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Der Mensch entflieht bebl\u00fchmten Pfaden,", "tokens": ["Der", "Mensch", "ent\u00b7flieht", "be\u00b7bl\u00fchm\u00b7ten", "Pfa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ihm die stille Freude winkt.", "tokens": ["Wo", "ihm", "die", "stil\u00b7le", "Freu\u00b7de", "winkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Gute selbst misbraucht er sich zum Schaden:", "tokens": ["Das", "Gu\u00b7te", "selbst", "mis\u00b7braucht", "er", "sich", "zum", "Scha\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu Gift wird Necktar, den er trinkt.", "tokens": ["Zu", "Gift", "wird", "Neck\u00b7tar", ",", "den", "er", "trinkt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Wenn Tantalus im h\u00f6chsten Gl\u00fccke", "tokens": ["Wenn", "Tan\u00b7ta\u00b7lus", "im", "h\u00f6chs\u00b7ten", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Selbst an der G\u00f6tter Tafel sitzt:", "tokens": ["Selbst", "an", "der", "G\u00f6t\u00b7ter", "Ta\u00b7fel", "sitzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denkt nicht sein Herz auf schwarze Bubenst\u00fccke,", "tokens": ["Denkt", "nicht", "sein", "Herz", "auf", "schwar\u00b7ze", "Bu\u00b7ben\u00b7st\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch da ihn Himmelstrank erhitzt?", "tokens": ["Noch", "da", "ihn", "Him\u00b7mel\u00b7strank", "er\u00b7hitzt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Fern von Olymps gestirnter Schwelle", "tokens": ["Fern", "von", "O\u00b7lymps", "ge\u00b7stirn\u00b7ter", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR"