{"textgrid.poem.36611": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Entfernt vom Lande der Romanen,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Entfernt vom Lande der Romanen,", "tokens": ["Ent\u00b7fernt", "vom", "Lan\u00b7de", "der", "Ro\u00b7ma\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Z\u00e4rtlichkeit den Scepter f\u00fchrt,", "tokens": ["Wo", "Z\u00e4rt\u00b7lich\u00b7keit", "den", "Scep\u00b7ter", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sing ich, bey Amors Unterthanen,", "tokens": ["Sing", "ich", ",", "bey", "A\u00b7mors", "Un\u00b7ter\u00b7tha\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die frey sind, weil er sie regiert.", "tokens": ["Die", "frey", "sind", ",", "weil", "er", "sie", "re\u00b7giert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich singe, Spr\u00f6de zu besiegen,", "tokens": ["Ich", "sin\u00b7ge", ",", "Spr\u00f6\u00b7de", "zu", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch keine mir zum Ehgemahl;", "tokens": ["Doch", "kei\u00b7ne", "mir", "zum", "Eh\u00b7ge\u00b7mahl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So macht nur Amor mir Vergn\u00fcgen,", "tokens": ["So", "macht", "nur", "A\u00b7mor", "mir", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NE", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So macht mir Hymen keine Quaal.", "tokens": ["So", "macht", "mir", "Hy\u00b7men", "kei\u00b7ne", "Qua\u00b7al", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich lieb und ehr euch all, ihr Sch\u00f6nen,", "tokens": ["Ich", "lieb", "und", "ehr", "euch", "all", ",", "ihr", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "NN", "PPER", "PIAT", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit weiser Unbest\u00e4ndigkeit;", "tokens": ["Mit", "wei\u00b7ser", "Un\u00b7be\u00b7st\u00e4n\u00b7dig\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum sollt ihr alle mich verh\u00f6hnen,", "tokens": ["Drum", "sollt", "ihr", "al\u00b7le", "mich", "ver\u00b7h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PIS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn einer einst mein Herz sich weiht.", "tokens": ["Wenn", "ei\u00b7ner", "einst", "mein", "Herz", "sich", "weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und die, die mich alsdann besieget,", "tokens": ["Und", "die", ",", "die", "mich", "als\u00b7dann", "be\u00b7sie\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die mich best\u00e4ndig macht, und treu,", "tokens": ["Die", "mich", "be\u00b7st\u00e4n\u00b7dig", "macht", ",", "und", "treu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die mich in Hymens Joch betr\u00fcget,", "tokens": ["Die", "mich", "in", "Hy\u00b7mens", "Joch", "be\u00b7tr\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die kr\u00f6ne mich mit Hirschgeweih.", "tokens": ["Die", "kr\u00f6\u00b7ne", "mich", "mit", "Hirschge\u00b7weih", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}