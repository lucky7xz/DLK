{"textgrid.poem.27831": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man sagt und hat gesagt von grossen Wunder-Wercken,", "tokens": ["Man", "sagt", "und", "hat", "ge\u00b7sagt", "von", "gros\u00b7sen", "Wun\u00b7der\u00b7\u00b7Wer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die wol zu mercken sind und waren wol zu mercken;", "tokens": ["Die", "wol", "zu", "mer\u00b7cken", "sind", "und", "wa\u00b7ren", "wol", "zu", "mer\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKZU", "VVINF", "VAFIN", "KON", "VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Noch ist ein gr\u00f6\u00dfres kaum, als da\u00df ein fromer Mann", "tokens": ["Noch", "ist", "ein", "gr\u00f6\u00df\u00b7res", "kaum", ",", "als", "da\u00df", "ein", "fro\u00b7mer", "Mann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "ADV", "$,", "KOKOM", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bey dieser b\u00f6sen Zeit from seyn und bleiben kan.", "tokens": ["Bey", "die\u00b7ser", "b\u00f6\u00b7sen", "Zeit", "from", "seyn", "und", "blei\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADJD", "VAINF", "KON", "VVINF", 