{"dta.poem.21071": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Alles um Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist es, das die Seele f&#252;llt?               ", "tokens": ["Was", "ist", "es", ",", "das", "die", "See\u00b7le", "f", "&#252;", "llt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "NE", "$(", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ach Liebe f\u00fcllt sie, Liebe!", "tokens": ["Ach", "Lie\u00b7be", "f\u00fcllt", "sie", ",", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie f\u00fcllt nicht Gold noch Goldeswerth,", "tokens": ["Sie", "f\u00fcllt", "nicht", "Gold", "noch", "Gol\u00b7des\u00b7werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht, was die \u00f6de Welt begehrt,", "tokens": ["Nicht", ",", "was", "die", "\u00f6\u00b7de", "Welt", "be\u00b7gehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PRELS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie f\u00fcllt nur Liebe, Liebe!", "tokens": ["Sie", "f\u00fcllt", "nur", "Lie\u00b7be", ",", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was ist es, das die Sehnsucht stillt?", "tokens": ["Was", "ist", "es", ",", "das", "die", "Sehn\u00b7sucht", "stillt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach, Liebe stillt sie, Liebe!", "tokens": ["Ach", ",", "Lie\u00b7be", "stillt", "sie", ",", "Lie\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "NN", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie stillt nicht Titel, Stand noch Rang,", "tokens": ["Sie", "stillt", "nicht", "Ti\u00b7tel", ",", "Stand", "noch", "Rang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "$,", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nicht des Ruhmes Schellenklang;", "tokens": ["Und", "nicht", "des", "Ruh\u00b7mes", "Schel\u00b7len\u00b7klang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie stillt nur Liebe, Liebe!", "tokens": ["Sie", "stillt", "nur", "Lie\u00b7be", ",", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was ists, wonach das Herz zerlechzt?", "tokens": ["Was", "ists", ",", "wo\u00b7nach", "das", "Herz", "zer\u00b7lechzt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PWAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es lechzet, ach, nach Liebe!", "tokens": ["Es", "lech\u00b7zet", ",", "ach", ",", "nach", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ITJ", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es schmachtet nicht nach Druck und Kuss,", "tokens": ["Es", "schmach\u00b7tet", "nicht", "nach", "Druck", "und", "Kuss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht nach der Wollust Vollgenuss;", "tokens": ["Nicht", "nach", "der", "Wol\u00b7lust", "Voll\u00b7ge\u00b7nuss", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es schmachtet nur nach Liebe,", "tokens": ["Es", "schmach\u00b7tet", "nur", "nach", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Gern geb' ich, was ich hab' und bin,", "tokens": ["Gern", "geb'", "ich", ",", "was", "ich", "hab'", "und", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "KON", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gern geb' ichs hin um Liebe.", "tokens": ["Gern", "geb'", "ichs", "hin", "um", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Reichthums bunter Seifenschaum,", "tokens": ["Des", "Reicht\u00b7hums", "bun\u00b7ter", "Sei\u00b7fen\u00b7schaum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wollust Rausch, des Ruhmes Traum,", "tokens": ["Der", "Wol\u00b7lust", "Rausch", ",", "des", "Ruh\u00b7mes", "Traum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was frommt mirs ohne Liebe?", "tokens": ["Was", "frommt", "mirs", "oh\u00b7ne", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NE", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Viel s\u00fcsser ist's gering und arm", "tokens": ["Viel", "s\u00fcs\u00b7ser", "ist's", "ge\u00b7ring", "und", "arm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An treuer Brust verschmachten,", "tokens": ["An", "treu\u00b7er", "Brust", "ver\u00b7schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als ungeliebt und liebelos", "tokens": ["Als", "un\u00b7ge\u00b7liebt", "und", "lie\u00b7be\u00b7los"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Tag verprassen, und im Schooss", "tokens": ["Den", "Tag", "ver\u00b7pras\u00b7sen", ",", "und", "im", "Scho\u00b7oss"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Wollust \u00fcbernachten!", "tokens": ["Der", "Wol\u00b7lust", "\u00fc\u00b7bern\u00b7ach\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O liebe Traute, liebe mich,", "tokens": ["O", "lie\u00b7be", "Trau\u00b7te", ",", "lie\u00b7be", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wahr, wie ich dich liebe!", "tokens": ["So", "wahr", ",", "wie", "ich", "dich", "lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann schwinde Ruhe, Ruhm und Gl\u00fcck!", "tokens": ["Dann", "schwin\u00b7de", "Ru\u00b7he", ",", "Ruhm", "und", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nimm alles, alles hin, Geschick;", "tokens": ["Nimm", "al\u00b7les", ",", "al\u00b7les", "hin", ",", "Ge\u00b7schick", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PIS", "$,", "PIS", "PTKVZ", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir gn\u00fcgt ", "tokens": ["Mir", "gn\u00fcgt"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "++", "measure": "spondeus"}}, "stanza.7": {"line.1": {"text": "Und w\u00e4r mein Loos, getrennt von dir,", "tokens": ["Und", "w\u00e4r", "mein", "Loos", ",", "ge\u00b7trennt", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "$,", "VVPP", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Leben zu vertrauren;", "tokens": ["Mein", "Le\u00b7ben", "zu", "ver\u00b7trau\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00fcsst' ich nur, dass du mich liebst,", "tokens": ["Und", "w\u00fcsst'", "ich", "nur", ",", "dass", "du", "mich", "liebst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00fcsste nur, dass du mir bliebst \u2014", "tokens": ["Und", "w\u00fcss\u00b7te", "nur", ",", "dass", "du", "mir", "bliebst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer d\u00fcrfte mich bedauren?", "tokens": ["Wer", "d\u00fcrf\u00b7te", "mich", "be\u00b7dau\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und w\u00e4r' ich in der Sklaverey,", "tokens": ["Und", "w\u00e4r'", "ich", "in", "der", "Skla\u00b7ve\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In freundeloser Wildniss,", "tokens": ["In", "freun\u00b7de\u00b7lo\u00b7ser", "Wild\u00b7niss", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00e4re dein, nur dein gewiss,", "tokens": ["Und", "w\u00e4\u00b7re", "dein", ",", "nur", "dein", "ge\u00b7wiss", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "$,", "ADV", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So w\u00e4re Sklaverey mir s\u00fcss,", "tokens": ["So", "w\u00e4\u00b7re", "Skla\u00b7ve\u00b7rey", "mir", "s\u00fcss", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Paradies die Wildniss.", "tokens": ["Und", "Pa\u00b7ra\u00b7dies", "die", "Wild\u00b7niss", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und h\u00fcllte Todesfinsterniss", "tokens": ["Und", "h\u00fcll\u00b7te", "To\u00b7des\u00b7fins\u00b7ter\u00b7niss"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich, meines Lebens Sonne,", "tokens": ["Dich", ",", "mei\u00b7nes", "Le\u00b7bens", "Son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und st\u00fcrb' ich nur, von Ihr gemeint,", "tokens": ["Und", "st\u00fcrb'", "ich", "nur", ",", "von", "Ihr", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Ihr beklagt, von Ihr beweint,", "tokens": ["Von", "Ihr", "be\u00b7klagt", ",", "von", "Ihr", "be\u00b7weint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So st\u00fcrb' ich, ach, mit Wonne!", "tokens": ["So", "st\u00fcrb'", "ich", ",", "ach", ",", "mit", "Won\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ITJ", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Viel besser ist's jung, kr\u00e4ftig, k\u00fchn", "tokens": ["Viel", "bes\u00b7ser", "ist's", "jung", ",", "kr\u00e4f\u00b7tig", ",", "k\u00fchn"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "ADJD", "VAFIN", "ADJD", "$,", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Arm der Liebe sterben,", "tokens": ["Im", "Arm", "der", "Lie\u00b7be", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als ungeliebt und liebelos", "tokens": ["Als", "un\u00b7ge\u00b7liebt", "und", "lie\u00b7be\u00b7los"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In dumpfer Freuden mattem Schooss", "tokens": ["In", "dum\u00b7pfer", "Freu\u00b7den", "mat\u00b7tem", "Scho\u00b7oss"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Veralten und verderben!", "tokens": ["Ver\u00b7al\u00b7ten", "und", "ver\u00b7der\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}