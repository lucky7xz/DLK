{"dta.poem.3558": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Und die\u00df und das und das ist mein .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Heute wollen wir Haber m\u00e4hn,               ", "tokens": ["Heu\u00b7te", "wol\u00b7len", "wir", "Ha\u00b7ber", "m\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Morgen wollen wir binden:", "tokens": ["Mor\u00b7gen", "wol\u00b7len", "wir", "bin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Wo ist denn die Liebste mein?", "tokens": ["Wo", "ist", "denn", "die", "Liebs\u00b7te", "mein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "PPOSAT", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wo soll ich sie finden?", "tokens": ["Wo", "soll", "ich", "sie", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Gestern Abend sah ich sie", "tokens": ["Ge\u00b7stern", "A\u00b7bend", "sah", "ich", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "PPER", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Unter einer Linden;", "tokens": ["Un\u00b7ter", "ei\u00b7ner", "Lin\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Ich gedacht in meinem Sinn,", "tokens": ["Ich", "ge\u00b7dacht", "in", "mei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ich will sie schon finden.", "tokens": ["Ich", "will", "sie", "schon", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Was f\u00fchr ich dann an meiner Hand,", "tokens": ["Was", "f\u00fchr", "ich", "dann", "an", "mei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das ganze Hausgesinde \u2014", "tokens": ["Das", "gan\u00b7ze", "Haus\u00b7ge\u00b7sin\u00b7de"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und die\u00df und das, und das ist mein,", "tokens": ["Und", "die\u00df", "und", "das", ",", "und", "das", "ist", "mein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "KON", "PDS", "$,", "KON", "PDS", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Das soll meine Liebste seyn.", "tokens": ["Das", "soll", "mei\u00b7ne", "Liebs\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}