{"textgrid.poem.55914": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Josuas Landtag", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wie der Strom am Ausgang seine D\u00e4mme", "tokens": ["So", "wie", "der", "Strom", "am", "Aus\u00b7gang", "sei\u00b7ne", "D\u00e4m\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "durchbricht mit seiner M\u00fcndung \u00dcberma\u00df,", "tokens": ["durch\u00b7bricht", "mit", "sei\u00b7ner", "M\u00fcn\u00b7dung", "\u00dc\u00b7berm\u00b7a\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "so brach nun durch die \u00c4ltesten der St\u00e4mme", "tokens": ["so", "brach", "nun", "durch", "die", "\u00c4l\u00b7tes\u00b7ten", "der", "St\u00e4m\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "zum letzten Mal die Stimme Josuas.", "tokens": ["zum", "letz\u00b7ten", "Mal", "die", "Stim\u00b7me", "Jo\u00b7su\u00b7as", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie waren die geschlagen, welche lachten,", "tokens": ["Wie", "wa\u00b7ren", "die", "ge\u00b7schla\u00b7gen", ",", "wel\u00b7che", "lach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "VVPP", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wie hielten alle Herz und H\u00e4nde an,", "tokens": ["wie", "hiel\u00b7ten", "al\u00b7le", "Herz", "und", "H\u00e4n\u00b7de", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIAT", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "als h\u00fcbe sich der L\u00e4rm von drei\u00dfig Schlachten", "tokens": ["als", "h\u00fc\u00b7be", "sich", "der", "L\u00e4rm", "von", "drei\u00b7\u00dfig", "Schlach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "PRF", "ART", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "in einem Mund; und dieser Mund begann.", "tokens": ["in", "ei\u00b7nem", "Mund", ";", "und", "die\u00b7ser", "Mund", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "KON", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wieder waren Tausende voll Staunen", "tokens": ["Und", "wie\u00b7der", "wa\u00b7ren", "Tau\u00b7sen\u00b7de", "voll", "Stau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wie an dem gro\u00dfen Tag vor Jericho,", "tokens": ["wie", "an", "dem", "gro\u00b7\u00dfen", "Tag", "vor", "Je\u00b7ri\u00b7cho", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "nun aber waren in ihm die Posaunen,", "tokens": ["nun", "a\u00b7ber", "wa\u00b7ren", "in", "ihm", "die", "Po\u00b7sau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und ihres Lebens Mauern schwankten so,", "tokens": ["und", "ih\u00b7res", "Le\u00b7bens", "Mau\u00b7ern", "schwank\u00b7ten", "so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "da\u00df sie sich w\u00e4lzten von Entsetzen tr\u00e4chtig", "tokens": ["da\u00df", "sie", "sich", "w\u00e4lz\u00b7ten", "von", "Ent\u00b7set\u00b7zen", "tr\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und wehrlos schon und \u00fcberw\u00e4ltigt, eh", "tokens": ["und", "wehr\u00b7los", "schon", "und", "\u00fc\u00b7berw\u00b7\u00e4l\u00b7tigt", ",", "eh"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "ADV", "KON", "VVPP", "$,", "KOUS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "sie's noch gedachten, wie er eigenm\u00e4chtig", "tokens": ["sie's", "noch", "ge\u00b7dach\u00b7ten", ",", "wie", "er", "ei\u00b7gen\u00b7m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "ADV", "VVFIN", "$,", "PWAV", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "zu Gibeon die Sonne anschrie: steh:", "tokens": ["zu", "Gi\u00b7be\u00b7on", "die", "Son\u00b7ne", "an\u00b7schrie", ":", "steh", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "VVFIN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und Gott ging hin, erschrocken wie ein Knecht,", "tokens": ["Und", "Gott", "ging", "hin", ",", "er\u00b7schro\u00b7cken", "wie", "ein", "Knecht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$,", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und hielt die Sonne, bis ihm seine H\u00e4nde", "tokens": ["und", "hielt", "die", "Son\u00b7ne", ",", "bis", "ihm", "sei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wehtaten, ob dem schlachtenden Geschlecht,", "tokens": ["weh\u00b7ta\u00b7ten", ",", "ob", "dem", "schlach\u00b7ten\u00b7den", "Ge\u00b7schlecht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "nur weil da einer wollte, da\u00df sie st\u00e4nde.", "tokens": ["nur", "weil", "da", "ei\u00b7ner", "woll\u00b7te", ",", "da\u00df", "sie", "st\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADV", "PIS", "VMFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und das war dieser; dieser Alte wars,", "tokens": ["Und", "das", "war", "die\u00b7ser", ";", "die\u00b7ser", "Al\u00b7te", "wars", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PDAT", "$.", "PDAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "von dem sie meinten, da\u00df er nicht mehr gelte", "tokens": ["von", "dem", "sie", "mein\u00b7ten", ",", "da\u00df", "er", "nicht", "mehr", "gel\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "inmitten seines hundertzehnten Jahrs.", "tokens": ["in\u00b7mit\u00b7ten", "sei\u00b7nes", "hun\u00b7dert\u00b7zehn\u00b7ten", "Jahrs", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da stand er auf und brach in ihre Zelte.", "tokens": ["Da", "stand", "er", "auf", "und", "brach", "in", "ih\u00b7re", "Zel\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Er ging wie Hagel nieder \u00fcber Halmen:", "tokens": ["Er", "ging", "wie", "Ha\u00b7gel", "nie\u00b7der", "\u00fc\u00b7ber", "Hal\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was wollt ihr Gott versprechen? Ungez\u00e4hlt", "tokens": ["Was", "wollt", "ihr", "Gott", "ver\u00b7spre\u00b7chen", "?", "Un\u00b7ge\u00b7z\u00e4hlt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "VVINF", "$.", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "stehn um euch G\u00f6tter, wartend da\u00df ihr w\u00e4hlt.", "tokens": ["stehn", "um", "euch", "G\u00f6t\u00b7ter", ",", "war\u00b7tend", "da\u00df", "ihr", "w\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "NN", "$,", "VVPP", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch wenn ihr w\u00e4hlt, wird euch der Herr zermalmen.", "tokens": ["Doch", "wenn", "ihr", "w\u00e4hlt", ",", "wird", "euch", "der", "Herr", "zer\u00b7mal\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und dann, mit einem Hochmut ohnegleichen:", "tokens": ["Und", "dann", ",", "mit", "ei\u00b7nem", "Hoch\u00b7mut", "oh\u00b7ne\u00b7glei\u00b7chen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich und mein Haus, wir bleiben ihm verm\u00e4hlt.", "tokens": ["Ich", "und", "mein", "Haus", ",", "wir", "blei\u00b7ben", "ihm", "ver\u00b7m\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Da schrien sie alle: Hilf uns, gieb ein Zeichen", "tokens": ["Da", "schri\u00b7en", "sie", "al\u00b7le", ":", "Hilf", "uns", ",", "gieb", "ein", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$.", "NE", "PPER", "$,", "VVIMP", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "und st\u00e4rke uns zu unserer schweren Wahl.", "tokens": ["und", "st\u00e4r\u00b7ke", "uns", "zu", "un\u00b7se\u00b7rer", "schwe\u00b7ren", "Wahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Aber sie sahn ihn, wie seit Jahren schweigend,", "tokens": ["A\u00b7ber", "sie", "sahn", "ihn", ",", "wie", "seit", "Jah\u00b7ren", "schwei\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PWAV", "APPR", "NN", "ADJD", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "zu seiner festen Stadt am Berge steigend;", "tokens": ["zu", "sei\u00b7ner", "fes\u00b7ten", "Stadt", "am", "Ber\u00b7ge", "stei\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und dann nicht mehr. Es war das letzte Mal.", "tokens": ["und", "dann", "nicht", "mehr", ".", "Es", "war", "das", "letz\u00b7te", "Mal", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}