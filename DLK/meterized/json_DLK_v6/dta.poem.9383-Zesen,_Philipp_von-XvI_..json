{"dta.poem.9383": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "XvI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Meine hand ist schon bem\u00fchet", "tokens": ["Mei\u00b7ne", "hand", "ist", "schon", "be\u00b7m\u00fc\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jhm zu winden einen Krantz/", "tokens": ["Jhm", "zu", "win\u00b7den", "ei\u00b7nen", "Krantz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "weil die sch\u00f6ne Rose bl\u00fchet/", "tokens": ["weil", "die", "sch\u00f6\u00b7ne", "Ro\u00b7se", "bl\u00fc\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "weil noch v\u00f6llig ist der glantz/", "tokens": ["weil", "noch", "v\u00f6l\u00b7lig", "ist", "der", "glantz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "weil die Tulpen und Narcissen", "tokens": ["weil", "die", "Tul\u00b7pen", "und", "Nar\u00b7cis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "noch zu Kr\u00e4ntzen dienen m\u00fcssen.", "tokens": ["noch", "zu", "Kr\u00e4nt\u00b7zen", "die\u00b7nen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}