{"textgrid.poem.59434": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Flucht der Heiligen Familie", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4nger fallen schon die Schatten,", "tokens": ["L\u00e4n\u00b7ger", "fal\u00b7len", "schon", "die", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die k\u00fchle Abendluft,", "tokens": ["Durch", "die", "k\u00fch\u00b7le", "A\u00b7ben\u00b7dluft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Waldw\u00e4rts \u00fcber stille Matten", "tokens": ["Wald\u00b7w\u00e4rts", "\u00fc\u00b7ber", "stil\u00b7le", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schreitet Joseph von der Kluft,", "tokens": ["Schrei\u00b7tet", "Jo\u00b7se\u00b7ph", "von", "der", "Kluft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "F\u00fchrt den Esel treu am Z\u00fcgel;", "tokens": ["F\u00fchrt", "den", "E\u00b7sel", "treu", "am", "Z\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Linde L\u00fcfte f\u00e4cheln kaum,", "tokens": ["Lin\u00b7de", "L\u00fcf\u00b7te", "f\u00e4\u00b7cheln", "kaum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "'s sind der Engel leise Fl\u00fcgel,", "tokens": ["'s", "sind", "der", "En\u00b7gel", "lei\u00b7se", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die das Kindlein sieht im Traum,", "tokens": ["Die", "das", "Kin\u00b7dlein", "sieht", "im", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und Maria schauet nieder", "tokens": ["Und", "Ma\u00b7ria", "schau\u00b7et", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Auf das Kind voll Lust und Leid,", "tokens": ["Auf", "das", "Kind", "voll", "Lust", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Singt im Herzen Wiegenlieder", "tokens": ["Singt", "im", "Her\u00b7zen", "Wie\u00b7gen\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "In der stillen Einsamkeit.", "tokens": ["In", "der", "stil\u00b7len", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Die Johannesw\u00fcrmchen kreisen", "tokens": ["Die", "Jo\u00b7han\u00b7nes\u00b7w\u00fcrm\u00b7chen", "krei\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Emsig leuchtend \u00fcbern Weg,", "tokens": ["Em\u00b7sig", "leuch\u00b7tend", "\u00fc\u00b7bern", "Weg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Wollen der Mutter Gottes weisen", "tokens": ["Wol\u00b7len", "der", "Mut\u00b7ter", "Got\u00b7tes", "wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Durch die Wildnis jeden Steg,", "tokens": ["Durch", "die", "Wild\u00b7nis", "je\u00b7den", "Steg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Und durchs Gras geht s\u00fc\u00dfes Schaudern,", "tokens": ["Und", "durchs", "Gras", "geht", "s\u00fc\u00b7\u00dfes", "Schau\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Streift es ihres Mantels Saum;", "tokens": ["Streift", "es", "ih\u00b7res", "Man\u00b7tels", "Saum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "B\u00e4chlein auch l\u00e4\u00dft jetzt sein Plaudern", "tokens": ["B\u00e4ch\u00b7lein", "auch", "l\u00e4\u00dft", "jetzt", "sein", "Plau\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.20": {"text": "Und die W\u00e4lder fl\u00fcstern kaum,", "tokens": ["Und", "die", "W\u00e4l\u00b7der", "fl\u00fcs\u00b7tern", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Da\u00df sie nicht die Flucht verraten.", "tokens": ["Da\u00df", "sie", "nicht", "die", "Flucht", "ver\u00b7ra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Und das Kindlein hob die Hand,", "tokens": ["Und", "das", "Kin\u00b7dlein", "hob", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Da sie ihm so Liebes taten,", "tokens": ["Da", "sie", "ihm", "so", "Lie\u00b7bes", "ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Segnete das stille Land,", "tokens": ["Seg\u00b7ne\u00b7te", "das", "stil\u00b7le", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Da\u00df die Erd mit Blumen, B\u00e4umen", "tokens": ["Da\u00df", "die", "Erd", "mit", "Blu\u00b7men", ",", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Fernerhin in Ewigkeit", "tokens": ["Fer\u00b7ner\u00b7hin", "in", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "N\u00e4chtlich mu\u00df vom Himmel tr\u00e4umen \u2013", "tokens": ["N\u00e4cht\u00b7lich", "mu\u00df", "vom", "Him\u00b7mel", "tr\u00e4u\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPRART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "O gebenedeite Zeit!", "tokens": ["O", "ge\u00b7be\u00b7ne\u00b7dei\u00b7te", "Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}