{"textgrid.poem.35458": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df \u2013 ich wei\u00df: Nur wie ein Meteor,", "tokens": ["Ich", "wei\u00df", "\u2013", "ich", "wei\u00df", ":", "Nur", "wie", "ein", "Me\u00b7te\u00b7or", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "$.", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der flammend kam, jach sich in Nacht verlor,", "tokens": ["Der", "flam\u00b7mend", "kam", ",", "jach", "sich", "in", "Nacht", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,", "VVFIN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Werd' ich durch unsre Dichtung streifen!", "tokens": ["Werd'", "ich", "durch", "uns\u00b7re", "Dich\u00b7tung", "strei\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Laute rauscht. Es jauchzt wie Sturmgesang, \u2013", "tokens": ["Die", "Lau\u00b7te", "rauscht", ".", "Es", "jauchzt", "wie", "Sturm\u00b7ge\u00b7sang", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "KOKOM", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie S\u00fcdwind kost \u2013 es gellt wie Trommelklang", "tokens": ["Wie", "S\u00fcd\u00b7wind", "kost", "\u2013", "es", "gellt", "wie", "Trom\u00b7mel\u00b7klang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "$(", "PPER", "VVPP", "KOKOM", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mein Lied und wird in alle Herzen greifen ...", "tokens": ["Mein", "Lied", "und", "wird", "in", "al\u00b7le", "Her\u00b7zen", "grei\u00b7fen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VAFIN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann bebt's j\u00e4h aus in schriller Dissonanz ...", "tokens": ["Dann", "bebt's", "j\u00e4h", "aus", "in", "schril\u00b7ler", "Dis\u00b7so\u00b7nanz", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Bl\u00fcten sind verdorrt, verspr\u00fcht der Glanz \u2013", "tokens": ["Die", "Bl\u00fc\u00b7ten", "sind", "ver\u00b7dorrt", ",", "ver\u00b7spr\u00fcht", "der", "Glanz", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es streicht der Abendwind durch die Zypressen ...", "tokens": ["Es", "streicht", "der", "Ab\u00b7end\u00b7wind", "durch", "die", "Zyp\u00b7res\u00b7sen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nur wen'ge weinen ... Sie verstummen bald.", "tokens": ["Nur", "wen'\u00b7ge", "wei\u00b7nen", "...", "Sie", "ver\u00b7stum\u00b7men", "bald", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVINF", "$(", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was ich getr\u00e4umt: ", "tokens": ["Was", "ich", "ge\u00b7tr\u00e4umt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ich aber werde bald vergessen ...", "tokens": ["Ich", "a\u00b7ber", "wer\u00b7de", "bald", "ver\u00b7ges\u00b7sen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}