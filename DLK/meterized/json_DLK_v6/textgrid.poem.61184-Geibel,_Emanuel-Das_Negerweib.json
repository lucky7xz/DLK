{"textgrid.poem.61184": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Das Negerweib", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo am gro\u00dfen Strom die Sicheln durch das hohe Rohrfeld klirren,", "tokens": ["Wo", "am", "gro\u00b7\u00dfen", "Strom", "die", "Si\u00b7cheln", "durch", "das", "ho\u00b7he", "Rohr\u00b7feld", "klir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und im Laub des Zuckerahorns farb'ge Papageien schwirren,", "tokens": ["Und", "im", "Laub", "des", "Zu\u00b7cke\u00b7ra\u00b7horns", "fa\u00b7rb'\u00b7ge", "Pa\u00b7pa\u00b7gei\u00b7en", "schwir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "--+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Sitzt das Negerweib, den Nacken bunt geziert mit Glaskorallen,", "tokens": ["Sitzt", "das", "Ne\u00b7ger\u00b7weib", ",", "den", "Na\u00b7cken", "bunt", "ge\u00b7ziert", "mit", "Glas\u00b7ko\u00b7ral\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "ADJD", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und dem Kn\u00e4blein auf dem Scho\u00dfe l\u00e4\u00dft ein Schlummerlied sie schallen:", "tokens": ["Und", "dem", "Kn\u00e4\u00b7blein", "auf", "dem", "Scho\u00b7\u00dfe", "l\u00e4\u00dft", "ein", "Schlum\u00b7mer\u00b7lied", "sie", "schal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "\u00bbschlaf, o schlaf, mein schwarzer Knabe, du zum Jammer mir geboren,", "tokens": ["\u00bb", "schlaf", ",", "o", "schlaf", ",", "mein", "schwar\u00b7zer", "Kna\u00b7be", ",", "du", "zum", "Jam\u00b7mer", "mir", "ge\u00b7bo\u00b7ren", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "FM", "FM", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPER", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Eh' zu leben du beginnest, ist dein Leben schon verloren,", "tokens": ["Eh'", "zu", "le\u00b7ben", "du", "be\u00b7gin\u00b7nest", ",", "ist", "dein", "Le\u00b7ben", "schon", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVFIN", "PPER", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Schlaf, o schlaf, verh\u00fcllt im Dunkel ruhn dir noch der Zukunft Schrecken,", "tokens": ["Schlaf", ",", "o", "schlaf", ",", "ver\u00b7h\u00fcllt", "im", "Dun\u00b7kel", "ruhn", "dir", "noch", "der", "Zu\u00b7kunft", "Schre\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "FM", "$,", "VVFIN", "APPRART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Nur zu fr\u00fch aus deinen Tr\u00e4umen wird der Grimm des Herrn dich wecken.", "tokens": ["Nur", "zu", "fr\u00fch", "aus", "dei\u00b7nen", "Tr\u00e4u\u00b7men", "wird", "der", "Grimm", "des", "Herrn", "dich", "we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "APPR", "PPOSAT", "NN", "VAFIN", "ART", "NE", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Was die Menschen Freude hei\u00dfen, wirst du nimmermehr empfinden,", "tokens": ["Was", "die", "Men\u00b7schen", "Freu\u00b7de", "hei\u00b7\u00dfen", ",", "wirst", "du", "nim\u00b7mer\u00b7mehr", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VVINF", "$,", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dort nur f\u00fchlt sich's, wo des Nigers Wellen durch die Flur sich winden.", "tokens": ["Dort", "nur", "f\u00fchlt", "sich's", ",", "wo", "des", "Ni\u00b7gers", "Wel\u00b7len", "durch", "die", "Flur", "sich", "win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "$,", "PWAV", "ART", "NN", "NN", "APPR", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Nie den Tiger wirst du f\u00e4llen mit dem Wurf der scharfen Lanzen,", "tokens": ["Nie", "den", "Ti\u00b7ger", "wirst", "du", "f\u00e4l\u00b7len", "mit", "dem", "Wurf", "der", "schar\u00b7fen", "Lan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Nie den Reigen deiner V\u00e4ter zu dem Schlag der Pauke tanzen.", "tokens": ["Nie", "den", "Rei\u00b7gen", "dei\u00b7ner", "V\u00e4\u00b7ter", "zu", "dem", "Schlag", "der", "Pau\u00b7ke", "tan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Nein, dein Tag wird sein voll Tr\u00e4nen, deine Nacht wird sein voll Klagen,", "tokens": ["Nein", ",", "dein", "Tag", "wird", "sein", "voll", "Tr\u00e4\u00b7nen", ",", "dei\u00b7ne", "Nacht", "wird", "sein", "voll", "Kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VAFIN", "PPOSAT", "ADJD", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wie das Tier des Feldes wirst du stumm das Joch der Wei\u00dfen tragen,", "tokens": ["Wie", "das", "Tier", "des", "Fel\u00b7des", "wirst", "du", "stumm", "das", "Joch", "der", "Wei\u00b7\u00dfen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VAFIN", "PPER", "ADJD", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wirst das Holz den Wei\u00dfen f\u00e4llen und das Rohr den Wei\u00dfen schneiden,", "tokens": ["Wirst", "das", "Holz", "den", "Wei\u00b7\u00dfen", "f\u00e4l\u00b7len", "und", "das", "Rohr", "den", "Wei\u00b7\u00dfen", "schnei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "VVINF", "KON", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Die von unserm Marke prassen und in unsern Schwei\u00df sich kleiden.", "tokens": ["Die", "von", "un\u00b7serm", "Mar\u00b7ke", "pras\u00b7sen", "und", "in", "un\u00b7sern", "Schwei\u00df", "sich", "klei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVINF", "KON", "APPR", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Kluge M\u00e4nner sind die Wei\u00dfen, sie durchfahren k\u00fchn die Meere,", "tokens": ["Klu\u00b7ge", "M\u00e4n\u00b7ner", "sind", "die", "Wei\u00b7\u00dfen", ",", "sie", "durch\u00b7fah\u00b7ren", "k\u00fchn", "die", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Blitzesglut und Schall des Donners schl\u00e4ft in ihrem Jagdgewehre,", "tokens": ["Blit\u00b7zes\u00b7glut", "und", "Schall", "des", "Don\u00b7ners", "schl\u00e4ft", "in", "ih\u00b7rem", "Jagd\u00b7ge\u00b7weh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ihre M\u00fchlen, dampfgetrieben, regen sich mit tausend Armen,", "tokens": ["Ih\u00b7re", "M\u00fch\u00b7len", ",", "dampf\u00b7ge\u00b7trie\u00b7ben", ",", "re\u00b7gen", "sich", "mit", "tau\u00b7send", "Ar\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PAV", "$,", "ADJA", "PRF", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Aber ach, bei ihrer Klugheit wohnt im Herzen kein Erbarmen.", "tokens": ["A\u00b7ber", "ach", ",", "bei", "ih\u00b7rer", "Klug\u00b7heit", "wohnt", "im", "Her\u00b7zen", "kein", "Er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Oftmals h\u00f6rt' ich auch die Stolzen sich mit ihrer Freiheit br\u00fcsten,", "tokens": ["Oft\u00b7mals", "h\u00f6rt'", "ich", "auch", "die", "Stol\u00b7zen", "sich", "mit", "ih\u00b7rer", "Frei\u00b7heit", "br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wie sie k\u00fchn vom Mutterlande losgerissen diese K\u00fcsten,", "tokens": ["Wie", "sie", "k\u00fchn", "vom", "Mut\u00b7ter\u00b7lan\u00b7de", "los\u00b7ge\u00b7ris\u00b7sen", "die\u00b7se", "K\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Aber \u00fcber jenen Edlen, der mit Mut das Wort gesprochen,", "tokens": ["A\u00b7ber", "\u00fc\u00b7ber", "je\u00b7nen", "Ed\u00b7len", ",", "der", "mit", "Mut", "das", "Wort", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "$,", "PRELS", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Da\u00df die Schwarzen Menschen w\u00e4ren, haben sie den Stab gebrochen.", "tokens": ["Da\u00df", "die", "Schwar\u00b7zen", "Men\u00b7schen", "w\u00e4\u00b7ren", ",", "ha\u00b7ben", "sie", "den", "Stab", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VAFIN", "$,", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "S\u00fc\u00df erklinget ihre Predigt, wie ein Gott f\u00fcr sie gestorben", "tokens": ["S\u00fc\u00df", "er\u00b7klin\u00b7get", "ih\u00b7re", "Pre\u00b7digt", ",", "wie", "ein", "Gott", "f\u00fcr", "sie", "ge\u00b7stor\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und durch solches Liebesopfer aller Welt das Heil erworben;", "tokens": ["Und", "durch", "sol\u00b7ches", "Lie\u00b7be\u00b7sop\u00b7fer", "al\u00b7ler", "Welt", "das", "Heil", "er\u00b7wor\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "PIAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Doch wie soll das Wort ich glauben, wohnt es nicht in ihren Seelen?", "tokens": ["Doch", "wie", "soll", "das", "Wort", "ich", "glau\u00b7ben", ",", "wohnt", "es", "nicht", "in", "ih\u00b7ren", "See\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ist denn das der Sinn der Liebe, da\u00df sie uns zu Tode qu\u00e4len?", "tokens": ["Ist", "denn", "das", "der", "Sinn", "der", "Lie\u00b7be", ",", "da\u00df", "sie", "uns", "zu", "To\u00b7de", "qu\u00e4\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "O du gro\u00dfer Geist, was taten meines armen Stamms Genossen,", "tokens": ["O", "du", "gro\u00b7\u00dfer", "Geist", ",", "was", "ta\u00b7ten", "mei\u00b7nes", "ar\u00b7men", "Stamms", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,", "PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df du \u00fcber uns die Schalen deines Zornes ausgegossen!", "tokens": ["Da\u00df", "du", "\u00fc\u00b7ber", "uns", "die", "Scha\u00b7len", "dei\u00b7nes", "Zor\u00b7nes", "aus\u00b7ge\u00b7gos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Sprich, wann wirst du mild dein Auge aus den Wolken zu uns wenden?", "tokens": ["Sprich", ",", "wann", "wirst", "du", "mild", "dein", "Au\u00b7ge", "aus", "den", "Wol\u00b7ken", "zu", "uns", "wen\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "PPOSAT", "NN", "APPR", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Sprich, o sprich, wann wird der Jammer deiner schwarzen Kinder enden?", "tokens": ["Sprich", ",", "o", "sprich", ",", "wann", "wird", "der", "Jam\u00b7mer", "dei\u00b7ner", "schwar\u00b7zen", "Kin\u00b7der", "en\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "ADJD", "$,", "PWAV", "VAFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Ach, das mag geschehen, wenn der Mississippi r\u00fcckw\u00e4rts flie\u00dfet,", "tokens": ["Ach", ",", "das", "mag", "ge\u00b7sche\u00b7hen", ",", "wenn", "der", "Mis\u00b7sis\u00b7sip\u00b7pi", "r\u00fcck\u00b7w\u00e4rts", "flie\u00b7\u00dfet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VMFIN", "VVINF", "$,", "KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wenn an hoher Baumwollstaude dunkelblau die Bl\u00fcte sprie\u00dfet,", "tokens": ["Wenn", "an", "ho\u00b7her", "Baum\u00b7woll\u00b7stau\u00b7de", "dun\u00b7kel\u00b7blau", "die", "Bl\u00fc\u00b7te", "sp