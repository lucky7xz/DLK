{"textgrid.poem.38877": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hinblickend zu des Lebens w\u00fcsten Meeren,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinblickend zu des Lebens w\u00fcsten Meeren,", "tokens": ["Hin\u00b7bli\u00b7ckend", "zu", "des", "Le\u00b7bens", "w\u00fcs\u00b7ten", "Mee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Versteh' ich, wie wir alle irren m\u00fcssen,", "tokens": ["Ver\u00b7steh'", "ich", ",", "wie", "wir", "al\u00b7le", "ir\u00b7ren", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "PPER", "PIS", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie wir von Wind und Wellen hingerissen", "tokens": ["Wie", "wir", "von", "Wind", "und", "Wel\u00b7len", "hin\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Rund angek\u00e4mpft fortschweben in den Leeren.", "tokens": ["Rund", "an\u00b7ge\u00b7k\u00e4mpft", "fort\u00b7schwe\u00b7ben", "in", "den", "Lee\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was hilft's mit Schwerd und Schild sich zu bewehren?", "tokens": ["Was", "hilft's", "mit", "Schwerd", "und", "Schild", "sich", "zu", "be\u00b7weh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "KON", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was frommt bei Sturm und wilden Regeng\u00fcssen", "tokens": ["Was", "frommt", "bei", "Sturm", "und", "wil\u00b7den", "Re\u00b7gen\u00b7g\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch der Magnet und unser bestes Wissen?", "tokens": ["Auch", "der", "Mag\u00b7net", "und", "un\u00b7ser", "bes\u00b7tes", "Wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir werden nimmer so zum Hafen kehren.", "tokens": ["Wir", "wer\u00b7den", "nim\u00b7mer", "so", "zum", "Ha\u00b7fen", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch will ein freundlich Feuer sich erhellen,", "tokens": ["Doch", "will", "ein", "freund\u00b7lich", "Feu\u00b7er", "sich", "er\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJD", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das froh ergl\u00e4nzt von hoher Th\u00fcrme Zinnen,", "tokens": ["Das", "froh", "er\u00b7gl\u00e4nzt", "von", "ho\u00b7her", "Th\u00fcr\u00b7me", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVPP", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann wei\u00df das Schiff wie es die Segel richte.", "tokens": ["Dann", "wei\u00df", "das", "Schiff", "wie", "es", "die", "Se\u00b7gel", "rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KOKOM", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So ward ich fr\u00fch gelenkt von deinem Lichte,", "tokens": ["So", "ward", "ich", "fr\u00fch", "ge\u00b7lenkt", "von", "dei\u00b7nem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Poesie lie\u00df mich den Weg gewinnen,", "tokens": ["Die", "Poe\u00b7sie", "lie\u00df", "mich", "den", "Weg", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Zur Heimath trugen mich die goldnen Wellen.", "tokens": ["Zur", "Hei\u00b7math", "tru\u00b7gen", "mich", "die", "gold\u00b7nen", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}