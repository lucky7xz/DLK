{"textgrid.poem.63812": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Vorfr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh, die Kastanien \u2013 noch nicht entfalten", "tokens": ["Sieh", ",", "die", "Kas\u00b7ta\u00b7ni\u00b7en", "\u2013", "noch", "nicht", "ent\u00b7fal\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "$(", "ADV", "PTKNEG", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sie ihre Knospen, harzig gebr\u00e4unt.", "tokens": ["Sie", "ih\u00b7re", "Knos\u00b7pen", ",", "har\u00b7zig", "ge\u00b7br\u00e4unt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Den wei\u00dfen Schneehut hat aufbehalten", "tokens": ["Den", "wei\u00b7\u00dfen", "Schnee\u00b7hut", "hat", "auf\u00b7be\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Monte Baldo, mein alter Freund.", "tokens": ["Der", "Mon\u00b7te", "Bal\u00b7do", ",", "mein", "al\u00b7ter", "Freund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Der sch\u00f6ne Fr\u00fchling kommt z\u00f6gernd heuer;", "tokens": ["Der", "sch\u00f6\u00b7ne", "Fr\u00fch\u00b7ling", "kommt", "z\u00f6\u00b7gernd", "heu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So warm der Mittag, die Nacht ist rauh.", "tokens": ["So", "warm", "der", "Mit\u00b7tag", ",", "die", "Nacht", "ist", "rauh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auch im Kamin ist ein kleines Feuer", "tokens": ["Auch", "im", "Ka\u00b7min", "ist", "ein", "klei\u00b7nes", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Noch sehr willkommen der lieben Frau.", "tokens": ["Noch", "sehr", "will\u00b7kom\u00b7men", "der", "lie\u00b7ben", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Jungfr\u00e4ulich herbe sind noch die L\u00fcfte,", "tokens": ["Jung\u00b7fr\u00e4u\u00b7lich", "her\u00b7be", "sind", "noch", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Noch hat kein Vogel sein Nest gebaut,", "tokens": ["Noch", "hat", "kein", "Vo\u00b7gel", "sein", "Nest", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch von der Halde wehn Veilchend\u00fcfte,", "tokens": ["Doch", "von", "der", "Hal\u00b7de", "wehn", "Veil\u00b7chen\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "NE", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "S\u00fc\u00df wie der Atem der jungen Braut.", "tokens": ["S\u00fc\u00df", "wie", "der", "A\u00b7tem", "der", "jun\u00b7gen", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Wer wei\u00df, wie bald uns der Lenz beschieden,", "tokens": ["Wer", "wei\u00df", ",", "wie", "bald", "uns", "der", "Lenz", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "ADV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des holde N\u00e4he sich schon verriet.", "tokens": ["Des", "hol\u00b7de", "N\u00e4\u00b7he", "sich", "schon", "ver\u00b7riet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich fand heut fr\u00fch an des M\u00e4rzen Iden", "tokens": ["Ich", "fand", "heut", "fr\u00fch", "an", "des", "M\u00e4r\u00b7zen", "I\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schon Pfirsichbl\u00fcten und dieses Lied.", "tokens": ["Schon", "Pfir\u00b7sich\u00b7bl\u00fc\u00b7ten", "und", "die\u00b7ses", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "PDAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}