{"textgrid.poem.58700": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "12. Das er\u00f6ffnete Heiligtum", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Jesu Blut ist nun der Vorhang weggenommen,", "tokens": ["In", "Je\u00b7su", "Blut", "ist", "nun", "der", "Vor\u00b7hang", "weg\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der gr\u00f6\u00dfte S\u00fcnder darf und kann und mu\u00df wohl kommen", "tokens": ["Der", "gr\u00f6\u00df\u00b7te", "S\u00fcn\u00b7der", "darf", "und", "kann", "und", "mu\u00df", "wohl", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "KON", "VMFIN", "KON", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu Gott ins Heiligtum; Geh, Seele, denn hinzu", "tokens": ["Zu", "Gott", "ins", "Hei\u00b7lig\u00b7tum", ";", "Geh", ",", "See\u00b7le", ",", "denn", "hin\u00b7zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "APPRART", "NN", "$.", "NE", "$,", "NN", "$,", "KON", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch Sterben und Gebet zu dieser tiefen Ruh!", "tokens": ["Durch", "Ster\u00b7ben", "und", "Ge\u00b7bet", "zu", "die\u00b7ser", "tie\u00b7fen", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}