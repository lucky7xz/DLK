{"dta.poem.7475": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XxXII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie Jakob hab' ich oft mit Gott gerungen,", "tokens": ["Wie", "Ja\u00b7kob", "hab'", "ich", "oft", "mit", "Gott", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Oft f\u00fchlt' ich meinen Glauben zweifelnd stocken,", "tokens": ["Oft", "f\u00fchlt'", "ich", "mei\u00b7nen", "Glau\u00b7ben", "zwei\u00b7felnd", "sto\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und oftmals haben Eure Kirchenglocken,", "tokens": ["Und", "oft\u00b7mals", "ha\u00b7ben", "Eu\u00b7re", "Kir\u00b7chen\u00b7glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich l\u00e4ugn' es nicht, verdrie\u00dflich mir geklungen.", "tokens": ["Ich", "l\u00e4ugn'", "es", "nicht", ",", "ver\u00b7drie\u00df\u00b7lich", "mir", "ge\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}