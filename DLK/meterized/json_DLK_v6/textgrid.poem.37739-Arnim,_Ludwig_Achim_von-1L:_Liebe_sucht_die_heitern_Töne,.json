{"textgrid.poem.37739": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebe sucht die heitern T\u00f6ne,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebe sucht die heitern T\u00f6ne,", "tokens": ["Lie\u00b7be", "sucht", "die", "hei\u00b7tern", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller Zeiten Fr\u00fchlingssch\u00f6ne,", "tokens": ["Al\u00b7ler", "Zei\u00b7ten", "Fr\u00fch\u00b7lings\u00b7sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einzig schmerzt ihr, da\u00df die T\u00f6ne", "tokens": ["Ein\u00b7zig", "schmerzt", "ihr", ",", "da\u00df", "die", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fliehen, eh sie Liebe kr\u00f6ne.", "tokens": ["Flie\u00b7hen", ",", "eh", "sie", "Lie\u00b7be", "kr\u00f6\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Suche auf in gr\u00fcnen Bergen,", "tokens": ["Su\u00b7che", "auf", "in", "gr\u00fc\u00b7nen", "Ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die T\u00f6ne sich verbergen,", "tokens": ["Wo", "die", "T\u00f6\u00b7ne", "sich", "ver\u00b7ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob sie sind bewacht von Zwergen,", "tokens": ["Ob", "sie", "sind", "be\u00b7wacht", "von", "Zwer\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob geraubt von Morgenlerchen?", "tokens": ["Ob", "ge\u00b7raubt", "von", "Mor\u00b7gen\u00b7ler\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Suche sie im stillen Thale,", "tokens": ["Su\u00b7che", "sie", "im", "stil\u00b7len", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem dunklen Sternensaale,", "tokens": ["In", "dem", "dunk\u00b7len", "Ster\u00b7nen\u00b7saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Seen gr\u00fcner Schale,", "tokens": ["In", "der", "Seen", "gr\u00fc\u00b7ner", "Scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Auf der Felsen moosger Kahle.", "tokens": ["Auf", "der", "Fel\u00b7sen", "moos\u00b7ger", "Kah\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Felsen selbst, von Lieb umrungen,", "tokens": ["Fel\u00b7sen", "selbst", ",", "von", "Lieb", "um\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat die Wurzel k\u00fchn durchdrungen,", "tokens": ["Hat", "die", "Wur\u00b7zel", "k\u00fchn", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht dem Ha\u00df ist Sieg gelungen,", "tokens": ["Nicht", "dem", "Ha\u00df", "ist", "Sieg", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebe h\u00e4lt sie fest umschlungen.", "tokens": ["Lie\u00b7be", "h\u00e4lt", "sie", "fest", "um\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie vereinet ferne Str\u00f6me,", "tokens": ["Sie", "ver\u00b7ei\u00b7net", "fer\u00b7ne", "Str\u00f6\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eint den Fels mit Wolkensch\u00f6ne.", "tokens": ["Eint", "den", "Fels", "mit", "Wol\u00b7ken\u00b7sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der holde Sieg dich kr\u00f6ne,", "tokens": ["Da\u00df", "der", "hol\u00b7de", "Sieg", "dich", "kr\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei wie meines Liedes T\u00f6ne;", "tokens": ["Sei", "wie", "mei\u00b7nes", "Lie\u00b7des", "T\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Das so still in meiner Seele,", "tokens": ["Das", "so", "still", "in", "mei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmer dringt durch meine Kehle:", "tokens": ["Nim\u00b7mer", "dringt", "durch", "mei\u00b7ne", "Keh\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So verhehle dich und stehle", "tokens": ["So", "ver\u00b7heh\u00b7le", "dich", "und", "steh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinen Gru\u00df aus meiner Seele!", "tokens": ["Mei\u00b7nen", "Gru\u00df", "aus", "mei\u00b7ner", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}