{"textgrid.poem.30634": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir fressen manches Thier,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir fressen manches Thier,", "tokens": ["Wir", "fres\u00b7sen", "man\u00b7ches", "Thier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das gr\u00f6sser ist denn wir;", "tokens": ["Das", "gr\u00f6s\u00b7ser", "ist", "denn", "wir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir d\u00fcrffen einen Raum", "tokens": ["Wir", "d\u00fcrf\u00b7fen", "ei\u00b7nen", "Raum"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von drey vier Elen kaum;", "tokens": ["Von", "drey", "vier", "E\u00b7len", "kaum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "CARD", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Noch ist kein Land genug,", "tokens": ["Noch", "ist", "kein", "Land", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das unsre Kost uns trug;", "tokens": ["Das", "uns\u00b7re", "Kost", "uns", "trug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Man mu\u00df sie suchen her", "tokens": ["Man", "mu\u00df", "sie", "su\u00b7chen", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Durch alles Land und Meer,", "tokens": ["Durch", "al\u00b7les", "Land", "und", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Da doch f\u00fcr unsrer Th\u00fcr", "tokens": ["Da", "doch", "f\u00fcr", "uns\u00b7rer", "Th\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Ist Nothdurfft und Geb\u00fchr;", "tokens": ["Ist", "Noth\u00b7durfft", "und", "Ge\u00b7b\u00fchr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Das macht der Gurgel Klufft,", "tokens": ["Das", "macht", "der", "Gur\u00b7gel", "Klufft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Die stets nach mehrem rufft.", "tokens": ["Die", "stets", "nach", "meh\u00b7rem", "rufft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}