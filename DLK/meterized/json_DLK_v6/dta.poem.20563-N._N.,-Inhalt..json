{"dta.poem.20563": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Inhalt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1848", "urn": "urn:nbn:de:kobv:b4-25129-1", "language": ["de:0.99"], "booktitle": "[N. N.]: Erinnerungen eines freiwilligen reitenden J\u00e4gers aus den Kriegsjahren 1813\u20131815. Berlin, 1848."}, "poem": {"stanza.1": {"line.1": {"text": "Du, der bei L\u00fcbeck, Eylau Preu\u00dfens Retter,", "tokens": ["Du", ",", "der", "bei", "L\u00fc\u00b7beck", ",", "Ey\u00b7lau", "Preu\u00b7\u00dfens", "Ret\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "NE", "$,", "NE", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Scharnhorst! Dem That, Entwurf erhaben, k&#252;hn!", "tokens": ["Scharn\u00b7horst", "!", "Dem", "That", ",", "Ent\u00b7wurf", "er\u00b7ha\u00b7ben", ",", "k", "&#252;", "hn", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "XML_entity", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "$,", "NN", "ADJD", "$,", "VVIMP", "$(", "VVINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Der Intriguant, der Witzling, wie der Sp\u00f6tter", "tokens": ["Der", "Int\u00b7ri\u00b7gu\u00b7ant", ",", "der", "Witz\u00b7ling", ",", "wie", "der", "Sp\u00f6t\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Vermochten nicht Dich in ihr Netz zu ziehn.", "tokens": ["Ver\u00b7moch\u00b7ten", "nicht", "Dich", "in", "ihr", "Netz", "zu", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPER", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du lie\u00df\u2019st sie walten, hofftest, da\u00df F\u00fcrst ", "tokens": ["Du", "lie\u00df'st", "sie", "wal\u00b7ten", ",", "hoff\u00b7test", ",", "da\u00df", "F\u00fcrst"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "VVFIN", "$,", "KOUS", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Nich w&#252;rde schlie\u00dfen B&#252;ndni\u00df mit Berlin. &#8212;", "tokens": ["Nich", "w", "&#252;", "rde", "schlie\u00b7\u00dfen", "B", "&#252;", "nd\u00b7ni\u00df", "mit", "Ber\u00b7lin", ".", "&#8212;"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word", "word", "word", "punct", "XML_entity"], "pos": ["PTKNEG", "NE", "$(", "NN", "VVFIN", "XY", "$(", "VVFIN", "APPR", "NE", "$.", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Dein Sinn hat uns der Freiheit Pfand gegeben!", "tokens": ["Dein", "Sinn", "hat", "uns", "der", "Frei\u00b7heit", "Pfand", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bei ihrem Morgenroth erlosch Dein Leben!", "tokens": ["Bei", "ih\u00b7rem", "Mor\u00b7gen\u00b7roth", "er\u00b7losch", "Dein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aus Ru\u00dflands Kriegsdienst achtzehnhundertsieben", "tokens": ["Aus", "Ru\u00df\u00b7lands", "Kriegs\u00b7dienst", "acht\u00b7zehn\u00b7hun\u00b7dert\u00b7sie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kamst Du. \u2014 Bald hob Dich Deines K\u00f6nigs Gunst. \u2014", "tokens": ["Kamst", "Du", ".", "Bald", "hob", "Dich", "Dei\u00b7nes", "K\u00f6\u00b7nigs", "Gunst", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Als Kind schon that\u2019st Du Dich im Schreiben \u00fcben,", "tokens": ["Als", "Kind", "schon", "that'st", "Du", "Dich", "im", "Schrei\u00b7ben", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So ward\u2019st Du Meister in der Kriegs-Schreibkunst.", "tokens": ["So", "ward'st", "Du", "Meis\u00b7ter", "in", "der", "Kriegs\u00b7Schreib\u00b7kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Jm klarsten Wasser fischtest, nicht im tr\u00fcben", "tokens": ["Jm", "klars\u00b7ten", "Was\u00b7ser", "fischtest", ",", "nicht", "im", "tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,", "PTKNEG", "APPRART", "ADJA"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Du! \u2014 Sch\u00e4dlich schien Dir stock\u2019ger Aktendunst. \u2014", "tokens": ["Du", "!", "Sch\u00e4d\u00b7lich", "schien", "Dir", "stock'\u00b7ger", "Ak\u00b7ten\u00b7dunst", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$.", "$(", "ADJD", "VVFIN", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Direktor hei\u00dfend, warst Du Kriegsminister.", "tokens": ["Di\u00b7rek\u00b7tor", "hei\u00b7\u00dfend", ",", "warst", "Du", "Kriegs\u00b7mi\u00b7nis\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Corse durchschaut Dich, rief: Rebelle ist Der!", "tokens": ["Cor\u00b7se", "durch\u00b7schaut", "Dich", ",", "rief", ":", "Re\u00b7bel\u00b7le", "ist", "Der", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "VVFIN", "$.", "NN", "VAFIN", "ART", "$."], "meter": "+--++--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Dein Scharfblick lie\u00df das Recht\u2019 und Best\u2019 Dich finden,", "tokens": ["Dein", "Scharf\u00b7blick", "lie\u00df", "das", "Recht'", "und", "Best'", "Dich", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gr\u00fcnde und Urtheil war bei Dir durchdacht.", "tokens": ["Gr\u00fcn\u00b7de", "und", "Ur\u00b7theil", "war", "bei", "Dir", "durch\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Des Geistes Blitz mit Wollen zu verbinden", "tokens": ["Des", "Geis\u00b7tes", "Blitz", "mit", "Wol\u00b7len", "zu", "ver\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vermochtest Du, sahst hell auch in der Nacht!", "tokens": ["Ver\u00b7moch\u00b7test", "Du", ",", "sahst", "hell", "auch", "in", "der", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da, wo Verzweiflung tobt in finstern Gr\u00fcnden", "tokens": ["Da", ",", "wo", "Ver\u00b7zwei\u00b7flung", "tobt", "in", "fins\u00b7tern", "Gr\u00fcn\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gingst Du getrost, doch nahmst Du\u2019s Bein in Acht!", "tokens": ["Gingst", "Du", "ge\u00b7trost", ",", "doch", "nahmst", "Du's", "Bein", "in", "Acht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$,", "ADV", "ADJD", "ADJA", "NN", "APPR", "CARD", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.7": {"text": "In Staats-Angst-Heilkunst warst Du Zunft-Altmeister!", "tokens": ["In", "Staats\u00b7Angst\u00b7Heil\u00b7kunst", "warst", "Du", "Zunft\u00b7Alt\u00b7meis\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dir glich ein Arzt, der alte ", "tokens": ["Dir", "glich", "ein", "Arzt", ",", "der", "al\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "S\u00fchnt schon der Tod ein Leben voll Verirrung,", "tokens": ["S\u00fchnt", "schon", "der", "Tod", "ein", "Le\u00b7ben", "voll", "Ver\u00b7ir\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "(der Heiland spricht: Tod ist der S\u00fcnden Sold.)", "tokens": ["(", "der", "Hei\u00b7land", "spricht", ":", "Tod", "ist", "der", "S\u00fcn\u00b7den", "Sold", ".", ")"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$.", "NN", "VAFIN", "ART", "NN", "NE", "$.", "$("], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "L\u00f6st ", "tokens": ["L\u00f6st"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Zerst\u00f6rt selbst Phylax-Wachen \u00fcber Gold;", "tokens": ["Zer\u00b7st\u00f6rt", "selbst", "Phy\u00b7la\u00b7xWa\u00b7chen", "\u00fc\u00b7ber", "Gold", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So bleibt es sch\u00f6ner, wenn der Selbstsucht Schwirrung", "tokens": ["So", "bleibt", "es", "sch\u00f6\u00b7ner", ",", "wenn", "der", "Selbst\u00b7sucht", "Schwir\u00b7rung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das Leben nicht ber\u00fchrt, das rein und hold. \u2014", "tokens": ["Das", "Le\u00b7ben", "nicht", "be\u00b7r\u00fchrt", ",", "das", "rein", "und", "hold", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$,", "PRELS", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Scharnhorst, als Weg gebahnt Du, Palm' erworben,", "tokens": ["Scharn\u00b7horst", ",", "als", "Weg", "ge\u00b7bahnt", "Du", ",", "Palm'", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "VVPP", "PPER", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Focht\u2019st todt Du fort, bist nicht zu fr\u00fch gestorben!", "tokens": ["Focht'st", "todt", "Du", "fort", ",", "bist", "nicht", "zu", "fr\u00fch", "ge\u00b7stor\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PTKVZ", "$,", "VAFIN", "PTKNEG", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}