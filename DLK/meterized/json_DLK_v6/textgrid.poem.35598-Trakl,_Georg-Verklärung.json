{"textgrid.poem.35598": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Verkl\u00e4rung", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn es Abend wird,", "tokens": ["Wenn", "es", "A\u00b7bend", "wird", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Verl\u00e4\u00dft dich leise ein blaues Antlitz.", "tokens": ["Ver\u00b7l\u00e4\u00dft", "dich", "lei\u00b7se", "ein", "blau\u00b7es", "Ant\u00b7litz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein kleiner Vogel singt im Tamarindenbaum.", "tokens": ["Ein", "klei\u00b7ner", "Vo\u00b7gel", "singt", "im", "Ta\u00b7ma\u00b7rin\u00b7den\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein sanfter M\u00f6nch", "tokens": ["Ein", "sanf\u00b7ter", "M\u00f6nch"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Faltet die erstorbenen H\u00e4nde.", "tokens": ["Fal\u00b7tet", "die", "ers\u00b7tor\u00b7be\u00b7nen", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ein wei\u00dfer Engel sucht Marien heim.", "tokens": ["Ein", "wei\u00b7\u00dfer", "En\u00b7gel", "sucht", "Ma\u00b7ri\u00b7en", "heim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein n\u00e4chtiger Kranz", "tokens": ["Ein", "n\u00e4ch\u00b7ti\u00b7ger", "Kranz"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Von Veilchen, Korn und purpurnen Trauben", "tokens": ["Von", "Veil\u00b7chen", ",", "Korn", "und", "pur\u00b7pur\u00b7nen", "Trau\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ist das Jahr des Schauenden.", "tokens": ["Ist", "das", "Jahr", "des", "Schau\u00b7en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Zu deinen F\u00fc\u00dfen", "tokens": ["Zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "\u00d6ffnen sich die Gr\u00e4ber der Toten,", "tokens": ["\u00d6ff\u00b7nen", "sich", "die", "Gr\u00e4\u00b7ber", "der", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wenn du die Stirne in die silbernen H\u00e4nde legst.", "tokens": ["Wenn", "du", "die", "Stir\u00b7ne", "in", "die", "sil\u00b7ber\u00b7nen", "H\u00e4n\u00b7de", "legst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Stille wohnt", "tokens": ["Stil\u00b7le", "wohnt"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "An deinem Mund der herbstliche Mond,", "tokens": ["An", "dei\u00b7nem", "Mund", "der", "herbst\u00b7li\u00b7che", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Trunken von Mohnsaft dunkler Gesang;", "tokens": ["Trun\u00b7ken", "von", "Mohn\u00b7saft", "dunk\u00b7ler", "Ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Blaue Blume,", "tokens": ["Blau\u00b7e", "Blu\u00b7me", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Die leise t\u00f6nt in vergilbtem Gestein.", "tokens": ["Die", "lei\u00b7se", "t\u00f6nt", "in", "ver\u00b7gilb\u00b7tem", "Ge\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}