{"dta.poem.2758": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Hochzeit-Lieder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer m\u00fcssen frische Myrten stehn/", "tokens": ["Hjer", "m\u00fcs\u00b7sen", "fri\u00b7sche", "Myr\u00b7ten", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Fu\u00df soll itzt auf Rosen gehn;", "tokens": ["Mein", "Fu\u00df", "soll", "itzt", "auf", "Ro\u00b7sen", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Gl\u00fccke will mir selber betten/", "tokens": ["Das", "Gl\u00fc\u00b7cke", "will", "mir", "sel\u00b7ber", "bet\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn die/ so meine Seele liebt/", "tokens": ["Denn", "die", "/", "so", "mei\u00b7ne", "See\u00b7le", "liebt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$(", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und der mein Hertze sich ergiebt/", "tokens": ["Und", "der", "mein", "Hert\u00b7ze", "sich", "er\u00b7giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSAT", "VVFIN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bind mich mit allzusch\u00f6nen Ketten.", "tokens": ["Bind", "mich", "mit", "all\u00b7zu\u00b7sch\u00f6\u00b7nen", "Ket\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie zeigt die Sch\u00e4tze ihrer Gunst", "tokens": ["Sie", "zeigt", "die", "Sch\u00e4t\u00b7ze", "ih\u00b7rer", "Gunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In gleicher Gluth in gleicher Brunst:", "tokens": ["In", "glei\u00b7cher", "Gluth", "in", "glei\u00b7cher", "Brunst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr Blick entdecket die Gedancken.", "tokens": ["Ihr", "Blick", "ent\u00b7de\u00b7cket", "die", "Ge\u00b7dan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hierleg ich meine Freyheit hin/", "tokens": ["Hier\u00b7leg", "ich", "mei\u00b7ne", "Frey\u00b7heit", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil ich ihr Liebs-Gefangner bin.", "tokens": ["Weil", "ich", "ihr", "Liebs\u00b7Ge\u00b7fang\u00b7ner", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jhr Wille setzt mir Ziel und Schrancken.", "tokens": ["Ihr", "Wil\u00b7le", "setzt", "mir", "Ziel", "und", "Schran\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Fr\u00fchling ihrer besten Zeit", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ih\u00b7rer", "bes\u00b7ten", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Voll Anmuth voller Lieblichkeit.", "tokens": ["Voll", "An\u00b7muth", "vol\u00b7ler", "Lieb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gibt meinen Geistern neues Leben.", "tokens": ["Gibt", "mei\u00b7nen", "Geis\u00b7tern", "neu\u00b7es", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich seh\u2019 auf ihrer Wangen Rund", "tokens": ["Ich", "seh'", "auf", "ih\u00b7rer", "Wan\u00b7gen", "Rund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In dem so sch\u00f6nen Zucker-Mund", "tokens": ["In", "dem", "so", "sch\u00f6\u00b7nen", "Zu\u00b7cke\u00b7rMund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Gratien leibhafftig schweben.", "tokens": ["Der", "Gra\u00b7ti\u00b7en", "leib\u00b7haff\u00b7tig", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Komm Sch\u00f6nste meiner Seelen Licht", "tokens": ["Komm", "Sch\u00f6ns\u00b7te", "mei\u00b7ner", "See\u00b7len", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df mich aus meinem Angesicht", "tokens": ["La\u00df", "mich", "aus", "mei\u00b7nem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Hertzens wahre Meynung lesen.", "tokens": ["Des", "Hert\u00b7zens", "wah\u00b7re", "Mey\u00b7nung", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wei\u00df von deiner edlen Treu/", "tokens": ["Ich", "wei\u00df", "von", "dei\u00b7ner", "ed\u00b7len", "Treu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ihr nichts vorzuziehen sey.", "tokens": ["Da\u00df", "ihr", "nichts", "vor\u00b7zu\u00b7zie\u00b7hen", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVIZU", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch jemals etwas gleich gewesen.", "tokens": ["Noch", "je\u00b7mals", "et\u00b7was", "gleich", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du bist mein Stern mein Paradei\u00df/", "tokens": ["Du", "bist", "mein", "Stern", "mein", "Pa\u00b7ra\u00b7dei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und was ich nicht zu nennen wei\u00df/", "tokens": ["Und", "was", "ich", "nicht", "zu", "nen\u00b7nen", "wei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Kern und Au\u00dfzug meiner Seele/", "tokens": ["Der", "Kern", "und", "Au\u00df\u00b7zug", "mei\u00b7ner", "See\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es soll in diesem Leib und Blut", "tokens": ["Es", "soll", "in", "die\u00b7sem", "Leib", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Stets brennen meine Liebes-Gluth", "tokens": ["Stets", "bren\u00b7nen", "mei\u00b7ne", "Lie\u00b7bes\u00b7Gluth"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVINF", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bi\u00df zu der schwartzen Grabes H\u00f6le.", "tokens": ["Bi\u00df", "zu", "der", "schwart\u00b7zen", "Gra\u00b7bes", "H\u00f6\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "La\u00df uns/ weil es der Himmel schafft/", "tokens": ["La\u00df", "uns", "/", "weil", "es", "der", "Him\u00b7mel", "schafft", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Jugend voller Bl\u00fcth und Safft/", "tokens": ["Der", "Ju\u00b7gend", "vol\u00b7ler", "Bl\u00fcth", "und", "Safft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Liebe Nectar-Strohm geniessen", "tokens": ["Der", "Lie\u00b7be", "Nec\u00b7ta\u00b7rStrohm", "ge\u00b7nies\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Bund/ der uns zusammenf\u00fcgt", "tokens": ["Den", "Bund", "/", "der", "uns", "zu\u00b7sam\u00b7men\u00b7f\u00fcgt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und beyder Hertz und Sinn vergn\u00fcgt/", "tokens": ["Und", "bey\u00b7der", "Hertz", "und", "Sinn", "ver\u00b7gn\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Besigelt ein empfindlich K\u00fcssen.", "tokens": ["Be\u00b7si\u00b7gelt", "ein", "emp\u00b7find\u00b7lich", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}