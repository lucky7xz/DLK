{"textgrid.poem.34710": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gekommen ist der Maie,", "tokens": ["Ge\u00b7kom\u00b7men", "ist", "der", "Mai\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Blumen und B\u00e4ume bl\u00fchn,", "tokens": ["Die", "Blu\u00b7men", "und", "B\u00e4u\u00b7me", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und durch die Himmelsbl\u00e4ue", "tokens": ["Und", "durch", "die", "Him\u00b7mels\u00b7bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die rosigen Wolken ziehn.", "tokens": ["Die", "ro\u00b7si\u00b7gen", "Wol\u00b7ken", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Nachtigallen singen", "tokens": ["Die", "Nach\u00b7ti\u00b7gal\u00b7len", "sin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herab aus der laubigen H\u00f6h',", "tokens": ["Her\u00b7ab", "aus", "der", "lau\u00b7bi\u00b7gen", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die wei\u00dfen L\u00e4mmer springen", "tokens": ["Die", "wei\u00b7\u00dfen", "L\u00e4m\u00b7mer", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im weichen gr\u00fcnen Klee.", "tokens": ["Im", "wei\u00b7chen", "gr\u00fc\u00b7nen", "Klee", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich kann nicht singen und springen,", "tokens": ["Ich", "kann", "nicht", "sin\u00b7gen", "und", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich liege krank im Gras;", "tokens": ["Ich", "lie\u00b7ge", "krank", "im", "Gras", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich h\u00f6re fernes Klingen,", "tokens": ["Ich", "h\u00f6\u00b7re", "fer\u00b7nes", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir tr\u00e4umt, ich wei\u00df nicht was.", "tokens": ["Mir", "tr\u00e4umt", ",", "ich", "wei\u00df", "nicht", "was", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}