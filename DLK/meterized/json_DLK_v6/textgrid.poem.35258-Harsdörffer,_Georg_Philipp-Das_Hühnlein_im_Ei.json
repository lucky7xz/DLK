{"textgrid.poem.35258": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "Das H\u00fchnlein im Ei", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich leb' und bin noch nicht auf diese Welt gekommen,", "tokens": ["Ich", "leb'", "und", "bin", "noch", "nicht", "auf", "die\u00b7se", "Welt", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "ADV", "PTKNEG", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch meiner Mutter Leib vor kurzer Zeit entnommen.", "tokens": ["Doch", "mei\u00b7ner", "Mut\u00b7ter", "Leib", "vor", "kur\u00b7zer", "Zeit", "ent\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann ich mein Haus zerbrech', er\u00f6ffn' ich meine Th\u00fcr,", "tokens": ["Wann", "ich", "mein", "Haus", "zer\u00b7brech'", ",", "er\u00b7\u00f6ffn'", "ich", "mei\u00b7ne", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was mich bedecken sollt', bedeck' ich noch in mir", "tokens": ["Was", "mich", "be\u00b7de\u00b7cken", "sollt'", ",", "be\u00b7deck'", "ich", "noch", "in", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "VMFIN", "$,", "VVFIN", "PPER", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}