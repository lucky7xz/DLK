{"textgrid.poem.58273": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "311. Die sicherste Retirade", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Jesus tr\u00e4gt den Fluch, der mich sonst h\u00e4tt' getroffen,", "tokens": ["Mein", "Je\u00b7sus", "tr\u00e4gt", "den", "Fluch", ",", "der", "mich", "sonst", "h\u00e4tt'", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schlie\u00dft durch seinen Tod des Vaters Herz mir offen;", "tokens": ["Und", "schlie\u00dft", "durch", "sei\u00b7nen", "Tod", "des", "Va\u00b7ters", "Herz", "mir", "of\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Flieh, Turtelt\u00e4ubchen, flieh in diese Ritzen ein", "tokens": ["Flieh", ",", "Tur\u00b7tel\u00b7t\u00e4ub\u00b7chen", ",", "flieh", "in", "die\u00b7se", "Rit\u00b7zen", "ein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "APPR", "PDAT", "NN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus aller Kreatur, wenn du willst sicher sein!", "tokens": ["Aus", "al\u00b7ler", "Kre\u00b7a\u00b7tur", ",", "wenn", "du", "willst", "si\u00b7cher", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "KOUS", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+++-+", "measure": "unknown.measure.septa"}}}}}