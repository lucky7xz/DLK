{"dta.poem.8670": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Reiterlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Wir streifen durchs Leben im schnellen Zug,", "tokens": ["Wir", "strei\u00b7fen", "durchs", "Le\u00b7ben", "im", "schnel\u00b7len", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ohne Rast wie die st\u00fcrmische Welle,", "tokens": ["Oh\u00b7ne", "Rast", "wie", "die", "st\u00fcr\u00b7mi\u00b7sche", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wir haschen die Frucht im Vor\u00fcberflug,", "tokens": ["Wir", "ha\u00b7schen", "die", "Frucht", "im", "Vor\u00b7\u00fc\u00b7berf\u00b7lug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und schlummern nicht ein an der Quelle;", "tokens": ["Und", "schlum\u00b7mern", "nicht", "ein", "an", "der", "Quel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wir pfl\u00fccken die Rose, wir saugen den Duft,", "tokens": ["Wir", "pfl\u00fc\u00b7cken", "die", "Ro\u00b7se", ",", "wir", "sau\u00b7gen", "den", "Duft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und streuen sie dann in die flatternde Luft.", "tokens": ["Und", "streu\u00b7en", "sie", "dann", "in", "die", "flat\u00b7tern\u00b7de", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Der Friedliche sitzet und lauert bang,", "tokens": ["Der", "Fried\u00b7li\u00b7che", "sit\u00b7zet", "und", "lau\u00b7ert", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Bis das Gl\u00fcck ihm poch' an die Th\u00fcre,", "tokens": ["Bis", "das", "Gl\u00fcck", "ihm", "poch'", "an", "die", "Th\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Noch sp\u00e4ht er beim Sterbegl\u00f6ckleinklang,", "tokens": ["Noch", "sp\u00e4ht", "er", "beim", "Ster\u00b7be\u00b7gl\u00f6c\u00b7klein\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ob das Gl\u00fcck an der Klinke nicht r\u00fchre;", "tokens": ["Ob", "das", "Gl\u00fcck", "an", "der", "Klin\u00b7ke", "nicht", "r\u00fch\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Wohl r\u00fchrt sich die Klink', und es tritt herein,", "tokens": ["Wohl", "r\u00fchrt", "sich", "die", "Klink'", ",", "und", "es", "tritt", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Erschrick nicht, du Armer, \u2014 es ist Freund Hein!", "tokens": ["Er\u00b7schrick", "nicht", ",", "du", "Ar\u00b7mer", ",", "es", "ist", "Freund", "Hein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "$,", "PPER", "NN", "$,", "$(", "PPER", "VAFIN", "NN", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Der Reiter verfolgt das entlaufende Gl\u00fcck,", "tokens": ["Der", "Rei\u00b7ter", "ver\u00b7folgt", "das", "ent\u00b7lau\u00b7fen\u00b7de", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Er fa\u00dft's an den fliegenden Locken,", "tokens": ["Er", "fa\u00dft's", "an", "den", "flie\u00b7gen\u00b7den", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und zwingt es zu sich auf den Sattel zur\u00fcck,", "tokens": ["Und", "zwingt", "es", "zu", "sich", "auf", "den", "Sat\u00b7tel", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Und umschlingt es mit wildem Frohlocken:", "tokens": ["Und", "um\u00b7schlingt", "es", "mit", "wil\u00b7dem", "Froh\u00b7lo\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+--", "measure": "anapaest.di.plus"}, "line.5": {"text": "\u201emu\u00dft reiten mit mir durch Nacht und Graus,", "tokens": ["\u201e", "mu\u00dft", "rei\u00b7ten", "mit", "mir", "durch", "Nacht", "und", "Graus", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "VVFIN", "APPR", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u201edurch Strom und Gekl\u00fcft zum blutigen Strau\u00df!\u201c", "tokens": ["\u201e", "durch", "Strom", "und", "Ge\u00b7kl\u00fcft", "zum", "blu\u00b7ti\u00b7gen", "Strau\u00df", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Wir reiten hinein in die laute Schlacht,", "tokens": ["Wir", "rei\u00b7ten", "hin\u00b7ein", "in", "die", "lau\u00b7te", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es tanzen die wiehernden Rosse", "tokens": ["Es", "tan\u00b7zen", "die", "wie\u00b7hern\u00b7den", "Ros\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dahin, wo der Donner am st\u00e4rksten kracht,", "tokens": ["Da\u00b7hin", ",", "wo", "der", "Don\u00b7ner", "am", "st\u00e4rks\u00b7ten", "kracht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Weit voran dem trippelnden Trosse;", "tokens": ["Weit", "vo\u00b7ran", "dem", "trip\u00b7peln\u00b7den", "Tros\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Dem Reiter kredenzt auf sein st\u00fcrmisch Gebot", "tokens": ["Dem", "Rei\u00b7ter", "kre\u00b7denzt", "auf", "sein", "st\u00fcr\u00b7misch", "Ge\u00b7bot"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Den ersten, den feurigsten Trunk der Tod!", "tokens": ["Den", "ers\u00b7ten", ",", "den", "feu\u00b7rigs\u00b7ten", "Trunk", "der", "Tod", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}