{"textgrid.poem.58785": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein Herz ist ja so eng und kalt,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Herz ist ja so eng und kalt,", "tokens": ["Dein", "Herz", "ist", "ja", "so", "eng", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du dienest Gott mit Furcht alleine;", "tokens": ["Du", "die\u00b7nest", "Gott", "mit", "Furcht", "al\u00b7lei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Lieb ihn, so wird dein Herze bald", "tokens": ["Lieb", "ihn", ",", "so", "wird", "dein", "Her\u00b7ze", "bald"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "ADV", "VAFIN", "PPOSAT", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erweitert, warm und Gott gemeine!", "tokens": ["Er\u00b7wei\u00b7tert", ",", "warm", "und", "Gott", "ge\u00b7mei\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "KON", "NN", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}