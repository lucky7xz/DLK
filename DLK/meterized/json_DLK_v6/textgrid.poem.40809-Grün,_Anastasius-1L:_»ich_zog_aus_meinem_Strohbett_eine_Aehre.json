{"textgrid.poem.40809": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbich zog aus meinem Strohbett eine Aehre", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich zog aus meinem Strohbett eine Aehre", "tokens": ["\u00bb", "ich", "zog", "aus", "mei\u00b7nem", "Stroh\u00b7bett", "ei\u00b7ne", "A\u00b7eh\u00b7re"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+----", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Und hielt sie lang vors Aug' in meinen H\u00e4nden;", "tokens": ["Und", "hielt", "sie", "lang", "vors", "Aug'", "in", "mei\u00b7nen", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als ob in ihr ein stiller Zauber w\u00e4re,", "tokens": ["Als", "ob", "in", "ihr", "ein", "stil\u00b7ler", "Zau\u00b7ber", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "PPOSAT", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Konnt' ich die Blicke nimmer von ihr wenden.", "tokens": ["Konnt'", "ich", "die", "Bli\u00b7cke", "nim\u00b7mer", "von", "ihr", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Feld voll Garben stieg vor meinen Blicken!", "tokens": ["Ein", "Feld", "voll", "Gar\u00b7ben", "stieg", "vor", "mei\u00b7nen", "Bli\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ha, wie sie fl\u00fcsternd durch einander gaukeln,", "tokens": ["Ha", ",", "wie", "sie", "fl\u00fcs\u00b7ternd", "durch", "ein\u00b7an\u00b7der", "gau\u00b7keln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "PPER", "VVPP", "APPR", "PRF", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Gesch\u00e4ftig mit den goldnen H\u00e4uptern nicken", "tokens": ["Ge\u00b7sch\u00e4f\u00b7tig", "mit", "den", "gold\u00b7nen", "H\u00e4up\u00b7tern", "ni\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und weithin ihres Meeres Wogen schaukeln!", "tokens": ["Und", "weit\u00b7hin", "ih\u00b7res", "Mee\u00b7res", "Wo\u00b7gen", "schau\u00b7keln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Von blanken Sicheln, durch die Schwaden ringend,", "tokens": ["Von", "blan\u00b7ken", "Si\u00b7cheln", ",", "durch", "die", "Schwa\u00b7den", "rin\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist, Silberk\u00e4hnen gleich, die\u00df Meer befahren,", "tokens": ["Ist", ",", "Sil\u00b7ber\u00b7k\u00e4h\u00b7nen", "gleich", ",", "die\u00df", "Meer", "be\u00b7fah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "ADV", "$,", "PDS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Schnitterm\u00e4dchen, aus den Wogen springend,", "tokens": ["Und", "Schnit\u00b7ter\u00b7m\u00e4d\u00b7chen", ",", "aus", "den", "Wo\u00b7gen", "sprin\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es sind der Meeresg\u00f6ttin Dienerschaaren.", "tokens": ["Es", "sind", "der", "Mee\u00b7res\u00b7g\u00f6t\u00b7tin", "Die\u00b7ner\u00b7schaa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und blanke D\u00f6rfer rings und gr\u00fcne H\u00fcgel,", "tokens": ["Und", "blan\u00b7ke", "D\u00f6r\u00b7fer", "rings", "und", "gr\u00fc\u00b7ne", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dar\u00fcber hin der ew'ge Himmel blauend", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "hin", "der", "ew'\u00b7ge", "Him\u00b7mel", "blau\u00b7end"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Lerchen drin, von Morgenroth die Fl\u00fcgel,", "tokens": ["Und", "Ler\u00b7chen", "drin", ",", "von", "Mor\u00b7gen\u00b7roth", "die", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "$,", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und von Gesang die Kehlen \u00fcberthauend!", "tokens": ["Und", "von", "Ge\u00b7sang", "die", "Keh\u00b7len", "\u00fc\u00b7bert\u00b7hau\u00b7end", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die W\u00e4lder s\u00e4useln, und die Quellen klingen,", "tokens": ["Die", "W\u00e4l\u00b7der", "s\u00e4u\u00b7seln", ",", "und", "die", "Quel\u00b7len", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dort um die Linde t\u00f6nt's von Fl\u00f6t' und Geigen,", "tokens": ["Dort", "um", "die", "Lin\u00b7de", "t\u00f6nt's", "von", "Fl\u00f6t'", "und", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NE", "NE", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df Bursch und Dirne sich im Reigen schwingen,", "tokens": ["Da\u00df", "Bursch", "und", "Dir\u00b7ne", "sich", "im", "Rei\u00b7gen", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und selbst die Bl\u00fcthen tanzen von den Zweigen", "tokens": ["Und", "selbst", "die", "Bl\u00fc\u00b7then", "tan\u00b7zen", "von", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die Garben ruhn den Jungfrau'n nun zu F\u00fc\u00dfen,", "tokens": ["Die", "Gar\u00b7ben", "ruhn", "den", "Jung\u00b7frau'n", "nun", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und auf den Garben farb'ge Kr\u00e4nze liegen;", "tokens": ["Und", "auf", "den", "Gar\u00b7ben", "fa\u00b7rb'\u00b7ge", "Kr\u00e4n\u00b7ze", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich fasse einen, um in eines s\u00fc\u00dfen,", "tokens": ["Ich", "fas\u00b7se", "ei\u00b7nen", ",", "um", "in", "ei\u00b7nes", "s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "$,", "KOUI", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geliebten Hauptes Locken ihn zu schmiegen.", "tokens": ["Ge\u00b7lieb\u00b7ten", "Haup\u00b7tes", "Lo\u00b7cken", "ihn", "zu", "schmie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Da rasselt mir am Arm die Kett' entgegen,", "tokens": ["Da", "ras\u00b7selt", "mir", "am", "Arm", "die", "Kett'", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Hand, der bebenden, entsinkt die Aehre!", "tokens": ["Der", "Hand", ",", "der", "be\u00b7ben\u00b7den", ",", "ent\u00b7sinkt", "die", "A\u00b7eh\u00b7re", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Du d\u00fcrrer Halm, wie h\u00e4tt' ich's denken m\u00f6gen,", "tokens": ["Du", "d\u00fcr\u00b7rer", "Halm", ",", "wie", "h\u00e4tt'", "ich's", "den\u00b7ken", "m\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PWAV", "VAFIN", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ich durch dich noch einst so elend w\u00e4re!\u00ab", "tokens": ["Da\u00df", "ich", "durch", "dich", "noch", "einst", "so", "e\u00b7lend", "w\u00e4\u00b7re", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "ADV", "ADV", "ADJD", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}