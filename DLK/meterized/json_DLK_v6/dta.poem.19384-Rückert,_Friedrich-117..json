{"dta.poem.19384": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "117.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O G\u00e4rtner, der du hier den Baum im Garten ziehst,", "tokens": ["O", "G\u00e4rt\u00b7ner", ",", "der", "du", "hier", "den", "Baum", "im", "Gar\u00b7ten", "ziehst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit stolzer Sch\u00f6pferlust auf deine Sch\u00f6pfung siehst!", "tokens": ["Mit", "stol\u00b7zer", "Sch\u00f6p\u00b7fer\u00b7lust", "auf", "dei\u00b7ne", "Sch\u00f6p\u00b7fung", "siehst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In Wahrheit hast du doch den Samen nicht gemacht,", "tokens": ["In", "Wahr\u00b7heit", "hast", "du", "doch", "den", "Sa\u00b7men", "nicht", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hast auch nicht daraus den Baum hervorgebracht.", "tokens": ["Und", "hast", "auch", "nicht", "da\u00b7raus", "den", "Baum", "her\u00b7vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PTKNEG", "PAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch dein ist das Verdienst, da\u00df du den Samen streutest,", "tokens": ["Doch", "dein", "ist", "das", "Ver\u00b7dienst", ",", "da\u00df", "du", "den", "Sa\u00b7men", "streu\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gro\u00df den Baum zu ziehn, nicht M\u00fch noch Sorgfalt scheutest.", "tokens": ["Und", "gro\u00df", "den", "Baum", "zu", "ziehn", ",", "nicht", "M\u00fch", "noch", "Sorg\u00b7falt", "scheu\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$,", "PTKNEG", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}