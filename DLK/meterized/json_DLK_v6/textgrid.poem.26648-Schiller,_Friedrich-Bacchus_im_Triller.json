{"textgrid.poem.26648": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Bacchus im Triller", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.57", "fr:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trille! Trille! blind und dumm,", "tokens": ["Tril\u00b7le", "!", "Tril\u00b7le", "!", "blind", "und", "dumm", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Taub und dumm,", "tokens": ["Taub", "und", "dumm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Trillt den saubern Kerl herum!", "tokens": ["Trillt", "den", "sau\u00b7bern", "Kerl", "he\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Manches St\u00fcck von altem Adel,", "tokens": ["Man\u00b7ches", "St\u00fcck", "von", "al\u00b7tem", "A\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vetter, hast du auf der Nadel.", "tokens": ["Vet\u00b7ter", ",", "hast", "du", "auf", "der", "Na\u00b7del", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vetter, \u00fcbel kommst du weg!", "tokens": ["Vet\u00b7ter", ",", "\u00fc\u00b7bel", "kommst", "du", "weg", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Manchen Kopf mit Dampf gef\u00fcllet,", "tokens": ["Man\u00b7chen", "Kopf", "mit", "Dampf", "ge\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Manchen hast du umgetrillet,", "tokens": ["Man\u00b7chen", "hast", "du", "um\u00b7ge\u00b7tril\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Manchen klugen Kopf ber\u00fclpet,", "tokens": ["Man\u00b7chen", "klu\u00b7gen", "Kopf", "be\u00b7r\u00fcl\u00b7pet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Manchen Magen umgestilpet,", "tokens": ["Man\u00b7chen", "Ma\u00b7gen", "um\u00b7ge\u00b7stil\u00b7pet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Umgew\u00e4lzt in seinem Speck,", "tokens": ["Um\u00b7ge\u00b7w\u00e4lzt", "in", "sei\u00b7nem", "Speck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Manchen Hut krumm aufgesetzet,", "tokens": ["Man\u00b7chen", "Hut", "krumm", "auf\u00b7ge\u00b7set\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Manches Lamm in Wut gehetzet,", "tokens": ["Man\u00b7ches", "Lamm", "in", "Wut", "ge\u00b7het\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "B\u00e4ume, Hecken, H\u00e4user, Gassen", "tokens": ["B\u00e4u\u00b7me", ",", "He\u00b7cken", ",", "H\u00e4u\u00b7ser", ",", "Gas\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Um uns Narren tanzen lassen.", "tokens": ["Um", "uns", "Nar\u00b7ren", "tan\u00b7zen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Darum kommst du \u00fcbel weg,", "tokens": ["Da\u00b7rum", "kommst", "du", "\u00fc\u00b7bel", "weg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Darum wirst auch du getrillet,", "tokens": ["Da\u00b7rum", "wirst", "auch", "du", "ge\u00b7tril\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.12": {"text": "Wirst auch du mit Dampf gef\u00fcllet,", "tokens": ["Wirst", "auch", "du", "mit", "Dampf", "ge\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Darum wirst auch du ber\u00fclpet,", "tokens": ["Da\u00b7rum", "wirst", "auch", "du", "be\u00b7r\u00fcl\u00b7pet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "PPER", "VVFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.14": {"text": "Wird dein Magen umgestilpet,", "tokens": ["Wird", "dein", "Ma\u00b7gen", "um\u00b7ge\u00b7stil\u00b7pet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Umgew\u00e4lzt in seinem Speck,", "tokens": ["Um\u00b7ge\u00b7w\u00e4lzt", "in", "sei\u00b7nem", "Speck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Darum kommst du \u00fcbel weg.", "tokens": ["Da\u00b7rum", "kommst", "du", "\u00fc\u00b7bel", "weg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Trille! Trille! blind und dumm,", "tokens": ["Tril\u00b7le", "!", "Tril\u00b7le", "!", "blind", "und", "dumm", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Taub und dumm,", "tokens": ["Taub", "und", "dumm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Trillt den saubern Kerl herum!", "tokens": ["Trillt", "den", "sau\u00b7bern", "Kerl", "he\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Siehst, wie du mit unsern Zungen,", "tokens": ["Siehst", ",", "wie", "du", "mit", "un\u00b7sern", "Zun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unserm Witz bist umgesprungen,", "tokens": ["Un\u00b7serm", "Witz", "bist", "um\u00b7ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Siehst du itzt, du lockrer Specht?", "tokens": ["Siehst", "du", "itzt", ",", "du", "lock\u00b7rer", "Specht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie du uns am Seil gezwirbelt,", "tokens": ["Wie", "du", "uns", "am", "Seil", "ge\u00b7zwir\u00b7belt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Uns im Ring herumgewirbelt,", "tokens": ["Uns", "im", "Ring", "her\u00b7um\u00b7ge\u00b7wir\u00b7belt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df uns Nacht ums Auge grauste,", "tokens": ["Da\u00df", "uns", "Nacht", "ums", "Au\u00b7ge", "graus\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPRART", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Da\u00df's uns in den Ohren sauste,", "tokens": ["Da\u00df's", "uns", "in", "den", "Oh\u00b7ren", "saus\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lerns in deinem K\u00e4figt recht!", "tokens": ["Lerns", "in", "dei\u00b7nem", "K\u00e4\u00b7figt", "recht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df wir vor dem Ohrgebr\u00fcmmel", "tokens": ["Da\u00df", "wir", "vor", "dem", "Ohr\u00b7ge\u00b7br\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmer Gottes blauen Himmel,", "tokens": ["Nim\u00b7mer", "Got\u00b7tes", "blau\u00b7en", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimmer sahen Stock und Steine,", "tokens": ["Nim\u00b7mer", "sa\u00b7hen", "Stock", "und", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Knackten auf die lieben Beine.", "tokens": ["Knack\u00b7ten", "auf", "die", "lie\u00b7ben", "Bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Siehst du itzt, du lockrer Specht?", "tokens": ["Siehst", "du", "itzt", ",", "du", "lock\u00b7rer", "Specht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df wir Gottes gelbe Sonne", "tokens": ["Da\u00df", "wir", "Got\u00b7tes", "gel\u00b7be", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr die Heidelberger Tonne,", "tokens": ["F\u00fcr", "die", "Hei\u00b7del\u00b7ber\u00b7ger", "Ton\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Berge, B\u00e4ume, T\u00fcrme, Schl\u00f6sser", "tokens": ["Ber\u00b7ge", ",", "B\u00e4u\u00b7me", ",", "T\u00fcr\u00b7me", ",", "Schl\u00f6s\u00b7ser"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Angesehn f\u00fcr Schoppengl\u00e4ser,", "tokens": ["An\u00b7ge\u00b7sehn", "f\u00fcr", "Schop\u00b7pen\u00b7gl\u00e4\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lernst dus itzt, du lockrer Specht?", "tokens": ["Lernst", "dus", "itzt", ",", "du", "lock\u00b7rer", "Specht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lerns in deinem K\u00e4figt recht!", "tokens": ["Lerns", "in", "dei\u00b7nem", "K\u00e4\u00b7figt", "recht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Trille! Trille! blind und dumm,", "tokens": ["Tril\u00b7le", "!", "Tril\u00b7le", "!", "blind", "und", "dumm", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Taub und dumm,", "tokens": ["Taub", "und", "dumm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Trill den saubern Kerl herum!", "tokens": ["Trill", "den", "sau\u00b7bern", "Kerl", "he\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Schwager, warst doch sonst voll R\u00e4nke,", "tokens": ["Schwa\u00b7ger", ",", "warst", "doch", "sonst", "voll", "R\u00e4n\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ADV", "ADV", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwager, wo nun deine Schw\u00e4nke,", "tokens": ["Schwa\u00b7ger", ",", "wo", "nun", "dei\u00b7ne", "Schw\u00e4n\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Pfiffe, schlauer Kopf?", "tokens": ["Dei\u00b7ne", "Pfif\u00b7fe", ",", "schlau\u00b7er", "Kopf", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Ausgepumpt sind deine Pfiffe,", "tokens": ["Aus\u00b7ge\u00b7pumpt", "sind", "dei\u00b7ne", "Pfif\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und zum Teufel sind die Kniffe!", "tokens": ["Und", "zum", "Teu\u00b7fel", "sind", "die", "Knif\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Albern wie ein Stutzer plaudern,", "tokens": ["Al\u00b7bern", "wie", "ein", "Stut\u00b7zer", "plau\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Waschweib wirst du kaudern,", "tokens": ["Wie", "ein", "Wasc\u00b7hweib", "wirst", "du", "kau\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Junker ist ein seichter Tropf.", "tokens": ["Jun\u00b7ker", "ist", "ein", "seich\u00b7ter", "Tropf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nun, so wei\u00dft dus \u2013 magst dich sch\u00e4men,", "tokens": ["Nun", ",", "so", "wei\u00dft", "dus", "\u2013", "magst", "dich", "sch\u00e4\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "NE", "$(", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Magst meintwegen Rei\u00dfaus nehmen,", "tokens": ["Magst", "meint\u00b7we\u00b7gen", "Rei\u00df\u00b7aus", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dem Hollunken Amor r\u00fchmen,", "tokens": ["Dem", "Hol\u00b7lun\u00b7ken", "A\u00b7mor", "r\u00fch\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Dran er soll Exempel nehmen.", "tokens": ["Dran", "er", "soll", "Ex\u00b7em\u00b7pel", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Fort, B\u00e4rnh\u00e4uter! tummle dich!", "tokens": ["Fort", ",", "B\u00e4rn\u00b7h\u00e4u\u00b7ter", "!", "tumm\u00b7le", "dich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Unser Witz, aus Glas gekerbet,", "tokens": ["Un\u00b7ser", "Witz", ",", "aus", "Glas", "ge\u00b7ker\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wie der Blitz ist er zerscherbet;", "tokens": ["Wie", "der", "Blitz", "ist", "er", "zer\u00b7scher\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Soll dich nicht der Triller treiben,", "tokens": ["Soll", "dich", "nicht", "der", "Tril\u00b7ler", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "La\u00df die Narrenspossen bleiben!", "tokens": ["La\u00df", "die", "Nar\u00b7rens\u00b7pos\u00b7sen", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Hasts verstanden? Denk an mich!", "tokens": ["Hasts", "ver\u00b7stan\u00b7den", "?", "Denk", "an", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "$.", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "W\u00fcster Vogel, packe dich!", "tokens": ["W\u00fcs\u00b7ter", "Vo\u00b7gel", ",", "pa\u00b7cke", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}