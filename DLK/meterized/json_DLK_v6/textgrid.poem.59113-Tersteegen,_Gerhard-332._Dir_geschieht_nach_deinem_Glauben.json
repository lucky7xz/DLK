{"textgrid.poem.59113": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "332. Dir geschieht nach deinem Glauben", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott will sich geben ohne Ende;", "tokens": ["Gott", "will", "sich", "ge\u00b7ben", "oh\u00b7ne", "En\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Unglaub' bindet ihm die H\u00e4nde.", "tokens": ["Der", "Un\u00b7glaub'", "bin\u00b7det", "ihm", "die", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er fordert nichts vor dir als: Komm!", "tokens": ["Er", "for\u00b7dert", "nichts", "vor", "dir", "als", ":", "Komm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPER", "KOUS", "$.", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann kommt er auch; ", "tokens": ["Dann", "kommt", "er", "auch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}