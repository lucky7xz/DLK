{"textgrid.poem.53087": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Crispin Derschow und Regina Bessel", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was dieses sawre Leben", "tokens": ["Was", "die\u00b7ses", "saw\u00b7re", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verkehr' in Honigseim,", "tokens": ["Ver\u00b7kehr'", "in", "Ho\u00b7ni\u00b7gseim", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wil ich durch wenig Reim'", "tokens": ["Wil", "ich", "durch", "we\u00b7nig", "Reim'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Itzt zu verstehen geben:", "tokens": ["Itzt", "zu", "ver\u00b7ste\u00b7hen", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Hertz, das Tugend-fest", "tokens": ["Ein", "Hertz", ",", "das", "Tu\u00b7gen\u00b7dfest"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sich seiner Vnschuld frewet,", "tokens": ["Sich", "sei\u00b7ner", "Vn\u00b7schuld", "fre\u00b7wet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Vnd wenn Ihm Vngl\u00fcck drewet,", "tokens": ["Vnd", "wenn", "Ihm", "Vn\u00b7gl\u00fcck", "dre\u00b7wet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Getrost auff Gott verlesst;", "tokens": ["Ge\u00b7trost", "auff", "Gott", "ver\u00b7lesst", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Leib, der wol gediehen,", "tokens": ["Ein", "Leib", ",", "der", "wol", "ge\u00b7die\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vnd nicht von n\u00f6then hat,", "tokens": ["Vnd", "nicht", "von", "n\u00f6\u00b7then", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df man vmb H\u00fclff vnd Rath", "tokens": ["Da\u00df", "man", "vmb", "H\u00fclff", "vnd", "Rath"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Aertzte mu\u00df bem\u00fchen,", "tokens": ["Die", "A\u00b7ertz\u00b7te", "mu\u00df", "be\u00b7m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ein Acker, der wol tr\u00e4gt,", "tokens": ["Ein", "A\u00b7cker", ",", "der", "wol", "tr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit keinem Menschen streiten,", "tokens": ["Mit", "kei\u00b7nem", "Men\u00b7schen", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein Herdt, der aller zeiten", "tokens": ["Ein", "Herdt", ",", "der", "al\u00b7ler", "zei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zur Notturfft Fewer hegt;", "tokens": ["Zur", "Not\u00b7turfft", "Fe\u00b7wer", "hegt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der klugen Einfallt Gaben,", "tokens": ["Der", "klu\u00b7gen", "Ein\u00b7fallt", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Tisch ohn allen Pracht,", "tokens": ["Ein", "Tisch", "ohn", "al\u00b7len", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wol ruhen bey der Nacht,", "tokens": ["Wol", "ru\u00b7hen", "bey", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Gew\u00fcnschte Freunde haben,", "tokens": ["Ge\u00b7w\u00fcnschte", "Freun\u00b7de", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ein Weib, da\u00df jhren Mann", "tokens": ["Ein", "Weib", ",", "da\u00df", "jhren", "Mann"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "In h\u00f6chsten trewen liebet,", "tokens": ["In", "h\u00f6chs\u00b7ten", "tre\u00b7wen", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vnd kl\u00fcglich nichts ver\u00fcbet", "tokens": ["Vnd", "kl\u00fcg\u00b7lich", "nichts", "ver\u00b7\u00fc\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PIS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Ihn bek\u00fcmmern kan;", "tokens": ["Das", "Ihn", "be\u00b7k\u00fcm\u00b7mern", "kan", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Von frembder Leute Sachen", "tokens": ["Von", "fremb\u00b7der", "Leu\u00b7te", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durchau\u00df geschieden seyn,", "tokens": ["Durch\u00b7au\u00df", "ge\u00b7schie\u00b7den", "seyn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie bringen wenig ein,", "tokens": ["Sie", "brin\u00b7gen", "we\u00b7nig", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr sich am meisten wachen,", "tokens": ["F\u00fcr", "sich", "am", "meis\u00b7ten", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPRART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Belieben seinen Standt,", "tokens": ["Be\u00b7lie\u00b7ben", "sei\u00b7nen", "Standt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Den Stoltz vnd Hochmut hassen,", "tokens": ["Den", "Stoltz", "vnd", "Hoch\u00b7mut", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sich wol gefallen lassen,", "tokens": ["Sich", "wol", "ge\u00b7fal\u00b7len", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was Gott Ihm zuerkandt;", "tokens": ["Was", "Gott", "Ihm", "zu\u00b7er\u00b7kandt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Au\u00df Vngedult vnd Leiden", "tokens": ["Au\u00df", "Vn\u00b7ge\u00b7dult", "vnd", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Todt nicht ruffen zwar,", "tokens": ["Den", "Todt", "nicht", "ruf\u00b7fen", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch stellt er sich nun dar,", "tokens": ["Doch", "stellt", "er", "sich", "nun", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Behertzt seyn abzuscheiden:", "tokens": ["Be\u00b7hertzt", "seyn", "ab\u00b7zu\u00b7schei\u00b7den", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die\u00df sind die wenig Reim',", "tokens": ["Die\u00df", "sind", "die", "we\u00b7nig", "Reim'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vnd haben kunt gegeben,", "tokens": ["Vnd", "ha\u00b7ben", "kunt", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Was dieses sawre Leben", "tokens": ["Was", "die\u00b7ses", "saw\u00b7re", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verkehr' in Honigseim.", "tokens": ["Ver\u00b7kehr'", "in", "Ho\u00b7ni\u00b7gseim", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}