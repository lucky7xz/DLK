{"textgrid.poem.58634": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gott hilft zwar allen gern, doch mu\u00dft du sein aufrichtig,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott hilft zwar allen gern, doch mu\u00dft du sein aufrichtig,", "tokens": ["Gott", "hilft", "zwar", "al\u00b7len", "gern", ",", "doch", "mu\u00dft", "du", "sein", "auf\u00b7rich\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PIS", "ADV", "$,", "ADV", "VMFIN", "PPER", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hast du ein falsches Herz, so ist dein Glaube nichtig;", "tokens": ["Hast", "du", "ein", "fal\u00b7sches", "Herz", ",", "so", "ist", "dein", "Glau\u00b7be", "nich\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verstelle dich doch nicht, Gott kennet deinen Grund,", "tokens": ["Ver\u00b7stel\u00b7le", "dich", "doch", "nicht", ",", "Gott", "ken\u00b7net", "dei\u00b7nen", "Grund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$,", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch die Gedanken selbst sind ihm, o Seele, kund!", "tokens": ["Auch", "die", "Ge\u00b7dan\u00b7ken", "selbst", "sind", "ihm", ",", "o", "See\u00b7le", ",", "kund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VAFIN", "PPER", "$,", "FM", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gedanken, nicht ", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "nicht"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mu\u00dft du als Eitelkeit, wo du sie findst, verlieren;", "tokens": ["Mu\u00dft", "du", "als", "Ei\u00b7tel\u00b7keit", ",", "wo", "du", "sie", "findst", ",", "ver\u00b7lie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "KOUS", "NN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Herr, hilf mir wachen stets, viel' st\u00fcrmen auf mich zu,", "tokens": ["Herr", ",", "hilf", "mir", "wa\u00b7chen", "stets", ",", "viel'", "st\u00fcr\u00b7men", "auf", "mich", "zu", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "VVINF", "VVFIN", "$,", "PIS", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In meine Herberg' doch soll nichts hinein als du!", "tokens": ["In", "mei\u00b7ne", "Her\u00b7ber\u00b7g'", "doch", "soll", "nichts", "hin\u00b7ein", "als", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VMFIN", "PIS", "ADV", "KOUS", "PPER", "$."], "meter": "-+-+----+-+-+", "measure": "unknown.measure.penta"}}}}}