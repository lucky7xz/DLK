{"textgrid.poem.39235": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: K\u00f6nig Erich sprach mit schwerem Sinn:", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nig Erich sprach mit schwerem Sinn:", "tokens": ["K\u00f6\u00b7nig", "E\u00b7rich", "sprach", "mit", "schwe\u00b7rem", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "\u00bbmeine Tochter ist weg, ich wei\u00df nicht, wohin?", "tokens": ["\u00bb", "mei\u00b7ne", "Toch\u00b7ter", "ist", "weg", ",", "ich", "wei\u00df", "nicht", ",", "wo\u00b7hin", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADV", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ich m\u00f6chte sie suchen und wei\u00df nicht, wie?\u00ab", "tokens": ["Ich", "m\u00f6ch\u00b7te", "sie", "su\u00b7chen", "und", "wei\u00df", "nicht", ",", "wie", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "KON", "VVFIN", "PTKNEG", "$,", "PWAV", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Rolf D\u00fcring sprach: \u00bbIch suche sie!\u00ab", "tokens": ["Rolf", "D\u00fc\u00b7ring", "sprach", ":", "\u00bb", "Ich", "su\u00b7che", "sie", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gar mannhaft sprach Rolf D\u00fcring.", "tokens": ["Gar", "mann\u00b7haft", "sprach", "Rolf", "D\u00fc\u00b7ring", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "NE", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Rolf D\u00fcring sprang ins Boot zur Stund'", "tokens": ["Rolf", "D\u00fc\u00b7ring", "sprang", "ins", "Boot", "zur", "Stund'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ruderte \u00fcber den \u00d6resund.", "tokens": ["Und", "ru\u00b7der\u00b7te", "\u00fc\u00b7ber", "den", "\u00d6\u00b7re\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Es pfiff der Fant manch lustigen Reim,", "tokens": ["Es", "pfiff", "der", "Fant", "manch", "lus\u00b7ti\u00b7gen", "Reim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "So fuhr Rolf D\u00fcring gen Riesenheim,", "tokens": ["So", "fuhr", "Rolf", "D\u00fc\u00b7ring", "gen", "Rie\u00b7sen\u00b7heim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NE", "APPR", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Gar freudig fuhr Rolf D\u00fcring.", "tokens": ["Gar", "freu\u00b7dig", "fuhr", "Rolf", "D\u00fc\u00b7ring", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "NE", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und als er kam vor des Riesen Tor,", "tokens": ["Und", "als", "er", "kam", "vor", "des", "Rie\u00b7sen", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Rolf D\u00fcring ritt die Stufen empor,", "tokens": ["Rolf", "D\u00fc\u00b7ring", "ritt", "die", "Stu\u00b7fen", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Wohl lag auf den Stufen manch' bleichend' Gebein,", "tokens": ["Wohl", "lag", "auf", "den", "Stu\u00b7fen", "man\u00b7ch'", "blei\u00b7chend'", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Rolf D\u00fcring pfiff und sprengte hinein,", "tokens": ["Rolf", "D\u00fc\u00b7ring", "pfiff", "und", "spreng\u00b7te", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Nicht bange war Rolf D\u00fcring.", "tokens": ["Nicht", "ban\u00b7ge", "war", "Rolf", "D\u00fc\u00b7ring", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VAFIN", "NE", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und als er kam vor des Riesen Schwell',", "tokens": ["Und", "als", "er", "kam", "vor", "des", "Rie\u00b7sen", "Schwell'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da stand im Saale ein langer Gesell,", "tokens": ["Da", "stand", "im", "Saa\u00b7le", "ein", "lan\u00b7ger", "Ge\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er stand und ragte als wie ein Haus,", "tokens": ["Er", "stand", "und", "rag\u00b7te", "als", "wie", "ein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Rolf D\u00fcring sah wie ein Zaunk\u00f6nig aus,", "tokens": ["Rolf", "D\u00fc\u00b7ring", "sah", "wie", "ein", "Zaun\u00b7k\u00f6\u00b7nig", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "KOKOM", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Was k\u00fcmmerte das Rolf D\u00fcring!", "tokens": ["Was", "k\u00fcm\u00b7mer\u00b7te", "das", "Rolf", "D\u00fc\u00b7ring", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Rolf D\u00fcring setzt die Sporen ein:", "tokens": ["Rolf", "D\u00fc\u00b7ring", "setzt", "die", "Spo\u00b7ren", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbherr Riese, Du mu\u00dft verloren sein!\u00ab", "tokens": ["\u00bb", "herr", "Rie\u00b7se", ",", "Du", "mu\u00dft", "ver\u00b7lo\u00b7ren", "sein", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "NN", "$,", "PPER", "VMFIN", "VVPP", "VAINF", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Riese lachte bei jedem Stich,", "tokens": ["Der", "Rie\u00b7se", "lach\u00b7te", "bei", "je\u00b7dem", "Stich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das war Rolf D\u00fcring sehr \u00e4rgerlich,", "tokens": ["Das", "war", "Rolf", "D\u00fc\u00b7ring", "sehr", "\u00e4r\u00b7ger\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "NE", "ADV", "ADJD", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Gar zornig ward Rolf D\u00fcring.", "tokens": ["Gar", "zor\u00b7nig", "ward", "Rolf", "D\u00fc\u00b7ring", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "NE", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbund w\u00e4rest Du l\u00e4nger, denn ein Mast,", "tokens": ["\u00bb", "und", "w\u00e4\u00b7rest", "Du", "l\u00e4n\u00b7ger", ",", "denn", "ein", "Mast", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "PPER", "ADJD", "$,", "KON", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu Boden mu\u00dft Du grober Gast!\u00ab", "tokens": ["Zu", "Bo\u00b7den", "mu\u00dft", "Du", "gro\u00b7ber", "Gast", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Anprallte der Ritter im vollen Galopp,", "tokens": ["An\u00b7prall\u00b7te", "der", "Rit\u00b7ter", "im", "vol\u00b7len", "Ga\u00b7lopp", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da fiel der Riese, das war ihm zu grob!", "tokens": ["Da", "fiel", "der", "Rie\u00b7se", ",", "das", "war", "ihm", "zu", "grob", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PDS", "VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und auf ihn sprang Rolf D\u00fcring:", "tokens": ["Und", "auf", "ihn", "sprang", "Rolf", "D\u00fc\u00b7ring", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "NE", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbheraus die Prinzessin im Augenblick!", "tokens": ["\u00bb", "he\u00b7raus", "die", "Prin\u00b7zes\u00b7sin", "im", "Au\u00b7gen\u00b7blick", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sonst schneid' ich Dir ab Dein zottig Genick!", "tokens": ["Sonst", "schneid'", "ich", "Dir", "ab", "Dein", "zot\u00b7tig", "Ge\u00b7nick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er stach drei Zoll tief, oder mehr,", "tokens": ["Er", "stach", "drei", "Zoll", "tief", ",", "o\u00b7der", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "ADJD", "$,", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da schrie der Riese: Ich strecke die Wehr!\u00ab", "tokens": ["Da", "schrie", "der", "Rie\u00b7se", ":", "Ich", "stre\u00b7cke", "die", "Wehr", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Zu heftig stach Rolf D\u00fcring.", "tokens": ["Zu", "hef\u00b7tig", "stach", "Rolf", "D\u00fc\u00b7ring", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "NE", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Rolf D\u00fcring zog, stolz war sein Zug,", "tokens": ["Rolf", "D\u00fc\u00b7ring", "zog", ",", "stolz", "war", "sein", "Zug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er hielt die Prinzessin im Sattelbug,", "tokens": ["Er", "hielt", "die", "Prin\u00b7zes\u00b7sin", "im", "Sat\u00b7tel\u00b7bug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vorn stapste der Riese und zagte sehr,", "tokens": ["Vorn", "staps\u00b7te", "der", "Rie\u00b7se", "und", "zag\u00b7te", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ihm sa\u00df im Nacken Rolf D\u00fcrings Speer;", "tokens": ["Ihm", "sa\u00df", "im", "Na\u00b7cken", "Rolf", "D\u00fc\u00b7rings", "Speer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NE", "NE", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Zu Meere zog Rolf D\u00fcring.", "tokens": ["Zu", "Mee\u00b7re", "zog", "Rolf", "D\u00fc\u00b7ring", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NE", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Rolf D\u00fcring schrie mit Ungest\u00fcm:", "tokens": ["Rolf", "D\u00fc\u00b7ring", "schrie", "mit", "Un\u00b7ge\u00b7st\u00fcm", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbnun trag' uns hin\u00fcber Du Unget\u00fcm,", "tokens": ["\u00bb", "nun", "trag'", "uns", "hin\u00b7\u00fc\u00b7ber", "Du", "Un\u00b7ge\u00b7t\u00fcm", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "PPER", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Auf den rechten Arm mich und mein Fr\u00e4ulein wert", "tokens": ["Auf", "den", "rech\u00b7ten", "Arm", "mich", "und", "mein", "Fr\u00e4u\u00b7lein", "wert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Und auf den linken nimm mein Pferd!\u00ab", "tokens": ["Und", "auf", "den", "lin\u00b7ken", "nimm", "mein", "Pferd", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "VVIMP", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gar dr\u00e4uend schrie Rolf D\u00fcring.", "tokens": ["Gar", "dr\u00e4u\u00b7end", "schrie", "Rolf", "D\u00fc\u00b7ring", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "NE", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Der Riese hob das rechte Bein", "tokens": ["Der", "Rie\u00b7se", "hob", "das", "rech\u00b7te", "Bein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und stiefelte in den Sund hinein,", "tokens": ["Und", "stie\u00b7fel\u00b7te", "in", "den", "Sund", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er h\u00e4tte sich gerne gesch\u00fcttelt, der Wicht,", "tokens": ["Er", "h\u00e4t\u00b7te", "sich", "ger\u00b7ne", "ge\u00b7sch\u00fct\u00b7telt", ",", "der", "Wicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "VVPP", "$,", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Allein er tat es lieber nicht,", "tokens": ["Al\u00b7lein", "er", "tat", "es", "lie\u00b7ber", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er forchte sich vor Rolf D\u00fcring. \u2013", "tokens": ["Er", "forch\u00b7te", "sich", "vor", "Rolf", "D\u00fc\u00b7ring", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NE", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "In Leire's Burg tanzt Herr und Gesind',", "tokens": ["In", "Lei\u00b7re's", "Burg", "tanzt", "Herr", "und", "Ge\u00b7sind'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da freit Rolf D\u00fcring des K\u00f6nigs Kind,", "tokens": ["Da", "freit", "Rolf", "D\u00fc\u00b7ring", "des", "K\u00f6\u00b7nigs", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NE", "NE", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wenn es wahr ist, was sie sagen,", "tokens": ["Und", "wenn", "es", "wahr", "ist", ",", "was", "sie", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So mu\u00dfte der Riese ins Bett sie tragen,", "tokens": ["So", "mu\u00df\u00b7te", "der", "Rie\u00b7se", "ins", "Bett", "sie", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ins Brautbett zu Rolf D\u00fcring.", "tokens": ["Ins", "Braut\u00b7bett", "zu", "Rolf", "