{"textgrid.poem.63202": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Der Friede", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Friede st\u00fcrzt ins Land", "tokens": ["Der", "Frie\u00b7de", "st\u00fcrzt", "ins", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gleich einem Schaf, von W\u00f6lfen angerissen.", "tokens": ["Gleich", "ei\u00b7nem", "Schaf", ",", "von", "W\u00f6l\u00b7fen", "an\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er tr\u00e4gt ein grau Gewand,", "tokens": ["Er", "tr\u00e4gt", "ein", "grau", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zerflattert und zersplissen.", "tokens": ["Zer\u00b7flat\u00b7tert", "und", "zer\u00b7splis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sein Antlitz ist zerfressen,", "tokens": ["Sein", "Ant\u00b7litz", "ist", "zer\u00b7fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein Auge ohne Glanz.", "tokens": ["Sein", "Au\u00b7ge", "oh\u00b7ne", "Glanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hat vergessen", "tokens": ["Er", "hat", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Den eignen Namen ganz.", "tokens": ["Den", "eig\u00b7nen", "Na\u00b7men", "ganz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gleich einem alten Kind", "tokens": ["Gleich", "ei\u00b7nem", "al\u00b7ten", "Kind"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "(gealtert fr\u00fch in Harmen)", "tokens": ["(", "ge\u00b7al\u00b7tert", "fr\u00fch", "in", "Har\u00b7men", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Steht er im Abendwind", "tokens": ["Steht", "er", "im", "Ab\u00b7end\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und bettelt um Erbarmen", "tokens": ["Und", "bet\u00b7telt", "um", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es gl\u00e4nzt sein blondes Haar,", "tokens": ["Es", "gl\u00e4nzt", "sein", "blon\u00b7des", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Sonne doch ein Teilchen.", "tokens": ["Der", "Son\u00b7ne", "doch", "ein", "Teil\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er bietet l\u00e4chelnd dar", "tokens": ["Er", "bie\u00b7tet", "l\u00e4\u00b7chelnd", "dar"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein welkes Herz und welke Veilchen.", "tokens": ["Ein", "wel\u00b7kes", "Herz", "und", "wel\u00b7ke", "Veil\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "PWAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}