{"textgrid.poem.33116": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst zu Turin am Hofe versammelte zum Feste", "tokens": ["Einst", "zu", "Tu\u00b7rin", "am", "Ho\u00b7fe", "ver\u00b7sam\u00b7mel\u00b7te", "zum", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "APPRART", "NN", "VVFIN", "APPRART", "NN"], "meter": "--+--+---+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der Herzog Karl der Dritte gar viele edle G\u00e4ste;", "tokens": ["Der", "Her\u00b7zog", "Karl", "der", "Drit\u00b7te", "gar", "vie\u00b7le", "ed\u00b7le", "G\u00e4s\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "ART", "NN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Man trank in vollen Z\u00fcgen den rothen Veltlinwein", "tokens": ["Man", "trank", "in", "vol\u00b7len", "Z\u00fc\u00b7gen", "den", "ro\u00b7then", "Velt\u00b7lin\u00b7wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und rings im Kreise scholl es von lust'gen Neckerei'n.", "tokens": ["Und", "rings", "im", "Krei\u00b7se", "scholl", "es", "von", "lust'\u00b7gen", "Ne\u00b7cke\u00b7rei'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "NE"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Um eine Tafel sa\u00dfen sie alle in der Runde,", "tokens": ["Um", "ei\u00b7ne", "Ta\u00b7fel", "sa\u00b7\u00dfen", "sie", "al\u00b7le", "in", "der", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "PPER", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die sich bereits ergeben dem s\u00fc\u00dfen Ehebunde;", "tokens": ["Die", "sich", "be\u00b7reits", "er\u00b7ge\u00b7ben", "dem", "s\u00fc\u00b7\u00dfen", "E\u00b7heb\u00b7un\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Um eine andre Alles, was jung und ledig war \u2013", "tokens": ["Um", "ei\u00b7ne", "and\u00b7re", "Al\u00b7les", ",", "was", "jung", "und", "le\u00b7dig", "war", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "PIS", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAFIN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein Blumenbeet zu schauen ist nicht so frisch und klar.", "tokens": ["Ein", "Blu\u00b7men\u00b7beet", "zu", "schau\u00b7en", "ist", "nicht", "so", "frisch", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VAFIN", "PTKNEG", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Her\u00fcber und hin\u00fcber stritt man mit keckem Sinnen,", "tokens": ["Her\u00b7\u00fc\u00b7ber", "und", "hin\u00b7\u00fc\u00b7ber", "stritt", "man", "mit", "ke\u00b7ckem", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VVFIN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+---+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ob's besser sei zu freien, ob besser nur zu minnen;", "tokens": ["Ob's", "bes\u00b7ser", "sei", "zu", "frei\u00b7en", ",", "ob", "bes\u00b7ser", "nur", "zu", "min\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "APPR", "ADJA", "$,", "KOUS", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es priesen Frau'n und M\u00e4nner den sch\u00f6nen Ehestand,", "tokens": ["Es", "prie\u00b7sen", "Frau'n", "und", "M\u00e4n\u00b7ner", "den", "sch\u00f6\u00b7nen", "E\u00b7hes\u00b7tand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn's auch vielleicht im Herzen ein Mancher anders fand.", "tokens": ["Wenn's", "auch", "viel\u00b7leicht", "im", "Her\u00b7zen", "ein", "Man\u00b7cher", "an\u00b7ders", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "APPRART", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Herr Corsant de la Bresse, der jungen Ritter Blume,", "tokens": ["Herr", "Cor\u00b7sant", "de", "la", "Bres\u00b7se", ",", "der", "jun\u00b7gen", "Rit\u00b7ter", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Erhob das Glas und sprach: \u00bbEs gilt der Freiheit Ruhme!", "tokens": ["Er\u00b7hob", "das", "Glas", "und", "sprach", ":", "\u00bb", "Es", "gilt", "der", "Frei\u00b7heit", "Ruh\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Freiheit von dem s\u00fc\u00dfen, doch l\u00e4st'gen Ehejoch,", "tokens": ["Der", "Frei\u00b7heit", "von", "dem", "s\u00fc\u00b7\u00dfen", ",", "doch", "l\u00e4st'\u00b7gen", "E\u00b7he\u00b7joch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sind's auch nur Rosenketten, so bleiben's Ketten doch!", "tokens": ["Sin\u00b7d's", "auch", "nur", "Ro\u00b7sen\u00b7ket\u00b7ten", ",", "so", "blei\u00b7ben's", "Ket\u00b7ten", "doch", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "NN", "$,", "ADV", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbwir weihen drum den Damen nicht minder hei\u00dfe Triebe,", "tokens": ["\u00bb", "wir", "wei\u00b7hen", "drum", "den", "Da\u00b7men", "nicht", "min\u00b7der", "hei\u00b7\u00dfe", "Trie\u00b7be", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PAV", "ART", "NN", "PTKNEG", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch nur vereint mit Freiheit kann preisen ich die Liebe!\u00ab", "tokens": ["Doch", "nur", "ver\u00b7eint", "mit", "Frei\u00b7heit", "kann", "prei\u00b7sen", "ich", "die", "Lie\u00b7be", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVPP", "APPR", "NN", "VMFIN", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die jungen Ritter riefen es ihm mit Jubel nach,", "tokens": ["Die", "jun\u00b7gen", "Rit\u00b7ter", "rie\u00b7fen", "es", "ihm", "mit", "Ju\u00b7bel", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Es stie\u00dfen an die Damen, jedoch ein wenig schwach.", "tokens": ["Es", "stie\u00b7\u00dfen", "an", "die", "Da\u00b7men", ",", "je\u00b7doch", "ein", "we\u00b7nig", "schwach", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "ADV", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Herr Simon von Blonay sprang auf mit edler Hitze,", "tokens": ["Herr", "Si\u00b7mon", "von", "Blo\u00b7nay", "sprang", "auf", "mit", "ed\u00b7ler", "Hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "NE", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Begl\u00fcckt seit einem Jahre f\u00fchlt er sich im Besitze", "tokens": ["Be\u00b7gl\u00fcckt", "seit", "ei\u00b7nem", "Jah\u00b7re", "f\u00fchlt", "er", "sich", "im", "Be\u00b7sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVFIN", "PPER", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Der sch\u00f6nsten Dame, die er so warm in Lieb' umfing,", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "Da\u00b7me", ",", "die", "er", "so", "warm", "in", "Lieb'", "um\u00b7fing", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df ihm die kecke Sprache tief durch die Seele ging.", "tokens": ["Da\u00df", "ihm", "die", "ke\u00b7cke", "Spra\u00b7che", "tief", "durch", "die", "See\u00b7le", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbdies Glas bring' ich der Ehe!\u00ab scholl seine Gegenrede,", "tokens": ["\u00bb", "dies", "Glas", "bring'", "ich", "der", "E\u00b7he", "!", "\u00ab", "scholl", "sei\u00b7ne", "Ge\u00b7gen\u00b7re\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "NN", "VVFIN", "PPER", "ART", "NN", "$.", "$(", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-++-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "\u00bbund fordre einen jeden mit mir heraus zur Fehde,", "tokens": ["\u00bb", "und", "ford\u00b7re", "ei\u00b7nen", "je\u00b7den", "mit", "mir", "he\u00b7raus", "zur", "Feh\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "ART", "PIAT", "APPR", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der es bezweifeln m\u00f6chte, ein Ehegatte sei,", "tokens": ["Der", "es", "be\u00b7zwei\u00b7feln", "m\u00f6ch\u00b7te", ",", "ein", "E\u00b7he\u00b7gat\u00b7te", "sei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "$,", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Weil er ein Weib genommen, nun minder stark und frei!", "tokens": ["Weil", "er", "ein", "Weib", "ge\u00b7nom\u00b7men", ",", "nun", "min\u00b7der", "stark", "und", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Und w\u00fcrdiger als Jungfrau'n sind diese wohl zu preisen,", "tokens": ["Und", "w\u00fcr\u00b7di\u00b7ger", "als", "Jung\u00b7frau'n", "sind", "die\u00b7se", "wohl", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "NN", "VAFIN", "PDS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Gattin wir und Mutter von unsern Kindern hei\u00dfen,", "tokens": ["Die", "Gat\u00b7tin", "wir", "und", "Mut\u00b7ter", "von", "un\u00b7sern", "Kin\u00b7dern", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "F\u00fcr deren Ruhm und Ansehn bin ich hier allezeit", "tokens": ["F\u00fcr", "de\u00b7ren", "Ruhm", "und", "An\u00b7sehn", "bin", "ich", "hier", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "KON", "NN", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Zu brechen eine Lanze mit jedem gern bereit!\u00ab", "tokens": ["Zu", "bre\u00b7chen", "ei\u00b7ne", "Lan\u00b7ze", "mit", "je\u00b7dem", "gern", "be\u00b7reit", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "APPR", "PIS", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Da klatschten alle M\u00e4nner, da nickten alle Frauen,", "tokens": ["Da", "klatschten", "al\u00b7le", "M\u00e4n\u00b7ner", ",", "da", "nick\u00b7ten", "al\u00b7le", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "KOUS", "PIS", "PIAT", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es war der Herr von Blonay gar herrlich anzuschauen;", "tokens": ["Es", "war", "der", "Herr", "von", "Blo\u00b7nay", "gar", "herr\u00b7lich", "an\u00b7zu\u00b7schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NE", "ADV", "ADJD", "VVIZU", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er hielt die Hand am Schwerte, sein Auge blitzte voll,", "tokens": ["Er", "hielt", "die", "Hand", "am", "Schwer\u00b7te", ",", "sein", "Au\u00b7ge", "blitz\u00b7te", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein sieggewohntes L\u00e4cheln um seine Lippen quoll.", "tokens": ["Ein", "sieg\u00b7ge\u00b7wohn\u00b7tes", "L\u00e4\u00b7cheln", "um", "sei\u00b7ne", "Lip\u00b7pen", "quoll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Im Kreis der jungen Ritter stand Corsant ganz so pr\u00e4chtig:", "tokens": ["Im", "Kreis", "der", "jun\u00b7gen", "Rit\u00b7ter", "stand", "Cor\u00b7sant", "ganz", "so", "pr\u00e4ch\u00b7tig", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "NE", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+---+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u00bbherr Simon von Blonay,\u00ab so rief er jetzo m\u00e4chtig,", "tokens": ["\u00bb", "herr", "Si\u00b7mon", "von", "Blo\u00b7nay", ",", "\u00ab", "so", "rief", "er", "jet\u00b7zo", "m\u00e4ch\u00b7tig", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NE", "APPR", "NE", "$,", "$(", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "\u00bbwir wollen Lanzen brechen mit Euch zu jeder Stund \u2013", "tokens": ["\u00bb", "wir", "wol\u00b7len", "Lan\u00b7zen", "bre\u00b7chen", "mit", "Euch", "zu", "je\u00b7der", "Stund", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "NN", "VVFIN", "APPR", "PPER", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ich f\u00fcr den Preis der Freiheit, Ihr f\u00fcr den Ehebund!\u00ab", "tokens": ["Ich", "f\u00fcr", "den", "Preis", "der", "Frei\u00b7heit", ",", "Ihr", "f\u00fcr", "den", "E\u00b7he\u00b7bund", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "$,", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Da schlugen in die H\u00e4nde die jungen Ritter wieder,", "tokens": ["Da", "schlu\u00b7gen", "in", "die", "H\u00e4n\u00b7de", "die", "jun\u00b7gen", "Rit\u00b7ter", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Allein die jungen Damen, sie blickten schweigend nieder,", "tokens": ["Al\u00b7lein", "die", "jun\u00b7gen", "Da\u00b7men", ",", "sie", "blick\u00b7ten", "schwei\u00b7gend", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Von ihnen h\u00e4tte jede es lieber wohl geseh'n,", "tokens": ["Von", "ih\u00b7nen", "h\u00e4t\u00b7te", "je\u00b7de", "es", "lie\u00b7ber", "wohl", "ge\u00b7seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIAT", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er m\u00f6cht' mit ihr zur Kirche, statt zu dem Kampfe geh'n.", "tokens": ["Er", "m\u00f6cht'", "mit", "ihr", "zur", "Kir\u00b7che", ",", "statt", "zu", "dem", "Kamp\u00b7fe", "geh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "APPRART", "NN", "$,", "KOUI", "APPR", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "Der Herzog hatte stille dem lust'gen Streit gelauschet,", "tokens": ["Der", "Her\u00b7zog", "hat\u00b7te", "stil\u00b7le", "dem", "lust'\u00b7gen", "Streit", "ge\u00b7lau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und sah, da\u00df nur zur Kurzweil man solche Fehde tauschet.", "tokens": ["Und", "sah", ",", "da\u00df", "nur", "zur", "Kurz\u00b7weil", "man", "sol\u00b7che", "Feh\u00b7de", "tau\u00b7schet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ADV", "APPRART", "NN", "PIS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So mocht' er wohl erlauben, da\u00df ohne Ha\u00df und Groll", "tokens": ["So", "mocht'", "er", "wohl", "er\u00b7lau\u00b7ben", ",", "da\u00df", "oh\u00b7ne", "Ha\u00df", "und", "Groll"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVINF", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Kampf der beiden Ritter sich bald vollziehen soll.", "tokens": ["Der", "Kampf", "der", "bei\u00b7den", "Rit\u00b7ter", "sich", "bald", "voll\u00b7zie\u00b7hen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PIAT", "NN", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "Er selber will bestimmen des muntern Kampfs Gesetze,", "tokens": ["Er", "sel\u00b7ber", "will", "be\u00b7stim\u00b7men", "des", "mun\u00b7tern", "Kampfs", "Ge\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "VVINF", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der ihm und seinem Hofe soll dienen zum Erg\u00f6tze \u2013", "tokens": ["Der", "ihm", "und", "sei\u00b7nem", "Ho\u00b7fe", "soll", "die\u00b7nen", "zum", "Er\u00b7g\u00f6t\u00b7ze", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KON", "PPOSAT", "NN", "VMFIN", "VVINF", "APPRART", "NN", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Zweimal mit stumpfer Lanze zu rennen Mann gen Mann", "tokens": ["Zwei\u00b7mal", "mit", "stump\u00b7fer", "Lan\u00b7ze", "zu", "ren\u00b7nen", "Mann", "gen", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "APPR", "NN"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Wird ihnen erst gestattet, und f\u00fcnfzehn G\u00e4nge dann", "tokens": ["Wird", "ih\u00b7nen", "erst", "ge\u00b7stat\u00b7tet", ",", "und", "f\u00fcnf\u00b7zehn", "G\u00e4n\u00b7ge", "dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,", "KON", "CARD", "NN", "ADV"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "Zu fechten mit dem Schwerte; wenn sich besiegt sollt' neigen", "tokens": ["Zu", "fech\u00b7ten", "mit", "dem", "Schwer\u00b7te", ";", "wenn", "sich", "be\u00b7siegt", "sollt'", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$.", "KOUS", "PRF", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Streiter f\u00fcr die Ehe, mu\u00df er die Kniee beugen", "tokens": ["Der", "Strei\u00b7ter", "f\u00fcr", "die", "E\u00b7he", ",", "mu\u00df", "er", "die", "Kni\u00b7ee", "beu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Vor'm Fr\u00e4ulein von Savoyen und ihre Gnad' erfleh'n,", "tokens": ["Vor'm", "Fr\u00e4u\u00b7lein", "von", "Sa\u00b7vo\u00b7yen", "und", "ih\u00b7re", "Gnad'", "er\u00b7fleh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dieweil sie noch bis jetzo lie\u00df alle Freier steh'n,", "tokens": ["Die\u00b7weil", "sie", "noch", "bis", "jet\u00b7zo", "lie\u00df", "al\u00b7le", "Frei\u00b7er", "steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "VVFIN", "PIAT", "NN", "VVFIN", "$,"], "meter": "----+-+-+-+-+", "measure": "unknown.measure.penta"}}, "stanza.15": {"line.1": {"text": "Und vor noch einer Jungfrau, die darf der Sieger nennen,", "tokens": ["Und", "vor", "noch", "ei\u00b7ner", "Jung\u00b7frau", ",", "die", "darf", "der", "Sie\u00b7ger", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ART", "NN", "$,", "PRELS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hat er als \u00fcberwunden im Staub sich zu bekennen \u2013", "tokens": ["Hat", "er", "als", "\u00fc\u00b7berw\u00b7un\u00b7den", "im", "Staub", "sich", "zu", "be\u00b7ken\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOUS", "ADJA", "APPRART", "NN", "PRF", "PTKZU", "VVINF", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch, wenn besiegt sollt' werden der led'ge junge Fant", "tokens": ["Doch", ",", "wenn", "be\u00b7siegt", "sollt'", "wer\u00b7den", "der", "le\u00b7d'\u00b7ge", "jun\u00b7ge", "Fant"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "ADJD", "VMFIN", "VAFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Hier durch des Ehek\u00e4mpen geschickte, tapfre Hand,", "tokens": ["Hier", "durch", "des", "E\u00b7he\u00b7k\u00e4m\u00b7pen", "ge\u00b7schick\u00b7te", ",", "tapf\u00b7re", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.16": {"line.1": {"text": "Mu\u00df er, o harte Strafe! erst reuig niederfallen", "tokens": ["Mu\u00df", "er", ",", "o", "har\u00b7te", "Stra\u00b7fe", "!", "erst", "reu\u00b7ig", "nie\u00b7der\u00b7fal\u00b7len"], "to