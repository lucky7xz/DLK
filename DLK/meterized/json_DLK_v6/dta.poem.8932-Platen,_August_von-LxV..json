{"dta.poem.8932": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "LxV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Gedenk' ich still vergangner Mi\u00dfgeschicke:", "tokens": ["Ge\u00b7denk'", "ich", "still", "ver\u00b7gang\u00b7ner", "Mi\u00df\u00b7ge\u00b7schi\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur\u00fcck nach Deutschland wend' ich kaum die Blicke,", "tokens": ["Zu\u00b7r\u00fcck", "nach", "Deutschland", "wend'", "ich", "kaum", "die", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Ja, kaum noch vorw\u00e4rts nach Italiens Gr\u00e4nzen.", "tokens": ["Ja", ",", "kaum", "noch", "vor\u00b7w\u00e4rts", "nach", "I\u00b7ta\u00b7li\u00b7ens", "Gr\u00e4n\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ADV", "ADV", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Vergebens hasch' ich nach getr\u00e4umten Kr\u00e4nzen,", "tokens": ["Ver\u00b7ge\u00b7bens", "hasch'", "ich", "nach", "ge\u00b7tr\u00e4um\u00b7ten", "Kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ich die Stirne, die mir brennt, erquicke,", "tokens": ["Da\u00df", "ich", "die", "Stir\u00b7ne", ",", "die", "mir", "brennt", ",", "er\u00b7qui\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Seufzer wehn, die selten ich ersticke,", "tokens": ["Und", "Seuf\u00b7zer", "wehn", ",", "die", "sel\u00b7ten", "ich", "er\u00b7sti\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,", "PRELS", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als k\u00f6nnten Seufzer das Gem\u00fcth erg\u00e4nzen!", "tokens": ["Als", "k\u00f6nn\u00b7ten", "Seuf\u00b7zer", "das", "Ge\u00b7m\u00fcth", "er\u00b7g\u00e4n\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wo ist ein Herz, das keine Schmerzen spalten?", "tokens": ["Wo", "ist", "ein", "Herz", ",", "das", "kei\u00b7ne", "Schmer\u00b7zen", "spal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wer an's Weltenende fl\u00fcchten w\u00fcrde,", "tokens": ["Und", "wer", "an's", "Wel\u00b7te\u00b7nen\u00b7de", "fl\u00fcch\u00b7ten", "w\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stets folgten ihm des Lebens Truggestalten.", "tokens": ["Stets", "folg\u00b7ten", "ihm", "des", "Le\u00b7bens", "Trug\u00b7ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Trost nur bleibt mir, da\u00df ich jeder B\u00fcrde", "tokens": ["Ein", "Trost", "nur", "bleibt", "mir", ",", "da\u00df", "ich", "je\u00b7der", "B\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vielleicht ein Gleichgewicht vermag zu halten", "tokens": ["Viel\u00b7leicht", "ein", "Gleich\u00b7ge\u00b7wicht", "ver\u00b7mag", "zu", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch meiner Seele ganze Kraft und W\u00fcrde.", "tokens": ["Durch", "mei\u00b7ner", "See\u00b7le", "gan\u00b7ze", "Kraft", "und", "W\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}