{"textgrid.poem.41033": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der Seherin Erscheinen", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort droben im Gebirge, wo rauh der Nordwind weht,", "tokens": ["Dort", "dro\u00b7ben", "im", "Ge\u00b7bir\u00b7ge", ",", "wo", "rauh", "der", "Nord\u00b7wind", "weht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,", "PWAV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von reinem Schnee bedecket, ihr stiller H\u00fcgel steht.", "tokens": ["Von", "rei\u00b7nem", "Schnee", "be\u00b7de\u00b7cket", ",", "ihr", "stil\u00b7ler", "H\u00fc\u00b7gel", "steht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "In \u00fcpp'ger Kr\u00e4uterf\u00fclle, bei warmem Sonnenschein,", "tokens": ["In", "\u00fcpp'\u00b7ger", "Kr\u00e4u\u00b7ter\u00b7f\u00fcl\u00b7le", ",", "bei", "war\u00b7mem", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da legten sie die H\u00fclle, die leichte, leicht hinein.", "tokens": ["Da", "leg\u00b7ten", "sie", "die", "H\u00fcl\u00b7le", ",", "die", "leich\u00b7te", ",", "leicht", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Da sang ich ihrem Sterben ein Lied aus tiefer Brust,", "tokens": ["Da", "sang", "ich", "ih\u00b7rem", "Ster\u00b7ben", "ein", "Lied", "aus", "tie\u00b7fer", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Da gab ich, ach! ihr Leben \u2013 weh! in des Marktes Wust!", "tokens": ["Da", "gab", "ich", ",", "ach", "!", "ihr", "Le\u00b7ben", "\u2013", "weh", "!", "in", "des", "Mark\u00b7tes", "Wust", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ITJ", "$.", "PPOSAT", "NN", "$(", "PTKVZ", "$.", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Die Nachtigallen schweigen, die Lerche schl\u00e4ft im Tal,", "tokens": ["Die", "Nach\u00b7ti\u00b7gal\u00b7len", "schwei\u00b7gen", ",", "die", "Ler\u00b7che", "schl\u00e4ft", "im", "Tal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Die Blumen sind erstorben, kalt blickt der Sonne Strahl.", "tokens": ["Die", "Blu\u00b7men", "sind", "ers\u00b7tor\u00b7ben", ",", "kalt", "blickt", "der", "Son\u00b7ne", "Strahl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Und jetzt auf ihrem H\u00fcgel stellt mancher Rab' sich ein,", "tokens": ["Und", "jetzt", "auf", "ih\u00b7rem", "H\u00fc\u00b7gel", "stellt", "man\u00b7cher", "Rab'", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Erhebt aus frost'ger Kehle auf ihm ein heiser Schrei'n.", "tokens": ["Er\u00b7hebt", "aus", "frost'\u00b7ger", "Keh\u00b7le", "auf", "ihm", "ein", "hei\u00b7ser", "Schrei'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "APPR", "PPER", "ART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Doch sieh! was schwebt dort nieder licht durch die d\u00fcstre Nacht?", "tokens": ["Doch", "sieh", "!", "was", "schwebt", "dort", "nie\u00b7der", "licht", "durch", "die", "d\u00fcst\u00b7re", "Nacht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PWS", "VVFIN", "ADV", "PTKVZ", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Du bist's! hat dich das Kr\u00e4chzen der Raben hergebracht?", "tokens": ["Du", "bist's", "!", "hat", "dich", "das", "Kr\u00e4ch\u00b7zen", "der", "Ra\u00b7ben", "her\u00b7ge\u00b7bracht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$.", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "\u00bbo Freund! der Menschen W\u00e4hnen, das st\u00f6ret nicht mein Licht;", "tokens": ["\u00bb", "o", "Freund", "!", "der", "Men\u00b7schen", "W\u00e4h\u00b7nen", ",", "das", "st\u00f6\u00b7ret", "nicht", "mein", "Licht", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$.", "ART", "NN", "NN", "$,", "PDS", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Dein Z\u00fcrnen und dein Gr\u00e4men, das l\u00e4\u00dft mich ruhen nicht.", "tokens": ["Dein", "Z\u00fcr\u00b7nen", "und", "dein", "Gr\u00e4\u00b7men", ",", "das", "l\u00e4\u00dft", "mich", "ru\u00b7hen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Ist nicht in dich gedrungen, was ich halb sterbend sprach", "tokens": ["Ist", "nicht", "in", "dich", "ge\u00b7drun\u00b7gen", ",", "was", "ich", "halb", "ster\u00b7bend", "sprach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "APPR", "PPER", "VVPP", "$,", "PWS", "PPER", "ADJD", "ADJD", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "An die, die mir im Leben zuf\u00fcgten Kreuz und Schmach?", "tokens": ["An", "die", ",", "die", "mir", "im", "Le\u00b7ben", "zu\u00b7f\u00fcg\u00b7ten", "Kreuz", "und", "Schmach", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "APPRART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.17": {"text": "'wie soll ich euch denn nennen, ihr, die ihr mich betr\u00fcbt,", "tokens": ["'wie", "soll", "ich", "euch", "denn", "nen\u00b7nen", ",", "ihr", ",", "die", "ihr", "mich", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,", "PPER", "$,", "PRELS", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+---+-+-+", "measure": "unknown.measure.hexa"}, "line.18": {"text": "Ich nenn' auch euch nur Freunde; ihr habt mich nur ge\u00fcbt.'", "tokens": ["Ich", "nenn'", "auch", "euch", "nur", "Freun\u00b7de", ";", "ihr", "habt", "mich", "nur", "ge\u00b7\u00fcbt", ".", "'"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "ADV", "NN", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-++---+-", "measure": "unknown.measure.hexa"}, "line.19": {"text": "Betr\u00fcbt mu\u00dft du auch werden, damit du wirst ge\u00fcbt;", "tokens": ["Be\u00b7tr\u00fcbt", "mu\u00dft", "du", "auch", "wer\u00b7den", ",", "da\u00b7mit", "du", "wirst", "ge\u00b7\u00fcbt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "VAINF", "$,", "KOUS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Wer hier nicht hat geduldet, der wird dort nicht geliebt.", "tokens": ["Wer", "hier", "nicht", "hat", "ge\u00b7dul\u00b7det", ",", "der", "wird", "dort", "nicht", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKNEG", "VAFIN", "VVPP", "$,", "PRELS", "VAFIN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.21": {"text": "Oft sagt' ich's ja hienieden, dein Glaube ist noch klein,", "tokens": ["Oft", "sagt'", "ich's", "ja", "hien\u00b7ie\u00b7den", ",", "dein", "Glau\u00b7be", "ist", "noch", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADV", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Lies oft im Buch der B\u00fccher und la\u00df die Menschen sein!\u00ab", "tokens": ["Lies", "oft", "im", "Buch", "der", "B\u00fc\u00b7cher", "und", "la\u00df", "die", "Men\u00b7schen", "sein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "ART", "NN", "KON", "VVIMP", "ART", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}