{"textgrid.poem.42118": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Tote Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was mir erwarb", "tokens": ["Was", "mir", "er\u00b7warb"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ihr s\u00fc\u00dfes Licht", "tokens": ["Ihr", "s\u00fc\u00b7\u00dfes", "Licht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Was ihr verdarb", "tokens": ["Was", "ihr", "ver\u00b7darb"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Mein Angesicht", "tokens": ["Mein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Warum sie starb", "tokens": ["Wa\u00b7rum", "sie", "starb"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ich wei\u00df es nicht.", "tokens": ["Ich", "wei\u00df", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die M\u00e4rchenbraut", "tokens": ["Die", "M\u00e4r\u00b7chen\u00b7braut"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Lag so im Tod", "tokens": ["Lag", "so", "im", "Tod"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dem Blick vertraut;", "tokens": ["Dem", "Blick", "ver\u00b7traut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Der Wange Rot", "tokens": ["Der", "Wan\u00b7ge", "Rot"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wer es geschaut", "tokens": ["Wer", "es", "ge\u00b7schaut"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PPER", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Fiel neu in Not.", "tokens": ["Fiel", "neu", "in", "Not", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Als h\u00fcbe sie", "tokens": ["Als", "h\u00fc\u00b7be", "sie"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die er gewann", "tokens": ["Die", "er", "ge\u00b7wann"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die wie der Fr\u00fch-", "tokens": ["Die", "wie", "der", "Fr\u00fch"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "KOKOM", "ART", "TRUNC"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Tau ihm zerrann", "tokens": ["Tau", "ihm", "zer\u00b7rann"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Als h\u00fcbe sie", "tokens": ["Als", "h\u00fc\u00b7be", "sie"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Zu sprechen an:", "tokens": ["Zu", "spre\u00b7chen", "an", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Was dich mir warb", "tokens": ["Was", "dich", "mir", "warb"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Damals im Licht", "tokens": ["Da\u00b7mals", "im", "Licht"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Was mich verdarb", "tokens": ["Was", "mich", "ver\u00b7darb"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "F\u00fcr dein Gesicht", "tokens": ["F\u00fcr", "dein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Warum ich starb", "tokens": ["Wa\u00b7rum", "ich", "starb"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ich wei\u00df es nicht.", "tokens": ["Ich", "wei\u00df", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Wir wissen beid", "tokens": ["Wir", "wis\u00b7sen", "beid"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Nicht wies geschah", "tokens": ["Nicht", "wies", "ge\u00b7schah"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wir sind im Leid", "tokens": ["Wir", "sind", "im", "Leid"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Uns nun ganz nah", "tokens": ["Uns", "nun", "ganz", "nah"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "An deine Seit", "tokens": ["An", "dei\u00b7ne", "Seit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Sehnt ich mich ja.", "tokens": ["Sehnt", "ich", "mich", "ja", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Reiche mir Lieber", "tokens": ["Rei\u00b7che", "mir", "Lie\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["NE", "PPER", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Noch deine Hand.", "tokens": ["Noch", "dei\u00b7ne", "Hand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ist sie im Fieber", "tokens": ["Ist", "sie", "im", "Fie\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wie ich sie fand", "tokens": ["Wie", "ich", "sie", "fand"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Als sie hin\u00fcber", "tokens": ["Als", "sie", "hin\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Gab mir den Brand?", "tokens": ["Gab", "mir", "den", "Brand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}