{"textgrid.poem.55911": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nig, h\u00f6rst du, wie mein Saitenspiel", "tokens": ["K\u00f6\u00b7nig", ",", "h\u00f6rst", "du", ",", "wie", "mein", "Sai\u00b7ten\u00b7spiel"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "KOKOM", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Fernen wirft, durch die wir uns bewegen:", "tokens": ["Fer\u00b7nen", "wirft", ",", "durch", "die", "wir", "uns", "be\u00b7we\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sterne treiben uns verwirrt entgegen,", "tokens": ["Ster\u00b7ne", "trei\u00b7ben", "uns", "ver\u00b7wirrt", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und wir fallen endlich wie ein Regen,", "tokens": ["und", "wir", "fal\u00b7len", "end\u00b7lich", "wie", "ein", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "und es bl\u00fcht, wo dieser Regen fiel.", "tokens": ["und", "es", "bl\u00fcht", ",", "wo", "die\u00b7ser", "Re\u00b7gen", "fiel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "M\u00e4dchen bl\u00fchen, die du noch erkannt,", "tokens": ["M\u00e4d\u00b7chen", "bl\u00fc\u00b7hen", ",", "die", "du", "noch", "er\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "die jetzt Frauen sind und mich verf\u00fchren;", "tokens": ["die", "jetzt", "Frau\u00b7en", "sind", "und", "mich", "ver\u00b7f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VAFIN", "KON", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "den Geruch der Jungfraun kannst du sp\u00fcren,", "tokens": ["den", "Ge\u00b7ruch", "der", "Jung\u00b7fraun", "kannst", "du", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und die Knaben stehen, angespannt", "tokens": ["und", "die", "Kna\u00b7ben", "ste\u00b7hen", ",", "an\u00b7ge\u00b7spannt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "schlank und atmend, an verschwiegnen T\u00fcren.", "tokens": ["schlank", "und", "at\u00b7mend", ",", "an", "ver\u00b7schwieg\u00b7nen", "T\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Da\u00df mein Klang dir alles wiederbr\u00e4chte.", "tokens": ["Da\u00df", "mein", "Klang", "dir", "al\u00b7les", "wie\u00b7der\u00b7br\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aber trunken taumelt mein Get\u00f6n:", "tokens": ["A\u00b7ber", "trun\u00b7ken", "tau\u00b7melt", "mein", "Ge\u00b7t\u00f6n", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Deine N\u00e4chte, K\u00f6nig, deine N\u00e4chte \u2013,", "tokens": ["Dei\u00b7ne", "N\u00e4ch\u00b7te", ",", "K\u00f6\u00b7nig", ",", "dei\u00b7ne", "N\u00e4ch\u00b7te", "\u2013", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "PPOSAT", "NN", "$(", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und wie waren, die dein Schaffen schw\u00e4chte,", "tokens": ["und", "wie", "wa\u00b7ren", ",", "die", "dein", "Schaf\u00b7fen", "schw\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "o wie waren alle Leiber sch\u00f6n.", "tokens": ["o", "wie", "wa\u00b7ren", "al\u00b7le", "Lei\u00b7ber", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "KOKOM", "VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Dein Erinnern glaub ich zu begleiten,", "tokens": ["Dein", "E\u00b7rin\u00b7nern", "glaub", "ich", "zu", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "weil ich ahne. Doch auf welchen Saiten", "tokens": ["weil", "ich", "ah\u00b7ne", ".", "Doch", "auf", "wel\u00b7chen", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "KON", "APPR", "PWAT", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "greif ich dir ihr dunkles Lustgest\u00f6hn? \u2013", "tokens": ["greif", "ich", "dir", "ihr", "dunk\u00b7les", "Lust\u00b7ge\u00b7st\u00f6hn", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}