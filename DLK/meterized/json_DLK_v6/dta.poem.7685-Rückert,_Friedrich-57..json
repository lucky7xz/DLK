{"dta.poem.7685": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht wachsen siehest du, wie aufmerksam du bist,", "tokens": ["Nicht", "wach\u00b7sen", "sie\u00b7hest", "du", ",", "wie", "auf\u00b7merk\u00b7sam", "du", "bist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VVFIN", "PPER", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Gras, doch merkst du bald, da\u00df es gewachsen ist.", "tokens": ["Das", "Gras", ",", "doch", "merkst", "du", "bald", ",", "da\u00df", "es", "ge\u00b7wach\u00b7sen", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So tr\u00f6ste dich, wo gleich nicht das Gedeihn erschien", "tokens": ["So", "tr\u00f6s\u00b7te", "dich", ",", "wo", "gleich", "nicht", "das", "Ge\u00b7deihn", "er\u00b7schien"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ADV", "PTKNEG", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von jedem Werk, zuletzt auf einmal ists gediehn.", "tokens": ["Von", "je\u00b7dem", "Werk", ",", "zu\u00b7letzt", "auf", "ein\u00b7mal", "ists", "ge\u00b7diehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT