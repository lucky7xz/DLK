{"textgrid.poem.49215": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "16. Beschlu\u00df-Elegie", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das blinde Liebeswerck, die s\u00fcsse Gisst der Sinnen", "tokens": ["Das", "blin\u00b7de", "Lie\u00b7bes\u00b7werck", ",", "die", "s\u00fcs\u00b7se", "Gisst", "der", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und rechte Zauberey hat letzlich hier ein End';", "tokens": ["Und", "rech\u00b7te", "Zau\u00b7be\u00b7rey", "hat", "letz\u00b7lich", "hier", "ein", "End'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es wird das lose Kind, so mich verf\u00fchren k\u00f6nnen,", "tokens": ["Es", "wird", "das", "lo\u00b7se", "Kind", ",", "so", "mich", "ver\u00b7f\u00fch\u00b7ren", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ADV", "PPER", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gott Lob, jetzt gantz und gar von mir hinweg gewendt.", "tokens": ["Gott", "Lob", ",", "jetzt", "gantz", "und", "gar", "von", "mir", "hin\u00b7weg", "ge\u00b7wendt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "ADV", "KON", "ADV", "APPR", "PPER", "APZR", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun suche, wo du wilt, dir anderwerts Poeten;", "tokens": ["Nun", "su\u00b7che", ",", "wo", "du", "wilt", ",", "dir", "an\u00b7der\u00b7werts", "Po\u00b7et\u00b7en", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$,", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hier, Venus, hab' ich mir gesteckt mein eignes Ziel;", "tokens": ["Hier", ",", "Ve\u00b7nus", ",", "hab'", "ich", "mir", "ge\u00b7steckt", "mein", "eig\u00b7nes", "Ziel", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VAFIN", "PPER", "PPER", "VVPP", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es ist mir deine Gunst jetzt weiter nicht von N\u00f6then;", "tokens": ["Es", "ist", "mir", "dei\u00b7ne", "Gunst", "jetzt", "wei\u00b7ter", "nicht", "von", "N\u00f6\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ADV", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich ha\u00df' all' Eitelkeit, es liebe, wer da wil.", "tokens": ["Ich", "ha\u00df'", "all'", "Ei\u00b7tel\u00b7keit", ",", "es", "lie\u00b7be", ",", "wer", "da", "wil", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PPER", "VVFIN", "$,", "PWS", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was meine schwache Hand vor dieser Zeit geschrieben,", "tokens": ["Was", "mei\u00b7ne", "schwa\u00b7che", "Hand", "vor", "die\u00b7ser", "Zeit", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Durch deinen Geist gef\u00fchrt, das ist der Jugend Schuld.", "tokens": ["Durch", "dei\u00b7nen", "Geist", "ge\u00b7f\u00fchrt", ",", "das", "ist", "der", "Ju\u00b7gend", "Schuld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich werde weiter nicht von solcher Lust getrieben;", "tokens": ["Ich", "wer\u00b7de", "wei\u00b7ter", "nicht", "von", "sol\u00b7cher", "Lust", "ge\u00b7trie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was dir geh\u00e4ssig ist, zu diesem trag' ich Huld.", "tokens": ["Was", "dir", "ge\u00b7h\u00e4s\u00b7sig", "ist", ",", "zu", "die\u00b7sem", "trag'", "ich", "Huld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VAFIN", "$,", "APPR", "PDAT", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wann Urtheil und Verstand bey mir zu Rathe sitzen,", "tokens": ["Wann", "Ur\u00b7theil", "und", "Ver\u00b7stand", "bey", "mir", "zu", "Ra\u00b7the", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPR", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So hattest du mir zwar beth\u00f6rt den jungen Sinn,", "tokens": ["So", "hat\u00b7test", "du", "mir", "zwar", "be\u00b7th\u00f6rt", "den", "jun\u00b7gen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Jetzt seh' ich, da\u00df dein Sohn sey ohne Wahn und Witzen,", "tokens": ["Jetzt", "seh'", "ich", ",", "da\u00df", "dein", "Sohn", "sey", "oh\u00b7ne", "Wahn", "und", "Wit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Du aber, Venus, selbst ein' edle Kuplerinn.", "tokens": ["Du", "a\u00b7ber", ",", "Ve\u00b7nus", ",", "selbst", "ein'", "ed\u00b7le", "Kup\u00b7le\u00b7rinn", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dein Wesen ist ein Marckt, da Leid wird feil getragen,", "tokens": ["Dein", "We\u00b7sen", "ist", "ein", "Marckt", ",", "da", "Leid", "wird", "feil", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "KOUS", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ein Winckel, da Verdru\u00df und Wehmuth innen steht,", "tokens": ["Ein", "Win\u00b7ckel", ",", "da", "Ver\u00b7dru\u00df", "und", "Weh\u00b7muth", "in\u00b7nen", "steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "NN", "KON", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein' Herberg' aller Noth, ein Siechhau\u00df vieler Plagen,", "tokens": ["Ein'", "Her\u00b7ber\u00b7g'", "al\u00b7ler", "Noth", ",", "ein", "Siech\u00b7hau\u00df", "vie\u00b7ler", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Ein Schiff der Pein, ein Meer, da Tugend untergeht.", "tokens": ["Ein", "Schiff", "der", "Pein", ",", "ein", "Meer", ",", "da", "Tu\u00b7gend", "un\u00b7ter\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wo soll die Sch\u00f6nheit seyn, wann alles wird vergehen,", "tokens": ["Wo", "soll", "die", "Sch\u00f6n\u00b7heit", "seyn", ",", "wann", "al\u00b7les", "wird", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "VAINF", "$,", "PWAV", "PIS", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Lippen von Corall, di\u00df Alabaster-Bild,", "tokens": ["Die", "Lip\u00b7pen", "von", "Co\u00b7rall", ",", "di\u00df", "A\u00b7la\u00b7bas\u00b7ter\u00b7Bild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,", "PDS", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Augen, so ihr seht gleich als zwo Sonnen stehen,", "tokens": ["Die", "Au\u00b7gen", ",", "so", "ihr", "seht", "gleich", "als", "zwo", "Son\u00b7nen", "ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "VVFIN", "ADV", "KOKOM", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der rothe Rosenmund, der weissen Br\u00fcste Schild?", "tokens": ["Der", "ro\u00b7the", "Ro\u00b7sen\u00b7mund", ",", "der", "weis\u00b7sen", "Br\u00fcs\u00b7te", "Schild", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Sie sollen, und wir auch, als Asch' und Staub entfliehen", "tokens": ["Sie", "sol\u00b7len", ",", "und", "wir", "auch", ",", "als", "Asch'", "und", "Staub", "ent\u00b7flie\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "KON", "PPER", "ADV", "$,", "KOUS", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und allzugleiche gehn den Weg der Eitelkeit:", "tokens": ["Und", "all\u00b7zu\u00b7glei\u00b7che", "gehn", "den", "Weg", "der", "Ei\u00b7tel\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Pracht, Hoffart, Gut und Geld, umb das wir uns so m\u00fchen,", "tokens": ["Pracht", ",", "Hof\u00b7fart", ",", "Gut", "und", "Geld", ",", "umb", "das", "wir", "uns", "so", "m\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "KON", "NN", "$,", "KOUI", "ART", "PPER", "PRF", "ADV", "ADJA", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.28": {"text": "Wird Wind und Fl\u00fcgel noch bekommen mit der Zeit.", "tokens": ["Wird", "Wind", "und", "Fl\u00fc\u00b7gel", "noch", "be\u00b7kom\u00b7men", "mit", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADV", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich la\u00df' es alles stehn; das Ende meiner Jugend", "tokens": ["Ich", "la\u00df'", "es", "al\u00b7les", "stehn", ";", "das", "En\u00b7de", "mei\u00b7ner", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "VVINF", "$.", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und Frucht der Liebeslust beschlie\u00df' ich gantz hierein:", "tokens": ["Und", "Frucht", "der", "Lie\u00b7bes\u00b7lust", "be\u00b7schlie\u00df'", "ich", "gantz", "hier\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ein Werck, das h\u00f6her ist, der Anfang meiner Tugend,", "tokens": ["Ein", "Werck", ",", "das", "h\u00f6\u00b7her", "ist", ",", "der", "An\u00b7fang", "mei\u00b7ner", "Tu\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ob dieses gleich verdirbt, soll nimmer sterblich seyn.", "tokens": ["Ob", "die\u00b7ses", "gleich", "ver\u00b7dirbt", ",", "soll", "nim\u00b7mer", "sterb\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADV", "VVFIN", "$,", "VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}