{"dta.poem.18428": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der Gro\u00df-Sultan.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was r\u00fcstest du dich noch? Schau Salanckemen an/", "tokens": ["Was", "r\u00fcs\u00b7test", "du", "dich", "noch", "?", "Schau", "Sa\u00b7lan\u00b7cke\u00b7men", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "$.", "NN", "NN", "PTKVZ", "$("], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dort lieget/ der dein blut vermischt mit fremder erden;", "tokens": ["Dort", "lie\u00b7get", "/", "der", "dein", "blut", "ver\u00b7mischt", "mit", "frem\u00b7der", "er\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ist dir etwan hier noch nicht genung gethan/", "tokens": ["Und", "ist", "dir", "et\u00b7wan", "hier", "noch", "nicht", "ge\u00b7nung", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADV", "PTKNEG", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So kan dir auch Bysantz ein Salanckemen werden.", "tokens": ["So", "kan", "dir", "auch", "By\u00b7santz", "ein", "Sa\u00b7lan\u00b7cke\u00b7men", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "NE", "ART", "NN", "VAINF", "$."], "meter": "-+--++-++--+-", "measure": "iambic.hexa.relaxed"}}}}}