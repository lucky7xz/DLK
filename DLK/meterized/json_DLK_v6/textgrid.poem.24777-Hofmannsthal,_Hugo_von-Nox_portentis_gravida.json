{"textgrid.poem.24777": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Nox portentis gravida", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In hohen B\u00e4umen ist ein Nebelspiel,", "tokens": ["In", "ho\u00b7hen", "B\u00e4u\u00b7men", "ist", "ein", "Ne\u00b7bel\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und drei der sch\u00f6nen Sterne funkeln nah:", "tokens": ["Und", "drei", "der", "sch\u00f6\u00b7nen", "Ster\u00b7ne", "fun\u00b7keln", "nah", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "ART", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Hyazinthen an der dunkeln Erde", "tokens": ["Die", "Hy\u00b7a\u00b7zin\u00b7then", "an", "der", "dun\u00b7keln", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erinnern sich, da\u00df hier geschehen werde,", "tokens": ["E\u00b7rin\u00b7nern", "sich", ",", "da\u00df", "hier", "ge\u00b7sche\u00b7hen", "wer\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KOUS", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was fr\u00fcher schon und \u00f6fter wohl geschah:", "tokens": ["Was", "fr\u00fc\u00b7her", "schon", "und", "\u00f6f\u00b7ter", "wohl", "ge\u00b7schah", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "KON", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df Hermes und die beiden Dioskuren,", "tokens": ["Da\u00df", "Her\u00b7mes", "und", "die", "bei\u00b7den", "Dios\u00b7ku\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "ART", "PIAT", "NN", "$,"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Funkelnd vor \u00dcbermut die luftigen Spuren", "tokens": ["Fun\u00b7kelnd", "vor", "\u00dc\u00b7ber\u00b7mut", "die", "luf\u00b7ti\u00b7gen", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Der windgetragenen Grazien umstellen", "tokens": ["Der", "wind\u00b7ge\u00b7tra\u00b7ge\u00b7nen", "Gra\u00b7zi\u00b7en", "um\u00b7stel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Und spielend, mit der Grausamkeit der Jagd,", "tokens": ["Und", "spie\u00b7lend", ",", "mit", "der", "Grau\u00b7sam\u00b7keit", "der", "Jagd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Sie aus den Wipfeln scheuchen, ja die Wellen", "tokens": ["Sie", "aus", "den", "Wip\u00b7feln", "scheu\u00b7chen", ",", "ja", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Des Flusses nahe treiben, bis es tagt.", "tokens": ["Des", "Flus\u00b7ses", "na\u00b7he", "trei\u00b7ben", ",", "bis", "es", "tagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Dichter hat wo anders seinen Weg,", "tokens": ["Der", "Dich\u00b7ter", "hat", "wo", "an\u00b7ders", "sei\u00b7nen", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PWAV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und mit den Augen der Meduse schauend", "tokens": ["Und", "mit", "den", "Au\u00b7gen", "der", "Me\u00b7du\u00b7se", "schau\u00b7end"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sieht er das umgelegene fahle Feld", "tokens": ["Sieht", "er", "das", "um\u00b7ge\u00b7le\u00b7ge\u00b7ne", "fah\u00b7le", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Sogleich entr\u00fcckt und wei\u00df nicht, wie es ist,", "tokens": ["Sog\u00b7leich", "ent\u00b7r\u00fcckt", "und", "wei\u00df", "nicht", ",", "wie", "es", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und f\u00fcgt es andern solchen Orten zu,", "tokens": ["Und", "f\u00fcgt", "es", "an\u00b7dern", "sol\u00b7chen", "Or\u00b7ten", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wo seine Seele wie ein Kind verstellt,", "tokens": ["Wo", "sei\u00b7ne", "See\u00b7le", "wie", "ein", "Kind", "ver\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Dasein hat von keiner sichern Frist", "tokens": ["Ein", "Da\u00b7sein", "hat", "von", "kei\u00b7ner", "si\u00b7chern", "Frist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In Adlersluft und abgestorbner Ruh.", "tokens": ["In", "Ad\u00b7ler\u00b7sluft", "und", "ab\u00b7ge\u00b7storb\u00b7ner", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dort streut er ihr die Schatten und die Scheine", "tokens": ["Dort", "streut", "er", "ihr", "die", "Schat\u00b7ten", "und", "die", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der Erdendinge hin und Edelsteine.", "tokens": ["Der", "Er\u00b7den\u00b7din\u00b7ge", "hin", "und", "E\u00b7del\u00b7stei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Den dritten Teil des Himmels aber nimmt", "tokens": ["Den", "drit\u00b7ten", "Teil", "des", "Him\u00b7mels", "a\u00b7ber", "nimmt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Wolke ein von solcher Todesschw\u00e4rze,", "tokens": ["Die", "Wol\u00b7ke", "ein", "von", "sol\u00b7cher", "To\u00b7des\u00b7schw\u00e4r\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie sie die Seele dessen anf\u00e4llt, der", "tokens": ["Wie", "sie", "die", "See\u00b7le", "des\u00b7sen", "an\u00b7f\u00e4llt", ",", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "PDS", "VVFIN", "$,", "PRELS"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Durch Nacht den Weg sich sucht mit einer Kerze:", "tokens": ["Durch", "Nacht", "den", "Weg", "sich", "sucht", "mit", "ei\u00b7ner", "Ker\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Wolke, die hinzog am n\u00e4chsten Morgen,", "tokens": ["Die", "Wol\u00b7ke", ",", "die", "hin\u00b7zog", "am", "n\u00e4chs\u00b7ten", "Mor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit Donnerschlag von tausenden Gewittern", "tokens": ["Mit", "Don\u00b7ner\u00b7schlag", "von", "tau\u00b7sen\u00b7den", "Ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und blauem Lichte stark wie nahe Sonnen", "tokens": ["Und", "blau\u00b7em", "Lich\u00b7te", "stark", "wie", "na\u00b7he", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADJD", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und schauerlichem Sturz von hei\u00dfen Steinen,", "tokens": ["Und", "schau\u00b7er\u00b7li\u00b7chem", "Sturz", "von", "hei\u00b7\u00dfen", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die Insel heimzusuchen, wo das Zittern", "tokens": ["Die", "In\u00b7sel", "heim\u00b7zu\u00b7su\u00b7chen", ",", "wo", "das", "Zit\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Aufbl\u00fchen lie\u00df die wundervollsten Wonnen,", "tokens": ["Auf\u00b7bl\u00fc\u00b7hen", "lie\u00df", "die", "wun\u00b7der\u00b7volls\u00b7ten", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor ungeheurer Angst erstorbenes Weinen", "tokens": ["Vor", "un\u00b7ge\u00b7heu\u00b7rer", "Angst", "ers\u00b7tor\u00b7be\u00b7nes", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Kaufpreis war: da\u00df in verst\u00f6rten G\u00e4rten,", "tokens": ["Der", "Kauf\u00b7preis", "war", ":", "da\u00df", "in", "ver\u00b7st\u00f6r\u00b7ten", "G\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$.", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die nie sich sahen, sich f\u00fcrs Leben fanden", "tokens": ["Die", "nie", "sich", "sa\u00b7hen", ",", "sich", "f\u00fcrs", "Le\u00b7ben", "fan\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PRF", "VVFIN", "$,", "PRF", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und, trunken sterbend, Rettung nicht begehrten;", "tokens": ["Und", ",", "trun\u00b7ken", "ster\u00b7bend", ",", "Ret\u00b7tung", "nicht", "be\u00b7gehr\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "VVPP", "$,", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df Gott entsprang den Luft- und Erdenbanden,", "tokens": ["Da\u00df", "Gott", "ent\u00b7sprang", "den", "Luft", "und", "Er\u00b7den\u00b7ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "ART", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Verwaiste Kinder gleich Propheten gl\u00fchten", "tokens": ["Ver\u00b7wais\u00b7te", "Kin\u00b7der", "gleich", "Pro\u00b7phe\u00b7ten", "gl\u00fch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und alle Seelen wie die Sterne bl\u00fchten.", "tokens": ["Und", "al\u00b7le", "See\u00b7len", "wie", "die", "Ster\u00b7ne", "bl\u00fch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}