{"dta.poem.15676": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "R\u00e4um' einen Ansto\u00df weg, der einen Schritt k\u00f6nnt' irren,", "tokens": ["R\u00e4um'", "ei\u00b7nen", "An\u00b7sto\u00df", "weg", ",", "der", "ei\u00b7nen", "Schritt", "k\u00f6nnt'", "ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,", "PRELS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jeden Irrthum, der k\u00f6nnt' einen Sinn verwirren.", "tokens": ["Und", "je\u00b7den", "Irr\u00b7thum", ",", "der", "k\u00f6nnt'", "ei\u00b7nen", "Sinn", "ver\u00b7wir\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und sei es lesend auch in einem Buche nur,", "tokens": ["Und", "sei", "es", "le\u00b7send", "auch", "in", "ei\u00b7nem", "Bu\u00b7che", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "ADV", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den falschgerathnen Zug, des Griffelfehltritts Spur,", "tokens": ["Den", "falschge\u00b7rath\u00b7nen", "Zug", ",", "des", "Grif\u00b7fel\u00b7fehl\u00b7tritts", "Spur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Da\u00df eines Andern einst, der lesend nach dir komme,", "tokens": ["Da\u00df", "ei\u00b7nes", "An\u00b7dern", "einst", ",", "der", "le\u00b7send", "nach", "dir", "kom\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "ADV", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verst\u00e4ndnisse der wegger\u00e4umte Fehler fromme.", "tokens": ["Ver\u00b7st\u00e4nd\u00b7nis\u00b7se", "der", "weg\u00b7ge\u00b7r\u00e4um\u00b7te", "Feh\u00b7ler", "from\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}