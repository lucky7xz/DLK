{"dta.poem.3935": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "25) Schreiben an einen Freund,  \n dem er vielerley aufrichtig berichtete.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Freund, mit was vor Danck erkenn ich", "tokens": ["Mein", "Freund", ",", "mit", "was", "vor", "Danck", "er\u00b7kenn", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PRELS", "APPR", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "deine Huld,", "tokens": ["dei\u00b7ne", "Huld", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Dein \u00fcberschickt Pr\u00e4sent setzt mich in neue Schuld,", "tokens": ["Dein", "\u00fc\u00b7bersc\u00b7hickt", "Pr\u00e4\u00b7sent", "setzt", "mich", "in", "neu\u00b7e", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie? k\u00f6nnt ich dir doch nur etwas dagegen geben,", "tokens": ["Wie", "?", "k\u00f6nnt", "ich", "dir", "doch", "nur", "et\u00b7was", "da\u00b7ge\u00b7gen", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "PPER", "ADV", "ADV", "PIS", "PAV", "VVINF", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Jedoch ich finde nichts in meinem armen Leben.", "tokens": ["Je\u00b7doch", "ich", "fin\u00b7de", "nichts", "in", "mei\u00b7nem", "ar\u00b7men", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PIS", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was hat der arme Pursch? nichts, B\u00fccher und", "tokens": ["Was", "hat", "der", "ar\u00b7me", "Pursch", "?", "nichts", ",", "B\u00fc\u00b7cher", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$.", "PIS", "$,", "NN", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Papier,", "tokens": ["Pa\u00b7pier", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Das ist kein Wiedergeld, du danckest nicht daf\u00fcr.", "tokens": ["Das", "ist", "kein", "Wie\u00b7der\u00b7geld", ",", "du", "dan\u00b7ckest", "nicht", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "$,", "PPER", "ADV", "PTKNEG", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was solte dir der Kram von meinen Sachen n\u00fctzen,", "tokens": ["Was", "sol\u00b7te", "dir", "der", "Kram", "von", "mei\u00b7nen", "Sa\u00b7chen", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du liebest nur Gespinst und prave Gorell-Spitzen,", "tokens": ["Du", "lie\u00b7best", "nur", "Ge\u00b7spinst", "und", "pra\u00b7ve", "Go\u00b7rell\u00b7Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dich reitzet weiter nichts, du h\u00fctest stets das Hau\u00df,", "tokens": ["Dich", "reit\u00b7zet", "wei\u00b7ter", "nichts", ",", "du", "h\u00fc\u00b7test", "stets", "das", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und rechnest, wie Arell, so Pfund als Wucher aus.", "tokens": ["Und", "rech\u00b7nest", ",", "wie", "A\u00b7rell", ",", "so", "Pfund", "als", "Wu\u00b7cher", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "NE", "$,", "ADV", "NN", "KOUS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch ist dein Wesen nicht, durch Vortheil zu be-", "tokens": ["Doch", "ist", "dein", "We\u00b7sen", "nicht", ",", "durch", "Vor\u00b7theil", "zu", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "PTKNEG", "$,", "APPR", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "r\u00fccken,", "tokens": ["r\u00fc\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Und armer Leute Schwei\u00df mit Falschheit zu er-", "tokens": ["Und", "ar\u00b7mer", "Leu\u00b7te", "Schwei\u00df", "mit", "Falschheit", "zu", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN", "APPR", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "dr\u00fccken.", "tokens": ["dr\u00fc\u00b7cken", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Den Armen giebt dein Hertz den g\u00fctigsten Erla\u00df,", "tokens": ["Den", "Ar\u00b7men", "giebt", "dein", "Hertz", "den", "g\u00fc\u00b7tigs\u00b7ten", "Er\u00b7la\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Es macht kein seuffzend Ach der Armen Auge na\u00df.", "tokens": ["Es", "macht", "kein", "seuff\u00b7zend", "Ach", "der", "Ar\u00b7men", "Au\u00b7ge", "na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "CARD", "NN", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Du hilffst, so viel du kanst, aus allen Leibes-N\u00f6then,", "tokens": ["Du", "hilffst", ",", "so", "viel", "du", "kanst", ",", "aus", "al\u00b7len", "Lei\u00b7bes\u00b7N\u00f6\u00b7then", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADV", "PPER", "VMFIN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Besonders auch, wie mir, den Kindern der Prophete\u0303.", "tokens": ["Be\u00b7son\u00b7ders", "auch", ",", "wie", "mir", ",", "den", "Kin\u00b7dern", "der", "Pro\u00b7phet\u1ebd", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "PPER", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So thue k\u00fcnfftighin der Mi\u00dfgunst ferner Tort,", "tokens": ["So", "thue", "k\u00fcnff\u00b7tig\u00b7hin", "der", "Mi\u00df\u00b7gunst", "fer\u00b7ner", "Tort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADV", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Pflantz Alter, Jahr und Gl\u00fcck in sp\u00e4te Zeiten fort.", "tokens": ["Pflantz", "Al\u00b7ter", ",", "Jahr", "und", "Gl\u00fcck", "in", "sp\u00e4\u00b7te", "Zei\u00b7ten", "fort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "KON", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Packt dich die Mi\u00dfgunst an, will dich die Frechheit", "tokens": ["Packt", "dich", "die", "Mi\u00df\u00b7gunst", "an", ",", "will", "dich", "die", "Frech\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "VMFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "stechen,", "tokens": ["ste\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Du wirst ihnn Trotz und Hohn durch deine Gro\u00df-", "tokens": ["Du", "wirst", "ihnn", "Trotz", "und", "Hohn", "durch", "dei\u00b7ne", "Gro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "NN", "KON", "NN", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "muth sprechen.", "tokens": ["muth", "spre\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "Es bleibet Rang u. Schatz ein schn\u00f6der Unterscheid,", "tokens": ["Es", "blei\u00b7bet", "Rang", "u.", "Schatz", "ein", "schn\u00f6\u00b7der", "Un\u00b7ter\u00b7scheid", ","], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Vor Alters lebte noch die liebe Redligkeit.", "tokens": ["Vor", "Al\u00b7ters", "leb\u00b7te", "noch", "die", "lie\u00b7be", "Red\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Was niedrig, hebet sich bis an gestirnten Himmel,", "tokens": ["Was", "nied\u00b7rig", ",", "he\u00b7bet", "sich", "bis", "an", "ge\u00b7stirn\u00b7ten", "Him\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "VVFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So steiget Mops, der Jeck, der abgeschmackte Lim\u0303el,", "tokens": ["So", "stei\u00b7get", "Mops", ",", "der", "Jeck", ",", "der", "ab\u00b7ge\u00b7schmack\u00b7te", "Lim\u0303el", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "ART", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Er kennt sich selber nicht, der Hochmut macht ihn", "tokens": ["Er", "kennt", "sich", "sel\u00b7ber", "nicht", ",", "der", "Hoch\u00b7mut", "macht", "ihn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "PTKNEG", "$,", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "schwer,", "tokens": ["schwer", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.33": {"text": "Als wenn er ", "tokens": ["Als", "wenn", "er"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.34": {"text": "Da stutzt der theure Mann, da hat er viel zu schaffen,", "tokens": ["Da", "stutzt", "der", "theu\u00b7re", "Mann", ",", "da", "hat", "er", "viel", "zu", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Inde\u00df schreyt jedes Kind: seht doch den alten Affen.", "tokens": ["In\u00b7de\u00df", "schreyt", "je\u00b7des", "Kind", ":", "seht", "doch", "den", "al\u00b7ten", "Af\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$.", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Jhn ziert ein weisses Hemd, ein Kleidgen k\u00f6stlich", "tokens": ["Jhn", "ziert", "ein", "weis\u00b7ses", "Hemd", ",", "ein", "Kleid\u00b7gen", "k\u00f6st\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "bund,", "tokens": ["bund", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.38": {"text": "Da stutzt der alte Mops, wie Matzens Fleischer-", "tokens": ["Da", "stutzt", "der", "al\u00b7te", "Mops", ",", "wie", "Mat\u00b7zens", "Flei\u00b7scher"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "PWAV", "NE", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Hund.", "tokens": ["Hund", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.40": {"text": "Er liebet das Complott von frechen Lotterbuben,", "tokens": ["Er", "lie\u00b7bet", "das", "Com\u00b7plott", "von", "fre\u00b7chen", "Lot\u00b7ter\u00b7bu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und achtet diese Welt gleich einer M\u00f6rder-Gruben.", "tokens": ["Und", "ach\u00b7tet", "die\u00b7se", "Welt", "gleich", "ei\u00b7ner", "M\u00f6r\u00b7der\u00b7Gru\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Er meinet, Gl\u00fcck und Wohl sey ihm mehr als gewi\u00df.", "tokens": ["Er", "mei\u00b7net", ",", "Gl\u00fcck", "und", "Wohl", "sey", "ihm", "mehr", "als", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "KON", "ADV", "VAFIN", "PPER", "PIAT", "KOKOM", "ADV", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "Sieh, ", "tokens": ["Sieh", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.44": {"text": "ten Ri\u00df", "tokens": ["ten", "Ri\u00df"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.45": {"text": "Von unsers Neiders Thun; die Zeit befiehlt zu eilen,", "tokens": ["Von", "un\u00b7sers", "Nei\u00b7ders", "Thun", ";", "die", "Zeit", "be\u00b7fiehlt", "zu", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "ART", "NN", "VVPP", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Nimm von mir Ku\u00df und Gru\u00df beym Ende dieser", "tokens": ["Nimm", "von", "mir", "Ku\u00df", "und", "Gru\u00df", "beym", "En\u00b7de", "die\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPER", "NN", "KON", "NN", "APPRART", "NN", "PDAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Zeilen.", "tokens": ["Zei\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}