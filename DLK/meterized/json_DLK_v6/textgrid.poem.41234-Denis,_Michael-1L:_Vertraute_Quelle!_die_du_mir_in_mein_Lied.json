{"textgrid.poem.41234": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vertraute Quelle! die du mir in mein Lied", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vertraute Quelle! die du mir in mein Lied", "tokens": ["Ver\u00b7trau\u00b7te", "Quel\u00b7le", "!", "die", "du", "mir", "in", "mein", "Lied"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PRELS", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Schon \u00f6fter stimmtest, Quelle! wie g\u00e4h bedeckt", "tokens": ["Schon", "\u00f6f\u00b7ter", "stimm\u00b7test", ",", "Quel\u00b7le", "!", "wie", "g\u00e4h", "be\u00b7deckt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "$,", "NN", "$.", "PWAV", "ADJD", "VVPP"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein k\u00fchner West mit falben Bl\u00e4ttern", "tokens": ["Ein", "k\u00fch\u00b7ner", "West", "mit", "fal\u00b7ben", "Bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Deine gekr\u00e4uselte Silberfl\u00e4che!", "tokens": ["Dei\u00b7ne", "ge\u00b7kr\u00e4u\u00b7sel\u00b7te", "Sil\u00b7ber\u00b7fl\u00e4\u00b7che", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Zu welcher Ahnung weckt mich ihr schneller Fall!", "tokens": ["Zu", "wel\u00b7cher", "Ah\u00b7nung", "weckt", "mich", "ihr", "schnel\u00b7ler", "Fall", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sind die\u00df die Bl\u00e4tter, welche der Lenz gebahr?", "tokens": ["Sind", "die\u00df", "die", "Bl\u00e4t\u00b7ter", ",", "wel\u00b7che", "der", "Lenz", "ge\u00b7bahr", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des Haines Zier, des M\u00fcden Schatten", "tokens": ["Des", "Hai\u00b7nes", "Zier", ",", "des", "M\u00fc\u00b7den", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Waren sie! \u2013 sind nun ein Spiel der Winde.", "tokens": ["Wa\u00b7ren", "sie", "!", "\u2013", "sind", "nun", "ein", "Spiel", "der", "Win\u00b7de", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$.", "$(", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Gedanke! m\u00e4chtig f\u00fcllst du die Seele mir!", "tokens": ["Ge\u00b7dan\u00b7ke", "!", "m\u00e4ch\u00b7tig", "f\u00fcllst", "du", "die", "See\u00b7le", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJD", "VVFIN", "PPER", "ART", "NN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie fleu\u00dft mir \u00fcber! Sterbliche! Sterbliche!", "tokens": ["Sie", "fleu\u00dft", "mir", "\u00fc\u00b7ber", "!", "Sterb\u00b7li\u00b7che", "!", "Sterb\u00b7li\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "$.", "NN", "$.", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So fallen wir! In diesen Bl\u00e4ttern", "tokens": ["So", "fal\u00b7len", "wir", "!", "In", "die\u00b7sen", "Bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwimmt mir der Menschlichkeit Loos vor Augen.", "tokens": ["Schwimmt", "mir", "der", "Menschlich\u00b7keit", "Loos", "vor", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NE", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Entw\u00f6lkt bestralt uns jetzo des Gl\u00fcck's Planet.", "tokens": ["Ent\u00b7w\u00f6lkt", "be\u00b7stralt", "uns", "jet\u00b7zo", "des", "Gl\u00fc\u00b7ck's", "Pla\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "NE", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der West des Ruhmes k\u00fchlet und hebet uns.", "tokens": ["Der", "West", "des", "Ruh\u00b7mes", "k\u00fch\u00b7let", "und", "he\u00b7bet", "uns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Uns tr\u00e4nkt ein Thau von Lebensfreuden.", "tokens": ["Uns", "tr\u00e4nkt", "ein", "Thau", "von", "Le\u00b7bens\u00b7freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00fcckliche Bl\u00e4tter! und nun! \u2013 Wir fallen!", "tokens": ["Gl\u00fcck\u00b7li\u00b7che", "Bl\u00e4t\u00b7ter", "!", "und", "nun", "!", "\u2013", "Wir", "fal\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "KON", "ADV", "$.", "$(", "PPER", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Nicht Glanz der Ahnen, Wiegen, die Purpur deckt,", "tokens": ["Nicht", "Glanz", "der", "Ah\u00b7nen", ",", "Wie\u00b7gen", ",", "die", "Pur\u00b7pur", "deckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ART", "NN", "$,", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nicht Lenz des Alters, wenn ihn die Sch\u00f6nheit auch", "tokens": ["Nicht", "Lenz", "des", "Al\u00b7ters", ",", "wenn", "ihn", "die", "Sch\u00f6n\u00b7heit", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "ADV"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit allen K\u00fcnsten unterst\u00fctzet,", "tokens": ["Mit", "al\u00b7len", "K\u00fcns\u00b7ten", "un\u00b7ter\u00b7st\u00fct\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bittet den kommenden Tod zur\u00fccke.", "tokens": ["Bit\u00b7tet", "den", "kom\u00b7men\u00b7den", "Tod", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Ihm stockt die Weisheit, lallt die Beredsamkeit,", "tokens": ["Ihm", "stockt", "die", "Weis\u00b7heit", ",", "lallt", "die", "Be\u00b7red\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Muth erbla\u00dft ihm. Hoher Troph\u00e4en Stolz", "tokens": ["Der", "Muth", "er\u00b7bla\u00dft", "ihm", ".", "Ho\u00b7her", "Trop\u00b7h\u00e4\u00b7en", "Stolz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "ADJA", "NN", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Beginnt vor ihm in Schutt zu sinken;", "tokens": ["Be\u00b7ginnt", "vor", "ihm", "in", "Schutt", "zu", "sin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kronen erbeben und Throne wanken.", "tokens": ["Kro\u00b7nen", "er\u00b7be\u00b7ben", "und", "Thro\u00b7ne", "wan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Du selbst, o Tugend! alles Verm\u00f6gende!", "tokens": ["Du", "selbst", ",", "o", "Tu\u00b7gend", "!", "al\u00b7les", "Ver\u00b7m\u00f6\u00b7gen\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "FM", "NN", "$.", "PIAT", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du selber rettest deine Verehrer nicht!", "tokens": ["Du", "sel\u00b7ber", "ret\u00b7test", "dei\u00b7ne", "Ver\u00b7eh\u00b7rer", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Staub des B\u00f6swichts und des Frommen", "tokens": ["Der", "Staub", "des", "B\u00f6s\u00b7wichts", "und", "des", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mischet sich unter des Wandrers Tritten. \u2013", "tokens": ["Mi\u00b7schet", "sich", "un\u00b7ter", "des", "Wand\u00b7rers", "Trit\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Noch heute sa\u00df Er, erster Monarch der Welt", "tokens": ["Noch", "heu\u00b7te", "sa\u00df", "Er", ",", "ers\u00b7ter", "Mon\u00b7arch", "der", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Der besten Gattinn z\u00e4rtlichstes Augenmerk,", "tokens": ["Der", "bes\u00b7ten", "Gat\u00b7tinn", "z\u00e4rt\u00b7lichs\u00b7tes", "Au\u00b7gen\u00b7merk", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Umgeben von geliebten Kindern,", "tokens": ["Um\u00b7ge\u00b7ben", "von", "ge\u00b7lieb\u00b7ten", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00fcrdig Aeonen hinan zu leben.", "tokens": ["W\u00fcr\u00b7dig", "A\u00b7e\u00b7o\u00b7nen", "hi\u00b7nan", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Noch t\u00f6nten Hymens Lieder ihm sanft ins Ohr,", "tokens": ["Noch", "t\u00f6n\u00b7ten", "Hy\u00b7mens", "Lie\u00b7der", "ihm", "sanft", "ins", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und pl\u00f6tzlich r\u00f6cheln T\u00f6ne des Tod's darein.", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "r\u00f6\u00b7cheln", "T\u00f6\u00b7ne", "des", "To\u00b7d's", "da\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "ART", "NN", "PAV", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sein Tag verlischt. Zum letztenmale", "tokens": ["Sein", "Tag", "ver\u00b7lischt", ".", "Zum", "letz\u00b7ten\u00b7ma\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "$.", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Segnet sein brechender Blick die V\u00f6lker. \u2013", "tokens": ["Seg\u00b7net", "sein", "bre\u00b7chen\u00b7der", "Blick", "die", "V\u00f6l\u00b7ker", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Noch heute sah dein sittsames Augenpaar", "tokens": ["Noch", "heu\u00b7te", "sah", "dein", "sitt\u00b7sa\u00b7mes", "Au\u00b7gen\u00b7paar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Reiz des Herbstes, Bester der J\u00fcnglinge", "tokens": ["Den", "Reiz", "des", "Herbs\u00b7tes", ",", "Bes\u00b7ter", "der", "J\u00fcng\u00b7lin\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "NN", "ART", "NN"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im frohen Haufen gleicher Freunde", "tokens": ["Im", "fro\u00b7hen", "Hau\u00b7fen", "glei\u00b7cher", "Freun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zog dich die Liebe zur Jagd ins Gr\u00fcne.", "tokens": ["Zog", "dich", "die", "Lie\u00b7be", "zur", "Jagd", "ins", "Gr\u00fc\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Da flog dein Ungl\u00fcck. Ach, du versahst es nicht!", "tokens": ["Da", "flog", "dein", "Un\u00b7gl\u00fcck", ".", "Ach", ",", "du", "ver\u00b7sahst", "es", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "ITJ", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein Bley! Die Sch\u00f6pfung wurde zur Nacht um dich,", "tokens": ["Ein", "Bley", "!", "Die", "Sch\u00f6p\u00b7fung", "wur\u00b7de", "zur", "Nacht", "um", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "VAFIN", "APPRART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und eines deiner holden Augen", "tokens": ["Und", "ei\u00b7nes", "dei\u00b7ner", "hol\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlo\u00df sich in ewige Finsternisse.", "tokens": ["Schlo\u00df", "sich", "in", "e\u00b7wi\u00b7ge", "Fins\u00b7ter\u00b7nis\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Gewi\u00df des Grabes wallen wir, ungewi\u00df", "tokens": ["Ge\u00b7wi\u00df", "des", "Gra\u00b7bes", "wal\u00b7len", "wir", ",", "un\u00b7ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "$,", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der schwarzen Stunde. Menschen! kein Augenblick", "tokens": ["Der", "schwar\u00b7zen", "Stun\u00b7de", ".", "Men\u00b7schen", "!", "kein", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "$.", "PIAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ist seines Folgers B\u00fcrge. Nebel", "tokens": ["Ist", "sei\u00b7nes", "Fol\u00b7gers", "B\u00fcr\u00b7ge", ".", "Ne\u00b7bel"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$.", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schweben auf jeglicher Spur der Zukunft.", "tokens": ["Schwe\u00b7ben", "auf", "jeg\u00b7li\u00b7cher", "Spur", "der", "Zu\u00b7kunft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "O gl\u00fccklich, welcher seine Bestimmung denkt,", "tokens": ["O", "gl\u00fcck\u00b7lich", ",", "wel\u00b7cher", "sei\u00b7ne", "Be\u00b7stim\u00b7mung", "denkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein Theil des Ganzen willig die Stelle f\u00fcllt,", "tokens": ["Ein", "Theil", "des", "Gan\u00b7zen", "wil\u00b7lig", "die", "Stel\u00b7le", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zu der ihn Jener auserw\u00e4hlte,", "tokens": ["Zu", "der", "ihn", "Je\u00b7ner", "au\u00b7ser\u00b7w\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "PDAT", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welcher ihn aufschuf und liebt und lohnet!", "tokens": ["Wel\u00b7cher", "ihn", "auf\u00b7schuf", "und", "liebt", "und", "loh\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "Er z\u00e4hlt sich sorgsam jeden der Tage vor,", "tokens": ["Er", "z\u00e4hlt", "sich", "sorg\u00b7sam", "je\u00b7den", "der", "Ta\u00b7ge", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "PIAT", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und jeder sieht ihn besser und \u00e4hnlicher", "tokens": ["Und", "je\u00b7der", "sieht", "ihn", "bes\u00b7ser", "und", "\u00e4hn\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADJD", "KON", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dem unerschaffnen Muster, jeder", "tokens": ["Dem", "un\u00b7er\u00b7schaff\u00b7nen", "Mus\u00b7ter", ",", "je\u00b7der"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00e4nzet bezeichnet mit Menschenliebe.", "tokens": ["Gl\u00e4n\u00b7zet", "be\u00b7zeich\u00b7net", "mit", "Men\u00b7schen\u00b7lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.15": {"line.1": {"text": "Erscheint der Abend, giebt er sich Rechenschaft,", "tokens": ["Er\u00b7scheint", "der", "A\u00b7bend", ",", "giebt", "er", "sich", "Re\u00b7chen\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "PRF", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und scheut den Zeugen seiner Gedanken nicht,", "tokens": ["Und", "scheut", "den", "Zeu\u00b7gen", "sei\u00b7ner", "Ge\u00b7dan\u00b7ken", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und spricht vergn\u00fcgt zu sich: \u00bbIch lebte!", "tokens": ["Und", "spricht", "ver\u00b7gn\u00fcgt", "zu", "sich", ":", "\u00bb", "Ich", "leb\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PRF", "$.", "$(", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlummer! umwalle mein Aug'! Ich lebte!\u00ab", "tokens": ["Schlum\u00b7mer", "!", "um\u00b7wal\u00b7le", "mein", "Aug'", "!", "Ich", "leb\u00b7te", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.16": {"line.1": {"text": "Zu folgen willig, wann die Natur gebeut,", "tokens": ["Zu", "fol\u00b7gen", "wil\u00b7lig", ",", "wann", "die", "Na\u00b7tur", "ge\u00b7beut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Schon lange Freund des Todes, erwartet er", "tokens": ["Schon", "lan\u00b7ge", "Freund", "des", "To\u00b7des", ",", "er\u00b7war\u00b7tet", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "NN", "ART", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit sich'rem L\u00e4cheln jene Stunde,", "tokens": ["Mit", "sich'\u00b7rem", "L\u00e4\u00b7cheln", "je\u00b7ne", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "N