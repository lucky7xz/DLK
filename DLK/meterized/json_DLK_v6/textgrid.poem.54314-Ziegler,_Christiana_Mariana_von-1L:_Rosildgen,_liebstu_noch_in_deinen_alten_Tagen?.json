{"textgrid.poem.54314": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Rosildgen, liebstu noch in deinen alten Tagen?", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rosildgen, liebstu noch in deinen alten Tagen?", "tokens": ["Ro\u00b7sild\u00b7gen", ",", "liebs\u00b7tu", "noch", "in", "dei\u00b7nen", "al\u00b7ten", "Ta\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie? mag Cupido doch dich noch so sp\u00e4the plagen.", "tokens": ["Wie", "?", "mag", "Cu\u00b7pi\u00b7do", "doch", "dich", "noch", "so", "sp\u00e4\u00b7the", "pla\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "NE", "KON", "PPER", "ADV", "ADV", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Grabscheid ist vor dich viel besser, wie mich d\u00fcnckt,", "tokens": ["Das", "Grab\u00b7scheid", "ist", "vor", "dich", "viel", "bes\u00b7ser", ",", "wie", "mich", "d\u00fcnckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "ADV", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als Amors Liebes-Pfeil, der dir ins Hertze dringt.", "tokens": ["Als", "A\u00b7mors", "Lie\u00b7bes\u00b7Pfeil", ",", "der", "dir", "ins", "Hert\u00b7ze", "dringt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "$,", "PRELS", "PPER", "APPRART", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Will sich Cupido nicht vor deinen Runtzeln scheuen?", "tokens": ["Will", "sich", "Cu\u00b7pi\u00b7do", "nicht", "vor", "dei\u00b7nen", "Runt\u00b7zeln", "scheu\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "NE", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denckt dein verschrumpfelt Hertz, ists m\u00f6glich, noch zu freyen?", "tokens": ["Denckt", "dein", "ver\u00b7schrump\u00b7felt", "Hertz", ",", "ists", "m\u00f6g\u00b7lich", ",", "noch", "zu", "frey\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "NN", "$,", "VAFIN", "ADJD", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach dein verwelckter Mund schickt sich gar nicht zum Ku\u00df,", "tokens": ["Ach", "dein", "ver\u00b7welck\u00b7ter", "Mund", "schickt", "sich", "gar", "nicht", "zum", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "ADV", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dein Wesen und Gestalt gleicht einer tauben Nu\u00df.", "tokens": ["Dein", "We\u00b7sen", "und", "Ge\u00b7stalt", "gleicht", "ei\u00b7ner", "tau\u00b7ben", "Nu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie sch\u00f6ne sitzt dir nicht der Braut-Crantz auf dem Neste?", "tokens": ["Wie", "sch\u00f6\u00b7ne", "sitzt", "dir", "nicht", "der", "Braut\u00b7Crantz", "auf", "dem", "Nes\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein jeder wundert sich und glaubet steiff und feste,", "tokens": ["Ein", "je\u00b7der", "wun\u00b7dert", "sich", "und", "glau\u00b7bet", "steiff", "und", "fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "KON", "VVFIN", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es w\u00e4r Johannis-Tag, an dem ein jedes Kind", "tokens": ["Es", "w\u00e4r", "Jo\u00b7han\u00b7nis\u00b7Tag", ",", "an", "dem", "ein", "je\u00b7des", "Kind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NE", "$,", "APPR", "PRELS", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Um einen alten Topf die sch\u00f6nsten Blumen wind.", "tokens": ["Um", "ei\u00b7nen", "al\u00b7ten", "Topf", "die", "sch\u00f6ns\u00b7ten", "Blu\u00b7men", "wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was w\u00fcrde Hymen doch bey deinem Opfer dencken,", "tokens": ["Was", "w\u00fcr\u00b7de", "Hy\u00b7men", "doch", "bey", "dei\u00b7nem", "Op\u00b7fer", "den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wann du wirst deinen Fu\u00df nach seinen Schwellen lencken?", "tokens": ["Wann", "du", "wirst", "dei\u00b7nen", "Fu\u00df", "nach", "sei\u00b7nen", "Schwel\u00b7len", "len\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wiewohl es ist ihm recht, indem er selbst bekennt,", "tokens": ["Wie\u00b7wohl", "es", "ist", "ihm", "recht", ",", "in\u00b7dem", "er", "selbst", "be\u00b7kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df altes Stroh und Holtz am allerbesten brennt.", "tokens": ["Da\u00df", "al\u00b7tes", "Stroh", "und", "Holtz", "am", "al\u00b7ler\u00b7bes\u00b7ten", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dein Liebster wird statt Kerns nur leere Schalen brechen,", "tokens": ["Dein", "Liebs\u00b7ter", "wird", "statt", "Kerns", "nur", "lee\u00b7re", "Scha\u00b7len", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Denn Venus sucht an ihm sich meisterlich zu r\u00e4chen.", "tokens": ["Denn", "Ve\u00b7nus", "sucht", "an", "ihm", "sich", "meis\u00b7ter\u00b7lich", "zu", "r\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPER", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "Er hat bald da, bald dort, gleichwie bekannt, genascht,", "tokens": ["Er", "hat", "bald", "da", ",", "bald", "dort", ",", "gleich\u00b7wie", "be\u00b7kannt", ",", "ge\u00b7nascht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "ADV", "ADV", "$,", "KON", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Daher er leeres Stroh an statt der Syrinx hascht.", "tokens": ["Da\u00b7her", "er", "lee\u00b7res", "Stroh", "an", "statt", "der", "Sy\u00b7rinx", "hascht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was dir an Jugend fehlt, ersetzen die Ducaten,", "tokens": ["Was", "dir", "an", "Ju\u00b7gend", "fehlt", ",", "er\u00b7set\u00b7zen", "die", "Du\u00b7ca\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Vielleicht ist deinem Mann mit selbigen gerathen.", "tokens": ["Viel\u00b7leicht", "ist", "dei\u00b7nem", "Mann", "mit", "sel\u00b7bi\u00b7gen", "ge\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wer wei\u00df, welch armes Kind noch deinen Schwei\u00df genie\u00dft,", "tokens": ["Wer", "wei\u00df", ",", "welch", "ar\u00b7mes", "Kind", "noch", "dei\u00b7nen", "Schwei\u00df", "ge\u00b7nie\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAT", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wann sich dein liebster Schatz mit ihr ins Zimmer schlie\u00dft.", "tokens": ["Wann", "sich", "dein", "liebs\u00b7ter", "Schatz", "mit", "ihr", "ins", "Zim\u00b7mer", "schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}