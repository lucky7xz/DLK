{"dta.poem.10268": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Zerbrochener Keilerkopf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Im Rabenhorst, im Dunkelforst,             ", "tokens": ["Im", "Ra\u00b7ben\u00b7horst", ",", "im", "Dun\u00b7kel\u00b7forst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo j\u00fcngst der Blitz den Eichbaum borst,", "tokens": ["Wo", "j\u00fcngst", "der", "Blitz", "den", "Eich\u00b7baum", "borst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Lamm wird dort geschoren:", "tokens": ["Kein", "Lamm", "wird", "dort", "ge\u00b7scho\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der K\u00f6nig griff den Keiler an,", "tokens": ["Der", "K\u00f6\u00b7nig", "griff", "den", "Kei\u00b7ler", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Keiler nahm den K\u00f6nig an,", "tokens": ["Der", "Kei\u00b7ler", "nahm", "den", "K\u00f6\u00b7nig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der K\u00f6nig scheint verloren.", "tokens": ["Der", "K\u00f6\u00b7nig", "scheint", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da st\u00fcrzt hervor, ein Jaguar,", "tokens": ["Da", "st\u00fcrzt", "her\u00b7vor", ",", "ein", "Ja\u00b7gu\u00b7ar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Funkelblick und Stachelhaar,", "tokens": ["Mit", "Fun\u00b7kel\u00b7blick", "und", "Sta\u00b7chel\u00b7haar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jung Henning durch die Bl\u00e4tter:", "tokens": ["Jung", "Hen\u00b7ning", "durch", "die", "Bl\u00e4t\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Diener aus des F\u00fcrsten Tro\u00df,", "tokens": ["Ein", "Die\u00b7ner", "aus", "des", "F\u00fcrs\u00b7ten", "Tro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Schwertgesell und Jagdgeno\u00df,", "tokens": ["Sein", "Schwert\u00b7ge\u00b7sell", "und", "Jagd\u00b7ge\u00b7no\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nun des Gebieters Retter.", "tokens": ["Nun", "des", "Ge\u00b7bie\u00b7ters", "Ret\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Des K\u00f6nigs Dank ist Turm und Land,", "tokens": ["Des", "K\u00f6\u00b7nigs", "Dank", "ist", "Turm", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er z\u00e4umt mit rot und g\u00fclden Band", "tokens": ["Er", "z\u00e4umt", "mit", "rot", "und", "g\u00fcl\u00b7den", "Band"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm seinen besten Rappen.", "tokens": ["Ihm", "sei\u00b7nen", "bes\u00b7ten", "Rap\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es schaut der Ritter durchs Visier,", "tokens": ["Es", "schaut", "der", "Rit\u00b7ter", "durchs", "Vi\u00b7sier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Eber droht, des Helmes Zier,", "tokens": ["Ein", "E\u00b7ber", "droht", ",", "des", "Hel\u00b7mes", "Zier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Eberkopf im Wappen.", "tokens": ["Ein", "E\u00b7ber\u00b7kopf", "im", "Wap\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Jahrhundert auf Jahrhundert rann,", "tokens": ["Jahr\u00b7hun\u00b7dert", "auf", "Jahr\u00b7hun\u00b7dert", "rann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Augenblick. Die Parze spann", "tokens": ["Ein", "Au\u00b7gen\u00b7blick", ".", "Die", "Par\u00b7ze", "spann"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gleichm\u00e4\u00dfig ihren Faden.", "tokens": ["Gleich\u00b7m\u00e4\u00b7\u00dfig", "ih\u00b7ren", "Fa\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Sippe flo\u00df, zuerst ein Quell,", "tokens": ["Die", "Sip\u00b7pe", "flo\u00df", ",", "zu\u00b7erst", "ein", "Quell", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann Flu\u00df und Strom, bald still und hell,", "tokens": ["Dann", "Flu\u00df", "und", "Strom", ",", "bald", "still", "und", "hell", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bald rauschend wie Kaskaden.", "tokens": ["Bald", "rau\u00b7schend", "wie", "Kas\u00b7ka\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Versandet. Noch ein letzter Blink,", "tokens": ["Ver\u00b7san\u00b7det", ".", "Noch", "ein", "letz\u00b7ter", "Blink", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es rinnt im Sonnenscheidewink", "tokens": ["Es", "rinnt", "im", "Son\u00b7nen\u00b7schei\u00b7de\u00b7wink"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Murmelbach von hinnen:", "tokens": ["Der", "Mur\u00b7mel\u00b7bach", "von", "hin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die kleine feine Eminenz", "tokens": ["Die", "klei\u00b7ne", "fei\u00b7ne", "E\u00b7mi\u00b7nenz"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Garten dort in Laub und Lenz,", "tokens": ["Im", "Gar\u00b7ten", "dort", "in", "Laub", "und", "Lenz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was steht sie tief in Sinnen?", "tokens": ["Was", "steht", "sie", "tief", "in", "Sin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Lanzenreiter, Tod genannt,", "tokens": ["Der", "Lan\u00b7zen\u00b7rei\u00b7ter", ",", "Tod", "ge\u00b7nannt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fchrt sicher seine Knochenhand,", "tokens": ["F\u00fchrt", "si\u00b7cher", "sei\u00b7ne", "Kno\u00b7chen\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er hat den Greis erstochen.", "tokens": ["Er", "hat", "den", "Greis", "er\u00b7sto\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zerpfl\u00fcckt, verwelkt das Kranzgeflecht,", "tokens": ["Zer\u00b7pfl\u00fcckt", ",", "ver\u00b7welkt", "das", "Kranz\u00b7ge\u00b7flecht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erloschen ist ein alt Geschlecht,", "tokens": ["Er\u00b7lo\u00b7schen", "ist", "ein", "alt", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Das Wappenschild zerbrochen.", "tokens": ["Das", "Wap\u00b7pen\u00b7schild", "zer\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}