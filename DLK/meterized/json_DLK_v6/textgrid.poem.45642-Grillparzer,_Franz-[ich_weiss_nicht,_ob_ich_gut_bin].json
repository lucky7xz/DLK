{"textgrid.poem.45642": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[ich weiss nicht, ob ich gut bin]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich weiss nicht, ob ich gut bin,", "tokens": ["Ich", "weiss", "nicht", ",", "ob", "ich", "gut", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich wei\u00df nur, da\u00df meine Gegner schlecht sind.", "tokens": ["Ich", "wei\u00df", "nur", ",", "da\u00df", "mei\u00b7ne", "Geg\u00b7ner", "schlecht", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So bleiben zweifelhaft meine Gaben:", "tokens": ["So", "blei\u00b7ben", "zwei\u00b7fel\u00b7haft", "mei\u00b7ne", "Ga\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir k\u00f6nnten ja beide Unrecht haben.", "tokens": ["Wir", "k\u00f6nn\u00b7ten", "ja", "bei\u00b7de", "Un\u00b7recht", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "VAINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}