{"textgrid.poem.33644": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Die Kr\u00e4hen", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Feldein nach einem d\u00fcrren Baum", "tokens": ["Feld\u00b7ein", "nach", "ei\u00b7nem", "d\u00fcr\u00b7ren", "Baum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fliegt eine Schar von Kr\u00e4hen,", "tokens": ["Fliegt", "ei\u00b7ne", "Schar", "von", "Kr\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die langsam wie im d\u00fcstern Traum", "tokens": ["Die", "lang\u00b7sam", "wie", "im", "d\u00fcs\u00b7tern", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "KOKOM", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die schwarzen Fl\u00fcgel bl\u00e4hen.", "tokens": ["Die", "schwar\u00b7zen", "Fl\u00fc\u00b7gel", "bl\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie sind hinausgesandt vom Tod,", "tokens": ["Sie", "sind", "hin\u00b7aus\u00b7ge\u00b7sandt", "vom", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie den Sturm die M\u00f6ven,", "tokens": ["Und", "wie", "den", "Sturm", "die", "M\u00f6\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verk\u00fcnden sie, wenn Unheil droht", "tokens": ["Ver\u00b7k\u00fcn\u00b7den", "sie", ",", "wenn", "Un\u00b7heil", "droht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Heide stillen H\u00f6fen.", "tokens": ["Der", "Hei\u00b7de", "stil\u00b7len", "H\u00f6\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wo sie sich nahen, rasselt wach", "tokens": ["Wo", "sie", "sich", "na\u00b7hen", ",", "ras\u00b7selt", "wach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "ADJA", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Hofhund an der Kette,", "tokens": ["Der", "Hof\u00b7hund", "an", "der", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00e4lzen sich mit Angst und Ach", "tokens": ["Und", "w\u00e4l\u00b7zen", "sich", "mit", "Angst", "und", "Ach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kranken auf dem Bette.", "tokens": ["Die", "Kran\u00b7ken", "auf", "dem", "Bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie bauen am Kamin ihr Nest,", "tokens": ["Sie", "bau\u00b7en", "am", "Ka\u00b7min", "ihr", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Dann stirbt der Herr des Hauses,", "tokens": ["Dann", "stirbt", "der", "Herr", "des", "Hau\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie laden schreiend sich zum Fest,", "tokens": ["Sie", "la\u00b7den", "schrei\u00b7end", "sich", "zum", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Rest des Leichenschmauses.", "tokens": ["Zum", "Rest", "des", "Lei\u00b7chen\u00b7schmau\u00b7ses", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es jagt ein dunkler Erdengeist", "tokens": ["Es", "jagt", "ein", "dunk\u00b7ler", "Er\u00b7den\u00b7geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ihren finstern Seelen;", "tokens": ["In", "ih\u00b7ren", "fins\u00b7tern", "See\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie fliegen, wo sein Finger weist,", "tokens": ["Sie", "flie\u00b7gen", ",", "wo", "sein", "Fin\u00b7ger", "weist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dahin aus ihren H\u00f6hlen.", "tokens": ["Da\u00b7hin", "aus", "ih\u00b7ren", "H\u00f6h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dort fliegen sie, je vier und vier \u2013", "tokens": ["Dort", "flie\u00b7gen", "sie", ",", "je", "vier", "und", "vier", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "CARD", "KON", "CARD", "$("], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wohin wohl heut beschieden?", "tokens": ["Wo\u00b7hin", "wohl", "heut", "be\u00b7schie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O m\u00f6gen gute Geister mir", "tokens": ["O", "m\u00f6\u00b7gen", "gu\u00b7te", "Geis\u00b7ter", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Heimathaus umfrieden!", "tokens": ["Mein", "Hei\u00b7ma\u00b7thaus", "um\u00b7frie\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}