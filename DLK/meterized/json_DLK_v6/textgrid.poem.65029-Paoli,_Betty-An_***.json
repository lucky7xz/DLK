{"textgrid.poem.65029": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "An ***", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du forderst r\u00fcckhaltslos Vertrauen?", "tokens": ["Du", "for\u00b7derst", "r\u00fcck\u00b7halts\u00b7los", "Ver\u00b7trau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du willst in deinem frommen Wahn", "tokens": ["Du", "willst", "in", "dei\u00b7nem", "from\u00b7men", "Wahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zutiefst in meine Seele schauen,", "tokens": ["Zu\u00b7tiefst", "in", "mei\u00b7ne", "See\u00b7le", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den st\u00fcrmereichen Ozean? \u2013", "tokens": ["Den", "st\u00fcr\u00b7me\u00b7rei\u00b7chen", "O\u00b7ze\u00b7an", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du willst behutsam, leise tauchen", "tokens": ["Du", "willst", "be\u00b7hut\u00b7sam", ",", "lei\u00b7se", "tau\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "$,", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In meine Wunden deine Hand?", "tokens": ["In", "mei\u00b7ne", "Wun\u00b7den", "dei\u00b7ne", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du w\u00e4hnst mit sanften Liebeshauchen", "tokens": ["Du", "w\u00e4hnst", "mit", "sanf\u00b7ten", "Lie\u00b7bes\u00b7hau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu mildern ihren hei\u00dfen Brand? \u2013", "tokens": ["Zu", "mil\u00b7dern", "ih\u00b7ren", "hei\u00b7\u00dfen", "Brand", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn ich nun deinen Wunsch erf\u00fcllte,", "tokens": ["Wenn", "ich", "nun", "dei\u00b7nen", "Wunsch", "er\u00b7f\u00fcll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn all' die Schmerzen, St\u00fcck f\u00fcr St\u00fcck,", "tokens": ["Wenn", "all'", "die", "Schmer\u00b7zen", ",", "St\u00fcck", "f\u00fcr", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich nun vor deinem Blick' enth\u00fcllte,", "tokens": ["Ich", "nun", "vor", "dei\u00b7nem", "Blick'", "ent\u00b7h\u00fcll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie bebtest du entsetzt zur\u00fcck!", "tokens": ["Wie", "beb\u00b7test", "du", "ent\u00b7setzt", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie schnell entwiche deinen Wangen", "tokens": ["Wie", "schnell", "ent\u00b7wi\u00b7che", "dei\u00b7nen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Jugend heit'rer Rosenschein,", "tokens": ["Der", "Ju\u00b7gend", "heit'\u00b7rer", "Ro\u00b7sen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und deinem Herzen das Verlangen,", "tokens": ["Und", "dei\u00b7nem", "Her\u00b7zen", "das", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Tr\u00f6ster solchem Weh zu sein!", "tokens": ["Ein", "Tr\u00f6s\u00b7ter", "sol\u00b7chem", "Weh", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Schon halb erfa\u00dft von dem Verderben", "tokens": ["Schon", "halb", "er\u00b7fa\u00dft", "von", "dem", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Spr\u00e4ch'st du: Was kann ich dir mehr sein? \u2013", "tokens": ["Spr\u00e4ch'st", "du", ":", "Was", "kann", "ich", "dir", "mehr", "sein", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "PWS", "VMFIN", "PPER", "PPER", "ADV", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts bleibt dir \u00fcbrig als zu sterben \u2013", "tokens": ["Nichts", "bleibt", "dir", "\u00fcb\u00b7rig", "als", "zu", "ster\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "KOKOM", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stirb denn, wie du gelebt: allein! \u2013", "tokens": ["Stirb", "denn", ",", "wie", "du", "ge\u00b7lebt", ":", "al\u00b7lein", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VVIMP", "ADV", "$,", "PWAV", "PPER", "VVPP", "$.", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Das will ich! ja, so will ich's halten!", "tokens": ["Das", "will", "ich", "!", "ja", ",", "so", "will", "ich's", "hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "$.", "PTKANT", "$,", "ADV", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ob auch mein Herz vergeht und bricht;", "tokens": ["Ob", "auch", "mein", "Herz", "ver\u00b7geht", "und", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Gift, davon ich mu\u00df erkalten,", "tokens": ["Das", "Gift", ",", "da\u00b7von", "ich", "mu\u00df", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In fremden Becher fl\u00f6\u00df ich's nicht.", "tokens": ["In", "frem\u00b7den", "Be\u00b7cher", "fl\u00f6\u00df", "ich's", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nein, unser Weg ist nicht gemeinsam,", "tokens": ["Nein", ",", "un\u00b7ser", "Weg", "ist", "nicht", "ge\u00b7mein\u00b7sam", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn Gl\u00fcck und Hoffnung sind noch dein.", "tokens": ["Denn", "Gl\u00fcck", "und", "Hoff\u00b7nung", "sind", "noch", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So lebe wohl und la\u00df mich einsam", "tokens": ["So", "le\u00b7be", "wohl", "und", "la\u00df", "mich", "ein\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "KON", "VVIMP", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In meiner Todesstunde sein. \u2013", "tokens": ["In", "mei\u00b7ner", "To\u00b7dess\u00b7tun\u00b7de", "sein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}