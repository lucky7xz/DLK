, "measure": "trochaic.tetra"}, "line.7": {"text": "Meiner Sch\u00f6pfung Werk?", "tokens": ["Mei\u00b7ner", "Sch\u00f6p\u00b7fung", "Werk", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "...", "tokens": ["..."], "token_info": ["punct"], "pos": ["$("]}}}}}