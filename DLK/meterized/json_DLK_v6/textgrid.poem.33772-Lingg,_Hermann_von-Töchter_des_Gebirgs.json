{"textgrid.poem.33772": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "T\u00f6chter des Gebirgs", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "T\u00f6chter des Gebirgs, die braunen", "tokens": ["T\u00f6ch\u00b7ter", "des", "Ge\u00b7birgs", ",", "die", "brau\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Indierm\u00e4dchen nahn zum Baden", "tokens": ["In\u00b7dier\u00b7m\u00e4d\u00b7chen", "nahn", "zum", "Ba\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich des Stromes Felsgestaden.", "tokens": ["Sich", "des", "Stro\u00b7mes", "Fels\u00b7ge\u00b7sta\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sie lauschen, wie sie staunen", "tokens": ["Wie", "sie", "lau\u00b7schen", ",", "wie", "sie", "stau\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Vor den sch\u00e4umenden Kaskaden!", "tokens": ["Vor", "den", "sch\u00e4u\u00b7men\u00b7den", "Kas\u00b7ka\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ungewohnt nicht ist die Stelle,", "tokens": ["Un\u00b7ge\u00b7wohnt", "nicht", "ist", "die", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKNEG", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber heut schon eingenommen:", "tokens": ["A\u00b7ber", "heut", "schon", "ein\u00b7ge\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wilde Pferde sind gekommen,", "tokens": ["Wil\u00b7de", "Pfer\u00b7de", "sind", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pferde der Pr\u00e4rie, die schnellen,", "tokens": ["Pfer\u00b7de", "der", "Pr\u00e4\u00b7rie", ",", "die", "schnel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Haben Furt und Flut durchschwommen.", "tokens": ["Ha\u00b7ben", "Furt", "und", "Flut", "durch\u00b7schwom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wiehernd tummelt sich die Gruppe,", "tokens": ["Wie\u00b7hernd", "tum\u00b7melt", "sich", "die", "Grup\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Jagt sich in ein stillres Becken.", "tokens": ["Jagt", "sich", "in", "ein", "still\u00b7res", "Be\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie da die N\u00fcstern strecken", "tokens": ["Wie", "sie", "da", "die", "N\u00fcs\u00b7tern", "stre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eines auf des andern Kruppe", "tokens": ["Ei\u00b7nes", "auf", "des", "an\u00b7dern", "Krup\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sich schmeicheln und sich lecken!", "tokens": ["Und", "sich", "schmei\u00b7cheln", "und", "sich", "le\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVINF", "KON", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch die M\u00e4dchen, sie besinnen", "tokens": ["Doch", "die", "M\u00e4d\u00b7chen", ",", "sie", "be\u00b7sin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich nicht lang, und wie im Fluge", "tokens": ["Sich", "nicht", "lang", ",", "und", "wie", "im", "Flu\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "PTKNEG", "ADJD", "$,", "KON", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sitzt schon jede fest am Buge,", "tokens": ["Sitzt", "schon", "je\u00b7de", "fest", "am", "Bu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauter junge Kriegerinnen,", "tokens": ["Lau\u00b7ter", "jun\u00b7ge", "Krie\u00b7ge\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bergest\u00f6chter, rasche, kluge.", "tokens": ["Ber\u00b7ge\u00b7st\u00f6ch\u00b7ter", ",", "ra\u00b7sche", ",", "klu\u00b7ge", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wurfspeer' halten ihre Z\u00e4hne.", "tokens": ["Wurf\u00b7speer'", "hal\u00b7ten", "ih\u00b7re", "Z\u00e4h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Federn Schmuck umflogen", "tokens": ["Von", "der", "Fe\u00b7dern", "Schmuck", "um\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den nackten Arm gebogen", "tokens": ["Und", "den", "nack\u00b7ten", "Arm", "ge\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um der Rosse Hals und M\u00e4hne,", "tokens": ["Um", "der", "Ros\u00b7se", "Hals", "und", "M\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Reiten sie hindurch die Wogen.", "tokens": ["Rei\u00b7ten", "sie", "hin\u00b7durch", "die", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Von der Felsen Schlinggeh\u00e4nge,", "tokens": ["Von", "der", "Fel\u00b7sen", "Schling\u00b7ge\u00b7h\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vom Gezweig der Riesenb\u00e4ume", "tokens": ["Vom", "Ge\u00b7zweig", "der", "Rie\u00b7sen\u00b7b\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Brechen sie sich gr\u00fcne Z\u00e4ume.", "tokens": ["Bre\u00b7chen", "sie", "sich", "gr\u00fc\u00b7ne", "Z\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um das schwellende Gedr\u00e4nge", "tokens": ["Um", "das", "schwel\u00b7len\u00b7de", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Spr\u00fcht der Wasserfall die Sch\u00e4ume.", "tokens": ["Spr\u00fcht", "der", "Was\u00b7ser\u00b7fall", "die", "Sch\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}