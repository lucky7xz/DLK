{"textgrid.poem.59663": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann die leisen B\u00e4chlein rauschen,", "tokens": ["Wann", "die", "lei\u00b7sen", "B\u00e4ch\u00b7lein", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00e4useln durch die Bl\u00e4tter bebt,", "tokens": ["S\u00e4u\u00b7seln", "durch", "die", "Bl\u00e4t\u00b7ter", "bebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df ich horchen, mu\u00df ich lauschen,", "tokens": ["Mu\u00df", "ich", "hor\u00b7chen", ",", "mu\u00df", "ich", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob der Liebste niederschwebt;", "tokens": ["Ob", "der", "Liebs\u00b7te", "nie\u00b7der\u00b7schwebt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann die Fr\u00fchlingsv\u00f6glein singen,", "tokens": ["Wann", "die", "Fr\u00fch\u00b7lings\u00b7v\u00f6\u00b7glein", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die ganze Blumenflor", "tokens": ["Und", "die", "gan\u00b7ze", "Blu\u00b7men\u00b7flor"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur ein Bl\u00fchen ist und Klingen,", "tokens": ["Nur", "ein", "Bl\u00fc\u00b7hen", "ist", "und", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Singt und klingt und bl\u00fcht er nur.", "tokens": ["Singt", "und", "klingt", "und", "bl\u00fcht", "er", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ich rufe meinen Schmerzen", "tokens": ["Und", "ich", "ru\u00b7fe", "mei\u00b7nen", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter manchem lauten Ach:", "tokens": ["Un\u00b7ter", "man\u00b7chem", "lau\u00b7ten", "Ach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bl\u00fcht auch ihr! \u2013 Ich will euch herzen,", "tokens": ["Bl\u00fcht", "auch", "ihr", "!", "\u2013", "Ich", "will", "euch", "her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "$.", "$(", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werdet frisch im Lenze wach!", "tokens": ["Wer\u00b7det", "frisch", "im", "Len\u00b7ze", "wach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bringt die sch\u00f6nste meiner Gaben,", "tokens": ["Bringt", "die", "sch\u00f6ns\u00b7te", "mei\u00b7ner", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bringt mir das verlorne Gl\u00fcck,", "tokens": ["Bringt", "mir", "das", "ver\u00b7lor\u00b7ne", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bringt mir meinen s\u00fc\u00dfen Knaben", "tokens": ["Bringt", "mir", "mei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In der alten Pracht zur\u00fcck.", "tokens": ["In", "der", "al\u00b7ten", "Pracht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und die Tr\u00e4nen flie\u00dfen milder,", "tokens": ["Und", "die", "Tr\u00e4\u00b7nen", "flie\u00b7\u00dfen", "mil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es schmilzt das starre Herz,", "tokens": ["Und", "es", "schmilzt", "das", "star\u00b7re", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die holden Liebesbilder", "tokens": ["Und", "die", "hol\u00b7den", "Lie\u00b7bes\u00b7bil\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zaubert neu der neue Schmerz,", "tokens": ["Zau\u00b7bert", "neu", "der", "neu\u00b7e", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebesbilder, Liebesschatten,", "tokens": ["Lie\u00b7bes\u00b7bil\u00b7der", ",", "Lie\u00b7bes\u00b7schat\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie bev\u00f6lkern jeden Raum;", "tokens": ["Sie", "be\u00b7v\u00f6l\u00b7kern", "je\u00b7den", "Raum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was wir haben, was wir hatten,", "tokens": ["Was", "wir", "ha\u00b7ben", ",", "was", "wir", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was wir lieben, hei\u00dfet Traum.", "tokens": ["Was", "wir", "lie\u00b7ben", ",", "hei\u00b7\u00dfet", "Traum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach! Ein s\u00fc\u00dfer Traum, verdunkelt", "tokens": ["Ach", "!", "Ein", "s\u00fc\u00b7\u00dfer", "Traum", ",", "ver\u00b7dun\u00b7kelt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "$.", "ART", "ADJA", "NN", "$,", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Erde Nebelluft,", "tokens": ["In", "der", "Er\u00b7de", "Ne\u00b7bel\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dessen hellstes Bild erfunkelt,", "tokens": ["Des\u00b7sen", "hells\u00b7tes", "Bild", "er\u00b7fun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann wir weinen auf der Gruft:", "tokens": ["Wann", "wir", "wei\u00b7nen", "auf", "der", "Gruft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Erde m\u00fcssen wir begraben", "tokens": ["Er\u00b7de", "m\u00fcs\u00b7sen", "wir", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und was in uns irdisch ist,", "tokens": ["Und", "was", "in", "uns", "ir\u00b7disch", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wollen wir im Lichte haben,", "tokens": ["Wol\u00b7len", "wir", "im", "Lich\u00b7te", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was vom Himmel g\u00f6ttlich ist.", "tokens": ["Was", "vom", "Him\u00b7mel", "g\u00f6tt\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}