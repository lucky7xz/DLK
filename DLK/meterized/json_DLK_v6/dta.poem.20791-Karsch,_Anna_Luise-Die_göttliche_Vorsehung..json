{"dta.poem.20791": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Die g\u00f6ttliche Vorsehung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sey mein Gesang, du, die von Ewigkeit               ", "tokens": ["Sey", "mein", "Ge\u00b7sang", ",", "du", ",", "die", "von", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PPER", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Jubelschall die Morgensterne lobten,", "tokens": ["Mit", "Ju\u00b7bel\u00b7schall", "die", "Mor\u00b7gens\u00b7ter\u00b7ne", "lob\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Allsehende! die eher als die Zeit", "tokens": ["All\u00b7se\u00b7hen\u00b7de", "!", "die", "e\u00b7her", "als", "die", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "ADV", "KOUS", "ART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und eher war als Meer und Kriege tobten,", "tokens": ["Und", "e\u00b7her", "war", "als", "Meer", "und", "Krie\u00b7ge", "tob\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "KOKOM", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Durchdringe du mit deiner Wahrheit Licht", "tokens": ["Durch\u00b7drin\u00b7ge", "du", "mit", "dei\u00b7ner", "Wahr\u00b7heit", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den dicken Schley\u2019r, vor die Vernunft gebreitet,", "tokens": ["Den", "di\u00b7cken", "Schley'r", ",", "vor", "die", "Ver\u00b7nunft", "ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und sey du selbst mein gro\u00dfer Unterricht,", "tokens": ["Und", "sey", "du", "selbst", "mein", "gro\u00b7\u00dfer", "Un\u00b7ter\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wenn sich mein Herz zu deinem Lob bereitet.", "tokens": ["Wenn", "sich", "mein", "Herz", "zu", "dei\u00b7nem", "Lob", "be\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dich l\u00e4ugnet frech der Unsinn und der Spott,", "tokens": ["Dich", "l\u00e4ug\u00b7net", "frech", "der", "Un\u00b7sinn", "und", "der", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Wurm, den du aus seinem Nichts gezogen;", "tokens": ["Der", "Wurm", ",", "den", "du", "aus", "sei\u00b7nem", "Nichts", "ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die L\u00fcste sind sein Himmel und sein Gott,", "tokens": ["Die", "L\u00fcs\u00b7te", "sind", "sein", "Him\u00b7mel", "und", "sein", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und in ihm baut das Laster Ehrenbogen.", "tokens": ["Und", "in", "ihm", "baut", "das", "Las\u00b7ter", "Eh\u00b7ren\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sein d\u00fcstrer Wahn der taumelt blind vorbey,", "tokens": ["Sein", "d\u00fcst\u00b7rer", "Wahn", "der", "tau\u00b7melt", "blind", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und die Natur wird nicht von ihm geh\u00f6ret,", "tokens": ["Und", "die", "Na\u00b7tur", "wird", "nicht", "von", "ihm", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PTKNEG", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die doch von dir mit zeugendem Geschrey,", "tokens": ["Die", "doch", "von", "dir", "mit", "zeu\u00b7gen\u00b7dem", "Ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit Harmonie und tausend Zungen lehret.", "tokens": ["Mit", "Har\u00b7mo\u00b7nie", "und", "tau\u00b7send", "Zun\u00b7gen", "leh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Eh du die Welt voll Ordnung und voll Pracht", "tokens": ["Eh", "du", "die", "Welt", "voll", "Ord\u00b7nung", "und", "voll", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hervorgeruft und Menschen werden lie\u00dfest,", "tokens": ["Her\u00b7vor\u00b7ge\u00b7ruft", "und", "Men\u00b7schen", "wer\u00b7den", "lie\u00b7\u00dfest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch ehe du den Staub beseelt gemacht", "tokens": ["Noch", "e\u00b7he", "du", "den", "Staub", "be\u00b7seelt", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ihn den Rang nach Engeln nehmen hie\u00dfest,", "tokens": ["Und", "ihn", "den", "Rang", "nach", "En\u00b7geln", "neh\u00b7men", "hie\u00b7\u00dfest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "APPR", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da lagen schon Jahrhunderte vor dir.", "tokens": ["Da", "la\u00b7gen", "schon", "Jahr\u00b7hun\u00b7der\u00b7te", "vor", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du sahst das Buch der Weltbegebenheiten,", "tokens": ["Du", "sahst", "das", "Buch", "der", "Welt\u00b7be\u00b7ge\u00b7ben\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und nanntest schon die Menschen, die in ihr", "tokens": ["Und", "nann\u00b7test", "schon", "die", "Men\u00b7schen", ",", "die", "in", "ihr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wie G\u00f6tter seyn und auf die Thronen schreiten!", "tokens": ["Wie", "G\u00f6t\u00b7ter", "seyn", "und", "auf", "die", "Thro\u00b7nen", "schrei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAINF", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Eh die Natur in ihre Werkstatt ging,", "tokens": ["Eh", "die", "Na\u00b7tur", "in", "ih\u00b7re", "Werk\u00b7statt", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und Gold und Stein den Glanz zu Kronen machte;", "tokens": ["Und", "Gold", "und", "Stein", "den", "Glanz", "zu", "Kro\u00b7nen", "mach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Eh Purpur noch um eine Schulter hing,", "tokens": ["Eh", "Pur\u00b7pur", "noch", "um", "ei\u00b7ne", "Schul\u00b7ter", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und eh\u2019 der Pomp den Herrschern Lasten brachte,", "tokens": ["Und", "eh'", "der", "Pomp", "den", "Herr\u00b7schern", "Las\u00b7ten", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da kanntest Du die Herrscher und die Last,", "tokens": ["Da", "kann\u00b7test", "Du", "die", "Herr\u00b7scher", "und", "die", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und V\u00f6lker, die sich vor dem Scepter beugen:", "tokens": ["Und", "V\u00f6l\u00b7ker", ",", "die", "sich", "vor", "dem", "Scep\u00b7ter", "beu\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dein war das Heft, das der Regente fa\u00dft,", "tokens": ["Dein", "war", "das", "Heft", ",", "das", "der", "Re\u00b7gen\u00b7te", "fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Du gabst es ihm, um deine Macht zu zeigen.", "tokens": ["Du", "gabst", "es", "ihm", ",", "um", "dei\u00b7ne", "Macht", "zu", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "$,", "KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Geme\u00dfne Gr\u00e4nzen setztest du dem Meer;", "tokens": ["Ge\u00b7me\u00df\u00b7ne", "Gr\u00e4n\u00b7zen", "setz\u00b7test", "du", "dem", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Meer gehorcht, dich h\u00f6rt die stolze Welle,", "tokens": ["Das", "Meer", "ge\u00b7horcht", ",", "dich", "h\u00f6rt", "die", "stol\u00b7ze", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Ungewitter brauste sie daher;", "tokens": ["Im", "Un\u00b7ge\u00b7wit\u00b7ter", "braus\u00b7te", "sie", "da\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie st\u00fcrzt zur\u00fcck und \u00fcber ihr wirds helle!", "tokens": ["Sie", "st\u00fcrzt", "zu\u00b7r\u00fcck", "und", "\u00fc\u00b7ber", "ihr", "wirds", "hel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "APPR", "PPER", "VAFIN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So setzest du dem Uebel in der Welt", "tokens": ["So", "set\u00b7zest", "du", "dem", "Ue\u00b7bel", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den Gr\u00e4nzstein hin. Es kennet ihn und schreitet", "tokens": ["Den", "Gr\u00e4nz\u00b7stein", "hin", ".", "Es", "ken\u00b7net", "ihn", "und", "schrei\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nicht weiter fort. Sobald es dir gef\u00e4llt,", "tokens": ["Nicht", "wei\u00b7ter", "fort", ".", "So\u00b7bald", "es", "dir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKVZ", "$.", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Bricht Schwerdt und Spie\u00df, und Ruhe wird verbreitet.", "tokens": ["Bricht", "Schwerdt", "und", "Spie\u00df", ",", "und", "Ru\u00b7he", "wird", "ver\u00b7brei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "$,", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Dein Griffel schrieb des Schicksals Tafeln voll", "tokens": ["Dein", "Grif\u00b7fel", "schrieb", "des", "Schick\u00b7sals", "Ta\u00b7feln", "voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem hellsten Wiz der Sterblichen verborgen,", "tokens": ["Dem", "hells\u00b7ten", "Wiz", "der", "Sterb\u00b7li\u00b7chen", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Steht alles da, was hier geschehen soll;", "tokens": ["Steht", "al\u00b7les", "da", ",", "was", "hier", "ge\u00b7sche\u00b7hen", "soll", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Menschen Gl\u00fcck, Vergn\u00fcgen, Noth und Sorgen,", "tokens": ["Des", "Men\u00b7schen", "Gl\u00fcck", ",", "Ver\u00b7gn\u00fc\u00b7gen", ",", "Noth", "und", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Kein Ohngef\u00e4hr macht unsre Fluren reich:", "tokens": ["Kein", "Ohn\u00b7ge\u00b7f\u00e4hr", "macht", "uns\u00b7re", "Flu\u00b7ren", "reich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kein Zufall bringt den Mangel und die F\u00fclle,", "tokens": ["Kein", "Zu\u00b7fall", "bringt", "den", "Man\u00b7gel", "und", "die", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nicht Kunst, nicht Flei\u00df macht unsre Aecker weich;", "tokens": ["Nicht", "Kunst", ",", "nicht", "Flei\u00df", "macht", "uns\u00b7re", "A\u00b7e\u00b7cker", "weich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"