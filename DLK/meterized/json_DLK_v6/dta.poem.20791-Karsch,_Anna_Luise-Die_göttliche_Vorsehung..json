", "ein", "Bach", "auf", "mich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "PRF", "ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch hast du mir nichts weiter aufgehoben,", "tokens": ["Doch", "hast", "du", "mir", "nichts", "wei\u00b7ter", "auf\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So gieb mir nur Zufriedenheit durch dich,", "tokens": ["So", "gieb", "mir", "nur", "Zu\u00b7frie\u00b7den\u00b7heit", "durch", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und sey mein Lied auf Erden und dort oben.", "tokens": ["Und", "sey", "mein", "Lied", "auf", "Er\u00b7den", "und", "dort", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}