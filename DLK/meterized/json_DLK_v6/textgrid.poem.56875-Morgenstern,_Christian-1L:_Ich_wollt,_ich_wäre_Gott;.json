{"textgrid.poem.56875": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wollt, ich w\u00e4re Gott;", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wollt, ich w\u00e4re Gott;", "tokens": ["Ich", "wollt", ",", "ich", "w\u00e4\u00b7re", "Gott", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "denn Mensch sein", "tokens": ["denn", "Mensch", "sein"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "PPOSAT"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "hei\u00dft Prahler sein.", "tokens": ["hei\u00dft", "Prah\u00b7ler", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "In Gedanken", "tokens": ["In", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "mit Sternen spielen \u2013", "tokens": ["mit", "Ster\u00b7nen", "spie\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Spiel f\u00fcr Dichter", "tokens": ["Spiel", "f\u00fcr", "Dich\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "und W\u00e4scherinnen.", "tokens": ["und", "W\u00e4\u00b7sche\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Aber w\u00e4r ich Gott ...", "tokens": ["A\u00b7ber", "w\u00e4r", "ich", "Gott", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "ich griffe mir", "tokens": ["ich", "grif\u00b7fe", "mir"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "per exemplum", "tokens": ["per", "ex\u00b7emp\u00b7lum"], "token_info": ["word", "word"], "pos": ["FM.la", "FM.la"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "ein violettbestrumpftes,", "tokens": ["ein", "vi\u00b7o\u00b7lett\u00b7be\u00b7strumpf\u00b7tes", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "schnupftabaktalariges,", "tokens": ["schnupf\u00b7ta\u00b7bak\u00b7ta\u00b7la\u00b7ri\u00b7ges", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "z\u00f6libatbettiges", "tokens": ["z\u00f6\u00b7li\u00b7bat\u00b7bet\u00b7ti\u00b7ges"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Pf\u00e4fflein", "tokens": ["Pf\u00e4f\u00b7flein"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "von irgend einem", "tokens": ["von", "ir\u00b7gend", "ei\u00b7nem"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADV", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "bigotten Planeten,", "tokens": ["bi\u00b7got\u00b7ten", "Pla\u00b7ne\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.10": {"text": "macht' es so gro\u00df", "tokens": ["macht'", "es", "so", "gro\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "wie mich selber", "tokens": ["wie", "mich", "sel\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "und h\u00e4ngt' ihm dann", "tokens": ["und", "h\u00e4ngt'", "ihm", "dann"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "das ganze Sternall", "tokens": ["das", "gan\u00b7ze", "Ster\u00b7nall"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "als Rosenkranz", "tokens": ["als", "Ro\u00b7sen\u00b7kranz"], "token_info": ["word", "word"], "pos": ["KOUS", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "\u00fcber die Hand \u2013", "tokens": ["\u00fc\u00b7ber", "die", "Hand", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "es abzubeten.", "tokens": ["es", "ab\u00b7zu\u00b7be\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVIZU", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "\u00bbhurtig, hurtig!", "tokens": ["\u00bb", "hur\u00b7tig", ",", "hur\u00b7tig", "!"], "token_info": ["punct", "word", "punct", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "Dein Lohn ist", "tokens": ["Dein", "Lohn", "ist"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "die ewige Seligkeit!\u00ab", "tokens": ["die", "e\u00b7wi\u00b7ge", "Se\u00b7lig\u00b7keit", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbaber Herr, Herr ...\u00ab", "tokens": ["\u00bb", "a\u00b7ber", "Herr", ",", "Herr", "...", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ADV", "NN", "$,", "NN", "$(", "$("], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "\u00bbnichts da! Gebetet!\u00ab", "tokens": ["\u00bb", "nichts", "da", "!", "Ge\u00b7be\u00b7tet", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PIS", "ADV", "$.", "VVPP", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Ach! da\u00df ich Mensch bin, \u2013", "tokens": ["Ach", "!", "da\u00df", "ich", "Mensch", "bin", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "NN", "VAFIN", "$,", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "ein Murmeltier,", "tokens": ["ein", "Mur\u00b7mel\u00b7tier", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "auf den Alpen", "tokens": ["auf", "den", "Al\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "passiver Begriffe.", "tokens": ["pas\u00b7si\u00b7ver", "Be\u00b7grif\u00b7fe", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}