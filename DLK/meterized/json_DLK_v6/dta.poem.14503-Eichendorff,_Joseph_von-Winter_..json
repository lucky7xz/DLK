{"dta.poem.14503": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Winter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie von Nacht verhangen,", "tokens": ["Wie", "von", "Nacht", "ver\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wu\u00dft' nicht, was ich will,", "tokens": ["Wu\u00dft'", "nicht", ",", "was", "ich", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Schon so lange, lange", "tokens": ["Schon", "so", "lan\u00b7ge", ",", "lan\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "ADV", "$,", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "War ich todtenstill.", "tokens": ["War", "ich", "tod\u00b7ten\u00b7still", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}