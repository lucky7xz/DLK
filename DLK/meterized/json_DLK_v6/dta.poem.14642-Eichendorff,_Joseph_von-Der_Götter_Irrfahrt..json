{"dta.poem.14642": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der G\u00f6tter Irrfahrt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Staunend auf den G\u00f6ttersitzen", "tokens": ["Stau\u00b7nend", "auf", "den", "G\u00f6t\u00b7ter\u00b7sit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Unsterblichen nun stehn,", "tokens": ["Die", "U\u00b7nsterb\u00b7li\u00b7chen", "nun", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Seh'n den Morgen dr\u00fcben blitzen,", "tokens": ["Seh'n", "den", "Mor\u00b7gen", "dr\u00fc\u00b7ben", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchlen Duft her\u00fcberweh'n,", "tokens": ["F\u00fch\u00b7len", "Duft", "her\u00b7\u00fcber\u00b7weh'n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und so s\u00fc\u00dfes Weh sie sp\u00fcren,", "tokens": ["Und", "so", "s\u00fc\u00b7\u00dfes", "Weh", "sie", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00f6sen leis ihr Schiff vom Strand,", "tokens": ["L\u00f6\u00b7sen", "leis", "ihr", "Schiff", "vom", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die L\u00fcfte sie verf\u00fchren", "tokens": ["Und", "die", "L\u00fcf\u00b7te", "sie", "ver\u00b7f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPER", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Fern durch's Meer zum jungen Land.", "tokens": ["Fern", "durch's", "Meer", "zum", "jun\u00b7gen", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}