{"dta.poem.19171": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nach Sonne, Mond und Stern in ihrem Strahlenblitze", "tokens": ["Nach", "Son\u00b7ne", ",", "Mond", "und", "Stern", "in", "ih\u00b7rem", "Strah\u00b7len\u00b7blit\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O deute nicht empor mit deiner Fingerspitze!", "tokens": ["O", "deu\u00b7te", "nicht", "em\u00b7por", "mit", "dei\u00b7ner", "Fin\u00b7ger\u00b7spit\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nach ihnen sp\u00e4hend hebt der Astronom sein Rohr,", "tokens": ["Nach", "ih\u00b7nen", "sp\u00e4\u00b7hend", "hebt", "der", "Ast\u00b7ro\u00b7nom", "sein", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du aber schaue nur mit frommem Aug' empor.", "tokens": ["Du", "a\u00b7ber", "schau\u00b7e", "nur", "mit", "from\u00b7mem", "Aug'", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn du hast ihre Bahn nicht ihnen vorzuschreiben;", "tokens": ["Denn", "du", "hast", "ih\u00b7re", "Bahn", "nicht", "ih\u00b7nen", "vor\u00b7zu\u00b7schrei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN", "PTKNEG", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du gehst darunter weg, und sie dort oben bleiben.", "tokens": ["Du", "gehst", "da\u00b7run\u00b7ter", "weg", ",", "und", "sie", "dort", "o\u00b7ben", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PTKVZ", "$,", "KON", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}