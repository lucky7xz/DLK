{"textgrid.poem.29248": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du t\u00f6dtest, Buchstabe;", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du t\u00f6dtest, Buchstabe;", "tokens": ["Du", "t\u00f6d\u00b7test", ",", "Buch\u00b7sta\u00b7be", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wem graut f\u00fcr dem Grabe,", "tokens": ["Wem", "graut", "f\u00fcr", "dem", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der lasse dich bleiben!", "tokens": ["Der", "las\u00b7se", "dich", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Drum h\u00fcten die Leute", "tokens": ["Drum", "h\u00fc\u00b7ten", "die", "Leu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "So fleissig sich heute", "tokens": ["So", "fleis\u00b7sig", "sich", "heu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PRF", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "F\u00fcr lesen und schreiben.", "tokens": ["F\u00fcr", "le\u00b7sen", "und", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}