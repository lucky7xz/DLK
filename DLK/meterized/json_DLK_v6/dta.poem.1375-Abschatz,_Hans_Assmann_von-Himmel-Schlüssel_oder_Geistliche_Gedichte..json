{"dta.poem.1375": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Das junge Jahr bekr\u00f6nt unl\u00e4ngst ein Blumen-Strau\u00df/", "tokens": ["Das", "jun\u00b7ge", "Jahr", "be\u00b7kr\u00f6nt", "un\u00b7l\u00e4ngst", "ein", "Blu\u00b7men\u00b7Strau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es legte seine Pracht im bunten Mayen aus;", "tokens": ["Es", "leg\u00b7te", "sei\u00b7ne", "Pracht", "im", "bun\u00b7ten", "Ma\u00b7yen", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bald wie\u00df es mehr erst\u00e4rckt der Felder gelbes Haar/", "tokens": ["Bald", "wie\u00df", "es", "mehr", "er\u00b7st\u00e4rckt", "der", "Fel\u00b7der", "gel\u00b7bes", "Haar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Leib/ der von der Gunst des Himmels fruchtbar war/", "tokens": ["Der", "Leib", "/", "der", "von", "der", "Gunst", "des", "Him\u00b7mels", "frucht\u00b7bar", "war", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "ART", "NN", "ART", "NN", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Schickt/ an der Seuffzer statt/ aus seiner heissen Scho\u00df", "tokens": ["Schickt", "/", "an", "der", "Seuff\u00b7zer", "statt", "/", "aus", "sei\u00b7ner", "heis\u00b7sen", "Scho\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "APPR", "ART", "NN", "PTKVZ", "$(", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die D\u00fcnste nach der Lufft/ draus Thau und Regen flo\u00df.", "tokens": ["Die", "D\u00fcns\u00b7te", "nach", "der", "Lufft", "/", "draus", "Thau", "und", "Re\u00b7gen", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "PAV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bald kleidet sich sein Herbst mit braunen Fr\u00fcchten an/", "tokens": ["Bald", "klei\u00b7det", "sich", "sein", "Herbst", "mit", "brau\u00b7nen", "Fr\u00fcch\u00b7ten", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gab was uns s\u00e4ttigen/ gab was uns tr\u00e4ncken kan.", "tokens": ["Gab", "was", "uns", "s\u00e4t\u00b7ti\u00b7gen", "/", "gab", "was", "uns", "tr\u00e4n\u00b7cken", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "PPER", "VVINF", "$(", "VVFIN", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.9": {"text": "Nun ist sein frostig Haubt voll Runtzeln und beschneyt:", "tokens": ["Nun", "ist", "sein", "fros\u00b7tig", "Haubt", "voll", "Runt\u00b7zeln", "und", "be\u00b7schneyt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJD", "NN", "ADJD", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So eilends \u00e4ndert sich auch unser Stand und Zeit!", "tokens": ["So", "ei\u00b7lends", "\u00e4n\u00b7dert", "sich", "auch", "un\u00b7ser", "Stand", "und", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ADV", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Jugend Bl\u00fctte geht/ den Blumen gleich/ vorbey/", "tokens": ["Der", "Ju\u00b7gend", "Bl\u00fct\u00b7te", "geht", "/", "den", "Blu\u00b7men", "gleich", "/", "vor\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$(", "ART", "NN", "ADV", "$(", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Mannschafft Sommer f\u00fchlt/ was Kummers Hitze sey/", "tokens": ["Der", "Mann\u00b7schafft", "Som\u00b7mer", "f\u00fchlt", "/", "was", "Kum\u00b7mers", "Hit\u00b7ze", "sey", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$(", "PWS", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wenn sich der Jahre Herbst will schicken zum Genu\u00df/", "tokens": ["Wenn", "sich", "der", "Jah\u00b7re", "Herbst", "will", "schi\u00b7cken", "zum", "Ge\u00b7nu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "NN", "VMFIN", "VVINF", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Erstarrt der Glieder Ei\u00df/ und folgt des Lebens Schlu\u00df.", "tokens": ["Er\u00b7starrt", "der", "Glie\u00b7der", "Ei\u00df", "/", "und", "folgt", "des", "Le\u00b7bens", "Schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$(", "KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}