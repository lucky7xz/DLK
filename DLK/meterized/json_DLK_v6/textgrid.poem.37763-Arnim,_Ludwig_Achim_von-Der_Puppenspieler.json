{"textgrid.poem.37763": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Puppenspieler", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist f\u00fcr Freude noch bei gro\u00dfen B\u00fchnen,", "tokens": ["Was", "ist", "f\u00fcr", "Freu\u00b7de", "noch", "bei", "gro\u00b7\u00dfen", "B\u00fch\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ist nichts Lustges mehr, kein wild Erk\u00fchnen,", "tokens": ["Da", "ist", "nichts", "Lust\u00b7ges", "mehr", ",", "kein", "wild", "Er\u00b7k\u00fch\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "NN", "ADV", "$,", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch ich war einst dabei, hab mitgemacht,", "tokens": ["Auch", "ich", "war", "einst", "da\u00b7bei", ",", "hab", "mit\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "PAV", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und hab in Jahren nicht dabei gelacht.", "tokens": ["Und", "hab", "in", "Jah\u00b7ren", "nicht", "da\u00b7bei", "ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "PTKNEG", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die guten alten Spieler werden schwach,", "tokens": ["Die", "gu\u00b7ten", "al\u00b7ten", "Spie\u00b7ler", "wer\u00b7den", "schwach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und ach das junge Volk w\u00e4chst schw\u00e4chlich nach,", "tokens": ["Und", "ach", "das", "jun\u00b7ge", "Volk", "w\u00e4chst", "schw\u00e4ch\u00b7lich", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was kann die Welt f\u00fcr Lust an Kindern haben?", "tokens": ["Was", "kann", "die", "Welt", "f\u00fcr", "Lust", "an", "Kin\u00b7dern", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "APPR", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es dankt das Publikum f\u00fcr k\u00fcnftige Gaben,", "tokens": ["Es", "dankt", "das", "Pub\u00b7li\u00b7kum", "f\u00fcr", "k\u00fcnf\u00b7ti\u00b7ge", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Will Fertges sehn; was sich erst bilden soll,", "tokens": ["Will", "Fert\u00b7ges", "sehn", ";", "was", "sich", "erst", "bil\u00b7den", "soll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VVINF", "$.", "PWS", "PRF", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das mache kein Geschrei, sonst hei\u00dft es toll.", "tokens": ["Das", "ma\u00b7che", "kein", "Ge\u00b7schrei", ",", "sonst", "hei\u00dft", "es", "toll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Den Kindern springt die Quint, wie ichs geh\u00f6rt", "tokens": ["Den", "Kin\u00b7dern", "springt", "die", "Quint", ",", "wie", "ichs", "ge\u00b7h\u00f6rt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PWAV", "PIS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Das Publikum ward ganz von Ha\u00df beth\u00f6rt,", "tokens": ["Das", "Pub\u00b7li\u00b7kum", "ward", "ganz", "von", "Ha\u00df", "be\u00b7th\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Es pocht, es l\u00e4rmt, und keiner schien mehr recht,", "tokens": ["Es", "pocht", ",", "es", "l\u00e4rmt", ",", "und", "kei\u00b7ner", "schien", "mehr", "recht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "KON", "PIS", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Es flohn die Schauspielleut aus dem Gefecht.", "tokens": ["Es", "flohn", "die", "Schau\u00b7spiel\u00b7leut", "aus", "dem", "Ge\u00b7fecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Da nahm ich nun mein Tuch, macht einen Knoten,", "tokens": ["Da", "nahm", "ich", "nun", "mein", "Tuch", ",", "macht", "ei\u00b7nen", "Kno\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und hab ein Kinderspiel dem Volk geboten,", "tokens": ["Und", "hab", "ein", "Kin\u00b7der\u00b7spiel", "dem", "Volk", "ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und wackelte damit und lie\u00df es tanzen,", "tokens": ["Und", "wa\u00b7ckel\u00b7te", "da\u00b7mit", "und", "lie\u00df", "es", "tan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "KON", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Ich ward vergn\u00fcgt und es gefiel im Ganzen.", "tokens": ["Ich", "ward", "ver\u00b7gn\u00fcgt", "und", "es", "ge\u00b7fiel", "im", "Gan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Ich nahm das Buch recht wie ein Kind in Lehre,", "tokens": ["Ich", "nahm", "das", "Buch", "recht", "wie", "ein", "Kind", "in", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KOKOM", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Als obs das Publikum, das edle w\u00e4re,", "tokens": ["Als", "obs", "das", "Pub\u00b7li\u00b7kum", ",", "das", "ed\u00b7le", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "$,", "ART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Und fragt es aus, wie es uns m\u00f6chte haben?", "tokens": ["Und", "fragt", "es", "aus", ",", "wie", "es", "uns", "m\u00f6ch\u00b7te", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "PPER", "PPER", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Da sprachs so viel von hohen K\u00fcnstlergaben,", "tokens": ["Da", "sprachs", "so", "viel", "von", "ho\u00b7hen", "K\u00fcnst\u00b7ler\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Doch wu\u00dft es nicht, wo die zu Kaufe w\u00e4ren;", "tokens": ["Doch", "wu\u00dft", "es", "nicht", ",", "wo", "die", "zu", "Kau\u00b7fe", "w\u00e4\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "ART", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Da mu\u00dfte ich es billig drin belehren,", "tokens": ["Da", "mu\u00df\u00b7te", "ich", "es", "bil\u00b7lig", "drin", "be\u00b7leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "\u00bbdie Kunst ist frei, sie brauchet viel Theater,", "tokens": ["\u00bb", "die", "Kunst", "ist", "frei", ",", "sie", "brau\u00b7chet", "viel", "The\u00b7a\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.27": {"text": "Wenn jenes reif, da tret' es hier erst ein!", "tokens": ["Wenn", "je\u00b7nes", "reif", ",", "da", "tret'", "es", "hier", "erst", "ein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADJD", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Doch weil f\u00fcr jetzt dies Schauspielhaus allein,", "tokens": ["Doch", "weil", "f\u00fcr", "jetzt", "dies", "Schau\u00b7spiel\u00b7haus", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ADV", "PDS", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "So m\u00fc\u00dft ihr auch den Sch\u00fclern gn\u00e4dig sein.\u00ab", "tokens": ["So", "m\u00fc\u00dft", "ihr", "auch", "den", "Sch\u00fc\u00b7lern", "gn\u00e4\u00b7dig", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Auf dieses Wort folgt Klatschen allgemein,", "tokens": ["Auf", "die\u00b7ses", "Wort", "folgt", "Klat\u00b7schen", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Ei dachte ich, und konnt es gar nicht fassen,", "tokens": ["Ei", "dach\u00b7te", "ich", ",", "und", "konnt", "es", "gar", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "KON", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Dies Schnupftuch kann jetzt mehr als K\u00fcnstler spa\u00dfen;", "tokens": ["Dies", "Schnupf\u00b7tuch", "kann", "jetzt", "mehr", "als", "K\u00fcnst\u00b7ler", "spa\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VMFIN", "ADV", "PIAT", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Die K\u00fcnstler sind zum Spa\u00df zu vornehm worden,", "tokens": ["Die", "K\u00fcnst\u00b7ler", "sind", "zum", "Spa\u00df", "zu", "vor\u00b7nehm", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "PTKA", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Und doch nicht gro\u00df genug zum tragschen Morden.", "tokens": ["Und", "doch", "nicht", "gro\u00df", "ge\u00b7nug", "zum", "trag\u00b7schen", "Mor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADJD", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Ich ging davon und machte kleine Puppen;", "tokens": ["Ich", "ging", "da\u00b7von", "und", "mach\u00b7te", "klei\u00b7ne", "Pup\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Viel hatt ich nicht zu brocken in die Suppen,", "tokens": ["Viel", "hatt", "ich", "nicht", "zu", "bro\u00b7cken", "in", "die", "Sup\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Doch essen auch nicht viel die kleinen Leut,", "tokens": ["Doch", "es\u00b7sen", "auch", "nicht", "viel", "die", "klei\u00b7nen", "Leut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Sie sind zu jeder Rolle stets bereit,", "tokens": ["Sie", "sind", "zu", "je\u00b7der", "Rol\u00b7le", "stets", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Um Kleider ist kein Streit, auch nicht um Tugend,", "tokens": ["Um", "Klei\u00b7der", "ist", "kein", "Streit", ",", "auch", "nicht", "um", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VAFIN", "PIAT", "NN", "$,", "ADV", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Auch nicht um Liebhaber, auch nicht um Jugend.", "tokens": ["Auch", "nicht", "um", "Lieb\u00b7ha\u00b7ber", ",", "auch", "nicht", "um", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "NN", "$,", "ADV", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Sie sind so alt, wie ich sie eben brauch,", "tokens": ["Sie", "sind", "so", "alt", ",", "wie", "ich", "sie", "e\u00b7ben", "brauch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PWAV", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Die wei\u00dfgenasten h\u00e4ng ich in den Rauch.", "tokens": ["Die", "wei\u00df\u00b7ge\u00b7nas\u00b7ten", "h\u00e4ng", "ich", "in", "den", "Rauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Mein Kopf f\u00fcllt mein Theater ganz allein;", "tokens": ["Mein", "Kopf", "f\u00fcllt", "mein", "The\u00b7a\u00b7ter", "ganz", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Sind meine Menschen gegen mich nur klein,", "tokens": ["Sind", "mei\u00b7ne", "Men\u00b7schen", "ge\u00b7gen", "mich", "nur", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "So bin ich darum wahrlich gro\u00df zu nennen,", "tokens": ["So", "bin", "ich", "da\u00b7rum", "wahr\u00b7lich", "gro\u00df", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Kann sie verbinden, und sie trennen,", "tokens": ["Kann", "sie", "ver\u00b7bin\u00b7den", ",", "und", "sie", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "KON", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Nach Eigensinn und nach Verstand", "tokens": ["Nach", "Ei\u00b7gen\u00b7sinn", "und", "nach", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Und bin ein rechter Gott in diesem Land;", "tokens": ["Und", "bin", "ein", "rech\u00b7ter", "Gott", "in", "die\u00b7sem", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.49": {"text": "Wei\u00df ich nichts mehr aus meinem Kopf zu sagen,", "tokens": ["Wei\u00df", "ich", "nichts", "mehr", "aus", "mei\u00b7nem", "Kopf", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "ADV", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.50": {"text": "So brauchen sie nur t\u00fcchtig sich zu schlagen,", "tokens": ["So", "brau\u00b7chen", "sie", "nur", "t\u00fcch\u00b7tig", "sich", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.51": {"text": "Und weil mein Casperl trefflich Tritte giebt,", "tokens": ["Und", "weil", "mein", "Cas\u00b7perl", "treff\u00b7lich", "Trit\u00b7te", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.52": {"text": "So schweigt Kritik und ich bin stets beliebt,", "tokens": ["So", "schweigt", "Kri\u00b7tik", "und", "ich", "bin", "stets", "be\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.53": {"text": "Ein jeder lacht, ein jeder giebt sein Geld,", "tokens": ["Ein", "je\u00b7der", "lacht", ",", "ein", "je\u00b7der", "giebt", "sein", "Geld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "ART", "PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.54": {"text": "Jetzt ist mein Casperl hier der gr\u00f6\u00dfte Held.", "tokens": ["Jetzt", "ist", "mein", "Cas\u00b7perl", "hier", "der", "gr\u00f6\u00df\u00b7te", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}