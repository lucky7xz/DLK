{"textgrid.poem.47850": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "An Klaus Groth", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn't Abend ward,", "tokens": ["Wenn't", "A\u00b7bend", "ward", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Un still de Welt un still dat Hart;", "tokens": ["Un", "still", "de", "Welt", "un", "still", "dat", "Hart", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn m\u00f6d up't Knee di liggt de Hand,", "tokens": ["Wenn", "m\u00f6d", "up't", "Knee", "di", "liggt", "de", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "FM", "FM", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Un ut din Husklock an de Wand", "tokens": ["Un", "ut", "din", "Hus\u00b7klock", "an", "de", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "----+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Du h\u00f6rst den Parpendikelslag,", "tokens": ["Du", "h\u00f6rst", "den", "Par\u00b7pen\u00b7di\u00b7kels\u00b7lag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "De nich to Woort keem \u00f6ver Dag;", "tokens": ["De", "nich", "to", "Woort", "keem", "\u00f6\u00b7ver", "Dag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn't Schummern in de Ecken liggt,", "tokens": ["Wenn't", "Schum\u00b7mern", "in", "de", "E\u00b7cken", "liggt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Un buten all de Nachtswulk fl\u00fcggt;", "tokens": ["Un", "bu\u00b7ten", "all", "de", "Nachts\u00b7wulk", "fl\u00fcggt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn denn noch eenmal kiekt de S\u00fcnn", "tokens": ["Wenn", "denn", "noch", "e\u00b7en\u00b7mal", "kiekt", "de", "S\u00fcnn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ADV", "VVFIN", "NE", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Mit golden Schiin to't Finster rin,", "tokens": ["Mit", "gol\u00b7den", "Schi\u00b7in", "to't", "Fins\u00b7ter", "rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Un, ehr de Slap k\u00fcmmt un de Nacht,", "tokens": ["Un", ",", "ehr", "de", "Slap", "k\u00fcmmt", "un", "de", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "NE", "NE", "VVFIN", "FM", "FM", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Noch eenmal allens l\u00e4vt un lacht \u2013", "tokens": ["Noch", "e\u00b7en\u00b7mal", "al\u00b7lens", "l\u00e4vt", "un", "lacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "FM", "VVFIN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Dat is so wat v\u00f6r't Minschenhart,", "tokens": ["Dat", "is", "so", "wat", "v\u00f6r't", "Min\u00b7schen\u00b7hart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wenn't Abend ward.", "tokens": ["Wenn't", "A\u00b7bend", "ward", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}