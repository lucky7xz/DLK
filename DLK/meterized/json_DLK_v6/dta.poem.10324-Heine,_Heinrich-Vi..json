{"dta.poem.10324": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Im s\u00fc\u00dfen Traum bei stiller Nacht,", "tokens": ["Im", "s\u00fc\u00b7\u00dfen", "Traum", "bei", "stil\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da kam zu mir, mit Zauberpracht,", "tokens": ["Da", "kam", "zu", "mir", ",", "mit", "Zau\u00b7ber\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die lang ersehnte Liebste mein,", "tokens": ["Die", "lang", "er\u00b7sehn\u00b7te", "Liebs\u00b7te", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und go\u00df mir Gluth in's Herz hinein.", "tokens": ["Und", "go\u00df", "mir", "Gluth", "in's", "Herz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wie ich schau', ergl\u00fch ich wild", "tokens": ["Und", "wie", "ich", "schau'", ",", "er\u00b7gl\u00fch", "ich", "wild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie ich schau, sie l\u00e4chelt mild,", "tokens": ["Und", "wie", "ich", "schau", ",", "sie", "l\u00e4\u00b7chelt", "mild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und l\u00e4chelt bis das Herz mir schwoll,", "tokens": ["Und", "l\u00e4\u00b7chelt", "bis", "das", "Herz", "mir", "schwoll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und st\u00fcrmisch k\u00fchn das Wort entquoll:", "tokens": ["Und", "st\u00fcr\u00b7misch", "k\u00fchn", "das", "Wort", "ent\u00b7quoll", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201enimm hin, nimm alles was da mein,", "tokens": ["\u201e", "nimm", "hin", ",", "nimm", "al\u00b7les", "was", "da", "mein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PTKVZ", "$,", "VVIMP", "PIS", "PWS", "ADV", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Liebstes will ich gern dir weih'n,", "tokens": ["Mein", "Liebs\u00b7tes", "will", "ich", "gern", "dir", "weih'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "D\u00fcrft' ich daf\u00fcr dein Buhle seyn,", "tokens": ["D\u00fcrft'", "ich", "da\u00b7f\u00fcr", "dein", "Buh\u00b7le", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PAV", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Mitternacht bis Hahnenschrei'n.\u201c", "tokens": ["Von", "Mit\u00b7ter\u00b7nacht", "bis", "Hah\u00b7nen\u00b7schrei'", "n.", "\u201c"], "token_info": ["word", "word", "word", "word", "abbreviation", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da staunt' mich an gar seltsamlich,", "tokens": ["Da", "staunt'", "mich", "an", "gar", "selt\u00b7sam\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lieb, so weh, und inniglich,", "tokens": ["So", "lieb", ",", "so", "weh", ",", "und", "in\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "PTKVZ", "$,", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sprach zu mir die sch\u00f6ne Maid:", "tokens": ["Und", "sprach", "zu", "mir", "die", "sch\u00f6\u00b7ne", "Maid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So gieb mir deine Seligkeit.", "tokens": ["So", "gieb", "mir", "dei\u00b7ne", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u201emein Leben s\u00fc\u00df, mein junges Blut,", "tokens": ["\u201e", "mein", "Le\u00b7ben", "s\u00fc\u00df", ",", "mein", "jun\u00b7ges", "Blut", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "G\u00e4b' ich, mit Freud und wohlgemuth,", "tokens": ["G\u00e4b'", "ich", ",", "mit", "Freud", "und", "wohl\u00b7ge\u00b7muth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "APPR", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr dich, o M\u00e4dchen, engelgleich, \u2014", "tokens": ["F\u00fcr", "dich", ",", "o", "M\u00e4d\u00b7chen", ",", "en\u00b7gel\u00b7gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$,", "ADJD", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch nimmermehr das Himmelreich.\u201c", "tokens": ["Doch", "nim\u00b7mer\u00b7mehr", "das", "Him\u00b7mel\u00b7reich", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wohl braust hervor mein rasches Wort,", "tokens": ["Wohl", "braust", "her\u00b7vor", "mein", "ra\u00b7sches", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch bl\u00fchet sch\u00f6ner immerfort,", "tokens": ["Doch", "bl\u00fc\u00b7het", "sch\u00f6\u00b7ner", "im\u00b7mer\u00b7fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und immer spricht die sch\u00f6ne Maid:", "tokens": ["Und", "im\u00b7mer", "spricht", "die", "sch\u00f6\u00b7ne", "Maid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O gieb mir deine Seligkeit!", "tokens": ["O", "gieb", "mir", "dei\u00b7ne", "Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dumpf dr\u00f6hnt die\u00df Wort mir in's Geh\u00f6r,", "tokens": ["Dumpf", "dr\u00f6hnt", "die\u00df", "Wort", "mir", "in's", "Ge\u00b7h\u00f6r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PDS", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schleudert mir ein Gluthenmeer", "tokens": ["Und", "schleu\u00b7dert", "mir", "ein", "Glu\u00b7then\u00b7meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl in den tiefsten Seelenraum;", "tokens": ["Wohl", "in", "den", "tiefs\u00b7ten", "See\u00b7len\u00b7raum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich athme schwer, ich athme kaum. \u2014", "tokens": ["Ich", "ath\u00b7me", "schwer", ",", "ich", "ath\u00b7me", "kaum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da waren wei\u00dfe Engelein,", "tokens": ["Da", "wa\u00b7ren", "wei\u00b7\u00dfe", "En\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die gl\u00e4nzten hell im Rosenschein;", "tokens": ["Die", "gl\u00e4nz\u00b7ten", "hell", "im", "Ro\u00b7sen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun aber st\u00fcrmte wild herauf", "tokens": ["Nun", "a\u00b7ber", "st\u00fcrm\u00b7te", "wild", "her\u00b7auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein gr\u00e4ulich schwarzer Koboldhauf'.", "tokens": ["Ein", "gr\u00e4u\u00b7lich", "schwar\u00b7zer", "Ko\u00b7bold\u00b7hauf'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die rangen mit den Engelein,", "tokens": ["Die", "ran\u00b7gen", "mit", "den", "En\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dr\u00e4ngten fort die Engelein;", "tokens": ["Und", "dr\u00e4ng\u00b7ten", "fort", "die", "En\u00b7ge\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und endlich auch die schwarze Schaar", "tokens": ["Und", "end\u00b7lich", "auch", "die", "schwar\u00b7ze", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Nebelduft zerronnen war. \u2014", "tokens": ["In", "Ne\u00b7bel\u00b7duft", "zer\u00b7ron\u00b7nen", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VVPP", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ich aber wollt' in Lust vergehn,", "tokens": ["Ich", "a\u00b7ber", "wollt'", "in", "Lust", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hielt im Arm mein Liebchen sch\u00f6n;", "tokens": ["Ich", "hielt", "im", "Arm", "mein", "Lieb\u00b7chen", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie'n Rehlein s\u00fc\u00df umschmiegt sie mich,", "tokens": ["Wie'n", "Reh\u00b7lein", "s\u00fc\u00df", "um\u00b7schmiegt", "sie", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch weint sie auch recht bitterlich.", "tokens": ["Doch", "weint", "sie", "auch", "recht", "bit\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Feins Liebchen weint; ich wei\u00df warum,", "tokens": ["Feins", "Lieb\u00b7chen", "weint", ";", "ich", "wei\u00df", "wa\u00b7rum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PWAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und k\u00fc\u00df' ihr Rosenm\u00fcndlein stumm. \u2014", "tokens": ["Und", "k\u00fc\u00df'", "ihr", "Ro\u00b7sen\u00b7m\u00fcnd\u00b7lein", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eo still', feins Lieb, die Thr\u00e4nenfluth,", "tokens": ["\u201e", "o", "still'", ",", "feins", "Lieb", ",", "die", "Thr\u00e4\u00b7nen\u00b7fluth", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "$,", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gieb her, feins Lieb, nur Minnegluth.\u201c", "tokens": ["Gieb", "her", ",", "feins", "Lieb", ",", "nur", "Min\u00b7ne\u00b7gluth", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "ADJA", "NN", "$,", "ADV", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "\u201eergieb dich meiner Minnegluth \u2014 \u201c", "tokens": ["\u201e", "er\u00b7gieb", "dich", "mei\u00b7ner", "Min\u00b7ne\u00b7gluth", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "PPOSAT", "NN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da pl\u00f6tzlich starr't zu Eis mein Blut;", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "starr't", "zu", "Eis", "mein", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Laut bebet auf der Erde Grund,", "tokens": ["Laut", "be\u00b7bet", "auf", "der", "Er\u00b7de", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.4": {"text": "Und \u00f6ffnet g\u00e4hnend seinen Schlund.", "tokens": ["Und", "\u00f6ff\u00b7net", "g\u00e4h\u00b7nend", "sei\u00b7nen", "Schlund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Und aus dem Abgrund schwarz und graus", "tokens": ["Und", "aus", "dem", "Ab\u00b7grund", "schwarz", "und", "graus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stieg wild die schwarze Schaar heraus.", "tokens": ["Stieg", "wild", "die", "schwar\u00b7ze", "Schaar", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus meinen Armen schwand feins Lieb;", "tokens": ["Aus", "mei\u00b7nen", "Ar\u00b7men", "schwand", "feins", "Lieb", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich ganz alleine stehen blieb.", "tokens": ["Ich", "ganz", "al\u00b7lei\u00b7ne", "ste\u00b7hen", "blieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Da tanzt im Kreise wunderbar,", "tokens": ["Da", "tanzt", "im", "Krei\u00b7