{"textgrid.poem.57380": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eine Rose, gepfl\u00fcckt vom liebenden M\u00e4dchen, das Thr\u00e4nen", "genre": "verse", "period": "N.A.", "pub_year": 1795, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Rose, gepfl\u00fcckt vom liebenden M\u00e4dchen, das Thr\u00e4nen", "tokens": ["Ei\u00b7ne", "Ro\u00b7se", ",", "ge\u00b7pfl\u00fcckt", "vom", "lie\u00b7ben\u00b7den", "M\u00e4d\u00b7chen", ",", "das", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "VVPP", "APPRART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Tr\u00fcbeten, lag, und welkte dahin,", "tokens": ["Tr\u00fc\u00b7be\u00b7ten", ",", "lag", ",", "und", "welk\u00b7te", "da\u00b7hin", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "KON", "VVFIN", "PAV", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Auf den best\u00e4ubten Blumen des Grases. Das bebende M\u00e4dchen", "tokens": ["Auf", "den", "be\u00b7st\u00e4ub\u00b7ten", "Blu\u00b7men", "des", "Gra\u00b7ses", ".", "Das", "be\u00b7ben\u00b7de", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Liess sie fallen, die doch", "tokens": ["Liess", "sie", "fal\u00b7len", ",", "die", "doch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "VVINF", "$,", "PRELS", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Eine der mosigen war, und nur erst Knospe. Ach jetzo", "tokens": ["Ei\u00b7ne", "der", "mo\u00b7si\u00b7gen", "war", ",", "und", "nur", "erst", "Knos\u00b7pe", ".", "Ach", "jet\u00b7zo"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ART", "NN", "VAFIN", "$,", "KON", "ADV", "ADV", "NN", "$.", "ITJ", "ADV"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Lag sie, und starb!", "tokens": ["Lag", "sie", ",", "und", "starb", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "$."], "meter": "++-+", "measure": "iambic.di"}, "line.7": {"text": "Blumen versammelten sich um sie, und Standen, und B\u00e4ume,", "tokens": ["Blu\u00b7men", "ver\u00b7sam\u00b7mel\u00b7ten", "sich", "um", "sie", ",", "und", "Stan\u00b7den", ",", "und", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "PPER", "$,", "KON", "NN", "$,", "KON", "NN", "$,"], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Dass sie sterben sie s\u00e4hn,", "tokens": ["Dass", "sie", "ster\u00b7ben", "sie", "s\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Und der eigenen Sterblichkeit sich erinnerten; dann sie,", "tokens": ["Und", "der", "ei\u00b7ge\u00b7nen", "Sterb\u00b7lich\u00b7keit", "sich", "e\u00b7rin\u00b7ner\u00b7ten", ";", "dann", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PRF", "VVFIN", "$.", "ADV", "PPER", "$,"], "meter": "--+--+-+-+--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "W\u00e4ren die Bl\u00e4tter ihr alle gedort,", "tokens": ["W\u00e4\u00b7ren", "die", "Bl\u00e4t\u00b7ter", "ihr", "al\u00b7le", "ge\u00b7dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "PIAT", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.11": {"text": "Mit dem Laube bestreuten der Beberesche. Am Grabe", "tokens": ["Mit", "dem", "Lau\u00b7be", "be\u00b7streu\u00b7ten", "der", "Be\u00b7be\u00b7re\u00b7sche", ".", "Am", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$.", "APPRART", "NN"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Sollte, mehr noch zu weinen, ihr dann", "tokens": ["Soll\u00b7te", ",", "mehr", "noch", "zu", "wei\u00b7nen", ",", "ihr", "dann"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "$,", "ADV", "ADV", "PTKZU", "VVINF", "$,", "PPER", "ADV"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Bleiben die J\u00fcngferliche. Die Traurenden nahten der St\u00e4te", "tokens": ["Blei\u00b7ben", "die", "J\u00fcng\u00b7fer\u00b7li\u00b7che", ".", "Die", "Trau\u00b7ren\u00b7den", "nah\u00b7ten", "der", "St\u00e4\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Bald, wo die welkende lag.", "tokens": ["Bald", ",", "wo", "die", "wel\u00b7ken\u00b7de", "lag", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.15": {"text": "Gr\u00fcbling eilte voran, und mit erzitterndem Blatte", "tokens": ["Gr\u00fcb\u00b7ling", "eil\u00b7te", "vo\u00b7ran", ",", "und", "mit", "er\u00b7zit\u00b7tern\u00b7dem", "Blat\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Folgte Weichling von fern.", "tokens": ["Folg\u00b7te", "Weich\u00b7ling", "von", "fern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "G\u00f6ttergeruch begann: So lieget sie denn, und so fr\u00fch schon!", "tokens": ["G\u00f6t\u00b7ter\u00b7ge\u00b7ruch", "be\u00b7gann", ":", "So", "lie\u00b7get", "sie", "denn", ",", "und", "so", "fr\u00fch", "schon", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ADV", "$,", "KON", "ADV", "ADJD", "ADV", "$."], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "H\u00e4tte das liebliche M\u00e4dchen ihr doch", "tokens": ["H\u00e4t\u00b7te", "das", "lieb\u00b7li\u00b7che", "M\u00e4d\u00b7chen", "ihr", "doch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "ADV"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.19": {"text": "Aus der Quelle gesch\u00f6pft, aus der es die Lerche sich tr\u00e4nket,", "tokens": ["Aus", "der", "Quel\u00b7le", "ge\u00b7sch\u00f6pft", ",", "aus", "der", "es", "die", "Ler\u00b7che", "sich", "tr\u00e4n\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "APPR", "PRELS", "PPER", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "Und die Nachtigall, dann", "tokens": ["Und", "die", "Nach\u00b7ti\u00b7gall", ",", "dann"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "$,", "ADV"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Ihren Stamm in die K\u00fchle gesenkt, und dort sie gelabet:", "tokens": ["Ih\u00b7ren", "Stamm", "in", "die", "K\u00fch\u00b7le", "ge\u00b7senkt", ",", "und", "dort", "sie", "ge\u00b7la\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVPP", "$,", "KON", "ADV", "PPER", "VVPP", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Ach so st\u00fcrbe sie nicht!", "tokens": ["Ach", "so", "st\u00fcr\u00b7be", "sie", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.23": {"text": "Schwebete nun noch nicht zu den Rosenschatten hinunter", "tokens": ["Schwe\u00b7be\u00b7te", "nun", "noch", "nicht", "zu", "den", "Ro\u00b7sen\u00b7schat\u00b7ten", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PTKNEG", "APPR", "ART", "NN", "APPO"], "meter": "+----+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.24": {"text": "In Elisiens Thal.", "tokens": ["In", "E\u00b7li\u00b7si\u00b7ens", "Thal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+---+", "measure": "dactylic.init"}, "line.25": {"text": "Also G\u00f6ttergeruch. Wenn der West schwieg, wurd' ihm die Stimme", "tokens": ["Al\u00b7so", "G\u00f6t\u00b7ter\u00b7ge\u00b7ruch", ".", "Wenn", "der", "West", "schwieg", ",", "wurd'", "ihm", "die", "Stim\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$.", "KOUS", "ART", "NN", "VVFIN", "$,", "VAFIN", "PPER", "ART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.26": {"text": "Rede nur, wenn er wehte, Gesang.", "tokens": ["Re\u00b7de", "nur", ",", "wenn", "er", "weh\u00b7te", ",", "Ge\u00b7sang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.27": {"text": "R\u00f6the, die tr\u00e4gt Herzbl\u00e4tter, Vernunft, und die Tochter des Maies,", "tokens": ["R\u00f6\u00b7the", ",", "die", "tr\u00e4gt", "Herz\u00b7bl\u00e4t\u00b7ter", ",", "Ver\u00b7nunft", ",", "und", "die", "Toch\u00b7ter", "des", "Mai\u00b7es", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "VVFIN", "NN", "$,", "NN", "$,", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.28": {"text": "Und Goldlilie klagten ihm nach,", "tokens": ["Und", "Gold\u00b7li\u00b7lie", "klag\u00b7ten", "ihm", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.29": {"text": "Flamm', und M\u00e4dchen im Busche, und Himmelskerze, und Ringel-", "tokens": ["Flamm'", ",", "und", "M\u00e4d\u00b7chen", "im", "Bu\u00b7sche", ",", "und", "Him\u00b7mels\u00b7ker\u00b7ze", ",", "und", "Rin\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "KON", "NN", "APPRART", "NN", "$,", "KON", "NN", "$,", "KON", "TRUNC"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Blume, die heilige, nach.", "tokens": ["Blu\u00b7me", ",", "die", "hei\u00b7li\u00b7ge", ",", "nach", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "$,", "APPR", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.31": {"text": "Aber itzt kamen in Eil herbey die wild und die schwarze", "tokens": ["A\u00b7ber", "itzt", "ka\u00b7men", "in", "Eil", "her\u00b7bey", "die", "wild", "und", "die", "schwar\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NN", "ADV", "ART", "ADJD", "KON", "ART", "ADJA"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Natterwurzel, herbey.", "tokens": ["Nat\u00b7ter\u00b7wur\u00b7zel", ",", "her\u00b7bey", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.33": {"text": "Eisenherz, mit ihm L\u00f6wenfuss, und der Wolfsgesichter;", "tokens": ["Ei\u00b7sen\u00b7herz", ",", "mit", "ihm", "L\u00f6\u00b7wen\u00b7fuss", ",", "und", "der", "Wolfs\u00b7ge\u00b7sich\u00b7ter", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "NN", "$,", "KON", "ART", "NN", "$."], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.34": {"text": "Also ruften sie aus:", "tokens": ["Al\u00b7so", "ruf\u00b7ten", "sie", "aus", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.35": {"text": "Th\u00f6richte, dass ihr da so wehklagt um Eine der Rosen;", "tokens": ["Th\u00f6\u00b7rich\u00b7te", ",", "dass", "ihr", "da", "so", "weh\u00b7klagt", "um", "Ei\u00b7ne", "der", "Ro\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "APPR", "ART", "ART", "NN", "$."], "meter": "-+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Und zu tausenden werden sie doch", "tokens": ["Und", "zu", "tau\u00b7sen\u00b7den", "wer\u00b7den", "sie", "doch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKZU", "VVINF", "VAFIN", "PPER", "ADV"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.37": {"text": "Unter Galliens Wilden, von Menschenblute bestr\u00f6met,", "tokens": ["Un\u00b7ter", "Gal\u00b7li\u00b7ens", "Wil\u00b7den", ",", "von", "Men\u00b7schen\u00b7blu\u00b7te", "be\u00b7str\u00f6\u00b7met", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.38": {"text": "In dem elisischen Felde verschwemt!", "tokens": ["In", "dem", "e\u00b7li\u00b7si\u00b7schen", "Fel\u00b7de", "ver\u00b7schwemt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.39": {"text": "Aber die weinende Weide vernahm mit Entsetzen die dumpfen", "tokens": ["A\u00b7ber", "die", "wei\u00b7nen\u00b7de", "Wei\u00b7de", "ver\u00b7nahm", "mit", "Ent\u00b7set\u00b7zen", "die", "dum\u00b7pfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "ART", "ADJA"], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.40": {"text": "T\u00f6ne, begann:", "tokens": ["T\u00f6\u00b7ne", ",", "be\u00b7gann", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.41": {"text": "Und du kamst, Zipresse, nicht auch, du grausame, kamst nicht!", "tokens": ["Und", "du", "kamst", ",", "Zip\u00b7res\u00b7se", ",", "nicht", "auch", ",", "du", "grau\u00b7sa\u00b7me", ",", "kamst", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "NN", "$,", "PTKNEG", "ADV", "$,", "PPER", "ADJA", "$,", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.42": {"text": "L\u00e4ssest allein", "tokens": ["L\u00e4s\u00b7sest", "al\u00b7lein"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.43": {"text": "\u00dcber sie mich die Z\u00e4hre des Grams hintr\u00e4ufeln; und doch ist", "tokens": ["\u00dc\u00b7ber", "sie", "mich", "die", "Z\u00e4h\u00b7re", "des", "Grams", "hin\u00b7tr\u00e4u\u00b7feln", ";", "und", "doch", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "PRF", "ART", "NN", "ART", "NN", "VVINF", "$.", "KON", "ADV", "VAFIN"], "meter": "+--+-+--+-+-+-+", "measure": "iambic.septa.invert"}, "line.44": {"text": "Sie, kaum Knospe, gewelkt!", "tokens": ["Sie", ",", "kaum", "Knos\u00b7pe", ",", "ge\u00b7welkt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "ADV", "NN", "$,", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.45": {"text": "Wird zu den Schemen nun bald der Pfirsichbl\u00fcthen hinabgehn,", "tokens": ["Wird", "zu", "den", "Sche\u00b7men", "nun", "bald", "der", "Pfir\u00b7sich\u00b7bl\u00fc\u00b7then", "hin\u00b7ab\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.46": {"text": "Wird zu der Veilchen hinab,", "tokens": ["Wird", "zu", "der", "Veil\u00b7chen", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.47": {"text": "Und Elfranken gehn. Sie sang es, s\u00e4uselte, senkte", "tokens": ["Und", "El\u00b7fran\u00b7ken", "gehn", ".", "Sie", "sang", "es", ",", "s\u00e4u\u00b7sel\u00b7te", ",", "senk\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "NN", "VVINF", "$.", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "$,", "VVFIN"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.48": {"text": "Tiefer den schwebenden Zweig.", "tokens": ["Tie\u00b7fer", "den", "schwe\u00b7ben\u00b7den", "Zweig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.49": {"text": "Aber die Traurenden wendeten sich. Da blieb an dem Grabe", "tokens": ["A\u00b7ber", "die", "Trau\u00b7ren\u00b7den", "wen\u00b7de\u00b7ten", "sich", ".", "Da", "blieb", "an", "dem", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "$.", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.50": {"text": "Schweigend die J\u00fcngferliche zur\u00fcck.", "tokens": ["Schwei\u00b7gend", "die", "J\u00fcng\u00b7fer\u00b7li\u00b7che", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}}}}