{"textgrid.poem.57624": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Radaunensee im Klotzow", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es taucht aus rabenschwarzer, stiller Flut", "tokens": ["Es", "taucht", "aus", "ra\u00b7ben\u00b7schwar\u00b7zer", ",", "stil\u00b7ler", "Flut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die dottergelbe, stolze Wasserrose;", "tokens": ["Die", "dot\u00b7ter\u00b7gel\u00b7be", ",", "stol\u00b7ze", "Was\u00b7ser\u00b7ro\u00b7se", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Fliegenpilzes feuerroter Hut,", "tokens": ["Des", "Flie\u00b7gen\u00b7pil\u00b7zes", "feu\u00b7er\u00b7ro\u00b7ter", "Hut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der leuchtet grell aus sammetgr\u00fcnem Moose.", "tokens": ["Der", "leuch\u00b7tet", "grell", "aus", "sam\u00b7met\u00b7gr\u00fc\u00b7nem", "Moo\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die d\u00fcstern Kiefern stehen stramm und steif,", "tokens": ["Die", "d\u00fcs\u00b7tern", "Kie\u00b7fern", "ste\u00b7hen", "stramm", "und", "steif", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zum Wasser b\u00fccken sich die schlanken Birken;", "tokens": ["Zum", "Was\u00b7ser", "b\u00fc\u00b7cken", "sich", "die", "schlan\u00b7ken", "Bir\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durchs Unterholz zieht schwer ein Nebelstreif", "tokens": ["Durchs", "Un\u00b7ter\u00b7holz", "zieht", "schwer", "ein", "Ne\u00b7bel\u00b7streif"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und l\u00e4\u00dft die wei\u00dfen Birken zaub'risch wirken.", "tokens": ["Und", "l\u00e4\u00dft", "die", "wei\u00b7\u00dfen", "Bir\u00b7ken", "zaub'\u00b7risch", "wir\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In wolkenloser, dunkelblauer H\u00f6h'", "tokens": ["In", "wol\u00b7ken\u00b7lo\u00b7ser", ",", "dun\u00b7kel\u00b7blau\u00b7er", "H\u00f6h'"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kommt m\u00fcden Flugs ein Reiher hergezogen \u2013", "tokens": ["Kommt", "m\u00fc\u00b7den", "Flugs", "ein", "Rei\u00b7her", "her\u00b7ge\u00b7zo\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr einen Abend am Radaunensee", "tokens": ["F\u00fcr", "ei\u00b7nen", "A\u00b7bend", "am", "Ra\u00b7dau\u00b7nen\u00b7see"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "G\u00e4b' ich den Rhein mit seinen goldnen Wogen.", "tokens": ["G\u00e4b'", "ich", "den", "Rhein", "mit", "sei\u00b7nen", "gold\u00b7nen", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NE", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}