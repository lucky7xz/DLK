{"textgrid.poem.59478": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der armen Sch\u00f6nheit Lebenslauf", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die arme Sch\u00f6nheit irrt auf Erden,", "tokens": ["Die", "ar\u00b7me", "Sch\u00f6n\u00b7heit", "irrt", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So lieblich Wetter drau\u00dfen ist,", "tokens": ["So", "lieb\u00b7lich", "Wet\u00b7ter", "drau\u00b7\u00dfen", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "M\u00f6cht gern recht viel gesehen werden,", "tokens": ["M\u00f6cht", "gern", "recht", "viel", "ge\u00b7se\u00b7hen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil jeder sie so freundlich gr\u00fc\u00dft.", "tokens": ["Weil", "je\u00b7der", "sie", "so", "freund\u00b7lich", "gr\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wer die arme Sch\u00f6nheit schauet,", "tokens": ["Und", "wer", "die", "ar\u00b7me", "Sch\u00f6n\u00b7heit", "schau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich wie auf gro\u00dfes Gl\u00fcck besinnt,", "tokens": ["Sich", "wie", "auf", "gro\u00b7\u00dfes", "Gl\u00fcck", "be\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KOKOM", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Seele f\u00fchlt sich recht erbauet,", "tokens": ["Die", "See\u00b7le", "f\u00fchlt", "sich", "recht", "er\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie wenn der Fr\u00fchling neu beginnt.", "tokens": ["Wie", "wenn", "der", "Fr\u00fch\u00b7ling", "neu", "be\u00b7ginnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da sieht sie viele sch\u00f6ne Knaben,", "tokens": ["Da", "sieht", "sie", "vie\u00b7le", "sch\u00f6\u00b7ne", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die reiten unten durch den Wind,", "tokens": ["Die", "rei\u00b7ten", "un\u00b7ten", "durch", "den", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "M\u00f6cht manchen gern im Arme haben,", "tokens": ["M\u00f6cht", "man\u00b7chen", "gern", "im", "Ar\u00b7me", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00fct dich, h\u00fct dich, du armes Kind!", "tokens": ["H\u00fct", "dich", ",", "h\u00fct", "dich", ",", "du", "ar\u00b7mes", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Da ziehn manch redliche Gesellen,", "tokens": ["Da", "ziehn", "manch", "red\u00b7li\u00b7che", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sagen: \u00bbHast nicht Geld, noch Haus,", "tokens": ["Die", "sa\u00b7gen", ":", "\u00bb", "Hast", "nicht", "Geld", ",", "noch", "Haus", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "VVINF", "$.", "$(", "VAFIN", "PTKNEG", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir f\u00fcrchten deine Augen helle,", "tokens": ["Wir", "f\u00fcrch\u00b7ten", "dei\u00b7ne", "Au\u00b7gen", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir haben nichts zum Hochzeitsschmaus.\u00ab", "tokens": ["Wir", "ha\u00b7ben", "nichts", "zum", "Hoch\u00b7zeits\u00b7schmaus", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Von andern tut sie sich wegdrehen,", "tokens": ["Von", "an\u00b7dern", "tut", "sie", "sich", "weg\u00b7dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "PRF", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil keiner ihr so wohl gef\u00e4llt,", "tokens": ["Weil", "kei\u00b7ner", "ihr", "so", "wohl", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die m\u00fcssen traurig weitergehen,", "tokens": ["Die", "m\u00fcs\u00b7sen", "trau\u00b7rig", "wei\u00b7ter\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und z\u00f6gen gern ans End der Welt.", "tokens": ["Und", "z\u00f6\u00b7gen", "gern", "ans", "End", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da sagt sie: \u00bbWas hilft mir mein Sehen,", "tokens": ["Da", "sagt", "sie", ":", "\u00bb", "Was", "hilft", "mir", "mein", "Se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ich w\u00fcnscht, ich w\u00e4re lieber blind,", "tokens": ["Ich", "w\u00fcnscht", ",", "ich", "w\u00e4\u00b7re", "lie\u00b7ber", "blind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da alle furchtsam von mir gehen,", "tokens": ["Da", "al\u00b7le", "furcht\u00b7sam", "von", "mir", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil gar so sch\u00f6n mein' Augen sind.\u00ab \u2013", "tokens": ["Weil", "gar", "so", "sch\u00f6n", "mein'", "Au\u00b7gen", "sind", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADJD", "PPOSAT", "NN", "VAFIN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nun sitzt sie hoch auf lichtem Schlosse,", "tokens": ["Nun", "sitzt", "sie", "hoch", "auf", "lich\u00b7tem", "Schlos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In sch\u00f6ne Kleider putzt sie sich,", "tokens": ["In", "sch\u00f6\u00b7ne", "Klei\u00b7der", "putzt", "sie", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Fenster gl\u00fchn, sie winkt vom Schlosse,", "tokens": ["Die", "Fens\u00b7ter", "gl\u00fchn", ",", "sie", "winkt", "vom", "Schlos\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sonne sinkt, das blendet dich.", "tokens": ["Die", "Son\u00b7ne", "sinkt", ",", "das", "blen\u00b7det", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Augen, die so furchtsam waren,", "tokens": ["Die", "Au\u00b7gen", ",", "die", "so", "furcht\u00b7sam", "wa\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die haben jetzt so freien Lauf,", "tokens": ["Die", "ha\u00b7ben", "jetzt", "so", "frei\u00b7en", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fort ist das Kr\u00e4nzlein aus den Haaren,", "tokens": ["Fort", "ist", "das", "Kr\u00e4nz\u00b7lein", "aus", "den", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hohe Federn stehn darauf.", "tokens": ["Und", "ho\u00b7he", "Fe\u00b7dern", "stehn", "da\u00b7rauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Das Kr\u00e4nzlein ist herausgerissen,", "tokens": ["Das", "Kr\u00e4nz\u00b7lein", "ist", "her\u00b7aus\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ganz ohne Scheu sie mich anlacht;", "tokens": ["Ganz", "oh\u00b7ne", "Scheu", "sie", "mich", "an\u00b7lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geh du vorbei: sie wird dich gr\u00fc\u00dfen,", "tokens": ["Geh", "du", "vor\u00b7bei", ":", "sie", "wird", "dich", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Winkt dir zu einer sch\u00f6nen Nacht. \u2013", "tokens": ["Winkt", "dir", "zu", "ei\u00b7ner", "sch\u00f6\u00b7nen", "Nacht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Da sieht sie die Gesellen wieder,", "tokens": ["Da", "sieht", "sie", "die", "Ge\u00b7sel\u00b7len", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die fahren unten auf dem Flu\u00df,", "tokens": ["Die", "fah\u00b7ren", "un\u00b7ten", "auf", "dem", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es singen laut die lust'gen Br\u00fcder,", "tokens": ["Es", "sin\u00b7gen", "laut", "die", "lust'\u00b7gen", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So furchtbar schallt des einen Gru\u00df:", "tokens": ["So", "furcht\u00b7bar", "schallt", "des", "ei\u00b7nen", "Gru\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "\u00bbwas bist du f\u00fcr 'ne sch\u00f6ne Leiche!", "tokens": ["\u00bb", "was", "bist", "du", "f\u00fcr", "'ne", "sch\u00f6\u00b7ne", "Lei\u00b7che", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So w\u00fcste ist mir meine Brust,", "tokens": ["So", "w\u00fcs\u00b7te", "ist", "mir", "mei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie bist du nun so arm, du Reiche,", "tokens": ["Wie", "bist", "du", "nun", "so", "arm", ",", "du", "Rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "PPER", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich hab an dir nicht weiter Lust!\u00ab", "tokens": ["Ich", "hab", "an", "dir", "nicht", "wei\u00b7ter", "Lust", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "PTKNEG", "ADV", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Der Wilde hat ihr so gefallen,", "tokens": ["Der", "Wil\u00b7de", "hat", "ihr", "so", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Laut schrie sie auf bei seinem Gru\u00df,", "tokens": ["Laut", "schrie", "sie", "auf", "bei", "sei\u00b7nem", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Schlo\u00df m\u00f6cht sie herunterfallen,", "tokens": ["Vom", "Schlo\u00df", "m\u00f6cht", "sie", "her\u00b7un\u00b7ter\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und unten ruhn im k\u00fchlen Flu\u00df. \u2013", "tokens": ["Und", "un\u00b7ten", "ruhn", "im", "k\u00fch\u00b7len", "Flu\u00df", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVINF", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Sie blieb nicht l\u00e4nger mehr da oben,", "tokens": ["Sie", "blieb", "nicht", "l\u00e4n\u00b7ger", "mehr", "da", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil alles anders worden war,", "tokens": ["Weil", "al\u00b7les", "an\u00b7ders", "wor\u00b7den", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor Schmerz ist ihr das Herz erhoben,", "tokens": ["Vor", "Schmerz", "ist", "ihr", "das", "Herz", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da ward's so kalt, doch himmlisch klar.", "tokens": ["Da", "ward's", "so", "kalt", ",", "doch", "himm\u00b7lisch", "klar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Da legt sie ab die goldnen Spangen,", "tokens": ["Da", "legt", "sie", "ab", "die", "gold\u00b7nen", "Span\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den falschen Putz und Ziererei,", "tokens": ["Den", "fal\u00b7schen", "Putz", "und", "Zie\u00b7re\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus dem verstockten Herzen drangen", "tokens": ["Aus", "dem", "ver\u00b7stock\u00b7ten", "Her\u00b7zen", "dran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die alten Tr\u00e4nen wieder frei.", "tokens": ["Die", "al\u00b7ten", "Tr\u00e4\u00b7nen", "wie\u00b7der", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Kein Stern wollt nicht die Nacht erhellen,", "tokens": ["Kein", "Stern", "wollt", "nicht", "die", "Nacht", "er\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da mu\u00dfte die Verliebte gehn,", "tokens": ["Da", "mu\u00df\u00b7te", "die", "Ver\u00b7lieb\u00b7te", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie rauscht der Flu\u00df! die Hunde bellen,", "tokens": ["Wie", "rauscht", "der", "Flu\u00df", "!", "die", "Hun\u00b7de", "bel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Fenster fern erleuchtet stehn.", "tokens": ["Die", "Fens\u00b7ter", "fern", "er\u00b7leuch\u00b7tet", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Nun bist du frei von deinen S\u00fcnden,", "tokens": ["Nun", "bist", "du", "frei", "von", "dei\u00b7nen", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lieb zog triumphierend ein,", "tokens": ["Die", "Lieb", "zog", "tri\u00b7um\u00b7phie\u00b7rend", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du wirst noch hohe Gnade finden,", "tokens": ["Du", "wirst", "noch", "ho\u00b7he", "Gna\u00b7de", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seele geht in Hafen ein.", "tokens": ["Die", "See\u00b7le", "geht", "in", "Ha\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Der Liebste war ein J\u00e4ger worden,", "tokens": ["Der", "Liebs\u00b7te", "war", "ein", "J\u00e4\u00b7ger", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Morgen schien so rosenrot,", "tokens": ["Der", "Mor\u00b7gen", "schien", "so", "ro\u00b7sen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da blies er lustig auf dem Horne,", "tokens": ["Da", "blies", "er", "lus\u00b7tig", "auf", "dem", "Hor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Blies immerfort in seiner Not.", "tokens": ["Blies", "im\u00b7mer\u00b7fort", "in", "sei\u00b7ner", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}