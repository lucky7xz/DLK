punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "ADJD", "VVFIN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Thut/ wenn ich es nur bekenne/", "tokens": ["Thut", "/", "wenn", "ich", "es", "nur", "be\u00b7ken\u00b7ne", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sylvia/ dein s\u00fcsser mund.", "tokens": ["Syl\u00b7via", "/", "dein", "s\u00fcs\u00b7ser", "mund", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sylvia/ dein s\u00fcsser ku\u00df", "tokens": ["Syl\u00b7via", "/", "dein", "s\u00fcs\u00b7ser", "ku\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$(", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Kan mir mehr erqvickung geben/", "tokens": ["Kan", "mir", "mehr", "er\u00b7qvic\u00b7kung", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als die ulmen jungen reben/", "tokens": ["Als", "die", "ul\u00b7men", "jun\u00b7gen", "re\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "ADJA", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Egypten Nilus flu\u00df;", "tokens": ["Und", "E\u00b7gyp\u00b7ten", "Ni\u00b7lus", "flu\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Mu\u00df ich gleich zuweilen borgen/", "tokens": ["Mu\u00df", "ich", "gleich", "zu\u00b7wei\u00b7len", "bor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So vertreibt doch alle morgen", "tokens": ["So", "ver\u00b7treibt", "doch", "al\u00b7le", "mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PIS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Meinen gram und meine sorgen/", "tokens": ["Mei\u00b7nen", "gram", "und", "mei\u00b7ne", "sor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sylvia/ dein s\u00fcsser ku\u00df.", "tokens": ["Syl\u00b7via", "/", "dein", "s\u00fcs\u00b7ser", "ku\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}