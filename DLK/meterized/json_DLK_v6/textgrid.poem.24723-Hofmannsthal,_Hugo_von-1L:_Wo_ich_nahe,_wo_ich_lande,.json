{"textgrid.poem.24723": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo ich nahe, wo ich lande,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ich nahe, wo ich lande,", "tokens": ["Wo", "ich", "na\u00b7he", ",", "wo", "ich", "lan\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKVZ", "$,", "PWAV", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da im Schatten, dort im Sande", "tokens": ["Da", "im", "Schat\u00b7ten", ",", "dort", "im", "San\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "ADV", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Werden sie sich zu mir setzen,", "tokens": ["Wer\u00b7den", "sie", "sich", "zu", "mir", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und ich werde sie ergetzen,", "tokens": ["Und", "ich", "wer\u00b7de", "sie", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Binden mit dem Schattenbande!", "tokens": ["Bin\u00b7den", "mit", "dem", "Schat\u00b7ten\u00b7ban\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "An den Dingen, die sie kennen,", "tokens": ["An", "den", "Din\u00b7gen", ",", "die", "sie", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lehr ich sie Geheimes nennen,", "tokens": ["Lehr", "ich", "sie", "Ge\u00b7hei\u00b7mes", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf und Nieder ihrer Glieder", "tokens": ["Auf", "und", "Nie\u00b7der", "ih\u00b7rer", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KON", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den Lauf der Sterne wieder,", "tokens": ["Und", "den", "Lauf", "der", "Ster\u00b7ne", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kaum verm\u00f6gen sies zu trennen!", "tokens": ["Kaum", "ver\u00b7m\u00f6\u00b7gen", "sies", "zu", "tren\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn ich spreche: \u00bbGro\u00dfe Macht", "tokens": ["Denn", "ich", "spre\u00b7che", ":", "\u00bb", "Gro\u00b7\u00dfe", "Macht"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lenkt den Tag, versenkt die Nacht,", "tokens": ["Lenkt", "den", "Tag", ",", "ver\u00b7senkt", "die", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch in Euch versenkt sind gleiche", "tokens": ["Doch", "in", "Euch", "ver\u00b7senkt", "sind", "glei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVPP", "VAFIN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sehr geheimnisvolle Reiche,", "tokens": ["Sehr", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7le", "Rei\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ruhig wie in einen Schacht.\u00ab", "tokens": ["Ru\u00b7hig", "wie", "in", "ei\u00b7nen", "Schacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "KOKOM", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da\u00df sie mit verhaltnem Grauen", "tokens": ["Da\u00df", "sie", "mit", "ver\u00b7halt\u00b7nem", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An sich selber niederschauen,", "tokens": ["An", "sich", "sel\u00b7ber", "nie\u00b7der\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von Geheimnis ganz durchwoben", "tokens": ["Von", "Ge\u00b7heim\u00b7nis", "ganz", "durch\u00b7wo\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchlen sich emporgehoben", "tokens": ["F\u00fch\u00b7len", "sich", "em\u00b7por\u00b7ge\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und den Himmel dunkler blauen!", "tokens": ["Und", "den", "Him\u00b7mel", "dunk\u00b7ler", "blau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}