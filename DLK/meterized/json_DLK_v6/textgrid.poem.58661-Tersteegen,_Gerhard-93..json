{"textgrid.poem.58661": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "93.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie k\u00f6stlich ist die Zierd' der innerlichen Seelen,", "tokens": ["Wie", "k\u00f6st\u00b7lich", "ist", "die", "Zierd'", "der", "in\u00b7ner\u00b7li\u00b7chen", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr sanft' und stiller Geist, ihr Wesen unverr\u00fcckt;", "tokens": ["Ihr", "sanft'", "und", "stil\u00b7ler", "Geist", ",", "ihr", "We\u00b7sen", "un\u00b7ver\u00b7r\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADJA", "NN", "$,", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es sind die Tugenden und Gnaden nicht zu z\u00e4hlen,", "tokens": ["Es", "sind", "die", "Tu\u00b7gen\u00b7den", "und", "Gna\u00b7den", "nicht", "zu", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Womit Jehova selbst die werte Braut geschm\u00fcckt!", "tokens": ["Wo\u00b7mit", "Je\u00b7ho\u00b7va", "selbst", "die", "wer\u00b7te", "Braut", "ge\u00b7schm\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie hat ein K\u00f6nigreich, am vollen Tisch sie sitzet,", "tokens": ["Sie", "hat", "ein", "K\u00f6\u00b7nig\u00b7reich", ",", "am", "vol\u00b7len", "Tisch", "sie", "sit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch Gottes Herrlichkeit wird sie vollkommen sch\u00f6n,", "tokens": ["Durch", "Got\u00b7tes", "Herr\u00b7lich\u00b7keit", "wird", "sie", "voll\u00b7kom\u00b7men", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VAFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch wenn sie sich auf sich und ihre Sch\u00f6ne st\u00fctzet", "tokens": ["Doch", "wenn", "sie", "sich", "auf", "sich", "und", "ih\u00b7re", "Sch\u00f6\u00b7ne", "st\u00fct\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPR", "PRF", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und sich gemeine macht, o weh, wie wird es gehn!", "tokens": ["Und", "sich", "ge\u00b7mei\u00b7ne", "macht", ",", "o", "weh", ",", "wie", "wird", "es", "gehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADJA", "VVFIN", "$,", "FM", "PTKVZ", "$,", "PWAV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}