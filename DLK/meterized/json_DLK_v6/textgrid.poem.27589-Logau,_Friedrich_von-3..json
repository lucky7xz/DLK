{"textgrid.poem.27589": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Musa, Venus, Charis schauet,", "tokens": ["Mu\u00b7sa", ",", "Ve\u00b7nus", ",", "Cha\u00b7ris", "schau\u00b7et", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie Am\u0153na Staffeln bauet,", "tokens": ["Wie", "Am\u0153\u00b7na", "Staf\u00b7feln", "bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auffzusteigen euren Thron.", "tokens": ["Auff\u00b7zu\u00b7stei\u00b7gen", "eu\u00b7ren", "Thron", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gebt ihr Raum zur rechten Seite;", "tokens": ["Gebt", "ihr", "Raum", "zur", "rech\u00b7ten", "Sei\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schaffet, da\u00df man ihr bereite", "tokens": ["Schaf\u00b7fet", ",", "da\u00df", "man", "ihr", "be\u00b7rei\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PIS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine frische Lorber-Kron'.", "tokens": ["Ei\u00b7ne", "fri\u00b7sche", "Lor\u00b7ber\u00b7Kron'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ph\u00f6bus lehnt ihr seinen Wagen,", "tokens": ["Ph\u00f6\u00b7bus", "lehnt", "ihr", "sei\u00b7nen", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihren Ruhm herumb zu tragen", "tokens": ["Ih\u00b7ren", "Ruhm", "he\u00b7rumb", "zu", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APZR", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Durch das blaue Sternen-Feld.", "tokens": ["Durch", "das", "blau\u00b7e", "Ster\u00b7nen\u00b7Feld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Hermes soll die Fl\u00fcgel fassen,", "tokens": ["Her\u00b7mes", "soll", "die", "Fl\u00fc\u00b7gel", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Da\u00df sie sei, verk\u00fcnden lassen,", "tokens": ["Da\u00df", "sie", "sei", ",", "ver\u00b7k\u00fcn\u00b7den", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "$,", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Zu dem ewig-seyn gesellt.", "tokens": ["Zu", "dem", "e\u00b7wig\u00b7seyn", "ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Billich! denn so hohe Sinnen", "tokens": ["Bil\u00b7lich", "!", "denn", "so", "ho\u00b7he", "Sin\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$.", "KON", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "M\u00fcssen andren Dank gewinnen,", "tokens": ["M\u00fcs\u00b7sen", "an\u00b7dren", "Dank", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Als ein kriechend Erde-Geist,", "tokens": ["Als", "ein", "krie\u00b7chend", "Er\u00b7de\u00b7Geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Den man aus dem eignen nennen", "tokens": ["Den", "man", "aus", "dem", "eig\u00b7nen", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "APPR", "ART", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Dennoch nicht mag recht erkennen,", "tokens": ["Den\u00b7noch", "nicht", "mag", "recht", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.18": {"text": "Weil er andres ist als heist.", "tokens": ["Weil", "er", "and\u00b7res", "ist", "als", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VAFIN", "KOKOM", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Sinnen, die vom Himmel kommen,", "tokens": ["Sin\u00b7nen", ",", "die", "vom", "Him\u00b7mel", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Werden billich aufgenommen", "tokens": ["Wer\u00b7den", "bil\u00b7lich", "auf\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "In das reine Himmel-klar,", "tokens": ["In", "das", "rei\u00b7ne", "Him\u00b7mel\u00b7klar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Da der schwartzen Erde Schatten", "tokens": ["Da", "der", "schwart\u00b7zen", "Er\u00b7de", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Glantz und Flammen ihrer Thaten", "tokens": ["Glantz", "und", "Flam\u00b7men", "ih\u00b7rer", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Nimmermehr vertunkeln thar.", "tokens": ["Nim\u00b7mer\u00b7mehr", "ver\u00b7tun\u00b7keln", "thar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}