{"textgrid.poem.59754": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Karl Vollertsen, des Schleswigers, Grab", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einen Biedermann deckt dieser Sand,", "tokens": ["Ei\u00b7nen", "Bie\u00b7der\u00b7mann", "deckt", "die\u00b7ser", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Der fiel f\u00fcrs liebe Vaterland.", "tokens": ["Der", "fiel", "f\u00fcrs", "lie\u00b7be", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als aus Osten die Kriegstrompete blies,", "tokens": ["Als", "aus", "Os\u00b7ten", "die", "Kriegs\u00b7trom\u00b7pe\u00b7te", "blies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Da nahm er freudig Schwert und Spie\u00df,", "tokens": ["Da", "nahm", "er", "freu\u00b7dig", "Schwert", "und", "Spie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es galt die Zwinger zu vertreiben:", "tokens": ["Es", "galt", "die", "Zwin\u00b7ger", "zu", "ver\u00b7trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da konnt' er nicht zu Hause bleiben.", "tokens": ["Da", "konnt'", "er", "nicht", "zu", "Hau\u00b7se", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da rief er seinem tapfern Sohn:", "tokens": ["Da", "rief", "er", "sei\u00b7nem", "tap\u00b7fern", "Sohn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So haben beide gegriffen zur Wehr,", "tokens": ["So", "ha\u00b7ben", "bei\u00b7de", "ge\u00b7grif\u00b7fen", "zur", "Wehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch nur der Sohn ist wiedergekommen,", "tokens": ["Doch", "nur", "der", "Sohn", "ist", "wie\u00b7der\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Vater hat eine Kugel genommen.", "tokens": ["Den", "Va\u00b7ter", "hat", "ei\u00b7ne", "Ku\u00b7gel", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Einen Biedermann deckt dieser Sand,", "tokens": ["Ei\u00b7nen", "Bie\u00b7der\u00b7mann", "deckt", "die\u00b7ser", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Der fiel f\u00fcrs liebe Vaterland.", "tokens": ["Der", "fiel", "f\u00fcrs", "lie\u00b7be", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Steh, Anglerj\u00fcngling, steh hier still,", "tokens": ["Steh", ",", "Ang\u00b7ler\u00b7j\u00fcng\u00b7ling", ",", "steh", "hier", "still", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Horch', was sein Geist dir sagen will.", "tokens": ["Horch'", ",", "was", "sein", "Geist", "dir", "sa\u00b7gen", "will", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPOSAT", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er ruft: ", "tokens": ["Er", "ruft", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Und auch: ", "tokens": ["Und", "auch", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Einen Biedermann deckt dieser Sand,", "tokens": ["Ei\u00b7nen", "Bie\u00b7der\u00b7mann", "deckt", "die\u00b7ser", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Karl Vollertsen war er genannt.", "tokens": ["Karl", "Vol\u00b7lert\u00b7sen", "war", "er", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Er war gegossen aus vollem Erz,", "tokens": ["Er", "war", "ge\u00b7gos\u00b7sen", "aus", "vol\u00b7lem", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aus vollem M\u00e4nnerstahl sein Herz.", "tokens": ["Aus", "vol\u00b7lem", "M\u00e4n\u00b7ner\u00b7stahl", "sein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das ruft: ", "tokens": ["Das", "ruft", ":"], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Doch auch: ", "tokens": ["Doch", "auch", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$."], "meter": "++", "measure": "spondeus"}}, "stanza.5": {"line.1": {"text": "Steh fromm vor dieses Grabes Mal.", "tokens": ["Steh", "fromm", "vor", "die\u00b7ses", "Gra\u00b7bes", "Mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Solange die Sonne geht zu Berg und Tal,", "tokens": ["So\u00b7lan\u00b7ge", "die", "Son\u00b7ne", "geht", "zu", "Berg", "und", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Solange schl\u00e4gt ein treues, deutsches Herz", "tokens": ["So\u00b7lan\u00b7ge", "schl\u00e4gt", "ein", "treu\u00b7es", ",", "deut\u00b7sches", "Herz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Hoffnung blicket himmelw\u00e4rts,", "tokens": ["Und", "Hoff\u00b7nung", "bli\u00b7cket", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ruft Vollertsen: ", "tokens": ["Ruft", "Vol\u00b7lert\u00b7sen", ":"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Doch auch: ", "tokens": ["Doch", "auch", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$."], "meter": "++", "measure": "spondeus"}}}}}