{"textgrid.poem.54093": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Ballade", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da sprach der Landrat unter St\u00f6hnen:", "tokens": ["Da", "sprach", "der", "Land\u00b7rat", "un\u00b7ter", "St\u00f6h\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbk\u00f6nnten Sie sich an meinen K\u00f6rper gew\u00f6hnen?\u00ab", "tokens": ["\u00bb", "k\u00f6nn\u00b7ten", "Sie", "sich", "an", "mei\u00b7nen", "K\u00f6r\u00b7per", "ge\u00b7w\u00f6h\u00b7nen", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und es sagte ihm Frau Kaludrigkeit:", "tokens": ["Und", "es", "sag\u00b7te", "ihm", "Frau", "Ka\u00b7lud\u00b7rig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "NN", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "\u00bbvielleicht. Vielleicht.", "tokens": ["\u00bb", "viel\u00b7leicht", ".", "Viel\u00b7leicht", "."], "token_info": ["punct", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "$.", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mit der Zeit . . . mit der Zeit . . . \u00ab", "tokens": ["Mit", "der", "Zeit", ".", ".", ".", "mit", "der", "Zeit", ".", ".", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$.", "$.", "APPR", "ART", "NN", "$.", "$.", "$.", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und der Landrat begann alln\u00e4chtlich im Schlafe", "tokens": ["Und", "der", "Land\u00b7rat", "be\u00b7gann", "all\u00b7n\u00e4cht\u00b7lich", "im", "Schla\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "laut zu sprechen und wurde ihr Schklafe", "tokens": ["laut", "zu", "spre\u00b7chen", "und", "wur\u00b7de", "ihr", "Sch\u00b7kla\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKZU", "VVINF", "KON", "VAFIN", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "und er war ihr h\u00f6rig und sah alle Zeit", "tokens": ["und", "er", "war", "ihr", "h\u00f6\u00b7rig", "und", "sah", "al\u00b7le", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADJD", "KON", "VVFIN", "PIAT", "NN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Frau Kaludrigkeit \u2013 Frau Kaludrigkeit!", "tokens": ["Frau", "Ka\u00b7lud\u00b7rig\u00b7keit", "\u2013", "Frau", "Ka\u00b7lud\u00b7rig\u00b7keit", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "NN", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Und obgleich der Landrat zum Zentrum geh\u00f6rte,", "tokens": ["Und", "ob\u00b7gleich", "der", "Land\u00b7rat", "zum", "Zent\u00b7rum", "ge\u00b7h\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "wars eine Schande, wie da\u00df er r\u00f6hrte;", "tokens": ["wars", "ei\u00b7ne", "Schan\u00b7de", ",", "wie", "da\u00df", "er", "r\u00f6hr\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "er schlich der Kaludrigkeit ums Haus . . .", "tokens": ["er", "schlich", "der", "Ka\u00b7lud\u00b7rig\u00b7keit", "ums", "Haus", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "APPRART", "NN", "$.", "$.", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die hie\u00df so \u2013 und sah ganz anders aus:", "tokens": ["Die", "hie\u00df", "so", "\u2013", "und", "sah", "ganz", "an\u00b7ders", "aus", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$(", "KON", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ihre Mutter hatte es einst in Brasilien", "tokens": ["Ih\u00b7re", "Mut\u00b7ter", "hat\u00b7te", "es", "einst", "in", "Bra\u00b7si\u00b7li\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "APPR", "NE"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "mit einem Herrn der bessern Familien.", "tokens": ["mit", "ei\u00b7nem", "Herrn", "der", "bes\u00b7sern", "Fa\u00b7mi\u00b7li\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie war ein Halbblut, ein Viertelblut:", "tokens": ["Sie", "war", "ein", "Halb\u00b7blut", ",", "ein", "Vier\u00b7tel\u00b7blut", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "nu\u00dfbraun, kreolisch; es stand ihr sehr gut.", "tokens": ["nu\u00df\u00b7braun", ",", "kre\u00b7o\u00b7lisch", ";", "es", "stand", "ihr", "sehr", "gut", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "$.", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Und der Landrat balzte: Wann ist es soweit?", "tokens": ["Und", "der", "Land\u00b7rat", "balz\u00b7te", ":", "Wann", "ist", "es", "so\u00b7weit", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$.", "PWAV", "VAFIN", "PPER", "ADV", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.10": {"text": "Frau Kaludrigkeit \u2013 Frau Kaludrigkeit!", "tokens": ["Frau", "Ka\u00b7lud\u00b7rig\u00b7keit", "\u2013", "Frau", "Ka\u00b7lud\u00b7rig\u00b7keit", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "NN", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Und eines Abends im Monat September", "tokens": ["Und", "ei\u00b7nes", "A\u00b7bends", "im", "Mo\u00b7nat", "Sep\u00b7tem\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "NN"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "war das Halbblut m\u00fcde von seinem Gebember", "tokens": ["war", "das", "Halb\u00b7blut", "m\u00fc\u00b7de", "von", "sei\u00b7nem", "Ge\u00b7bem\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "und zog sich aus. Und sagte: \u00bbIch bin . . . \u00ab", "tokens": ["und", "zog", "sich", "aus", ".", "Und", "sag\u00b7te", ":", "\u00bb", "Ich", "bin", ".", ".", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "$.", "KON", "VVFIN", "$.", "$(", "PPER", "VAFIN", "$.", "$.", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "und legte sich herrlich nu\u00dfbraun hin.", "tokens": ["und", "leg\u00b7te", "sich", "herr\u00b7lich", "nu\u00df\u00b7braun", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der Landrat dachte, ihn tr\u00e4fe der Schlag!", "tokens": ["Der", "Land\u00b7rat", "dach\u00b7te", ",", "ihn", "tr\u00e4\u00b7fe", "der", "Schlag", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Unvorbereitet fand ihn der Tag.", "tokens": ["Un\u00b7vor\u00b7be\u00b7rei\u00b7tet", "fand", "ihn", "der", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Nie h\u00e4tt er gehofft, es noch zu erreichen.", "tokens": ["Nie", "h\u00e4tt", "er", "ge\u00b7hofft", ",", "es", "noch", "zu", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und er ging hin und tat desgleichen.", "tokens": ["Und", "er", "ging", "hin", "und", "tat", "des\u00b7glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie lag auf den Armen und atmete kaum.", "tokens": ["Sie", "lag", "auf", "den", "Ar\u00b7men", "und", "at\u00b7me\u00b7te", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "ADV", "$."], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihr Pyjama flammte, ein bunter Traum.", "tokens": ["Ihr", "Py\u00b7ja\u00b7ma", "flamm\u00b7te", ",", "ein", "bun\u00b7ter", "Traum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Er glaubte, ihren Herzschlag zu sp\u00fcren.", "tokens": ["Er", "glaub\u00b7te", ",", "ih\u00b7ren", "Herz\u00b7schlag", "zu", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er wagte sie nicht mehr zu ber\u00fchren . . .", "tokens": ["Er", "wag\u00b7te", "sie", "nicht", "mehr", "zu", "be\u00b7r\u00fch\u00b7ren", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKZU", "VVINF", "$.", "$.", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Er sann, der Landrat. Was war das, soeben?", "tokens": ["Er", "sann", ",", "der", "Land\u00b7rat", ".", "Was", "war", "das", ",", "soe\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$.", "PWS", "VAFIN", "PDS", "$,", "ADV", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie hatte ihm alles und nichts gegeben.", "tokens": ["Sie", "hat\u00b7te", "ihm", "al\u00b7les", "und", "nichts", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "KON", "PIS", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Und obgleich der Landrat vom Zentrum war,", "tokens": ["Und", "ob\u00b7gleich", "der", "Land\u00b7rat", "vom", "Zent\u00b7rum", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "wurde ihm pl\u00f6tzlich eines klar:", "tokens": ["wur\u00b7de", "ihm", "pl\u00f6tz\u00b7lich", "ei\u00b7nes", "klar", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ART", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Er war nicht der Mann f\u00fcr dieses Wesen.", "tokens": ["Er", "war", "nicht", "der", "Mann", "f\u00fcr", "die\u00b7ses", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Sie war ein Buch. Er k\u00f6nnt es nicht lesen.", "tokens": ["Sie", "war", "ein", "Buch", ".", "Er", "k\u00f6nnt", "es", "nicht", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Was dann zwischen Liebenden vor sich geht,", "tokens": ["Was", "dann", "zwi\u00b7schen", "Lie\u00b7ben\u00b7den", "vor", "sich", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "ist eine leere Formalit\u00e4t.", "tokens": ["ist", "ei\u00b7ne", "lee\u00b7re", "For\u00b7ma\u00b7li\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Und so lernte der Mann in Minutenfrist,", "tokens": ["Und", "so", "lern\u00b7te", "der", "Mann", "in", "Mi\u00b7nu\u00b7ten\u00b7frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "da\u00df nicht jede Erf\u00fcllung Erf\u00fcllung ist.", "tokens": ["da\u00df", "nicht", "je\u00b7de", "Er\u00b7f\u00fcl\u00b7lung", "Er\u00b7f\u00fcl\u00b7lung", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PIAT", "NN", "NN", "VAFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und bel\u00e4stigte nie mehr seit dieser Zeit", "tokens": ["Und", "be\u00b7l\u00e4s\u00b7tig\u00b7te", "nie", "mehr", "seit", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "die sch\u00f6ne Frau Inez Kaludrigkeit.", "tokens": ["die", "sch\u00f6\u00b7ne", "Frau", "I\u00b7nez", "Ka\u00b7lud\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}