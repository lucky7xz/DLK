{"dta.poem.23120": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "200. Was man liebt/ in da\u00df verwandelt  \n man sich au\u00df  S.  Augustino.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch was du liebst in da\u00df wirstu verwandelt werde\u0303", "tokens": ["Mensch", "was", "du", "liebst", "in", "da\u00df", "wirs\u00b7tu", "ver\u00b7wan\u00b7delt", "werd\u1ebd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PWS", "PPER", "VVFIN", "APPR", "KOUS", "PPER", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gott wirstu liebstu GOtt/ und Erde liebstu Erden.", "tokens": ["Gott", "wirs\u00b7tu", "liebs\u00b7tu", "Gott", "/", "und", "Er\u00b7de", "liebs\u00b7tu", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVFIN", "NN", "$(", "KON", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}