{"textgrid.poem.54432": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Der Autor vber seinen Geburts-Tag", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Ich di\u00df Jammerhau\u00df der Welt solt erst beschreiten/", "tokens": ["Als", "Ich", "di\u00df", "Jam\u00b7mer\u00b7hau\u00df", "der", "Welt", "solt", "erst", "be\u00b7schrei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "NN", "ART", "NN", "VMFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd nichts als Angst v\u00f1 Noth/ man hier gewertig war;", "tokens": ["Vnd", "nichts", "als", "Angst", "v\u00f1", "Noth", "/", "man", "hier", "ge\u00b7wer\u00b7tig", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "NN", "APPR", "NN", "$(", "PIS", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vmbringstu Jesu mich mit deiner Engel Schar.", "tokens": ["Vm\u00b7brings\u00b7tu", "Je\u00b7su", "mich", "mit", "dei\u00b7ner", "En\u00b7gel", "Schar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PRF", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch der auffsicht! (ob schon mein Fu\u00df hat m\u00fcssen gleiten", "tokens": ["Durch", "der", "auff\u00b7sicht", "!", "(", "ob", "schon", "mein", "Fu\u00df", "hat", "m\u00fcs\u00b7sen", "glei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "$(", "KOUS", "ADV", "PPOSAT", "NN", "VAFIN", "VMFIN", "VVINF"], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.5": {"text": "So sehr/ da\u00df man mir auch das Grab offt wolt bereiten)", "tokens": ["So", "sehr", "/", "da\u00df", "man", "mir", "auch", "das", "Grab", "offt", "wolt", "be\u00b7rei\u00b7ten", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "KOUS", "PIS", "PPER", "ADV", "ART", "NN", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich dennoch bin entsetzt viel tausendfacher Gfahr/", "tokens": ["Ich", "den\u00b7noch", "bin", "ent\u00b7setzt", "viel", "tau\u00b7send\u00b7fa\u00b7cher", "Gfahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "VVPP", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Di\u00df hastu meinen Geist versichert hell vnd klar.", "tokens": ["Di\u00df", "has\u00b7tu", "mei\u00b7nen", "Geist", "ver\u00b7si\u00b7chert", "hell", "vnd", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil du mich an dem Tag ins Leben th\u00e4test leiten/", "tokens": ["Weil", "du", "mich", "an", "dem", "Tag", "ins", "Le\u00b7ben", "th\u00e4\u00b7test", "lei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "An dehm der Engel-Printz den Teuffel ", "tokens": ["An", "dehm", "der", "En\u00b7gel\u00b7Printz", "den", "Teuf\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "O der du mich bi\u00dfher so wunderlich gef\u00fchrt/", "tokens": ["O", "der", "du", "mich", "bi\u00df\u00b7her", "so", "wun\u00b7der\u00b7lich", "ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "PPER", "PRF", "ADV", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Gib da\u00df das Lebensziel/ so Ich noch hie zu lauffen/", "tokens": ["Gib", "da\u00df", "das", "Le\u00b7bens\u00b7ziel", "/", "so", "Ich", "noch", "hie", "zu", "lauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KOUS", "ART", "NN", "$(", "ADV", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Durch dieser W\u00e4chter Schutz mir m\u00f6ge sicher seyn:", "tokens": ["Durch", "die\u00b7ser", "W\u00e4ch\u00b7ter", "Schutz", "mir", "m\u00f6\u00b7ge", "si\u00b7cher", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Vnnd wenn der letzte Tag des Todes nun bricht ein/", "tokens": ["Vnnd", "wenn", "der", "letz\u00b7te", "Tag", "des", "To\u00b7des", "nun", "bricht", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So la\u00df mich fr\u00f6lich gehn zu deiner Engel hauffen.", "tokens": ["So", "la\u00df", "mich", "fr\u00f6\u00b7lich", "gehn", "zu", "dei\u00b7ner", "En\u00b7gel", "hauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}