{"textgrid.poem.39701": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vergib, o Freund! da\u00df ich mit kind'scher Sprache,", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergib, o Freund! da\u00df ich mit kind'scher Sprache,", "tokens": ["Ver\u00b7gib", ",", "o", "Freund", "!", "da\u00df", "ich", "mit", "kin\u00b7d'\u00b7scher", "Spra\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "NN", "$.", "KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus deines Herzens tiefem Heiligthume,", "tokens": ["Aus", "dei\u00b7nes", "Her\u00b7zens", "tie\u00b7fem", "Hei\u00b7lig\u00b7thu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Akkorde leise nachzulallen wage,", "tokens": ["Ak\u00b7kor\u00b7de", "lei\u00b7se", "nach\u00b7zu\u00b7lal\u00b7len", "wa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Beim H\u00f6chsten aber sch\u00fclerhaft verstumme.", "tokens": ["Beim", "H\u00f6chs\u00b7ten", "a\u00b7ber", "sch\u00fc\u00b7ler\u00b7haft", "ver\u00b7stum\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und reden m\u00f6cht' ich doch zu deinem Ruhme,", "tokens": ["Und", "re\u00b7den", "m\u00f6cht'", "ich", "doch", "zu", "dei\u00b7nem", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergib der K\u00fchnheit, da\u00df ich nicht verzage.", "tokens": ["Ver\u00b7gib", "der", "K\u00fchn\u00b7heit", ",", "da\u00df", "ich", "nicht", "ver\u00b7za\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Sommer mein' ich mit der Einen Blume,", "tokens": ["Den", "Som\u00b7mer", "mein'", "ich", "mit", "der", "Ei\u00b7nen", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Einen Strahl entwand ich nur dem Tage.", "tokens": ["Und", "Ei\u00b7nen", "Strahl", "ent\u00b7wand", "ich", "nur", "dem", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch die Natur in ihrer heil'gen F\u00fclle", "tokens": ["Doch", "die", "Na\u00b7tur", "in", "ih\u00b7rer", "heil'\u00b7gen", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie offenbart sich ganz in jedem Handeln,", "tokens": ["Sie", "of\u00b7fen\u00b7bart", "sich", "ganz", "in", "je\u00b7dem", "Han\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das h\u00f6chste Leben in der tiefsten Stille.", "tokens": ["Das", "h\u00f6chs\u00b7te", "Le\u00b7ben", "in", "der", "tiefs\u00b7ten", "Stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Erhascht' ich einen Zug aus deinem Bilde,", "tokens": ["Er\u00b7hascht'", "ich", "ei\u00b7nen", "Zug", "aus", "dei\u00b7nem", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie reichlich auch Gedanken in dir wandeln,", "tokens": ["Wie", "reich\u00b7lich", "auch", "Ge\u00b7dan\u00b7ken", "in", "dir", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So bist du's ganz in deiner frommen Milde.", "tokens": ["So", "bist", "du's", "ganz", "in", "dei\u00b7ner", "from\u00b7men", "Mil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}