{"dta.poem.19143": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "CxXI.  Neu-Jahrs-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr uns gesalbtes Haupt,", "tokens": ["F\u00fcr", "uns", "ge\u00b7salb\u00b7tes", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr uns gezeugter Saame,", "tokens": ["F\u00fcr", "uns", "ge\u00b7zeug\u00b7ter", "Saa\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr uns genannter Nahme,", "tokens": ["F\u00fcr", "uns", "ge\u00b7nann\u00b7ter", "Nah\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr jeden, der es glaubt:", "tokens": ["F\u00fcr", "je\u00b7den", ",", "der", "es", "glaubt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du siehst vor deinen Thronen,", "tokens": ["Du", "siehst", "vor", "dei\u00b7nen", "Thro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo Majest\u00e4ten wohnen;", "tokens": ["Wo", "Ma\u00b7jes\u00b7t\u00e4\u00b7ten", "woh\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du siehst di\u00df kleine Heer.", "tokens": ["Du", "siehst", "di\u00df", "klei\u00b7ne", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ach! wenns das Grosse w\u00e4r.", "tokens": ["Ach", "!", "wenns", "das", "Gros\u00b7se", "w\u00e4r", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir leben ja darum,", "tokens": ["Wir", "le\u00b7ben", "ja", "da\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df wir dem JEsu leben,", "tokens": ["Da\u00df", "wir", "dem", "Je\u00b7su", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NE", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der sich f\u00fcr uns gegeben,", "tokens": ["Der", "sich", "f\u00fcr", "uns", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir suchen um und um", "tokens": ["Wir", "su\u00b7chen", "um", "und", "um"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "KOUI"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jm Gro\u00dfen wie im Kleinen,", "tokens": ["Jm", "Gro\u00b7\u00dfen", "wie", "im", "Klei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es treu mit Jhm zu meynen:", "tokens": ["Es", "treu", "mit", "Jhm", "zu", "mey\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir suchens: Aber ach!", "tokens": ["Wir", "su\u00b7chens", ":", "A\u00b7ber", "ach", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Das ist noch nicht die Sach.", "tokens": ["Das", "ist", "noch", "nicht", "die", "Sach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dich lieben, lieber GOtt!", "tokens": ["Dich", "lie\u00b7ben", ",", "lie\u00b7ber", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das ist zur Pflicht geworden", "tokens": ["Das", "ist", "zur", "Pflicht", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "VAPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bey dem gefallnen Orden,", "tokens": ["Bey", "dem", "ge\u00b7fall\u00b7nen", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herr, dir und uns zum Spott.", "tokens": ["Herr", ",", "dir", "und", "uns", "zum", "Spott", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "KON", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dich lieben, dich umfassen,", "tokens": ["Dich", "lie\u00b7ben", ",", "dich", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sich deinen H\u00e4nden lassen,", "tokens": ["Sich", "dei\u00b7nen", "H\u00e4n\u00b7den", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "War erst der Creatur", "tokens": ["War", "erst", "der", "Crea\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Pur lautere Natur.", "tokens": ["Pur", "lau\u00b7te\u00b7re", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Wir haben abermahl,", "tokens": ["Wir", "ha\u00b7ben", "a\u00b7ber\u00b7mahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du Alter ausser Jahren,", "tokens": ["Du", "Al\u00b7ter", "aus\u00b7ser", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Jahr daher erfahren", "tokens": ["Ein", "Jahr", "da\u00b7her", "er\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PAV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Trieb der Gnaden-Wahl,", "tokens": ["Den", "Trieb", "der", "Gna\u00b7den\u00b7Wahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie l\u00e4st ihr Werck nicht sch\u00e4nden:", "tokens": ["Sie", "l\u00e4st", "ihr", "Werck", "nicht", "sch\u00e4n\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und wers nicht sehen kan,", "tokens": ["Und", "wers", "nicht", "se\u00b7hen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der ist ein blinder Mann.", "tokens": ["Der", "ist", "ein", "blin\u00b7der", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ja, Amen! du hast recht,", "tokens": ["Ja", ",", "A\u00b7men", "!", "du", "hast", "recht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$.", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dein Ja ist Ja geblieben,", "tokens": ["Dein", "Ja", "ist", "Ja", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Herrnhuth ist beklieben,", "tokens": ["Und", "Herrn\u00b7huth", "ist", "be\u00b7klie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein eigenes Geschlecht:", "tokens": ["Dein", "ei\u00b7ge\u00b7nes", "Ge\u00b7schlecht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+---+", "measure": "dactylic.init"}, "line.5": {"text": "Dein Nein wird Nein bedeuten,", "tokens": ["Dein", "Nein", "wird", "Nein", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch alle Ewigkeiten.", "tokens": ["Durch", "al\u00b7le", "E\u00b7wig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sprichst du zu etwas Nein,", "tokens": ["Sprichst", "du", "zu", "et\u00b7was", "Nein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKA", "PIS", "PTKANT", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Das fall uns nimmer ein.", "tokens": ["Das", "fall", "uns", "nim\u00b7mer", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wer ist nun noch bey GOtt?", "tokens": ["Wer", "ist", "nun", "noch", "bey", "Gott", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Jhr Br\u00fcder, wer kans sagen?", "tokens": ["Ihr", "Br\u00fc\u00b7der", ",", "wer", "kans", "sa\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch ists auch noth zu fragen?", "tokens": ["Doch", "ists", "auch", "noth", "zu", "fra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Solt ein gerechter Loth", "tokens": ["Solt", "ein", "ge\u00b7rech\u00b7ter", "Loth"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In Sodoms S\u00fcnden-Mauren", "tokens": ["In", "So\u00b7doms", "S\u00fcn\u00b7den\u00b7Mau\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Bey seinem GOtte dauren,", "tokens": ["Bey", "sei\u00b7nem", "Got\u00b7te", "dau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wir bey Salems Schein", "tokens": ["Und", "wir", "bey", "Sa\u00b7lems", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der Liebe untreu seyn.", "tokens": ["Der", "Lie\u00b7be", "un\u00b7treu", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O nein! in unsrer Schul", "tokens": ["O", "nein", "!", "in", "uns\u00b7rer", "Schul"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKANT", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Lernt man zu Christi F\u00fcssen", "tokens": ["Lernt", "man", "zu", "Chris\u00b7ti", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von Gnad auf Treue schliessen,", "tokens": ["Von", "Gnad", "auf", "Treu\u00b7e", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom Kampff auff Christi Stuhl,", "tokens": ["Vom", "Kampff", "auff", "Chris\u00b7ti", "Stuhl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir lernen uns verkennen,", "tokens": ["Wir", "ler\u00b7nen", "uns", "ver\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wir lernen JEsum nennen,", "tokens": ["Wir", "ler\u00b7nen", "Je\u00b7sum", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "VVINF", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.7": {"text": "Und jedes Wort das hafft,", "tokens": ["Und", "je\u00b7des", "Wort", "das", "hafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und wird zu Geist und Krafft.", "tokens": ["Und", "wird", "zu", "Geist", "und", "Krafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Erscheine, grosser Freund!", "tokens": ["Er\u00b7schei\u00b7ne", ",", "gros\u00b7ser", "Freund", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In deiner Creutz-Gemeine,", "tokens": ["In", "dei\u00b7ner", "Creutz\u00b7Ge\u00b7mei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In Herrlichkeit erscheine:", "tokens": ["In", "Herr\u00b7lich\u00b7keit", "er\u00b7schei\u00b7ne", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Errette manchen Feind", "tokens": ["Er\u00b7ret\u00b7te", "man\u00b7chen", "Feind"], "token_info": ["word", "word", "word"], "pos": ["NN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu diesen Gnaden-Stunden", "tokens": ["Zu", "die\u00b7sen", "Gna\u00b7den\u00b7Stun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Jm Steinritz deiner Wunden,", "tokens": ["Jm", "Stein\u00b7ritz", "dei\u00b7ner", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bi\u00df er mit uns zugleich", "tokens": ["Bi\u00df", "er", "mit", "uns", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Liebt die Gedult am Reich.", "tokens": ["Liebt", "die", "Ge\u00b7dult", "am", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.9": {"line.1": {"text": "Uns aber segne du", "tokens": ["Uns", "a\u00b7ber", "seg\u00b7ne", "du"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"t