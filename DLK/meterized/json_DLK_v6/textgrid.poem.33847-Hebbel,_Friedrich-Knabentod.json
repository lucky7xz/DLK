{"textgrid.poem.33847": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Knabentod", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Berg, der Knab',", "tokens": ["Vom", "Berg", ",", "der", "Knab'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der zieht hinab", "tokens": ["Der", "zieht", "hin\u00b7ab"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "In hei\u00dfen Sommertagen;", "tokens": ["In", "hei\u00b7\u00dfen", "Som\u00b7mer\u00b7ta\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Tannenwald,", "tokens": ["Im", "Tan\u00b7nen\u00b7wald", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Da macht er Halt,", "tokens": ["Da", "macht", "er", "Halt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Er kann sich kaum noch tragen.", "tokens": ["Er", "kann", "sich", "kaum", "noch", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Den wilden Bach,", "tokens": ["Den", "wil\u00b7den", "Bach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Er sieht ihn jach", "tokens": ["Er", "sieht", "ihn", "jach"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "In's Thal herunter sch\u00e4umen;", "tokens": ["In's", "Thal", "her\u00b7un\u00b7ter", "sch\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APZR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihn d\u00fcrstet sehr,", "tokens": ["Ihn", "d\u00fcrs\u00b7tet", "sehr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Nun noch viel mehr:", "tokens": ["Nun", "noch", "viel", "mehr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Nur hin! Wer w\u00fcrde s\u00e4umen!", "tokens": ["Nur", "hin", "!", "Wer", "w\u00fcr\u00b7de", "s\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "PWS", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da ist die Flut!", "tokens": ["Da", "ist", "die", "Flut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "O, in der Glut,", "tokens": ["O", ",", "in", "der", "Glut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Was kann so k\u00f6stlich blinken!", "tokens": ["Was", "kann", "so", "k\u00f6st\u00b7lich", "blin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er sch\u00f6pft und trinkt,", "tokens": ["Er", "sch\u00f6pft", "und", "trinkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Er st\u00fcrzt und sinkt", "tokens": ["Er", "st\u00fcrzt", "und", "sinkt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und trinkt noch im Versinken!", "tokens": ["Und", "trinkt", "noch", "im", "Ver\u00b7sin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Lied ist aus,", "tokens": ["Das", "Lied", "ist", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und macht's dir Graus:", "tokens": ["Und", "macht's", "dir", "Graus", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wer wird's im Winter singen!", "tokens": ["Wer", "wird's", "im", "Win\u00b7ter", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur Sommerzeit", "tokens": ["Zur", "Som\u00b7mer\u00b7zeit"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Bist du bereit,", "tokens": ["Bist", "du", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Dem Knaben nachzuspringen.", "tokens": ["Dem", "Kna\u00b7ben", "nach\u00b7zu\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}