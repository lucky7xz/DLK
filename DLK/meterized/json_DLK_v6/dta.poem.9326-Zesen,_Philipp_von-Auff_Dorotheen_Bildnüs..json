{"dta.poem.9326": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Auff Dorotheen Bildn\u00fcs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ziert doch Jungfern wohl? ich m\u00f6cht es gerne", "tokens": ["Was", "ziert", "doch", "Jung\u00b7fern", "wohl", "?", "ich", "m\u00f6cht", "es", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "NN", "ADV", "$.", "PPER", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wissen/", "tokens": ["wis\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "ey mein/ ey sagt miers doch? Das unversch\u00e4mte k\u00fcssen", "tokens": ["ey", "mein", "/", "ey", "sagt", "miers", "doch", "?", "Das", "un\u00b7ver\u00b7sch\u00e4m\u00b7te", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPOSAT", "$(", "NE", "VVFIN", "ADV", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mit jedem/ oder ists der g\u00fcldnen Kleider-pracht?", "tokens": ["mit", "je\u00b7dem", "/", "o\u00b7der", "ists", "der", "g\u00fcld\u00b7nen", "Klei\u00b7der\u00b7pracht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$(", "KON", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O nein: die reden seyn mier warlich sehr verdacht.", "tokens": ["O", "nein", ":", "die", "re\u00b7den", "seyn", "mier", "war\u00b7lich", "sehr", "ver\u00b7dacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "ART", "VVINF", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich wei\u00df was besser ziert; geht fraget Dorotheen/", "tokens": ["Ich", "wei\u00df", "was", "bes\u00b7ser", "ziert", ";", "geht", "fra\u00b7get", "Do\u00b7ro\u00b7theen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "ADJD", "VVFIN", "$.", "VVFIN", "VVFIN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was gilts/ die keuschheit ists/ Sie wird es euch gestehe\u0303/", "tokens": ["Was", "gilts", "/", "die", "keuschheit", "ists", "/", "Sie", "wird", "es", "euch", "ge\u00b7steh\u1ebd", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "ART", "NN", "VAFIN", "$(", "PPER", "VAFIN", "PPER", "PPER", "VVPP", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "da\u00df sie den Todt begehrt/ eh sie von keuschheit zier", "tokens": ["da\u00df", "sie", "den", "Todt", "be\u00b7gehrt", "/", "eh", "sie", "von", "keuschheit", "zier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$(", "KOUS", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "vnd remen Lehre wich; der folget nun auch Jhr.", "tokens": ["vnd", "re\u00b7men", "Leh\u00b7re", "wich", ";", "der", "fol\u00b7get", "nun", "auch", "Ihr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$.", "ART", "VVFIN", "ADV", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}