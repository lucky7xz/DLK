{"dta.poem.17137": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "238.  Die innerliche Geburt Gottes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ach freude! GOtt wird Mensch/ und ist auch schon ge-", "tokens": ["Ach", "freu\u00b7de", "!", "Gott", "wird", "Mensch", "/", "und", "ist", "auch", "schon", "ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "NN", "VAFIN", "NN", "$(", "KON", "VAFIN", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "bohren!", "tokens": ["boh\u00b7ren", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wo da? In mir: Er hat zur Mutter mich erkohren.", "tokens": ["Wo", "da", "?", "In", "mir", ":", "Er", "hat", "zur", "Mut\u00b7ter", "mich", "er\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$.", "APPR", "PPER", "$.", "PPER", "VAFIN", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie gehet es dann zu? ", "tokens": ["Wie", "ge\u00b7het", "es", "dann", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Krieppel ein mein Hertz/ der Leib der ist die H\u00f6l/", "tokens": ["Das", "Kriep\u00b7pel", "ein", "mein", "Hertz", "/", "der", "Leib", "der", "ist", "die", "H\u00f6l", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPOSAT", "NN", "$(", "ART", "NN", "ART", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die neu Gerechtigkeit sind Windeln und sind Binden:", "tokens": ["Die", "neu", "Ge\u00b7rech\u00b7tig\u00b7keit", "sind", "Win\u00b7deln", "und", "sind", "Bin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "NN", "KON", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Joseph Gottes Furcht: Die Kr\u00e4ffte de\u00df Gem\u00fctts", "tokens": ["Der", "Jo\u00b7se\u00b7ph", "Got\u00b7tes", "Furcht", ":", "Die", "Kr\u00e4ff\u00b7te", "de\u00df", "Ge\u00b7m\u00fctts"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NE", "NN", "NN", "$.", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Sind Engel die sich freun: Die Klarheit ist jhr Blitz:", "tokens": ["Sind", "En\u00b7gel", "die", "sich", "freun", ":", "Die", "Klar\u00b7heit", "ist", "jhr", "Blitz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PRELS", "PRF", "VVINF", "$.", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die keusche Sinnen sind die Hirten die jhn finden.", "tokens": ["Die", "keu\u00b7sche", "Sin\u00b7nen", "sind", "die", "Hir\u00b7ten", "die", "jhn", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "ART", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}