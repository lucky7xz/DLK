{"textgrid.poem.58794": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "13. Sei stets bereit!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es k\u00f6nnt' in dieser Stund' geschehen,", "tokens": ["Es", "k\u00f6nnt'", "in", "die\u00b7ser", "Stund'", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du zur Ewigkeit m\u00fc\u00dft'st gehen;", "tokens": ["Da\u00df", "du", "zur", "E\u00b7wig\u00b7keit", "m\u00fc\u00dft'st", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O Seel', was wolltest du ", "tokens": ["O", "Seel'", ",", "was", "woll\u00b7test", "du"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PWS", "VMFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn du es wei\u00dft, so tu es ", "tokens": ["Wenn", "du", "es", "wei\u00dft", ",", "so", "tu", "es"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}