{"dta.poem.8075": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Graf Eberstein .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Zu Speier im Saale, da hebt sich ein Klingen,", "tokens": ["Zu", "Spei\u00b7er", "im", "Saa\u00b7le", ",", "da", "hebt", "sich", "ein", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "NN", "$,", "ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mit Fackeln und Kerzen ein Tanzen und Springen.", "tokens": ["Mit", "Fa\u00b7ckeln", "und", "Ker\u00b7zen", "ein", "Tan\u00b7zen", "und", "Sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Graf Eberstein", "tokens": ["Graf", "E\u00b7bers\u00b7tein"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "-+--", "measure": "dactylic.init"}, "line.4": {"text": "F\u00fchret den Reihn", "tokens": ["F\u00fch\u00b7ret", "den", "Reihn"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Mit des Kaisers holdseligem T\u00f6chterlein.", "tokens": ["Mit", "des", "Kai\u00b7sers", "hold\u00b7se\u00b7li\u00b7gem", "T\u00f6ch\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Und als er sie schwingt nun im luftigen Reigen,", "tokens": ["Und", "als", "er", "sie", "schwingt", "nun", "im", "luf\u00b7ti\u00b7gen", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da fl\u00fcstert sie leise, sie kann\u2019s nicht verschweigen:", "tokens": ["Da", "fl\u00fcs\u00b7tert", "sie", "lei\u00b7se", ",", "sie", "kann's", "nicht", "ver\u00b7schwei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+--+-++--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u201egraf Eberstein,", "tokens": ["\u201e", "graf", "E\u00b7bers\u00b7tein", ","], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "H\u00fcte dich fein!", "tokens": ["H\u00fc\u00b7te", "dich", "fein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Heut Nacht wird dein Schl\u00f6\u00dflein gef\u00e4hrdet seyn.\u201c", "tokens": ["Heut", "Nacht", "wird", "dein", "Schl\u00f6\u00df\u00b7lein", "ge\u00b7f\u00e4hr\u00b7det", "seyn", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ei! denket der Graf, Euer kaiserlich\u2019 Gnaden,", "tokens": ["Ei", "!", "den\u00b7ket", "der", "Graf", ",", "Eu\u00b7er", "kai\u00b7ser\u00b7lich'", "Gna\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "$,", "PPOSAT", "PIAT", "NN", "$,"], "meter": "----++-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So habt Ihr mich darum zum Tanze geladen!", "tokens": ["So", "habt", "Ihr", "mich", "da\u00b7rum", "zum", "Tan\u00b7ze", "ge\u00b7la\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "PAV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er sucht sein Ro\u00df,", "tokens": ["Er", "sucht", "sein", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "L\u00e4\u00dft seinen Tro\u00df", "tokens": ["L\u00e4\u00dft", "sei\u00b7nen", "Tro\u00df"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und jagt nach seinem gef\u00e4hrdeten Schlo\u00df.", "tokens": ["Und", "jagt", "nach", "sei\u00b7nem", "ge\u00b7f\u00e4hr\u00b7de\u00b7ten", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Um Ebersteins Veste da wimmelt\u2019s von Streitern,", "tokens": ["Um", "E\u00b7bers\u00b7teins", "Ves\u00b7te", "da", "wim\u00b7melt's", "von", "Strei\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NE", "NE", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie schleichen im Nebel mit Hacken und Leitern.", "tokens": ["Sie", "schlei\u00b7chen", "im", "Ne\u00b7bel", "mit", "Ha\u00b7cken", "und", "Lei\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Graf Eberstein", "tokens": ["Graf", "E\u00b7bers\u00b7tein"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "-+--", "measure": "dactylic.init"}, "line.4": {"text": "Gr\u00fc\u00dfet sie fein,", "tokens": ["Gr\u00fc\u00b7\u00dfet", "sie", "fein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Er wirft sie vom Wall in die Gr\u00e4ben hinein.", "tokens": ["Er", "wirft", "sie", "vom", "Wall", "in", "die", "Gr\u00e4\u00b7ben", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Als nun der Herr Kaiser am Morgen gekommen,", "tokens": ["Als", "nun", "der", "Herr", "Kai\u00b7ser", "am", "Mor\u00b7gen", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da meint er, es seye die Burg schon genommen.", "tokens": ["Da", "meint", "er", ",", "es", "se\u00b7ye", "die", "Burg", "schon", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch auf dem Wall", "tokens": ["Doch", "auf", "dem", "Wall"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Tanzen mit Schall", "tokens": ["Tan\u00b7zen", "mit", "Schall"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Der Graf und seine Gewappneten all.", "tokens": ["Der", "Graf", "und", "sei\u00b7ne", "Ge\u00b7wapp\u00b7ne\u00b7ten", "all", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "PIAT", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "\u201eherr Kaiser! beschleicht ihr ein andermal Schl\u00f6sser,", "tokens": ["\u201e", "herr", "Kai\u00b7ser", "!", "be\u00b7schleicht", "ihr", "ein", "an\u00b7der\u00b7mal", "Schl\u00f6s\u00b7ser", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "VVFIN", "PPER", "ART", "ADV", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Thut\u2019s Noth, Ihr verstehet auf\u2019s Tanzen Euch besser.", "tokens": ["Thut's", "Noth", ",", "Ihr", "ver\u00b7ste\u00b7het", "auf's", "Tan\u00b7zen", "Euch", "bes\u00b7ser", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "PPER", "ADJD", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Euer T\u00f6chterlein", "tokens": ["Eu\u00b7er", "T\u00f6ch\u00b7ter\u00b7lein"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Tanzet so fein,", "tokens": ["Tan\u00b7zet", "so", "fein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Dem soll meine Veste ge\u00f6ffnet seyn.\u201c", "tokens": ["Dem", "soll", "mei\u00b7ne", "Ves\u00b7te", "ge\u00b7\u00f6ff\u00b7net", "seyn", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "VVPP", "VAINF", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Im Schlosse des Grafen, da hebt sich ein Klingen,", "tokens": ["Im", "Schlos\u00b7se", "des", "Gra\u00b7fen", ",", "da", "hebt", "sich", "ein", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mit Fackeln und Kerzen ein Tanzen und Springen.", "tokens": ["Mit", "Fa\u00b7ckeln", "und", "Ker\u00b7zen", "ein", "Tan\u00b7zen", "und", "Sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Graf Eberstein", "tokens": ["Graf", "E\u00b7bers\u00b7tein"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "-+--", "measure": "dactylic.init"}, "line.4": {"text": "F\u00fchret den Reihn", "tokens": ["F\u00fch\u00b7ret", "den", "Reihn"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Mit des Kaisers holdseligem T\u00f6chterlein.", "tokens": ["Mit", "des", "Kai\u00b7sers", "hold\u00b7se\u00b7li\u00b7gem", "T\u00f6ch\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.8": {"line.1": {"text": "Und als er sie schwingt nun im br\u00e4utlichen Reigen,", "tokens": ["Und", "als", "er", "sie", "schwingt", "nun", "im", "br\u00e4ut\u00b7li\u00b7chen", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da fl\u00fcstert er leise, nicht kann er\u2019s verschweigen:", "tokens": ["Da", "fl\u00fcs\u00b7tert", "er", "lei\u00b7se", ",", "nicht", "kann", "er's", "ver\u00b7schwei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "PTKNEG", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "\u201esch\u00f6n Jungfr\u00e4ulein,", "tokens": ["\u201e", "sch\u00f6n", "Jung\u00b7fr\u00e4u\u00b7lein", ","], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "H\u00fcte dich fein!", "tokens": ["H\u00fc\u00b7te", "dich", "fein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Heut Nacht wird ein Schl\u00f6\u00dflein gef\u00e4hrdet seyn.\u201c", "tokens": ["Heut", "Nacht", "wird", "ein", "Schl\u00f6\u00df\u00b7lein", "ge\u00b7f\u00e4hr\u00b7det", "seyn", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "NN", "VAFIN", "ART", "NN", "VVPP", "VAINF", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}