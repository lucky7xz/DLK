{"textgrid.poem.24457": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber Ihre schwartze Z\u00e4hne", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Leib ist Salamander Art/", "tokens": ["Dein", "Leib", "ist", "Sa\u00b7la\u00b7man\u00b7der", "Art", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Den keine Flamme nie versehret.", "tokens": ["Den", "kei\u00b7ne", "Flam\u00b7me", "nie", "ver\u00b7seh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch deine Z\u00e4hne sind zu zart/", "tokens": ["Doch", "dei\u00b7ne", "Z\u00e4h\u00b7ne", "sind", "zu", "zart", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und durch des Hertzens Glut in schwartzen Ru\u00df verkehret.", "tokens": ["Und", "durch", "des", "Hert\u00b7zens", "Glut", "in", "schwart\u00b7zen", "Ru\u00df", "ver\u00b7keh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}