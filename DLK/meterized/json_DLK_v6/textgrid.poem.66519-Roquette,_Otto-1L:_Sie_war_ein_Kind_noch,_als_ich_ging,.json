{"textgrid.poem.66519": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie war ein Kind noch, als ich ging,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie war ein Kind noch, als ich ging,", "tokens": ["Sie", "war", "ein", "Kind", "noch", ",", "als", "ich", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf das ich wenig Acht gegeben;", "tokens": ["Auf", "das", "ich", "we\u00b7nig", "Acht", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "CARD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die jetzt mich fremd und kalt empfing", "tokens": ["Die", "jetzt", "mich", "fremd", "und", "kalt", "emp\u00b7fing"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PPER", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist hold erbl\u00fcht zum M\u00e4dchenleben.", "tokens": ["Ist", "hold", "er\u00b7bl\u00fcht", "zum", "M\u00e4d\u00b7chen\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch ob auch jungfr\u00e4ulich erwacht", "tokens": ["Doch", "ob", "auch", "jung\u00b7fr\u00e4u\u00b7lich", "er\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ADJD", "VVPP"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Ihr blaues Aug' und ihre Wange,", "tokens": ["Ihr", "blau\u00b7es", "Aug'", "und", "ih\u00b7re", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Kinderherz noch spricht und lacht", "tokens": ["Ein", "Kin\u00b7der\u00b7herz", "noch", "spricht", "und", "lacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aus ihrem Wesen und Gesange.", "tokens": ["Aus", "ih\u00b7rem", "We\u00b7sen", "und", "Ge\u00b7san\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie singt und summt den ganzen Tag,", "tokens": ["Sie", "singt", "und", "summt", "den", "gan\u00b7zen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Bienent\u00f6n' um Blumen ziehen,", "tokens": ["Wie", "Bienen\u00b7t\u00f6n'", "um", "Blu\u00b7men", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und was sie thun und schaffen mag", "tokens": ["Und", "was", "sie", "thun", "und", "schaf\u00b7fen", "mag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "VVINF", "KON", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Begleitet ist's von Melodieen.", "tokens": ["Be\u00b7glei\u00b7tet", "ist's", "von", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "NE", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und will sie mir am Fensterlein", "tokens": ["Und", "will", "sie", "mir", "am", "Fens\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur spr\u00f6de meinen Gru\u00df erwiedern,", "tokens": ["Nur", "spr\u00f6\u00b7de", "mei\u00b7nen", "Gru\u00df", "er\u00b7wie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie singt sich mir in's Herz hinein", "tokens": ["Sie", "singt", "sich", "mir", "in's", "Herz", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PPER", "APPRART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit ihren Kinderliedern.", "tokens": ["Mit", "ih\u00b7ren", "Kin\u00b7der\u00b7lie\u00b7dern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}