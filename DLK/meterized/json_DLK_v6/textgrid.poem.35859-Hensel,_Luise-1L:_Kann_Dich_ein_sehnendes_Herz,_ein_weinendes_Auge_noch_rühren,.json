{"textgrid.poem.35859": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kann Dich ein sehnendes Herz, ein weinendes Auge noch r\u00fchren,", "genre": "verse", "period": "N.A.", "pub_year": 1823, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kann Dich ein sehnendes Herz, ein weinendes Auge noch r\u00fchren,", "tokens": ["Kann", "Dich", "ein", "seh\u00b7nen\u00b7des", "Herz", ",", "ein", "wei\u00b7nen\u00b7des", "Au\u00b7ge", "noch", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Lebt noch mein Bildni\u00df in Dir, ach! so erbarme Dich mein!", "tokens": ["Lebt", "noch", "mein", "Bild\u00b7ni\u00df", "in", "Dir", ",", "ach", "!", "so", "er\u00b7bar\u00b7me", "Dich", "mein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PPER", "$,", "ITJ", "$.", "ADV", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Komm und befreie das Herz, dem Du Dich einst gn\u00e4dig erzeigtest,", "tokens": ["Komm", "und", "be\u00b7frei\u00b7e", "das", "Herz", ",", "dem", "Du", "Dich", "einst", "gn\u00e4\u00b7dig", "er\u00b7zeig\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Komm und erfreue das Aug', das Dich verwundete einst.", "tokens": ["Komm", "und", "er\u00b7freu\u00b7e", "das", "Aug'", ",", "das", "Dich", "ver\u00b7wun\u00b7de\u00b7te", "einst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "ADV", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Siehe, der Fr\u00fchling ist da; es bl\u00fchet die liebliche Schlehe", "tokens": ["Sie\u00b7he", ",", "der", "Fr\u00fch\u00b7ling", "ist", "da", ";", "es", "bl\u00fc\u00b7het", "die", "lieb\u00b7li\u00b7che", "Schle\u00b7he"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "ART", "NN", "VAFIN", "ADV", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und in den W\u00e4ldern erschallt l\u00e4ngst schon der Nachtigall Lied.", "tokens": ["Und", "in", "den", "W\u00e4l\u00b7dern", "er\u00b7schallt", "l\u00e4ngst", "schon", "der", "Nach\u00b7ti\u00b7gall", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Unsere Maien sind gr\u00fcn \u2013 mich labt nicht ihr freundliches Gr\u00fcnen.", "tokens": ["Un\u00b7se\u00b7re", "Mai\u00b7en", "sind", "gr\u00fcn", "\u2013", "mich", "labt", "nicht", "ihr", "freund\u00b7li\u00b7ches", "Gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$(", "PPER", "VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Duftende Veilchen erbl\u00fch'n, aber nicht freut mich ihr Duft. \u2013", "tokens": ["Duf\u00b7ten\u00b7de", "Veil\u00b7chen", "er\u00b7bl\u00fch'n", ",", "a\u00b7ber", "nicht", "freut", "mich", "ihr", "Duft", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,", "ADV", "PTKNEG", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Einsam verbirgt sich die Taub' im dunkelen, wilden Geh\u00f6lze,", "tokens": ["Ein\u00b7sam", "ver\u00b7birgt", "sich", "die", "Taub'", "im", "dun\u00b7ke\u00b7len", ",", "wil\u00b7den", "Ge\u00b7h\u00f6l\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Ruft durch die Stille des Hains, aber es schweiget der Hain.", "tokens": ["Ruft", "durch", "die", "Stil\u00b7le", "des", "Hains", ",", "a\u00b7ber", "es", "schwei\u00b7get", "der", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Klage, du Arme, mir nur, du findest in mir die Gesellin:", "tokens": ["Kla\u00b7ge", ",", "du", "Ar\u00b7me", ",", "mir", "nur", ",", "du", "fin\u00b7dest", "in", "mir", "die", "Ge\u00b7sel\u00b7lin", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "$,", "PPER", "ADV", "$,", "PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Einsam und sehnend bist du, einsam und sehnend bin ich.", "tokens": ["Ein\u00b7sam", "und", "seh\u00b7nend", "bist", "du", ",", "ein\u00b7sam", "und", "seh\u00b7nend", "bin", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "PPER", "$,", "ADJD", "KON", "VVPP", "VAFIN", "PPER", "$."], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Siehe, ich wei\u00df deinen Schmerz: der Liebste, den du dir erkoren,", "tokens": ["Sie\u00b7he", ",", "ich", "wei\u00df", "dei\u00b7nen", "Schmerz", ":", "der", "Liebs\u00b7te", ",", "den", "du", "dir", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Flog in die Ferne dahin, lie\u00df dich Betr\u00fcbte zur\u00fcck.", "tokens": ["Flog", "in", "die", "Fer\u00b7ne", "da\u00b7hin", ",", "lie\u00df", "dich", "Be\u00b7tr\u00fcb\u00b7te", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PAV", "$,", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ach! auch der Einzige mein, den fr\u00fch meine Seele erw\u00e4hlet,", "tokens": ["Ach", "!", "auch", "der", "Ein\u00b7zi\u00b7ge", "mein", ",", "den", "fr\u00fch", "mei\u00b7ne", "See\u00b7le", "er\u00b7w\u00e4h\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ART", "ADJA", "PPOSAT", "$,", "ART", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Floh durch die Wolken hinauf, lie\u00df mich Einsame zur\u00fcck", "tokens": ["Floh", "durch", "die", "Wol\u00b7ken", "hin\u00b7auf", ",", "lie\u00df", "mich", "Ein\u00b7sa\u00b7me", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,", "VVFIN", "PPER", "NN", "PTKVZ"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}}, "stanza.5": {"line.1": {"text": "Schwester, nun h\u00f6re den Rath, den heut meine Seele ersonnen:", "tokens": ["Schwes\u00b7ter", ",", "nun", "h\u00f6\u00b7re", "den", "Rath", ",", "den", "heut", "mei\u00b7ne", "See\u00b7le", "er\u00b7son\u00b7nen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Fortan bewohnen nun Zwei liebend und sehnend den Hain,", "tokens": ["For\u00b7tan", "be\u00b7woh\u00b7nen", "nun", "Zwei", "lie\u00b7bend", "und", "seh\u00b7nend", "den", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "CARD", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Girren und klagen vereint und rufen den s\u00fc\u00dfesten Namen,", "tokens": ["Gir\u00b7ren", "und", "kla\u00b7gen", "ver\u00b7eint", "und", "ru\u00b7fen", "den", "s\u00fc\u00b7\u00dfes\u00b7ten", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "VVPP", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Bis sich der Fl\u00fcgelein Paar st\u00e4rker den Schultern enthebt.", "tokens": ["Bis", "sich", "der", "Fl\u00fc\u00b7ge\u00b7lein", "Paar", "st\u00e4r\u00b7ker", "den", "Schul\u00b7tern", "ent\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "NN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Dann, o der Wonne! dann tr\u00e4gt uns \u00fcber den Mond das Gefieder \u2013", "tokens": ["Dann", ",", "o", "der", "Won\u00b7ne", "!", "dann", "tr\u00e4gt", "uns", "\u00fc\u00b7ber", "den", "Mond", "das", "Ge\u00b7fie\u00b7der", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dann, o der Wonne! dann wird Liebe mit Liebe vereint.", "tokens": ["Dann", ",", "o", "der", "Won\u00b7ne", "!", "dann", "wird", "Lie\u00b7be", "mit", "Lie\u00b7be", "ver\u00b7eint", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "ART", "NN", "$.", "ADV", "VAFIN", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ach, und das freudlose Heut \u2013 wir wollen es duldend ertragen,", "tokens": ["Ach", ",", "und", "das", "freud\u00b7lo\u00b7se", "Heut", "\u2013", "wir", "wol\u00b7len", "es", "dul\u00b7dend", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "ART", "ADJA", "NN", "$(", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Sind wir doch morgen schon Ihm selig dort oben geeint.", "tokens": ["Sind", "wir", "doch", "mor\u00b7gen", "schon", "Ihm", "se\u00b7lig", "dort", "o\u00b7ben", "ge\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "PPER", "ADJD", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}}}}}