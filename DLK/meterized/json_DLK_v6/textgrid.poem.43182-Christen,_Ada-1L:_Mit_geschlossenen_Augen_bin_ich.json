{"textgrid.poem.43182": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit geschlossenen Augen bin ich", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit geschlossenen Augen bin ich", "tokens": ["Mit", "ge\u00b7schlos\u00b7se\u00b7nen", "Au\u00b7gen", "bin", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER"], "meter": "+-+--+-++", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Durch die \u00f6de Nacht gewandelt,", "tokens": ["Durch", "die", "\u00f6\u00b7de", "Nacht", "ge\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Habe wie im Traum gef\u00fchlt.", "tokens": ["Ha\u00b7be", "wie", "im", "Traum", "ge\u00b7f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Habe wie im Traum gehandelt.", "tokens": ["Ha\u00b7be", "wie", "im", "Traum", "ge\u00b7han\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Pl\u00f6tzlich bin ich aufgewacht,", "tokens": ["Pl\u00f6tz\u00b7lich", "bin", "ich", "auf\u00b7ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hell den Blick Dir zugewendet \u2013", "tokens": ["Hell", "den", "Blick", "Dir", "zu\u00b7ge\u00b7wen\u00b7det", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn Du hast in meine Nacht", "tokens": ["Denn", "Du", "hast", "in", "mei\u00b7ne", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der Erkenntni\u00df Licht gesendet.", "tokens": ["Der", "Er\u00b7kennt\u00b7ni\u00df", "Licht", "ge\u00b7sen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}