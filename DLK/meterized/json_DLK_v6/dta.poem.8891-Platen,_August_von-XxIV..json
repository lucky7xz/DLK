{"dta.poem.8891": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "O Tizian, du Mann voll Kraft und Leben!", "tokens": ["O", "Ti\u00b7zi\u00b7an", ",", "du", "Mann", "voll", "Kraft", "und", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jezt siehst du mich vor deiner Gr\u00f6\u00dfe beben,", "tokens": ["Jezt", "siehst", "du", "mich", "vor", "dei\u00b7ner", "Gr\u00f6\u00b7\u00dfe", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seit ich Mari\u00e4 Himmelfahrt betrachtet!", "tokens": ["Seit", "ich", "Ma\u00b7ri\u00e4", "Him\u00b7mel\u00b7fahrt", "be\u00b7trach\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Von Wolken war mein tr\u00fcber Sinn umnachtet,", "tokens": ["Von", "Wol\u00b7ken", "war", "mein", "tr\u00fc\u00b7ber", "Sinn", "um\u00b7nach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie deiner Heil'gen sie zu F\u00fc\u00dfen schweben:", "tokens": ["Wie", "dei\u00b7ner", "Heil'\u00b7gen", "sie", "zu", "F\u00fc\u00b7\u00dfen", "schwe\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun seh' ich selbst dich gegen Himmel streben,", "tokens": ["Nun", "seh'", "ich", "selbst", "dich", "ge\u00b7gen", "Him\u00b7mel", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wonach so br\u00fcnstiglich Maria trachtet!", "tokens": ["Wo\u00b7nach", "so", "br\u00fcns\u00b7tig\u00b7lich", "Ma\u00b7ria", "trach\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "NE", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Dir fast zur Seite zeigt sich Pordenone:", "tokens": ["Dir", "fast", "zur", "Sei\u00b7te", "zeigt", "sich", "Por\u00b7de\u00b7no\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVFIN", "PRF", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr wolltet lebend nicht einander weichen,", "tokens": ["Ihr", "woll\u00b7tet", "le\u00b7bend", "nicht", "ein\u00b7an\u00b7der", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Tode hat nun jeder seine Krone!", "tokens": ["Im", "To\u00b7de", "hat", "nun", "je\u00b7der", "sei\u00b7ne", "Kro\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Verbr\u00fcdert m\u00f6gt ihr noch die H\u00e4nde reichen", "tokens": ["Ver\u00b7br\u00fc\u00b7dert", "m\u00f6gt", "ihr", "noch", "die", "H\u00e4n\u00b7de", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem treuen, vaterl\u00e4ndischen Giorgione,", "tokens": ["Dem", "treu\u00b7en", ",", "va\u00b7ter\u00b7l\u00e4n\u00b7di\u00b7schen", "Gi\u00b7or\u00b7gio\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und jenem Paul, dem wen'ge Maler gleichen!", "tokens": ["Und", "je\u00b7nem", "Paul", ",", "dem", "wen'\u00b7ge", "Ma\u00b7ler", "glei\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PRELS", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}