{"dta.poem.5765": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein St\u00fcck Materie", "tokens": ["Ein", "St\u00fcck", "Ma\u00b7te\u00b7rie"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-++-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Zu seiner letzten Theilung hinzubringen;", "tokens": ["Zu", "sei\u00b7ner", "letz\u00b7ten", "Thei\u00b7lung", "hin\u00b7zu\u00b7brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "W\u00fcrd es ihm gleichwol nicht gelingen.", "tokens": ["W\u00fcrd", "es", "ihm", "gleich\u00b7wol", "nicht", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Das allerkleinste Theil bleibt dennoch ausgespannt.", "tokens": ["Das", "al\u00b7ler\u00b7kleins\u00b7te", "Theil", "bleibt", "den\u00b7noch", "aus\u00b7ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie klein der Punct auch ist, zu welchen wir es ziehn;", "tokens": ["Wie", "klein", "der", "Punct", "auch", "ist", ",", "zu", "wel\u00b7chen", "wir", "es", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADV", "VAFIN", "$,", "APPR", "PWAT", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist es dennoch bekannt;", "tokens": ["Ist", "es", "den\u00b7noch", "be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er bleibt f\u00fcr sich und nichts zerst\u00f6ret ihn.", "tokens": ["Er", "bleibt", "f\u00fcr", "sich", "und", "nichts", "zer\u00b7st\u00f6\u00b7ret", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "KON", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}