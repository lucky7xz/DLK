{"dta.poem.13278": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eund \u00fcbrigens: je mehr ein Volck den F\u00fcrsten liebt,", "tokens": ["\u201e", "und", "\u00fcb\u00b7ri\u00b7gens", ":", "je", "mehr", "ein", "Volck", "den", "F\u00fcrs\u00b7ten", "liebt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "$.", "ADV", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "590\u201dJe mehr es ihm davon erregte Zeichen gibt.", "tokens": ["\"", "Je", "mehr", "es", "ihm", "da\u00b7von", "er\u00b7reg\u00b7te", "Zei\u00b7chen", "gibt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PPER", "PPER", "PAV", "VVFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201enach dem ein Zufall ist, nach dem macht es Geb\u00e4rden:", "tokens": ["\u201e", "nach", "dem", "ein", "Zu\u00b7fall", "ist", ",", "nach", "dem", "macht", "es", "Ge\u00b7b\u00e4r\u00b7den", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "ART", "NN", "VAFIN", "$,", "APPR", "ART", "VVFIN", "PPER", "NN", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "\u201ees bebt, es rufft, es schreyt, es weist auch die Beschwerden", "tokens": ["\u201e", "es", "bebt", ",", "es", "rufft", ",", "es", "schreyt", ",", "es", "weist", "auch", "die", "Be\u00b7schwer\u00b7den"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eso bald ein Ungl\u00fcck droht. Und gehn die Sachen gut,", "tokens": ["\u201e", "so", "bald", "ein", "Un\u00b7gl\u00fcck", "droht", ".", "Und", "gehn", "die", "Sa\u00b7chen", "gut", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ART", "NN", "VVFIN", "$.", "KON", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eso springt und singet es, es fa\u00dft von neuen Muth;", "tokens": ["\u201e", "so", "springt", "und", "sin\u00b7get", "es", ",", "es", "fa\u00dft", "von", "neu\u00b7en", "Muth", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "KON", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die sogenannten Laxenburger-\nV\u00f6gel/ die um das Jahr 1720. sich\n\nzum erstenmahl sehen liessen.", "tokens": ["Die", "so\u00b7ge\u00b7nann\u00b7ten", "La\u00b7xen\u00b7bur\u00b7ger", "V\u00f6\u00b7gel", "/", "die", "um", "das", "Jahr", "1720", ".", "sich", "zum", "ers\u00b7ten\u00b7mahl", "se\u00b7hen", "lies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "number", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "TRUNC", "NN", "$(", "ART", "APPR", "ART", "NN", "CARD", "$.", "PRF", "APPRART", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+--+--+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.8": {"text": "595\u201dEs w\u00fcnschet Gl\u00fcck dazu, und glaubet vorzusehen,", "tokens": ["\"", "Es", "w\u00fcn\u00b7schet", "Gl\u00fcck", "da\u00b7zu", ",", "und", "glau\u00b7bet", "vor\u00b7zu\u00b7se\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "NN", "PAV", "$,", "KON", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eda\u00df, was es hofft und g\u00f6nnt, unfehlbar soll geschehen.", "tokens": ["\u201e", "da\u00df", ",", "was", "es", "hofft", "und", "g\u00f6nnt", ",", "un\u00b7fehl\u00b7bar", "soll", "ge\u00b7sche\u00b7hen", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eseynd die\u00df nicht Wirckungen der unverf\u00e4lschten Treu?", "tokens": ["\u201e", "seynd", "die\u00df", "nicht", "Wir\u00b7ckun\u00b7gen", "der", "un\u00b7ver\u00b7f\u00e4lschten", "Treu", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PDS", "PTKNEG", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "\u201eso wei\u00df ich nicht, warum es zu verachten sey.", "tokens": ["\u201e", "so", "wei\u00df", "ich", "nicht", ",", "wa\u00b7rum", "es", "zu", "ver\u00b7ach\u00b7ten", "sey", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "PTKZU", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eman dencke, was man will; ich lobe dieses Schreyen;", "tokens": ["\u201e", "man", "den\u00b7cke", ",", "was", "man", "will", ";", "ich", "lo\u00b7be", "die\u00b7ses", "Schre\u00b7yen", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "$,", "PRELS", "PIS", "VMFIN", "$.", "PPER", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.13": {"text": "600\u201dGOtt la\u00df es diesem Haus, dem Ruff nach, angedeihen!", "tokens": ["\"", "Gott", "la\u00df", "es", "die\u00b7sem", "Haus", ",", "dem", "Ruff", "nach", ",", "an\u00b7ge\u00b7dei\u00b7hen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "NN", "VVFIN", "PPER", "PDAT", "NN", "$,", "ART", "NN", "PTKVZ", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}