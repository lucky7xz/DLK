{"textgrid.poem.60127": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf dem frischen Rasen-Sitze,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf dem frischen Rasen-Sitze,", "tokens": ["Auf", "dem", "fri\u00b7schen", "Ra\u00b7sen\u00b7Sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier am kleinen Wasserfall,", "tokens": ["Hier", "am", "klei\u00b7nen", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6r' ich von des Thurmes Spitze,", "tokens": ["H\u00f6r'", "ich", "von", "des", "Thur\u00b7mes", "Spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frommes Gl\u00f6cklein, deinen Schall.", "tokens": ["From\u00b7mes", "Gl\u00f6c\u00b7klein", ",", "dei\u00b7nen", "Schall", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "T\u00f6nst, o Gl\u00f6cklein, nennst ihn lauter,", "tokens": ["T\u00f6nst", ",", "o", "Gl\u00f6c\u00b7klein", ",", "nennst", "ihn", "lau\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem mein Herz entgegenbebt,", "tokens": ["Dem", "mein", "Herz", "ent\u00b7ge\u00b7gen\u00b7bebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihn, der freundlicher, vertrauter", "tokens": ["Ihn", ",", "der", "freund\u00b7li\u00b7cher", ",", "ver\u00b7trau\u00b7ter"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "$,", "PRELS", "ADJD", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier im Gr\u00fcnen mich umschwebt.", "tokens": ["Hier", "im", "Gr\u00fc\u00b7nen", "mich", "um\u00b7schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Leise murmeln es die B\u00e4che,", "tokens": ["Lei\u00b7se", "mur\u00b7meln", "es", "die", "B\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df er Flur und Aue liebt,", "tokens": ["Da\u00df", "er", "Flur", "und", "Au\u00b7e", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die Rose, die ich breche,", "tokens": ["Da\u00df", "die", "Ro\u00b7se", ",", "die", "ich", "bre\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir ein guter Vater giebt;", "tokens": ["Mir", "ein", "gu\u00b7ter", "Va\u00b7ter", "giebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da\u00df er aus der zarten H\u00fclle", "tokens": ["Da\u00df", "er", "aus", "der", "zar\u00b7ten", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selbst die goldnen Fr\u00fcchte winkt,", "tokens": ["Selbst", "die", "gold\u00b7nen", "Fr\u00fcch\u00b7te", "winkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und durch ihn des Lebens F\u00fclle", "tokens": ["Und", "durch", "ihn", "des", "Le\u00b7bens", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jede neue Knospe trinkt.", "tokens": ["Je\u00b7de", "neu\u00b7e", "Knos\u00b7pe", "trinkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schalle, Gl\u00f6cklein! Ach, was bliebe", "tokens": ["Schal\u00b7le", ",", "Gl\u00f6c\u00b7klein", "!", "Ach", ",", "was", "blie\u00b7be"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "ITJ", "$,", "PWS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jenem Himmel, diesem Gr\u00fcn?", "tokens": ["Je\u00b7nem", "Him\u00b7mel", ",", "die\u00b7sem", "Gr\u00fcn", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! kein Leben, keine Liebe,", "tokens": ["Ach", "!", "kein", "Le\u00b7ben", ",", "kei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine Freude, sonder ihn!", "tokens": ["Kei\u00b7ne", "Freu\u00b7de", ",", "son\u00b7der", "ihn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KON", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Morgens, wenn auf Busch und Pflanze", "tokens": ["Mor\u00b7gens", ",", "wenn", "auf", "Busch", "und", "Pflan\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fchler Thau die Perlen s\u00e4t,", "tokens": ["K\u00fch\u00b7ler", "Thau", "die", "Per\u00b7len", "s\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stimmen froh im Sonnenglanze,", "tokens": ["Stim\u00b7men", "froh", "im", "Son\u00b7nen\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "V\u00f6glein mit in mein Gebet.", "tokens": ["V\u00f6\u00b7glein", "mit", "in", "mein", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.7": {"line.1": {"text": "Und am Abend, wenn es dunkelt,", "tokens": ["Und", "am", "A\u00b7bend", ",", "wenn", "es", "dun\u00b7kelt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seh' ich seinen milden Schein:", "tokens": ["Seh'", "ich", "sei\u00b7nen", "mil\u00b7den", "Schein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo das Heer der Sterne funkelt,", "tokens": ["Wo", "das", "Heer", "der", "Ster\u00b7ne", "fun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wacht er \u00fcber Thal und Hain;", "tokens": ["Wacht", "er", "\u00fc\u00b7ber", "Thal", "und", "Hain", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Leuchtet mir auf meinen Wegen,", "tokens": ["Leuch\u00b7tet", "mir", "auf", "mei\u00b7nen", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Labt die Wiese, n\u00e4hrt das Feld,", "tokens": ["Labt", "die", "Wie\u00b7se", ",", "n\u00e4hrt", "das", "Feld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spricht den v\u00e4terlichen Segen", "tokens": ["Spricht", "den", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber die entschlafne Welt.", "tokens": ["Ue\u00b7ber", "die", "ent\u00b7schlaf\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Seiner freu' ich mich im Lenze,", "tokens": ["Sei\u00b7ner", "freu'", "ich", "mich", "im", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn man Veilchen-Kr\u00e4nze flicht;", "tokens": ["Wenn", "man", "Veil\u00b7chen\u00b7Kr\u00e4n\u00b7ze", "flicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seiner, wenn die Schnitter-T\u00e4nze", "tokens": ["Sei\u00b7ner", ",", "wenn", "die", "Schnit\u00b7ter\u00b7T\u00e4n\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sturm und Hagel unterbricht.", "tokens": ["Sturm", "und", "Ha\u00b7gel", "un\u00b7ter\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Sollt' ich seiner mich nicht freuen?", "tokens": ["Sollt'", "ich", "sei\u00b7ner", "mich", "nicht", "freu\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singen nicht, da\u00df Wolke, Wind,", "tokens": ["Sin\u00b7gen", "nicht", ",", "da\u00df", "Wol\u00b7ke", ",", "Wind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch die Blitze, wenn sie dr\u00e4uen,", "tokens": ["Auch", "die", "Blit\u00b7ze", ",", "wenn", "sie", "dr\u00e4u\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Vaters H\u00e4nden sind?", "tokens": ["In", "des", "Va\u00b7ters", "H\u00e4n\u00b7den", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Da\u00df an \u00f6den Felsen-Kl\u00fcften", "tokens": ["Da\u00df", "an", "\u00f6\u00b7den", "Fel\u00b7sen\u00b7K\u00b7l\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebend er vor\u00fcbergeht,", "tokens": ["Lie\u00b7bend", "er", "vor\u00b7\u00fc\u00b7ber\u00b7geht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in d\u00fcstern Todten-Gr\u00fcften", "tokens": ["Und", "in", "d\u00fcs\u00b7tern", "Tod\u00b7ten\u00b7Gr\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Des Erhalters Athem weht?", "tokens": ["Des", "Er\u00b7hal\u00b7ters", "A\u00b7them", "weht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}