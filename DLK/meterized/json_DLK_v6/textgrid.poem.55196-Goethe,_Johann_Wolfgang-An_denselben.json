{"textgrid.poem.55196": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "An denselben", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Statt den Menschen in den Tieren", "tokens": ["Statt", "den", "Men\u00b7schen", "in", "den", "Tie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu verlieren,", "tokens": ["Zu", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Findest du ihn klar darin", "tokens": ["Fin\u00b7dest", "du", "ihn", "klar", "da\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und belebst, als wahrer Dichter,", "tokens": ["Und", "be\u00b7lebst", ",", "als", "wah\u00b7rer", "Dich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schaf- und s\u00e4uisches Gelichter", "tokens": ["Schaf", "und", "s\u00e4u\u00b7i\u00b7sches", "Ge\u00b7lich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["TRUNC", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit Gesinnung wie mit Sinn.", "tokens": ["Mit", "Ge\u00b7sin\u00b7nung", "wie", "mit", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOKOM", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch der Esel kommt zu Ehren", "tokens": ["Auch", "der", "E\u00b7sel", "kommt", "zu", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und yaht uns weise Lehren.", "tokens": ["Und", "yaht", "uns", "wei\u00b7se", "Leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das, was Buffon nur begonnen,", "tokens": ["Das", ",", "was", "Buf\u00b7fon", "nur", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "NE", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt durch Tischbein an die Sonnen.", "tokens": ["Kommt", "durch", "Tischbein", "an", "die", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}