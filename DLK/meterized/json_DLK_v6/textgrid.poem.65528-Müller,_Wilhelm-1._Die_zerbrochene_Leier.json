{"textgrid.poem.65528": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1. Die zerbrochene Leier", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schlug zu St\u00fccken meine Leier, ergrimmt auf diese tolle Welt.", "tokens": ["Ich", "schlug", "zu", "St\u00fc\u00b7cken", "mei\u00b7ne", "Lei\u00b7er", ",", "er\u00b7grimmt", "auf", "die\u00b7se", "tol\u00b7le", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Doch bald empfand ich lange Weile, und eine neue ward bestellt.", "tokens": ["Doch", "bald", "emp\u00b7fand", "ich", "lan\u00b7ge", "Wei\u00b7le", ",", "und", "ei\u00b7ne", "neu\u00b7e", "ward", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "NN", "$,", "KON", "ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Indessen klimpr' ich auf der Saite, die an der alte h\u00e4ngen blieb:", "tokens": ["In\u00b7des\u00b7sen", "klim\u00b7pr'", "ich", "auf", "der", "Sai\u00b7te", ",", "die", "an", "der", "al\u00b7te", "h\u00e4n\u00b7gen", "blieb", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "VVINF", "VVFIN", "$."], "meter": "-+-+--+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "So lange nehmt, geneigte Leser, mit kleinen Reimen auch vorlieb.", "tokens": ["So", "lan\u00b7ge", "nehmt", ",", "ge\u00b7neig\u00b7te", "Le\u00b7ser", ",", "mit", "klei\u00b7nen", "Rei\u00b7men", "auch", "vor\u00b7lieb", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}}}}