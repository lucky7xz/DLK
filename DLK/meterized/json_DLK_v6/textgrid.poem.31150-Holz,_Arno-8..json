{"textgrid.poem.31150": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Scheiden, ach das Scheiden,", "tokens": ["Das", "Schei\u00b7den", ",", "ach", "das", "Schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "XY", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer hat das nur erdacht", "tokens": ["Wer", "hat", "das", "nur", "er\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PDS", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ein so schweres Leiden", "tokens": ["Und", "ein", "so", "schwe\u00b7res", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir \u00fcbers Herz gebracht?", "tokens": ["Mir", "\u00fc\u00b7bers", "Herz", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und w\u00e4r's ein Kr\u00e4utelein,", "tokens": ["Und", "w\u00e4r's", "ein", "Kr\u00e4u\u00b7tel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich n\u00e4hm mein Messerlein", "tokens": ["Ich", "n\u00e4hm", "mein", "Mes\u00b7ser\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wollte flink zerschneiden", "tokens": ["Und", "woll\u00b7te", "flink", "zer\u00b7schnei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die b\u00f6sen W\u00fcrzelein.", "tokens": ["Die", "b\u00f6\u00b7sen", "W\u00fcr\u00b7zel\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich h\u00f6rte von den Weiben", "tokens": ["Ich", "h\u00f6r\u00b7te", "von", "den", "Wei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herzliebe und Herzleid,", "tokens": ["Herz\u00b7lie\u00b7be", "und", "Herz\u00b7leid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wo Herzelieb mag bleiben,", "tokens": ["Wo", "Her\u00b7ze\u00b7lieb", "mag", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist Herzeleid nicht weit.", "tokens": ["Ist", "Her\u00b7ze\u00b7leid", "nicht", "weit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Herzliebe war uns hold", "tokens": ["Herz\u00b7lie\u00b7be", "war", "uns", "hold"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und fluchs kam angetrollt,", "tokens": ["Und", "fluchs", "kam", "an\u00b7ge\u00b7trollt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Schwester zu vertreiben,", "tokens": ["Die", "Schwes\u00b7ter", "zu", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Herzleide, die ihr grollt.", "tokens": ["Herz\u00b7lei\u00b7de", ",", "die", "ihr", "grollt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Aus Thor und Thurm und Mauern", "tokens": ["Aus", "Thor", "und", "Thurm", "und", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zieh ich hinab das Thal", "tokens": ["Zieh", "ich", "hin\u00b7ab", "das", "Thal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und blicke noch in Trauern", "tokens": ["Und", "bli\u00b7cke", "noch", "in", "Trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur\u00fcck zum letzten Mal.", "tokens": ["Zu\u00b7r\u00fcck", "zum", "letz\u00b7ten", "Mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Horch, wie die Winde gehn,", "tokens": ["Horch", ",", "wie", "die", "Win\u00b7de", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Schau, wie die Bl\u00e4tter wehn \u2013", "tokens": ["Schau", ",", "wie", "die", "Bl\u00e4t\u00b7ter", "wehn", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "VVINF", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Ach Gott, wie lang wird's dauern,", "tokens": ["Ach", "Gott", ",", "wie", "lang", "wird's", "dau\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWAV", "ADJD", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bis wir uns wiedersehn!", "tokens": ["Bis", "wir", "uns", "wie\u00b7der\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}