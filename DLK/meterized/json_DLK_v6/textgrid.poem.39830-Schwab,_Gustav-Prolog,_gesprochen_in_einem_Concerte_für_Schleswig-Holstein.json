{"textgrid.poem.39830": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Prolog, gesprochen in einem Concerte f\u00fcr Schleswig-Holstein", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im friedevollen Reich der T\u00f6ne", "tokens": ["Im", "frie\u00b7de\u00b7vol\u00b7len", "Reich", "der", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vereinigt uns die Harmonie;", "tokens": ["Ver\u00b7ei\u00b7nigt", "uns", "die", "Har\u00b7mo\u00b7nie", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da herrscht sie noch in ihrer Sch\u00f6ne,", "tokens": ["Da", "herrscht", "sie", "noch", "in", "ih\u00b7rer", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch aus dem Leben schwindet sie:", "tokens": ["Doch", "aus", "dem", "Le\u00b7ben", "schwin\u00b7det", "sie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn drau\u00dfen klafft ja die Entzweiung,", "tokens": ["Denn", "drau\u00b7\u00dfen", "klafft", "ja", "die", "Ent\u00b7zwei\u00b7ung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Denn drau\u00dfen st\u00fcrmt ja die Parteiung,", "tokens": ["Denn", "drau\u00b7\u00dfen", "st\u00fcrmt", "ja", "die", "Par\u00b7tei\u00b7ung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die sich der Eintracht starr verschlie\u00dft;", "tokens": ["Die", "sich", "der", "Ein\u00b7tracht", "starr", "ver\u00b7schlie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Freunde trennt und Feinde koppelt,", "tokens": ["Die", "Freun\u00b7de", "trennt", "und", "Fein\u00b7de", "kop\u00b7pelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Den Ha\u00df versch\u00e4rft, das Gift verdoppelt", "tokens": ["Den", "Ha\u00df", "ver\u00b7sch\u00e4rft", ",", "das", "Gift", "ver\u00b7dop\u00b7pelt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und \u00e4tzend in die Wunden gie\u00dft.", "tokens": ["Und", "\u00e4t\u00b7zend", "in", "die", "Wun\u00b7den", "gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja, h\u00fctet \u00e4ngstlich euch, ihr Stimmen,", "tokens": ["Ja", ",", "h\u00fc\u00b7tet", "\u00e4ngst\u00b7lich", "euch", ",", "ihr", "Stim\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ADJD", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr zum Einklang fertig seid,", "tokens": ["Die", "ihr", "zum", "Ein\u00b7klang", "fer\u00b7tig", "seid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df nicht den Mi\u00dflaut euch, den grimmen,", "tokens": ["Da\u00df", "nicht", "den", "Mi\u00df\u00b7laut", "euch", ",", "den", "grim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PPER", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einhauche der entbrannte Streit.", "tokens": ["Ein\u00b7hau\u00b7che", "der", "ent\u00b7brann\u00b7te", "Streit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was soll geschehen, was entstehen?", "tokens": ["Was", "soll", "ge\u00b7sche\u00b7hen", ",", "was", "ent\u00b7ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "VVPP", "$,", "PWS", "VVINF", "$."], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Schon wieder liegt die Zeit in Wehen,", "tokens": ["Schon", "wie\u00b7der", "liegt", "die", "Zeit", "in", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein unheimliches Krachen ist's.", "tokens": ["Ein", "un\u00b7heim\u00b7li\u00b7ches", "Kra\u00b7chen", "ist'", "s."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NE"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Schon regt in Meinung und im Worte", "tokens": ["Schon", "regt", "in", "Mei\u00b7nung", "und", "im", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der Krieg sich \u2013 dr\u00e4ngt sich nach der Pforte", "tokens": ["Der", "Krieg", "sich", "\u2013", "dr\u00e4ngt", "sich", "nach", "der", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "$(", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Mi\u00dfgestalt des B\u00fcrgerzwists.", "tokens": ["Die", "Mi\u00df\u00b7ge\u00b7stalt", "des", "B\u00fcr\u00b7ger\u00b7zwists", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo eint sich der gespaltne Wille?", "tokens": ["Wo", "eint", "sich", "der", "ge\u00b7spalt\u00b7ne", "Wil\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ist des Volkes Herz noch ganz?", "tokens": ["Wo", "ist", "des", "Vol\u00b7kes", "Herz", "noch", "ganz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ruht das Land der Waffenstille?", "tokens": ["Wo", "ruht", "das", "Land", "der", "Waf\u00b7fen\u00b7stil\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo winkt das Elis Griechenlands?", "tokens": ["Wo", "winkt", "das", "E\u00b7lis", "Grie\u00b7chen\u00b7lands", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo sitzt auf seinem goldnen Throne", "tokens": ["Wo", "sitzt", "auf", "sei\u00b7nem", "gold\u00b7nen", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Zeus Olympia's, die Krone", "tokens": ["Der", "Zeus", "O\u00b7lym\u00b7pia's", ",", "die", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NE", "NE", "$,", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Die unbestrittne, auf dem Haupt?", "tokens": ["Die", "un\u00b7be\u00b7stritt\u00b7ne", ",", "auf", "dem", "Haupt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Antlitz voll Majest\u00e4t und G\u00fcte,", "tokens": ["Ant\u00b7litz", "voll", "Ma\u00b7jes\u00b7t\u00e4t", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Der Huld, des Heils lebend'ge Bl\u00fcte,", "tokens": ["Der", "Huld", ",", "des", "Heils", "le\u00b7ben\u00b7d'\u00b7ge", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Wo l\u00e4chelst du, geliebt, geglaubt?", "tokens": ["Wo", "l\u00e4\u00b7chelst", "du", ",", "ge\u00b7liebt", ",", "ge\u00b7glaubt", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Land, das die heil'ge Sage feiert,", "tokens": ["Land", ",", "das", "die", "heil'\u00b7ge", "Sa\u00b7ge", "fei\u00b7ert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Nach dem des Geistes Auge sieht;", "tokens": ["Nach", "dem", "des", "Geis\u00b7tes", "Au\u00b7ge", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus dem Novemberqualm entschleiert", "tokens": ["Aus", "dem", "No\u00b7vem\u00b7ber\u00b7qualm", "ent\u00b7schlei\u00b7ert"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich uns dein priesterlich Gebiet.", "tokens": ["Sich", "uns", "dein", "pries\u00b7ter\u00b7lich", "Ge\u00b7biet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Tempel stralet, wo der Richter,", "tokens": ["Der", "Tem\u00b7pel", "stra\u00b7let", ",", "wo", "der", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der V\u00f6lkerk\u00e4mpfe letzter Schlichter,", "tokens": ["Der", "V\u00f6l\u00b7ker\u00b7k\u00e4mp\u00b7fe", "letz\u00b7ter", "Schlich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Nationenhirte, thront.", "tokens": ["Der", "Na\u00b7ti\u00b7o\u00b7nen\u00b7hir\u00b7te", ",", "thront", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Nicken seiner dunkeln Brauen", "tokens": ["Das", "Ni\u00b7cken", "sei\u00b7ner", "dun\u00b7keln", "Brau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Durchz\u00fcckt die Welt mit frommem Grauen,", "tokens": ["Durch\u00b7z\u00fcckt", "die", "Welt", "mit", "from\u00b7mem", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Es scheut, was ihm zu F\u00fc\u00dfen wohnt.", "tokens": ["Es", "scheut", ",", "was", "ihm", "zu", "F\u00fc\u00b7\u00dfen", "wohnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Wallfahrt nach des Rechtes Quelle", "tokens": ["Die", "Wall\u00b7fahrt", "nach", "des", "Rech\u00b7tes", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcllt seit Jahrtausenden den Pfad.", "tokens": ["F\u00fcllt", "seit", "Jahr\u00b7tau\u00b7sen\u00b7den", "den", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer ist's, der heute dieser Schwelle", "tokens": ["Wer", "ist's", ",", "der", "heu\u00b7te", "die\u00b7ser", "Schwel\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit schwankem, krankem, Schritte naht?", "tokens": ["Mit", "schwan\u00b7kem", ",", "kran\u00b7kem", ",", "Schrit\u00b7te", "naht", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Weib im schwarzen Wittwenkleide,", "tokens": ["Ein", "Weib", "im", "schwar\u00b7zen", "Witt\u00b7wen\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Aug' von unnennbarem Leide", "tokens": ["Das", "Aug'", "von", "un\u00b7nenn\u00b7ba\u00b7rem", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gl\u00e4nzt traurig durch der Schleier Flor.", "tokens": ["Gl\u00e4nzt", "trau\u00b7rig", "durch", "der", "Schlei\u00b7er", "Flor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Welch ein Gef\u00fchl durchz\u00fcckt uns Alle?", "tokens": ["Welch", "ein", "Ge\u00b7f\u00fchl", "durch\u00b7z\u00fcckt", "uns", "Al\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wie sehnlich zieht's uns nach der Halle,", "tokens": ["Wie", "sehn\u00b7lich", "zieht's", "uns", "nach", "der", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}}, "stanza.6": {"line.1": {"text": "Sie beut dem Mitleid, beut dem Spotte", "tokens": ["Sie", "beut", "dem", "Mit\u00b7leid", ",", "beut", "dem", "Spot\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr edles Haupt in Ruhe dar,", "tokens": ["Ihr", "ed\u00b7les", "Haupt", "in", "Ru\u00b7he", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wirft sich nieder vor dem Gotte", "tokens": ["Sie", "wirft", "sich", "nie\u00b7der", "vor", "dem", "Got\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und opfert an dem Hochaltar.", "tokens": ["Und", "op\u00b7fert", "an", "dem", "Hoc\u00b7hal\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": 