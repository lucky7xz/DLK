{"textgrid.poem.61601": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4rchen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n Manar trat aus dem wilden Wald,", "tokens": ["Sch\u00f6n", "Ma\u00b7nar", "trat", "aus", "dem", "wil\u00b7den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sie trat in den pr\u00e4chtigsten Garten;", "tokens": ["Sie", "trat", "in", "den", "pr\u00e4ch\u00b7tigs\u00b7ten", "Gar\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da bl\u00fchten die Rosen rot und wei\u00df,", "tokens": ["Da", "bl\u00fch\u00b7ten", "die", "Ro\u00b7sen", "rot", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und lustig sprangen die Wasser.", "tokens": ["Und", "lus\u00b7tig", "spran\u00b7gen", "die", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und \u00fcber den Rosen und Wassern stieg", "tokens": ["Und", "\u00fc\u00b7ber", "den", "Ro\u00b7sen", "und", "Was\u00b7sern", "stieg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ein Schlo\u00df mit schimmernden Kuppeln,", "tokens": ["Ein", "Schlo\u00df", "mit", "schim\u00b7mern\u00b7den", "Kup\u00b7peln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zwei Fl\u00fcgelpferde standen am Tor", "tokens": ["Zwei", "Fl\u00fc\u00b7gel\u00b7pfer\u00b7de", "stan\u00b7den", "am", "Tor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Aus gr\u00fcnem Erz gegossen.", "tokens": ["Aus", "gr\u00fc\u00b7nem", "Erz", "ge\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6n Manar schritt in das Schlo\u00df hinein,", "tokens": ["Sch\u00f6n", "Ma\u00b7nar", "schritt", "in", "das", "Schlo\u00df", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Empor die schweigenden Treppen;", "tokens": ["Em\u00b7por", "die", "schwei\u00b7gen\u00b7den", "Trep\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zw\u00f6lf Harfen hingen im Pfeilergang,", "tokens": ["Zw\u00f6lf", "Har\u00b7fen", "hin\u00b7gen", "im", "Pfei\u00b7ler\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Spinnen woben dar\u00fcber.", "tokens": ["Die", "Spin\u00b7nen", "wo\u00b7ben", "da\u00b7r\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und als sie trat in den ersten Saal,", "tokens": ["Und", "als", "sie", "trat", "in", "den", "ers\u00b7ten", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da stand eine Tafel ger\u00fcstet", "tokens": ["Da", "stand", "ei\u00b7ne", "Ta\u00b7fel", "ge\u00b7r\u00fcs\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und funkelnder Wein in lichtem Kristall,", "tokens": ["Und", "fun\u00b7keln\u00b7der", "Wein", "in", "lich\u00b7tem", "Kris\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch niemand kam, sich zu letzen.", "tokens": ["Doch", "nie\u00b7mand", "kam", ",", "sich", "zu", "let\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Und als sie trat in das zweite Gemach,", "tokens": ["Und", "als", "sie", "trat", "in", "das", "zwei\u00b7te", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da lag auf seidenen Kissen", "tokens": ["Da", "lag", "auf", "sei\u00b7de\u00b7nen", "Kis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das sch\u00f6nste Weib in goldnem Gelock,", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Weib", "in", "gold\u00b7nem", "Ge\u00b7lock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Doch schlief sie bleiernen Schlummer.", "tokens": ["Doch", "schlief", "sie", "blei\u00b7er\u00b7nen", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Und als sie trat in den dritten Saal,", "tokens": ["Und", "als", "sie", "trat", "in", "den", "drit\u00b7ten", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da sa\u00df bei verhangenen Fenstern", "tokens": ["Da", "sa\u00df", "bei", "ver\u00b7han\u00b7ge\u00b7nen", "Fens\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Im d\u00e4mmernden Raum auf g\u00fcldenem Stuhl", "tokens": ["Im", "d\u00e4m\u00b7mern\u00b7den", "Raum", "auf", "g\u00fcl\u00b7de\u00b7nem", "Stuhl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein schattenhafter K\u00f6nig.", "tokens": ["Ein", "schat\u00b7ten\u00b7haf\u00b7ter", "K\u00f6\u00b7nig", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sein Antlitz war nicht jung noch alt,", "tokens": ["Sein", "Ant\u00b7litz", "war", "nicht", "jung", "noch", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Haar war unbeschoren;", "tokens": ["Sein", "Haar", "war", "un\u00b7be\u00b7scho\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf seinen blassen Z\u00fcgen lag", "tokens": ["Auf", "sei\u00b7nen", "blas\u00b7sen", "Z\u00fc\u00b7gen", "lag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein unergr\u00fcndliches Elend.", "tokens": ["Ein", "un\u00b7er\u00b7gr\u00fcnd\u00b7li\u00b7ches", "E\u00b7lend", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Sch\u00f6n Manar sprach voll Mitleid: \u00bbHerr,", "tokens": ["Sch\u00f6n", "Ma\u00b7nar", "sprach", "voll", "Mit\u00b7leid", ":", "\u00bb", "Herr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADJD", "NN", "$.", "$(", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O br\u00fcte nicht hier so d\u00fcster!", "tokens": ["O", "br\u00fc\u00b7te", "nicht", "hier", "so", "d\u00fcs\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Welt ist drau\u00dfen voll Sonnenschein", "tokens": ["Die", "Welt", "ist", "drau\u00b7\u00dfen", "voll", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und voll von Rosen der Garten.", "tokens": ["Und", "voll", "von", "Ro\u00b7sen", "der", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "\u00bbwas gehst du nicht, am funkelnden Wein", "tokens": ["\u00bb", "was", "gehst", "du", "nicht", ",", "am", "fun\u00b7keln\u00b7den", "Wein"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PWS", "VVFIN", "PPER", "PTKNEG", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Dein trauriges Herz zu erquicken?", "tokens": ["Dein", "trau\u00b7ri\u00b7ges", "Herz", "zu", "er\u00b7qui\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Was weckst du die schlafende Jungfrau nicht", "tokens": ["Was", "weckst", "du", "die", "schla\u00b7fen\u00b7de", "Jung\u00b7frau", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mit K\u00fcssen zu Lust und Liebe?\u00ab", "tokens": ["Mit", "K\u00fcs\u00b7sen", "zu", "Lust", "und", "Lie\u00b7be", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Der K\u00f6nig hub zu ihr empor", "tokens": ["Der", "K\u00f6\u00b7nig", "hub", "zu", "ihr", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die gramerloschenen Augen;", "tokens": ["Die", "gra\u00b7mer\u00b7lo\u00b7sche\u00b7nen", "Au\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er sch\u00fcttelte tr\u00fcb das Haupt, doch kam", "tokens": ["Er", "sch\u00fct\u00b7tel\u00b7te", "tr\u00fcb", "das", "Haupt", ",", "doch", "kam"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,", "ADV", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kein Wort von seinen Lippen.", "tokens": ["Kein", "Wort", "von", "sei\u00b7nen", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Er schlug den Purpurmantel zur\u00fcck", "tokens": ["Er", "schlug", "den", "Pur\u00b7pur\u00b7man\u00b7tel", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Von seiner linken Seite,", "tokens": ["Von", "sei\u00b7ner", "lin\u00b7ken", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da war sie nicht Fleisch, da war sie nicht Bein,", "tokens": ["Da", "war", "sie", "nicht", "Fleisch", ",", "da", "war", "sie", "nicht", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "NN", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da war sie schwarzer Marmor \u2013 \u2013", "tokens": ["Da", "war", "sie", "schwar\u00b7zer", "Mar\u00b7mor", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}