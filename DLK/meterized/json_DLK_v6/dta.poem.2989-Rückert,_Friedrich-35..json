{"dta.poem.2989": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du kannst in der Natur nicht ein Gebilde streichen,", "tokens": ["Du", "kannst", "in", "der", "Na\u00b7tur", "nicht", "ein", "Ge\u00b7bil\u00b7de", "strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und siehst Zusammenhang in allen ihren Reichen,", "tokens": ["Und", "siehst", "Zu\u00b7sam\u00b7men\u00b7hang", "in", "al\u00b7len", "ih\u00b7ren", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vom Stein zur Pflanze, von der Pflanze bis zum Thier,", "tokens": ["Vom", "Stein", "zur", "Pflan\u00b7ze", ",", "von", "der", "Pflan\u00b7ze", "bis", "zum", "Thier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,", "APPR", "ART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und von dem Thier hinan, o stolzer Mensch, zu dir.", "tokens": ["Und", "von", "dem", "Thier", "hi\u00b7nan", ",", "o", "stol\u00b7zer", "Mensch", ",", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ", "$,", "FM", "ADJA", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du siehst das H\u00f6here vom Niederen getragen;", "tokens": ["Du", "siehst", "das", "H\u00f6\u00b7he\u00b7re", "vom", "Nie\u00b7de\u00b7ren", "ge\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nimm dis, und jenem ziehst du weg die Unterlagen.", "tokens": ["Nimm", "dis", ",", "und", "je\u00b7nem", "ziehst", "du", "weg", "die", "Un\u00b7ter\u00b7la\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "$,", "KON", "PDS", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Warum denn irrt es dich, da\u00df in des Geistes Reich", "tokens": ["Wa\u00b7rum", "denn", "irrt", "es", "dich", ",", "da\u00df", "in", "des", "Geis\u00b7tes", "Reich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PRF", "$,", "KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vorstellungsweisen auch nicht sind an H\u00f6he gleich?", "tokens": ["Vor\u00b7stel\u00b7lungs\u00b7wei\u00b7sen", "auch", "nicht", "sind", "an", "H\u00f6\u00b7he", "gleich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "VAFIN", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}}, "stanza.5": {"line.1": {"text": "Du selber hast dich noch zur h\u00f6chsten nicht erhoben,", "tokens": ["Du", "sel\u00b7ber", "hast", "dich", "noch", "zur", "h\u00f6chs\u00b7ten", "nicht", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ADV", "APPRART", "ADJA", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du nicht einsiehst, da\u00df Gott auch die niedern loben.", "tokens": ["Wenn", "du", "nicht", "ein\u00b7siehst", ",", "da\u00df", "Gott", "auch", "die", "nie\u00b7dern", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "$,", "KOUS", "NN", "ADV", "ART", "PIS", "VVINF", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}