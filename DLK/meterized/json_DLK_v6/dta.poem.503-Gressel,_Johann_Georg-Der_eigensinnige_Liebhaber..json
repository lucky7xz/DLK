{"dta.poem.503": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der eigensinnige Liebhaber.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Das M\u00e4dgen liebe ich/ da\u00df mir nicht g\u00fcnstig ist/", "tokens": ["Das", "M\u00e4d\u00b7gen", "lie\u00b7be", "ich", "/", "da\u00df", "mir", "nicht", "g\u00fcns\u00b7tig", "ist", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PTKNEG", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dasselbe will ich nicht/ das mich mit Willen k\u00fc\u00dft.", "tokens": ["Das\u00b7sel\u00b7be", "will", "ich", "nicht", "/", "das", "mich", "mit", "Wil\u00b7len", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "$(", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Jhn aber nicht mit Lust und Fr\u00f6lichkeit vergn\u00fcgen.", "tokens": ["Jhn", "a\u00b7ber", "nicht", "mit", "Lust", "und", "Fr\u00f6\u00b7lich\u00b7keit", "ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den angebohtnen Kn\u00df/ den schlag\u2019 ich in den Wind/", "tokens": ["Den", "an\u00b7ge\u00b7boht\u00b7nen", "Kn\u00df", "/", "den", "schlag'", "ich", "in", "den", "Wind", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und den verlang\u2019 ich nicht/ der mir nicht wird verg\u00fcnnt.", "tokens": ["Und", "den", "ver\u00b7lang'", "ich", "nicht", "/", "der", "mir", "nicht", "wird", "ver\u00b7g\u00fcnnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "PTKNEG", "$(", "PRELS", "PPER", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Sinnen will ich nicht mit Wollust \u00fcberschwemmen/", "tokens": ["Die", "Sin\u00b7nen", "will", "ich", "nicht", "mit", "Wol\u00b7lust", "\u00fc\u00b7bersc\u00b7hwem\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den Unmuth aber doch in seinem Lauffe hemmen.", "tokens": ["Den", "Un\u00b7muth", "a\u00b7ber", "doch", "in", "sei\u00b7nem", "Lauf\u00b7fe", "hem\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Jen\u2019 hat zu viel der Lust/ und diese nichts in sich/", "tokens": ["Jen'", "hat", "zu", "viel", "der", "Lust", "/", "und", "die\u00b7se", "nichts", "in", "sich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKA", "PIS", "ART", "NN", "$(", "KON", "PDS", "PIS", "APPR", "PRF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich will die Mittel-Bahn von diesen beyden gehen/", "tokens": ["Ich", "will", "die", "Mit\u00b7tel\u00b7Bahn", "von", "die\u00b7sen", "bey\u00b7den", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PDAT", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und will nach einer Frau/ die etwas listig stehen/", "tokens": ["Und", "will", "nach", "ei\u00b7ner", "Frau", "/", "die", "et\u00b7was", "lis\u00b7tig", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ART", "NN", "$(", "ART", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die die Verschlagenheit/ und lose Schlackheit liebt/", "tokens": ["Die", "die", "Ver\u00b7schla\u00b7gen\u00b7heit", "/", "und", "lo\u00b7se", "Schlack\u00b7heit", "liebt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$(", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und sich dem/ was ich will/ allzeit entgegen giebt/", "tokens": ["Und", "sich", "dem", "/", "was", "ich", "will", "/", "all\u00b7zeit", "ent\u00b7ge\u00b7gen", "giebt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "$(", "PWS", "PPER", "VMFIN", "$(", "ADV", "PTKVZ", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die wieder z\u00fcrnt und schilt/ und gleich darzwischen schl\u00e4get/", "tokens": ["Die", "wie\u00b7der", "z\u00fcrnt", "und", "schilt", "/", "und", "gleich", "darz\u00b7wi\u00b7schen", "schl\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "KON", "VVFIN", "$(", "KON", "ADV", "PAV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wenn meine Hand noch nicht den Kn\u00fcttel angereget.", "tokens": ["Wenn", "mei\u00b7ne", "Hand", "noch", "nicht", "den", "Kn\u00fct\u00b7tel", "an\u00b7ge\u00b7re\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}