{"textgrid.poem.26424": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tagesanbruch im Biwak, grau und tr\u00fcb.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tagesanbruch im Biwak, grau und tr\u00fcb.", "tokens": ["Ta\u00b7ge\u00b7san\u00b7bruch", "im", "Bi\u00b7wak", ",", "grau", "und", "tr\u00fcb", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Nach schlafloser Nacht trete ich fr\u00fch aus dem Zelt", "tokens": ["Nach", "schlaf\u00b7lo\u00b7ser", "Nacht", "tre\u00b7te", "ich", "fr\u00fch", "aus", "dem", "Zelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "In die Morgenk\u00fchle, und gehe langsam", "tokens": ["In", "die", "Mor\u00b7gen\u00b7k\u00fch\u00b7le", ",", "und", "ge\u00b7he", "lang\u00b7sam"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "VVFIN", "ADJD"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Weg am Feldlazarett vorbei.", "tokens": ["Den", "Weg", "am", "Feld\u00b7la\u00b7za\u00b7rett", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Drei Gestalten liegen hingestreckt auf Tragbahren", "tokens": ["Drei", "Ge\u00b7stal\u00b7ten", "lie\u00b7gen", "hin\u00b7ge\u00b7streckt", "auf", "Trag\u00b7bah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "VVPP", "APPR", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Drau\u00dfen vorm Zelt, \u00fcber jede eine Decke gebreitet,", "tokens": ["Drau\u00b7\u00dfen", "vorm", "Zelt", ",", "\u00fc\u00b7ber", "je\u00b7de", "ei\u00b7ne", "De\u00b7cke", "ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPR", "PIAT", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}}, "stanza.3": {"line.1": {"text": "Die schwere wollene graubraune Decke, die alles verh\u00fcllt.", "tokens": ["Die", "schwe\u00b7re", "wol\u00b7le\u00b7ne", "grau\u00b7brau\u00b7ne", "De\u00b7cke", ",", "die", "al\u00b7les", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "ADJA", "NN", "$,", "PRELS", "PIS", "VVPP", "$."], "meter": "-+-+--+--+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Heimlich hingezogen, bleibe ich schweigend stehn.", "tokens": ["Heim\u00b7lich", "hin\u00b7ge\u00b7zo\u00b7gen", ",", "blei\u00b7be", "ich", "schwei\u00b7gend", "stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Dann mit behutsamer Hand vom Antlitz des N\u00e4chsten", "tokens": ["Dann", "mit", "be\u00b7hut\u00b7sa\u00b7mer", "Hand", "vom", "Ant\u00b7litz", "des", "N\u00e4chs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hebe ich leise die Decke:", "tokens": ["He\u00b7be", "ich", "lei\u00b7se", "die", "De\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Wer bist du, mein Kamerad?", "tokens": ["Wer", "bist", "du", ",", "mein", "Ka\u00b7me\u00b7rad", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dann tret ich zum Zweiten:", "tokens": ["Dann", "tret", "ich", "zum", "Zwei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und wer bist du, mein Knabe, mein Liebling?", "tokens": ["Und", "wer", "bist", "du", ",", "mein", "Kna\u00b7be", ",", "mein", "Lieb\u00b7ling", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Du mit dem rosigen Hauch auf der Wange?", "tokens": ["Du", "mit", "dem", "ro\u00b7si\u00b7gen", "Hauch", "auf", "der", "Wan\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}}}}}