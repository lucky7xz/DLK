{"dta.poem.840": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxVII.  Auf das Fest des Todes  JESU CHRI-  \n STI,  oder auff den guten Freytag.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Oschmertz! das Leben stirbt! O Wunder! GOTT mu\u00df ", "tokens": ["Oschmertz", "!", "das", "Le\u00b7ben", "stirbt", "!", "O", "Wun\u00b7der", "!", "GoTT", "mu\u00df"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$.", "ART", "NN", "VVFIN", "$.", "NE", "NN", "$.", "NE", "VMFIN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "leiden! ", "tokens": ["lei\u00b7den", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der alles tr\u00e4gt f\u00e4llt hin/ die Ehre wird veracht/", "tokens": ["Der", "al\u00b7les", "tr\u00e4gt", "f\u00e4llt", "hin", "/", "die", "Eh\u00b7re", "wird", "ver\u00b7acht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "VVFIN", "PTKVZ", "$(", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der alles deckt ist nackt/ der T\u00f6ster ist verschmacht", "tokens": ["Der", "al\u00b7les", "deckt", "ist", "nackt", "/", "der", "T\u00f6s\u00b7ter", "ist", "ver\u00b7schmacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "VAFIN", "ADJD", "$(", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Lufft vnd w\u00e4lder schuff/ mu\u00df Lufft vnd W\u00e4lder meiden!", "tokens": ["Der", "Lufft", "vnd", "w\u00e4l\u00b7der", "schuff", "/", "mu\u00df", "Lufft", "vnd", "W\u00e4l\u00b7der", "mei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$(", "VMFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er hat die Lufft zur pein/ vnd mu\u00df am Holtz abscheyden.", "tokens": ["Er", "hat", "die", "Lufft", "zur", "pein", "/", "vnd", "mu\u00df", "am", "Holtz", "ab\u00b7schey\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$(", "KON", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Glantz der Herrligkeit/ verschwind\u2019t in herber nacht", "tokens": ["Der", "Glantz", "der", "Herr\u00b7lig\u00b7keit", "/", "ver\u00b7schwind't", "in", "her\u00b7ber", "nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$(", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Segen wird ein Fluch/ die vnersch\u00f6pffte macht/", "tokens": ["Der", "Se\u00b7gen", "wird", "ein", "Fluch", "/", "die", "vner\u00b7sch\u00f6pff\u00b7te", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$(", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Hat keine kr\u00e4ffte mehr! den K\u00f6nig aller Heyden", "tokens": ["Hat", "kei\u00b7ne", "kr\u00e4ff\u00b7te", "mehr", "!", "den", "K\u00f6\u00b7nig", "al\u00b7ler", "Hey\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "$.", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Erw\u00fcrg\u2019t d\u2019 Knechte Schar. Was bo\u00dfheit hat verschuld\u2019t", "tokens": ["Er\u00b7w\u00fcr\u00b7g't", "d'", "Knech\u00b7te", "Schar", ".", "Was", "bo\u00df\u00b7heit", "hat", "ver\u00b7schuld't"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "NN", "NN", "$.", "PWS", "NN", "VAFIN", "VVPP"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zahlt Vnschuld willig au\u00df/ wie embsig ist gedult", "tokens": ["Zahlt", "Vn\u00b7schuld", "wil\u00b7lig", "au\u00df", "/", "wie", "emb\u00b7sig", "ist", "ge\u00b7dult"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "ADJD", "PTKVZ", "$(", "PWAV", "ADJD", "VAFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vn\u00df GOttes grosse gunst aufs newe vor zu bringen.", "tokens": ["Vn\u00df", "Got\u00b7tes", "gros\u00b7se", "gunst", "aufs", "ne\u00b7we", "vor", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "APPRART", "ADJA", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "O h\u00e4rter al\u00df ein Stein/ den nicht die trew bewegt/", "tokens": ["O", "h\u00e4r\u00b7ter", "al\u00df", "ein", "Stein", "/", "den", "nicht", "die", "trew", "be\u00b7wegt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KOKOM", "ART", "NN", "$(", "ART", "PTKNEG", "ART", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn Sonn vnd Tag verschwartzt/ wenn sich der Erd-", "tokens": ["Wenn", "Sonn", "vnd", "Tag", "ver\u00b7schwartzt", "/", "wenn", "sich", "der", "Erd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "VVPP", "$(", "KOUS", "PRF", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "krei\u00df regt ", "tokens": ["krei\u00df", "regt"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Wenn Todten selbst erstehn vnd hartte Fel\u00df aufspringen.", "tokens": ["Wenn", "Tod\u00b7ten", "selbst", "er\u00b7stehn", "vnd", "hart\u00b7te", "Fel\u00df", "auf\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVINF", "KON", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}