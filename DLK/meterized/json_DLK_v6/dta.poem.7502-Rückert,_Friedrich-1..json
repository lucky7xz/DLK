{"dta.poem.7502": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein indischer Brahman, geboren auf der Flur,", "tokens": ["Ein", "in\u00b7di\u00b7scher", "Brah\u00b7man", ",", "ge\u00b7bo\u00b7ren", "auf", "der", "Flur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der nichts gelesen als den Weda der Natur;", "tokens": ["Der", "nichts", "ge\u00b7le\u00b7sen", "als", "den", "We\u00b7da", "der", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVPP", "KOKOM", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hat viel gesehn, gedacht, noch mehr geahnt, gef\u00fchlt,", "tokens": ["Hat", "viel", "ge\u00b7sehn", ",", "ge\u00b7dacht", ",", "noch", "mehr", "ge\u00b7ahnt", ",", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,", "VVPP", "$,", "ADV", "ADV", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit Betrachtungen die Leidenschaft gek\u00fchlt;", "tokens": ["Und", "mit", "Be\u00b7trach\u00b7tun\u00b7gen", "die", "Lei\u00b7den\u00b7schaft", "ge\u00b7k\u00fchlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Spricht bald was klar ihm ward, bald um sich's klar zu machen,", "tokens": ["Spricht", "bald", "was", "klar", "ihm", "ward", ",", "bald", "um", "sich's", "klar", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PWS", "ADJD", "PPER", "VAFIN", "$,", "ADV", "APPR", "PIS", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von ihn angeh'nden halb, halb nicht angeh'nden Sachen.", "tokens": ["Von", "ihn", "an\u00b7geh'n\u00b7den", "halb", ",", "halb", "nicht", "an\u00b7geh'n\u00b7den", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "ADJD", "$,", "ADJD", "PTKNEG", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Er hat die Eigenheit, nur Einzelnes zu sehn,", "tokens": ["Er", "hat", "die", "Ei\u00b7gen\u00b7heit", ",", "nur", "Ein\u00b7zel\u00b7nes", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+----+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Doch alles Einzelne als Ganzes zu verstehn.", "tokens": ["Doch", "al\u00b7les", "Ein\u00b7zel\u00b7ne", "als", "Gan\u00b7zes", "zu", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJA", "KOUS", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}, "stanza.5": {"line.1": {"text": "Woran er immer nur sieht schimmern einen Glanz,", "tokens": ["Wo\u00b7ran", "er", "im\u00b7mer", "nur", "sieht", "schim\u00b7mern", "ei\u00b7nen", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVFIN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird ein Betk\u00fcgelchen an seinem Rosenkranz.", "tokens": ["Wird", "ein", "Bet\u00b7k\u00fc\u00b7gel\u00b7chen", "an", "sei\u00b7nem", "Ro\u00b7sen\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}