{"textgrid.poem.65900": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die Mutter", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor Demeter, der besten, schwand die Tochter.", "tokens": ["Vor", "De\u00b7me\u00b7ter", ",", "der", "bes\u00b7ten", ",", "schwand", "die", "Toch\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "\u00bbmein Kind!\u00ab war der Verletzten Scheidungs-Schrei.", "tokens": ["\u00bb", "mein", "Kind", "!", "\u00ab", "war", "der", "Ver\u00b7letz\u00b7ten", "Schei\u00b7dungs\u00b7Schrei", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$.", "$(", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der G\u00f6ttin Geist ergriff sich Raserei:", "tokens": ["Der", "G\u00f6t\u00b7tin", "Geist", "er\u00b7griff", "sich", "Ra\u00b7se\u00b7rei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PRF", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie strich von still \u2013 zu j\u00e4h \u2013 voll Leid, bepochter", "tokens": ["Sie", "strich", "von", "still", "\u2013", "zu", "j\u00e4h", "\u2013", "voll", "Leid", ",", "be\u00b7poch\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "ADJD", "APPR", "ADJD", "$(", "PTKA", "ADJD", "$(", "ADJD", "NN", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Umgrottung Pans. Sie horchte; doch vermocht er", "tokens": ["Um\u00b7grot\u00b7tung", "Pans", ".", "Sie", "horch\u00b7te", ";", "doch", "ver\u00b7mocht", "er"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$.", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht Wort, noch Ort zu finden, wo sie sei.", "tokens": ["Nicht", "Wort", ",", "noch", "Ort", "zu", "fin\u00b7den", ",", "wo", "sie", "sei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "ADV", "NN", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbso hilf mir H\u00e9kate! Mein Kind befrei", "tokens": ["\u00bb", "so", "hilf", "mir", "H\u00e9\u00b7ka\u00b7te", "!", "Mein", "Kind", "be\u00b7frei"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "VVIMP", "PPER", "NE", "$.", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom R\u00e4uber! Wer entwand, wo unterjocht er", "tokens": ["Vom", "R\u00e4u\u00b7ber", "!", "Wer", "ent\u00b7wand", ",", "wo", "un\u00b7ter\u00b7jocht", "er"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "PWS", "VVFIN", "$,", "PWAV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das einzige, mir zarte M\u00e4gdelein?\u00ab", "tokens": ["Das", "ein\u00b7zi\u00b7ge", ",", "mir", "zar\u00b7te", "M\u00e4g\u00b7del\u00b7ein", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Rang Demeter. Das helle Weib am Weg", "tokens": ["Rang", "De\u00b7me\u00b7ter", ".", "Das", "hel\u00b7le", "Weib", "am", "Weg"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$.", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Belauschte seinen hohen Weihestein.", "tokens": ["Be\u00b7lauschte", "sei\u00b7nen", "ho\u00b7hen", "Wei\u00b7he\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbo Mutter, h\u00fclle schwarz dich ein und leg", "tokens": ["\u00bb", "o", "Mut\u00b7ter", ",", "h\u00fcl\u00b7le", "schwarz", "dich", "ein", "und", "leg"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "FM", "NN", "$,", "VVFIN", "ADJD", "PPER", "PTKVZ", "KON", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vors Totentor das Ohr! Bei meinem Schein\u00ab,", "tokens": ["Vors", "To\u00b7ten\u00b7tor", "das", "Ohr", "!", "Bei", "mei\u00b7nem", "Schein", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "APPR", "PPOSAT", "NN", "$(", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So rief es: \u00bbfindest du den letzten Steg!\u00ab", "tokens": ["So", "rief", "es", ":", "\u00bb", "fin\u00b7dest", "du", "den", "letz\u00b7ten", "Steg", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}