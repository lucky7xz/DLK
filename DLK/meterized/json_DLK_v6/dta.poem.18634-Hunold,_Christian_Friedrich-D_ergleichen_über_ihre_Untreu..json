{"dta.poem.18634": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "D ergleichen \u00fcber ihre Untreu.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Kan wohl ein sterblicher von mehrern Ungl\u00fcck wissen?", "tokens": ["Kan", "wohl", "ein", "sterb\u00b7li\u00b7cher", "von", "meh\u00b7rern", "Un\u00b7gl\u00fcck", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn mein Vergn\u00fcgen stirbt so leben Angst und Noth:", "tokens": ["Wenn", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "stirbt", "so", "le\u00b7ben", "Angst", "und", "Noth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Licht verbirget sich/ doch nicht die Finsternissen/", "tokens": ["Mein", "Licht", "ver\u00b7bir\u00b7get", "sich", "/", "doch", "nicht", "die", "Fins\u00b7ter\u00b7nis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "$(", "ADV", "PTKNEG", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Leben selber flieht/ so martert mich der Todt.", "tokens": ["Mein", "Le\u00b7ben", "sel\u00b7ber", "flieht", "/", "so", "mar\u00b7tert", "mich", "der", "Todt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Liebste will mein Feind/ die Feinde Freunde werden", "tokens": ["Das", "Liebs\u00b7te", "will", "mein", "Feind", "/", "die", "Fein\u00b7de", "Freun\u00b7de", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "$(", "ART", "NN", "NN", "VAINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Furien sind da/ die weil mein Engel fleucht/", "tokens": ["Die", "Fu\u00b7ri\u00b7en", "sind", "da", "/", "die", "weil", "mein", "En\u00b7gel", "fleucht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$(", "ART", "KOUS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Meisterin verbannt den Sch\u00fcler von der Erden/", "tokens": ["Die", "Meis\u00b7te\u00b7rin", "ver\u00b7bannt", "den", "Sch\u00fc\u00b7ler", "von", "der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil er den Griffel nicht mit puren Gold bestreicht/", "tokens": ["Weil", "er", "den", "Grif\u00b7fel", "nicht", "mit", "pu\u00b7ren", "Gold", "be\u00b7streicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch dein Magnet ist falsch wenn er nur Gold will ziehen/", "tokens": ["Doch", "dein", "Mag\u00b7net", "ist", "falsch", "wenn", "er", "nur", "Gold", "will", "zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "KOUS", "PPER", "ADV", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie zog er denn zu vor auch Fleisch und Blut an sich?", "tokens": ["Wie", "zog", "er", "denn", "zu", "vor", "auch", "Fleisch", "und", "Blut", "an", "sich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "APPR", "ADV", "NN", "KON", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Nadel darf sich sonst nach Norden nur bem\u00fchen", "tokens": ["Die", "Na\u00b7del", "darf", "sich", "sonst", "nach", "Nor\u00b7den", "nur", "be\u00b7m\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADV", "APPR", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und hier ist Kupffer mehr als kostbar Ertz vor dich.", "tokens": ["Und", "hier", "ist", "Kupf\u00b7fer", "mehr", "als", "kost\u00b7bar", "Ertz", "vor", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "PIAT", "KOKOM", "ADJD", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch kan man schlecht Metal nicht auch zu Golde machen?", "tokens": ["Doch", "kan", "man", "schlecht", "Me\u00b7tal", "nicht", "auch", "zu", "Gol\u00b7de", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "ADJD", "NN", "PTKNEG", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ach ja wer mit den Stein der Weisen ist begl\u00fcckt:", "tokens": ["Ach", "ja", "wer", "mit", "den", "Stein", "der", "Wei\u00b7sen", "ist", "be\u00b7gl\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "PWS", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und hab' ich diesen nicht? Erinnre dich mit lachen", "tokens": ["Und", "hab'", "ich", "die\u00b7sen", "nicht", "?", "E\u00b7rinn\u00b7re", "dich", "mit", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PDS", "PTKNEG", "$.", "VVFIN", "PRF", "APPR", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wie offt du ihn so wohl mit deiner Hand gedr\u00fcckt.", "tokens": ["Wie", "offt", "du", "ihn", "so", "wohl", "mit", "dei\u00b7ner", "Hand", "ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch dein Ged\u00e4chtni\u00df ist dem Monde gleich zu sch\u00e4tzen/", "tokens": ["Doch", "dein", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "ist", "dem", "Mon\u00b7de", "gleich", "zu", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NE", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Es nimmet bey mir ab/ bey andern wird es voll.", "tokens": ["Es", "nim\u00b7met", "bey", "mir", "ab", "/", "bey", "an\u00b7dern", "wird", "es", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$(", "APPR", "PIS", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nun will der Geist den Leib in gleichen grade setzen", "tokens": ["Nun", "will", "der", "Geist", "den", "Leib", "in", "glei\u00b7chen", "gra\u00b7de", "set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "ART", "NN", "APPR", "ADJA", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und deinem Hochmuth dient nur fremder Liebes-Zoll.", "tokens": ["Und", "dei\u00b7nem", "Hoch\u00b7muth", "dient", "nur", "frem\u00b7der", "Lie\u00b7bes\u00b7Zoll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Venus kan mich zwar nicht ihren Stiff-Sohn nennen", "tokens": ["Die", "Ve\u00b7nus", "kan", "mich", "zwar", "nicht", "ih\u00b7ren", "Stiff\u00b7Sohn", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Sie l\u00e4sset der Natur die steiffen Z\u00fcgel nach.", "tokens": ["Sie", "l\u00e4s\u00b7set", "der", "Na\u00b7tur", "die", "steif\u00b7fen", "Z\u00fc\u00b7gel", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Doch Hymens Fackel darf bey solcher Gluth nicht brennen/", "tokens": ["Doch", "Hy\u00b7mens", "Fa\u00b7ckel", "darf", "bey", "sol\u00b7cher", "Gluth", "nicht", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VMFIN", "APPR", "PIAT", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der Juno Tempel wei\u00dft den Weg zum Schlaff-Gemach.", "tokens": ["Der", "Ju\u00b7no", "Tem\u00b7pel", "wei\u00dft", "den", "Weg", "zum", "Schlaff\u00b7Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ach k\u00f6ndte meine Hand so kostbar Weyrauch streuen/", "tokens": ["Ach", "k\u00f6nd\u00b7te", "mei\u00b7ne", "Hand", "so", "kost\u00b7bar", "Wey\u00b7rauch", "streu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "ADV", "ADJD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So w\u00fcrde mir dein Schoo\u00df noch zum Altar gereicht:", "tokens": ["So", "w\u00fcr\u00b7de", "mir", "dein", "Schoo\u00df", "noch", "zum", "Al\u00b7tar", "ge\u00b7reicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich m\u00fcste Gold-Tinctur/ wie Perlen Tropffen speyen/", "tokens": ["Ich", "m\u00fcs\u00b7te", "Gold\u00b7Tinc\u00b7tur", "/", "wie", "Per\u00b7len", "Tropf\u00b7fen", "spe\u00b7yen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "$(", "KOKOM", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.28": {"text": "Weil Lieb und Gelbesucht nur bey den Mitteln weicht.", "tokens": ["Weil", "Lieb", "und", "Gel\u00b7be\u00b7sucht", "nur", "bey", "den", "Mit\u00b7teln", "weicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Doch f\u00fclle nur die Hand/ und h\u00fcte dich f\u00fcr Pillen", "tokens": ["Doch", "f\u00fcl\u00b7le", "nur", "die", "Hand", "/", "und", "h\u00fc\u00b7te", "dich", "f\u00fcr", "Pil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$(", "KON", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die nach neun Monate die W\u00fcrckung lassen sehn/", "tokens": ["Die", "nach", "neun", "Mo\u00b7na\u00b7te", "die", "W\u00fcr\u00b7ckung", "las\u00b7sen", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "CARD", "NN", "ART", "NN", "VVINF", "VVINF", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.31": {"text": "Die M\u00e4ssigkeit kan auch den Liebes-Hunger stillen/", "tokens": ["Die", "M\u00e4s\u00b7sig\u00b7keit", "kan", "auch", "den", "Lie\u00b7bes\u00b7Hun\u00b7ger", "stil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sonst wird Verr\u00e4hterey durch eine Wieg' entstehn.", "tokens": ["Sonst", "wird", "Ver\u00b7r\u00e4h\u00b7te\u00b7rey", "durch", "ei\u00b7ne", "Wieg'", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Nun meine Sorgfalt kan nicht deinen Zorn verdienen/", "tokens": ["Nun", "mei\u00b7ne", "Sorg\u00b7falt", "kan", "nicht", "dei\u00b7nen", "Zorn", "ver\u00b7die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Du liessest mich ja sonst darum bek\u00fcmmert seyn.", "tokens": ["Du", "lies\u00b7sest", "mich", "ja", "sonst", "da\u00b7rum", "be\u00b7k\u00fcm\u00b7mert", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "PAV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ich fragte: Will es noch in guten Wohlseyn gr\u00fcnen?", "tokens": ["Ich", "frag\u00b7te", ":", "Will", "es", "noch", "in", "gu\u00b7ten", "Wohl\u00b7seyn", "gr\u00fc\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Bl\u00f6st Zephirs Lieblichkeit auch k\u00fchle Lufft hinein?", "tokens": ["Bl\u00f6st", "Ze\u00b7phirs", "Lieb\u00b7lich\u00b7keit", "auch", "k\u00fch\u00b7le", "Lufft", "hin\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wie schl\u00e4gt der Pul\u00df auch noch? Wie schreiben sich die Zeiten?", "tokens": ["Wie", "schl\u00e4gt", "der", "Pul\u00df", "auch", "noch", "?", "Wie", "schrei\u00b7ben", "sich", "die", "Zei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADV", "$.", "PWAV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Bleicht irgends Purpur roht die Rosen im Gesicht?", "tokens": ["Bleicht", "ir\u00b7gends", "Pur\u00b7pur", "roht", "die", "Ro\u00b7sen", "im", "Ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wie thut es dir auch weh im Tantzen und im Schreiten?", "tokens": ["Wie", "thut", "es", "dir", "auch", "weh", "im", "Tant\u00b7zen", "und", "im", "Schrei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und was die Liebe mehr von dieser Sache spricht.", "tokens": ["Und", "was", "die", "Lie\u00b7be", "mehr", "von", "die\u00b7ser", "Sa\u00b7che", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": [