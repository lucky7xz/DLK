{"dta.poem.8890": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxIII .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Und wirft nur Schatten her aus alten Tagen,", "tokens": ["Und", "wirft", "nur", "Schat\u00b7ten", "her", "aus", "al\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "APZR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es liegt der Leu der Republik erschlagen,", "tokens": ["Es", "liegt", "der", "Leu", "der", "Re\u00b7pub\u00b7lik", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und \u00f6de feiern seines Kerkers R\u00e4ume.", "tokens": ["Und", "\u00f6\u00b7de", "fei\u00b7ern", "sei\u00b7nes", "Ker\u00b7kers", "R\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die ehrnen Hengste, die durch salz'ge Sch\u00e4ume", "tokens": ["Die", "ehr\u00b7nen", "Hengs\u00b7te", ",", "die", "durch", "sa\u00b7lz'\u00b7ge", "Sch\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dahergeschleppt, auf jener Kirche ragen,", "tokens": ["Da\u00b7her\u00b7ge\u00b7schleppt", ",", "auf", "je\u00b7ner", "Kir\u00b7che", "ra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht mehr dieselben sind sie, ach! sie tragen", "tokens": ["Nicht", "mehr", "die\u00b7sel\u00b7ben", "sind", "sie", ",", "ach", "!", "sie", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ADV", "PDS", "VAFIN", "PPER", "$,", "ITJ", "$.", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des korsikan'schen Ueberwinders Z\u00e4ume.", "tokens": ["Des", "kor\u00b7si\u00b7kan'\u00b7schen", "Ue\u00b7ber\u00b7win\u00b7ders", "Z\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wo ist das Volk von K\u00f6nigen geblieben,", "tokens": ["Wo", "ist", "das", "Volk", "von", "K\u00f6\u00b7ni\u00b7gen", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das diese Marmorh\u00e4user durfte bauen,", "tokens": ["Das", "die\u00b7se", "Mar\u00b7mor\u00b7h\u00e4u\u00b7ser", "durf\u00b7te", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PDAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die nun verfallen und gemach zerstieben?", "tokens": ["Die", "nun", "ver\u00b7fal\u00b7len", "und", "ge\u00b7mach", "zer\u00b7stie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nur selten finden auf des Enkels Brauen", "tokens": ["Nur", "sel\u00b7ten", "fin\u00b7den", "auf", "des", "En\u00b7kels", "Brau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVINF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Ahnen gro\u00dfe Z\u00fcge sich geschrieben,", "tokens": ["Der", "Ah\u00b7nen", "gro\u00b7\u00dfe", "Z\u00fc\u00b7ge", "sich", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An Dogengr\u00e4bern in den Stein gehauen.", "tokens": ["An", "Do\u00b7gen\u00b7gr\u00e4\u00b7bern", "in", "den", "Stein", "ge\u00b7hau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}