{"textgrid.poem.31578": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Tugend-Vberflu\u00df / st\u00e4ts-quellendes Verm\u00f6gen /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Tugend-Vberflu\u00df / st\u00e4ts-quellendes Verm\u00f6gen /", "tokens": ["Du", "Tu\u00b7gen\u00b7d\u00b7V\u00b7ber\u00b7flu\u00df", "/", "st\u00e4ts\u00b7quel\u00b7len\u00b7des", "Ver\u00b7m\u00f6\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "du ewig-gebends Gut / bleibst doch untrennbar ganz!", "tokens": ["du", "e\u00b7wig\u00b7ge\u00b7bends", "Gut", "/", "bleibst", "doch", "un\u00b7trenn\u00b7bar", "ganz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "VVFIN", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-++-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Ach Allerleuchtender / doch ungesehner Glanz!", "tokens": ["Ach", "Al\u00b7ler\u00b7leuch\u00b7ten\u00b7der", "/", "doch", "un\u00b7ge\u00b7seh\u00b7ner", "Glanz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$(", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die selbst' und einig Ruh / kanst alles doch bewegen!", "tokens": ["die", "selbst'", "und", "ei\u00b7nig", "Ruh", "/", "kanst", "al\u00b7les", "doch", "be\u00b7we\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJD", "NN", "$(", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "wohnst in dem Jubel-Thron / und bist doch st\u00e4ts zugegen.", "tokens": ["wohnst", "in", "dem", "Ju\u00b7bel\u00b7Thron", "/", "und", "bist", "doch", "st\u00e4ts", "zu\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$(", "KON", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.6": {"text": "Man siht dich nicht / und tr\u00e4gst den Hoheit Strahlen Kranz.", "tokens": ["Man", "siht", "dich", "nicht", "/", "und", "tr\u00e4gst", "den", "Ho\u00b7heit", "Strah\u00b7len", "Kranz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "$(", "KON", "VVFIN", "ART", "NN", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "wir sind vor dir / wie Staub: noch wachst f\u00fcr unser Schanz /", "tokens": ["wir", "sind", "vor", "dir", "/", "wie", "Staub", ":", "noch", "wachst", "f\u00fcr", "un\u00b7ser", "Schanz", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$(", "KOKOM", "NN", "$.", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und gibst / die wir den Fluch verdienet / Himmel-Segen.", "tokens": ["und", "gibst", "/", "die", "wir", "den", "Fluch", "ver\u00b7die\u00b7net", "/", "Him\u00b7mel\u00b7Se\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PRELS", "PPER", "ART", "NN", "VVFIN", "$(", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du Vnerreichlichkeit im Wesen / Willen / Wundern /", "tokens": ["Du", "Vner\u00b7reich\u00b7lich\u00b7keit", "im", "We\u00b7sen", "/", "Wil\u00b7len", "/", "Wun\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "APPRART", "NN", "$(", "NN", "$(", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "besonders in der G\u00fct / die dich schier \u00fcbertrifft!", "tokens": ["be\u00b7son\u00b7ders", "in", "der", "G\u00fct", "/", "die", "dich", "schier", "\u00fc\u00b7bert\u00b7rifft", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$(", "PRELS", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "la\u00df mich den Lobes-Geist / zu loben / recht aufmundern.", "tokens": ["la\u00df", "mich", "den", "Lo\u00b7bes\u00b7Geist", "/", "zu", "lo\u00b7ben", "/", "recht", "auf\u00b7mun\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$(", "PTKZU", "VVINF", "$(", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Dein Klarheit recht verkl\u00e4r' / in meiner dunklen Schrifft /", "tokens": ["Dein", "Klar\u00b7heit", "recht", "ver\u00b7kl\u00e4r'", "/", "in", "mei\u00b7ner", "dunk\u00b7len", "Schrifft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$(", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "du Erzvollkomnes Gut / du All-und Einigkeit /", "tokens": ["du", "Erz\u00b7voll\u00b7kom\u00b7nes", "Gut", "/", "du", "All\u00b7\u00b7und", "Ei\u00b7nig\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "PPER", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "du Dreyheit / die in Eins besteht und geht allzeit!", "tokens": ["du", "Drey\u00b7heit", "/", "die", "in", "Eins", "be\u00b7steht", "und", "geht", "all\u00b7zeit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "ART", "APPR", "NN", "VVFIN", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}