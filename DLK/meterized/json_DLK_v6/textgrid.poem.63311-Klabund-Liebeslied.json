{"textgrid.poem.63311": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Liebeslied", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hui \u00fcber drei Oktaven", "tokens": ["Hui", "\u00fc\u00b7ber", "drei", "Ok\u00b7ta\u00b7ven"], "token_info": ["word", "word", "word", "word"], "pos": ["FM", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Glissando unsre Lust.", "tokens": ["Glis\u00b7san\u00b7do", "uns\u00b7re", "Lust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df mich noch einmal schlafen", "tokens": ["La\u00df", "mich", "noch", "ein\u00b7mal", "schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An deiner Brust.", "tokens": ["An", "dei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Fern schleicht der Morgen sachte,", "tokens": ["Fern", "schleicht", "der", "Mor\u00b7gen", "sach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Hahn, kein K\u00f6ter kl\u00e4fft.", "tokens": ["Kein", "Hahn", ",", "kein", "K\u00f6\u00b7ter", "kl\u00e4fft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du brauchst doch erst um achte", "tokens": ["Du", "brauchst", "doch", "erst", "um", "ach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ins Gesch\u00e4ft.", "tokens": ["Ins", "Ge\u00b7sch\u00e4ft", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "La\u00df die Matratze knarren!", "tokens": ["La\u00df", "die", "Ma\u00b7trat\u00b7ze", "knar\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach hinten schl\u00e4ft der Wirt.", "tokens": ["Nach", "hin\u00b7ten", "schl\u00e4ft", "der", "Wirt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie deine Augen starren!", "tokens": ["Wie", "dei\u00b7ne", "Au\u00b7gen", "star\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Atem girrt!", "tokens": ["Dein", "A\u00b7tem", "girrt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Um deine Stirn der Morgen", "tokens": ["Um", "dei\u00b7ne", "Stirn", "der", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Flicht einen bleichen Kranz.", "tokens": ["Flicht", "ei\u00b7nen", "blei\u00b7chen", "Kranz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du ruhst in ihm geborgen", "tokens": ["Du", "ruhst", "in", "ihm", "ge\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als eine Heilige und Jungfrau ganz.", "tokens": ["Als", "ei\u00b7ne", "Hei\u00b7li\u00b7ge", "und", "Jung\u00b7frau", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}