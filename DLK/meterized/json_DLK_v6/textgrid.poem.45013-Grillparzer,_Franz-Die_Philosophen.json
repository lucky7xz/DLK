{"textgrid.poem.45013": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Die Philosophen", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur \u00fcberbieten wollen sie,", "tokens": ["Nur", "\u00fc\u00b7berb\u00b7ie\u00b7ten", "wol\u00b7len", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Eitelkeit zu Dank,", "tokens": ["Der", "Ei\u00b7tel\u00b7keit", "zu", "Dank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Biegt Hegel erst ein Paroli,", "tokens": ["Biegt", "He\u00b7gel", "erst", "ein", "Pa\u00b7ro\u00b7li", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ART", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Spielt Schelling sein va banque.", "tokens": ["Spielt", "Schel\u00b7ling", "sein", "va", "ban\u00b7que", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPOSAT", "FM", "FM", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}