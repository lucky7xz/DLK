{"textgrid.poem.48663": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "26. Schmerz der Trennung", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was s\u00e4umst du dich, o Seele, zu zerspringen", "tokens": ["Was", "s\u00e4umst", "du", "dich", ",", "o", "See\u00b7le", ",", "zu", "zer\u00b7sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "$,", "FM", "NN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "f\u00fcr Angst, f\u00fcr Qual, die dich und mich umringen,", "tokens": ["f\u00fcr", "Angst", ",", "f\u00fcr", "Qual", ",", "die", "dich", "und", "mich", "um\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "PRELS", "PPER", "KON", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und bist noch du, mein Herze, nicht entzwei?", "tokens": ["Und", "bist", "noch", "du", ",", "mein", "Her\u00b7ze", ",", "nicht", "ent\u00b7zwei", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPER", "$,", "PPOSAT", "VVFIN", "$,", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Tu's doch, tu's bald und mach' uns beide frei!", "tokens": ["Tu's", "doch", ",", "tu's", "bald", "und", "mach'", "uns", "bei\u00b7de", "frei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "NE", "ADV", "KON", "VVFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O da\u00df ich doch den Tag erleben m\u00fcssen,", "tokens": ["O", "da\u00df", "ich", "doch", "den", "Tag", "er\u00b7le\u00b7ben", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "der mir verbeut das sch\u00f6ne Kind zu k\u00fcssen,", "tokens": ["der", "mir", "ver\u00b7beut", "das", "sch\u00f6\u00b7ne", "Kind", "zu", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "der mir versagt das liebe Mensch zu sehn!", "tokens": ["der", "mir", "ver\u00b7sagt", "das", "lie\u00b7be", "Mensch", "zu", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ach mir! was mehr? Es ist um mich geschehn.", "tokens": ["Ach", "mir", "!", "was", "mehr", "?", "Es", "ist", "um", "mich", "ge\u00b7schehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "$.", "PWS", "ADV", "$.", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Anstat da\u00df ich nicht eine Viertelstunde", "tokens": ["An\u00b7stat", "da\u00df", "ich", "nicht", "ei\u00b7ne", "Vier\u00b7tel\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "vor kunte sein von ihrem s\u00fc\u00dfen Munde,", "tokens": ["vor", "kun\u00b7te", "sein", "von", "ih\u00b7rem", "s\u00fc\u00b7\u00dfen", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VMFIN", "PPOSAT", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "da mu\u00df ich nun sein ewig ohne sie.", "tokens": ["da", "mu\u00df", "ich", "nun", "sein", "e\u00b7wig", "oh\u00b7ne", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wo? Ach! wo ist sie nun, die werte, die?", "tokens": ["Wo", "?", "Ach", "!", "wo", "ist", "sie", "nun", ",", "die", "wer\u00b7te", ",", "die", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "$.", "ITJ", "$.", "PWAV", "VAFIN", "PPER", "ADV", "$,", "PRELS", "VVFIN", "$,", "PRELS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Sagts sicher nach, ihr stummen Wasserscharen,", "tokens": ["Sagts", "si\u00b7cher", "nach", ",", "ihr", "stum\u00b7men", "Was\u00b7ser\u00b7scha\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "wie herzlich oft wir beide bei euch waren.", "tokens": ["wie", "herz\u00b7lich", "oft", "wir", "bei\u00b7de", "bei", "euch", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "PPER", "PIS", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Bringts k\u00fchnlich aus, ihr L\u00fcfte, was ihr wi\u00dft,", "tokens": ["Bringts", "k\u00fchn\u00b7lich", "aus", ",", "ihr", "L\u00fcf\u00b7te", ",", "was", "ihr", "wi\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "wie vielmal wir uns haben la\u00df gek\u00fc\u00dft.", "tokens": ["wie", "viel\u00b7mal", "wir", "uns", "ha\u00b7ben", "la\u00df", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Du bla\u00dfer Mund, was ists nunmehr gewesen,", "tokens": ["Du", "bla\u00b7\u00dfer", "Mund", ",", "was", "ists", "nun\u00b7mehr", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PWS", "VAFIN", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "da\u00df du so oft von ihrem bist genesen?", "tokens": ["da\u00df", "du", "so", "oft", "von", "ih\u00b7rem", "bist", "ge\u00b7ne\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Wo ist dein Geist, ihr s\u00fc\u00dfer Atem bin.", "tokens": ["Wo", "ist", "dein", "Geist", ",", "ihr", "s\u00fc\u00b7\u00dfer", "A\u00b7tem", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "von dessen Kraft ich noch verz\u00e4ubert bin.", "tokens": ["von", "des\u00b7sen", "Kraft", "ich", "noch", "ver\u00b7z\u00e4u\u00b7bert", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ich ruf' euch an, o Sonn, o Mon, o Sternen", "tokens": ["Ich", "ruf'", "euch", "an", ",", "o", "Sonn", ",", "o", "Mon", ",", "o", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "FM", "NN", "$,", "FM", "FM", "$,", "FM", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "und was uns sonst das Gl\u00fccke wirkt von Fernen,", "tokens": ["und", "was", "uns", "sonst", "das", "Gl\u00fc\u00b7cke", "wirkt", "von", "Fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ART", "NN", "VVFIN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "ich ruf' euch an, seid Zeugen \u00fcber mir,", "tokens": ["ich", "ruf'", "euch", "an", ",", "seid", "Zeu\u00b7gen", "\u00fc\u00b7ber", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "VAFIN", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "was ich f\u00fcr Angst hier leide wegen ihr.", "tokens": ["was", "ich", "f\u00fcr", "Angst", "hier", "lei\u00b7de", "we\u00b7gen", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Gehabt euch wol, ihr sch\u00f6nsten meiner Tage,", "tokens": ["Ge\u00b7habt", "euch", "wol", ",", "ihr", "sch\u00f6ns\u00b7ten", "mei\u00b7ner", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "der ich mit ihr so viel zu haben pflage!", "tokens": ["der", "ich", "mit", "ihr", "so", "viel", "zu", "ha\u00b7ben", "pfla\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "ADV", "PIS", "PTKZU", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Gehab dich wol, du manche s\u00fc\u00dfe Nacht,", "tokens": ["Ge\u00b7hab", "dich", "wol", ",", "du", "man\u00b7che", "s\u00fc\u00b7\u00dfe", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "die ich mit ihr in Liebe durchgebracht!", "tokens": ["die", "ich", "mit", "ihr", "in", "Lie\u00b7be", "durch\u00b7ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "APPR", "PPOSAT", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Ade, o Platz, den G\u00f6ttern selbst begehret,", "tokens": ["A\u00b7de", ",", "o", "Platz", ",", "den", "G\u00f6t\u00b7tern", "selbst", "be\u00b7ge\u00b7hret", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.30": {"text": "der du sie mir so vielmal hast gew\u00e4ret,", "tokens": ["der", "du", "sie", "mir", "so", "viel\u00b7mal", "hast", "ge\u00b7w\u00e4\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "PPER", "ADV", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Sei tausentmal, sei tausent, tausentmal", "tokens": ["Sei", "tau\u00b7sent\u00b7mal", ",", "sei", "tau\u00b7sent", ",", "tau\u00b7sent\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "ADV", "$,", "VAFIN", "VVPP", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "gegr\u00fc\u00dft! Du bleibst in Lust, ich leb' in Qual,", "tokens": ["ge\u00b7gr\u00fc\u00dft", "!", "Du", "bleibst", "in", "Lust", ",", "ich", "leb'", "in", "Qual", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "PPER", "VVFIN", "APPR", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Ihr B\u00e4ch', ihr P\u00fcsch, ihr G\u00e4rten und Gefilder", "tokens": ["Ihr", "B\u00e4ch'", ",", "ihr", "P\u00fcsch", ",", "ihr", "G\u00e4r\u00b7ten", "und", "Ge\u00b7fil\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "und was ihr hegt, ihr sch\u00f6nen Lenzenbilder,", "tokens": ["und", "was", "ihr", "hegt", ",", "ihr", "sch\u00f6\u00b7nen", "Len\u00b7zen\u00b7bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "du Sommerlust, du Herbst, du Winterzier,", "tokens": ["du", "Som\u00b7mer\u00b7lust", ",", "du", "Herbst", ",", "du", "Win\u00b7ter\u00b7zier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "zu guter Nacht, ich scheid', ihr bleibt bei ihr!", "tokens": ["zu", "gu\u00b7ter", "Nacht", ",", "ich", "scheid'", ",", "ihr", "bleibt", "bei", "ihr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}