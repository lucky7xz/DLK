{"textgrid.poem.62282": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[la\u00df' das Gute mich erringen]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df' das Gute mich erringen,", "tokens": ["La\u00df'", "das", "Gu\u00b7te", "mich", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das B\u00f6se nie,", "tokens": ["Und", "das", "B\u00f6\u00b7se", "nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Gro\u00dfe Taten mich vollbringen", "tokens": ["Gro\u00b7\u00dfe", "Ta\u00b7ten", "mich", "voll\u00b7brin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herr, und segne sie!", "tokens": ["Herr", ",", "und", "seg\u00b7ne", "sie", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "La\u00df' das Haupt mich hoch erheben", "tokens": ["La\u00df'", "das", "Haupt", "mich", "hoch", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber Spanne Zeit", "tokens": ["Ue\u00b7ber", "Span\u00b7ne", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mutig schaffen, wirken, streben", "tokens": ["Mu\u00b7tig", "schaf\u00b7fen", ",", "wir\u00b7ken", ",", "stre\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADJD", "VVINF", "$,", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr die fernste Zeit. \u2013", "tokens": ["F\u00fcr", "die", "ferns\u00b7te", "Zeit", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}