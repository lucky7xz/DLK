{"dta.poem.19957": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Bon Von  Ihr, im Winter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vom Winter zu gesunden,", "tokens": ["Vom", "Win\u00b7ter", "zu", "ge\u00b7sun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Flog Lerche himmelw\u00e4rts.", "tokens": ["Flog", "Ler\u00b7che", "him\u00b7mel\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Noch stund, das Herz voll Wunden,", "tokens": ["Noch", "stund", ",", "das", "Herz", "voll", "Wun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich da im stummen Schmerz,", "tokens": ["Ich", "da", "im", "stum\u00b7men", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da fandest du den Armen,", "tokens": ["Da", "fan\u00b7dest", "du", "den", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und nahmst ihn mit Erbarmen", "tokens": ["Und", "nahmst", "ihn", "mit", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In's jugendliche Herz.", "tokens": ["In's", "ju\u00b7gend\u00b7li\u00b7che", "Herz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In dir sich ihm entfaltet,", "tokens": ["In", "dir", "sich", "ihm", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Leben wunderbar,", "tokens": ["Ein", "Le\u00b7ben", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fortan ihm neu gestaltet", "tokens": ["For\u00b7tan", "ihm", "neu", "ge\u00b7stal\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPER", "ADJD", "VVPP"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die ganze Erde war,", "tokens": ["Die", "gan\u00b7ze", "Er\u00b7de", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kampf war aus ihr geschieden,", "tokens": ["Kampf", "war", "aus", "ihr", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Er sah sie nur in Frieden", "tokens": ["Er", "sah", "sie", "nur", "in", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Aus deinem Auge klar.", "tokens": ["Aus", "dei\u00b7nem", "Au\u00b7ge", "klar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was j\u00fcngst ihm b\u00f6s geschienen,", "tokens": ["Was", "j\u00fcngst", "ihm", "b\u00f6s", "ge\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erschien ihm fromm und gut,", "tokens": ["Er\u00b7schien", "ihm", "fromm", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wollt' er Feinden dienen", "tokens": ["So", "wollt'", "er", "Fein\u00b7den", "die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Armen und mit Blut;", "tokens": ["Mit", "Ar\u00b7men", "und", "mit", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gestillt war alles Sehnen,", "tokens": ["Ge\u00b7stillt", "war", "al\u00b7les", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Getrocknet eitle Thr\u00e4nen", "tokens": ["Ge\u00b7trock\u00b7net", "eit\u00b7le", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In frommer Liebe Glut.", "tokens": ["In", "from\u00b7mer", "Lie\u00b7be", "Glut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Jetzt da die Welt in Schmerzen", "tokens": ["Jetzt", "da", "die", "Welt", "in", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kalt liegt und bl\u00fctenarm,", "tokens": ["Kalt", "liegt", "und", "bl\u00fc\u00b7ten\u00b7arm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Umf\u00e4ngt in deinem Herzen", "tokens": ["Um\u00b7f\u00e4ngt", "in", "dei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihn noch ein Fr\u00fchling warm.", "tokens": ["Ihn", "noch", "ein", "Fr\u00fch\u00b7ling", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Fern von der Welt Get\u00fcmmel,", "tokens": ["Fern", "von", "der", "Welt", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ruht dort ein Stern im Himmel,", "tokens": ["Ruht", "dort", "ein", "Stern", "im", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fchlt nicht der Erde Harm.", "tokens": ["F\u00fchlt", "nicht", "der", "Er\u00b7de", "Harm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}