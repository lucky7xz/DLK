{"dta.poem.15851": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Viel Freunde hab' ich, die mehr meiner Poesie", "tokens": ["Viel", "Freun\u00b7de", "hab'", "ich", ",", "die", "mehr", "mei\u00b7ner", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "$,", "PRELS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als meine Freunde sind, kaum nenn' ich Freunde sie.", "tokens": ["Als", "mei\u00b7ne", "Freun\u00b7de", "sind", ",", "kaum", "nenn'", "ich", "Freun\u00b7de", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur du bist ganz mein Freund, nicht meiner Poesie;", "tokens": ["Nur", "du", "bist", "ganz", "mein", "Freund", ",", "nicht", "mei\u00b7ner", "Poe\u00b7sie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von allem sagst du mir, von meinen Versen nie.", "tokens": ["Von", "al\u00b7lem", "sagst", "du", "mir", ",", "von", "mei\u00b7nen", "Ver\u00b7sen", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "PPER", "$,", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measu