{"textgrid.poem.49301": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "An Nicolaum G\u00f6ttling", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "G\u00f6ttling, alter werter Freund, Stuben-, Tisch- und Bettgeselle,", "tokens": ["G\u00f6tt\u00b7ling", ",", "al\u00b7ter", "wer\u00b7ter", "Freund", ",", "Stu\u00b7ben", ",", "Tisch", "und", "Bett\u00b7ge\u00b7sel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "ADJA", "NN", "$,", "TRUNC", "$,", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "An der Stelle,", "tokens": ["An", "der", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Wo die Warnou sich ergeu\u00dft,", "tokens": ["Wo", "die", "War\u00b7nou", "sich", "er\u00b7geu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Da sie fleu\u00dft", "tokens": ["Da", "sie", "fleu\u00dft"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "In das gro\u00dfe Meer mit Freuden,", "tokens": ["In", "das", "gro\u00b7\u00dfe", "Meer", "mit", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind wir beiden", "tokens": ["Sind", "wir", "bei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "PIAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "L\u00e4ngst gewesen in der Lehr',", "tokens": ["L\u00e4ngst", "ge\u00b7we\u00b7sen", "in", "der", "Lehr'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Aufzufassen Kunst und Ehr'.", "tokens": ["Auf\u00b7zu\u00b7fas\u00b7sen", "Kunst", "und", "Ehr'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ach, wie gern hab' ichs gelesen,", "tokens": ["Ach", ",", "wie", "gern", "hab'", "ichs", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADV", "VAFIN", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wo du nach der Zeit gewesen.", "tokens": ["Wo", "du", "nach", "der", "Zeit", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Magdeburg, dein Vaterland, das dir hat durch Gott gegeben", "tokens": ["Mag\u00b7de\u00b7burg", ",", "dein", "Va\u00b7ter\u00b7land", ",", "das", "dir", "hat", "durch", "Gott", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VAFIN", "APPR", "NN", "VVPP"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Dieses Leben,", "tokens": ["Die\u00b7ses", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Schickte dich auf Rostock hin,", "tokens": ["Schick\u00b7te", "dich", "auf", "Ros\u00b7tock", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Da dein Sinn", "tokens": ["Da", "dein", "Sinn"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Wolte kaufen in der Jugend", "tokens": ["Wol\u00b7te", "kau\u00b7fen", "in", "der", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Kunst und Tugend;", "tokens": ["Kunst", "und", "Tu\u00b7gend", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Damals war ich dein Gesell", "tokens": ["Da\u00b7mals", "war", "ich", "dein", "Ge\u00b7sell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "An der edlen Weisheit Stell',", "tokens": ["An", "der", "ed\u00b7len", "Weis\u00b7heit", "Stell'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Als man lernen kont' in Frieden,", "tokens": ["Als", "man", "ler\u00b7nen", "kont'", "in", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Bis der Krieg uns hat geschieden.", "tokens": ["Bis", "der", "Krieg", "uns", "hat", "ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Rotenburg, die sch\u00f6ne Stadt, hat dir deinen Flei\u00df belohnet,", "tokens": ["Ro\u00b7ten\u00b7burg", ",", "die", "sch\u00f6\u00b7ne", "Stadt", ",", "hat", "dir", "dei\u00b7nen", "Flei\u00df", "be\u00b7loh\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,", "VAFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Da nun wohnet", "tokens": ["Da", "nun", "woh\u00b7net"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.23": {"text": "St\u00fcrzel, der ber\u00fchmte Mann,", "tokens": ["St\u00fcr\u00b7zel", ",", "der", "be\u00b7r\u00fchm\u00b7te", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Der da kan", "tokens": ["Der", "da", "kan"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "VMFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Wol regieren, wol studieren,", "tokens": ["Wol", "re\u00b7gie\u00b7ren", ",", "wol", "stu\u00b7die\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "L\u00e4\u00dft auch sp\u00fcren,", "tokens": ["L\u00e4\u00dft", "auch", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Da\u00df er dir und mir ist hold;", "tokens": ["Da\u00df", "er", "dir", "und", "mir", "ist", "hold", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Solches sch\u00e4tz' ich \u00fcber Gold.", "tokens": ["Sol\u00b7ches", "sch\u00e4tz'", "ich", "\u00fc\u00b7ber", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Nun, mein Bruder, kurz zu schreiben:", "tokens": ["Nun", ",", "mein", "Bru\u00b7der", ",", "kurz", "zu", "schrei\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Rist sol sein und dein verbleiben.", "tokens": ["Rist", "sol", "sein", "und", "dein", "ver\u00b7blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VAINF", "KON", "PPOSAT", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}