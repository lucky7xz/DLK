{"dta.poem.11730": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie einem Thiere mag zu Muth seyn, kann ich doch", "tokens": ["Wie", "ei\u00b7nem", "Thie\u00b7re", "mag", "zu", "Muth", "seyn", ",", "kann", "ich", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VMFIN", "APPR", "NN", "VAINF", "$,", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Begreifen, weil ich selbst als Kind auf Vieren kroch.", "tokens": ["Be\u00b7grei\u00b7fen", ",", "weil", "ich", "selbst", "als", "Kind", "auf", "Vie\u00b7ren", "kroch", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "KOUS", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie einem Vogel sei zu Sinn, begreif' ich nicht,", "tokens": ["Wie", "ei\u00b7nem", "Vo\u00b7gel", "sei", "zu", "Sinn", ",", "be\u00b7greif'", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "APPR", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil stets die Schwinge mir gebrach, und noch gebricht.", "tokens": ["Weil", "stets", "die", "Schwin\u00b7ge", "mir", "ge\u00b7brach", ",", "und", "noch", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "$,", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was alles da so leicht fliegt unterm Himmelsbogen,", "tokens": ["Was", "al\u00b7les", "da", "so", "leicht", "fliegt", "un\u00b7term", "Him\u00b7mels\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "ADV", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus einer andern Welt scheint es hereingeflogen;", "tokens": ["Aus", "ei\u00b7ner", "an\u00b7dern", "Welt", "scheint", "es", "her\u00b7ein\u00b7ge\u00b7flo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Aus einer andern Zeit. Es ging die gro\u00dfe Flut", "tokens": ["Aus", "ei\u00b7ner", "an\u00b7dern", "Zeit", ".", "Es", "ging", "die", "gro\u00b7\u00dfe", "Flut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur \u00fcber Thiertrotz weg, nicht \u00fcber Vogelmut.", "tokens": ["Nur", "\u00fc\u00b7ber", "Thier\u00b7trotz", "weg", ",", "nicht", "\u00fc\u00b7ber", "Vo\u00b7gel\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKVZ", "$,", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sie schwebten, wie zuerst der Geist auf Wassern schwebte,", "tokens": ["Sie", "schweb\u00b7ten", ",", "wie", "zu\u00b7erst", "der", "Geist", "auf", "Was\u00b7sern", "schweb\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sahen zu, wie sich die Sch\u00f6pfung neu belebte.", "tokens": ["Und", "sa\u00b7hen", "zu", ",", "wie", "sich", "die", "Sch\u00f6p\u00b7fung", "neu", "be\u00b7leb\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "PWAV", "PRF", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und wie ein Vogel jetzt, wenn ab in einem Kreise", "tokens": ["Und", "wie", "ein", "Vo\u00b7gel", "jetzt", ",", "wenn", "ab", "in", "ei\u00b7nem", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "$,", "KOUS", "PTKVZ", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Welt ein Fr\u00fchling stirbt, zum andern macht die Reise;", "tokens": ["Der", "Welt", "ein", "Fr\u00fch\u00b7ling", "stirbt", ",", "zum", "an\u00b7dern", "macht", "die", "Rei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,", "APPRART", "ADJA", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So fliegt, wann diesen Stern, ob fremd' ob eigne, Glut", "tokens": ["So", "fliegt", ",", "wann", "die\u00b7sen", "Stern", ",", "ob", "fremd'", "ob", "eig\u00b7ne", ",", "Glut"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PDAT", "NN", "$,", "KOUS", "ADV", "KOUS", "ADJA", "$,", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verzehrt, ein Vogel fern zu andern wohlgemut.", "tokens": ["Ver\u00b7zehrt", ",", "ein", "Vo\u00b7gel", "fern", "zu", "an\u00b7dern", "wohl\u00b7ge\u00b7mut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ihr V\u00f6gel, seid gegr\u00fc\u00dft, und gr\u00fc\u00dft mir alle Fernen,", "tokens": ["Ihr", "V\u00f6\u00b7gel", ",", "seid", "ge\u00b7gr\u00fc\u00dft", ",", "und", "gr\u00fc\u00dft", "mir", "al\u00b7le", "Fer\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "VVPP", "$,", "KON", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von denen ich gelernt, und die von mir einst lernen.", "tokens": ["Von", "de\u00b7nen", "ich", "ge\u00b7lernt", ",", "und", "die", "von", "mir", "einst", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "$,", "KON", "ART", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Ihr habt mir manchen Gru\u00df gebracht aus fremden Land,", "tokens": ["Ihr", "habt", "mir", "man\u00b7chen", "Gru\u00df", "ge\u00b7bracht", "aus", "frem\u00b7den", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und manchen, den ich als vom Himmel her verstand.", "tokens": ["Und", "man\u00b7chen", ",", "den", "ich", "als", "vom", "Him\u00b7mel", "her", "ver\u00b7stand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "KOKOM", "APPRART", "NN", "APZR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}