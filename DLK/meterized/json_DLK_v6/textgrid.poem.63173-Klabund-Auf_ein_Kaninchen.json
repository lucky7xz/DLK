{"textgrid.poem.63173": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein Kaninchen", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weisse Felle, die ich streicheln durfte:", "tokens": ["Weis\u00b7se", "Fel\u00b7le", ",", "die", "ich", "strei\u00b7cheln", "durf\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Vorhang vor dem Heiligtum.", "tokens": ["Vor\u00b7hang", "vor", "dem", "Hei\u00b7lig\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Im Get\u00f6n der spitzen Ohren schlurfte", "tokens": ["Im", "Ge\u00b7t\u00f6n", "der", "spit\u00b7zen", "Oh\u00b7ren", "schlurf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Eine Reisigsammlerin: der Ruhm.", "tokens": ["Ei\u00b7ne", "Rei\u00b7si\u00b7gsamm\u00b7le\u00b7rin", ":", "der", "Ruhm", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sonne sass im Dschungel deiner Lende,", "tokens": ["Son\u00b7ne", "sass", "im", "Dschun\u00b7gel", "dei\u00b7ner", "Len\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wiegte sich als goldne M\u00f6we weit", "tokens": ["Wieg\u00b7te", "sich", "als", "gold\u00b7ne", "M\u00f6\u00b7we", "weit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "KOUS", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Auf den Meeren der gekalkten W\u00e4nde,", "tokens": ["Auf", "den", "Mee\u00b7ren", "der", "ge\u00b7kalk\u00b7ten", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn der Hund im Hellen schreit.", "tokens": ["Wenn", "der", "Hund", "im", "Hel\u00b7len", "schreit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "St\u00e4be st\u00fcrzten: aus den Katakomben", "tokens": ["St\u00e4\u00b7be", "st\u00fcrz\u00b7ten", ":", "aus", "den", "Ka\u00b7ta\u00b7kom\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$.", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deiner H\u00f6hlung, die das Gr\u00fcne barg.", "tokens": ["Dei\u00b7ner", "H\u00f6h\u00b7lung", ",", "die", "das", "Gr\u00fc\u00b7ne", "barg", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Deine Augen, rote Rhomben,", "tokens": ["Dei\u00b7ne", "Au\u00b7gen", ",", "ro\u00b7te", "Rhom\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schliefen in der M\u00fcdigkeiten Sarg.", "tokens": ["Schlie\u00b7fen", "in", "der", "M\u00fc\u00b7dig\u00b7kei\u00b7ten", "Sarg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Dich zertrat der grosse Bernhardiner,", "tokens": ["Dich", "zer\u00b7trat", "der", "gros\u00b7se", "Bern\u00b7har\u00b7di\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus dem Maule schwebte Kohl und Strunk.", "tokens": ["Aus", "dem", "Mau\u00b7le", "schweb\u00b7te", "Kohl", "und", "Strunk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Als des Todes allezeit getreuer Diener", "tokens": ["Als", "des", "To\u00b7des", "al\u00b7le\u00b7zeit", "ge\u00b7treu\u00b7er", "Die\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Sprangst du pfeifend in die D\u00e4mmerung.", "tokens": ["Sprangst", "du", "pfei\u00b7fend", "in", "die", "D\u00e4m\u00b7me\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}