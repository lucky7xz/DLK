{"textgrid.poem.58401": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "439. Vergi\u00df es nicht!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du mu\u00dft dich selber hassen", "tokens": ["Du", "mu\u00dft", "dich", "sel\u00b7ber", "has\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Gott dich gr\u00fcndlich lassen", "tokens": ["Und", "Gott", "dich", "gr\u00fcnd\u00b7lich", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und lieben fest und rein;", "tokens": ["Und", "lie\u00b7ben", "fest", "und", "rein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Stets trachten, ihm in allen", "tokens": ["Stets", "trach\u00b7ten", ",", "ihm", "in", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVINF", "$,", "PPER", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Aufs beste zu gefallen,", "tokens": ["Aufs", "bes\u00b7te", "zu", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "La\u00df deine Freude sein!", "tokens": ["La\u00df", "dei\u00b7ne", "Freu\u00b7de", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}