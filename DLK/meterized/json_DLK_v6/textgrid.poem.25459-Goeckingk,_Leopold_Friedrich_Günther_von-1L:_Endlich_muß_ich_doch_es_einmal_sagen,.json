{"textgrid.poem.25459": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Endlich mu\u00df ich doch es einmal sagen,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Endlich mu\u00df ich doch es einmal sagen,", "tokens": ["End\u00b7lich", "mu\u00df", "ich", "doch", "es", "ein\u00b7mal", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was ich l\u00e4nger nicht verschweigen kann.", "tokens": ["Was", "ich", "l\u00e4n\u00b7ger", "nicht", "ver\u00b7schwei\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Treuer ", "tokens": ["Treu\u00b7er"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Die du hattest, naht der letzt' heran!", "tokens": ["Die", "du", "hat\u00b7test", ",", "naht", "der", "letzt'", "he\u00b7ran", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "$,", "VVFIN", "ART", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "T\u00e4glich, siehst du, wachsen meine Jungen", "tokens": ["T\u00e4g\u00b7lich", ",", "siehst", "du", ",", "wach\u00b7sen", "mei\u00b7ne", "Jun\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und die Zahl von ihren Foderungen,", "tokens": ["Und", "die", "Zahl", "von", "ih\u00b7ren", "Fo\u00b7de\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Aber, ", "tokens": ["A\u00b7ber", ","], "token_info": ["word", "punct"], "pos": ["KON", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Kahl geb\u00fcrstet hast du meine Kleider,", "tokens": ["Kahl", "ge\u00b7b\u00fcrs\u00b7tet", "hast", "du", "mei\u00b7ne", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Und mein Hut, du weist es selber, bricht.", "tokens": ["Und", "mein", "Hut", ",", "du", "weist", "es", "sel\u00b7ber", ",", "bricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Dennoch, wie so oft du auch den Schneider", "tokens": ["Den\u00b7noch", ",", "wie", "so", "oft", "du", "auch", "den", "Schnei\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ADV", "ADV", "PPER", "ADV", "ART", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Rufest, riefst du doch f\u00fcr mich ihn nicht.", "tokens": ["Ru\u00b7fest", ",", "riefst", "du", "doch", "f\u00fcr", "mich", "ihn", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ADV", "APPR", "PPER", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Aber, wenn ich in dem alten Rocke,", "tokens": ["A\u00b7ber", ",", "wenn", "ich", "in", "dem", "al\u00b7ten", "Ro\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "So da steh' an dem Rainettenbaum',", "tokens": ["So", "da", "steh'", "an", "dem", "Rai\u00b7net\u00b7ten\u00b7baum'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Und die Jungen kommen auf dem Stocke,", "tokens": ["Und", "die", "Jun\u00b7gen", "kom\u00b7men", "auf", "dem", "Sto\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Meinen Acten-Riemen statt dem Zaum',", "tokens": ["Mei\u00b7nen", "Ac\u00b7ten\u00b7Rie\u00b7men", "statt", "dem", "Zaum'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "Ihrer Mutter Strumpfband statt der Peitsche,", "tokens": ["Ih\u00b7rer", "Mut\u00b7ter", "Strumpf\u00b7band", "statt", "der", "Peit\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Angeritten \u2013 ha! das geht durchs Mark!", "tokens": ["An\u00b7ge\u00b7rit\u00b7ten", "\u2013", "ha", "!", "das", "geht", "durchs", "Mark", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ITJ", "$.", "PDS", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Alle reiche Kleider, die der Deutsche", "tokens": ["Al\u00b7le", "rei\u00b7che", "Klei\u00b7der", ",", "die", "der", "Deut\u00b7sche"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.20": {"text": "Wie du weist, verschenkt' ich meinen Blessen,", "tokens": ["Wie", "du", "weist", ",", "ver\u00b7schenkt'", "ich", "mei\u00b7nen", "Bles\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Und doch war der Blesse mir so werth!", "tokens": ["Und", "doch", "war", "der", "Bles\u00b7se", "mir", "so", "werth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.22": {"text": "F\u00fcr den Hafer, den er sonst gefressen,", "tokens": ["F\u00fcr", "den", "Ha\u00b7fer", ",", "den", "er", "sonst", "ge\u00b7fres\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Kauft' ich Fritzen manch gemahltes Pferd", "tokens": ["Kauft'", "ich", "Frit\u00b7zen", "manch", "ge\u00b7mahl\u00b7tes", "Pferd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "NN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "Ging zu Fu\u00df im Feld' umher spatzieren,", "tokens": ["Ging", "zu", "Fu\u00df", "im", "Feld'", "um\u00b7her", "spat\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPRART", "NN", "PTKVZ", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "War, es wenig achtend, Abends lahm,", "tokens": ["War", ",", "es", "we\u00b7nig", "ach\u00b7tend", ",", "A\u00b7bends", "lahm", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PPER", "ADV", "ADJD", "$,", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.26": {"text": "Wenn juchheiend nur mit seinen Thieren", "tokens": ["Wenn", "juch\u00b7hei\u00b7end", "nur", "mit", "sei\u00b7nen", "Thie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Fritz mir im Galopp' entgegen kam,", "tokens": ["Fritz", "mir", "im", "Ga\u00b7lopp'", "ent\u00b7ge\u00b7gen", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "APPO", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.28": {"text": "Aller Nationen Pferde kannte,", "tokens": ["Al\u00b7ler", "Na\u00b7ti\u00b7o\u00b7nen", "Pfer\u00b7de", "kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "Aller Arten Hunde Namen nannte,", "tokens": ["Al\u00b7ler", "Ar\u00b7ten", "Hun\u00b7de", "Na\u00b7men", "nann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Und vom Tigerthier' in Afrika", "tokens": ["Und", "vom", "Ti\u00b7gert\u00b7hier'", "in", "Af\u00b7ri\u00b7ka"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "APPR", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.31": {"text": "Schreckliche Geschichten mir erz\u00e4hlte,", "tokens": ["Schreck\u00b7li\u00b7che", "Ge\u00b7schich\u00b7ten", "mir", "er\u00b7z\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "Und mich k\u00fcssend, und mich streichelnd qu\u00e4lte:", "tokens": ["Und", "mich", "k\u00fcs\u00b7send", ",", "und", "mich", "strei\u00b7chelnd", "qu\u00e4l\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "$,", "KON", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.33": {"text": "Nun erz\u00e4hl' du auch mir was, Papa!", "tokens": ["Nun", "er\u00b7z\u00e4hl'", "du", "auch", "mir", "was", ",", "Pa\u00b7pa", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "PIS", "$,", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.34": {"text": "Werde, guter ", "tokens": ["Wer\u00b7de", ",", "gu\u00b7ter"], "token_info": ["word", "punct", "word"], "pos": ["VAFIN", "$,", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.35": {"text": "Da\u00df ich auch von dir mich trennen mu\u00df.", "tokens": ["Da\u00df", "ich", "auch", "von", "dir", "mich", "tren\u00b7nen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.36": {"text": "Ich, der nie Fortunens G\u00fcrtel l\u00f6se,", "tokens": ["Ich", ",", "der", "nie", "For\u00b7tu\u00b7nens", "G\u00fcr\u00b7tel", "l\u00f6\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "NE", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.37": {"text": "Dem sie selten einen lauen Ku\u00df", "tokens": ["Dem", "sie", "sel\u00b7ten", "ei\u00b7nen", "lau\u00b7en", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.38": {"text": "Nur erlaubet, soll ich armen Bauren,", "tokens": ["Nur", "er\u00b7lau\u00b7bet", ",", "soll", "ich", "ar\u00b7men", "Bau\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VMFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.39": {"text": "Guten Rath, nach Louisdor-Gewicht,", "tokens": ["Gu\u00b7ten", "Rath", ",", "nach", "Louis\u00b7dor\u00b7Ge\u00b7wicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.40": {"text": "K\u00fcnftig geben? Und sie kalt bedauren,", "tokens": ["K\u00fcnf\u00b7tig", "ge\u00b7ben", "?", "Und", "sie", "kalt", "be\u00b7dau\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$.", "KON", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.41": {"text": "Wenn f\u00fcr sie kein fetter Truthahn spricht?", "tokens": ["Wenn", "f\u00fcr", "sie", "kein", "fet\u00b7ter", "Trut\u00b7hahn", "spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.42": {"text": "Soll ich um ein H\u00f6schen f\u00fcr die Jungen,", "tokens": ["Soll", "ich", "um", "ein", "H\u00f6\u00b7schen", "f\u00fcr", "die", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.43": {"text": "Mit dem Schneider l\u00e4rmen, zanken, drohn,", "tokens": ["Mit", "dem", "Schnei\u00b7der", "l\u00e4r\u00b7men", ",", "zan\u00b7ken", ",", "drohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NE", "VVFIN", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.44": {"text": "Bis ich noch zwei Groschen abgedungen,", "tokens": ["Bis", "ich", "noch", "zwei", "Gro\u00b7schen", "ab\u00b7ge\u00b7dun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "CARD", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.45": {"text": "Ach! vielleicht des Mannes ganzen Lohn!", "tokens": ["Ach", "!", "viel\u00b7leicht", "des", "Man\u00b7nes", "gan\u00b7zen", "Lohn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.46": {"text": "Willst du mich vor Sonnen-Aufgang wecken,", "tokens": ["Willst", "du", "mich", "vor", "Son\u00b7nen\u00b7Auf\u00b7gang", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.47": {"text": "Noch ein Licht auf meinen Leuchter stecken,", "tokens": ["Noch", "ein", "Licht", "auf", "mei\u00b7nen", "Leuch\u00b7ter", "ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.48": {"text": "Wann bei keinem Nachbar Licht mehr brennt,", "tokens": ["Wann", "bei", "kei\u00b7nem", "Nach\u00b7bar", "Licht", "mehr", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PIAT", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.49": {"text": "Jede Me\u00df' ein B\u00fcchlein auszuhecken,", "tokens": ["Je\u00b7de", "Me\u00df'", "ein", "B\u00fcch\u00b7lein", "aus\u00b7zu\u00b7he\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.50": {"text": "Das man in der n\u00e4chsten nicht mehr kennt?", "tokens": ["Das", "man", "in", "der", "n\u00e4chs\u00b7ten", "nicht", "mehr", "kennt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "APPR", "ART", "ADJA", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.51": {"text": "Sieh! die\u00df alles, was ich ohne kalten", "tokens": ["Sieh", "!", "die\u00df", "al\u00b7les", ",", "was", "ich", "oh\u00b7ne", "kal\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PDS", "PIS", "$,", "PWS", "PPER", "APPR", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.52": {"text": "Schauer, kaum einmal recht denken kann,", "tokens": ["Schau\u00b7er", ",", "kaum", "ein\u00b7mal", "recht", "den\u00b7ken", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADV", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "+-++-+--+", "measure": "iambic.penta.chol"}, "line.53": {"text": "M\u00fc\u00dft' ich thun, dich l\u00e4nger zu behalten,", "tokens": ["M\u00fc\u00dft'", "ich", "thun", ",", "dich", "l\u00e4n\u00b7ger", "zu", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.54": {"text": "Darum fasse dich, und sey ein Mann!", "tokens": ["Da\u00b7rum", "fas\u00b7se", "dich", ",", "und", "sey", "ein", "Mann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KON", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.55": {"text": "Wolltest du nicht oft von mir sonst wissen,", "tokens": ["Woll\u00b7test", "du", "nicht", "oft", "von", "mir", "sonst", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.56": {"text": "Was man Weisheit nenne? H\u00f6re mich!", "tokens": ["Was", "man", "Weis\u00b7heit", "nen\u00b7ne", "?", "H\u00f6\u00b7re", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PIS", "NN", "VVFIN", "$.", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.57": {"text": "Hast du nichts bei mir gelernt, so lerne", "tokens": ["Hast", "du", "nichts", "bei", "mir", "ge\u00b7lernt", ",", "so", "ler\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "PIS", "APPR", "PPER", "VVPP", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.58": {"text": "Wenigstens die\u00df Eine noch von mir;", "tokens": ["We\u00b7nigs\u00b7tens", "die\u00df", "Ei\u00b7ne", "noch", "von", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "ART", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.59": {"text": "O! Zufriedenheit folgt in die Ferne", "tokens": ["O", "!", "Zu\u00b7frie\u00b7den\u00b7heit", "folgt", "in", "die", "Fer\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.60": {"text": "Dann gewi\u00df auf jedem Schritte dir.", "tokens": ["Dann", "ge\u00b7wi\u00df", "auf", "je\u00b7dem", "Schrit\u00b7te", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PIAT", "NN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.61": {"text": "Komm' nur morgen fr\u00fch herauf, und siehe,", "tokens": ["Komm'", "nur", "mor\u00b7gen", "fr\u00fch", "her\u00b7auf", ",", "und", "sie\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "ADJD", "PTKVZ", "$,", "KON", "VVIMP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.62": {"text": "Ob ich mich nicht hurtiger, als du,", "tokens": ["Ob", "ich", "mich", "nicht", "hur\u00b7ti\u00b7ger", ",", "als", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "ADJD", "$,", "KOUS", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.63": {"text": "Ohne Murren ob der kleinen M\u00fche,", "tokens": ["Oh\u00b7ne", "Mur\u00b7ren", "ob", "der", "klei\u00b7nen", "M\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.64": {"text": "Kleiden will, vom Kopf' bis auf die Schuh'.", "tokens": ["Klei\u00b7den", "will", ",", "vom", "Kopf'", "bis", "auf", "die", "Schuh'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "APPRART", "NN", "APPR", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.65": {"text": "Der du dich f\u00fcr mich des Schlafes gerne,", "tokens": ["Der", "du", "dich", "f\u00fcr", "mich", "des", "Schla\u00b7fes", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "APPR", "PPER", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.66": {"text": "Wie so s\u00fc\u00df der dein' auch ist, entschlugst,", "tokens": ["Wie", "so", "s\u00fc\u00df", "der", "dein'", "auch", "ist", ",", "ent\u00b7schlugst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "ART", "PPOSAT", "ADV", "VAFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.67": {"text": "Und im hohen Schnee die Blendlaterne", "tokens": ["Und", "im", "ho\u00b7hen", "Schnee", "die", "Blend\u00b7la\u00b7ter\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.68": {"text": "Vor mir her, so rasch und willig trugst,", "tokens": ["Vor", "mir", "her", ",", "so", "rasch", "und", "wil\u00b7lig", "trugst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$,", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.69": {"text": "Als ich die, die ich nun ganz besitze,", "tokens": ["Als", "ich", "die", ",", "die", "ich", "nun", "ganz", "be\u00b7sit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.70": {"text": "Nur zu sehen, keine Nacht mehr schlief,", "tokens": ["Nur", "zu", "se\u00b7hen", ",", "kei\u00b7ne", "Nacht", "mehr", "schlief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.71": {"text": "Und durch Fl\u00fcss' und Wald, in Frost und Hitze,", "tokens": ["Und", "durch", "Fl\u00fcss'", "und", "Wald", ",", "in", "Frost", "und", "Hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.72": {"text": "Oft mit dir in dunkeln N\u00e4chten lief:", "tokens": ["Oft", "mit", "dir", "in", "dun\u00b7keln", "N\u00e4ch\u00b7ten", "lief", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.73": {"text": "O du m\u00fcssest, w\u00e4r' er noch so selten,", "tokens": ["O", "du", "m\u00fcs\u00b7sest", ",", "w\u00e4r'", "er", "noch", "so", "sel\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VMFIN", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.74": {"text": "Doch den Herrn bald finden, der fortan", "tokens": ["Doch", "den", "Herrn", "bald", "fin\u00b7den", ",", "der", "for\u00b7tan"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "VVINF", "$,", "PRELS", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.75": {"text": "Freund, wie ich, dir sey, und das vergelten,", "tokens": ["Freund", ",", "wie", "ich", ",", "dir", "sey", ",", "und", "das", "ver\u00b7gel\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "$,", "PPER", "VAFIN", "$,", "KON", "PDS", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.76": {"text": "Was ich, leider! nur verdanken kann!", "tokens": ["Was", "ich", ",", "lei\u00b7der", "!", "nur", "ver\u00b7dan\u00b7ken", "kann", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "$,", "ADV", "$.", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}