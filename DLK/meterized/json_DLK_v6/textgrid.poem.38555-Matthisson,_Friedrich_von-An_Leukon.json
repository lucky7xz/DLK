{"textgrid.poem.38555": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Leukon", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trink des Stromes der Freude! sieh! noch fluthet", "tokens": ["Trink", "des", "Stro\u00b7mes", "der", "Freu\u00b7de", "!", "sieh", "!", "noch", "flut\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN", "$.", "VVIMP", "$.", "ADV", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Seiner sch\u00e4umenden Wogen ganze F\u00fclle", "tokens": ["Sei\u00b7ner", "sch\u00e4u\u00b7men\u00b7den", "Wo\u00b7gen", "gan\u00b7ze", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Durch die rosenbekr\u00e4nzten Thale deiner", "tokens": ["Durch", "die", "ro\u00b7sen\u00b7be\u00b7kr\u00e4nz\u00b7ten", "Tha\u00b7le", "dei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Bl\u00fchenden Jugend!", "tokens": ["Bl\u00fc\u00b7hen\u00b7den", "Ju\u00b7gend", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "St\u00fcrme werden sein lichtes Silber wandeln,", "tokens": ["St\u00fcr\u00b7me", "wer\u00b7den", "sein", "lich\u00b7tes", "Sil\u00b7ber", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Dornen auf dem bebl\u00fcmten Ufer wildern:", "tokens": ["Dor\u00b7nen", "auf", "dem", "be\u00b7bl\u00fcm\u00b7ten", "U\u00b7fer", "wil\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Da\u00df des Sch\u00f6pfenden Blut die aufgew\u00fchlten", "tokens": ["Da\u00df", "des", "Sch\u00f6p\u00b7fen\u00b7den", "Blut", "die", "auf\u00b7ge\u00b7w\u00fchl\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "ART", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wellen bepurpurt!", "tokens": ["Wel\u00b7len", "be\u00b7pur\u00b7purt", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}