{"textgrid.poem.49339": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie gro\u00df ist dieser Freudentag,", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie gro\u00df ist dieser Freudentag,", "tokens": ["Wie", "gro\u00df", "ist", "die\u00b7ser", "Freu\u00b7den\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Daran man sich versamlen mag,", "tokens": ["Da\u00b7ran", "man", "sich", "ver\u00b7sam\u00b7len", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu loben unsern Gott allein,", "tokens": ["Zu", "lo\u00b7ben", "un\u00b7sern", "Gott", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der itz sein Volk l\u00e4\u00dft fr\u00f6lich sein!", "tokens": ["Der", "itz", "sein", "Volk", "l\u00e4\u00dft", "fr\u00f6\u00b7lich", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer ist, der dieses nicht bedenkt,", "tokens": ["Wer", "ist", ",", "der", "die\u00b7ses", "nicht", "be\u00b7denkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PDS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df Gott uns seinen Sohn geschenkt,", "tokens": ["Da\u00df", "Gott", "uns", "sei\u00b7nen", "Sohn", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Uns, die wir sa\u00dfen in Gefahr,", "tokens": ["Uns", ",", "die", "wir", "sa\u00b7\u00dfen", "in", "Ge\u00b7fahr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verdamt zu bleiben immerdar?", "tokens": ["Ver\u00b7damt", "zu", "blei\u00b7ben", "im\u00b7mer\u00b7dar", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VVINF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Engel macht uns alle gleich", "tokens": ["Der", "En\u00b7gel", "macht", "uns", "al\u00b7le", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Durch seine Botschaft freudenreich,", "tokens": ["Durch", "sei\u00b7ne", "Bot\u00b7schaft", "freu\u00b7den\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Weil gro\u00dfe Freud' in dieser Frist", "tokens": ["Weil", "gro\u00b7\u00dfe", "Freud'", "in", "die\u00b7ser", "Frist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Uns allen widerfahren ist.", "tokens": ["Uns", "al\u00b7len", "wi\u00b7der\u00b7fah\u00b7ren", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Gott rufet jetzt in seinen Sal", "tokens": ["Gott", "ru\u00b7fet", "jetzt", "in", "sei\u00b7nen", "Sal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Die Menschenkinder allzumal;", "tokens": ["Die", "Men\u00b7schen\u00b7kin\u00b7der", "all\u00b7zu\u00b7mal", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Denn er ist auch der Heiden Licht,", "tokens": ["Denn", "er", "ist", "auch", "der", "Hei\u00b7den", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Kein Volk bleibt ausgeschlossen nicht.", "tokens": ["Kein", "Volk", "bleibt", "aus\u00b7ge\u00b7schlos\u00b7sen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "VVIZU", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Seid froh, ihr Herren und ihr Knecht',", "tokens": ["Seid", "froh", ",", "ihr", "Her\u00b7ren", "und", "ihr", "Knecht'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ihr werdet heilig und gerecht", "tokens": ["Ihr", "wer\u00b7det", "hei\u00b7lig", "und", "ge\u00b7recht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Durch dieses Kindleins Lieb' und Flei\u00df,", "tokens": ["Durch", "die\u00b7ses", "Kin\u00b7dleins", "Lieb'", "und", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Das gar von keiner S\u00fcnde wei\u00df.", "tokens": ["Das", "gar", "von", "kei\u00b7ner", "S\u00fcn\u00b7de", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Ihr Reich' und Arm', euch sei bewust", "tokens": ["Ihr", "Reich'", "und", "Arm'", ",", "euch", "sei", "be\u00b7wust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,", "PPER", "VAFIN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Die wunders\u00fc\u00dfe Weihnachtslust,", "tokens": ["Die", "wun\u00b7der\u00b7s\u00fc\u00b7\u00dfe", "Weih\u00b7nachts\u00b7lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Empfanget itz mit frischem Mut", "tokens": ["Emp\u00b7fan\u00b7get", "itz", "mit", "fri\u00b7schem", "Mut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Eur Jesulein, das h\u00f6chste Gut.", "tokens": ["Eur", "Je\u00b7su\u00b7lein", ",", "das", "h\u00f6chs\u00b7te", "Gut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Die\u00df Freudenfest geht mich auch an,", "tokens": ["Die\u00df", "Freu\u00b7den\u00b7fest", "geht", "mich", "auch", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "So, da\u00df ich k\u00fchnlich r\u00fchmen kan:", "tokens": ["So", ",", "da\u00df", "ich", "k\u00fchn\u00b7lich", "r\u00fch\u00b7men", "kan", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Geboren ist die\u00df Kindelein", "tokens": ["Ge\u00b7bo\u00b7ren", "ist", "die\u00df", "Kin\u00b7de\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PDS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Auch mir, wie k\u00f6nt' ich traurig sein?", "tokens": ["Auch", "mir", ",", "wie", "k\u00f6nt'", "ich", "trau\u00b7rig", "sein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PWAV", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Die\u00df Kindlein ist erzeuget zwar", "tokens": ["Die\u00df", "Kin\u00b7dlein", "ist", "er\u00b7zeu\u00b7get", "zwar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "VAFIN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Von Ewigkeit, jedoch gebar", "tokens": ["Von", "E\u00b7wig\u00b7keit", ",", "je\u00b7doch", "ge\u00b7bar"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Maria solches auch zur Zeit", "tokens": ["Ma\u00b7ria", "sol\u00b7ches", "auch", "zur", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PIAT", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Der neuen R\u00f6mer-Obrigkeit.", "tokens": ["Der", "neu\u00b7en", "R\u00f6\u00b7mer\u00b7O\u00b7brig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.33": {"text": "Die\u00df ist das Kind, das mehr vermag", "tokens": ["Die\u00df", "ist", "das", "Kind", ",", "das", "mehr", "ver\u00b7mag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Als alles, auch noch alle Tag", "tokens": ["Als", "al\u00b7les", ",", "auch", "noch", "al\u00b7le", "Tag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "$,", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Geboren wird an manchem Ort", "tokens": ["Ge\u00b7bo\u00b7ren", "wird", "an", "man\u00b7chem", "Ort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "In uns durchs Sacrament und Wort.", "tokens": ["In", "uns", "durchs", "Sa\u00b7cra\u00b7ment", "und", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Was jene Hirten dort gesehn,", "tokens": ["Was", "je\u00b7ne", "Hir\u00b7ten", "dort", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Das kan noch t\u00e4glich uns geschehn:", "tokens": ["Das", "kan", "noch", "t\u00e4g\u00b7lich", "uns", "ge\u00b7schehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Das Kind wird auch geboren heut,", "tokens": ["Das", "Kind", "wird", "auch", "ge\u00b7bo\u00b7ren", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Im Fall man seiner sich erfreut.", "tokens": ["Im", "Fall", "man", "sei\u00b7ner", "sich", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "PPOSAT", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Heut ist es zwar in seinem Reich'", "tokens": ["Heut", "ist", "es", "zwar", "in", "sei\u00b7nem", "Reich'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Ein K\u00f6nig, dem kein ander gleich,", "tokens": ["Ein", "K\u00f6\u00b7nig", ",", "dem", "kein", "an\u00b7der", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Und dennoch bleibt sein treuer Sinn", "tokens": ["Und", "den\u00b7noch", "bleibt", "sein", "treu\u00b7er", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "So freundlich, als er war vorhin.", "tokens": ["So", "freund\u00b7lich", ",", "als", "er", "war", "vor\u00b7hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Er gibt uns heut' auch gar sein Herz;", "tokens": ["Er", "gibt", "uns", "heut'", "auch", "gar", "sein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Ja, wenn uns Tr\u00fcbsal, Angst und Schmerz", "tokens": ["Ja", ",", "wenn", "uns", "Tr\u00fcb\u00b7sal", ",", "Angst", "und", "Schmerz"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Betr\u00fcben oft bis in den Tod,", "tokens": ["Be\u00b7tr\u00fc\u00b7ben", "oft", "bis", "in", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "So hilft er uns aus aller Not.", "tokens": ["So", "hilft", "er", "uns", "aus", "al\u00b7ler", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Ei, la\u00dft uns diesem Jesulein", "tokens": ["Ei", ",", "la\u00dft", "uns", "die\u00b7sem", "Je\u00b7su\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Auch heute ganz ergeben sein,", "tokens": ["Auch", "heu\u00b7te", "ganz", "er\u00b7ge\u00b7ben", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Da\u00df er uns wiedrum Gutes thu,", "tokens": ["Da\u00df", "er", "uns", "wie\u00b7drum", "Gu\u00b7tes", "thu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Ja, stets in unser Seelen ruh'.", "tokens": ["Ja", ",", "stets", "in", "un\u00b7ser", "See\u00b7len", "ruh'", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "O Freud' und Lust zu dieser Frist,", "tokens": ["O", "Freud'", "und", "Lust", "zu", "die\u00b7ser", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Darin der Heiland Jesus Christ,", "tokens": ["Da\u00b7rin", "der", "Hei\u00b7land", "Je\u00b7sus", "Christ", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Der hochverlangte Wunderheld,", "tokens": ["Der", "hoch\u00b7ver\u00b7lang\u00b7te", "Wun\u00b7der\u00b7held", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Geboren ist ein Mensch zur Welt!", "tokens": ["Ge\u00b7bo\u00b7ren", "ist", "ein", "Mensch", "zur", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Ach Gott, wie gro\u00df war die Gefahr,", "tokens": ["Ach", "Gott", ",", "wie", "gro\u00df", "war", "die", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Als uns der Satan ganz und gar", "tokens": ["Als", "uns", "der", "Sa\u00b7tan", "ganz", "und", "gar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "Verstricket hielt in seinem Reich", "tokens": ["Ver\u00b7stri\u00b7cket", "hielt", "in", "sei\u00b7nem", "Reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.60": {"text": "Und plagt' uns grausam alle gleich!", "tokens": ["Und", "plagt'", "uns", "grau\u00b7sam", "al\u00b7le", "gleich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.61": {"text": "Bald aber wie die\u00df Kind ankam", "tokens": ["Bald", "a\u00b7ber", "wie", "die\u00df", "Kind", "an\u00b7kam"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOKOM", "PDS", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.62": {"text": "Und unser Not zu Herzen nahm,", "tokens": ["Und", "un\u00b7ser", "Not", "zu", "Her\u00b7zen", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.63": {"text": "Da wurden aus des Teufels Macht", "tokens": ["Da", "wur\u00b7den", "aus", "d