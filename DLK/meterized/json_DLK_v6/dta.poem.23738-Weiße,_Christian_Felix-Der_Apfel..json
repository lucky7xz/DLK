{"dta.poem.23738": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Apfel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Als j\u00fcngst H\u00e4nschen in dem Gras", "tokens": ["Als", "j\u00fcngst", "H\u00e4n\u00b7schen", "in", "dem", "Gras"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich ein Blumenstr\u00e4uschen las,", "tokens": ["Sich", "ein", "Blu\u00b7men\u00b7str\u00e4u\u00b7schen", "las", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fand er, welch Vergn\u00fcgen!", "tokens": ["Fand", "er", ",", "welch", "Ver\u00b7gn\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Einen Apfel liegen.", "tokens": ["Ei\u00b7nen", "Ap\u00b7fel", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "H\u00e4nschen h\u00fcpfte froh daher;", "tokens": ["H\u00e4n\u00b7schen", "h\u00fcpf\u00b7te", "froh", "da\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u201eey wie wundersch\u00f6n ist er!\u201e", "tokens": ["\u201e", "ey", "wie", "wun\u00b7der\u00b7sch\u00f6n", "ist", "er", "!", "\u201e"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "KOKOM", "ADJD", "VAFIN", "PPER", "$.", "$("], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Sprach er; meinem Magen", "tokens": ["Sprach", "er", ";", "mei\u00b7nem", "Ma\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "$.", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Soll er wohl behagen.", "tokens": ["Soll", "er", "wohl", "be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Voll Begierde bi\u00df er zu &#8212;", "tokens": ["Voll", "Be\u00b7gier\u00b7de", "bi\u00df", "er", "zu", "&#8212;"], "token_info": ["word", "word", "word", "word", "word", "XML_entity"], "pos": ["ADJD", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4nschen, o was sprudelst du?", "tokens": ["H\u00e4n\u00b7schen", ",", "o", "was", "spru\u00b7delst", "du", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "PWS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will dem kleinen Gecken", "tokens": ["Will", "dem", "klei\u00b7nen", "Ge\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nicht der Apfel schmecken?", "tokens": ["Nicht", "der", "Ap\u00b7fel", "schme\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "O sprach er: \u201dder Wurm ist drinn,\u201d", "tokens": ["O", "sprach", "er", ":", "\"", "der", "Wurm", "ist", "drinn", ",", "\""], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "$(", "ART", "NN", "VAFIN", "ADV", "$,", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und warf ihn entr\u00fcstet hin:", "tokens": ["Und", "warf", "ihn", "ent\u00b7r\u00fcs\u00b7tet", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u201eeine sch\u00f6ne L\u00fcgen", "tokens": ["\u201e", "ei\u00b7ne", "sch\u00f6\u00b7ne", "L\u00fc\u00b7gen"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "La\u00df ich mich betr\u00fcgen!\u201e", "tokens": ["La\u00df", "ich", "mich", "be\u00b7tr\u00fc\u00b7gen", "!", "\u201e"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "PRF", "VVINF", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}