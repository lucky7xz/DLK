{"textgrid.poem.66073": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Es schneit", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der erste Schnee, weich und dicht,", "tokens": ["Der", "ers\u00b7te", "Schnee", ",", "weich", "und", "dicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Die ersten wirbelnden Flocken.", "tokens": ["Die", "ers\u00b7ten", "wir\u00b7beln\u00b7den", "Flo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Kinder dr\u00e4ngen ihr Gesicht", "tokens": ["Die", "Kin\u00b7der", "dr\u00e4n\u00b7gen", "ihr", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ans Fenster und frohlocken.", "tokens": ["Ans", "Fens\u00b7ter", "und", "froh\u00b7lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da wird nun das letzte bischen Gr\u00fcn", "tokens": ["Da", "wird", "nun", "das", "letz\u00b7te", "bi\u00b7schen", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ADJA", "ADV", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Leise, leise begraben.", "tokens": ["Lei\u00b7se", ",", "lei\u00b7se", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Aber die jungen Wangen gl\u00fchn,", "tokens": ["A\u00b7ber", "die", "jun\u00b7gen", "Wan\u00b7gen", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sie wollen den Winter haben.", "tokens": ["Sie", "wol\u00b7len", "den", "Win\u00b7ter", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Schlittenfahrt und Schellenklang", "tokens": ["Schlit\u00b7ten\u00b7fahrt", "und", "Schel\u00b7len\u00b7klang"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und Schneeb\u00e4lle um die Ohren!", "tokens": ["Und", "Schnee\u00b7b\u00e4l\u00b7le", "um", "die", "Oh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u2013 Kindergl\u00fcck, wo bist du? Lang,", "tokens": ["\u2013", "Kin\u00b7der\u00b7gl\u00fcck", ",", "wo", "bist", "du", "?", "Lang", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "NN", "$,", "PWAV", "VAFIN", "PPER", "$.", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lang verschneit und erfroren.", "tokens": ["Lang", "ver\u00b7schneit", "und", "er\u00b7fro\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Fallen die Flocken weich und dicht,", "tokens": ["Fal\u00b7len", "die", "Flo\u00b7cken", "weich", "und", "dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Stehen wir wohl erschrocken,", "tokens": ["Ste\u00b7hen", "wir", "wohl", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Aber die Kleinen begreifens nicht,", "tokens": ["A\u00b7ber", "die", "Klei\u00b7nen", "be\u00b7grei\u00b7fens", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Gl\u00e4nzen vor Gl\u00fcck und frohlocken.", "tokens": ["Gl\u00e4n\u00b7zen", "vor", "Gl\u00fcck", "und", "froh\u00b7lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}