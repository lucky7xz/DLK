{"textgrid.poem.66743": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als junger Fant", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als junger Fant", "tokens": ["Als", "jun\u00b7ger", "Fant"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Bin ich hinausgezogen,", "tokens": ["Bin", "ich", "hin\u00b7aus\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das neue Land", "tokens": ["Das", "neu\u00b7e", "Land"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Sucht' ich auf hohen Wogen;", "tokens": ["Sucht'", "ich", "auf", "ho\u00b7hen", "Wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was t\u00e4uschte, schwand ...", "tokens": ["Was", "t\u00e4uschte", ",", "schwand", "..."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VVFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Mein Traum ward nicht betrogen:", "tokens": ["Mein", "Traum", "ward", "nicht", "be\u00b7tro\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ob manches Eiland nebelhaft verblich,", "tokens": ["Ob", "man\u00b7ches", "Ei\u00b7land", "ne\u00b7bel\u00b7haft", "ver\u00b7blich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Eins ward Gestalt, und eines segnet mich.", "tokens": ["Eins", "ward", "Ge\u00b7stalt", ",", "und", "ei\u00b7nes", "seg\u00b7net", "mich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,", "KON", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der St\u00fcrme Wut", "tokens": ["Der", "St\u00fcr\u00b7me", "Wut"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Sah ich vor\u00fcberschreiten,", "tokens": ["Sah", "ich", "vor\u00b7\u00fc\u00b7bersc\u00b7hrei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Haie Brut", "tokens": ["Der", "Hai\u00b7e", "Brut"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Den hohlen Rachen spreiten ...", "tokens": ["Den", "hoh\u00b7len", "Ra\u00b7chen", "sprei\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nun teilt die Flut", "tokens": ["Nun", "teilt", "die", "Flut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Sich fromm zu beiden Seiten,", "tokens": ["Sich", "fromm", "zu", "bei\u00b7den", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und aus dem Urscho\u00df reiner Sehnsucht bl\u00fcht", "tokens": ["Und", "aus", "dem", "Ur\u00b7scho\u00df", "rei\u00b7ner", "Sehn\u00b7sucht", "bl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Erf\u00fcllung mir, eh noch mein Tag vergl\u00fcht.", "tokens": ["Er\u00b7f\u00fcl\u00b7lung", "mir", ",", "eh", "noch", "mein", "Tag", "ver\u00b7gl\u00fcht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein voll Gel\u00e4ut", "tokens": ["Ein", "voll", "Ge\u00b7l\u00e4ut"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Erzittert von den H\u00f6hen,", "tokens": ["Er\u00b7zit\u00b7tert", "von", "den", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Leben reut", "tokens": ["Das", "Le\u00b7ben", "reut"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Mich nicht mit Kampf und Wehen ...", "tokens": ["Mich", "nicht", "mit", "Kampf", "und", "We\u00b7hen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Gott gebeut,", "tokens": ["Mein", "Gott", "ge\u00b7beut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die sch\u00f6ne Welt zu sehen,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Welt", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die wundersam erst aus der Tiefe taucht,", "tokens": ["Die", "wun\u00b7der\u00b7sam", "erst", "aus", "der", "Tie\u00b7fe", "taucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.8": {"text": "Wenn der Vulkan der wilden Kraft verraucht.", "tokens": ["Wenn", "der", "Vul\u00b7kan", "der", "wil\u00b7den", "Kraft", "ver\u00b7raucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}