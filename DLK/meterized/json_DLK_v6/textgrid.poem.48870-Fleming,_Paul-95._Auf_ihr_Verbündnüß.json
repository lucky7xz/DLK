{"textgrid.poem.48870": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "95. Auf ihr Verb\u00fcndn\u00fc\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Schatten, die ihr nur alleine bei uns seid,", "tokens": ["Ihr", "Schat\u00b7ten", ",", "die", "ihr", "nur", "al\u00b7lei\u00b7ne", "bei", "uns", "seid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und du auch stille Luft, die unsern Odem reget,", "tokens": ["und", "du", "auch", "stil\u00b7le", "Luft", ",", "die", "un\u00b7sern", "O\u00b7dem", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "seid Zeugen zwischen uns! Der Eid ist abgeleget,", "tokens": ["seid", "Zeu\u00b7gen", "zwi\u00b7schen", "uns", "!", "Der", "Eid", "ist", "ab\u00b7ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PPER", "$.", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der Eid, der mir und ihr sol nimmermehr sein leid, -", "tokens": ["der", "Eid", ",", "der", "mir", "und", "ihr", "sol", "nim\u00b7mer\u00b7mehr", "sein", "leid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "KON", "PPER", "VMFIN", "ADV", "PPOSAT", "ADJD", "$,", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "di\u00df ist mein und ihr Schlu\u00df: es berste List und Neid, -", "tokens": ["di\u00df", "ist", "mein", "und", "ihr", "Schlu\u00df", ":", "es", "bers\u00b7te", "List", "und", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "KON", "PPOSAT", "NN", "$.", "PPER", "ADJA", "NN", "KON", "NN", "$,", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "so lang' ein Pusch sein Laub, die Erde Kr\u00e4uter tr\u00e4get", "tokens": ["so", "lang'", "ein", "Pusch", "sein", "Laub", ",", "die", "Er\u00b7de", "Kr\u00e4u\u00b7ter", "tr\u00e4\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und ein belebter Geist sich in der Flut beweget.", "tokens": ["und", "ein", "be\u00b7leb\u00b7ter", "Geist", "sich", "in", "der", "Flut", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Di\u00df soll sein Ende sein, wenn mehr ist keine Zeit.", "tokens": ["Di\u00df", "soll", "sein", "En\u00b7de", "sein", ",", "wenn", "mehr", "ist", "kei\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "VAINF", "$,", "KOUS", "PIS", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So treue ", "tokens": ["So", "treu\u00b7e"], "token_info": ["word", "word"], "pos": ["ADV", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "wenn solche Freundschaft macht ein Herze mit dem Herzen.", "tokens": ["wenn", "sol\u00b7che", "Freund\u00b7schaft", "macht", "ein", "Her\u00b7ze", "mit", "dem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "ART", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es m\u00f6gen Andre nun von ihrer Liebe Pein,", "tokens": ["Es", "m\u00f6\u00b7gen", "And\u00b7re", "nun", "von", "ih\u00b7rer", "Lie\u00b7be", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "von Angst, von Grausamkeit, von dem und jenem klagen;", "tokens": ["von", "Angst", ",", "von", "Grau\u00b7sam\u00b7keit", ",", "von", "dem", "und", "je\u00b7nem", "kla\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "ART", "KON", "PDAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "zwei Herzen, das sind wir, die k\u00f6nnen redlich sagen,", "tokens": ["zwei", "Her\u00b7zen", ",", "das", "sind", "wir", ",", "die", "k\u00f6n\u00b7nen", "red\u00b7lich", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PDS", "VAFIN", "PPER", "$,", "PRELS", "VMINF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df von der Liebe sie noch nie betr\u00fcbet sein.", "tokens": ["da\u00df", "von", "der", "Lie\u00b7be", "sie", "noch", "nie", "be\u00b7tr\u00fc\u00b7bet", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "ADV", "ADV", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}