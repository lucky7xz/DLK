{"textgrid.poem.66505": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: In der hellen Sonntagsfr\u00fche,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der hellen Sonntagsfr\u00fche,", "tokens": ["In", "der", "hel\u00b7len", "Sonn\u00b7tags\u00b7fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mir fl\u00fcgelt Herz und Fu\u00df,", "tokens": ["Die", "mir", "fl\u00fc\u00b7gelt", "Herz", "und", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bl\u00fch' im Herzen mir, erbl\u00fche,", "tokens": ["Bl\u00fch'", "im", "Her\u00b7zen", "mir", ",", "er\u00b7bl\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sangeslust zum Liedesgru\u00df!", "tokens": ["San\u00b7ges\u00b7lust", "zum", "Lie\u00b7des\u00b7gru\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie vom Thal die Glocken klingen", "tokens": ["Wie", "vom", "Thal", "die", "Glo\u00b7cken", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus dem goldnen Nebelduft,", "tokens": ["Aus", "dem", "gold\u00b7nen", "Ne\u00b7bel\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll mein Sang sich aufw\u00e4rts schwingen", "tokens": ["Soll", "mein", "Sang", "sich", "auf\u00b7w\u00e4rts", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "PRF", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch die reine Morgenluft.", "tokens": ["Durch", "die", "rei\u00b7ne", "Mor\u00b7gen\u00b7luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und als ob mir Antwort riefe", "tokens": ["Und", "als", "ob", "mir", "Ant\u00b7wort", "rie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOKOM", "KOUS", "PPER", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dort der Firnen Silberschnee,", "tokens": ["Dort", "der", "Fir\u00b7nen", "Sil\u00b7ber\u00b7schnee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort der blauen Himmelstiefe", "tokens": ["Dort", "der", "blau\u00b7en", "Him\u00b7mel\u00b7stie\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Feuchtes Wiederspiel im See:", "tokens": ["Feuch\u00b7tes", "Wie\u00b7der\u00b7spiel", "im", "See", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6r' ich Worte wiedert\u00f6nen,", "tokens": ["H\u00f6r'", "ich", "Wor\u00b7te", "wie\u00b7der\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Holde Namen im Gesang,", "tokens": ["Hol\u00b7de", "Na\u00b7men", "im", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die mit seligem Gew\u00f6hnen", "tokens": ["Die", "mit", "se\u00b7li\u00b7gem", "Ge\u00b7w\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ich zu Lied und Reimen schlang.", "tokens": ["Ich", "zu", "Lied", "und", "Rei\u00b7men", "schlang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ruft dem Wiederhall der Ferne", "tokens": ["Ruft", "dem", "Wie\u00b7der\u00b7hall", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heut der Wandrer seinen Reim,", "tokens": ["Heut", "der", "Wand\u00b7rer", "sei\u00b7nen", "Reim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, in dieser Fr\u00fch' wie gerne", "tokens": ["Ach", ",", "in", "die\u00b7ser", "Fr\u00fch'", "wie", "ger\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PDAT", "NN", "KOKOM", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4r' ich auch bei euch daheim!", "tokens": ["W\u00e4r'", "ich", "auch", "bei", "euch", "da\u00b7heim", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Herz und Augen zu entz\u00fccken", "tokens": ["Herz", "und", "Au\u00b7gen", "zu", "ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In geliebtem Zauberkreis,", "tokens": ["In", "ge\u00b7lieb\u00b7tem", "Zau\u00b7ber\u00b7kreis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der das Sch\u00f6nste vor den Blicken", "tokens": ["Der", "das", "Sch\u00f6ns\u00b7te", "vor", "den", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Noch zu \u00fcberstrahlen wei\u00df!", "tokens": ["Noch", "zu", "\u00fc\u00b7bers\u00b7trah\u00b7len", "wei\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Alle Musen zum Geleite", "tokens": ["Al\u00b7le", "Mu\u00b7sen", "zum", "Ge\u00b7lei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gabt ihr mir auf meine Fahrt,", "tokens": ["Gabt", "ihr", "mir", "auf", "mei\u00b7ne", "Fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so bleibt auch in der Weite", "tokens": ["Und", "so", "bleibt", "auch", "in", "der", "Wei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herz und Sinn mir wohlbewahrt.", "tokens": ["Herz", "und", "Sinn", "mir", "wohl\u00b7be\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Will zu lang der Weg mir werden,", "tokens": ["Will", "zu", "lang", "der", "Weg", "mir", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKA", "ADJD", "ART", "NN", "PPER", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wei\u00df ich treu mir zugesellt", "tokens": ["Wei\u00df", "ich", "treu", "mir", "zu\u00b7ge\u00b7sellt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Meinen Himmel schon auf Erden", "tokens": ["Mei\u00b7nen", "Him\u00b7mel", "schon", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und im Herzen meine Welt!", "tokens": ["Und", "im", "Her\u00b7zen", "mei\u00b7ne", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}