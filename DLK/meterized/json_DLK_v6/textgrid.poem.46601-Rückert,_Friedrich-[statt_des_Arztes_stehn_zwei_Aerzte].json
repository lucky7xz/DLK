{"textgrid.poem.46601": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[statt des Arztes stehn zwei Aerzte]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Statt des Arztes stehn zwei Aerzte", "tokens": ["Statt", "des", "Arz\u00b7tes", "stehn", "zwei", "A\u00b7erz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN", "CARD", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Nun am Bett, und jeder schaut,", "tokens": ["Nun", "am", "Bett", ",", "und", "je\u00b7der", "schaut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "KON", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob des Lebens Tag sich schw\u00e4rzte,", "tokens": ["Ob", "des", "Le\u00b7bens", "Tag", "sich", "schw\u00e4rz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder Hoffnungsmorgen graut.", "tokens": ["O\u00b7der", "Hoff\u00b7nungs\u00b7mor\u00b7gen", "graut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch da sind so viele Zeichen,", "tokens": ["Doch", "da", "sind", "so", "vie\u00b7le", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Die einander heben auf;", "tokens": ["Die", "ein\u00b7an\u00b7der", "he\u00b7ben", "auf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und indessen sie vergleichen,", "tokens": ["Und", "in\u00b7des\u00b7sen", "sie", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geht das Schicksal seinen Lauf.", "tokens": ["Geht", "das", "Schick\u00b7sal", "sei\u00b7nen", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Besser w\u00e4re mir gerathen,", "tokens": ["Bes\u00b7ser", "w\u00e4\u00b7re", "mir", "ge\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ich h\u00e4tte diese Nacht", "tokens": ["Wenn", "ich", "h\u00e4t\u00b7te", "die\u00b7se", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Geschenk, das seinem Pathen", "tokens": ["Das", "Ge\u00b7schenk", ",", "das", "sei\u00b7nem", "Pa\u00b7then"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Gevatter Tod gemacht.", "tokens": ["Der", "Ge\u00b7vat\u00b7ter", "Tod", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Zum ber\u00fchmten Arzt erhoben", "tokens": ["Zum", "be\u00b7r\u00fchm\u00b7ten", "Arzt", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ward er ohne viel Verstand,", "tokens": ["Ward", "er", "oh\u00b7ne", "viel", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil er sah am Bett, ob oben", "tokens": ["Weil", "er", "sah", "am", "Bett", ",", "ob", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,", "KOUS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stand der Tod, ob unten stand.", "tokens": ["Stand", "der", "Tod", ",", "ob", "un\u00b7ten", "stand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Stand zu H\u00e4upten der Gevatter,", "tokens": ["Stand", "zu", "H\u00e4up\u00b7ten", "der", "Ge\u00b7vat\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und man rief den Doktor bei,", "tokens": ["Und", "man", "rief", "den", "Dok\u00b7tor", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sagt er ohne viel Geflatter,", "tokens": ["Sagt", "er", "oh\u00b7ne", "viel", "Ge\u00b7flat\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df kein Kraut gewachsen sei.", "tokens": ["Da\u00df", "kein", "Kraut", "ge\u00b7wach\u00b7sen", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Aber stand der Tod beim Fu\u00dfe,", "tokens": ["A\u00b7ber", "stand", "der", "Tod", "beim", "Fu\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gab der Doktor Arzenein,", "tokens": ["Gab", "der", "Dok\u00b7tor", "Ar\u00b7zen\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Tod mit leichtem Gru\u00dfe", "tokens": ["Und", "der", "Tod", "mit", "leich\u00b7tem", "Gru\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zog davon, als m\u00fc\u00dft' es sein.", "tokens": ["Zog", "da\u00b7von", ",", "als", "m\u00fc\u00dft'", "es", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "$,", "KOUS", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch einst stand der Tod beim Haupte,", "tokens": ["Doch", "einst", "stand", "der", "Tod", "beim", "Haup\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Als des Todes Pathe sich", "tokens": ["Als", "des", "To\u00b7des", "Pa\u00b7the", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen k\u00fchnen Streich erlaubte,", "tokens": ["Ei\u00b7nen", "k\u00fch\u00b7nen", "Streich", "er\u00b7laub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem der Tod gezwungen wich.", "tokens": ["Dem", "der", "Tod", "ge\u00b7zwun\u00b7gen", "wich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Um lie\u00df er die Bettstatt drehen,", "tokens": ["Um", "lie\u00df", "er", "die", "Bett\u00b7statt", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da\u00df der Tod zu F\u00fc\u00dfen kam,", "tokens": ["Da\u00df", "der", "Tod", "zu", "F\u00fc\u00b7\u00dfen", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von dannen mu\u00dfte gehen,", "tokens": ["Und", "von", "dan\u00b7nen", "mu\u00df\u00b7te", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne da\u00df er's Haupt hinnahm.", "tokens": ["Oh\u00b7ne", "da\u00df", "er's", "Haupt", "hin\u00b7nahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PIS", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und so h\u00e4tt' ich leicht das Bette", "tokens": ["Und", "so", "h\u00e4tt'", "ich", "leicht", "das", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meines Lieblings umgedreht,", "tokens": ["Mei\u00b7nes", "Lieb\u00b7lings", "um\u00b7ge\u00b7dreht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich die Gewi\u00dfheit h\u00e4tte,", "tokens": ["Wenn", "ich", "die", "Ge\u00b7wi\u00df\u00b7heit", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df der Tod zu H\u00e4upten steht.", "tokens": ["Da\u00df", "der", "Tod", "zu", "H\u00e4up\u00b7ten", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Doch er steht vielleicht zu F\u00fc\u00dfen,", "tokens": ["Doch", "er", "steht", "viel\u00b7leicht", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und so r\u00fcckt ich ihn an's Haupt.", "tokens": ["Und", "so", "r\u00fcckt", "ich", "ihn", "an's", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drum werd' ich es lassen m\u00fcssen", "tokens": ["Drum", "werd'", "ich", "es", "las\u00b7sen", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "PPER", "VVINF", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem, der ihm die Macht erlaubt.", "tokens": ["Dem", ",", "der", "ihm", "die", "Macht", "er\u00b7laubt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Mag der Tod hier stehn zu F\u00fc\u00dfen,", "tokens": ["Mag", "der", "Tod", "hier", "stehn", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder mag er stehn beim Haupt;", "tokens": ["O\u00b7der", "mag", "er", "stehn", "beim", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem werd' ich es lassen m\u00fcssen,", "tokens": ["Dem", "werd'", "ich", "es", "las\u00b7sen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PPER", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der dem Tod die Macht geraubt.", "tokens": ["Der", "dem", "Tod", "die", "Macht", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}