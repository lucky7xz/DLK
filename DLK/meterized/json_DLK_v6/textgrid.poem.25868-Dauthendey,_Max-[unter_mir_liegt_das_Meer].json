{"textgrid.poem.25868": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[unter mir liegt das Meer]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter mir liegt das Meer.", "tokens": ["Un\u00b7ter", "mir", "liegt", "das", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "\u00dcber der grauen, dunkelnden Fl\u00e4che", "tokens": ["\u00dc\u00b7ber", "der", "grau\u00b7en", ",", "dun\u00b7keln\u00b7den", "Fl\u00e4\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wiegt sich die helle, funkelnde M\u00f6we,", "tokens": ["Wiegt", "sich", "die", "hel\u00b7le", ",", "fun\u00b7keln\u00b7de", "M\u00f6\u00b7we", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "++-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wellen g\u00e4hnen,", "tokens": ["Wel\u00b7len", "g\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Jede Welle blitzt schieferdunkel,", "tokens": ["Je\u00b7de", "Wel\u00b7le", "blitzt", "schie\u00b7fer\u00b7dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Der Tod sitzt im Wasser,", "tokens": ["Der", "Tod", "sitzt", "im", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Klappt mit dem Kiefer,", "tokens": ["Klappt", "mit", "dem", "Kie\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Schnappt mit den Z\u00e4hnen.", "tokens": ["Schnappt", "mit", "den", "Z\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}