{"textgrid.poem.52129": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Eltern klaget nicht umb euer Liebes Kind,", "tokens": ["Ihr", "El\u00b7tern", "kla\u00b7get", "nicht", "umb", "eu\u00b7er", "Lie\u00b7bes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr wi\u00dft ja, da\u00df dem Herrn auch lieb die Kinder sind;", "tokens": ["Ihr", "wi\u00dft", "ja", ",", "da\u00df", "dem", "Herrn", "auch", "lieb", "die", "Kin\u00b7der", "sind", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "ART", "NN", "ADV", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob euer J\u00fcngster Sohn mit euch nicht spielt noch schertzt,", "tokens": ["Ob", "eu\u00b7er", "J\u00fcngs\u00b7ter", "Sohn", "mit", "euch", "nicht", "spielt", "noch", "schertzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "PTKNEG", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Seht, wie Ihn Christus selbst der Kinder Vater hertzt;", "tokens": ["Seht", ",", "wie", "Ihn", "Chris\u00b7tus", "selbst", "der", "Kin\u00b7der", "Va\u00b7ter", "hertzt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "NE", "ADV", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ob ihr das Werthe Pfand nicht auff den Armen tragt,", "tokens": ["Ob", "ihr", "das", "Wert\u00b7he", "Pfand", "nicht", "auff", "den", "Ar\u00b7men", "tragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Seht, wie es schwebt und gl\u00e4ntzt vor Gott, und ihm behagt:", "tokens": ["Seht", ",", "wie", "es", "schwebt", "und", "gl\u00e4ntzt", "vor", "Gott", ",", "und", "ihm", "be\u00b7hagt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$,", "KON", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ob Euern seelgen Schatz der Tod macht kalt und bleich,", "tokens": ["Ob", "Eu\u00b7ern", "seel\u00b7gen", "Schatz", "der", "Tod", "macht", "kalt", "und", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Seht ihn, ein Theil von euch (o Trost!) in Gottes Reich.", "tokens": ["Seht", "ihn", ",", "ein", "Theil", "von", "euch", "(", "o", "Trost", "!", ")", "in", "Got\u00b7tes", "Reich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "APPR", "PPER", "$(", "FM", "NN", "$.", "$(", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und darumb, Eltern, tragt nicht seinetwegen Pein,", "tokens": ["Und", "da\u00b7rumb", ",", "El\u00b7tern", ",", "tragt", "nicht", "sei\u00b7net\u00b7we\u00b7gen", "Pein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "$,", "NN", "$,", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denn Gott wil, da\u00df das Kind so fr\u00fch sol seelig seyn.", "tokens": ["Denn", "Gott", "wil", ",", "da\u00df", "das", "Kind", "so", "fr\u00fch", "sol", "see\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "$,", "KOUS", "ART", "NN", "ADV", "ADJD", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Rothe Ruhr f\u00fchrt es ja aus der argen Welt,", "tokens": ["Die", "Ro\u00b7the", "Ruhr", "f\u00fchrt", "es", "ja", "aus", "der", "ar\u00b7gen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Doch JESUS rothe Blut daf\u00fcr in Himmels Zelt.", "tokens": ["Doch", "Je\u00b7SUS", "ro\u00b7the", "Blut", "da\u00b7f\u00fcr", "in", "Him\u00b7mels", "Zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJA", "NN", "PAV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}