{"textgrid.poem.56443": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bist du nun tot? Da hebt die Brust sich noch,", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bist du nun tot? Da hebt die Brust sich noch,", "tokens": ["Bist", "du", "nun", "tot", "?", "Da", "hebt", "die", "Brust", "sich", "noch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$.", "ADV", "VVFIN", "ART", "NN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es war ein Schatten, der dar\u00fcber fegt,", "tokens": ["Es", "war", "ein", "Schat\u00b7ten", ",", "der", "da\u00b7r\u00fc\u00b7ber", "fegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der in der ungewissen D\u00e4mmrung kroch", "tokens": ["Der", "in", "der", "un\u00b7ge\u00b7wis\u00b7sen", "D\u00e4mm\u00b7rung", "kroch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom Vorhang, der im Nachtwind Falten schl\u00e4gt.", "tokens": ["Vom", "Vor\u00b7hang", ",", "der", "im", "Nacht\u00b7wind", "Fal\u00b7ten", "schl\u00e4gt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie ist dein Kehlkopf blau, draus \u00e4chzend fuhr", "tokens": ["Wie", "ist", "dein", "Kehl\u00b7kopf", "blau", ",", "draus", "\u00e4ch\u00b7zend", "fuhr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "ADJD", "$,", "PAV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dein leises St\u00f6hnen von der H\u00e4nde Druck.", "tokens": ["Dein", "lei\u00b7ses", "St\u00f6h\u00b7nen", "von", "der", "H\u00e4n\u00b7de", "Druck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das ist der W\u00fcrgemale tiefe Spur,", "tokens": ["Das", "ist", "der", "W\u00fcr\u00b7ge\u00b7ma\u00b7le", "tie\u00b7fe", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du nimmst ins Grab sie nun als letzten Schmuck.", "tokens": ["Du", "nimmst", "ins", "Grab", "sie", "nun", "als", "letz\u00b7ten", "Schmuck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "ADV", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die wei\u00dfen Br\u00fcste schimmern hoch empor,", "tokens": ["Die", "wei\u00b7\u00dfen", "Br\u00fcs\u00b7te", "schim\u00b7mern", "hoch", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Indes dein stummes Haupt nach hinten sank,", "tokens": ["In\u00b7des", "dein", "stum\u00b7mes", "Haupt", "nach", "hin\u00b7ten", "sank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das aus dem Haar den Silberkamm verlor.", "tokens": ["Das", "aus", "dem", "Haar", "den", "Sil\u00b7ber\u00b7kamm", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bist du das, die ich einst so hei\u00df umschlang?", "tokens": ["Bist", "du", "das", ",", "die", "ich", "einst", "so", "hei\u00df", "um\u00b7schlang", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDS", "$,", "PRELS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Bin ich denn der, der einst bei dir geruht", "tokens": ["Bin", "ich", "denn", "der", ",", "der", "einst", "bei", "dir", "ge\u00b7ruht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "$,", "PRELS", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor Liebe toll und bittrer Leidenschaft,", "tokens": ["Vor", "Lie\u00b7be", "toll", "und", "bit\u00b7trer", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der in dich sank wie in ein Meer von Glut", "tokens": ["Der", "in", "dich", "sank", "wie", "in", "ein", "Meer", "von", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "VVFIN", "KOKOM", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und deine Br\u00fcste trank wie Traubensaft?", "tokens": ["Und", "dei\u00b7ne", "Br\u00fcs\u00b7te", "trank", "wie", "Trau\u00b7ben\u00b7saft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Bin ich denn der, der so voll Zorn gebrannt", "tokens": ["Bin", "ich", "denn", "der", ",", "der", "so", "voll", "Zorn", "ge\u00b7brannt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "$,", "PRELS", "ADV", "ADJD", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie einer H\u00f6llenfackel G\u00f6ttlichkeit,", "tokens": ["Wie", "ei\u00b7ner", "H\u00f6l\u00b7len\u00b7fa\u00b7ckel", "G\u00f6tt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und deine Kehle wie im Rausch umspannt,", "tokens": ["Und", "dei\u00b7ne", "Keh\u00b7le", "wie", "im", "Rausch", "um\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KOKOM", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In Hasses ungeheurer Freudigkeit?", "tokens": ["In", "Has\u00b7ses", "un\u00b7ge\u00b7heu\u00b7rer", "Freu\u00b7dig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ist das nicht alles nur ein w\u00fcster Traum?", "tokens": ["Ist", "das", "nicht", "al\u00b7les", "nur", "ein", "w\u00fcs\u00b7ter", "Traum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "PIS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich bin so ruhig und so fern der Gier.", "tokens": ["Ich", "bin", "so", "ru\u00b7hig", "und", "so", "fern", "der", "Gier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die fernen Glocken zittern in dem Raum,", "tokens": ["Die", "fer\u00b7nen", "Glo\u00b7cken", "zit\u00b7tern", "in", "dem", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es ist so still wie in den Kirchen hier.", "tokens": ["Es", "ist", "so", "still", "wie", "in", "den", "Kir\u00b7chen", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KOKOM", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wie ist das alles fremd und sonderbar?", "tokens": ["Wie", "ist", "das", "al\u00b7les", "fremd", "und", "son\u00b7der\u00b7bar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo bist du nun? Was gibst du Antwort nicht?", "tokens": ["Wo", "bist", "du", "nun", "?", "Was", "gibst", "du", "Ant\u00b7wort", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$.", "PWS", "VVFIN", "PPER", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u2013 Ihr nackter Leib ist kalt und eisesklar", "tokens": ["\u2013", "Ihr", "nack\u00b7ter", "Leib", "ist", "kalt", "und", "ei\u00b7ses\u00b7klar"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im blassen Schein vom blauen Ampellicht. \u2013", "tokens": ["Im", "blas\u00b7sen", "Schein", "vom", "blau\u00b7en", "Am\u00b7pel\u00b7licht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Was lie\u00df sie alles auch so stumm geschehn.", "tokens": ["Was", "lie\u00df", "sie", "al\u00b7les", "auch", "so", "stumm", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PIS", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie wird mir furchtbar, wenn so stumm sie liegt.", "tokens": ["Sie", "wird", "mir", "furcht\u00b7bar", ",", "wenn", "so", "stumm", "sie", "liegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O w\u00e4re nur ein Tropfen Bluts zu sehn.", "tokens": ["O", "w\u00e4\u00b7re", "nur", "ein", "Trop\u00b7fen", "Bluts", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was ist das, hat sie ihren Kopf gewiegt?", "tokens": ["Was", "ist", "das", ",", "hat", "sie", "ih\u00b7ren", "Kopf", "ge\u00b7wiegt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "$,", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ich will hier fort. \u2013 Er st\u00fcrzt aus dem Gemach.", "tokens": ["Ich", "will", "hier", "fort", ".", "\u2013", "Er", "st\u00fcrzt", "aus", "dem", "Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Nachtwind, der im Haar der Toten zischt,", "tokens": ["Der", "Nacht\u00b7wind", ",", "der", "im", "Haar", "der", "To\u00b7ten", "zischt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "L\u00f6st leis es auf. Es weht dem Winde nach,", "tokens": ["L\u00f6st", "leis", "es", "auf", ".", "Es", "weht", "dem", "Win\u00b7de", "nach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gleich schwarzer Flamme, die im Sturm verlischt.", "tokens": ["Gleich", "schwar\u00b7zer", "Flam\u00b7me", ",", "die", "im", "Sturm", "ver\u00b7lischt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}