{"dta.poem.12029": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer f\u00e4hrt durch ein Gefild, sieht hinter sich versinken", "tokens": ["Wer", "f\u00e4hrt", "durch", "ein", "Ge\u00b7fild", ",", "sieht", "hin\u00b7ter", "sich", "ver\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein reizend Landschaftbild, ein andres vorwerts winken.", "tokens": ["Ein", "rei\u00b7zend", "Land\u00b7schaft\u00b7bild", ",", "ein", "and\u00b7res", "vor\u00b7werts", "win\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ART", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht halten kann er das, und dieses fest nicht fassen,", "tokens": ["Nicht", "hal\u00b7ten", "kann", "er", "das", ",", "und", "die\u00b7ses", "fest", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "PDS", "$,", "KON", "PDS", "VVFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor\u00fcbergleiten mu\u00df er eins ums andre lassen.", "tokens": ["Vor\u00b7\u00fc\u00b7berg\u00b7lei\u00b7ten", "mu\u00df", "er", "eins", "ums", "and\u00b7re", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PIS", "APPRART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Im gr\u00f6\u00dfern Ma\u00dfstab nur und auf viel ernstre Weise", "tokens": ["Im", "gr\u00f6\u00b7\u00dfern", "Ma\u00df\u00b7stab", "nur", "und", "auf", "viel", "ernst\u00b7re", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADV", "KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erf\u00e4hrt dasselbe, wer durchs Leben macht die Reise.", "tokens": ["Er\u00b7f\u00e4hrt", "das\u00b7sel\u00b7be", ",", "wer", "durchs", "Le\u00b7ben", "macht", "die", "Rei\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "$,", "PWS", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du hast es oft geh\u00f6rt; doch hast du's je gef\u00fchlt,", "tokens": ["Du", "hast", "es", "oft", "ge\u00b7h\u00f6rt", ";", "doch", "hast", "du's", "je", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVFIN", "$.", "ADV", "VAFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie schmerzenreiche Lust hinweg das Leben sp\u00fclt?", "tokens": ["Wie", "schmer\u00b7zen\u00b7rei\u00b7che", "Lust", "hin\u00b7weg", "das", "Le\u00b7ben", "sp\u00fclt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APZR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}