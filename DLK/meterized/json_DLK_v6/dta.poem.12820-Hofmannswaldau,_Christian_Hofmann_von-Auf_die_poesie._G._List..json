{"dta.poem.12820": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die poesie.  \n G. List.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du hoch-gepriesne kunst! du nectar kluger sinnen!", "tokens": ["Du", "hoch\u00b7ge\u00b7pries\u00b7ne", "kunst", "!", "du", "nec\u00b7tar", "klu\u00b7ger", "sin\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich mu\u00df ein edler geist, als perle, lieb gewinnen.", "tokens": ["Dich", "mu\u00df", "ein", "ed\u00b7ler", "geist", ",", "als", "per\u00b7le", ",", "lieb", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "$,", "KOUS", "NN", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du hauch! der nur aus GOtt auf reine hertzen geht,", "tokens": ["Du", "hauch", "!", "der", "nur", "aus", "Gott", "auf", "rei\u00b7ne", "hert\u00b7zen", "geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "ART", "ADV", "APPR", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du schatz! der auch dem rost und motten widersteht.", "tokens": ["Du", "schatz", "!", "der", "auch", "dem", "rost", "und", "mot\u00b7ten", "wi\u00b7der\u00b7steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$.", "ART", "ADV", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich bin dir als ein kind schon heimlich nachgegangen,", "tokens": ["Ich", "bin", "dir", "als", "ein", "kind", "schon", "heim\u00b7lich", "nach\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "KOUS", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich suchte tag und nacht den lorbeer zu erlangen.", "tokens": ["Ich", "such\u00b7te", "tag", "und", "nacht", "den", "lor\u00b7beer", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du stehest mit mir auf, du legest dich mit mir,", "tokens": ["Du", "ste\u00b7hest", "mit", "mir", "auf", ",", "du", "le\u00b7gest", "dich", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich ziehe deinen crantz der sch\u00f6nsten crone f\u00fcr.", "tokens": ["Ich", "zie\u00b7he", "dei\u00b7nen", "crantz", "der", "sch\u00f6ns\u00b7ten", "cro\u00b7ne", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und hast du gleich bisher dem unvergn\u00fcgten magen", "tokens": ["Und", "hast", "du", "gleich", "bis\u00b7her", "dem", "un\u00b7ver\u00b7gn\u00fcg\u00b7ten", "ma\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Noch keinen rothen wein, noch wildpret eingetragen;", "tokens": ["Noch", "kei\u00b7nen", "ro\u00b7then", "wein", ",", "noch", "wild\u00b7pret", "ein\u00b7ge\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So schleppest du mir doch die pest nicht in das hau\u00df,", "tokens": ["So", "schlep\u00b7pest", "du", "mir", "doch", "die", "pest", "nicht", "in", "das", "hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ART", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vielleicht bricht k\u00fcnfftig noch dein g\u00f6ldner nutzen aus.", "tokens": ["Viel\u00b7leicht", "bricht", "k\u00fcnff\u00b7tig", "noch", "dein", "g\u00f6ld\u00b7ner", "nut\u00b7zen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ADV", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wei\u00df ein geschickter kopff was hohes vorzubringen,", "tokens": ["Wei\u00df", "ein", "ge\u00b7schick\u00b7ter", "kopff", "was", "ho\u00b7hes", "vor\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PWS", "ADJA", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und nicht mann, weib und ding alleine herzusingen,", "tokens": ["Und", "nicht", "mann", ",", "weib", "und", "ding", "al\u00b7lei\u00b7ne", "her\u00b7zu\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "NN", "$,", "VVFIN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So \u00f6ffnet er dadurch die \u00fcberschwehre bahn,", "tokens": ["So", "\u00f6ff\u00b7net", "er", "da\u00b7durch", "die", "\u00fc\u00b7bersc\u00b7hweh\u00b7re", "bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die zu der F\u00fcrsten gunst auf rosen f\u00fchren kan.", "tokens": ["Die", "zu", "der", "F\u00fcrs\u00b7ten", "gunst", "auf", "ro\u00b7sen", "f\u00fch\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "APPR", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ja viele haben offt durch flei\u00df in wenig stunden,", "tokens": ["Ja", "vie\u00b7le", "ha\u00b7ben", "offt", "durch", "flei\u00df", "in", "we\u00b7nig", "stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIS", "VAFIN", "ADV", "APPR", "NN", "APPR", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Bey g\u00f6ttern dieser welt fast einen himmel funden;", "tokens": ["Bey", "g\u00f6t\u00b7tern", "die\u00b7ser", "welt", "fast", "ei\u00b7nen", "him\u00b7mel", "fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "PDAT", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da mancher, der da schl\u00e4ft, nicht selten mit verdru\u00df,", "tokens": ["Da", "man\u00b7cher", ",", "der", "da", "schl\u00e4ft", ",", "nicht", "sel\u00b7ten", "mit", "ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "ADV", "VVFIN", "$,", "PTKNEG", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "In einem nessel-strau\u00df zu tantze gehen mu\u00df.", "tokens": ["In", "ei\u00b7nem", "nes\u00b7sel\u00b7strau\u00df", "zu", "tant\u00b7ze", "ge\u00b7hen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVFIN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So bleibest du demnach mein eintziges vergn\u00fcgen,", "tokens": ["So", "blei\u00b7best", "du", "dem\u00b7nach", "mein", "eint\u00b7zi\u00b7ges", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Durch dich will ich den neid mit seinem gifft besiegen.", "tokens": ["Durch", "dich", "will", "ich", "den", "neid", "mit", "sei\u00b7nem", "gifft", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Du mehr als zarte lust! du zuckerst meine pein,", "tokens": ["Du", "mehr", "als", "zar\u00b7te", "lust", "!", "du", "zu\u00b7ckerst", "mei\u00b7ne", "pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "KOKOM", "ADJA", "NN", "$.", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.24": {"text": "Und wirst des kummers tod und grab der sorgen seyn.", "tokens": ["Und", "wirst", "des", "kum\u00b7mers", "tod", "und", "grab", "der", "sor\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "KON", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}