{"textgrid.poem.43940": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun Kind, ich kan dich nicht mehr bitten,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun Kind, ich kan dich nicht mehr bitten,", "tokens": ["Nun", "Kind", ",", "ich", "kan", "dich", "nicht", "mehr", "bit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Behalt mein Herz in treuer Brust.", "tokens": ["Be\u00b7halt", "mein", "Herz", "in", "treu\u00b7er", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Denckmahl deiner muntren Sitten", "tokens": ["Das", "Denck\u00b7mahl", "dei\u00b7ner", "mun\u00b7tren", "Sit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erweckt mir auch von weiten Lust,", "tokens": ["Er\u00b7weckt", "mir", "auch", "von", "wei\u00b7ten", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wo ich reise, wohn und bin,", "tokens": ["Und", "wo", "ich", "rei\u00b7se", ",", "wohn", "und", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "PWAV", "KON", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da folgt mir dein Ged\u00e4chtn\u00fc\u00df hin.", "tokens": ["Da", "folgt", "mir", "dein", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Waldhorn klingt bey Abendstunden", "tokens": ["Ein", "Wald\u00b7horn", "klingt", "bey", "A\u00b7bends\u00b7tun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von weiten durch die G\u00e4rthen sch\u00f6n,", "tokens": ["Von", "wei\u00b7ten", "durch", "die", "G\u00e4r\u00b7then", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es reizt das Blut verliebter Wunden", "tokens": ["Es", "reizt", "das", "Blut", "ver\u00b7lieb\u00b7ter", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4st die Geister fl\u00fcchtig gehn;", "tokens": ["Und", "l\u00e4st", "die", "Geis\u00b7ter", "fl\u00fcch\u00b7tig", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jedoch erg\u00f6zt mich das Geh\u00f6r", "tokens": ["Je\u00b7doch", "er\u00b7g\u00f6zt", "mich", "das", "Ge\u00b7h\u00f6r"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von deinem Wohlseyn noch viel mehr.", "tokens": ["Von", "dei\u00b7nem", "Wohl\u00b7seyn", "noch", "viel", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das Gl\u00fccke spielt mir tausend Po\u00dfen", "tokens": ["Das", "Gl\u00fc\u00b7cke", "spielt", "mir", "tau\u00b7send", "Po\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lockt mich auf des Hofes Ei\u00df,", "tokens": ["Und", "lockt", "mich", "auf", "des", "Ho\u00b7fes", "Ei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich folg ihm klug und unverdro\u00dfen,", "tokens": ["Ich", "folg", "ihm", "klug", "und", "un\u00b7ver\u00b7dro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So gut ich seine T\u00fccke weis;", "tokens": ["So", "gut", "ich", "sei\u00b7ne", "T\u00fc\u00b7cke", "weis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Vorsicht leite, wie sie will,", "tokens": ["Die", "Vor\u00b7sicht", "lei\u00b7te", ",", "wie", "sie", "will", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich halt in allen Wettern still.", "tokens": ["Ich", "halt", "in", "al\u00b7len", "Wet\u00b7tern", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Gegend, wo ich jezund dichte,", "tokens": ["Die", "Ge\u00b7gend", ",", "wo", "ich", "je\u00b7zund", "dich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist einsam, schatticht, k\u00fchl und gr\u00fcn;", "tokens": ["Ist", "ein\u00b7sam", ",", "schat\u00b7ticht", ",", "k\u00fchl", "und", "gr\u00fcn", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier h\u00f6r ich bey der schlancken Fichte", "tokens": ["Hier", "h\u00f6r", "ich", "bey", "der", "schlan\u00b7cken", "Fich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den sanften Wind nach Leipzig ziehn", "tokens": ["Den", "sanf\u00b7ten", "Wind", "nach", "Leip\u00b7zig", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und geb ihm allzeit br\u00fcnstiglich", "tokens": ["Und", "geb", "ihm", "all\u00b7zeit", "br\u00fcns\u00b7tig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Viel tausend hei\u00dfe K\u00fc\u00df an dich.", "tokens": ["Viel", "tau\u00b7send", "hei\u00b7\u00dfe", "K\u00fc\u00df", "an", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hier kan ich mich der Zeit bequemen,", "tokens": ["Hier", "kan", "ich", "mich", "der", "Zeit", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hier ist mir Still und Ort geneigt,", "tokens": ["Hier", "ist", "mir", "Still", "und", "Ort", "ge\u00b7neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die gro\u00dfe Rechnung vorzunehmen,", "tokens": ["Die", "gro\u00b7\u00dfe", "Rech\u00b7nung", "vor\u00b7zu\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie viel mir Leipzig Guts erzeigt;", "tokens": ["Wie", "viel", "mir", "Leip\u00b7zig", "Guts", "er\u00b7zeigt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch alles, was ich sch\u00e4zen kan,", "tokens": ["Doch", "al\u00b7les", ",", "was", "ich", "sch\u00e4\u00b7zen", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das k\u00f6mmt auf deinen Umgang an.", "tokens": ["Das", "k\u00f6mmt", "auf", "dei\u00b7nen", "Um\u00b7gang", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Erinnre dich der ersten K\u00fc\u00dfe,", "tokens": ["E\u00b7rinn\u00b7re", "dich", "der", "ers\u00b7ten", "K\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die niemand als der Schatten sah;", "tokens": ["Die", "nie\u00b7mand", "als", "der", "Schat\u00b7ten", "sah", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie machten mir die \u00c4pfel s\u00fc\u00dfe;", "tokens": ["Sie", "mach\u00b7ten", "mir", "die", "\u00c4p\u00b7fel", "s\u00fc\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, w\u00e4re doch die Zeit noch da!", "tokens": ["Ach", ",", "w\u00e4\u00b7re", "doch", "die", "Zeit", "noch", "da", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "ADV", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gedenck an Pfeifers Schlafgemach", "tokens": ["Ge\u00b7denck", "an", "Pfei\u00b7fers", "Schlaf\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und zehle dort die Wollust nach.", "tokens": ["Und", "zeh\u00b7le", "dort", "die", "Wol\u00b7lust", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Umgang wurd uns sonst verbothen,", "tokens": ["Der", "Um\u00b7gang", "wurd", "uns", "sonst", "ver\u00b7bo\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir suchten die geheimste Bahn,", "tokens": ["Wir", "such\u00b7ten", "die", "ge\u00b7heims\u00b7te", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir riefen die verwandten Todten", "tokens": ["Wir", "rie\u00b7fen", "die", "ver\u00b7wand\u00b7ten", "Tod\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Zeugen unsrer Freundschaft an", "tokens": ["Zu", "Zeu\u00b7gen", "uns\u00b7rer", "Freund\u00b7schaft", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und lie\u00dfen bey verschwiegner Pein", "tokens": ["Und", "lie\u00b7\u00dfen", "bey", "ver\u00b7schwieg\u00b7ner", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Kirchhof unsre Freystatt seyn.", "tokens": ["Den", "Kirch\u00b7hof", "uns\u00b7re", "Freys\u00b7tatt", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}