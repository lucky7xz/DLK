{"dta.poem.19860": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Das D\u00f6rfchen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.85", "sq:0.14"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Ich r\u00fchme mir               ", "tokens": ["Ich", "r\u00fch\u00b7me", "mir"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mein D\u00f6rfchen hier!", "tokens": ["Mein", "D\u00f6rf\u00b7chen", "hier", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Denn sch\u00f6nre Auen,", "tokens": ["Denn", "sch\u00f6n\u00b7re", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Als rings umher", "tokens": ["Als", "rings", "um\u00b7her"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die Blicke schauen,", "tokens": ["Die", "Bli\u00b7cke", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Sind nirgends mehr.", "tokens": ["Sind", "nir\u00b7gends", "mehr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Welch ein Gefilde,", "tokens": ["Welch", "ein", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Zum sch\u00f6nsten Bilde", "tokens": ["Zum", "sch\u00f6ns\u00b7ten", "Bil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "F\u00fcr Dietrichs Hand!", "tokens": ["F\u00fcr", "Diet\u00b7richs", "Hand", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Hier Felsenwand,", "tokens": ["Hier", "Fel\u00b7sen\u00b7wand", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Dort Aehrenfelder,", "tokens": ["Dort", "A\u00b7eh\u00b7ren\u00b7fel\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Und Wiesengr\u00fcn,", "tokens": ["Und", "Wie\u00b7sen\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Dem blaue W\u00e4lder", "tokens": ["Dem", "blau\u00b7e", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Die Gr\u00e4nze ziehn!", "tokens": ["Die", "Gr\u00e4n\u00b7ze", "ziehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "An jener H\u00f6he", "tokens": ["An", "je\u00b7ner", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Die Sch\u00e4ferei,", "tokens": ["Die", "Sch\u00e4\u00b7fe\u00b7rei", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Und in der N\u00e4he", "tokens": ["Und", "in", "der", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Mein Sorgenfrei!", "tokens": ["Mein", "Sor\u00b7gen\u00b7frei", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "So nenn\u2019 ich meine", "tokens": ["So", "nenn'", "ich", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Geliebte, kleine", "tokens": ["Ge\u00b7lieb\u00b7te", ",", "klei\u00b7ne"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "Einsiedelei,", "tokens": ["Ein\u00b7sie\u00b7de\u00b7lei", ","], "token_info": ["word", "punct"], "pos": ["PIAT", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.22": {"text": "Worin ich lebe,", "tokens": ["Wo\u00b7rin", "ich", "le\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Zur Lust verstekt,", "tokens": ["Zur", "Lust", "vers\u00b7tekt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "Die ein Gewebe", "tokens": ["Die", "ein", "Ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["ART", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "Von Ulm und Rebe,", "tokens": ["Von", "Ulm", "und", "Re\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "Gr\u00fcn \u00fcberdekt.", "tokens": ["Gr\u00fcn", "\u00fc\u00b7ber\u00b7dekt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Dort kr\u00e4nzen Schlehen", "tokens": ["Dort", "kr\u00e4n\u00b7zen", "Schle\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die braune Kluft,", "tokens": ["Die", "brau\u00b7ne", "Kluft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und Pappeln wehen", "tokens": ["Und", "Pap\u00b7peln", "we\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "In blauer Luft.", "tokens": ["In", "blau\u00b7er", "Luft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mit sanftem Rieseln", "tokens": ["Mit", "sanf\u00b7tem", "Rie\u00b7seln"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Schleicht hier gemach", "tokens": ["Schleicht", "hier", "ge\u00b7mach"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Auf Silberkieseln", "tokens": ["Auf", "Sil\u00b7ber\u00b7kie\u00b7seln"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Ein heller Bach;", "tokens": ["Ein", "hel\u00b7ler", "Bach", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Fliest unter Zweigen,", "tokens": ["Fliest", "un\u00b7ter", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Die \u00fcber ihn", "tokens": ["Die", "\u00fc\u00b7ber", "ihn"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Sich w\u00f6lbend neigen,", "tokens": ["Sich", "w\u00f6l\u00b7bend", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Bald sch\u00fcchtern hin;", "tokens": ["Bald", "sch\u00fcch\u00b7tern", "hin", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "L\u00e4st bald im Spiegel", "tokens": ["L\u00e4st", "bald", "im", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Den gr\u00fcnen H\u00fcgel,", "tokens": ["Den", "gr\u00fc\u00b7nen", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Wo L\u00e4mmer gehn,", "tokens": ["Wo", "L\u00e4m\u00b7mer", "gehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Des Ufers B\u00fcschgen", "tokens": ["Des", "U\u00b7fers", "B\u00fcschgen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Und alle Fischgen", "tokens": ["Und", "al\u00b7le", "Fischgen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Im Grunde sehn.", "tokens": ["Im", "Grun\u00b7de", "sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Da gleiten Schmerlen", "tokens": ["Da", "glei\u00b7ten", "Schmer\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Und blasen Perlen.", "tokens": ["Und", "bla\u00b7sen", "Per\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "Ihr schneller Lauf", "tokens": ["Ihr", "schnel\u00b7ler", "Lauf"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Geht bald hernieder", "tokens": ["Geht", "bald", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKVZ"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Und bald herauf", "tokens": ["Und", "bald", "her\u00b7auf"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "Zur Fl\u00e4che wieder.", "tokens": ["Zur", "Fl\u00e4\u00b7che", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6n ist die Flur;", "tokens": ["Sch\u00f6n", "ist", "die", "Flur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Allein Elise", "tokens": ["Al\u00b7lein", "E\u00b7li\u00b7se"], "token_info": ["word", "word"], "pos": ["ADV", "NE"], "meter": "-+---", "measure": "dactylic.init"}, "line.3": {"text": "Macht sie mir nur", "tokens": ["Macht", "sie", "mir", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "PPER", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Zum Paradiese.", "tokens": ["Zum", "Pa\u00b7ra\u00b7die\u00b7se", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Der erste Blik", "tokens": ["Der", "ers\u00b7te", "Blik"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Des Morgens wecket", "tokens": ["Des", "Mor\u00b7gens", "we\u00b7cket"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Auch unser Gl\u00fck.", "tokens": ["Auch", "un\u00b7ser", "Gl\u00fck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Nur leicht bedecket", "tokens": ["Nur", "leicht", "be\u00b7de\u00b7cket"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "F\u00fchrt sie mich hin,", "tokens": ["F\u00fchrt", "sie", "mich", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Wo Florens Beete", "tokens": ["Wo", "Flo\u00b7rens", "Bee\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NE", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Die K\u00f6nigin", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Der Morgenr\u00f6te", "tokens": ["Der", "Mor\u00b7gen\u00b7r\u00f6\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Mit Thr\u00e4nen n\u00e4st,", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "n\u00e4st", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Und Perlen blizen", "tokens": ["Und", "Per\u00b7len", "bli\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Von allen Spizen", "tokens": ["Von", "al\u00b7len", "Spi\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Des Grases l\u00e4st.", "tokens": ["Des", "Gra\u00b7ses", "l\u00e4st", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Die Knospe spaltet", "tokens": ["Die", "Knos\u00b7pe", "spal\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Die volle Brust;", "tokens": ["Die", "vol\u00b7le", "Brust", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Die Blume faltet", "tokens": ["Die", "Blu\u00b7me", "fal\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Sich auf zur Lust.", "tokens": ["Sich", "auf", "zur", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Sie bl\u00fcht, und bl\u00fchet", "tokens": ["Sie", "bl\u00fcht", ",", "und", "bl\u00fc\u00b7het"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Doch sch\u00f6ner nicht,", "tokens": ["Doch", "sch\u00f6\u00b7ner", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Als das Gesicht", "tokens": ["Als", "das", "Ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.20": {"text": "Elisens gl\u00fchet.", "tokens": ["E\u00b7li\u00b7sens", "gl\u00fc\u00b7het", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Wanns heisser wird", "tokens": ["Wanns", "heis\u00b7ser", "wird"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Geht man selbander", "tokens": ["Geht", "man", "sel\u00b7ban\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Zu dem M\u00e4ander,", "tokens": ["Zu", "dem", "M\u00e4\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der unten irt.", "tokens": ["Der", "un\u00b7ten", "irt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Da sinkt zum Bade", "tokens": ["Da", "sinkt", "zum", "Ba\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Der Sch\u00e4ferin,", "tokens": ["Der", "Sch\u00e4\u00b7fe\u00b7rin", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "An das Gestade,", "tokens": ["An", "das", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": 