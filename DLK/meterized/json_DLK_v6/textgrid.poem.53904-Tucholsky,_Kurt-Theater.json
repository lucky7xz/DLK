{"textgrid.poem.53904": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Theater", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sohn (durch die Reihen): Limonade, Eskimo-Eis, belegte Br\u00f6tchen!", "tokens": ["Der", "Sohn", "(", "durch", "die", "Rei\u00b7hen", ")", ":", "Li\u00b7mo\u00b7na\u00b7de", ",", "Es\u00b7ki\u00b7mo\u00b7Eis", ",", "be\u00b7leg\u00b7te", "Br\u00f6t\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "ART", "NN", "$(", "$.", "NN", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Die Tochter (ebenso): Programme, Zettel!", "tokens": ["Die", "Toch\u00b7ter", "(", "e\u00b7ben\u00b7so", ")", ":", "Pro\u00b7gram\u00b7me", ",", "Zet\u00b7tel", "!"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "$(", "$.", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vater (ablehnend): O bitte, Zensur-Exemplar!", "tokens": ["Va\u00b7ter", "(", "ab\u00b7leh\u00b7nend", ")", ":", "O", "bit\u00b7te", ",", "Zen\u00b7sur\u00b7Ex\u00b7em\u00b7plar", "!"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "VVPP", "$(", "$.", "NE", "VVFIN", "$,", "NN", "$."], "meter": "+-+---+-+-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Der 1. Kritiker (im Gespr\u00e4ch mit dem zweiten): Gl\u00e4nzender Einfall.", "tokens": ["Der", "1.", "Kri\u00b7ti\u00b7ker", "(", "im", "Ge\u00b7spr\u00e4ch", "mit", "dem", "zwei\u00b7ten", ")", ":", "Gl\u00e4n\u00b7zen\u00b7der", "Ein\u00b7fall", "."], "token_info": ["word", "ordinal", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "APPRART", "NN", "APPR", "ART", "ADJA", "$(", "$.", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der 2. Kritiker: Das h\u00e4tte uns einfallen m\u00fcssen.", "tokens": ["Der", "2.", "Kri\u00b7ti\u00b7ker", ":", "Das", "h\u00e4t\u00b7te", "uns", "ein\u00b7fal\u00b7len", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "ordinal", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PDS", "VAFIN", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der 1. Kritiker: Es wird uns schon noch einfallen.", "tokens": ["Der", "1.", "Kri\u00b7ti\u00b7ker", ":", "Es", "wird", "uns", "schon", "noch", "ein\u00b7fal\u00b7len", "."], "token_info": ["word", "ordinal", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der 2. Kritiker: Man sagt ohnehin, da\u00df die Idee von Ihnen ist.", "tokens": ["Der", "2.", "Kri\u00b7ti\u00b7ker", ":", "Man", "sagt", "oh\u00b7ne\u00b7hin", ",", "da\u00df", "die", "I\u00b7dee", "von", "Ih\u00b7nen", "ist", "."], "token_info": ["word", "ordinal", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PIS", "VVFIN", "ADV", "$,", "KOUS", "ART", "NN", "APPR", "PPER", "VAFIN", "$."], "meter": "-+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Der 1. Kritiker: Noch nicht.", "tokens": ["Der", "1.", "Kri\u00b7ti\u00b7ker", ":", "Noch", "nicht", "."], "token_info": ["word", "ordinal", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Vorhang am Fenster geht auf, der Vorhang des Theaters schlie\u00dft sich.", "tokens": ["Der", "Vor\u00b7hang", "am", "Fens\u00b7ter", "geht", "auf", ",", "der", "Vor\u00b7hang", "des", "The\u00b7a\u00b7ters", "schlie\u00dft", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "ART", "NN", "VVFIN", "PRF", "$."], "meter": "-+--+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}}}}}