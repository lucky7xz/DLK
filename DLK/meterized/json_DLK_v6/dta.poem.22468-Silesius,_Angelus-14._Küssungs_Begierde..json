{"dta.poem.22468": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "14. K\u00fcssungs Begierde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Ach la\u00df mich doch mein Kind mein GOtt an deinen", "tokens": ["Ach", "la\u00df", "mich", "doch", "mein", "Kind", "mein", "Gott", "an", "dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVIMP", "PPER", "ADV", "PPOSAT", "NN", "PPOSAT", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcssen/", "tokens": ["F\u00fcs\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Nur einen Augenblik da\u00df minste Br\u00fcnklein k\u00fcssen.", "tokens": ["Nur", "ei\u00b7nen", "Au\u00b7gen\u00b7blik", "da\u00df", "mins\u00b7te", "Br\u00fcn\u00b7klein", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KOUS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich wei\u00df werd\u2019 ich von Dir nur blo\u00df ber\u00fchret sein/", "tokens": ["Ich", "wei\u00df", "werd'", "ich", "von", "Dir", "nur", "blo\u00df", "be\u00b7r\u00fch\u00b7ret", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VAFIN", "PPER", "APPR", "PPER", "ADV", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df straks verschwinden wird/ mein\u2019/ und auch deine", "tokens": ["Da\u00df", "straks", "ve