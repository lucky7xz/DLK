{"dta.poem.17657": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "279.  Was im Orte der Welt vor der  \n Welt gewest.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Eh Gott die Welt erschuf was war in diesem Ort?", "tokens": ["Eh", "Gott", "die", "Welt", "er\u00b7schuf", "was", "war", "in", "die\u00b7sem", "Ort", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "PIS", "VAFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es war der Ort selb selbst/ Gott und sein Ewges Wort.", "tokens": ["Es", "war", "der", "Ort", "selb", "selbst", "/", "Gott", "und", "sein", "Ew\u00b7ges", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "ADV", "$(", "NN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}