{"textgrid.poem.33664": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsanfang", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Tage l\u00e4nger werden,", "tokens": ["Wenn", "die", "Ta\u00b7ge", "l\u00e4n\u00b7ger", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4chst das Herz auch in der Brust;", "tokens": ["W\u00e4chst", "das", "Herz", "auch", "in", "der", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leichter wird es dann auf Erden,", "tokens": ["Leich\u00b7ter", "wird", "es", "dann", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles atmet Lust.", "tokens": ["Al\u00b7les", "at\u00b7met", "Lust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Alles atmet Lust und Sehnen,", "tokens": ["Al\u00b7les", "at\u00b7met", "Lust", "und", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heimlich nur im jungen Jahr", "tokens": ["Heim\u00b7lich", "nur", "im", "jun\u00b7gen", "Jahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denkt ein Armer noch mit Tr\u00e4nen,", "tokens": ["Denkt", "ein", "Ar\u00b7mer", "noch", "mit", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ein Winter war!", "tokens": ["Da\u00df", "ein", "Win\u00b7ter", "war", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}