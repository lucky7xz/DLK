{"dta.poem.8663": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Indifferentist .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Ob du, ein Sokrates, den Schirlingsbecher", "tokens": ["Ob", "du", ",", "ein", "Sok\u00b7ra\u00b7tes", ",", "den", "Schir\u00b7lings\u00b7be\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ART", "NE", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf's Wohl des Vaterlandes l\u00e4chelnd trinkst;", "tokens": ["Auf's", "Wohl", "des", "Va\u00b7ter\u00b7lan\u00b7des", "l\u00e4\u00b7chelnd", "trinkst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob du, ein schn\u00f6der, teuflischer Verbrecher,", "tokens": ["Ob", "du", ",", "ein", "schn\u00f6\u00b7der", ",", "teuf\u00b7li\u00b7scher", "Ver\u00b7bre\u00b7cher", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Henkerbeil getroffen, fluchend sinkst;", "tokens": ["Vom", "Hen\u00b7ker\u00b7beil", "ge\u00b7trof\u00b7fen", ",", "flu\u00b7chend", "sinkst", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ob dein Genie sein Werk den raschen Zeiten", "tokens": ["Ob", "dein", "Ge\u00b7nie", "sein", "Werk", "den", "ra\u00b7schen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geschleudert, ein Gebirg, in ihre Bahn,", "tokens": ["Ge\u00b7schleu\u00b7dert", ",", "ein", "Ge\u00b7birg", ",", "in", "ih\u00b7re", "Bahn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sie an seinem Fu\u00df vor\u00fcberschreiten,", "tokens": ["Da\u00df", "sie", "an", "sei\u00b7nem", "Fu\u00df", "vor\u00b7\u00fc\u00b7bersc\u00b7hrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und grauend seine Gipfel starren an;", "tokens": ["Und", "grau\u00b7end", "sei\u00b7ne", "Gip\u00b7fel", "star\u00b7ren", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ob nichts dein langes Leben war hienieden,", "tokens": ["Ob", "nichts", "dein", "lan\u00b7ges", "Le\u00b7ben", "war", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als f\u00fcr's Gew\u00fcrm des Grabes eine Mast;", "tokens": ["Als", "f\u00fcr's", "Ge\u00b7w\u00fcrm", "des", "Gra\u00b7bes", "ei\u00b7ne", "Mast", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob du, der Menschheit Fesseln anzuschmieden,", "tokens": ["Ob", "du", ",", "der", "Menschheit", "Fes\u00b7seln", "an\u00b7zu\u00b7schmie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "NN", "VVIZU", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein toller Held die bange Welt durchrast:", "tokens": ["Ein", "tol\u00b7ler", "Held", "die", "ban\u00b7ge", "Welt", "durc\u00b7hrast", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ist just so wichtig, als: ob nur im Kreise", "tokens": ["Ist", "just", "so", "wich\u00b7tig", ",", "als", ":", "ob", "nur", "im", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "$,", "KOUS", "$.", "KOUS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einf\u00f6rmig stets das Aufgu\u00dfthierchen schwimmt,", "tokens": ["Ein\u00b7f\u00f6r\u00b7mig", "stets", "das", "Auf\u00b7gu\u00df\u00b7thier\u00b7chen", "schwimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob es vielleicht nach rechts die gro\u00dfe Reise,", "tokens": ["Ob", "es", "viel\u00b7leicht", "nach", "rechts", "die", "gro\u00b7\u00dfe", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vielleicht nach links im Tropfen unternimmt.", "tokens": ["Viel\u00b7leicht", "nach", "links", "im", "Trop\u00b7fen", "un\u00b7ter\u00b7nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}