{"dta.poem.9325": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .  \n Sonnet ne\u00fcer Art/  \n so sich mit weiblichen anf\u00e4ht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "ungrad\u2019 ist den G\u00f6tternlieb; dreymahl ist er auch gebunden", "tokens": ["un\u00b7grad'", "ist", "den", "G\u00f6t\u00b7tern\u00b7lieb", ";", "drey\u00b7mahl", "ist", "er", "auch", "ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "dreyer farben saden sind \u00fcmb den harten hals gewunden.", "tokens": ["drey\u00b7er", "far\u00b7ben", "sa\u00b7den", "sind", "\u00fcmb", "den", "har\u00b7ten", "hals", "ge\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "VAFIN", "APPR", "ART", "ADJA", "NN", "VAPP", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}