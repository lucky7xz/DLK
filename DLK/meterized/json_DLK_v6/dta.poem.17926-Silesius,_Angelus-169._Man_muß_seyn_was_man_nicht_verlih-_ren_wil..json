{"dta.poem.17926": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "169.  Man mu\u00df seyn/ was man nicht verlih-  \n ren wil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Weis\u2019 ist was er hat. Wiltu das Feinperlein.", "tokens": ["Der", "Weis'", "ist", "was", "er", "hat", ".", "Wil\u00b7tu", "das", "Fein\u00b7per\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PWS", "PPER", "VAFIN", "$.", "VMFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Himmels nicht verliehrn/ so mustu s\u2019 selber seyn.", "tokens": ["Des", "Him\u00b7mels", "nicht", "ver\u00b7liehrn", "/", "so", "mus\u00b7tu", "s'", "sel\u00b7ber", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$(", "ADV", "VMFIN", "ADV", "ADV", "VAINF", "$."], "meter": "-+-+-+----+-+", "measure": "unknown.measure.penta"}}}}}