{"textgrid.poem.28030": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An keinen schweren Fall, den sie begangen h\u00e4tte,", "tokens": ["An", "kei\u00b7nen", "schwe\u00b7ren", "Fall", ",", "den", "sie", "be\u00b7gan\u00b7gen", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denkt Cypria; sie f\u00e4llt offt, aber nur ins Bette;", "tokens": ["Denkt", "Cyp\u00b7ria", ";", "sie", "f\u00e4llt", "offt", ",", "a\u00b7ber", "nur", "ins", "Bet\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$.", "PPER", "VVFIN", "ADV", "$,", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Sie ist sonst schweren Fall bem\u00fcht zu \u00fcbergehen;", "tokens": ["Sie", "ist", "sonst", "schwe\u00b7ren", "Fall", "be\u00b7m\u00fcht", "zu", "\u00fc\u00b7ber\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00e4llt nicht ins Bette sie und f\u00e4llt, geschiehts im stehen.", "tokens": ["F\u00e4llt", "nicht", "ins", "Bet\u00b7te", "sie", "und", "f\u00e4llt", ",", "ge\u00b7schiehts", "im", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPRART", "NN", "PPER", "KON", "VVFIN", "$,", "ADV", "APPRART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}