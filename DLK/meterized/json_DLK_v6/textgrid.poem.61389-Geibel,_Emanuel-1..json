{"textgrid.poem.61389": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der alte K\u00f6nig zog zu Wald,", "tokens": ["Der", "al\u00b7te", "K\u00f6\u00b7nig", "zog", "zu", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist ein Jagen heute!", "tokens": ["Das", "ist", "ein", "Ja\u00b7gen", "heu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Renner schnaubt, das H\u00fcfthorn schallt,", "tokens": ["Der", "Ren\u00b7ner", "schnaubt", ",", "das", "H\u00fcf\u00b7thorn", "schallt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Busche bellt die Meute.", "tokens": ["Im", "Bu\u00b7sche", "bellt", "die", "Meu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und als die Sonn' im Mittag steht,", "tokens": ["Und", "als", "die", "Sonn'", "im", "Mit\u00b7tag", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da hat im Buchengehege", "tokens": ["Da", "hat", "im", "Bu\u00b7chen\u00b7ge\u00b7he\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des K\u00f6nigs rosiges T\u00f6chterlein", "tokens": ["Des", "K\u00f6\u00b7nigs", "ro\u00b7si\u00b7ges", "T\u00f6ch\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verloren sich vom Wege.", "tokens": ["Ver\u00b7lo\u00b7ren", "sich", "vom", "We\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie reitet sacht, es reitet mit ihr", "tokens": ["Sie", "rei\u00b7tet", "sacht", ",", "es", "rei\u00b7tet", "mit", "ihr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Pag' im gelben Haare,", "tokens": ["Der", "Pag'", "im", "gel\u00b7ben", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00e4re sie nicht des K\u00f6nigs Kind,", "tokens": ["Und", "w\u00e4\u00b7re", "sie", "nicht", "des", "K\u00f6\u00b7nigs", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie taugten zum sch\u00f6nsten Paare.", "tokens": ["Sie", "taug\u00b7ten", "zum", "sch\u00f6ns\u00b7ten", "Paa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Er schaut sie an, sein Herz erbebt,", "tokens": ["Er", "schaut", "sie", "an", ",", "sein", "Herz", "er\u00b7bebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Forst wird immer dichter,", "tokens": ["Der", "Forst", "wird", "im\u00b7mer", "dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Wangen brennen ihm bis zur Stirn,", "tokens": ["Die", "Wan\u00b7gen", "bren\u00b7nen", "ihm", "bis", "zur", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit brennenden Wangen spricht er:", "tokens": ["Mit", "bren\u00b7nen\u00b7den", "Wan\u00b7gen", "spricht", "er", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbdu hold holdselige Prinze\u00df,", "tokens": ["\u00bb", "du", "hold", "hold\u00b7se\u00b7li\u00b7ge", "Prin\u00b7ze\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich kann's nicht mehr verschweigen,", "tokens": ["Ich", "kann's", "nicht", "mehr", "ver\u00b7schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein junges Herz, das bricht vor Lieb',", "tokens": ["Mein", "jun\u00b7ges", "Herz", ",", "das", "bricht", "vor", "Lieb'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PDS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz, das ist dein eigen.", "tokens": ["Mein", "Herz", ",", "das", "ist", "dein", "ei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O d\u00fcrft' ich auf den roten Mund", "tokens": ["O", "d\u00fcrft'", "ich", "auf", "den", "ro\u00b7ten", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein einzigmal dich k\u00fcssen!", "tokens": ["Ein", "ein\u00b7zig\u00b7mal", "dich", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich w\u00e4re der seligste Mann von der Welt,", "tokens": ["Ich", "w\u00e4\u00b7re", "der", "se\u00b7ligs\u00b7te", "Mann", "von", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Sollt' ich drum sterben m\u00fcssen.\u00ab", "tokens": ["Sollt'", "ich", "drum", "ster\u00b7ben", "m\u00fcs\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PAV", "VVINF", "VMINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie sagt nicht Ja, sie sagt nicht Nein,", "tokens": ["Sie", "sagt", "nicht", "Ja", ",", "sie", "sagt", "nicht", "Nein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKANT", "$,", "PPER", "VVFIN", "PTKNEG", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie hemmt des Rosses Z\u00fcgel,", "tokens": ["Sie", "hemmt", "des", "Ros\u00b7ses", "Z\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und als sie sich vom Sattel schwingt,", "tokens": ["Und", "als", "sie", "sich", "vom", "Sat\u00b7tel", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da h\u00e4lt er ihr den B\u00fcgel.", "tokens": ["Da", "h\u00e4lt", "er", "ihr", "den", "B\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie schreiten hinein in den tiefen Wald,", "tokens": ["Sie", "schrei\u00b7ten", "hin\u00b7ein", "in", "den", "tie\u00b7fen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da sind so schattig die Lauben,", "tokens": ["Da", "sind", "so", "schat\u00b7tig", "die", "Lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da singt von Liebe die Nachtigall,", "tokens": ["Da", "singt", "von", "Lie\u00b7be", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und girren die Turteltauben.", "tokens": ["Und", "gir\u00b7ren", "die", "Tur\u00b7tel\u00b7tau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Da sprie\u00dft die rote, die wilde Ros'", "tokens": ["Da", "sprie\u00dft", "die", "ro\u00b7te", ",", "die", "wil\u00b7de", "Ros'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In gr\u00fcnen Finsternissen;", "tokens": ["In", "gr\u00fc\u00b7nen", "Fins\u00b7ter\u00b7nis\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da beut am Grund das frische Moos", "tokens": ["Da", "beut", "am", "Grund", "das", "fri\u00b7sche", "Moos"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Lieb' ein Ruhekissen.", "tokens": ["Der", "Lieb'", "ein", "Ru\u00b7he\u00b7kis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sie ruhn im Moos bei der wilden Ros',", "tokens": ["Sie", "ruhn", "im", "Moos", "bei", "der", "wil\u00b7den", "Ros'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Rosse lassen sie grasen,", "tokens": ["Die", "Ros\u00b7se", "las\u00b7sen", "sie", "gra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie h\u00f6ren nicht mehr die Nachtigall", "tokens": ["Sie", "h\u00f6\u00b7ren", "nicht", "mehr", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und nicht der J\u00e4ger Blasen \u2013", "tokens": ["Und", "nicht", "der", "J\u00e4\u00b7ger", "Bla\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Du alter K\u00f6nig, harre nicht!", "tokens": ["Du", "al\u00b7ter", "K\u00f6\u00b7nig", ",", "har\u00b7re", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sch\u00f6nste der Prinzessen,", "tokens": ["Die", "sch\u00f6ns\u00b7te", "der", "Prin\u00b7zes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Sie hat in deines Pagen Arm", "tokens": ["Sie", "hat", "in", "dei\u00b7nes", "Pa\u00b7gen", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich und die Welt vergessen.", "tokens": ["Dich", "und", "die", "Welt", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}