{"dta.poem.13055": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edie schnelle F\u00e4rtigkeit zu folgen wies so viel,", "tokens": ["\u201e", "die", "schnel\u00b7le", "F\u00e4r\u00b7tig\u00b7keit", "zu", "fol\u00b7gen", "wies", "so", "viel", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "PTKZU", "VVINF", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "490\u201dAls wann des Mondes Will' und Vorschlag wohlgefiel.", "tokens": ["\"", "Als", "wann", "des", "Mon\u00b7des", "Will'", "und", "Vor\u00b7schlag", "wohl\u00b7ge\u00b7fiel", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PWAV", "ART", "NN", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eman eilte nach dem Ort, wo sich schon eine R\u00f6the", "tokens": ["\u201e", "man", "eil\u00b7te", "nach", "dem", "Ort", ",", "wo", "sich", "schon", "ei\u00b7ne", "R\u00f6\u00b7the"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PIS", "VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "PRF", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ejedoch noch halb verschleyrt aus dem Gew\u00f6lck\u2019 erh\u00f6hte;", "tokens": ["\u201e", "je\u00b7doch", "noch", "halb", "ver\u00b7schleyrt", "aus", "dem", "Ge\u00b7w\u00f6lck'", "er\u00b7h\u00f6h\u00b7te", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ADJD", "VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "\u201edes aufgeblasnen Monds versilbert helles Rund", "tokens": ["\u201e", "des", "auf\u00b7ge\u00b7blas\u00b7nen", "Monds", "ver\u00b7sil\u00b7bert", "hel\u00b7les", "Rund"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "VVPP", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eflog vor bi\u00df an den Ort der ersten Morgen-Stund.", "tokens": ["\u201e", "flog", "vor", "bi\u00df", "an", "den", "Ort", "der", "ers\u00b7ten", "Mor\u00b7gen\u00b7Stund", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "495\u201dWas Majest\u00e4t und Pomp! ein Meer mit Bliz bes\u00e4met!", "tokens": ["\"", "Was", "Ma\u00b7jes\u00b7t\u00e4t", "und", "Pomp", "!", "ein", "Meer", "mit", "Bliz", "be\u00b7s\u00e4\u00b7met", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "NN", "KON", "NN", "$.", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ekein Wunder, wann der Glanz die Sonne selbst besch\u00e4met!", "tokens": ["\u201e", "kein", "Wun\u00b7der", ",", "wann", "der", "Glanz", "die", "Son\u00b7ne", "selbst", "be\u00b7sch\u00e4\u00b7met", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ewillf\u00e4hrigkeit und Freud erschien an jedem Stern;", "tokens": ["\u201e", "will\u00b7f\u00e4h\u00b7rig\u00b7keit", "und", "Freud", "er\u00b7schien", "an", "je\u00b7dem", "Stern", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "KON", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201edes Schimmers Regungen bezeugten klar, wie gern", "tokens": ["\u201e", "des", "Schim\u00b7mers", "Re\u00b7gun\u00b7gen", "be\u00b7zeug\u00b7ten", "klar", ",", "wie", "gern"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ART", "NN", "NN", "VVFIN", "ADJD", "$,", "PWAV", "ADV"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "\u201eman diesem Rath gefolgt. Sie schienen sich zu winden,", "tokens": ["\u201e", "man", "die\u00b7sem", "Rath", "ge\u00b7folgt", ".", "Sie", "schie\u00b7nen", "sich", "zu", "win\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "PDAT", "NN", "VVPP", "$.", "PPER", "VVFIN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "500\u201dAls wollten sie die Welt mit ihrer Freud entz\u00fcnden.", "tokens": ["\"", "Als", "woll\u00b7ten", "sie", "die", "Welt", "mit", "ih\u00b7rer", "Freud", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}