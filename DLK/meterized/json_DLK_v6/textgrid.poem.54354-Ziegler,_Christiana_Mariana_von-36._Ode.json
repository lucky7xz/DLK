{"textgrid.poem.54354": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "36. Ode", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sey mir beliebter Ort gepriesen,", "tokens": ["Sey", "mir", "be\u00b7lieb\u00b7ter", "Ort", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den mir das Schicksal angewiesen", "tokens": ["Den", "mir", "das", "Schick\u00b7sal", "an\u00b7ge\u00b7wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ich die Ruhe finden soll,", "tokens": ["Wo", "ich", "die", "Ru\u00b7he", "fin\u00b7den", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich will hiermit den Endschlu\u00df fassen,", "tokens": ["Ich", "will", "hier\u00b7mit", "den", "End\u00b7schlu\u00df", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dich Lebenslang nicht zu verlassen,", "tokens": ["Dich", "Le\u00b7bens\u00b7lang", "nicht", "zu", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bey dir ist mir nur gar zu wohl.", "tokens": ["Bey", "dir", "ist", "mir", "nur", "gar", "zu", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ADV", "ADV", "APPR", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Es m\u00f6gen andre sanfte sitzen,", "tokens": ["Es", "m\u00f6\u00b7gen", "and\u00b7re", "sanf\u00b7te", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Mauren die von Marmor blitzen,", "tokens": ["In", "Mau\u00b7ren", "die", "von", "Mar\u00b7mor", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schlie\u00dft euch nur in Pall\u00e4ste ein;", "tokens": ["Schlie\u00dft", "euch", "nur", "in", "Pal\u00b7l\u00e4s\u00b7te", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich werd euch solche nicht beneiden,", "tokens": ["Ich", "werd", "euch", "sol\u00b7che", "nicht", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Auge sucht sich hier zu weiden,", "tokens": ["Mein", "Au\u00b7ge", "sucht", "sich", "hier", "zu", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Aufenthalt kann sch\u00f6ner seyn.", "tokens": ["Kein", "Auf\u00b7ent\u00b7halt", "kann", "sch\u00f6\u00b7ner", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zufriedenheit n\u00e4hrt meine Sinnen,", "tokens": ["Zu\u00b7frie\u00b7den\u00b7heit", "n\u00e4hrt", "mei\u00b7ne", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Mich treibt kein Sturm, kein Blitz von hinnen,", "tokens": ["Mich", "treibt", "kein", "Sturm", ",", "kein", "Blitz", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts ist dem Vorsatz hinderlich:", "tokens": ["Nichts", "ist", "dem", "Vor\u00b7satz", "hin\u00b7der\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier werden mich nicht Dornen stechen,", "tokens": ["Hier", "wer\u00b7den", "mich", "nicht", "Dor\u00b7nen", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hier wirds mir nie an Lust gebrechen,", "tokens": ["Hier", "wirds", "mir", "nie", "an", "Lust", "ge\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dies weis ich, hier begr\u00e4bt man mich.", "tokens": ["Dies", "weis", "ich", ",", "hier", "be\u00b7gr\u00e4bt", "man", "mich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKVZ", "PPER", "$,", "ADV", "VVFIN", "PIS", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}