{"dta.poem.8781": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Die mit balsamischem Blatt verband mich!", "tokens": ["Die", "mit", "bal\u00b7sa\u00b7mi\u00b7schem", "Blatt", "ver\u00b7band", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Als mich in Flammen umdroht Verzweiflung,", "tokens": ["Als", "mich", "in", "Flam\u00b7men", "um\u00b7droht", "Ver\u00b7zwei\u00b7flung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Deckte des Glaubens Asbestgewand mich;", "tokens": ["Deck\u00b7te", "des", "Glau\u00b7bens", "As\u00b7best\u00b7ge\u00b7wand", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "PPER", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Irrend durchstrich ich das wald'ge Dickicht,", "tokens": ["Ir\u00b7rend", "durch\u00b7strich", "ich", "das", "wald'\u00b7ge", "Di\u00b7ckicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Aber der fl\u00f6tende Vogel fand mich;", "tokens": ["A\u00b7ber", "der", "fl\u00f6\u00b7ten\u00b7de", "Vo\u00b7gel", "fand", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Wellen verschlangen mich, doch der Delphin", "tokens": ["Wel\u00b7len", "ver\u00b7schlan\u00b7gen", "mich", ",", "doch", "der", "Del\u00b7phin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADV", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Segelte ruhig an's gr\u00fcne Land mich;", "tokens": ["Se\u00b7gel\u00b7te", "ru\u00b7hig", "an's", "gr\u00fc\u00b7ne", "Land", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "ADJA", "NN", "PPER", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Nieder vom Berge zur Tiefe glitt ich,", "tokens": ["Nie\u00b7der", "vom", "Ber\u00b7ge", "zur", "Tie\u00b7fe", "glitt", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "Aber die Rebe des Bergs umwand mich.", "tokens": ["A\u00b7ber", "die", "Re\u00b7be", "des", "Bergs", "um\u00b7wand", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}