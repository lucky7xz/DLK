{"dta.poem.6595": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den durchsichtigen und nicht durch-  \n sichtigen Dingen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So lang der Sonnen Gluth die neue Welt anlacht,", "tokens": ["So", "lang", "der", "Son\u00b7nen", "Gluth", "die", "neu\u00b7e", "Welt", "an\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Liegt unsere versenckt, in einer tieffen Nacht;", "tokens": ["Liegt", "un\u00b7se\u00b7re", "ver\u00b7senckt", ",", "in", "ei\u00b7ner", "tief\u00b7fen", "Nacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "VVPP", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hernach erhebet sich derselben dicker Schleyer,", "tokens": ["Her\u00b7nach", "er\u00b7he\u00b7bet", "sich", "der\u00b7sel\u00b7ben", "di\u00b7cker", "Schle\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "So uns bisher vom hellen Sonnen-Feuer", "tokens": ["So", "uns", "bis\u00b7her", "vom", "hel\u00b7len", "Son\u00b7nen\u00b7Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den sch\u00f6nen Glantz geraubt. Der Morgenr\u00f6the Pracht", "tokens": ["Den", "sch\u00f6\u00b7nen", "Glantz", "ge\u00b7raubt", ".", "Der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "Pracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Giebt den verlohrnen Glantz an jedem Vorwurf wieder.", "tokens": ["Giebt", "den", "ver\u00b7lohr\u00b7nen", "Glantz", "an", "je\u00b7dem", "Vor\u00b7wurf", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es gehet kaum sobald der Tag im Westen nieder;", "tokens": ["Es", "ge\u00b7het", "kaum", "so\u00b7bald", "der", "Tag", "im", "Wes\u00b7ten", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So \u00fcberdecket uns aufs neu die schwartze Nacht.", "tokens": ["So", "\u00fc\u00b7berd\u00b7e\u00b7cket", "uns", "aufs", "neu", "die", "schwart\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es wechselt alles ab. Doch m\u00f6gen noch so sch\u00f6n", "tokens": ["Es", "wech\u00b7selt", "al\u00b7les", "ab", ".", "Doch", "m\u00f6\u00b7gen", "noch", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "PTKVZ", "$.", "KON", "VMFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die neuen Scenen uns die Farben mahlen,", "tokens": ["Die", "neu\u00b7en", "Sce\u00b7nen", "uns", "die", "Far\u00b7ben", "mah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So fl\u00fcchtig oder daurhafft seyn;", "tokens": ["So", "fl\u00fcch\u00b7tig", "o\u00b7der", "daur\u00b7hafft", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So ist doch auf der Welt, von allem, was wir sehn,", "tokens": ["So", "ist", "doch", "auf", "der", "Welt", ",", "von", "al\u00b7lem", ",", "was", "wir", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "ART", "NN", "$,", "APPR", "PIS", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nichts anders, als nur Sonnen-Schein", "tokens": ["Nichts", "an\u00b7ders", ",", "als", "nur", "Son\u00b7nen\u00b7Schein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "ADV", "$,", "KOUS", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Nichts anders, als nur Sonnen-Strahlen.", "tokens": ["Nichts", "an\u00b7ders", ",", "als", "nur", "Son\u00b7nen\u00b7Strah\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "$,", "KOUS", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}