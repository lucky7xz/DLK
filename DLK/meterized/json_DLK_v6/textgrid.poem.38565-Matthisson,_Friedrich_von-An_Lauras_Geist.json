{"textgrid.poem.38565": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Lauras Geist", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn im Irrgang dieses Lebens,", "tokens": ["Wenn", "im", "Irr\u00b7gang", "die\u00b7ses", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Freund,", "tokens": ["Oh\u00b7ne", "Freund", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Kummervoll mein Auge weint,", "tokens": ["Kum\u00b7mer\u00b7voll", "mein", "Au\u00b7ge", "weint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Erdenwonnen keine", "tokens": ["Und", "der", "Er\u00b7den\u00b7won\u00b7nen", "kei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mich erfreuen kann:", "tokens": ["Mich", "er\u00b7freu\u00b7en", "kann", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "O erscheine", "tokens": ["O", "er\u00b7schei\u00b7ne"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Tr\u00f6stend mir, du engelreine,", "tokens": ["Tr\u00f6s\u00b7tend", "mir", ",", "du", "en\u00b7gel\u00b7rei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gottgeweihte Seele dann!", "tokens": ["Gott\u00b7ge\u00b7weih\u00b7te", "See\u00b7le", "dann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn ich m\u00fcd' und trostlos wanke,", "tokens": ["Wenn", "ich", "m\u00fcd'", "und", "trost\u00b7los", "wan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach! verkannt,", "tokens": ["Ach", "!", "ver\u00b7kannt", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "An des Kummers kalter Hand,", "tokens": ["An", "des", "Kum\u00b7mers", "kal\u00b7ter", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch verwachs'ne, wilde Haine,", "tokens": ["Durch", "ver\u00b7wachs'\u00b7ne", ",", "wil\u00b7de", "Hai\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ohne Stab und Bahn:", "tokens": ["Oh\u00b7ne", "Stab", "und", "Bahn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "O erscheine", "tokens": ["O", "er\u00b7schei\u00b7ne"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Leitend mir, du engelreine,", "tokens": ["Lei\u00b7tend", "mir", ",", "du", "en\u00b7gel\u00b7rei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PPER", "$,", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gottgeweihte Seele dann!", "tokens": ["Gott\u00b7ge\u00b7weih\u00b7te", "See\u00b7le", "dann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn mein Geist einst, gleich der Sonne", "tokens": ["Wenn", "mein", "Geist", "einst", ",", "gleich", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Goldnem Licht,", "tokens": ["Gold\u00b7nem", "Licht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Durch des Todes Wolke bricht,", "tokens": ["Durch", "des", "To\u00b7des", "Wol\u00b7ke", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und, da\u00df er sich dir vereine,", "tokens": ["Und", ",", "da\u00df", "er", "sich", "dir", "ver\u00b7ei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PRF", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Schimmert Himmelan:", "tokens": ["Schim\u00b7mert", "Him\u00b7me\u00b7lan", ":"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "O erscheine", "tokens": ["O", "er\u00b7schei\u00b7ne"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Liebend mir, du engelreine,", "tokens": ["Lie\u00b7bend", "mir", ",", "du", "en\u00b7gel\u00b7rei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PPER", "$,", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gottgeweihte Seele dann!", "tokens": ["Gott\u00b7ge\u00b7weih\u00b7te", "See\u00b7le", "dann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}