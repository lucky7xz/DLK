{"textgrid.poem.62721": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Sendschreiben an Ihro Hochgebohrenen Excellenz Herrn von Br\u00fchl", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.85", "fr:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "HeRR!", "tokens": ["HeRR", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.2": {"text": "ist es nicht zu viel, da\u00df eine fremde Hand,", "tokens": ["ist", "es", "nicht", "zu", "viel", ",", "da\u00df", "ei\u00b7ne", "frem\u00b7de", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PTKA", "PIS", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Da\u00df jetzt ein Weibesbild, so dir ganz unbekannt,", "tokens": ["Da\u00df", "jetzt", "ein", "Wei\u00b7bes\u00b7bild", ",", "so", "dir", "ganz", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "ADV", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Schreiben an Dich schickt? Und deine Staats-Gesch\u00e4fte", "tokens": ["Ein", "Schrei\u00b7ben", "an", "Dich", "schickt", "?", "Und", "dei\u00b7ne", "Staats\u00b7Ge\u00b7sch\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$.", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zu unterbrechen sucht? Ich kenne deine Kr\u00e4fte", "tokens": ["Zu", "un\u00b7ter\u00b7bre\u00b7chen", "sucht", "?", "Ich", "ken\u00b7ne", "dei\u00b7ne", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "VVFIN", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und deinen hohen Geist, und die Vollkommenheit,", "tokens": ["Und", "dei\u00b7nen", "ho\u00b7hen", "Geist", ",", "und", "die", "Voll\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So deine Seele schm\u00fcckt, wodurch du allezeit", "tokens": ["So", "dei\u00b7ne", "See\u00b7le", "schm\u00fcckt", ",", "wo\u00b7durch", "du", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$,", "PWAV", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wodurch ", "tokens": ["Wo\u00b7durch"], "token_info": ["word"], "pos": ["PWAV"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Dem ", "tokens": ["Dem"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Du hast den grossen Geist vom Anfang schon empfangen,", "tokens": ["Du", "hast", "den", "gros\u00b7sen", "Geist", "vom", "An\u00b7fang", "schon", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du bist von Kindheit an mit F\u00fcrsten umgegangen;", "tokens": ["Du", "bist", "von", "Kind\u00b7heit", "an", "mit", "F\u00fcrs\u00b7ten", "um\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "APPR", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So hast du fr\u00fch genug die Staats-Kunst eingesehn.", "tokens": ["So", "hast", "du", "fr\u00fch", "ge\u00b7nug", "die", "Staats\u00b7Kunst", "ein\u00b7ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Drum kan der K\u00f6nig auch mit seinem Br\u00fchl bestehn.", "tokens": ["Drum", "kan", "der", "K\u00f6\u00b7nig", "auch", "mit", "sei\u00b7nem", "Br\u00fchl", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wer kennet nicht dein Amt und deine raren Gaben,", "tokens": ["Wer", "ken\u00b7net", "nicht", "dein", "Amt", "und", "dei\u00b7ne", "ra\u00b7ren", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.16": {"text": "Du bist gelehrt und klug, drum hassest Du die Zunft", "tokens": ["Du", "bist", "ge\u00b7lehrt", "und", "klug", ",", "drum", "has\u00b7sest", "Du", "die", "Zunft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "ADJD", "$,", "PAV", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der M\u00e4nner, welche Witz, Kunst, Gaben und Vernunft,", "tokens": ["Der", "M\u00e4n\u00b7ner", ",", "wel\u00b7che", "Witz", ",", "Kunst", ",", "Ga\u00b7ben", "und", "Ver\u00b7nunft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAT", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dem weiblichen Geschlecht durchaus bestreiten wollen,", "tokens": ["Dem", "weib\u00b7li\u00b7chen", "Ge\u00b7schlecht", "durc\u00b7haus", "be\u00b7strei\u00b7ten", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und meinen, da\u00df wir nicht die Federn f\u00fchren sollen.", "tokens": ["Und", "mei\u00b7nen", ",", "da\u00df", "wir", "nicht", "die", "Fe\u00b7dern", "f\u00fch\u00b7ren", "sol\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dein Geist hat jederzeit das Frauenvolk geliebt,", "tokens": ["Dein", "Geist", "hat", "je\u00b7der\u00b7zeit", "das", "Frau\u00b7en\u00b7volk", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Das sich in Wissenschaft und edlen Sitten \u00fcbt;", "tokens": ["Das", "sich", "in", "Wis\u00b7sen\u00b7schaft", "und", "ed\u00b7len", "Sit\u00b7ten", "\u00fcbt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der Weisheit folgt, sie ehrt, und sucht galant zu leben.", "tokens": ["Der", "Weis\u00b7heit", "folgt", ",", "sie", "ehrt", ",", "und", "sucht", "ga\u00b7lant", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "$,", "KON", "VVFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Was meine Feder reitzt, da\u00df sie sich an Dich wagt,", "tokens": ["Was", "mei\u00b7ne", "Fe\u00b7der", "reitzt", ",", "da\u00df", "sie", "sich", "an", "Dich", "wagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und dir an meiner statt durch Verse etwas sagt.", "tokens": ["Und", "dir", "an", "mei\u00b7ner", "statt", "durch", "Ver\u00b7se", "et\u00b7was", "sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Drum, ", "tokens": ["Drum", ","], "token_info": ["word", "punct"], "pos": ["PAV", "$,"], "meter": "-", "measure": "single.down"}, "line.26": {"text": "Ich bitt gehorsamlich, du wollst mich nicht besch\u00e4men,", "tokens": ["Ich", "bitt", "ge\u00b7hor\u00b7sam\u00b7lich", ",", "du", "wollst", "mich", "nicht", "be\u00b7sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und mein Gesuch verschm\u00e4hn. Gewahr mir meiner Bitt,", "tokens": ["Und", "mein", "Ge\u00b7such", "ver\u00b7schm\u00e4hn", ".", "Ge\u00b7wahr", "mir", "mei\u00b7ner", "Bitt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$.", "NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Geh zu den K\u00f6nig hin/ und nimm das P\u00e4ckgen mit,", "tokens": ["Geh", "zu", "den", "K\u00f6\u00b7nig", "hin", "/", "und", "nimm", "das", "P\u00e4ck\u00b7gen", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKVZ", "$(", "KON", "VVIMP", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Das diesem Briefe folgt, und gieb es seinen H\u00e4nden,", "tokens": ["Das", "die\u00b7sem", "Brie\u00b7fe", "folgt", ",", "und", "gieb", "es", "sei\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PDAT", "NN", "VVFIN", "$,", "KON", "VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und bitt, Er woll sein Aug in Gnaden darauf wenden.", "tokens": ["Und", "bitt", ",", "Er", "woll", "sein", "Aug", "in", "Gna\u00b7den", "da\u00b7rauf", "wen\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Gnade, so du mir durch diesen Dienst erweist,", "tokens": ["Die", "Gna\u00b7de", ",", "so", "du", "mir", "durch", "die\u00b7sen", "Dienst", "er\u00b7weist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "PRF", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Werd ich mit Herz und Mund, Kiel, Dinte, Hand und Geist", "tokens": ["Werd", "ich", "mit", "Herz", "und", "Mund", ",", "Kiel", ",", "Din\u00b7te", ",", "Hand", "und", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN", "KON", "NN", "$,", "NE", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Bis an den letzten Hauch hoch zu verehren trachten.", "tokens": ["Bis", "an", "den", "letz\u00b7ten", "Hauch", "hoch", "zu", "ver\u00b7eh\u00b7ren", "trach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Du wollst mich deiner Gnad, die\u00df w\u00fcnsch ich, w\u00fcrdig achten.", "tokens": ["Du", "wollst", "mich", "dei\u00b7ner", "Gnad", ",", "die\u00df", "w\u00fcnsch", "ich", ",", "w\u00fcr\u00b7dig", "ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "PPER", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}