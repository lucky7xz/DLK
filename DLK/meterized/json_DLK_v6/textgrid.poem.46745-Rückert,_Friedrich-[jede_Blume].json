{"textgrid.poem.46745": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[jede Blume]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jede Blume,", "tokens": ["Je\u00b7de", "Blu\u00b7me", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Die du gern gesehn,", "tokens": ["Die", "du", "gern", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hab ich als ein", "tokens": ["Hab", "ich", "als", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "KOUS", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Weh zu \u00fcberstehn.", "tokens": ["Weh", "zu", "\u00fc\u00b7bers\u00b7tehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Jede mu\u00df ich", "tokens": ["Je\u00b7de", "mu\u00df", "ich"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VMFIN", "PPER"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Erst darauf ansehn,", "tokens": ["Erst", "da\u00b7rauf", "an\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wie sie jetzt sich", "tokens": ["Wie", "sie", "jetzt", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ausnimmt in den Wehn.", "tokens": ["Aus\u00b7nimmt", "in", "den", "Wehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Jede Knospe", "tokens": ["Je\u00b7de", "Knos\u00b7pe"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Scheint mich anzusehn,", "tokens": ["Scheint", "mich", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVIZU", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn sie ohne", "tokens": ["Wenn", "sie", "oh\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "APPR"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Dich mich her sieht gehn;", "tokens": ["Dich", "mich", "her", "sieht", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APZR", "VVFIN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ihrem Fragen", "tokens": ["Ih\u00b7rem", "Fra\u00b7gen"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Mu\u00df ich Rede stehn:", "tokens": ["Mu\u00df", "ich", "Re\u00b7de", "stehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Was ist deinem", "tokens": ["Was", "ist", "dei\u00b7nem"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VAFIN", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Lieben Kind geschehn?", "tokens": ["Lie\u00b7ben", "Kind", "ge\u00b7schehn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Diese Wasser,", "tokens": ["Die\u00b7se", "Was\u00b7ser", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Die durch Wiesen gehn,", "tokens": ["Die", "durch", "Wie\u00b7sen", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Jene Pfade,", "tokens": ["Je\u00b7ne", "Pfa\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Die am Berg sich drehn;", "tokens": ["Die", "am", "Berg", "sich", "drehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Alles mu\u00df mein", "tokens": ["Al\u00b7les", "mu\u00df", "mein"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VMFIN", "PPOSAT"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Auge neu bestehn,", "tokens": ["Au\u00b7ge", "neu", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wie es anders", "tokens": ["Wie", "es", "an\u00b7ders"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Aussieht in den Wehn.", "tokens": ["Aus\u00b7sieht", "in", "den", "Wehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}