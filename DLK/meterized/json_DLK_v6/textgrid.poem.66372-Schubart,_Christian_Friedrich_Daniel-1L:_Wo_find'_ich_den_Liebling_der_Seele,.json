{"textgrid.poem.66372": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo find' ich den Liebling der Seele,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo find' ich den Liebling der Seele,", "tokens": ["Wo", "find'", "ich", "den", "Lieb\u00b7ling", "der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Den Gott mir zum Manne erkor?", "tokens": ["Den", "Gott", "mir", "zum", "Man\u00b7ne", "er\u00b7kor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich s\u00e4ng' ihm mit schallender Kehle", "tokens": ["Ich", "s\u00e4ng'", "ihm", "mit", "schal\u00b7len\u00b7der", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dies Liedlein so gern in das Ohr!", "tokens": ["Dies", "Lied\u00b7lein", "so", "gern", "in", "das", "Ohr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "O k\u00e4m' er, wie wollt' ich ihm singen,", "tokens": ["O", "k\u00e4m'", "er", ",", "wie", "wollt'", "ich", "ihm", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PWAV", "VMFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dem Trauten, so lange umschlingen,", "tokens": ["Dem", "Trau\u00b7ten", ",", "so", "lan\u00b7ge", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Bis innig er's f\u00fchlte, wie ich,", "tokens": ["Bis", "in\u00b7nig", "er's", "f\u00fchl\u00b7te", ",", "wie", "ich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PIS", "VVFIN", "$,", "PWAV", "PPER", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Gott hab' ihn geschaffen f\u00fcr mich.", "tokens": ["Gott", "hab'", "ihn", "ge\u00b7schaf\u00b7fen", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "APPR", "PPER", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "J\u00fcngst sa\u00df ich, vom Monde beschienen,", "tokens": ["J\u00fcngst", "sa\u00df", "ich", ",", "vom", "Mon\u00b7de", "be\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Am Bettlein so einsam, so leer;", "tokens": ["Am", "Bet\u00b7tlein", "so", "ein\u00b7sam", ",", "so", "leer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da sah ich mit freundlichen Mienen", "tokens": ["Da", "sah", "ich", "mit", "freund\u00b7li\u00b7chen", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Den J\u00fcngling, wie Hermann war er.", "tokens": ["Den", "J\u00fcng\u00b7ling", ",", "wie", "Her\u00b7mann", "war", "er", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "NE", "VAFIN", "PPER", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Es flammte der himmlische Zunder", "tokens": ["Es", "flamm\u00b7te", "der", "himm\u00b7li\u00b7sche", "Zun\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Der Liebe die Augen herunter,", "tokens": ["Der", "Lie\u00b7be", "die", "Au\u00b7gen", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Hoch, schlank, nicht zu weich, nicht zu wild,", "tokens": ["Hoch", ",", "schlank", ",", "nicht", "zu", "weich", ",", "nicht", "zu", "wild", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$,", "PTKNEG", "PTKA", "ADJD", "$,", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "War meines Erw\u00e4hlten Gebild.", "tokens": ["War", "mei\u00b7nes", "Er\u00b7w\u00e4hl\u00b7ten", "Ge\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Auch wallte die br\u00e4unliche Locke", "tokens": ["Auch", "wall\u00b7te", "die", "br\u00e4un\u00b7li\u00b7che", "Lo\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Dem J\u00fcngling ins sch\u00f6ne Gesicht.", "tokens": ["Dem", "J\u00fcng\u00b7ling", "ins", "sch\u00f6\u00b7ne", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Er redte, die silberne Glocke", "tokens": ["Er", "red\u00b7te", ",", "die", "sil\u00b7ber\u00b7ne", "Glo\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ert\u00f6net so lieblich mir nicht.", "tokens": ["Er\u00b7t\u00f6\u00b7net", "so", "lieb\u00b7lich", "mir", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PPER", "PTKNEG", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Bald flie\u00dfen, so sprach sie, die Flammen", "tokens": ["Bald", "flie\u00b7\u00dfen", ",", "so", "sprach", "sie", ",", "die", "Flam\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVFIN", "PPER", "$,", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Der Herzen in einem zusammen;", "tokens": ["Der", "Her\u00b7zen", "in", "ei\u00b7nem", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Mit m\u00e4chtigem Drange f\u00fchl's ich,", "tokens": ["Mit", "m\u00e4ch\u00b7ti\u00b7gem", "Dran\u00b7ge", "f\u00fchl's", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Gott hab' ihn geschaffen f\u00fcr mich.", "tokens": ["Gott", "hab'", "ihn", "ge\u00b7schaf\u00b7fen", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "APPR", "PPER", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Doch harre, die br\u00e4utliche Stunde,", "tokens": ["Doch", "har\u00b7re", ",", "die", "br\u00e4ut\u00b7li\u00b7che", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Bald steigt sie von Osten herauf,", "tokens": ["Bald", "steigt", "sie", "von", "Os\u00b7ten", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und dr\u00fcckt deinem gl\u00fchenden Munde", "tokens": ["Und", "dr\u00fcckt", "dei\u00b7nem", "gl\u00fc\u00b7hen\u00b7den", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die K\u00fcsse des Br\u00e4utigams auf.", "tokens": ["Die", "K\u00fcs\u00b7se", "des", "Br\u00e4u\u00b7ti\u00b7gams", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NE", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Ach ende, du Traute, das Sehnen", "tokens": ["Ach", "en\u00b7de", ",", "du", "Trau\u00b7te", ",", "das", "Seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "NN", "$,", "PPER", "NN", "$,", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Des Herzens, und spare die Thr\u00e4nen;", "tokens": ["Des", "Her\u00b7zens", ",", "und", "spa\u00b7re", "die", "Thr\u00e4\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Denn alles das Deine ist mein,", "tokens": ["Denn", "al\u00b7les", "das", "Dei\u00b7ne", "ist", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "PPOSAT", "VAFIN", "PPOSAT", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Und alles das Meine ist dein.", "tokens": ["Und", "al\u00b7les", "das", "Mei\u00b7ne", "ist", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Ich bebte, ich schwamm in Entz\u00fccken,", "tokens": ["Ich", "beb\u00b7te", ",", "ich", "schwamm", "in", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ich wagt' es mit bebender Hand,", "tokens": ["Ich", "wagt'", "es", "mit", "be\u00b7ben\u00b7der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den J\u00fcngling an Busen zu dr\u00fccken,", "tokens": ["Den", "J\u00fcng\u00b7ling", "an", "Bu\u00b7sen", "zu", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Doch, ach! die Erscheinung verschwand.", "tokens": ["Doch", ",", "ach", "!", "die", "Er\u00b7schei\u00b7nung", "ver\u00b7schwand", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Wo bist du nun, heiliger Schatten", "tokens": ["Wo", "bist", "du", "nun", ",", "hei\u00b7li\u00b7ger", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Des Trauten, des z\u00e4rtlichen Gatten?", "tokens": ["Des", "Trau\u00b7ten", ",", "des", "z\u00e4rt\u00b7li\u00b7chen", "Gat\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Dein k\u00fcnftiges Weibchen sitzt hier,", "tokens": ["Dein", "k\u00fcnf\u00b7ti\u00b7ges", "Weib\u00b7chen", "sitzt", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und schmachtet vergeblich nach dir.", "tokens": ["Und", "schmach\u00b7tet", "ver\u00b7geb\u00b7lich", "nach", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}