{"textgrid.poem.40244": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun wei\u00dft du, Herz, was immer so", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun wei\u00dft du, Herz, was immer so", "tokens": ["Nun", "wei\u00dft", "du", ",", "Herz", ",", "was", "im\u00b7mer", "so"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "PRELS", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "in deinen W\u00fcnschen bangt und gl\u00fcht,", "tokens": ["in", "dei\u00b7nen", "W\u00fcn\u00b7schen", "bangt", "und", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wie nach dem ersten Sonnenschimmer", "tokens": ["wie", "nach", "dem", "ers\u00b7ten", "Son\u00b7nen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die graue Nacht verlangt und gl\u00fcht,", "tokens": ["die", "grau\u00b7e", "Nacht", "ver\u00b7langt", "und", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und was in deinen L\u00fcsten", "tokens": ["und", "was", "in", "dei\u00b7nen", "L\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "nach Seele d\u00fcrstet wie nach Blut,", "tokens": ["nach", "See\u00b7le", "d\u00fcrs\u00b7tet", "wie", "nach", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und was dich jagt von Herz zu Herz", "tokens": ["und", "was", "dich", "jagt", "von", "Herz", "zu", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "VVFIN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "aus dumpfer Sucht zu lichter Glut.", "tokens": ["aus", "dum\u00b7pfer", "Sucht", "zu", "lich\u00b7ter", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In fr\u00fcher Morgenstunde", "tokens": ["In", "fr\u00fc\u00b7her", "Mor\u00b7gen\u00b7stun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "hielt heut mein Alb mich schwer umstrickt:", "tokens": ["hielt", "heut", "mein", "Alb", "mich", "schwer", "um\u00b7strickt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "aus meinem Herzen wuchs ein Baum,", "tokens": ["aus", "mei\u00b7nem", "Her\u00b7zen", "wuchs", "ein", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "o wie er dr\u00fcckt! er schwankt und nickt;", "tokens": ["o", "wie", "er", "dr\u00fcckt", "!", "er", "schwankt", "und", "nickt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "KON", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "sein seltsam Laubwerk thut sich auf,", "tokens": ["sein", "selt\u00b7sam", "Laub\u00b7werk", "thut", "sich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und aus den d\u00fcstern Zweigen rauscht", "tokens": ["und", "aus", "den", "d\u00fcs\u00b7tern", "Zwei\u00b7gen", "rauscht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "mit gro\u00dfen hei\u00dfen Augen", "tokens": ["mit", "gro\u00b7\u00dfen", "hei\u00b7\u00dfen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "ein junges Vampyrweib \u2013 und lauscht.", "tokens": ["ein", "jun\u00b7ges", "Vam\u00b7py\u00b7rweib", "\u2013", "und", "lauscht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da kam genaht und ist schon da", "tokens": ["Da", "kam", "ge\u00b7naht", "und", "ist", "schon", "da"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "VVPP", "KON", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Apoll im Sonnenwagen;", "tokens": ["A\u00b7poll", "im", "Son\u00b7nen\u00b7wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "es flammt sein Blick den Baum hinan,", "tokens": ["es", "flammt", "sein", "Blick", "den", "Baum", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die Vampyrbraut genie\u00dft den Bann", "tokens": ["die", "Vam\u00b7pyr\u00b7braut", "ge\u00b7nie\u00dft", "den", "Bann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "mit d\u00fcrstendem Behagen.", "tokens": ["mit", "d\u00fcrs\u00b7ten\u00b7dem", "Be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es sehnt sein Arm sich wild empor,", "tokens": ["Es", "sehnt", "sein", "Arm", "sich", "wild", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "vier Augen leuchten trunken;", "tokens": ["vier", "Au\u00b7gen", "leuch\u00b7ten", "trun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "das Nachtweib und der Sonnenf\u00fcrst,", "tokens": ["das", "Nacht\u00b7weib", "und", "der", "Son\u00b7nen\u00b7f\u00fcrst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.9": {"text": "sie liegen hingesunken.", "tokens": ["sie", "lie\u00b7gen", "hin\u00b7ge\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es pre\u00dft mein Herz die schwere Last", "tokens": ["Es", "pre\u00dft", "mein", "Herz", "die", "schwe\u00b7re", "Last"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der \u00fcppigen Sekunden,", "tokens": ["der", "\u00fcp\u00b7pi\u00b7gen", "Se\u00b7kun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "es stampft auf mir der Rosse Hast \u2013", "tokens": ["es", "stampft", "auf", "mir", "der", "Ros\u00b7se", "Hast", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "er hat sich ihr entwunden.", "tokens": ["er", "hat", "sich", "ihr", "ent\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schon schwillt ihr Bauch von seiner Frucht,", "tokens": ["Schon", "schwillt", "ihr", "Bauch", "von", "sei\u00b7ner", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "hohl fleht ihr Auge: bleibe!", "tokens": ["hohl", "fleht", "ihr", "Au\u00b7ge", ":", "blei\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Er st\u00f6\u00dft sie sich vom Leibe,", "tokens": ["Er", "st\u00f6\u00dft", "sie", "sich", "vom", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "von Ekel zuckt des Fu\u00dfes Wucht,", "tokens": ["von", "E\u00b7kel", "zuckt", "des", "Fu\u00b7\u00dfes", "Wucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "hin ras't des Wagens goldne Flucht.", "tokens": ["hin", "ras't", "des", "Wa\u00b7gens", "gold\u00b7ne", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es windet sich im Krampfe", "tokens": ["Es", "win\u00b7det", "sich", "im", "Kramp\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und st\u00f6hnt das graue Mutterweib,", "tokens": ["und", "st\u00f6hnt", "das", "grau\u00b7e", "Mut\u00b7ter\u00b7weib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "mit ihren Vampyrfingern gr\u00e4bt", "tokens": ["mit", "ih\u00b7ren", "Vam\u00b7pyr\u00b7fin\u00b7gern", "gr\u00e4bt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sie sich den Lichtsohn aus dem Leib,", "tokens": ["sie", "sich", "den", "Licht\u00b7sohn", "aus", "dem", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "er \u00e4chzt \u2013 ein Schrei \u2013 Erbarmen: ich,", "tokens": ["er", "\u00e4chzt", "\u2013", "ein", "Schrei", "\u2013", "Er\u00b7bar\u00b7men", ":", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ART", "NN", "$(", "NN", "$.", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "da bin ich wach \u2013\u2013doch h\u00f6r'ich,", "tokens": ["da", "bin", "ich", "wach", "\u2013", "\u2013", "doch", "h\u00f6r'\u00b7ich", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$(", "$(", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "wie noch ihr Fluch und Segen hallt:", "tokens": ["wie", "noch", "ihr", "Fluch", "und", "Se\u00b7gen", "hallt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Drum sollst du dulden dies dein Herz,", "tokens": ["Drum", "sollst", "du", "dul\u00b7den", "dies", "dein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "VVINF", "PDS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das so von W\u00fcnschen bangt und gl\u00fcht,", "tokens": ["das", "so", "von", "W\u00fcn\u00b7schen", "bangt", "und", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wie nach dem ersten Sonnenschimmer", "tokens": ["wie", "nach", "dem", "ers\u00b7ten", "Son\u00b7nen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die graue Nacht verlangt und gl\u00fcht,", "tokens": ["die", "grau\u00b7e", "Nacht", "ver\u00b7langt", "und", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und sollst in deinen L\u00fcsten", "tokens": ["und", "sollst", "in", "dei\u00b7nen", "L\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "nach Seele d\u00fcrsten wie nach Blut,", "tokens": ["nach", "See\u00b7le", "d\u00fcrs\u00b7ten", "wie", "nach", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und sollst dich m\u00fchn von Herz zu Herz", "tokens": ["und", "sollst", "dich", "m\u00fchn", "von", "Herz", "zu", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PRF", "ADJD", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "aus dumpfer Sucht zu lichter Glut!", "tokens": ["aus", "dum\u00b7pfer", "Sucht", "zu", "lich\u00b7ter", "Glut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}