{"textgrid.poem.60154": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn es den Erdeng\u00f6ttern einst gef\u00e4llt", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn es den Erdeng\u00f6ttern einst gef\u00e4llt", "tokens": ["Wenn", "es", "den", "Er\u00b7den\u00b7g\u00f6t\u00b7tern", "einst", "ge\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von ihrem Thronensiz herabzusteigen,", "tokens": ["Von", "ih\u00b7rem", "Thro\u00b7nen\u00b7siz", "her\u00b7ab\u00b7zu\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ohne Purpur sich des Volkes Blik zu zeigen,", "tokens": ["Und", "oh\u00b7ne", "Pur\u00b7pur", "sich", "des", "Vol\u00b7kes", "Blik", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PRF", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dann werden sie die Lust der Welt!", "tokens": ["Dann", "wer\u00b7den", "sie", "die", "Lust", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So spielt in goldnen Zeiten Friederich", "tokens": ["So", "spielt", "in", "gold\u00b7nen", "Zei\u00b7ten", "Frie\u00b7de\u00b7rich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu Sansoucis von allem Volk geh\u00f6ret,", "tokens": ["Zu", "San\u00b7sou\u00b7cis", "von", "al\u00b7lem", "Volk", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein g\u00f6ttlich Fl\u00f6tenspiel, das ihn Apoll gelehret:", "tokens": ["Sein", "g\u00f6tt\u00b7lich", "Fl\u00f6\u00b7ten\u00b7spiel", ",", "das", "ihn", "A\u00b7poll", "ge\u00b7leh\u00b7ret", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PRELS", "PPER", "NE", "VVPP", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und alle Welt erg\u00f6tzet sich!", "tokens": ["Und", "al\u00b7le", "Welt", "er\u00b7g\u00f6t\u00b7zet", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So sassest du erhabne K\u00f6nigin", "tokens": ["So", "sas\u00b7sest", "du", "er\u00b7hab\u00b7ne", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf ausgeh\u00f6ltem Holtze ganze Stunden,", "tokens": ["Auf", "aus\u00b7ge\u00b7h\u00f6l\u00b7tem", "Holt\u00b7ze", "gan\u00b7ze", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und fuhrest ohne Stolz, den nie dein Herz empfunden", "tokens": ["Und", "fuh\u00b7rest", "oh\u00b7ne", "Stolz", ",", "den", "nie", "dein", "Herz", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor deines Volkes Blick dahin!", "tokens": ["Vor", "dei\u00b7nes", "Vol\u00b7kes", "Blick", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die leichten Wimpel wehten \u00fcber dir,", "tokens": ["Die", "leich\u00b7ten", "Wim\u00b7pel", "weh\u00b7ten", "\u00fc\u00b7ber", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie Fahnen die ein Triumphirer bringet", "tokens": ["Wie", "Fah\u00b7nen", "die", "ein", "Tri\u00b7um\u00b7phi\u00b7rer", "brin\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom Felde, wo sein Arm den Feind bezwang; so zwinget", "tokens": ["Vom", "Fel\u00b7de", ",", "wo", "sein", "Arm", "den", "Feind", "be\u00b7zwang", ";", "so", "zwin\u00b7get"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der starke L\u00f6w, ein Panterthier!", "tokens": ["Der", "star\u00b7ke", "L\u00f6w", ",", "ein", "Pan\u00b7ter\u00b7thier", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Dichtkunst Schwester, die Music, erscholl", "tokens": ["Der", "Dicht\u00b7kunst", "Schwes\u00b7ter", ",", "die", "Mu\u00b7sic", ",", "er\u00b7scholl"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In sanftged\u00e4mpftem feinem s\u00fcssem Thone;", "tokens": ["In", "sanft\u00b7ge\u00b7d\u00e4mpf\u00b7tem", "fei\u00b7nem", "s\u00fcs\u00b7sem", "Tho\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und du vergassest ganz den Glanz der K\u00f6nigs Crone", "tokens": ["Und", "du", "ver\u00b7gas\u00b7sest", "ganz", "den", "Glanz", "der", "K\u00f6\u00b7nigs", "Cro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und warest sanfter Freuden voll!", "tokens": ["Und", "wa\u00b7rest", "sanf\u00b7ter", "Freu\u00b7den", "voll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Sonnen Antlitz, unverschleyert sch\u00f6n,", "tokens": ["Der", "Son\u00b7nen", "Ant\u00b7litz", ",", "un\u00b7ver\u00b7schley\u00b7ert", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sah auf dein kleines Schiff mit unverwandten", "tokens": ["Sah", "auf", "dein", "klei\u00b7nes", "Schiff", "mit", "un\u00b7ver\u00b7wand\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und strahlenreichen Blicken, streute Diamanten", "tokens": ["Und", "strah\u00b7len\u00b7rei\u00b7chen", "Bli\u00b7cken", ",", "streu\u00b7te", "Di\u00b7a\u00b7man\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und lie\u00df die Fahrt durch Silber gehn.", "tokens": ["Und", "lie\u00df", "die", "Fahrt", "durch", "Sil\u00b7ber", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Elbe f\u00fchlte Vorzug, nennte klein", "tokens": ["Die", "El\u00b7be", "f\u00fchl\u00b7te", "Vor\u00b7zug", ",", "nenn\u00b7te", "klein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NE", "VVFIN", "NN", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des deutschen Reiches stolzere Gew\u00e4sser;", "tokens": ["Des", "deut\u00b7schen", "Rei\u00b7ches", "stol\u00b7ze\u00b7re", "Ge\u00b7w\u00e4s\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Beschift, Holdseeligste, von Dir, d\u00fcnkt sie sich besser", "tokens": ["Be\u00b7schift", ",", "Hold\u00b7see\u00b7ligs\u00b7te", ",", "von", "Dir", ",", "d\u00fcnkt", "sie", "sich", "bes\u00b7ser"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "APPR", "PPER", "$,", "VVFIN", "PPER", "PRF", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und edler, als der breite Rhein.", "tokens": ["Und", "ed\u00b7ler", ",", "als", "der", "brei\u00b7te", "Rhein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "KOUS", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Unsichtbar hieng ein ganzes Nimphen Chor", "tokens": ["Un\u00b7sicht\u00b7bar", "hieng", "ein", "gan\u00b7zes", "Nim\u00b7phen", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Rund um das Schiff, und wollte sich erfrischen", "tokens": ["Rund", "um", "das", "Schiff", ",", "und", "woll\u00b7te", "sich", "er\u00b7fri\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "KON", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An Luft, die dich gek\u00fchlt: und selbst den kalten Fischen", "tokens": ["An", "Luft", ",", "die", "dich", "ge\u00b7k\u00fchlt", ":", "und", "selbst", "den", "kal\u00b7ten", "Fi\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVPP", "$.", "KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hub Ehrfurcht ihren Kopf empor!", "tokens": ["Hub", "Ehr\u00b7furcht", "ih\u00b7ren", "Kopf", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "An deiner linken Seite sassen zwo", "tokens": ["An", "dei\u00b7ner", "lin\u00b7ken", "Sei\u00b7te", "sas\u00b7sen", "zwo"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "CARD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gleich holde Wesen; Aehnlichkeit der Z\u00fcge", "tokens": ["Gleich", "hol\u00b7de", "We\u00b7sen", ";", "A\u00b7ehn\u00b7lich\u00b7keit", "der", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$.", "NN", "ART", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Verrieth sie mir; Ihr Herz war \u00fcber neue Siege", "tokens": ["Ver\u00b7rieth", "sie", "mir", ";", "Ihr", "Herz", "war", "\u00fc\u00b7ber", "neu\u00b7e", "Sie\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$.", "PPOSAT", "NN", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Helden, deines Bruders froh.", "tokens": ["Des", "Hel\u00b7den", ",", "dei\u00b7nes", "Bru\u00b7ders", "froh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Er schleudert Schrecken, Niedersturz und Flucht,", "tokens": ["Er", "schleu\u00b7dert", "Schre\u00b7cken", ",", "Nie\u00b7der\u00b7sturz", "und", "Flucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In Franckreichs Heere; l\u00e4sset Blitze schiessen,", "tokens": ["In", "Fran\u00b7ck\u00b7reichs", "Hee\u00b7re", ";", "l\u00e4s\u00b7set", "Blit\u00b7ze", "schies\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$.", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bis Ludewig, dem er die Lilien zerrissen,", "tokens": ["Bis", "Lu\u00b7de\u00b7wig", ",", "dem", "er", "die", "Li\u00b7li\u00b7en", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem\u00fcthig wird, und Frieden sucht.", "tokens": ["De\u00b7m\u00fct\u00b7hig", "wird", ",", "und", "Frie\u00b7den", "sucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Er kommt der Friede von des Himmels H\u00f6h.", "tokens": ["Er", "kommt", "der", "Frie\u00b7de", "von", "des", "Him\u00b7mels", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dich, F\u00fcrstin, wird an stillen Sommertagen", "tokens": ["Dich", ",", "F\u00fcrs\u00b7tin", ",", "wird", "an", "stil\u00b7len", "Som\u00b7mer\u00b7ta\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vor Friedrichs Angesicht ein goldnes Fahrzeug tragen", "tokens": ["Vor", "Fried\u00b7richs", "An\u00b7ge\u00b7sicht", "ein", "gold\u00b7nes", "Fahr\u00b7zeug", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf wieder stolz gewordner Spree.", "tokens": ["Auf", "wie\u00b7der", "stolz", "ge\u00b7word\u00b7ner", "Spree", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}