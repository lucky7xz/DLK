{"textgrid.poem.36057": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "XvIII. Der Nacht-Ritt.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie stellt voll bunten Glanzes sich dem das Leben dar,", "tokens": ["Wie", "stellt", "voll", "bun\u00b7ten", "Glan\u00b7zes", "sich", "dem", "das", "Le\u00b7ben", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADJD", "ADJA", "NN", "PRF", "ART", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Der L\u00e4nder mag durchreisen, frei, jeder Sorge baar!", "tokens": ["Der", "L\u00e4n\u00b7der", "mag", "durc\u00b7hrei\u00b7sen", ",", "frei", ",", "je\u00b7der", "Sor\u00b7ge", "baar", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,", "ADJD", "$,", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es wird solch heitrem Pilger die Welt zum Eigenthum;", "tokens": ["Es", "wird", "solch", "heit\u00b7rem", "Pil\u00b7ger", "die", "Welt", "zum", "Ei\u00b7gen\u00b7thum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Oft ungeahnet findet er ein Elysium.", "tokens": ["Oft", "un\u00b7ge\u00b7ah\u00b7net", "fin\u00b7det", "er", "ein", "E\u00b7ly\u00b7si\u00b7um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Auf hohen Bergen steht er, die Arme breitend weit,", "tokens": ["Auf", "ho\u00b7hen", "Ber\u00b7gen", "steht", "er", ",", "die", "Ar\u00b7me", "brei\u00b7tend", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die L\u00e4nder ruhen drunten in stiller Herrlichkeit.", "tokens": ["Die", "L\u00e4n\u00b7der", "ru\u00b7hen", "drun\u00b7ten", "in", "stil\u00b7ler", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da darf die Brust sich heben so lebenvoll, so reich,", "tokens": ["Da", "darf", "die", "Brust", "sich", "he\u00b7ben", "so", "le\u00b7ben\u00b7voll", ",", "so", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Seele w\u00fcnscht zu schweben dem Felsen-Adler gleich.", "tokens": ["Die", "See\u00b7le", "w\u00fcnscht", "zu", "schwe\u00b7ben", "dem", "Fel\u00b7sen\u00b7Ad\u00b7ler", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Nie ringt ein Wunsch so m\u00e4chtig sich von dem Herzen los,", "tokens": ["Nie", "ringt", "ein", "Wunsch", "so", "m\u00e4ch\u00b7tig", "sich", "von", "dem", "Her\u00b7zen", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Als, wenn die Sch\u00f6pfung pr\u00e4chtig vor Augen liegt, und gross,", "tokens": ["Als", ",", "wenn", "die", "Sch\u00f6p\u00b7fung", "pr\u00e4ch\u00b7tig", "vor", "Au\u00b7gen", "liegt", ",", "und", "gross", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "ART", "NN", "ADJD", "APPR", "NN", "VVFIN", "$,", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Wunsch: auf Zauberfl\u00fcgeln, wie Wolken, hinzuziehn,", "tokens": ["Der", "Wunsch", ":", "auf", "Zau\u00b7ber\u00b7fl\u00fc\u00b7geln", ",", "wie", "Wol\u00b7ken", ",", "hin\u00b7zu\u00b7ziehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$.", "APPR", "NN", "$,", "PWAV", "NN", "$,", "VVIZU", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hoch \u00fcber Bergen, H\u00fcgeln und sanfter Th\u00e4ler Gr\u00fcn. \u2013", "tokens": ["Hoch", "\u00fc\u00b7ber", "Ber\u00b7gen", ",", "H\u00fc\u00b7geln", "und", "sanf\u00b7ter", "Th\u00e4\u00b7ler", "Gr\u00fcn", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "NN", "KON", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "In Erfurt h\u00e4lt ein Gastmahl ein Edler vom Geschlecht;", "tokens": ["In", "Er\u00b7furt", "h\u00e4lt", "ein", "Gast\u00b7mahl", "ein", "Ed\u00b7ler", "vom", "Ge\u00b7schlecht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Schon haben manche Stunde die G\u00e4ste froh gezecht;", "tokens": ["Schon", "ha\u00b7ben", "man\u00b7che", "Stun\u00b7de", "die", "G\u00e4s\u00b7te", "froh", "ge\u00b7zecht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Oft denken sie des Magus, der Allen wohlbekannt,", "tokens": ["Oft", "den\u00b7ken", "sie", "des", "Ma\u00b7gus", ",", "der", "Al\u00b7len", "wohl\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Oft wird ein Wunsch nach Faustus zur Ferne hingesandt.", "tokens": ["Oft", "wird", "ein", "Wunsch", "nach", "Faus\u00b7tus", "zur", "Fer\u00b7ne", "hin\u00b7ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NE", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Der aber weilet fern jetzt, in Prag und hoch verehrt,", "tokens": ["Der", "a\u00b7ber", "wei\u00b7let", "fern", "jetzt", ",", "in", "Prag", "und", "hoch", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADJD", "ADV", "$,", "APPR", "NE", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wo sich sein Zauberruhmkranz mit mancher Bl\u00fcthe mehrt.", "tokens": ["Wo", "sich", "sein", "Zau\u00b7ber\u00b7ruhm\u00b7kranz", "mit", "man\u00b7cher", "Bl\u00fc\u00b7the", "mehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "---+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es str\u00f6met ihm die F\u00fclle prachtreicher Gaben zu,", "tokens": ["Es", "str\u00f6\u00b7met", "ihm", "die", "F\u00fcl\u00b7le", "prach\u00b7trei\u00b7cher", "Ga\u00b7ben", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er findet Ruhm, doch nimmer die heiss ersehnte Ruh.", "tokens": ["Er", "fin\u00b7det", "Ruhm", ",", "doch", "nim\u00b7mer", "die", "heiss", "er\u00b7sehn\u00b7te", "Ruh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "ADV", "ADV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Du Seelen-Schaukelbettlein, Du weiche Mutterbrust,", "tokens": ["Du", "See\u00b7len\u00b7Schau\u00b7kel\u00b7bett\u00b7lein", ",", "Du", "wei\u00b7che", "Mut\u00b7ter\u00b7brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Du Himmelstraum voll Engel und Paradieseslust!", "tokens": ["Du", "Him\u00b7mel\u00b7straum", "voll", "En\u00b7gel", "und", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Du, ", "tokens": ["Du", ","], "token_info": ["word", "punct"], "pos": ["PPER", "$,"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Soll sich nicht gl\u00fccklich preisen, so lang' er Dich vermisst! \u2013", "tokens": ["Soll", "sich", "nicht", "gl\u00fcck\u00b7lich", "prei\u00b7sen", ",", "so", "lang'", "er", "Dich", "ver\u00b7misst", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PRF", "PTKNEG", "ADJD", "VVFIN", "$,", "ADV", "ADV", "PPER", "PPER", "VVFIN", "$.", "$("], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.7": {"line.1": {"text": "Es hebt der Wirth den Becher, und ruft: \u00bbDir bring' ich's, Faust!", "tokens": ["Es", "hebt", "der", "Wirth", "den", "Be\u00b7cher", ",", "und", "ruft", ":", "\u00bb", "Dir", "bring'", "ich's", ",", "Faust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,", "KON", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PIS", "$,", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der fern von seinen alten, bei neuen Freunden haust!", "tokens": ["Der", "fern", "von", "sei\u00b7nen", "al\u00b7ten", ",", "bei", "neu\u00b7en", "Freun\u00b7den", "haust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "ADJA", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dein Wohlsein, und vern\u00e4hmest Du meine W\u00fcnsche dort,", "tokens": ["Dein", "Wohl\u00b7sein", ",", "und", "ver\u00b7n\u00e4h\u00b7mest", "Du", "mei\u00b7ne", "W\u00fcn\u00b7sche", "dort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "So w\u00e4rst Du klug und k\u00e4mest in unsern Kreis sofort!\u00ab", "tokens": ["So", "w\u00e4rst", "Du", "klug", "und", "k\u00e4\u00b7mest", "in", "un\u00b7sern", "Kreis", "so\u00b7fort", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Faust sieht im Zauberspiegel der fernen Freunde Schaar,", "tokens": ["Faust", "sieht", "im", "Zau\u00b7ber\u00b7spie\u00b7gel", "der", "fer\u00b7nen", "Freun\u00b7de", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und ihren Wunsch vernimmt er, und ruft Pr\u00e4stigiar,", "tokens": ["Und", "ih\u00b7ren", "Wunsch", "ver\u00b7nimmt", "er", ",", "und", "ruft", "Pr\u00e4s\u00b7ti\u00b7gi\u00b7ar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Und fliegt auf dunkelm Nachtross, wie Klinsor einst gethan,", "tokens": ["Und", "fliegt", "auf", "dun\u00b7kelm", "Nach\u00b7tross", ",", "wie", "Klin\u00b7sor", "einst", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,", "PWAV", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Als er zum Sangeskampfe nach Wartburg zog heran.", "tokens": ["Als", "er", "zum", "San\u00b7ges\u00b7kamp\u00b7fe", "nach", "Wart\u00b7burg", "zog", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Es hat nicht lang' den Becher der heitre Wirth geleert,", "tokens": ["Es", "hat", "nicht", "lang'", "den", "Be\u00b7cher", "der", "heit\u00b7re", "Wirth", "ge\u00b7leert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+----+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Da wird ein lautes Pochen am Hofthor schon geh\u00f6rt.", "tokens": ["Da", "wird", "ein", "lau\u00b7tes", "Po\u00b7chen", "am", "Hof\u00b7thor", "schon", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+--++--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein Reiter steigt vom Schwarzross; der Diener \u00f6ffnet schnell;", "tokens": ["Ein", "Rei\u00b7ter", "steigt", "vom", "Schwarz\u00b7ross", ";", "der", "Die\u00b7ner", "\u00f6ff\u00b7net", "schnell", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$.", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "\u00bbgeh, gr\u00fcsse Deine Herrschaft vom Faustus! Flink Gesell!\u00ab", "tokens": ["\u00bb", "geh", ",", "gr\u00fcs\u00b7se", "Dei\u00b7ne", "Herr\u00b7schaft", "vom", "Faus\u00b7tus", "!", "Flink", "Ge\u00b7sell", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$.", "NN", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Der Diener eilt ins Zimmer, thut Meldung von dem Gast,", "tokens": ["Der", "Die\u00b7ner", "eilt", "ins", "Zim\u00b7mer", ",", "thut", "Mel\u00b7dung", "von", "dem", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Drob Staunen und Bewundrung den Freundeskreis erfasst.", "tokens": ["Drob", "Stau\u00b7nen", "und", "Be\u00b7wund\u00b7rung", "den", "Freun\u00b7des\u00b7kreis", "er\u00b7fasst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dem Hausherrn d\u00fcnkt unglaublich des Knechts Bericht zu sein,", "tokens": ["Dem", "Haus\u00b7herrn", "d\u00fcnkt", "un\u00b7glaub\u00b7lich", "des", "Knechts", "Be\u00b7richt", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "NN", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da tritt, den er verk\u00fcndet, der Zauberer herein.", "tokens": ["Da", "tritt", ",", "den", "er", "ver\u00b7k\u00fcn\u00b7det", ",", "der", "Zau\u00b7be\u00b7rer", "her\u00b7ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "\u00bbdein Wunsch ist hingedrungen zu mir, wie fern ich war,", "tokens": ["\u00bb", "dein", "Wunsch", "ist", "hin\u00b7ge\u00b7drun\u00b7gen", "zu", "mir", ",", "wie", "fern", "ich", "war", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADV", "APPR", "PPER", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gleich hab' ich mich geschwungen auf den Pr\u00e4stigiar;", "tokens": ["Gleich", "hab'", "ich", "mich", "ge\u00b7schwun\u00b7gen", "auf", "den", "Pr\u00e4s\u00b7ti\u00b7gi\u00b7ar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Der trug zum Ross verwandelt mich windschnell durch die Nacht,", "tokens": ["Der", "trug", "zum", "Ross", "ver\u00b7wan\u00b7delt", "mich", "wind\u00b7schnell", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ich dachte gern der Freunde, die mein so treu gedacht!\u00ab", "tokens": ["Ich", "dach\u00b7te", "gern", "der", "Freun\u00b7de", ",", "die", "mein", "so", "treu", "ge\u00b7dacht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPOSAT", "ADV", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "\u00bbund mag uns nun erg\u00f6tzen ein lustger Zauberschwank,", "tokens": ["\u00bb", "und", "mag", "uns", "nun", "er\u00b7g\u00f6t\u00b7zen", "ein", "lust\u00b7ger", "Zau\u00b7ber\u00b7schwank", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VMFIN", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zum alten Wein des Gastwirths bring' ich Euch neuen Trank.", "tokens": ["Zum", "al\u00b7ten", "Wein", "des", "Gast\u00b7wirths", "bring'", "ich", "Euch", "neu\u00b7en", "Trank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "PPER", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Quell des Traubenblutes soll springen aus dem Tisch!\u00ab", "tokens": ["Der", "Quell", "des", "Trau\u00b7ben\u00b7blu\u00b7tes", "soll", "sprin\u00b7gen", "aus", "dem", "Tisch", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "VVINF", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er bohrt, und in die Becher quillt Nektar s\u00fcss und frisch.", "tokens": ["Er", "bohrt", ",", "und", "in", "die", "Be\u00b7cher", "quillt", "Nek\u00b7tar", "s\u00fcss", "und", "frisch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "VVFIN", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "Die G\u00e4ste trinken jubelnd die goldne Zauberfluth,", "tokens": ["Die", "G\u00e4s\u00b7te", "trin\u00b7ken", "ju\u00b7belnd", "die", "gold\u00b7ne", "Zau\u00b7ber\u00b7fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hoch w\u00e4chst der Baum der Freude, die Wangen purpurt Gluth.", "tokens": ["Hoch", "w\u00e4chst", "der", "Baum", "der", "Freu\u00b7de", ",", "die", "Wan\u00b7gen", "pur\u00b7purt", "Gluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Und wie in volle Bl\u00fcthen die Lust der Zecher bricht,", "tokens": ["Und", "wie", "in", "vol\u00b7le", "Bl\u00fc\u00b7then", "die", "Lust", "der", "Ze\u00b7cher", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Kommt schnell herein der Diener mit bleichem Angesicht.", "tokens": ["Kommt", "schnell", "her\u00b7ein", "der", "Die\u00b7ner", "mit", "blei\u00b7chem", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "\u00bbdas Ross, Eu'r Ross, Herr Doktor \u2013 nicht zu ers\u00e4tt'gen ist es.", "tokens": ["\u00bb", "das", "Ross", ",", "Eu'r", "Ross", ",", "Herr", "Dok\u00b7tor", "\u2013", "nicht", "zu", "er\u00b7s\u00e4tt'\u00b7gen", "ist", "es", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "NE", "NE", "$,", "NN", "NN", "$(", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "PPER", "$."], "meter": "-++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "F\u00fcr zwanzig hat's gefressen, noch wie der B\u00f6se frisst es!\u00ab", "tokens": ["F\u00fcr", "zwan\u00b7zig", "hat's", "ge\u00b7fres\u00b7sen", ",", "noch", "wie", "der", "B\u00f6\u00b7se", "frisst", "es", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "CARD", "VAFIN", "VVPP", "$,", "ADV", "KOKOM", "ART", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dess alle G\u00e4ste lachen, und Faustus sp\u00f6ttisch sagt:", "tokens": ["Dess", "al\u00b7le", "G\u00e4s\u00b7te", "la\u00b7chen", ",", "und", "Faus\u00b7tus", "sp\u00f6t\u00b7tisch", "sagt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "$,", "KON", "NE", "ADJD", "VVFIN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "\u00bbdie Reise wohl wird machen, dass ihm so viel behagt.\u00ab", "tokens": ["\u00bb", "die", "Rei\u00b7se", "wohl", "wird", "ma\u00b7chen", ",", "dass", "ihm", "so", "viel", "be\u00b7hagt", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "ADV", "VAFIN", "VVINF", "$,", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "Drauf, wie die Morgenr\u00f6the kaum fliegt im Ost empor,", "tokens": ["Drauf", ",", "wie", "die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "kaum", "fliegt", "im", "Ost", "em\u00b7por", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "ART", "NN", "ADV", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da schallt ein lautes Wiehern herauf an Aller Ohr.", "tokens": ["Da", "schallt", "ein", "lau\u00b7tes", "Wie\u00b7hern", "her\u00b7auf", "an", "Al\u00b7ler", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie L\u00f6wenstimme br\u00fcllend, dass das Geb\u00e4lk erbebt,", "tokens": ["Wie", "L\u00f6\u00b7wen\u00b7stim\u00b7me", "br\u00fcl\u00b7lend", ",", "dass", "das", "Ge\u00b7b\u00e4lk", "er\u00b7bebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und unges\u00e4umet Faustus vom Sessel sich erhebt.", "tokens": ["Und", "un\u00b7ge\u00b7s\u00e4u\u00b7met", "Faus\u00b7tus", "vom", "Ses\u00b7sel", "sich", "er\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NE", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.16": {"line.1": {"text": "\u00bblebt wohl! Mein Diener ruft mir!\u00ab Er geht hinab zum Ross,", "tokens": ["\u00bb", "lebt", "wohl", "!", "Mein", "Die\u00b7ner", "ruft", "mir", "!", "\u00ab", "Er", "geht", "hin\u00b7ab", "zum", "Ross", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "$.", "$(", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das, als er's kaum bestiegen, mit ihm die Luft durchschoss;", "tokens": ["Das", ",", "als", "er's", "kaum", "be\u00b7stie\u00b7gen", ",", "mit", "ihm", "die", "Luft", "durch\u00b7schoss", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PIS", "ADV", "VVPP", "$,", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Weit fort und immer weiter, sich selber zu entfliehn", "tokens": ["Weit", "fort", "und", "im\u00b7mer", "wei\u00b7ter", ",", "sich", "sel\u00b7ber", "zu", "ent\u00b7fliehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "KON", "ADV", "ADV", "$,", "PRF", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Peitscht es der d\u00fcstre Reiter, und z\u00fcrnend tr\u00e4gt es ihn.", "tokens": ["Peitscht", "es", "der", "d\u00fcst\u00b7re", "Rei\u00b7ter", ",", "und", "z\u00fcr\u00b7nend", "tr\u00e4gt", "es", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "KON", "ADJD", "VVFIN", "PPER", "PPER", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.17": {"line.1": {"text": "Ihm stellt voll bunten Glanzes sich nicht das Leben dar,", "tokens": ["Ihm", "stellt", "voll", "bun\u00b7ten", "Glan\u00b7zes", "sich", "nicht", "das", "Le\u00b7ben", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJA", "NN", "PRF", "PTKNEG", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Durch Donnerwolken tr\u00e4gt ihn voll Grimm Pr\u00e4stigiar;", "tokens": ["Durch", "Don\u00b7ner\u00b7wol\u00b7ken", "tr\u00e4gt", "ihn", "voll", "Grimm", "Pr\u00e4s\u00b7ti\u00b7gi\u00b7ar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "NE", "NE", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Fort braust er ungez\u00fcgelt mit wildem Sturmwindflug,", "tokens": ["Fort", "braust", "er", "un\u00b7ge\u00b7z\u00fc\u00b7gelt", "mit", "wil\u00b7dem", "Sturm\u00b7wind\u00b7flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Von dessen Macht befl\u00fcgelt, der ihn in Ketten schlug.", "tokens": ["Von", "des\u00b7sen", "Macht", "be\u00b7fl\u00fc\u00b7gelt", ",", "der", "ihn", "in", "Ket\u00b7ten", "schlug", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "VVPP", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}