{"textgrid.poem.60589": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Schmaus", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die schwarze Nacht verbreitet wieder", "tokens": ["Die", "schwar\u00b7ze", "Nacht", "ver\u00b7brei\u00b7tet", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr melancholisches Gefieder;", "tokens": ["Ihr", "me\u00b7lan\u00b7cho\u00b7li\u00b7sches", "Ge\u00b7fie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Der sternenvolle Himmel brennt:", "tokens": ["Der", "ster\u00b7nen\u00b7vol\u00b7le", "Him\u00b7mel", "brennt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch Komus k\u00f6mmt, der Wein und Lieder", "tokens": ["Doch", "Ko\u00b7mus", "k\u00f6mmt", ",", "der", "Wein", "und", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "$,", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Uns seinen Freunden g\u00f6nnt.", "tokens": ["Uns", "sei\u00b7nen", "Freun\u00b7den", "g\u00f6nnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des frohen Komus Fackel gl\u00e4nzet", "tokens": ["Des", "fro\u00b7hen", "Ko\u00b7mus", "Fa\u00b7ckel", "gl\u00e4n\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Uns sch\u00f6ner, als die Sonne gl\u00e4nzet:", "tokens": ["Uns", "sch\u00f6\u00b7ner", ",", "als", "die", "Son\u00b7ne", "gl\u00e4n\u00b7zet", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er selbst er\u00f6ffnet unsern Schmaus,", "tokens": ["Er", "selbst", "er\u00b7\u00f6ff\u00b7net", "un\u00b7sern", "Schmaus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Epheu feierlich bekr\u00e4nzet,", "tokens": ["Mit", "E\u00b7pheu", "fei\u00b7er\u00b7lich", "be\u00b7kr\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und ruft den Frieden aus.", "tokens": ["Und", "ruft", "den", "Frie\u00b7den", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich, droht er, flieh und la\u00df euch streiten,", "tokens": ["Ich", ",", "droht", "er", ",", "flieh", "und", "la\u00df", "euch", "strei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "PPER", "$,", "VVFIN", "KON", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wofern ihr nicht auf andre Zeiten", "tokens": ["Wo\u00b7fern", "ihr", "nicht", "auf", "and\u00b7re", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den schulgelehrten Zank verschlie\u00dft,", "tokens": ["Den", "schul\u00b7ge\u00b7lehr\u00b7ten", "Zank", "ver\u00b7schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht, fern von trocknen Streitigkeiten,", "tokens": ["Nicht", ",", "fern", "von", "trock\u00b7nen", "Strei\u00b7tig\u00b7kei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des Lebens heut genie\u00dft.", "tokens": ["Des", "Le\u00b7bens", "heut", "ge\u00b7nie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "La\u00dft sich, mit Bluhmen in den Haaren,", "tokens": ["La\u00dft", "sich", ",", "mit", "Bluh\u00b7men", "in", "den", "Haa\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PRF", "$,", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Grazien und Musen paaren,", "tokens": ["Die", "Gra\u00b7zi\u00b7en", "und", "Mu\u00b7sen", "paa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur diese lade Bacchus ein!", "tokens": ["Nur", "die\u00b7se", "la\u00b7de", "Bac\u00b7chus", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihnen nur, ihr habts erfahren,", "tokens": ["Mit", "ih\u00b7nen", "nur", ",", "ihr", "habts", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$,", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Schmeckt auch der beste Wein.", "tokens": ["Schmeckt", "auch", "der", "bes\u00b7te", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Scherzt, doch mit freundschaftvollem Herzen!", "tokens": ["Scherzt", ",", "doch", "mit", "freund\u00b7schaft\u00b7vol\u00b7lem", "Her\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer stets mit stachelreichen Scherzen", "tokens": ["Wer", "stets", "mit", "sta\u00b7chel\u00b7rei\u00b7chen", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf liebe Freunde schalkhaft pa\u00dft,", "tokens": ["Auf", "lie\u00b7be", "Freun\u00b7de", "schalk\u00b7haft", "pa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und jauchzend lacht bey ihren Schmerzen,", "tokens": ["Und", "jauch\u00b7zend", "lacht", "bey", "ih\u00b7ren", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sey ihrer Feinde Gast!", "tokens": ["Sey", "ih\u00b7rer", "Fein\u00b7de", "Gast", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ja, ja, wir wollen alles meiden,", "tokens": ["Ja", ",", "ja", ",", "wir", "wol\u00b7len", "al\u00b7les", "mei\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was dir misf\u00e4llt, du Gott der Freuden,", "tokens": ["Was", "dir", "mis\u00b7f\u00e4llt", ",", "du", "Gott", "der", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der uns mit Wein entgegen lacht!", "tokens": ["Der", "uns", "mit", "Wein", "ent\u00b7ge\u00b7gen", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sto\u00dft an, anstatt hier Durst zu leiden!", "tokens": ["Sto\u00dft", "an", ",", "an\u00b7statt", "hier", "Durst", "zu", "lei\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KOUI", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auf eine G\u00f6tternacht!", "tokens": ["Auf", "ei\u00b7ne", "G\u00f6t\u00b7ter\u00b7nacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}