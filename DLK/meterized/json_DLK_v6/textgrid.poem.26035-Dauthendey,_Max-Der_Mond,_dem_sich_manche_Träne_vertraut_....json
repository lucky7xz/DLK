{"textgrid.poem.26035": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Der Mond, dem sich manche Tr\u00e4ne vertraut ...", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mond, dem sich manche Tr\u00e4ne vertraut,", "tokens": ["Der", "Mond", ",", "dem", "sich", "man\u00b7che", "Tr\u00e4\u00b7ne", "ver\u00b7traut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "PIAT", "NN", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der breit die blauenden Treppen der Sehnsucht", "tokens": ["Der", "breit", "die", "blau\u00b7en\u00b7den", "Trep\u00b7pen", "der", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Jedem in seinen Himmel baut;", "tokens": ["Je\u00b7dem", "in", "sei\u00b7nen", "Him\u00b7mel", "baut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der mit dem Verliebten die N\u00e4chte hinwacht,", "tokens": ["Der", "mit", "dem", "Ver\u00b7lieb\u00b7ten", "die", "N\u00e4ch\u00b7te", "hin\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ihm folgt ein Geleit verz\u00fcckt durch die Nacht,", "tokens": ["Ihm", "folgt", "ein", "Ge\u00b7leit", "ver\u00b7z\u00fcckt", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Er senkt in Fl\u00fcsse und Seen die Spur", "tokens": ["Er", "senkt", "in", "Fl\u00fcs\u00b7se", "und", "Seen", "die", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Vom Brandschein der Augen und ihrem Schwur.", "tokens": ["Vom", "Brand\u00b7schein", "der", "Au\u00b7gen", "und", "ih\u00b7rem", "Schwur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "PPOSAT", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}