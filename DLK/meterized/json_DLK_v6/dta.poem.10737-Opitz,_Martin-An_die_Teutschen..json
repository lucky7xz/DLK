{"dta.poem.10737": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "An die Teutschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ihrklagt/ jhr habt vor euch noch einen schweren Berge", "tokens": ["Ihr\u00b7klagt", "/", "jhr", "habt", "vor", "euch", "noch", "ei\u00b7nen", "schwe\u00b7ren", "Ber\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "PPER", "VAFIN", "APPR", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zusteigen/ bi\u00df jhr kompt zu de\u00df Lufstpferdts Herberge/", "tokens": ["Zu\u00b7stei\u00b7gen", "/", "bi\u00df", "jhr", "kompt", "zu", "de\u00df", "Lufst\u00b7pferdts", "Her\u00b7ber\u00b7ge", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Vnd zu der Ewigen gedechmus g\u00fcldnem Thron/", "tokens": ["Vnd", "zu", "der", "E\u00b7wi\u00b7gen", "ge\u00b7dech\u00b7mus", "g\u00fcld\u00b7nem", "Thron", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vff dem bewaret ligt die Edle Lohrberkron;", "tokens": ["Vff", "dem", "be\u00b7wa\u00b7ret", "ligt", "die", "Ed\u00b7le", "Lohr\u00b7ber\u00b7kron", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wohlan/ wie da\u00df jhr dann so still hienider sitzet/", "tokens": ["Wo\u00b7hlan", "/", "wie", "da\u00df", "jhr", "dann", "so", "still", "hie\u00b7ni\u00b7der", "sit\u00b7zet", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOKOM", "KOUS", "PPER", "ADV", "ADV", "ADJD", "PAV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd vor Teutschlandes Ehr nit auch ein wenig schwitzet.", "tokens": ["Vnd", "vor", "Teutschlan\u00b7des", "Ehr", "nit", "auch", "ein", "we\u00b7nig", "schwit\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "PTKNEG", "ADV", "ART", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Je weiter ist der weg/ je reicher ist der Lohn:", "tokens": ["Je", "wei\u00b7ter", "ist", "der", "weg", "/", "je", "rei\u00b7cher", "ist", "der", "Lohn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "PTKVZ", "$(", "ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ist dan der Weg gering/ je gr\u00f6sser ist eur hon.", "tokens": ["Ist", "dan", "der", "Weg", "ge\u00b7ring", "/", "je", "gr\u00f6s\u00b7ser", "ist", "eur", "hon", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$(", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}