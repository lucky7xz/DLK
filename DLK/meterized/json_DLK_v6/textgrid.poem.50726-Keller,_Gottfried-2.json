{"textgrid.poem.50726": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gehet eine sch\u00f6ne Sage", "tokens": ["Es", "ge\u00b7het", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie M\u00e4rchenduft auf Erden um,", "tokens": ["Wie", "M\u00e4r\u00b7chen\u00b7duft", "auf", "Er\u00b7den", "um", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie eine s\u00fc\u00dfe Sehnsuchtsklage", "tokens": ["Wie", "ei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Sehn\u00b7suchts\u00b7kla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In lauer Fr\u00fchlingsnacht herum.", "tokens": ["In", "lau\u00b7er", "Fr\u00fch\u00b7lings\u00b7nacht", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das ist das Lied vom V\u00f6lkerfrieden", "tokens": ["Das", "ist", "das", "Lied", "vom", "V\u00f6l\u00b7ker\u00b7frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und von dem letzten Menschengl\u00fcck,", "tokens": ["Und", "von", "dem", "letz\u00b7ten", "Men\u00b7schen\u00b7gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von goldner Zeit, die einst hienieden", "tokens": ["Von", "gold\u00b7ner", "Zeit", ",", "die", "einst", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Glanz und Reinheit kehrt zur\u00fcck;", "tokens": ["Mit", "Glanz", "und", "Rein\u00b7heit", "kehrt", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo einig alle V\u00f6lker beten", "tokens": ["Wo", "ei\u00b7nig", "al\u00b7le", "V\u00f6l\u00b7ker", "be\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum ", "tokens": ["Zum"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Von jenem Tag, wo den Propheten", "tokens": ["Von", "je\u00b7nem", "Tag", ",", "wo", "den", "Pro\u00b7phe\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr ehern Recht gesprochen wird!", "tokens": ["Ihr", "e\u00b7hern", "Recht", "ge\u00b7spro\u00b7chen", "wird", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Das ist das eitle Widerstreben,", "tokens": ["Das", "ist", "das", "eit\u00b7le", "Wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das es f\u00fcr Traum und Wahnsinn h\u00e4lt.", "tokens": ["Das", "es", "f\u00fcr", "Traum", "und", "Wahn\u00b7sinn", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer diese Hoffnung hat verloren", "tokens": ["Wer", "die\u00b7se", "Hoff\u00b7nung", "hat", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PDAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und b\u00f6slich sie verloren gab,", "tokens": ["Und", "b\u00f6s\u00b7lich", "sie", "ver\u00b7lo\u00b7ren", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der w\u00e4re besser ungeboren,", "tokens": ["Der", "w\u00e4\u00b7re", "bes\u00b7ser", "un\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihm geb\u00fchrt kein Menschengrab!", "tokens": ["Und", "ihm", "ge\u00b7b\u00fchrt", "kein", "Men\u00b7schen\u00b7grab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}