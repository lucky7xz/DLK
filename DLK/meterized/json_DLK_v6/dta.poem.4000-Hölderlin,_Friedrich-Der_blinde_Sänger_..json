{"dta.poem.4000": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der blinde S\u00e4nger .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Wo bist Du, Jugendliches! das immer mich", "tokens": ["Wo", "bist", "Du", ",", "Ju\u00b7gend\u00b7li\u00b7ches", "!", "das", "im\u00b7mer", "mich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NN", "$.", "ART", "ADV", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zur Stunde weckt des Morgens, wo bist Du,", "tokens": ["Zur", "Stun\u00b7de", "weckt", "des", "Mor\u00b7gens", ",", "wo", "bist", "Du", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADV", "$,", "PWAV", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Licht?", "tokens": ["Licht", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Das Herz ist wach, doch h\u00e4lt und hemmt in", "tokens": ["Das", "Herz", "ist", "wach", ",", "doch", "h\u00e4lt", "und", "hemmt", "in"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADV", "VVFIN", "KON", "VVFIN", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Heiligem Zauber die Nacht mich immer.", "tokens": ["Hei\u00b7li\u00b7gem", "Zau\u00b7ber", "die", "Nacht", "mich", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "PPER", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Sonst lauscht ich um die D\u00e4mmerung gern, sonst", "tokens": ["Sonst", "lauscht", "ich", "um", "die", "D\u00e4m\u00b7me\u00b7rung", "gern", ",", "sonst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "harrt'", "tokens": ["harrt'"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Ich gerne Dein am H\u00fcgel, und nie umsonst!", "tokens": ["Ich", "ger\u00b7ne", "Dein", "am", "H\u00fc\u00b7gel", ",", "und", "nie", "um\u00b7sonst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "APPRART", "NN", "$,", "KON", "ADV", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nie t\u00e4uschten mich, Du Holdes! Deine", "tokens": ["Nie", "t\u00e4uschten", "mich", ",", "Du", "Hol\u00b7des", "!", "Dei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "NN", "$.", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Boten, die L\u00fcfte, denn immer kamst Du,", "tokens": ["Bo\u00b7ten", ",", "die", "L\u00fcf\u00b7te", ",", "denn", "im\u00b7mer", "kamst", "Du", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,", "KON", "ADV", "VVFIN", "PPER", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Kamst allbeseligend den gewohnten Pfad", "tokens": ["Kamst", "all\u00b7be\u00b7se\u00b7li\u00b7gend", "den", "ge\u00b7wohn\u00b7ten", "Pfad"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Herein in Deiner Sch\u00f6ne, wo bist Du Licht?", "tokens": ["Her\u00b7ein", "in", "Dei\u00b7ner", "Sch\u00f6\u00b7ne", ",", "wo", "bist", "Du", "Licht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "PWAV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Herz ist wieder wach, doch bannt und", "tokens": ["Das", "Herz", "ist", "wie\u00b7der", "wach", ",", "doch", "bannt", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "$,", "ADV", "ADJD", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hemmt die unendliche Nacht mich immer.", "tokens": ["Hemmt", "die", "un\u00b7end\u00b7li\u00b7che", "Nacht", "mich", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Mir gr\u00fcnten sonst die Lauben, es leuchteten", "tokens": ["Mir", "gr\u00fcn\u00b7ten", "sonst", "die", "Lau\u00b7ben", ",", "es", "leuch\u00b7te\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Blumen, wie die eignen Augen, mir,", "tokens": ["Die", "Blu\u00b7men", ",", "wie", "die", "eig\u00b7nen", "Au\u00b7gen", ",", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht ferne war das Angesicht der", "tokens": ["Nicht", "fer\u00b7ne", "war", "das", "An\u00b7ge\u00b7sicht", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VAFIN", "ART", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lieben, und leuchtete mir, und droben", "tokens": ["Lie\u00b7ben", ",", "und", "leuch\u00b7te\u00b7te", "mir", ",", "und", "dro\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "$,", "KON", "VVFIN", "PPER", "$,", "KON", "ADV"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Und um die W\u00e4lder sah ich die Fittige", "tokens": ["Und", "um", "die", "W\u00e4l\u00b7der", "sah", "ich", "die", "Fit\u00b7ti\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Himmels fliegen, da ich ein J\u00fcngling war;", "tokens": ["Des", "Him\u00b7mels", "flie\u00b7gen", ",", "da", "ich", "ein", "J\u00fcng\u00b7ling", "war", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nun sitz' ich still allein, von einer", "tokens": ["Nun", "sitz'", "ich", "still", "al\u00b7lein", ",", "von", "ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "$,", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stunde zur anderen, und Gestalten", "tokens": ["Stun\u00b7de", "zur", "an\u00b7de\u00b7ren", ",", "und", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "$,", "KON", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Aus Lieb und Leid der helleren Tage schafft,", "tokens": ["Aus", "Lieb", "und", "Leid", "der", "hel\u00b7le\u00b7ren", "Ta\u00b7ge", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zur eignen Freude nun mein Gedanke sich,", "tokens": ["Zur", "eig\u00b7nen", "Freu\u00b7de", "nun", "mein", "Ge\u00b7dan\u00b7ke", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "PRF", "$,"], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Und ferne lausch' ich hin, ob nicht ein", "tokens": ["Und", "fer\u00b7ne", "lausch'", "ich", "hin", ",", "ob", "nicht", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PTKNEG", "ART"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Freundlicher Retter vielleicht mir komme.", "tokens": ["Freund\u00b7li\u00b7cher", "Ret\u00b7ter", "viel\u00b7leicht", "mir", "kom\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Dann h\u00f6r' ich oft den Wagen des Donneres", "tokens": ["Dann", "h\u00f6r'", "ich", "oft", "den", "Wa\u00b7gen", "des", "Don\u00b7ne\u00b7res"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Am Mittag, wenn der eherne nahe kommt", "tokens": ["Am", "Mit\u00b7tag", ",", "wenn", "der", "e\u00b7her\u00b7ne", "na\u00b7he", "kommt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und ihm das Haus bebt, und der Boden", "tokens": ["Und", "ihm", "das", "Haus", "bebt", ",", "und", "der", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unter ihm dr\u00f6hnt, und der Berg es nachhallt.", "tokens": ["Un\u00b7ter", "ihm", "dr\u00f6hnt", ",", "und", "der", "Berg", "es", "nach\u00b7hallt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "KON", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.8": {"line.1": {"text": "Den Retter h\u00f6r' ich dann in der Nacht, ich h\u00f6r'", "tokens": ["Den", "Ret\u00b7ter", "h\u00f6r'", "ich", "dann", "in", "der", "Nacht", ",", "ich", "h\u00f6r'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihn t\u00f6dtend, den Befreier, belebend ihn,", "tokens": ["Ihn", "t\u00f6d\u00b7tend", ",", "den", "Be\u00b7frei\u00b7er", ",", "be\u00b7le\u00b7bend", "ihn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "ART", "NN", "$,", "VVPP", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Donnerer, vom Untergang zum", "tokens": ["Den", "Don\u00b7ne\u00b7rer", ",", "vom", "Un\u00b7ter\u00b7gang", "zum"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Orient eilen und ihm nach t\u00f6nt ihr,", "tokens": ["O\u00b7rient", "ei\u00b7len", "und", "ihm", "nach", "t\u00f6nt", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "PPER", "APPR", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Ihr meiner Seele Saiten! es lebt mit ihm", "tokens": ["Ihr", "mei\u00b7ner", "See\u00b7le", "Sai\u00b7ten", "!", "es", "lebt", "mit", "ihm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$.", "PPER", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mein Geist, und wie die Quelle dem Strome folgt,", "tokens": ["Mein", "Geist", ",", "und", "wie", "die", "Quel\u00b7le", "dem", "Stro\u00b7me", "folgt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wohin er trachtet, so geleit' ich", "tokens": ["Wo\u00b7hin", "er", "trach\u00b7tet", ",", "so", "ge\u00b7leit'", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gerne den Sicheren auf der Irrbahn.", "tokens": ["Ger\u00b7ne", "den", "Si\u00b7che\u00b7ren", "auf", "der", "Irr\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Wohin? wohin? ich h\u00f6re Dich da und dort,", "tokens": ["Wo\u00b7hin", "?", "wo\u00b7hin", "?", "ich", "h\u00f6\u00b7re", "Dich", "da", "und", "dort", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PWAV", "$.", "PPER", "VVFIN", "PPER", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Du Herrlicher! und rings um die Erde t\u00f6nt's!", "tokens": ["Du", "Herr\u00b7li\u00b7cher", "!", "und", "rings", "um", "die", "Er\u00b7de", "t\u00f6nt's", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "KON", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wo endest Du? und was, was ist es", "tokens": ["Wo", "en\u00b7dest", "Du", "?", "und", "was", ",", "was", "ist", "es"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$.", "KON", "PWS", "$,", "PWS", "VAFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ueber den Wolken? und o wie wird mir!", "tokens": ["Ue\u00b7ber", "den", "Wol\u00b7ken", "?", "und", "o", "wie", "wird", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "KON", "FM", "KOKOM", "VAFIN", "PPER", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Tag! Tag! Du \u00fcber st\u00fcrzenden Wolken! sey", "tokens": ["Tag", "!", "Tag", "!", "Du", "\u00fc\u00b7ber", "st\u00fcr\u00b7zen\u00b7den", "Wol\u00b7ken", "!", "sey"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$.", "NN", "$.", "PPER", "APPR", "ADJA", "NN", "$.", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Willkommen mir! es bl\u00fchet mein Auge Dir.", "tokens": ["Will\u00b7kom\u00b7men", "mir", "!", "es", "bl\u00fc\u00b7het", "mein", "Au\u00b7ge", "Dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O Jugendlicht! o Gl\u00fcck! das alte", "tokens": ["O", "Ju\u00b7gend\u00b7licht", "!", "o", "Gl\u00fcck", "!", "das", "al\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "FM", "NN", "$.", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wieder! doch geistiger rinnst Du nieder,", "tokens": ["Wie\u00b7der", "!", "doch", "geis\u00b7ti\u00b7ger", "rinnst", "Du", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Du goldner Quell aus heiligem Kelch! und Du,", "tokens": ["Du", "gold\u00b7ner", "Quell", "aus", "hei\u00b7li\u00b7gem", "Kelch", "!", "und", "Du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "ADJA", "NN", "$.", "KON", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du gr\u00fcner Boden! friedliche Wieg'! und Du,", "tokens": ["Du", "gr\u00fc\u00b7ner", "Bo\u00b7den", "!", "fried\u00b7li\u00b7che", "Wieg'", "!", "und", "Du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "ADJA", "NN", "$.", "KON", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Haus meiner V\u00e4ter! und ihr Lieben,", "tokens": ["Haus", "mei\u00b7ner", "V\u00e4\u00b7ter", "!", "und", "ihr", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$.", "KON", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die mir begegneten einst, o nahet,", "tokens": ["Die", "mir", "be\u00b7ge\u00b7gne\u00b7ten", "einst", ",", "o", "na\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "$,", "FM", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "O kommt, da\u00df euer, euer die Freude sey,", "tokens": ["O", "kommt", ",", "da\u00df", "eu\u00b7er", ",", "eu\u00b7er", "die", "Freu\u00b7de", "sey", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KOUS", "PPOSAT", "$,", "PPOSAT", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihr alle! da\u00df euch segne der Sehnende!", "tokens": ["Ihr", "al\u00b7le", "!", "da\u00df", "euch", "seg\u00b7ne", "der", "Seh\u00b7nen\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$.", "KOUS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "O nehmt, da\u00df ich's ertrage, mir das", "tokens": ["O", "nehmt", ",", "da\u00df", "ich's", "er\u00b7tra\u00b7ge", ",", "mir", "das"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$,", "PPER", "ART"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Leben, das G\u00f6ttliche mir vom Herzen!", "tokens": ["Le\u00b7ben", ",", "das", "G\u00f6tt\u00b7li\u00b7che", "mir", "vom", "Her\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}