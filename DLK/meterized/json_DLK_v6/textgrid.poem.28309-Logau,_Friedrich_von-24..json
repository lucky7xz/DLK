{"textgrid.poem.28309": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Obs recht, obs ehrlich sey, was Honoratus thut,", "tokens": ["Obs", "recht", ",", "obs", "ehr\u00b7lich", "sey", ",", "was", "Ho\u00b7no\u00b7ra\u00b7tus", "thut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "KOUS", "ADJD", "VAFIN", "$,", "PRELS", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da fragt er wenig drum; er h\u00e4lt nur di\u00df f\u00fcr gut,", "tokens": ["Da", "fragt", "er", "we\u00b7nig", "drum", ";", "er", "h\u00e4lt", "nur", "di\u00df", "f\u00fcr", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PAV", "$.", "PPER", "VVFIN", "ADV", "PDS", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was gut zu schmausen bringt. Ey! darff man doch wol sagen,", "tokens": ["Was", "gut", "zu", "schmau\u00b7sen", "bringt", ".", "Ey", "!", "darff", "man", "doch", "wol", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PTKZU", "VVINF", "VVFIN", "$.", "NN", "$.", "VMFIN", "PIS", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im Maule steh sein Recht; sein Ehre wohn im Magen.", "tokens": ["Im", "Mau\u00b7le", "steh", "sein", "Recht", ";", "sein", "Eh\u00b7re", "wohn", "im", "Ma\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}