{"textgrid.poem.36459": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "9. Die Raupe und der Schmetterling", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine kleine Raupe lag,", "tokens": ["Ei\u00b7ne", "klei\u00b7ne", "Rau\u00b7pe", "lag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In ihr Leichentuch gesponnen,", "tokens": ["In", "ihr", "Lei\u00b7chen\u00b7tuch", "ge\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tot im Angesicht der Sonnen,", "tokens": ["Tot", "im", "An\u00b7ge\u00b7sicht", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es war der sch\u00f6nste Tag.", "tokens": ["Und", "es", "war", "der", "sch\u00f6ns\u00b7te", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ein sch\u00f6ner Schmetterling", "tokens": ["Und", "ein", "sch\u00f6\u00b7ner", "Schmet\u00b7ter\u00b7ling"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kam geflogen, setzte sich", "tokens": ["Kam", "ge\u00b7flo\u00b7gen", ",", "setz\u00b7te", "sich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "VVPP", "$,", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Still daneben, sagte: ", "tokens": ["Still", "da\u00b7ne\u00b7ben", ",", "sag\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJD", "PAV", "$,", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Pl\u00f6tzlich warf", "tokens": ["Pl\u00f6tz\u00b7lich", "warf"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Sie die Schal' ab, lie\u00df sie liegen,", "tokens": ["Sie", "die", "Schal'", "ab", ",", "lie\u00df", "sie", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "--++--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und der sch\u00f6ne Schmetterling", "tokens": ["Und", "der", "sch\u00f6\u00b7ne", "Schmet\u00b7ter\u00b7ling"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sah den neuen Engel fliegen,", "tokens": ["Sah", "den", "neu\u00b7en", "En\u00b7gel", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn ich ihn so nennen darf.", "tokens": ["Wenn", "ich", "ihn", "so", "nen\u00b7nen", "darf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}