{"dta.poem.16938": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "39.  Der beste Stand unter dem Creutze.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Das Blutt das unserm HERRN au\u00df seiner Wunden", "tokens": ["Das", "Blutt", "das", "un\u00b7serm", "HeRRN", "au\u00df", "sei\u00b7ner", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "ART", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "fleust/", "tokens": ["fleust", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ist seiner liebe Thau damit Er untz begeust:", "tokens": ["Ist", "sei\u00b7ner", "lie\u00b7be", "Thau", "da\u00b7mit", "Er", "untz", "be\u00b7geust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wiltu befeuchtet seyn/ und Unverwelklich bl\u00fchen/", "tokens": ["Wil\u00b7tu", "be\u00b7feuch\u00b7tet", "seyn", "/", "und", "Un\u00b7ver\u00b7wel\u00b7klich", "bl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "VAINF", "$(", "KON", "ADJD", "VVFIN", "$("], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "So mustu nicht einmal von seinem Creutze fliehen.", "tokens": ["So", "mus\u00b7tu", "nicht", "ein\u00b7mal", "von", "sei\u00b7nem", "Creut\u00b7ze", "flie\u00b7hen", "."], "token_in