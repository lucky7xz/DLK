{"textgrid.poem.43659": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Fliegenf\u00e4nger", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ich mich auch dreh' und schmiege,", "tokens": ["Wie", "ich", "mich", "auch", "dreh'", "und", "schmie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "CARD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer bist du wieder hier.", "tokens": ["Im\u00b7mer", "bist", "du", "wie\u00b7der", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Warte nur, du b\u00f6se Fliege!", "tokens": ["War\u00b7te", "nur", ",", "du", "b\u00f6\u00b7se", "Flie\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich ist es aus mit dir.", "tokens": ["End\u00b7lich", "ist", "es", "aus", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eine Fliege, die mich sticht,", "tokens": ["Ei\u00b7ne", "Flie\u00b7ge", ",", "die", "mich", "sticht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schon' ich nicht.", "tokens": ["Schon'", "ich", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Macht nur meine Klappe klapp!", "tokens": ["Macht", "nur", "mei\u00b7ne", "Klap\u00b7pe", "klapp", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "ITJ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist es aus mit dir schwipp schwapp!", "tokens": ["Ist", "es", "aus", "mit", "dir", "schwipp", "schwapp", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "APPR", "PPER", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Schwipp schwapp!", "tokens": ["Schwipp", "schwapp", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Klapp klapp!", "tokens": ["Klapp", "klapp", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "ITJ", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Setz' dich mal auf meine Mappe,", "tokens": ["Setz'", "dich", "mal", "auf", "mei\u00b7ne", "Map\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Auf das s\u00fc\u00dfe Zuckermehl!", "tokens": ["Auf", "das", "s\u00fc\u00b7\u00dfe", "Zu\u00b7cker\u00b7mehl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm' ich mit der Fliegenklappe,", "tokens": ["Komm'", "ich", "mit", "der", "Flie\u00b7gen\u00b7klap\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlag' ich ganz gewi\u00df nicht fehl.", "tokens": ["Schlag'", "ich", "ganz", "ge\u00b7wi\u00df", "nicht", "fehl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Freilich, Fliegenschnepper schnappen", "tokens": ["Frei\u00b7lich", ",", "Flie\u00b7gen\u00b7schnep\u00b7per", "schnap\u00b7pen"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Besser dich als ich es kann,", "tokens": ["Bes\u00b7ser", "dich", "als", "ich", "es", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KOUS", "PPER", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch ich werd' auch dich ertappen", "tokens": ["Doch", "ich", "werd'", "auch", "dich", "er\u00b7tap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und verloren bist du dann.", "tokens": ["Und", "ver\u00b7lo\u00b7ren", "bist", "du", "dann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}