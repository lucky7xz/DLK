{"textgrid.poem.33286": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Mein System", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In meines Lebens Flitterjahren", "tokens": ["In", "mei\u00b7nes", "Le\u00b7bens", "Flit\u00b7ter\u00b7jah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Best\u00fcrmt' ich Paphos, Heiligthum,", "tokens": ["Be\u00b7st\u00fcrmt'", "ich", "Pa\u00b7phos", ",", "Hei\u00b7lig\u00b7thum", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch kaum als die vor\u00fcber waren,", "tokens": ["Doch", "kaum", "als", "die", "vor\u00b7\u00fc\u00b7ber", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "ART", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da winkten Ehre mir und Ruhm.", "tokens": ["Da", "wink\u00b7ten", "Eh\u00b7re", "mir", "und", "Ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich griff zugleich nach Schwert und Leyer,", "tokens": ["Ich", "griff", "zu\u00b7gleich", "nach", "Schwert", "und", "Le\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-++", "measure": "unknown.measure.penta"}, "line.2": {"text": "Allein die Musen gaben mir,", "tokens": ["Al\u00b7lein", "die", "Mu\u00b7sen", "ga\u00b7ben", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Manchem ihrer lauen Freyer,", "tokens": ["Wie", "Man\u00b7chem", "ih\u00b7rer", "lau\u00b7en", "Frey\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Korb, und wiesen mir die Th\u00fcr.", "tokens": ["Den", "Korb", ",", "und", "wie\u00b7sen", "mir", "die", "Th\u00fcr."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-++", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Ich baute nun auf Mavors Gnade,", "tokens": ["Ich", "bau\u00b7te", "nun", "auf", "Ma\u00b7vors", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch lud er mich gef\u00e4llig ein,", "tokens": ["Auch", "lud", "er", "mich", "ge\u00b7f\u00e4l\u00b7lig", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich hoffte auf dem grossen Rade", "tokens": ["Ich", "hoff\u00b7te", "auf", "dem", "gros\u00b7sen", "Ra\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fortunens gl\u00fccklicher zu sein,", "tokens": ["For\u00b7tu\u00b7nens", "gl\u00fcck\u00b7li\u00b7cher", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Von einem Ordensband umwunden,", "tokens": ["Von", "ei\u00b7nem", "Or\u00b7dens\u00b7band", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tr\u00e4umt' ich den sch\u00f6nsten Lorbeerstrau\u00df,", "tokens": ["Tr\u00e4umt'", "ich", "den", "sch\u00f6ns\u00b7ten", "Lor\u00b7beer\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schnell war dies Luftphantom verschwunden,", "tokens": ["Schnell", "war", "dies", "Luft\u00b7phan\u00b7tom", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PDS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein jeder Friede blies es aus.", "tokens": ["Ein", "je\u00b7der", "Frie\u00b7de", "blies", "es", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun bot ich der Vernunft die Ehre", "tokens": ["Nun", "bot", "ich", "der", "Ver\u00b7nunft", "die", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von meiner sp\u00e4ten Huldigung:", "tokens": ["Von", "mei\u00b7ner", "sp\u00e4\u00b7ten", "Hul\u00b7di\u00b7gung", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein Vierziger, so dacht' ich, w\u00e4re", "tokens": ["Ein", "Vier\u00b7zi\u00b7ger", ",", "so", "dacht'", "ich", ",", "w\u00e4\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "PPER", "$,", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nun wohl f\u00fcr sie nicht mehr zu jung.", "tokens": ["Nun", "wohl", "f\u00fcr", "sie", "nicht", "mehr", "zu", "jung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "PTKNEG", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Allein sie fand es noch gef\u00e4hrlich,", "tokens": ["Al\u00b7lein", "sie", "fand", "es", "noch", "ge\u00b7f\u00e4hr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und stellte sich, o Nilis, dar", "tokens": ["Und", "stell\u00b7te", "sich", ",", "o", "Ni\u00b7lis", ",", "dar"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PRF", "$,", "FM", "FM", "$,", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In deinem Bilde, weil sonst schwerlich", "tokens": ["In", "dei\u00b7nem", "Bil\u00b7de", ",", "weil", "sonst", "schwer\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Fl\u00fcchtling fest zu halten war.", "tokens": ["Der", "Fl\u00fccht\u00b7ling", "fest", "zu", "hal\u00b7ten", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O ja, sie brauchte nicht vergebens", "tokens": ["O", "ja", ",", "sie", "brauch\u00b7te", "nicht", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "ITJ", "$,", "PPER", "VVFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich, Zauberin, zu ihrer List,", "tokens": ["Dich", ",", "Zau\u00b7be\u00b7rin", ",", "zu", "ih\u00b7rer", "List", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die bald die Freude meines Lebens,", "tokens": ["Die", "bald", "die", "Freu\u00b7de", "mei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bald meiner Launen Geisel ist.", "tokens": ["Bald", "mei\u00b7ner", "Lau\u00b7nen", "Gei\u00b7sel", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Gen\u00fcglich, wenn jetzt mit Vergn\u00fcgen", "tokens": ["Ge\u00b7n\u00fcg\u00b7lich", ",", "wenn", "jetzt", "mit", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir der Geschmack die Tafel w\u00fcrzt,", "tokens": ["Mir", "der", "Ge\u00b7schmack", "die", "Ta\u00b7fel", "w\u00fcrzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Zufried'ner als bei Nektarz\u00fcgen,", "tokens": ["Zu\u00b7frie\u00b7d'\u00b7ner", "als", "bei", "Nek\u00b7tar\u00b7z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn mir ein Freund die Zeit verk\u00fcrzt:", "tokens": ["Wenn", "mir", "ein", "Freund", "die", "Zeit", "ver\u00b7k\u00fcrzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Gleich fertig, meinen Arm zu heben,", "tokens": ["Gleich", "fer\u00b7tig", ",", "mei\u00b7nen", "Arm", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Joseph mich zu Schlachten zieht,", "tokens": ["Wenn", "Jo\u00b7se\u00b7ph", "mich", "zu", "Schlach\u00b7ten", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als willig, mir allein zu leben,", "tokens": ["Als", "wil\u00b7lig", ",", "mir", "al\u00b7lein", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn mich sein Wahlblick \u00fcbersieht;", "tokens": ["Wenn", "mich", "sein", "Wahl\u00b7blick", "\u00fc\u00b7ber\u00b7sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Will ich ihn Sieg auf Sieg mit Freuden", "tokens": ["Will", "ich", "ihn", "Sieg", "auf", "Sieg", "mit", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "NN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um seine Schl\u00e4fe winden seh'n,", "tokens": ["Um", "sei\u00b7ne", "Schl\u00e4\u00b7fe", "win\u00b7den", "seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nur die Gl\u00fccklichen beneiden,", "tokens": ["Und", "nur", "die", "Gl\u00fcck\u00b7li\u00b7chen", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ihm dabei zur Seite steh'n.", "tokens": ["Die", "ihm", "da\u00b7bei", "zur", "Sei\u00b7te", "steh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "PPER", "PAV", "APPRART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Wenn and're ihren Kriegsruhm lieber", "tokens": ["Wenn", "an\u00b7d'\u00b7re", "ih\u00b7ren", "Kriegs\u00b7ruhm", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auf faule Zeitungsl\u00fcgen bau'n,", "tokens": ["Auf", "fau\u00b7le", "Zei\u00b7tungs\u00b7l\u00fc\u00b7gen", "bau'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ihren Feinden gegen\u00fcber", "tokens": ["Als", "ih\u00b7ren", "Fein\u00b7den", "ge\u00b7gen\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPO"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Tode selbst in's Auge schau'n;", "tokens": ["Dem", "To\u00b7de", "selbst", "in's", "Au\u00b7ge", "schau'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Will ich der grossen M\u00e4nner Schatten", "tokens": ["Will", "ich", "der", "gros\u00b7sen", "M\u00e4n\u00b7ner", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beneiden um ihr Heldenthum,", "tokens": ["Be\u00b7nei\u00b7den", "um", "ihr", "Hel\u00b7den\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nur nach ihren grossen Thaten", "tokens": ["Und", "nur", "nach", "ih\u00b7ren", "gros\u00b7sen", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich sehnen, nicht nach ihrem Ruhm.", "tokens": ["Mich", "seh\u00b7nen", ",", "nicht", "nach", "ih\u00b7rem", "Ruhm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Und wenn dereinst mit milder'm Blicke", "tokens": ["Und", "wenn", "de\u00b7reinst", "mit", "mil\u00b7der'm", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Schicksal nieder auf mich sieht,", "tokens": ["Das", "Schick\u00b7sal", "nie\u00b7der", "auf", "mich", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann nehm' ich halb von meinem Gl\u00fccke,", "tokens": ["Dann", "nehm'", "ich", "halb", "von", "mei\u00b7nem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und halb theil' ich es andern mit.", "tokens": ["Und", "halb", "theil'", "ich", "es", "an\u00b7dern", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Belehrt vom Werth des Gl\u00fccks hienieden", "tokens": ["Be\u00b7lehrt", "vom", "Werth", "des", "Gl\u00fccks", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch eig'ne Widerw\u00e4rtigkeit,", "tokens": ["Durch", "eig'\u00b7ne", "Wi\u00b7der\u00b7w\u00e4r\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Leb' ich mit meinem Loos zufrieden", "t