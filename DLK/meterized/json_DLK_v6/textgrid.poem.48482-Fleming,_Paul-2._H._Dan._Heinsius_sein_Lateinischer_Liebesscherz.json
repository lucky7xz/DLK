{"textgrid.poem.48482": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "2. H. Dan. Heinsius sein Lateinischer Liebesscherz", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Lieb das gabe mir, als sie mich gestern liebte,", "tokens": ["Mein", "Lieb", "das", "ga\u00b7be", "mir", ",", "als", "sie", "mich", "ge\u00b7stern", "lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "ein s\u00fc\u00dfes K\u00fcsselein, noch s\u00fc\u00dfer als der Wein,", "tokens": ["ein", "s\u00fc\u00b7\u00dfes", "K\u00fcs\u00b7sel\u00b7ein", ",", "noch", "s\u00fc\u00b7\u00dfer", "als", "der", "Wein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der sonst der s\u00fc\u00dfste hei\u00dft. Ich, als sie di\u00df ver\u00fcbte,", "tokens": ["der", "sonst", "der", "s\u00fc\u00df\u00b7ste", "hei\u00dft", ".", "Ich", ",", "als", "sie", "di\u00df", "ver\u00b7\u00fcb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "VVFIN", "$.", "PPER", "$,", "KOUS", "PPER", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "entf\u00e4rbte g\u00e4nzlich mich. Ich nam ihr H\u00e4lselein", "tokens": ["ent\u00b7f\u00e4rb\u00b7te", "g\u00e4nz\u00b7lich", "mich", ".", "Ich", "nam", "ihr", "H\u00e4l\u00b7sel\u00b7ein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "und hing mich sehnlich dran. Ich sah in einem Sehen", "tokens": ["und", "hing", "mich", "sehn\u00b7lich", "dran", ".", "Ich", "sah", "in", "ei\u00b7nem", "Se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "ihr in ihr Angesicht'; ich sah ihr stetig drein", "tokens": ["ihr", "in", "ihr", "An\u00b7ge\u00b7sicht'", ";", "ich", "sah", "ihr", "ste\u00b7tig", "drein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "und hing das Haupt nach ihr. Ach, sprach ich, kans geschehen,", "tokens": ["und", "hing", "das", "Haupt", "nach", "ihr", ".", "Ach", ",", "sprach", "ich", ",", "kans", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPER", "$.", "ITJ", "$,", "VVFIN", "PPER", "$,", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "da\u00df du, mein Leben, kanst mir Armen g\u00fcnstig sein?", "tokens": ["da\u00df", "du", ",", "mein", "Le\u00b7ben", ",", "kanst", "mir", "Ar\u00b7men", "g\u00fcns\u00b7tig", "sein", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "NN", "$,", "VMFIN", "PPER", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Worauf sie lachend was von ihrem sch\u00f6nen Munde", "tokens": ["Wo\u00b7rauf", "sie", "la\u00b7chend", "was", "von", "ih\u00b7rem", "sch\u00f6\u00b7nen", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "ADJD", "PWS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "aus tiefster Seelen raus, wei\u00df noch nicht, was es war,", "tokens": ["aus", "tiefs\u00b7ter", "See\u00b7len", "raus", ",", "wei\u00df", "noch", "nicht", ",", "was", "es", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,", "VVFIN", "ADV", "PTKNEG", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "mir blies in meinen Mund. Sie bliese mehr zur Stunde,", "tokens": ["mir", "blies", "in", "mei\u00b7nen", "Mund", ".", "Sie", "blie\u00b7se", "mehr", "zur", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "noch etwas, wei\u00df nicht was, das feucht und laulecht gar.", "tokens": ["noch", "et\u00b7was", ",", "wei\u00df", "nicht", "was", ",", "das", "feucht", "und", "lau\u00b7lecht", "gar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "VVFIN", "PTKNEG", "PIS", "$,", "PRELS", "ADJD", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So bald ich dieses nur befund' in meinem Herzen,", "tokens": ["So", "bald", "ich", "die\u00b7ses", "nur", "be\u00b7fund'", "in", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PDS", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "beraubt' es mich der Seel' und aller Sinnen Kraft.", "tokens": ["be\u00b7raubt'", "es", "mich", "der", "Seel'", "und", "al\u00b7ler", "Sin\u00b7nen", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "KON", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Daher ich, ohne Seel' und ohne mich, mit Schmerzen", "tokens": ["Da\u00b7her", "ich", ",", "oh\u00b7ne", "Seel'", "und", "oh\u00b7ne", "mich", ",", "mit", "Schmer\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "PPER", "$,", "KOUI", "NN", "KON", "APPR", "PPER", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "lauf' immer hin und her in einer frembden Haft.", "tokens": ["lauf'", "im\u00b7mer", "hin", "und", "her", "in", "ei\u00b7ner", "fremb\u00b7den", "Haft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "KON", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach Lieb, ich suche mich mit Weinen aller Enden!", "tokens": ["Ach", "Lieb", ",", "ich", "su\u00b7che", "mich", "mit", "Wei\u00b7nen", "al\u00b7ler", "En\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PPER", "VVFIN", "PRF", "APPR", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ach! ach! verk\u00e4ufstu denn so teuer einen Ku\u00df?", "tokens": ["Ach", "!", "ach", "!", "ver\u00b7k\u00e4ufs\u00b7tu", "denn", "so", "teu\u00b7er", "ei\u00b7nen", "Ku\u00df", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "XY", "$.", "VVFIN", "ADV", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ach! freilich tustu mir die Seel' und Herz entwenden,", "tokens": ["Ach", "!", "frei\u00b7lich", "tus\u00b7tu", "mir", "die", "Seel'", "und", "Herz", "ent\u00b7wen\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "nun ich in deiner Seel' und Herzen leben mu\u00df.", "tokens": ["nun", "ich", "in", "dei\u00b7ner", "Seel'", "und", "Her\u00b7zen", "le\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "PPOSAT", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ach! wein' ich oder nicht? Was soll ich doch beginnen?", "tokens": ["Ach", "!", "wein'", "ich", "o\u00b7der", "nicht", "?", "Was", "soll", "ich", "doch", "be\u00b7gin\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "KON", "PTKNEG", "$.", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mit Tr\u00e4nen sie doch nicht erweichet werden kan,", "tokens": ["Mit", "Tr\u00e4\u00b7nen", "sie", "doch", "nicht", "er\u00b7wei\u00b7chet", "wer\u00b7den", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADV", "PTKNEG", "VVPP", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "sie nehret sich vielmehr von meinem Thr\u00e4nenrinnen.", "tokens": ["sie", "neh\u00b7ret", "sich", "viel\u00b7mehr", "von", "mei\u00b7nem", "Thr\u00e4\u00b7nen\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ich will umb einen Ku\u00df sie freundlich sprechen an.", "tokens": ["Ich", "will", "umb", "ei\u00b7nen", "Ku\u00df", "sie", "freund\u00b7lich", "spre\u00b7chen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "PPER", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich will sie sehen an, ich will fort immer weiden", "tokens": ["Ich", "will", "sie", "se\u00b7hen", "an", ",", "ich", "will", "fort", "im\u00b7mer", "wei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VMFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "in ihrem Odem mich, sie in dem meinen sich.", "tokens": ["in", "ih\u00b7rem", "O\u00b7dem", "mich", ",", "sie", "in", "dem", "mei\u00b7nen", "sich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "$,", "PPER", "APPR", "ART", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So werd' ich meine Seel' antreffen voller Freuden,", "tokens": ["So", "werd'", "ich", "mei\u00b7ne", "Seel'", "an\u00b7tref\u00b7fen", "vol\u00b7ler", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "so oft ich ihrer mu\u00df begeben g\u00e4nzlich mich.", "tokens": ["so", "oft", "ich", "ih\u00b7rer", "mu\u00df", "be\u00b7ge\u00b7ben", "g\u00e4nz\u00b7lich", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "VMFIN", "VVINF", "ADJD", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}