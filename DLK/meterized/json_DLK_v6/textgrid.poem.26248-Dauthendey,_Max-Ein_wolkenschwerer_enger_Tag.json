{"textgrid.poem.26248": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Ein wolkenschwerer enger Tag", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein wolkenschwerer enger Tag,", "tokens": ["Ein", "wol\u00b7ken\u00b7schwe\u00b7rer", "en\u00b7ger", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ich ihn in der Heimat mag,", "tokens": ["Wie", "ich", "ihn", "in", "der", "Hei\u00b7mat", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "APPR", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Liegt \u00fcber Reisfeld und Vulkan.", "tokens": ["Liegt", "\u00fc\u00b7ber", "Reis\u00b7feld", "und", "Vul\u00b7kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NE", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Morgen sieht sich dunkel an.", "tokens": ["Der", "Mor\u00b7gen", "sieht", "sich", "dun\u00b7kel", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und der Mimosenb\u00e4ume Zeile", "tokens": ["Und", "der", "Mi\u00b7mo\u00b7sen\u00b7b\u00e4u\u00b7me", "Zei\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Windstill am Wege. Und ich teile", "tokens": ["Wind\u00b7still", "am", "We\u00b7ge", ".", "Und", "ich", "tei\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "$.", "KON", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Ernst der Stra\u00dfe, die gebleicht", "tokens": ["Den", "Ernst", "der", "Stra\u00b7\u00dfe", ",", "die", "ge\u00b7bleicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wunschlos in graue Fernen reicht.", "tokens": ["Wun\u00b7schlos", "in", "grau\u00b7e", "Fer\u00b7nen", "reicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Und lautlos, wie nur V\u00f6gel fliehen,", "tokens": ["Und", "laut\u00b7los", ",", "wie", "nur", "V\u00f6\u00b7gel", "flie\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Javanen durch die Felder ziehen.", "tokens": ["Ja\u00b7va\u00b7nen", "durch", "die", "Fel\u00b7der", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie eilen wie Gedanken fort", "tokens": ["Sie", "ei\u00b7len", "wie", "Ge\u00b7dan\u00b7ken", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gr\u00fc\u00dfen nur mit Fl\u00fcsterwort.", "tokens": ["Und", "gr\u00fc\u00b7\u00dfen", "nur", "mit", "Fl\u00fcs\u00b7ter\u00b7wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Lautlos zu sein, ist ihr Behagen.", "tokens": ["Laut\u00b7los", "zu", "sein", ",", "ist", "ihr", "Be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VAINF", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So still; man h\u00f6rt die Wolken fragen:", "tokens": ["So", "still", ";", "man", "h\u00f6rt", "die", "Wol\u00b7ken", "fra\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "PIS", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo will der Weg der Menschen hin?", "tokens": ["Wo", "will", "der", "Weg", "der", "Men\u00b7schen", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wunschlosigkeit gibt frommen Sinn.", "tokens": ["Wun\u00b7schlo\u00b7sig\u00b7keit", "gibt", "from\u00b7men", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}