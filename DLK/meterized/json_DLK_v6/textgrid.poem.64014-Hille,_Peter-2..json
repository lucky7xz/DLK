{"textgrid.poem.64014": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Urwalds Riesen splittern", "tokens": ["Des", "Ur\u00b7walds", "Rie\u00b7sen", "split\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Nacht durchflammenden Gewittern.", "tokens": ["In", "Nacht", "durch\u00b7flam\u00b7men\u00b7den", "Ge\u00b7wit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es heult wie Kn\u00e4ul von dem Wirt geschoben,", "tokens": ["Es", "heult", "wie", "Kn\u00e4ul", "von", "dem", "Wirt", "ge\u00b7scho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf stillen Stra\u00dfen mit wilden Messern toben;", "tokens": ["Auf", "stil\u00b7len", "Stra\u00b7\u00dfen", "mit", "wil\u00b7den", "Mes\u00b7sern", "to\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Dann bin ich in meinem Element,", "tokens": ["Dann", "bin", "ich", "in", "mei\u00b7nem", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "In meinen Augen einsam brennt", "tokens": ["In", "mei\u00b7nen", "Au\u00b7gen", "ein\u00b7sam", "brennt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Menschen hassende Temperament", "tokens": ["Das", "Men\u00b7schen", "has\u00b7sen\u00b7de", "Tem\u00b7pe\u00b7ra\u00b7ment"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Melancholie.", "tokens": ["Me\u00b7lan\u00b7cho\u00b7lie", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Das d\u00fcstere Gestirn Genie", "tokens": ["Das", "d\u00fcs\u00b7te\u00b7re", "Ge\u00b7stirn", "Ge\u00b7nie"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Flammt", "tokens": ["Flammt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Verdammt", "tokens": ["Ver\u00b7dammt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "In meinen zwei Pupillen.", "tokens": ["In", "mei\u00b7nen", "zwei", "Pu\u00b7pil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Donner gro\u00df und hoch der wilde Willen.", "tokens": ["Don\u00b7ner", "gro\u00df", "und", "hoch", "der", "wil\u00b7de", "Wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}