{"textgrid.poem.46709": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[morgenlicht!]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Morgenlicht!", "tokens": ["Mor\u00b7gen\u00b7licht", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Weckst du heut' und morgen nicht", "tokens": ["Weckst", "du", "heut'", "und", "mor\u00b7gen", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "KON", "ADV", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine schlafen gegangnen,", "tokens": ["Mei\u00b7ne", "schla\u00b7fen", "ge\u00b7gang\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Die vom Schlummer umfangnen,", "tokens": ["Die", "vom", "Schlum\u00b7mer", "um\u00b7fang\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Dicht von Schatten umhangnen, dicht?", "tokens": ["Dicht", "von", "Schat\u00b7ten", "um\u00b7hang\u00b7nen", ",", "dicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVINF", "$,", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Die vom Schlummer umfangnen,", "tokens": ["Die", "vom", "Schlum\u00b7mer", "um\u00b7fang\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Meine schlafen gegangnen,", "tokens": ["Mei\u00b7ne", "sc