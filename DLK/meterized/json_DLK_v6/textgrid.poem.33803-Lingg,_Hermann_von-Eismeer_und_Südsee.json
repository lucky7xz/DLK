{"textgrid.poem.33803": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Eismeer und S\u00fcdsee", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im h\u00f6chsten Nordmeer liegt ein Schiff, an Schollen Eises festgeschraubt,", "tokens": ["Im", "h\u00f6chs\u00b7ten", "Nord\u00b7meer", "liegt", "ein", "Schiff", ",", "an", "Schol\u00b7len", "Ei\u00b7ses", "fest\u00b7ge\u00b7schraubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "APPR", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Die Mannschaft auf dem Decke schl\u00e4ft, der Schnee liegt \u00fcber ihrem Haupt.", "tokens": ["Die", "Mann\u00b7schaft", "auf", "dem", "De\u00b7cke", "schl\u00e4ft", ",", "der", "Schnee", "liegt", "\u00fc\u00b7ber", "ih\u00b7rem", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Wie gellend auch der Nordwind pfeift, die Segel h\u00e4ngen eisumstarrt;", "tokens": ["Wie", "gel\u00b7lend", "auch", "der", "Nord\u00b7wind", "pfeift", ",", "die", "Se\u00b7gel", "h\u00e4n\u00b7gen", "ei\u00b7sum\u00b7starrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "ADV", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Kein Mast und keine Planke st\u00f6hnt, kein Tau und auch kein Ruder knarrt.", "tokens": ["Kein", "Mast", "und", "kei\u00b7ne", "Plan\u00b7ke", "st\u00f6hnt", ",", "kein", "Tau", "und", "auch", "kein", "Ru\u00b7der", "knarrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "VVFIN", "$,", "PIAT", "NN", "KON", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Doch jede Nacht das Nordlicht scheint und leuchtet in den wei\u00dfen Tod,", "tokens": ["Doch", "je\u00b7de", "Nacht", "das", "Nord\u00b7licht", "scheint", "und", "leuch\u00b7tet", "in", "den", "wei\u00b7\u00dfen", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Die hohlen Augen gl\u00fchen hell, die bleichen Wangen werden rot.", "tokens": ["Die", "hoh\u00b7len", "Au\u00b7gen", "gl\u00fc\u00b7hen", "hell", ",", "die", "blei\u00b7chen", "Wan\u00b7gen", "wer\u00b7den", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$,", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Es malen sich ins Segeltuch Eisblumen, riesig, tropengro\u00df,", "tokens": ["Es", "ma\u00b7len", "sich", "ins", "Se\u00b7gel\u00b7tuch", "Eis\u00b7blu\u00b7men", ",", "rie\u00b7sig", ",", "tro\u00b7pen\u00b7gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "NN", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Kristallne Bl\u00fcten, geisterhaft, kalt, unbewegt und d\u00fcftelos.", "tokens": ["Kris\u00b7tall\u00b7ne", "Bl\u00fc\u00b7ten", ",", "geis\u00b7ter\u00b7haft", ",", "kalt", ",", "un\u00b7be\u00b7wegt", "und", "d\u00fcf\u00b7te\u00b7los", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}}, "stanza.3": {"line.1": {"text": "Vom dunklen Eisgebirge sehn gewalt'ge Schatten schwarz herab,", "tokens": ["Vom", "dunk\u00b7len", "Eis\u00b7ge\u00b7bir\u00b7ge", "sehn", "ge\u00b7walt'\u00b7ge", "Schat\u00b7ten", "schwarz", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "ADJA", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Wie von der Urwelt Tieren, die versteint hier ruhn im Felsengrab,", "tokens": ["Wie", "von", "der", "Ur\u00b7welt", "Tie\u00b7ren", ",", "die", "ver\u00b7steint", "hier", "ruhn", "im", "Fel\u00b7sen\u00b7grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "$,", "PRELS", "VVFIN", "ADV", "VVINF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und gleich als g\u00e4rte jetzt noch tief, tief unterm Schnee die Feuerkraft,", "tokens": ["Und", "gleich", "als", "g\u00e4r\u00b7te", "jetzt", "noch", "tief", ",", "tief", "un\u00b7term", "Schnee", "die", "Feu\u00b7er\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "VVFIN", "ADV", "ADV", "ADJD", "$,", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "So rollt ein tiefer Donner oft, da\u00df weit das Eis in Schluchten klafft.", "tokens": ["So", "rollt", "ein", "tie\u00b7fer", "Don\u00b7ner", "oft", ",", "da\u00df", "weit", "das", "Eis", "in", "Schluch\u00b7ten", "klafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,", "KOUS", "ADJD", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Und in der S\u00fcdsee liegt ein Schiff, das liegt so still und unbewegt,", "tokens": ["Und", "in", "der", "S\u00fcd\u00b7see", "liegt", "ein", "Schiff", ",", "das", "liegt", "so", "still", "und", "un\u00b7be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,", "PDS", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Ins windstill blaue Meer hinein wie in ein offnes Grab gelegt.", "tokens": ["Ins", "wind\u00b7still", "blau\u00b7e", "Meer", "hin\u00b7ein", "wie", "in", "ein", "off\u00b7nes", "Grab", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "PTKVZ", "KOKOM", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Von Leichen ist der Bord bemannt, die sehn so hohl und ausgebrannt,", "tokens": ["Von", "Lei\u00b7chen", "ist", "der", "Bord", "be\u00b7mannt", ",", "die", "sehn", "so", "hohl", "und", "aus\u00b7ge\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Als h\u00e4tten ihre Mumien die Katakomben ausgesandt.", "tokens": ["Als", "h\u00e4t\u00b7ten", "ih\u00b7re", "Mu\u00b7mi\u00b7en", "die", "Ka\u00b7ta\u00b7kom\u00b7ben", "aus\u00b7ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Die Sandbank ward zum faulen Sumpf, und aus dem Sumpfe w\u00e4chst hervor", "tokens": ["Die", "Sand\u00b7bank", "ward", "zum", "fau\u00b7len", "Sumpf", ",", "und", "aus", "dem", "Sump\u00b7fe", "w\u00e4chst", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPRART", "ADJA", "NN", "$,", "KON", "APPR", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Ein \u00fcppig wuchernd Pflanzenreich von Seetang, Schimmel, Moos und Rohr.", "tokens": ["Ein", "\u00fcp\u00b7pig", "wu\u00b7chernd", "Pflan\u00b7zen\u00b7reich", "von", "See\u00b7tang", ",", "Schim\u00b7mel", ",", "Moos", "und", "Rohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "ADJD", "APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Verfaulend liegt das Fahrzeug da, aus jeder lecken Spalte fa\u00dft", "tokens": ["Ver\u00b7fau\u00b7lend", "liegt", "das", "Fahr\u00b7zeug", "da", ",", "aus", "je\u00b7der", "le\u00b7cken", "Spal\u00b7te", "fa\u00dft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "ART", "NN", "PTKVZ", "$,", "APPR", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Ein Gr\u00fcnes Wurzel und erhebt sich rankend bis empor zum Mast.", "tokens": ["Ein", "Gr\u00fc\u00b7nes", "Wur\u00b7zel", "und", "er\u00b7hebt", "sich", "ran\u00b7kend", "bis", "em\u00b7por", "zum", "Mast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "PRF", "VVPP", "KON", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Von gr\u00fcnem Laubdach ist bedeckt das Haupt der Toten fort und fort,", "tokens": ["Von", "gr\u00fc\u00b7nem", "Laub\u00b7dach", "ist", "be\u00b7deckt", "das", "Haupt", "der", "To\u00b7ten", "fort", "und", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ADJD", "ART", "NN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und Blumen bl\u00fchn aus ihrem Mund, als spr\u00e4chen sie das Lebenswort.", "tokens": ["Und", "Blu\u00b7men", "bl\u00fchn", "aus", "ih\u00b7rem", "Mund", ",", "als", "spr\u00e4\u00b7chen", "sie", "das", "Le\u00b7bens\u00b7wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "KOUS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Statt Wimpeln weht das lange Schilf, und wo die Schiffslatern' gebrannt,", "tokens": ["Statt", "Wim\u00b7peln", "weht", "das", "lan\u00b7ge", "Schilf", ",", "und", "wo", "die", "Schiffs\u00b7la\u00b7tern'", "ge\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Fliegt Nachts ein gr\u00fcner Gl\u00fchwurm auf und leuchtet wie ein Diamant.", "tokens": ["Fliegt", "Nachts", "ein", "gr\u00fc\u00b7ner", "Gl\u00fch\u00b7wurm", "auf", "und", "leuch\u00b7tet", "wie", "ein", "Di\u00b7a\u00b7mant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "KON", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}