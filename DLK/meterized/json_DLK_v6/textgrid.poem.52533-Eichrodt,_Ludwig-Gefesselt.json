{"textgrid.poem.52533": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Gefesselt", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebesgl\u00fcck und Liebesschmerz \u2013", "tokens": ["Lie\u00b7bes\u00b7gl\u00fcck", "und", "Lie\u00b7bes\u00b7schmerz", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Minute macht zum Sklaven,", "tokens": ["Die", "Mi\u00b7nu\u00b7te", "macht", "zum", "Skla\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O des Gottes Pfeile trafen", "tokens": ["O", "des", "Got\u00b7tes", "Pfei\u00b7le", "tra\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein gest\u00e4hlt gewappnet Herz.", "tokens": ["Mein", "ge\u00b7st\u00e4hlt", "ge\u00b7wapp\u00b7net", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Trage Ketten, golden s\u00fc\u00df,", "tokens": ["Tra\u00b7ge", "Ket\u00b7ten", ",", "gol\u00b7den", "s\u00fc\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber immer sind es Ketten,", "tokens": ["A\u00b7ber", "im\u00b7mer", "sind", "es", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Goldne Ketten, s\u00fc\u00dfe Ketten,", "tokens": ["Gold\u00b7ne", "Ket\u00b7ten", ",", "s\u00fc\u00b7\u00dfe", "Ket\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber Ketten sinds gewi\u00df.", "tokens": ["A\u00b7ber", "Ket\u00b7ten", "sinds", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In des Lebens Bl\u00fcthenzeit", "tokens": ["In", "des", "Le\u00b7bens", "Bl\u00fc\u00b7then\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief verletzt und schwer gebunden,", "tokens": ["Tief", "ver\u00b7letzt", "und", "schwer", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in Fesseln und in Wunden", "tokens": ["Und", "in", "Fes\u00b7seln", "und", "in", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dennoch diese Seligkeit?", "tokens": ["Den\u00b7noch", "die\u00b7se", "Se\u00b7lig\u00b7keit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}