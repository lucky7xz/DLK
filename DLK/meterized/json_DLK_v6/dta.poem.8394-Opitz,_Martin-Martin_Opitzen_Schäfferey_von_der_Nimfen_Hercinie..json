{"dta.poem.8394": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Martin Opitzen Sch\u00e4fferey  \n von der Nimfen Hercinie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1630", "urn": "urn:nbn:de:kobv:b4-200905197863", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Sch\u00e4fferey Von der Nimfen Hercinie. Breslau, 1630."}, "poem": {"stanza.1": {"line.1": {"text": "Du Hochberedter mann/ dem Rom mu\u00df schuldig sein", "tokens": ["Du", "Hoch\u00b7be\u00b7red\u00b7ter", "mann", "/", "dem", "Rom", "mu\u00df", "schul\u00b7dig", "sein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$(", "ART", "NE", "VMFIN", "ADJD", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die freyheit/ vndt sich selbst/ vndt alle sein Latein/", "tokens": ["Die", "frey\u00b7heit", "/", "vndt", "sich", "selbst", "/", "vndt", "al\u00b7le", "sein", "La\u00b7tein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "PRF", "ADV", "$(", "KON", "PIS", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der waldt hier der durch dich in newen baw ist kommen/", "tokens": ["Der", "waldt", "hier", "der", "durch", "dich", "in", "ne\u00b7wen", "baw", "ist", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "APPR", "PPER", "APPR", "ADJA", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Di\u00df Vorwerg wo du dir zue schreiben f\u00fcrgenommen/", "tokens": ["Di\u00df", "Vor\u00b7werg", "wo", "du", "dir", "zue", "schrei\u00b7ben", "f\u00fcr\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PWAV", "PPER", "PPER", "PTKZU", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zue suchen deine rhue/ ist schoner als zuevor:", "tokens": ["Zue", "su\u00b7chen", "dei\u00b7ne", "rhue", "/", "ist", "scho\u00b7ner", "als", "zu\u00b7e\u00b7vor", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "VVFIN", "$(", "VAFIN", "ADJD", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Vndt di\u00df noch nicht genung; es springt ein quell empor/", "tokens": ["Vndt", "di\u00df", "noch", "nicht", "ge\u00b7nung", ";", "es", "springt", "ein", "quell", "em\u00b7por", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "PTKNEG", "ADV", "$.", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein newes wunderquell/ das vnter andern sachen", "tokens": ["Ein", "ne\u00b7wes", "wun\u00b7der\u00b7quell", "/", "das", "vn\u00b7ter", "an\u00b7dern", "sa\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "APPR", "PIS", "VVINF"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Ein bl\u00f6des angesicht kan klar vndt lauter machen.", "tokens": ["Ein", "bl\u00f6\u00b7des", "an\u00b7ge\u00b7sicht", "kan", "klar", "vndt", "lau\u00b7ter", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der ort/ o Cicero/ thut dieses alles dir/", "tokens": ["Der", "ort", "/", "o", "Ci\u00b7ce\u00b7ro", "/", "thut", "die\u00b7ses", "al\u00b7les", "dir", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "FM", "FM", "$(", "VVFIN", "PDAT", "PIS", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der edle brunnen quillet nur wegen dein herf\u00fcr:", "tokens": ["Der", "ed\u00b7le", "brun\u00b7nen", "quil\u00b7let", "nur", "we\u00b7gen", "dein", "her\u00b7f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Dann weil man weit vndt breit dich lesen wirdt auff erden/", "tokens": ["Dann", "weil", "man", "weit", "vndt", "breit", "dich", "le\u00b7sen", "wirdt", "auff", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "ADJD", "KON", "ADJD", "PPER", "VVINF", "VAFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So mu\u00df das wa\u00dfer auch der augen artzney werden.", "tokens": ["So", "mu\u00df", "das", "wa\u00b7\u00dfer", "auch", "der", "au\u00b7gen", "artz\u00b7ney", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "ADV", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}