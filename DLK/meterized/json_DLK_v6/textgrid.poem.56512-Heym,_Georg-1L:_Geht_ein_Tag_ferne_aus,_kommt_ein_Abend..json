{"textgrid.poem.56512": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geht ein Tag ferne aus, kommt ein Abend.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geht ein Tag ferne aus, kommt ein Abend.", "tokens": ["Geht", "ein", "Tag", "fer\u00b7ne", "aus", ",", "kommt", "ein", "A\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,", "VVFIN", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Brennt ein Stern in der H\u00f6he zur Nacht.", "tokens": ["Brennt", "ein", "Stern", "in", "der", "H\u00f6\u00b7he", "zur", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wehet das Gras. Und die Wege alle", "tokens": ["We\u00b7het", "das", "Gras", ".", "Und", "die", "We\u00b7ge", "al\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "KON", "ART", "NN", "PIAT"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Werden in D\u00e4mmrung zusammengebracht.", "tokens": ["Wer\u00b7den", "in", "D\u00e4mm\u00b7rung", "zu\u00b7sam\u00b7men\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Viele sind \u00fcber die Steige gegangen.", "tokens": ["Vie\u00b7le", "sind", "\u00fc\u00b7ber", "die", "Stei\u00b7ge", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Ihre Schatten sind ferne zu sehn,", "tokens": ["Ih\u00b7re", "Schat\u00b7ten", "sind", "fer\u00b7ne", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und sie tragen an schwankenden Stangen", "tokens": ["Und", "sie", "tra\u00b7gen", "an", "schwan\u00b7ken\u00b7den", "Stan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Ihre Fackeln, die wandern und wehn.", "tokens": ["Ih\u00b7re", "Fa\u00b7ckeln", ",", "die", "wan\u00b7dern", "und", "wehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "KON", "VVINF", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Mauern sind viele, und Gr\u00e4ber, und wenige B\u00e4ume.", "tokens": ["Mau\u00b7ern", "sind", "vie\u00b7le", ",", "und", "Gr\u00e4\u00b7ber", ",", "und", "we\u00b7ni\u00b7ge", "B\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "$,", "KON", "NN", "$,", "KON", "PIAT", "NN", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "Manche Tore darin, wo der Lorbeer trauert.", "tokens": ["Man\u00b7che", "To\u00b7re", "da\u00b7rin", ",", "wo", "der", "Lor\u00b7beer", "trau\u00b7ert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PAV", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-++--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Viele sitzen in Haufen \u00fcber den Kreuzen,", "tokens": ["Vie\u00b7le", "sit\u00b7zen", "in", "Hau\u00b7fen", "\u00fc\u00b7ber", "den", "Kreu\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Ihre Lichter beh\u00fctend, wenn der Regen schauert.", "tokens": ["Ih\u00b7re", "Lich\u00b7ter", "be\u00b7h\u00fc\u00b7tend", ",", "wenn", "der", "Re\u00b7gen", "schau\u00b7ert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Und ein Rot steckt im Walde, d\u00fcrr wie ein Finger,", "tokens": ["Und", "ein", "Rot", "steckt", "im", "Wal\u00b7de", ",", "d\u00fcrr", "wie", "ein", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wo der Abend h\u00e4nget in wolkiger Zeit", "tokens": ["Wo", "der", "A\u00b7bend", "h\u00e4n\u00b7get", "in", "wol\u00b7ki\u00b7ger", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit dem wenigen Licht. Und geringer", "tokens": ["Mit", "dem", "we\u00b7ni\u00b7gen", "Licht", ".", "Und", "ge\u00b7rin\u00b7ger"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "PIAT", "NN", "$.", "KON", "ADJA"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Rings ist das Nahe, und die Weite so weit.", "tokens": ["Rings", "ist", "das", "Na\u00b7he", ",", "und", "die", "Wei\u00b7te", "so", "weit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "KON", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Doch ewig ist der Wind, der nimmer schweiget", "tokens": ["Doch", "e\u00b7wig", "ist", "der", "Wind", ",", "der", "nim\u00b7mer", "schwei\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In dunklem Lande, herbstlich schon erbraunet,", "tokens": ["In", "dunk\u00b7lem", "Lan\u00b7de", ",", "herbst\u00b7lich", "schon", "er\u00b7brau\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der dunkle Bilder viel vor\u00fcber zeiget", "tokens": ["Der", "dunk\u00b7le", "Bil\u00b7der", "viel", "vor\u00b7\u00fc\u00b7ber", "zei\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dunkle Worte fl\u00fcchtig tr\u00fcbe raunet.", "tokens": ["Und", "dunk\u00b7le", "Wor\u00b7te", "fl\u00fcch\u00b7tig", "tr\u00fc\u00b7be", "rau\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}