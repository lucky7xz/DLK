{"dta.poem.16104": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "158.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der du erschufst die Welt, ohn' ihrer zu bed\u00fcrfen,", "tokens": ["Der", "du", "er\u00b7schufst", "die", "Welt", ",", "ohn'", "ih\u00b7rer", "zu", "be\u00b7d\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "$,", "APPR", "PPOSAT", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erschaffen hast du sie nach deiner Lieb' Entw\u00fcrfen,", "tokens": ["Er\u00b7schaf\u00b7fen", "hast", "du", "sie", "nach", "dei\u00b7ner", "Lieb'", "Ent\u00b7w\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nach deiner Weisheit Plan, dem Zwecke deiner Macht;", "tokens": ["Nach", "dei\u00b7ner", "Weis\u00b7heit", "Plan", ",", "dem", "Zwe\u00b7cke", "dei\u00b7ner", "Macht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und kein Nachdenken denkt, was du hast vorgedacht.", "tokens": ["Und", "kein", "Nach\u00b7den\u00b7ken", "denkt", ",", "was", "du", "hast", "vor\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Vorbringen kann kein Wort, was deins hervorgebracht.", "tokens": ["Vor\u00b7brin\u00b7gen", "kann", "kein", "Wort", ",", "was", "deins", "her\u00b7vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIAT", "NN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch hast du die Vernunft geschaffen, dich zu denken,", "tokens": ["Doch", "hast", "du", "die", "Ver\u00b7nunft", "ge\u00b7schaf\u00b7fen", ",", "dich", "zu", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Geist, nach dir den Flug, Unsichtbarer, zu lenken,", "tokens": ["Den", "Geist", ",", "nach", "dir", "den", "Flug", ",", "Un\u00b7sicht\u00b7ba\u00b7rer", ",", "zu", "len\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPER", "ART", "NN", "$,", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Sehnsucht Str\u00f6m', o Meer, in dich sich zu versenken:", "tokens": ["Der", "Sehn\u00b7sucht", "Str\u00f6m'", ",", "o", "Meer", ",", "in", "dich", "sich", "zu", "ver\u00b7sen\u00b7ken", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "FM", "NN", "$,", "APPR", "PPER", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Den wir am Anfang, den wir sehn am Ende stehn,", "tokens": ["Den", "wir", "am", "An\u00b7fang", ",", "den", "wir", "sehn", "am", "En\u00b7de", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dem wir kommen und zu dem wir alle gehn.", "tokens": ["Von", "dem", "wir", "kom\u00b7men", "und", "zu", "dem", "wir", "al\u00b7le", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVINF", "KON", "APPR", "PRELS", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Woher ich kam, wohin ich gehe, wei\u00df ich nicht,", "tokens": ["Wo\u00b7her", "ich", "kam", ",", "wo\u00b7hin", "ich", "ge\u00b7he", ",", "wei\u00df", "ich", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur dis, von Gott zu Gott, ist meine Zuversicht.", "tokens": ["Nur", "dis", ",", "von", "Gott", "zu", "Gott", ",", "ist", "mei\u00b7ne", "Zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "$,", "APPR", "NN", "APPR", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}