{"textgrid.poem.40645": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du h\u00e4ltst dich gut im Kampf, o Inselveste,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du h\u00e4ltst dich gut im Kampf, o Inselveste,", "tokens": ["Du", "h\u00e4ltst", "dich", "gut", "im", "Kampf", ",", "o", "In\u00b7sel\u00b7ves\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,", "FM", "FM", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Wog' und Wind, mit Schmugglern und Korsaren;", "tokens": ["Mit", "Wo\u00b7g'", "und", "Wind", ",", "mit", "Schmug\u00b7glern", "und", "Kor\u00b7sa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+--++-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Doch schlimmer sind die schmeichelnden Gefahren,", "tokens": ["Doch", "schlim\u00b7mer", "sind", "die", "schmei\u00b7cheln\u00b7den", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drum f\u00fcrcht' auch Rosenbl\u00e4tter, laue Weste!", "tokens": ["Drum", "f\u00fcrcht'", "auch", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter", ",", "lau\u00b7e", "Wes\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Jetzt landen hier, Parf\u00fcm in Wort und Haaren,", "tokens": ["Jetzt", "lan\u00b7den", "hier", ",", "Par\u00b7f\u00fcm", "in", "Wort", "und", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit seidnem Kleid und Sinn, die schlimmern G\u00e4ste;", "tokens": ["Mit", "seid\u00b7nem", "Kleid", "und", "Sinn", ",", "die", "schlim\u00b7mern", "G\u00e4s\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wegsp\u00fclt das Meer vielleicht ihr Leibgebreste,", "tokens": ["Weg\u00b7sp\u00fclt", "das", "Meer", "viel\u00b7leicht", "ihr", "Leib\u00b7ge\u00b7bres\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch nicht, woran die Seele krankt den Schaaren.", "tokens": ["Doch", "nicht", ",", "wo\u00b7ran", "die", "See\u00b7le", "krankt", "den", "Schaa\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PWAV", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der alte Feind nagt an dem Felsenneste,", "tokens": ["Der", "al\u00b7te", "Feind", "nagt", "an", "dem", "Fel\u00b7sen\u00b7nes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der neue Freund an deiner alten Sitte,", "tokens": ["Der", "neu\u00b7e", "Freund", "an", "dei\u00b7ner", "al\u00b7ten", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch Fels und Sitte ruhn in festem Kitte;", "tokens": ["Doch", "Fels", "und", "Sit\u00b7te", "ruhn", "in", "fes\u00b7tem", "Kit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So wahrst du noch von beiden heil'ge Reste,", "tokens": ["So", "wahrst", "du", "noch", "von", "bei\u00b7den", "heil'\u00b7ge", "Res\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch St\u00fcck um St\u00fcck zerbr\u00f6ckeln sie, und leise", "tokens": ["Doch", "St\u00fcck", "um", "St\u00fcck", "zer\u00b7br\u00f6\u00b7ckeln", "sie", ",", "und", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN", "PPER", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ins Meer auch sinkt der V\u00e4ter schlichte Weise.", "tokens": ["Ins", "Meer", "auch", "sinkt", "der", "V\u00e4\u00b7ter", "schlich\u00b7te", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}