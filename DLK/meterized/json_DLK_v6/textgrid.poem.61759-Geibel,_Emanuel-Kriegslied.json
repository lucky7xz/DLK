{"textgrid.poem.61759": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Kriegslied", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Empor, mein Volk! Das Schwert zur Hand!", "tokens": ["Em\u00b7por", ",", "mein", "Volk", "!", "Das", "Schwert", "zur", "Hand", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$.", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und brich hervor in Haufen!", "tokens": ["Und", "brich", "her\u00b7vor", "in", "Hau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vom heil'gen Zorn ums Vaterland", "tokens": ["Vom", "heil'\u00b7gen", "Zorn", "ums", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Feuer la\u00df dich taufen!", "tokens": ["Mit", "Feu\u00b7er", "la\u00df", "dich", "tau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Erbfeind beut dir Schmach und Spott,", "tokens": ["Der", "Erb\u00b7feind", "beut", "dir", "Schmach", "und", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Ma\u00df ist voll, zur Schlacht mit Gott!", "tokens": ["Das", "Ma\u00df", "ist", "voll", ",", "zur", "Schlacht", "mit", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vorw\u00e4rts!", "tokens": ["Vor\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Dein Haus in Frieden auszubaun,", "tokens": ["Dein", "Haus", "in", "Frie\u00b7den", "aus\u00b7zu\u00b7baun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stand all dein Sinn und Wollen,", "tokens": ["Stand", "all", "dein", "Sinn", "und", "Wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da bricht den Hader er vom Zaun", "tokens": ["Da", "bricht", "den", "Ha\u00b7der", "er", "vom", "Zaun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Gift und Neid geschwollen.", "tokens": ["Von", "Gift", "und", "Neid", "ge\u00b7schwol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Komm' \u00fcber ihn und seine Brut", "tokens": ["Komm'", "\u00fc\u00b7ber", "ihn", "und", "sei\u00b7ne", "Brut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das frevelhaft vergo\u00dfne Blut!", "tokens": ["Das", "fre\u00b7vel\u00b7haft", "ver\u00b7go\u00df\u00b7ne", "Blut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vorw\u00e4rts!", "tokens": ["Vor\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Wir tr\u00e4umen nicht von raschem Sieg,", "tokens": ["Wir", "tr\u00e4u\u00b7men", "nicht", "von", "ra\u00b7schem", "Sieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von leichten Ruhmesz\u00fcgen,", "tokens": ["Von", "leich\u00b7ten", "Ruh\u00b7mes\u00b7z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Weltgericht ist dieser Krieg,", "tokens": ["Ein", "Welt\u00b7ge\u00b7richt", "ist", "die\u00b7ser", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und stark der Geist der L\u00fcgen.", "tokens": ["Und", "stark", "der", "Geist", "der", "L\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch der einst unsrer V\u00e4ter Burg,", "tokens": ["Doch", "der", "einst", "uns\u00b7rer", "V\u00e4\u00b7ter", "Burg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Getrost, er f\u00fchrt auch uns hindurch!", "tokens": ["Ge\u00b7trost", ",", "er", "f\u00fchrt", "auch", "uns", "hin\u00b7durch", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "PPER", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vorw\u00e4rts!", "tokens": ["Vor\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Schon l\u00e4\u00dft er klar bei Tag und Nacht", "tokens": ["Schon", "l\u00e4\u00dft", "er", "klar", "bei", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uns seine Zeichen schauen,", "tokens": ["Uns", "sei\u00b7ne", "Zei\u00b7chen", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Flammen hat er angefacht", "tokens": ["Die", "Flam\u00b7men", "hat", "er", "an\u00b7ge\u00b7facht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In allen deutschen Gauen.", "tokens": ["In", "al\u00b7len", "deut\u00b7schen", "Gau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von Stamm zu Stamme lodert's fort:", "tokens": ["Von", "Stamm", "zu", "Stam\u00b7me", "lo\u00b7dert's", "fort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Mainstrom mehr, kein S\u00fcd und Nord!", "tokens": ["Kein", "Ma\u00b7ins\u00b7trom", "mehr", ",", "kein", "S\u00fcd", "und", "Nord", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$,", "PIAT", "NN", "KON", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Vorw\u00e4rts!", "tokens": ["Vor\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "Voran denn, k\u00fchner Preu\u00dfenaar,", "tokens": ["Vo\u00b7ran", "denn", ",", "k\u00fch\u00b7ner", "Preu\u00b7\u00dfe\u00b7naar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Voran durch Schlacht und Grausen!", "tokens": ["Vo\u00b7ran", "durch", "Schlacht", "und", "Grau\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie Sturmwind schwellt dein Fl\u00fcgelpaar", "tokens": ["Wie", "Sturm\u00b7wind", "schwellt", "dein", "Fl\u00fc\u00b7gel\u00b7paar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Himmel her ein Brausen,", "tokens": ["Vom", "Him\u00b7mel", "her", "ein", "Brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APZR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das ist des alten Bl\u00fcchers Geist,", "tokens": ["Das", "ist", "des", "al\u00b7ten", "Bl\u00fc\u00b7chers", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der dir die rechte Stra\u00dfe weist.", "tokens": ["Der", "dir", "die", "rech\u00b7te", "Stra\u00b7\u00dfe", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vorw\u00e4rts!", "tokens": ["Vor\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.6": {"line.1": {"text": "Flieg, Adler, flieg! Wir st\u00fcrmen nach,", "tokens": ["Flieg", ",", "Ad\u00b7ler", ",", "flieg", "!", "Wir", "st\u00fcr\u00b7men", "nach", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$.", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein einig Volk in Waffen.", "tokens": ["Ein", "ei\u00b7nig", "Volk", "in", "Waf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir st\u00fcrmen nach, ob tausendfach", "tokens": ["Wir", "st\u00fcr\u00b7men", "nach", ",", "ob", "tau\u00b7send\u00b7fach"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Todes Pforten klaffen.", "tokens": ["Des", "To\u00b7des", "Pfor\u00b7ten", "klaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und fallen wir: Flieg, Adler, flieg!", "tokens": ["Und", "fal\u00b7len", "wir", ":", "Flieg", ",", "Ad\u00b7ler", ",", "flieg", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "NN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus unsrem Blute w\u00e4chst der Sieg.", "tokens": ["Aus", "uns\u00b7rem", "Blu\u00b7te", "w\u00e4chst", "der", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vorw\u00e4rts!", "tokens": ["Vor\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}