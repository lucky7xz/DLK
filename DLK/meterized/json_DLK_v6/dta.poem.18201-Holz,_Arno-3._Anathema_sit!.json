{"dta.poem.18201": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "3.  \n  Anathema sit!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Viele stehn im Daniel Sanders,", "tokens": ["Vie\u00b7le", "stehn", "im", "Da\u00b7ni\u00b7el", "San\u00b7ders", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NE", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Viele stehn auch noch wo anders,", "tokens": ["Vie\u00b7le", "stehn", "auch", "noch", "wo", "an\u00b7ders", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "PWAV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch verha\u00dft vor allen sind", "tokens": ["Doch", "ver\u00b7ha\u00dft", "vor", "al\u00b7len", "sind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PIS", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese mir, mein liebes Kind:", "tokens": ["Die\u00b7se", "mir", ",", "mein", "lie\u00b7bes", "Kind", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weihrauchf\u00e4sser und Crucifixe,", "tokens": ["Weih\u00b7rauch\u00b7f\u00e4s\u00b7ser", "und", "Cru\u00b7ci\u00b7fi\u00b7xe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+++-+", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Tinte, Schwefel und Stiefelwichse,", "tokens": ["Tin\u00b7te", ",", "Schwe\u00b7fel", "und", "Stie\u00b7fel\u00b7wich\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Englische Peers und russische Knuten,", "tokens": ["Eng\u00b7li\u00b7sche", "Peers", "und", "rus\u00b7si\u00b7sche", "Knu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "T\u00fcrkische Paschahs und deutsche Rekruten,", "tokens": ["T\u00fcr\u00b7ki\u00b7sche", "Pa\u00b7schahs", "und", "deut\u00b7sche", "Rek\u00b7ru\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+---+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Throne, Kasernen und Schweinekofen,", "tokens": ["Thro\u00b7ne", ",", "Ka\u00b7ser\u00b7nen", "und", "Schwei\u00b7ne\u00b7ko\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-----+-+-", "measure": "dactylic.init"}, "line.10": {"text": "Parven\u00fcs und Naturphilosophen,", "tokens": ["Par\u00b7ve\u00b7n\u00fcs", "und", "Na\u00b7tur\u00b7phi\u00b7lo\u00b7so\u00b7phen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Enten, Seeschlangen, Juden und Zwiebeln,", "tokens": ["En\u00b7ten", ",", "See\u00b7schlan\u00b7gen", ",", "Ju\u00b7den", "und", "Zwie\u00b7beln", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Alte Jungfern und enge Stiebeln,", "tokens": ["Al\u00b7te", "Jung\u00b7fern", "und", "en\u00b7ge", "Stie\u00b7beln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Wei\u00dfe Handschuh und schwarze Fr\u00e4cke,", "tokens": ["Wei\u00b7\u00dfe", "Hand\u00b7schuh", "und", "schwar\u00b7ze", "Fr\u00e4\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Krinolinen und Chapeau cl\u00e4que,", "tokens": ["Kri\u00b7no\u00b7li\u00b7nen", "und", "Cha\u00b7pe\u00b7au", "cl\u00e4\u00b7que", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "NE", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "H\u00e4morrhoiden, Milben und Maden,", "tokens": ["H\u00e4\u00b7morr\u00b7ho\u00b7i\u00b7den", ",", "Mil\u00b7ben", "und", "Ma\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "Gardeleutnants und Wachtparaden,", "tokens": ["Gar\u00b7de\u00b7leut\u00b7nants", "und", "Wacht\u00b7pa\u00b7ra\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Filsl\u00e4use, Fl\u00f6he, Motten und Wanzen,", "tokens": ["Fils\u00b7l\u00e4u\u00b7se", ",", "Fl\u00f6\u00b7he", ",", "Mot\u00b7ten", "und", "Wan\u00b7zen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.18": {"text": "Hofr\u00e4the, Kammerherren und Schranzen,", "tokens": ["Hof\u00b7r\u00e4\u00b7the", ",", "Kam\u00b7mer\u00b7her\u00b7ren", "und", "Schran\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ballerinen und Syphiliten,", "tokens": ["Bal\u00b7le\u00b7ri\u00b7nen", "und", "Sy\u00b7phi\u00b7li\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Schwiegerm\u00fctter und Jesuiten,", "tokens": ["Schwie\u00b7ger\u00b7m\u00fct\u00b7ter", "und", "Je\u00b7su\u00b7i\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Unterr\u00f6cke, Corsetts und Matratzen,", "tokens": ["Un\u00b7ter\u00b7r\u00f6\u00b7cke", ",", "Cor\u00b7setts", "und", "Ma\u00b7trat\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "KON", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Vipern, Kokotten und Kammerkatzen,", "tokens": ["Vi\u00b7pern", ",", "Ko\u00b7kot\u00b7ten", "und", "Kam\u00b7mer\u00b7kat\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Toaste, Kloaken und Vitriole,", "tokens": ["Toas\u00b7te", ",", "Klo\u00b7a\u00b7ken", "und", "Vi\u00b7tri\u00b7o\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Panzerfregatten und Staatsmonopole,", "tokens": ["Pan\u00b7zer\u00b7fre\u00b7gat\u00b7ten", "und", "Staats\u00b7mo\u00b7no\u00b7po\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-+++-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Kriegervereine und Schwadroneure,", "tokens": ["Krie\u00b7ger\u00b7ver\u00b7ei\u00b7ne", "und", "Schwad\u00b7ro\u00b7neu\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Luccagagen und Opernten\u00f6re,", "tokens": ["Luc\u00b7ca\u00b7ga\u00b7gen", "und", "O\u00b7pern\u00b7te\u00b7n\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Wagnerianer und Patrioten,", "tokens": ["Wag\u00b7ne\u00b7ri\u00b7a\u00b7ner", "und", "Pat\u00b7ri\u00b7o\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Schneider, Kastraten und Idioten,", "tokens": ["Schnei\u00b7der", ",", "Kas\u00b7tra\u00b7ten", "und", "I\u00b7dio\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Falsche Z\u00f6pfe und echte Biere,", "tokens": ["Fal\u00b7sche", "Z\u00f6p\u00b7fe", "und", "ech\u00b7te", "Bie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "N\u00e4hmaschinen und Klimperklaviere,", "tokens": ["N\u00e4h\u00b7ma\u00b7schi\u00b7nen", "und", "Klim\u00b7per\u00b7kla\u00b7vie\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Schnupfstabackdosen und M\u00e4dchen f\u00fcr Alles", "tokens": ["Schnupf\u00b7sta\u00b7back\u00b7do\u00b7sen", "und", "M\u00e4d\u00b7chen", "f\u00fcr", "Al\u00b7les"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPR", "PIS"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Und \u2014 ", "tokens": ["Und"], "token_info": ["word", "punct"], "pos": ["KON", "$("], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Sympathisch zwar und angenehm", "tokens": ["Sym\u00b7pa\u00b7thisch", "zwar", "und", "an\u00b7ge\u00b7nehm"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ist meiner Treu mir keins von dem,", "tokens": ["Ist", "mei\u00b7ner", "Treu", "mir", "keins", "von", "dem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "PIAT", "APPR", "ART", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Doch bei vern\u00fcnftiger Beschauung", "tokens": ["Doch", "bei", "ver\u00b7n\u00fcnf\u00b7ti\u00b7ger", "Be\u00b7schau\u00b7ung"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "St\u00f6rt mir auch keines die Verdauung.", "tokens": ["St\u00f6rt", "mir", "auch", "kei\u00b7nes", "die", "Ver\u00b7dau\u00b7ung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "So leb ich lustig ", "tokens": ["So", "leb", "ich", "lus\u00b7tig"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Wie jener Mops im Paletot.", "tokens": ["Wie", "je\u00b7ner", "Mops", "im", "Pa\u00b7le\u00b7tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Nur Eins macht stets mich tapfer weichen", "tokens": ["Nur", "Eins", "macht", "stets", "mich", "tap\u00b7fer", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "ADV", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Und l\u00e4\u00dft mich kreidewei\u00df erbleichen ....", "tokens": ["Und", "l\u00e4\u00dft", "mich", "krei\u00b7de\u00b7wei\u00df", "er\u00b7blei\u00b7chen", "...."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "O Gott, mir wird das Herz so schwer:", "tokens": ["O", "Gott", ",", "mir", "wird", "das", "Herz", "so", "schwer", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Nachbarin, euer Fl\u00e4schchen her!", "tokens": ["Nach\u00b7ba\u00b7rin", ",", "eu\u00b7er", "Fl\u00e4\u00b7schchen", "her", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.25": {"text": "Das Wort bleibt mir im Halse stecken,", "tokens": ["Das", "Wort", "bleibt", "mir", "im", "Hal\u00b7se", "ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "So oft ich auch daran gedacht \u2014", "tokens": ["So", "oft", "ich", "auch", "da\u00b7ran", "ge\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "PAV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Das ist der schrecklichste der Schrecken:", "tokens": ["Das", "ist", "der", "schreck\u00b7lichs\u00b7te", "der", "Schre\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}