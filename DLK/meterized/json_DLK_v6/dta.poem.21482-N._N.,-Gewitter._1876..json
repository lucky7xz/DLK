{"dta.poem.21482": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Gewitter.  \n 1876.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Den ganzen Abend hat es schon gegrollt               ", "tokens": ["Den", "gan\u00b7zen", "A\u00b7bend", "hat", "es", "schon", "ge\u00b7grollt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und bang gefl\u00fcstert in dem dunklen Laube,", "tokens": ["Und", "bang", "ge\u00b7fl\u00fcs\u00b7tert", "in", "dem", "dunk\u00b7len", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Landweg kam in Wind der Staub gerollt,", "tokens": ["Am", "Land\u00b7weg", "kam", "in", "Wind", "der", "Staub", "ge\u00b7rollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Wolke flog geh\u00fcllt in dunkle Haube,", "tokens": ["Die", "Wol\u00b7ke", "flog", "ge\u00b7h\u00fcllt", "in", "dunk\u00b7le", "Hau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Scheu hat der Vogel sich ins Nest geduckt,", "tokens": ["Scheu", "hat", "der", "Vo\u00b7gel", "sich", "ins", "Nest", "ge\u00b7duckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NE", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Der Hase barg sich in dem Laub voll Schrecken,", "tokens": ["Der", "Ha\u00b7se", "barg", "sich", "in", "dem", "Laub", "voll", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Als fern im Ost der erste Blitz gezuckt,", "tokens": ["Als", "fern", "im", "Ost", "der", "ers\u00b7te", "Blitz", "ge\u00b7zuckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der erste Regen rauschte durch die Hecken.", "tokens": ["Der", "ers\u00b7te", "Re\u00b7gen", "rauschte", "durch", "die", "He\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Nun ist\u2019s herauf, hinsaust die tolle Jagd", "tokens": ["Nun", "ist's", "her\u00b7auf", ",", "hin\u00b7saust", "die", "tol\u00b7le", "Jagd"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PTKVZ", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Sturmes durch den Schlo\u00dfhof, in dem Weiher", "tokens": ["Des", "Stur\u00b7mes", "durch", "den", "Schlo\u00df\u00b7hof", ",", "in", "dem", "Wei\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "W\u00fchlt dumpf die Flut, wie dunkle Winternacht", "tokens": ["W\u00fchlt", "dumpf", "die", "Flut", ",", "wie", "dunk\u00b7le", "Win\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "H\u00e4ngt \u00fcber Thurm und Dach der Wolkenschleier,", "tokens": ["H\u00e4ngt", "\u00fc\u00b7ber", "Thurm", "und", "Dach", "der", "Wol\u00b7ken\u00b7schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Wipfel sausen und das Schilfrohr pfeift \u2014", "tokens": ["Die", "Wip\u00b7fel", "sau\u00b7sen", "und", "das", "Schilf\u00b7rohr", "pfeift"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein toller Junker, geht\u2019s durch Teich und Binsen,", "tokens": ["Ein", "tol\u00b7ler", "Jun\u00b7ker", ",", "geht's", "durch", "Teich", "und", "Bin\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Hei, wie der Nebeldunst vor\u00fcber schleift,", "tokens": ["Hei", ",", "wie", "der", "Ne\u00b7bel\u00b7dunst", "vor\u00b7\u00fc\u00b7ber", "schleift", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Ein H\u00f6llenzug mit Winseln und mit Grinsen.", "tokens": ["Ein", "H\u00f6l\u00b7len\u00b7zug", "mit", "Win\u00b7seln", "und", "mit", "Grin\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hahi und Hussa, wie das jagt und tollt.", "tokens": ["Ha\u00b7hi", "und", "Hus\u00b7sa", ",", "wie", "das", "jagt", "und", "tollt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$,", "PWAV", "PDS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Blitz f\u00e4llt zuckend hin, auf erz\u2019nem Wagen", "tokens": ["Der", "Blitz", "f\u00e4llt", "zu\u00b7ckend", "hin", ",", "auf", "er\u00b7z'\u00b7nem", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Kommt krachend hinterher der Donner angerollt,", "tokens": ["Kommt", "kra\u00b7chend", "hin\u00b7ter\u00b7her", "der", "Don\u00b7ner", "an\u00b7ge\u00b7rollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom Wolkenmantel dicht den Leib umschlagen.", "tokens": ["Vom", "Wol\u00b7ken\u00b7man\u00b7tel", "dicht", "den", "Leib", "um\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Feuerstrahl f\u00e4hrt prasselnd aus dem Wald,", "tokens": ["Ein", "Feu\u00b7er\u00b7strahl", "f\u00e4hrt", "pras\u00b7selnd", "aus", "dem", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und jach zum Himmel blitzen Flammenfluthen,", "tokens": ["Und", "jach", "zum", "Him\u00b7mel", "blit\u00b7zen", "Flam\u00b7men\u00b7flu\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Drein jagt der Sturm, da\u00df Hang und Heide hallt,", "tokens": ["Drein", "jagt", "der", "Sturm", ",", "da\u00df", "Hang", "und", "Hei\u00b7de", "hallt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "NN", "KON", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und peitscht die L\u00fcfte mit rothgl\u00fch\u2019nden Ruthen.", "tokens": ["Und", "peitscht", "die", "L\u00fcf\u00b7te", "mit", "ro\u00b7th\u00b7gl\u00fch'n\u00b7den", "Ru\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "O, k\u00f6nnt\u2019 ich doch auf dieser Wolken Nacht", "tokens": ["O", ",", "k\u00f6nnt'", "ich", "doch", "auf", "die\u00b7ser", "Wol\u00b7ken", "Nacht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VMFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In Feuerlettern meine Dichtung schreiben,", "tokens": ["In", "Feu\u00b7er\u00b7let\u00b7tern", "mei\u00b7ne", "Dich\u00b7tung", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Dichtung, h\u00f6ll- und himmelhei\u00df entfacht,", "tokens": ["Die", "Dich\u00b7tung", ",", "h\u00f6ll", "und", "him\u00b7mel\u00b7hei\u00df", "ent\u00b7facht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "TRUNC", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und mit dem Sturm durch alle Lande treiben.", "tokens": ["Und", "mit", "dem", "Sturm", "durch", "al\u00b7le", "Lan\u00b7de", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann sollte, wie bei wirbelndem Trommelklang,", "tokens": ["Dann", "soll\u00b7te", ",", "wie", "bei", "wir\u00b7beln\u00b7dem", "Trom\u00b7mel\u00b7klang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+----+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "Die Menschheit aus dem tr\u00e4gen Tr\u00e4umen schrecken,", "tokens": ["Die", "Menschheit", "aus", "dem", "tr\u00e4\u00b7gen", "Tr\u00e4u\u00b7men", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Schlafmordend sollte mein Gesang", "tokens": ["Schlaf\u00b7mor\u00b7dend", "soll\u00b7te", "mein", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPOSAT", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu heil\u2019gem Kampf die M\u00fcden wecken.", "tokens": ["Zu", "heil'\u00b7gem", "Kampf", "die", "M\u00fc\u00b7den", "we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}