{"textgrid.poem.35648": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "11. Deutschland", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was flogst du, Stolz des Deutschen, zur Sternenh\u00f6h',", "tokens": ["Was", "flogst", "du", ",", "Stolz", "des", "Deut\u00b7schen", ",", "zur", "Ster\u00b7nen\u00b7h\u00f6h'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "ART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und blickest l\u00e4chelnd nieder auf alles Volk,", "tokens": ["Und", "bli\u00b7ckest", "l\u00e4\u00b7chelnd", "nie\u00b7der", "auf", "al\u00b7les", "Volk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vom Aufgang bis zum Niedergange,", "tokens": ["Vom", "Auf\u00b7gang", "bis", "zum", "Nie\u00b7der\u00b7gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welchem du K\u00f6nig' und Feldherrn sandtest?", "tokens": ["Wel\u00b7chem", "du", "K\u00f6\u00b7nig'", "und", "Feld\u00b7herrn", "sand\u00b7test", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "H\u00f6rst du der Sklavenkette Gerassel nicht,", "tokens": ["H\u00f6rst", "du", "der", "Skla\u00b7ven\u00b7ket\u00b7te", "Ge\u00b7ras\u00b7sel", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "PTKNEG", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die uns der Franke (Fluch dir, o M\u00f6nch, der ihn", "tokens": ["Die", "uns", "der", "Fran\u00b7ke", "(", "Fluch", "dir", ",", "o", "M\u00f6nch", ",", "der", "ihn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "$(", "NN", "PPER", "$,", "FM", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Gro\u00dfen pries!) um unsern Nacken", "tokens": ["Den", "Gro\u00b7\u00dfen", "pries", "!", ")", "um", "un\u00b7sern", "Na\u00b7cken"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Warf, als, mit triefendem Stahl der Herrschsucht,", "tokens": ["Warf", ",", "als", ",", "mit", "trie\u00b7fen\u00b7dem", "Stahl", "der", "Herrschsucht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "$,", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Er, Gottes Sache l\u00fcgend, ein frommes Volk", "tokens": ["Er", ",", "Got\u00b7tes", "Sa\u00b7che", "l\u00fc\u00b7gend", ",", "ein", "from\u00b7mes", "Volk"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "NN", "VVPP", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Samt seinen Priestern schlachtet', und Wittekind,", "tokens": ["Samt", "sei\u00b7nen", "Pries\u00b7tern", "schlach\u00b7tet'", ",", "und", "Wit\u00b7te\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Statt Wodans unsichtbarer Gottheit,", "tokens": ["Statt", "Wo\u00b7dans", "un\u00b7sicht\u00b7ba\u00b7rer", "Got\u00b7theit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wurmigen G\u00f6tzen Geruch zu streun zwang?", "tokens": ["Wur\u00b7mi\u00b7gen", "G\u00f6t\u00b7zen", "Ge\u00b7ruch", "zu", "streun", "zwang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Nicht deutsches Herzens; Vater der Knechte dort,", "tokens": ["Nicht", "deut\u00b7sches", "Her\u00b7zens", ";", "Va\u00b7ter", "der", "Knech\u00b7te", "dort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$.", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Thuiskons Abart! kroch er zum stolzen Stuhl", "tokens": ["Thu\u00b7is\u00b7kons", "Ab\u00b7art", "!", "kroch", "er", "zum", "stol\u00b7zen", "Stuhl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des Pfaffen Roms, und schenkt', o Hermann,", "tokens": ["Des", "Pfaf\u00b7fen", "Roms", ",", "und", "schenkt'", ",", "o", "Her\u00b7mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "KON", "VVFIN", "$,", "FM", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Deine Cherusker dem Bann des W\u00fctrichs!", "tokens": ["Dei\u00b7ne", "Che\u00b7rus\u00b7ker", "dem", "Bann", "des", "W\u00fct\u00b7richs", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-----+-+-", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Nicht deutsches Herzens; Erbe des Julischen", "tokens": ["Nicht", "deut\u00b7sches", "Her\u00b7zens", ";", "Er\u00b7be", "des", "Ju\u00b7li\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADJA", "NN", "$.", "NN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Tyrannenthrones, gab er zur Armengift", "tokens": ["Ty\u00b7ran\u00b7nen\u00b7thro\u00b7nes", ",", "gab", "er", "zur", "Ar\u00b7men\u00b7gift"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Freiheitssang altdeutscher Tugend,", "tokens": ["Den", "Frei\u00b7heits\u00b7sang", "alt\u00b7deut\u00b7scher", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welchem die Adler in Winfeld sanken!", "tokens": ["Wel\u00b7chem", "die", "Ad\u00b7ler", "in", "Win\u00b7feld", "san\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Jetzt starb die Freiheit unter Despotenfu\u00df;", "tokens": ["Jetzt", "starb", "die", "Frei\u00b7heit", "un\u00b7ter", "Des\u00b7po\u00b7ten\u00b7fu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vernunft und Tugend floh vor dem Geierblick", "tokens": ["Ver\u00b7nunft", "und", "Tu\u00b7gend", "floh", "vor", "dem", "Gei\u00b7er\u00b7blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der feisten M\u00f6nch'; entmannte Harfen", "tokens": ["Der", "feis\u00b7ten", "M\u00f6n\u00b7ch'", ";", "ent\u00b7mann\u00b7te", "Har\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Fr\u00f6nten dem Wahn und dem goldnen Laster!", "tokens": ["Fr\u00f6n\u00b7ten", "dem", "Wahn", "und", "dem", "gold\u00b7nen", "Las\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "O weine, Stolberg! Weine! Sie rasselt noch", "tokens": ["O", "wei\u00b7ne", ",", "Stol\u00b7berg", "!", "Wei\u00b7ne", "!", "Sie", "ras\u00b7selt", "noch"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "NE", "$.", "NN", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Franken Kette! Wenige mochte nur,", "tokens": ["Des", "Fran\u00b7ken", "Ket\u00b7te", "!", "We\u00b7ni\u00b7ge", "moch\u00b7te", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "PIAT", "VVFIN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von Gott zum Heiland ausger\u00fcstet,", "tokens": ["Von", "Gott", "zum", "Hei\u00b7land", "aus\u00b7ge\u00b7r\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Luther dem schimpflichen Joch entrei\u00dfen!", "tokens": ["Lu\u00b7ther", "dem", "schimpf\u00b7li\u00b7chen", "Joch", "ent\u00b7rei\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Ruf' nicht dem Britten, da\u00df er in strahlender", "tokens": ["Ruf'", "nicht", "dem", "Brit\u00b7ten", ",", "da\u00df", "er", "in", "strah\u00b7len\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "ADJA"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Urv\u00e4ter Heimat sp\u00e4he der Tugend Sitz!", "tokens": ["Ur\u00b7v\u00e4\u00b7ter", "Hei\u00b7mat", "sp\u00e4\u00b7he", "der", "Tu\u00b7gend", "Sitz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Still trau'rt ein kleiner Rest des Samens,", "tokens": ["Still", "trau'rt", "ein", "klei\u00b7ner", "Rest", "des", "Sa\u00b7mens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welchen der Nachen des Angeln f\u00fchrte!", "tokens": ["Wel\u00b7chen", "der", "Na\u00b7chen", "des", "An\u00b7geln", "f\u00fchr\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Nach Wollust schnaubt der lodernde J\u00fcngling jetzt;", "tokens": ["Nach", "Wol\u00b7lust", "schnaubt", "der", "lo\u00b7dern\u00b7de", "J\u00fcng\u00b7ling", "jetzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Mann nach Gold; in lauer Geb\u00fcsche Nacht", "tokens": ["Der", "Mann", "nach", "Gold", ";", "in", "lau\u00b7er", "Ge\u00b7b\u00fc\u00b7sche", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$.", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Lustwandeln freche M\u00e4dchench\u00f6re,", "tokens": ["Lust\u00b7wan\u00b7deln", "fre\u00b7che", "M\u00e4d\u00b7chen\u00b7ch\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schmachtend in Galliens reichsten T\u00f6nen.", "tokens": ["Schmach\u00b7tend", "in", "Gal\u00b7li\u00b7ens", "reichs\u00b7ten", "T\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "O dichtet ihnen, S\u00e4nger Germanias,", "tokens": ["O", "dich\u00b7tet", "ih\u00b7nen", ",", "S\u00e4n\u00b7ger", "Ger\u00b7ma\u00b7ni\u00b7as", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein neues Buhllied! Singet den Horchenden", "tokens": ["Ein", "neu\u00b7es", "Buhl\u00b7lied", "!", "Sin\u00b7get", "den", "Hor\u00b7chen\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Des Rosenbetts geheime Zauber,", "tokens": ["Des", "Ro\u00b7sen\u00b7betts", "ge\u00b7hei\u00b7me", "Zau\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Oder die taumelnden Lustgelage!", "tokens": ["O\u00b7der", "die", "tau\u00b7meln\u00b7den", "Lust\u00b7ge\u00b7la\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Ein lautes H\u00e4ndeklatschen erwartet euch! \u2013", "tokens": ["Ein", "lau\u00b7tes", "H\u00e4n\u00b7de\u00b7klat\u00b7schen", "er\u00b7war\u00b7tet", "euch", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihr wollt nicht? Weiht der Tugend das ernste Spiel? \u2013", "tokens": ["Ihr", "wollt", "nicht", "?", "Weiht", "der", "Tu\u00b7gend", "das", "erns\u00b7te", "Spiel", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "$.", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ha! flieht, und sucht im fernen Norden", "tokens": ["Ha", "!", "flieht", ",", "und", "sucht", "im", "fer\u00b7nen", "Nor\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "$,", "KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Eurem verkannten Gesange H\u00f6rer!", "tokens": ["Eu\u00b7rem", "ver\u00b7kann\u00b7ten", "Ge\u00b7san\u00b7ge", "H\u00f6\u00b7rer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Vertilgt auf ewig seist du, o Schauernacht,", "tokens": ["Ver\u00b7tilgt", "auf", "e\u00b7wig", "seist", "du", ",", "o", "Schau\u00b7er\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJD", "VAFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da ich Jehovahs Dienste die Harfe schwur!", "tokens": ["Da", "ich", "Je\u00b7ho\u00b7vahs", "Diens\u00b7te", "die", "Har\u00b7fe", "schwur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Vertilgt, ihr Thr\u00e4nen, so ich einsam", "tokens": ["Ver\u00b7tilgt", ",", "ihr", "Thr\u00e4\u00b7nen", ",", "so", "ich", "ein\u00b7sam"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "PPOSAT", "NN", "$,", "ADV", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An den unsterblichen Malen weinte!", "tokens": ["An", "den", "uns\u00b7terb\u00b7li\u00b7chen", "Ma\u00b7len", "wein\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": 