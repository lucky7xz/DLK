{"textgrid.poem.31460": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein sch\u00f6ne Sach / im Leiden Fr\u00fcchte bringen!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein sch\u00f6ne Sach / im Leiden Fr\u00fcchte bringen!", "tokens": ["Ein", "sch\u00f6\u00b7ne", "Sach", "/", "im", "Lei\u00b7den", "Fr\u00fcch\u00b7te", "brin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die Edlen Stein / zeugt die gesalzne Flut.", "tokens": ["die", "Ed\u00b7len", "Stein", "/", "zeugt", "die", "ge\u00b7salz\u00b7ne", "Flut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es wird das Gold vollkommen in der Glut.", "tokens": ["Es", "wird", "das", "Gold", "voll\u00b7kom\u00b7men", "in", "der", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus hartem Fel\u00df die s\u00fcssen Brunnen springen.", "tokens": ["Aus", "har\u00b7tem", "Fel\u00df", "die", "s\u00fcs\u00b7sen", "Brun\u00b7nen", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Rose mu\u00df her durch die D\u00f6rner dringen.", "tokens": ["Die", "Ro\u00b7se", "mu\u00df", "her", "durch", "die", "D\u00f6r\u00b7ner", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die M\u00e4rtyr-Kron / w\u00e4chst aus vergossnem Blut /", "tokens": ["Die", "M\u00e4r\u00b7ty\u00b7rKron", "/", "w\u00e4chst", "aus", "ver\u00b7goss\u00b7nem", "Blut", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "aus Plag' und Streit der Christlich Helden-muht.", "tokens": ["aus", "Plag'", "und", "Streit", "der", "Christ\u00b7lich", "Hel\u00b7den\u00b7\u00b7muht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wer hoch will seyn / mu\u00df nach der Hoheit ringen.", "tokens": ["Wer", "hoch", "will", "seyn", "/", "mu\u00df", "nach", "der", "Ho\u00b7heit", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VMFIN", "VAINF", "$(", "VMFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Frucht in Gedult / ist k\u00f6stlich / aber schwer.", "tokens": ["Frucht", "in", "Ge\u00b7dult", "/", "ist", "k\u00f6st\u00b7lich", "/", "a\u00b7ber", "schwer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$(", "VAFIN", "ADJD", "$(", "ADV", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "In Winters Zeit / sch\u00e4tzt man die Fr\u00fccht vielmehr:", "tokens": ["In", "Win\u00b7ters", "Zeit", "/", "sch\u00e4tzt", "man", "die", "Fr\u00fccht", "viel\u00b7mehr", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$(", "VVFIN", "PIS", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "weil seltsam ist / sie damals zu erlangen.", "tokens": ["weil", "selt\u00b7sam", "ist", "/", "sie", "da\u00b7mals", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAFIN", "$(", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wen sau\u00df und brau\u00df / wen knall und strahl nicht irrt /", "tokens": ["Wen", "sau\u00df", "und", "brau\u00df", "/", "wen", "knall", "und", "strahl", "nicht", "irrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "VVFIN", "$(", "PWS", "ADJD", "KON", "ADV", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "zum Himmels-Port der Tugend-Fr\u00fccht' hinf\u00fchrt:", "tokens": ["zum", "Him\u00b7mels\u00b7Port", "der", "Tu\u00b7gen\u00b7dFr\u00fccht'", "hin\u00b7f\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "der wird mit Prei\u00df der Creutzbesiegung prangen.", "tokens": ["der", "wird", "mit", "Prei\u00df", "der", "Creutz\u00b7be\u00b7sie\u00b7gung", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}