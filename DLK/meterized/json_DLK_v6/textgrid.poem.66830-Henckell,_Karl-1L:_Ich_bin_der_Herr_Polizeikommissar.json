{"textgrid.poem.66830": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin der Herr Polizeikommissar", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin der Herr Polizeikommissar", "tokens": ["Ich", "bin", "der", "Herr", "Po\u00b7li\u00b7zei\u00b7kom\u00b7mis\u00b7sar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fcrchtegott Heinerich Unerbittlich,", "tokens": ["F\u00fcrch\u00b7te\u00b7gott", "Hei\u00b7ne\u00b7rich", "Un\u00b7er\u00b7bitt\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJD", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "In jeder Suppe find' ich das Haar,", "tokens": ["In", "je\u00b7der", "Sup\u00b7pe", "find'", "ich", "das", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die der Regierung nicht appetitlich.", "tokens": ["Die", "der", "Re\u00b7gie\u00b7rung", "nicht", "ap\u00b7pe\u00b7tit\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Aufl\u00f6s' ich Versammlungen wunderbar,", "tokens": ["Auf\u00b7l\u00f6s'", "ich", "Ver\u00b7samm\u00b7lun\u00b7gen", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "ADJD", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die nicht gottesf\u00fcrchtig und fromm und sittlich,", "tokens": ["Die", "nicht", "got\u00b7tes\u00b7f\u00fcrch\u00b7tig", "und", "fromm", "und", "sitt\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Meine Gewandtheit ist anerkannt,", "tokens": ["Mei\u00b7ne", "Ge\u00b7wand\u00b7theit", "ist", "an\u00b7er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Ich bin des Pr\u00e4sidiums rechte Hand.", "tokens": ["Ich", "bin", "des", "Pr\u00e4\u00b7si\u00b7di\u00b7ums", "rech\u00b7te", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fchre ein f\u00f6rmliches W\u00f6rterbuch", "tokens": ["Ich", "f\u00fch\u00b7re", "ein", "f\u00f6rm\u00b7li\u00b7ches", "W\u00f6r\u00b7ter\u00b7buch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Grundst\u00fcrzender, staatsgef\u00e4hrlicher Phrasen,", "tokens": ["Grund\u00b7st\u00fcr\u00b7zen\u00b7der", ",", "staats\u00b7ge\u00b7f\u00e4hr\u00b7li\u00b7cher", "Phra\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich wittre am weitsten das rote Tuch", "tokens": ["Ich", "witt\u00b7re", "am", "weits\u00b7ten", "das", "ro\u00b7te", "Tuch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mit einer der sch\u00e4rfsten politischen Nasen.", "tokens": ["Mit", "ei\u00b7ner", "der", "sch\u00e4rfs\u00b7ten", "po\u00b7li\u00b7ti\u00b7schen", "Na\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ein feiner Geruch, ein guter Geruch", "tokens": ["Ein", "fei\u00b7ner", "Ge\u00b7ruch", ",", "ein", "gu\u00b7ter", "Ge\u00b7ruch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Warnt bald mich vor explodierenden Gasen \u2013", "tokens": ["Warnt", "bald", "mich", "vor", "ex\u00b7plo\u00b7die\u00b7ren\u00b7den", "Ga\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Sowie ich was rieche, gleich ruf' ich auch: Stopp!", "tokens": ["So\u00b7wie", "ich", "was", "rie\u00b7che", ",", "gleich", "ruf'", "ich", "auch", ":", "Stopp", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "PIS", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$.", "NN", "$."], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Und st\u00fclpe den blanken Helm auf den Kopp.", "tokens": ["Und", "st\u00fcl\u00b7pe", "den", "blan\u00b7ken", "Helm", "auf", "den", "Kopp", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Es ist f\u00fcrwahr eine d\u00fcstere Zeit,", "tokens": ["Es", "ist", "f\u00fcr\u00b7wahr", "ei\u00b7ne", "d\u00fcs\u00b7te\u00b7re", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Unheimlich st\u00fcrmen die wilden Gedanken,", "tokens": ["Un\u00b7heim\u00b7lich", "st\u00fcr\u00b7men", "die", "wil\u00b7den", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Ich sp\u00fcr' es als W\u00e4chter der Sicherheit,", "tokens": ["Ich", "sp\u00fcr'", "es", "als", "W\u00e4ch\u00b7ter", "der", "Si\u00b7cher\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "NE", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wie die heiligen S\u00e4ulen der Ordnung wanken.", "tokens": ["Wie", "die", "hei\u00b7li\u00b7gen", "S\u00e4u\u00b7len", "der", "Ord\u00b7nung", "wan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Nackt r\u00fcttelt der Simson der Treulosigkeit", "tokens": ["Nackt", "r\u00fct\u00b7telt", "der", "Sim\u00b7son", "der", "Treu\u00b7lo\u00b7sig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "An allen g\u00f6ttlich-menschlichen Schranken.", "tokens": ["An", "al\u00b7len", "g\u00f6tt\u00b7lich\u00b7menschli\u00b7chen", "Schran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich l\u00f6se meine Versammlungen auf,", "tokens": ["Ich", "l\u00f6\u00b7se", "mei\u00b7ne", "Ver\u00b7samm\u00b7lun\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die Dinge doch nehmen ihren Verlauf.", "tokens": ["Die", "Din\u00b7ge", "doch", "neh\u00b7men", "ih\u00b7ren", "Ver\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}