{"dta.poem.9889": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Worzu hat mich der himmel doch ersehn?", "tokens": ["Wor\u00b7zu", "hat", "mich", "der", "him\u00b7mel", "doch", "er\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00df denn mein hertz nur gantz in banden stehen?", "tokens": ["Mu\u00df", "denn", "mein", "hertz", "nur", "gantz", "in", "ban\u00b7den", "ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "ADV", "ADV", "APPR", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach freylich ja/ es ist um mich geschehn!", "tokens": ["Ach", "frey\u00b7lich", "ja", "/", "es", "ist", "um", "mich", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADV", "$(", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich soll hinfort der freyheit m\u00fc\u00dfig gehen.", "tokens": ["Ich", "soll", "hin\u00b7fort", "der", "frey\u00b7heit", "m\u00fc\u00b7\u00dfig", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du hast mich dir/ o liebliche Belinde/", "tokens": ["Du", "hast", "mich", "dir", "/", "o", "lieb\u00b7li\u00b7che", "Be\u00b7lin\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "$(", "FM", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zum sclaven gantz durch einen blick gemacht/", "tokens": ["Zum", "scla\u00b7ven", "gantz", "durch", "ei\u00b7nen", "blick", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So da\u00df ich mich gantz ausser mir befinde.", "tokens": ["So", "da\u00df", "ich", "mich", "gantz", "aus\u00b7ser", "mir", "be\u00b7fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie weit hat mich die lieb", "tokens": ["Wie", "weit", "hat", "mich", "die", "lieb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ART", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Euch bet ich an/ ihr feuerreichen augen/", "tokens": ["Euch", "bet", "ich", "an", "/", "ihr", "feu\u00b7er\u00b7rei\u00b7chen", "au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ihr mich habt in volle glut gesetzt/", "tokens": ["Die", "ihr", "mich", "habt", "in", "vol\u00b7le", "glut", "ge\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus euch mu\u00df man die liebe in sich saugen/", "tokens": ["Aus", "euch", "mu\u00df", "man", "die", "lie\u00b7be", "in", "sich", "sau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PIS", "ART", "VVFIN", "APPR", "PRF", "VVINF", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "So bald man sich an eurem glantz ergetzt;", "tokens": ["So", "bald", "man", "sich", "an", "eu\u00b7rem", "glantz", "er\u00b7getzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es mu\u00df euch selbst der sch\u00f6nste demant weichen/", "tokens": ["Es", "mu\u00df", "euch", "selbst", "der", "sch\u00f6ns\u00b7te", "de\u00b7mant", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ART", "ADJA", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sein blitz wird nie gleich eurem strahle gehn;", "tokens": ["Sein", "blitz", "wird", "nie", "gleich", "eu\u00b7rem", "strah\u00b7le", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Belinde ist dem himmel zu vergleichen/", "tokens": ["Be\u00b7lin\u00b7de", "ist", "dem", "him\u00b7mel", "zu", "ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dieweil an ihr so sch\u00f6ne sterne stehn.", "tokens": ["Die\u00b7weil", "an", "ihr", "so", "sch\u00f6\u00b7ne", "ster\u00b7ne", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Erz\u00fcrne nicht/ du sonne meiner seelen/", "tokens": ["Er\u00b7z\u00fcr\u00b7ne", "nicht", "/", "du", "son\u00b7ne", "mei\u00b7ner", "see\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$(", "PPER", "VVFIN", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sich so weit mein mattes hertze wagt/", "tokens": ["Da\u00df", "sich", "so", "weit", "mein", "mat\u00b7tes", "hert\u00b7ze", "wagt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Indem es dir mit zittern und mit qu\u00e4len", "tokens": ["In\u00b7dem", "es", "dir", "mit", "zit\u00b7tern", "und", "mit", "qu\u00e4\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPR", "VVINF", "KON", "APPR", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem\u00fcthigst ietzt sein bittres leiben klagt.", "tokens": ["De\u00b7m\u00fct\u00b7higst", "ietzt", "sein", "bitt\u00b7res", "lei\u00b7ben", "klagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die anmuth/ so auf deinen wangen spielet/", "tokens": ["Die", "an\u00b7muth", "/", "so", "auf", "dei\u00b7nen", "wan\u00b7gen", "spie\u00b7let", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hat selbiges verfesselt und verstrickt/", "tokens": ["Hat", "sel\u00b7bi\u00b7ges", "ver\u00b7fes\u00b7selt", "und", "ver\u00b7strickt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "KON", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und weil es nichts als lauter feuer f\u00fchlet/", "tokens": ["Und", "weil", "es", "nichts", "als", "lau\u00b7ter", "feu\u00b7er", "f\u00fch\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "KOKOM", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So will es auch durch feuer seyn erquickt.", "tokens": ["So", "will", "es", "auch", "durch", "feu\u00b7er", "seyn", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hier liege ich zu deinen zarten f\u00fcssen/", "tokens": ["Hier", "lie\u00b7ge", "ich", "zu", "dei\u00b7nen", "zar\u00b7ten", "f\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nim sch\u00f6nste mich zu deinem diener an;", "tokens": ["Nim", "sch\u00f6ns\u00b7te", "mich", "zu", "dei\u00b7nem", "die\u00b7ner", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich suche nichts als deine hand zu k\u00fcssen/", "tokens": ["Ich", "su\u00b7che", "nichts", "als", "dei\u00b7ne", "hand", "zu", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "KOKOM", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die stets so sehr die hertzen fesseln kan:", "tokens": ["Die", "stets", "so", "sehr", "die", "hert\u00b7zen", "fes\u00b7seln", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das meinige sey dir hiemit ergeben/", "tokens": ["Das", "mei\u00b7ni\u00b7ge", "sey", "dir", "hie\u00b7mit", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSS", "VAFIN", "PPER", "PAV", "VVPP", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Verschm\u00e4h es nicht/ es r\u00fchris ein keuscher trieb;", "tokens": ["Ver\u00b7schm\u00e4h", "es", "nicht", "/", "es", "r\u00fch\u00b7ris", "ein", "keu\u00b7scher", "trieb", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$(", "PPER", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Es w\u00fcnscht bey dir in diensten stets zu leben/", "tokens": ["Es", "w\u00fcnscht", "bey", "dir", "in", "diens\u00b7ten", "stets", "zu", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "PDAT", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Denn du bist mir mehr als mein leben lieb.", "tokens": ["Denn", "du", "bist", "mir", "mehr", "als", "mein", "le\u00b7ben", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "PIAT", "KOKOM", "PPOSAT", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}