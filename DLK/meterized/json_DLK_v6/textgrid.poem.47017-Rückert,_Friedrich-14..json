{"textgrid.poem.47017": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bald, wenn dein Blick mir Mut ins Herz gegossen,", "tokens": ["Bald", ",", "wenn", "dein", "Blick", "mir", "Mut", "ins", "Herz", "ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "PPER", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ergie\u00dft sich meine Zung' in lust'gen Wogen;", "tokens": ["Er\u00b7gie\u00dft", "sich", "mei\u00b7ne", "Zung'", "in", "lust'\u00b7gen", "Wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bald, wenn dein Wort mir drauf den Mut entzogen,", "tokens": ["Bald", ",", "wenn", "dein", "Wort", "mir", "drauf", "den", "Mut", "ent\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "PPER", "PAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schlie\u00dft sich das Herz, die Rede flie\u00dft verdrossen.", "tokens": ["Schlie\u00dft", "sich", "das", "Herz", ",", "die", "Re\u00b7de", "flie\u00dft", "ver\u00b7dros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bald spornt dein Zorn mich, da\u00df gleich st\u00f6rr'gen Rossen", "tokens": ["Bald", "spornt", "dein", "Zorn", "mich", ",", "da\u00df", "gleich", "st\u00f6rr'\u00b7gen", "Ros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "$,", "KOUS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Witz sich b\u00e4umt in keckem Sprung und Bogen;", "tokens": ["Der", "Witz", "sich", "b\u00e4umt", "in", "ke\u00b7ckem", "Sprung", "und", "Bo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bald, wenn du wieder scheinen willst gewogen,", "tokens": ["Bald", ",", "wenn", "du", "wie\u00b7der", "schei\u00b7nen", "willst", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schweig' ich verstockt, dir und mir selbst zum Possen.", "tokens": ["Schweig'", "ich", "ver\u00b7stockt", ",", "dir", "und", "mir", "selbst", "zum", "Pos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$,", "PPER", "KON", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wohl klagst du: o der Art, nicht zu entschuld'gen!", "tokens": ["Wohl", "klagst", "du", ":", "o", "der", "Art", ",", "nicht", "zu", "ent\u00b7schuld'\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "FM", "ART", "NN", "$,", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer fort und fort so sch\u00f6n gleich unbest\u00e4nd'gem", "tokens": ["Wer", "fort", "und", "fort", "so", "sch\u00f6n", "gleich", "un\u00b7be\u00b7st\u00e4n\u00b7d'\u00b7gem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKVZ", "KON", "PTKVZ", "ADV", "ADJD", "ADV", "ADJA"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.3": {"text": "April sich ziert, was ist mit dem zu machen?", "tokens": ["Ap\u00b7ril", "sich", "ziert", ",", "was", "ist", "mit", "dem", "zu", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVFIN", "$,", "PWS", "VAFIN", "APPR", "ART", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch klag' auch ich: dich selbst mu\u00dft du beschuld'gen,", "tokens": ["Doch", "klag'", "auch", "ich", ":", "dich", "selbst", "mu\u00dft", "du", "be\u00b7schuld'\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "$.", "PPER", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn ich April bin, da du zu best\u00e4nd'gem", "tokens": ["Wenn", "ich", "Ap\u00b7ril", "bin", ",", "da", "du", "zu", "be\u00b7st\u00e4n\u00b7d'\u00b7gem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "$,", "KOUS", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.3": {"text": "Mai mich, wenn du mich liebtest, k\u00f6nntest machen.", "tokens": ["Mai", "mich", ",", "wenn", "du", "mich", "lieb\u00b7test", ",", "k\u00f6nn\u00b7test", "ma\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,", "VMFIN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}