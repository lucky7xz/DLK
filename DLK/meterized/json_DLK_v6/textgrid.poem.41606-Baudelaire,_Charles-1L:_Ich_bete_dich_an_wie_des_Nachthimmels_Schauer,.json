{"textgrid.poem.41606": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bete dich an wie des Nachthimmels Schauer,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bete dich an wie des Nachthimmels Schauer,", "tokens": ["Ich", "be\u00b7te", "dich", "an", "wie", "des", "Nacht\u00b7him\u00b7mels", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KOKOM", "ART", "NN", "NE", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "O gro\u00dfe Stumme, o Urne der Trauer!", "tokens": ["O", "gro\u00b7\u00dfe", "Stum\u00b7me", ",", "o", "Ur\u00b7ne", "der", "Trau\u00b7er", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und lieb nur hei\u00dfer dich, weil, Sch\u00f6ne, du mich fliehst,", "tokens": ["Und", "lieb", "nur", "hei\u00b7\u00dfer", "dich", ",", "weil", ",", "Sch\u00f6\u00b7ne", ",", "du", "mich", "fliehst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ADJA", "PPER", "$,", "KOUS", "$,", "NN", "$,", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und weil, Stern meiner Nacht, voll Hohn du niedersiehst", "tokens": ["Und", "weil", ",", "Stern", "mei\u00b7ner", "Nacht", ",", "voll", "Hohn", "du", "nie\u00b7der\u00b7siehst"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "$,", "NN", "PPOSAT", "NN", "$,", "ADJD", "NN", "PPER", "VVFIN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und sp\u00f6ttisch l\u00e4chelnd scheinst die gro\u00dfe Kluft zu weiten,", "tokens": ["Und", "sp\u00f6t\u00b7tisch", "l\u00e4\u00b7chelnd", "scheinst", "die", "gro\u00b7\u00dfe", "Kluft", "zu", "wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die mich getrennt h\u00e4lt von den blauen Ewigkeiten.", "tokens": ["Die", "mich", "ge\u00b7trennt", "h\u00e4lt", "von", "den", "blau\u00b7en", "E\u00b7wig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich st\u00fcrme zum Angriff, ich klettre hinauf,", "tokens": ["Ich", "st\u00fcr\u00b7me", "zum", "An\u00b7griff", ",", "ich", "klett\u00b7re", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie zu Leichen sich hindr\u00e4ngt der W\u00fcrmer Hauf,", "tokens": ["Wie", "zu", "Lei\u00b7chen", "sich", "hin\u00b7dr\u00e4ngt", "der", "W\u00fcr\u00b7mer", "Hauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PRF", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Und lieb dich, grausam Tier, ob auch dein Stolz mich h\u00f6hne,", "tokens": ["Und", "lieb", "dich", ",", "grau\u00b7sam", "Tier", ",", "ob", "auch", "dein", "Stolz", "mich", "h\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "$,", "ADJD", "NN", "$,", "KOUS", "ADV", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im kalten Glanz, durch den nur gr\u00f6\u00dfer deine Sch\u00f6ne.", "tokens": ["Im", "kal\u00b7ten", "Glanz", ",", "durch", "den", "nur", "gr\u00f6\u00b7\u00dfer", "dei\u00b7ne", "Sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ART", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}