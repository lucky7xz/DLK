{"textgrid.poem.59388": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Morgengebet", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wunderbares, tiefes Schweigen,", "tokens": ["O", "wun\u00b7der\u00b7ba\u00b7res", ",", "tie\u00b7fes", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie einsam ist's noch auf der Welt!", "tokens": ["Wie", "ein\u00b7sam", "ist's", "noch", "auf", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die W\u00e4lder nur sich leise neigen,", "tokens": ["Die", "W\u00e4l\u00b7der", "nur", "sich", "lei\u00b7se", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als ging' der Herr durchs stille Feld.", "tokens": ["Als", "ging'", "der", "Herr", "durchs", "stil\u00b7le", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fchl mich recht wie neu geschaffen,", "tokens": ["Ich", "f\u00fchl", "mich", "recht", "wie", "neu", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KOKOM", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ist die Sorge nun und Not?", "tokens": ["Wo", "ist", "die", "Sor\u00b7ge", "nun", "und", "Not", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was mich noch gestern wollt erschlaffen,", "tokens": ["Was", "mich", "noch", "ge\u00b7stern", "wollt", "er\u00b7schlaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVINF", "VMFIN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ich sch\u00e4m mich des im Morgenrot.", "tokens": ["Ich", "sch\u00e4m", "mich", "des", "im", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Welt mit ihrem Gram und Gl\u00fccke", "tokens": ["Die", "Welt", "mit", "ih\u00b7rem", "Gram", "und", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Will ich, ein Pilger, frohbereit", "tokens": ["Will", "ich", ",", "ein", "Pil\u00b7ger", ",", "froh\u00b7be\u00b7reit"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "$,", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Betreten nur wie eine Br\u00fccke", "tokens": ["Be\u00b7tre\u00b7ten", "nur", "wie", "ei\u00b7ne", "Br\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu dir, Herr, \u00fcbern Strom der Zeit.", "tokens": ["Zu", "dir", ",", "Herr", ",", "\u00fc\u00b7bern", "Strom", "der", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NN", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und buhlt mein Lied, auf Weltgunst lauernd,", "tokens": ["Und", "buhlt", "mein", "Lied", ",", "auf", "Welt\u00b7gunst", "lau\u00b7ernd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um schn\u00f6den Sold der Eitelkeit:", "tokens": ["Um", "schn\u00f6\u00b7den", "Sold", "der", "Ei\u00b7tel\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zerschlag mein Saitenspiel, und schauernd", "tokens": ["Zer\u00b7schlag", "mein", "Sai\u00b7ten\u00b7spiel", ",", "und", "schau\u00b7ernd"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schweig ich vor dir in Ewigkeit.", "tokens": ["Schweig", "ich", "vor", "dir", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}