{"dta.poem.23706": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das Thierchen ohne Nahmen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wolt es schnell erhaschen,", "tokens": ["Ich", "wolt", "es", "schnell", "er\u00b7ha\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und dann wolt ich es fragen;", "tokens": ["Und", "dann", "wolt", "ich", "es", "fra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie wohnt es sich bei Sch\u00f6nen?", "tokens": ["Wie", "wohnt", "es", "sich", "bei", "Sch\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Allein es sprang vom Halse,", "tokens": ["Al\u00b7lein", "es", "sprang", "vom", "Hal\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und h\u00fcpfte nach den H\u00fcgeln,", "tokens": ["Und", "h\u00fcpf\u00b7te", "nach", "den", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die an dem Halse grenzen.", "tokens": ["Die", "an", "dem", "Hal\u00b7se", "gren\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich sah es wieder sitzen.", "tokens": ["Ich", "sah", "es", "wie\u00b7der", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es sah sich um, und lachte,", "tokens": ["Es", "sah", "sich", "um", ",", "und", "lach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und, triegen nicht die Minen,", "tokens": ["Und", ",", "trie\u00b7gen", "nicht", "die", "Mi\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "So schiens, als wolt es sprechen:", "tokens": ["So", "schiens", ",", "als", "wolt", "es", "spre\u00b7chen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Nun solt du mich nicht kriegen,", "tokens": ["Nun", "solt", "du", "mich", "nicht", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Izt will ich weiter h\u00fcpfen.", "tokens": ["Izt", "will", "ich", "wei\u00b7ter", "h\u00fcp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Du darfst mich nicht verfolgen,", "tokens": ["Du", "darfst", "mich", "nicht", "ver\u00b7fol\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Wohin ich itzo h\u00fcpfe.", "tokens": ["Wo\u00b7hin", "ich", "it\u00b7zo", "h\u00fcp\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Du h\u00f6rtest nicht, Filinde,", "tokens": ["Du", "h\u00f6r\u00b7test", "nicht", ",", "Fi\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Als ich zum Thierchen sagte:", "tokens": ["Als", "ich", "zum", "Thier\u00b7chen", "sag\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Adieu du kleiner Springer,", "tokens": ["A\u00b7die\u00b7u", "du", "klei\u00b7ner", "Sprin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "D\u00fcrft ich dich nur verfolgen,", "tokens": ["D\u00fcrft", "ich", "dich", "nur", "ver\u00b7fol\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Wie bald wolt ich dich kriegen!", "tokens": ["Wie", "bald", "wolt", "ich", "dich", "krie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}