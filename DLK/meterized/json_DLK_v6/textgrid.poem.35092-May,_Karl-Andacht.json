{"textgrid.poem.35092": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Andacht", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei mir gegr\u00fc\u00dft in stiller Stunde,", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", "in", "stil\u00b7ler", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du liebes, frohes H\u00e4ndefalten!", "tokens": ["Du", "lie\u00b7bes", ",", "fro\u00b7hes", "H\u00e4n\u00b7de\u00b7fal\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du tr\u00e4gst zum Himmel auf die Kunde,", "tokens": ["Du", "tr\u00e4gst", "zum", "Him\u00b7mel", "auf", "die", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich vertraue seinem Walten.", "tokens": ["Da\u00df", "ich", "ver\u00b7trau\u00b7e", "sei\u00b7nem", "Wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des Tages Last ist mir genommen,", "tokens": ["Des", "Ta\u00b7ges", "Last", "ist", "mir", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und meine Seele ruht im Herrn;", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "ruht", "im", "Herrn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich darf mit Dank und Bitten kommen,", "tokens": ["Ich", "darf", "mit", "Dank", "und", "Bit\u00b7ten", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und ich, ich komme ja so gern.", "tokens": ["Und", "ich", ",", "ich", "kom\u00b7me", "ja", "so", "gern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sei mir gegr\u00fc\u00dft in stiller Stunde,", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", "in", "stil\u00b7ler", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du liebes, frohes H\u00e4ndefalten!", "tokens": ["Du", "lie\u00b7bes", ",", "fro\u00b7hes", "H\u00e4n\u00b7de\u00b7fal\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du bringst vom Himmel mir die Kunde,", "tokens": ["Du", "bringst", "vom", "Him\u00b7mel", "mir", "die", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df mich des Vaters Hand wird halten.", "tokens": ["Da\u00df", "mich", "des", "Va\u00b7ters", "Hand", "wird", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des Tages Stimmen sind verklungen,", "tokens": ["Des", "Ta\u00b7ges", "Stim\u00b7men", "sind", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und meine Seele ruht im Herrn;", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "ruht", "im", "Herrn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es t\u00f6nen in mir andre Zungen,", "tokens": ["Es", "t\u00f6\u00b7nen", "in", "mir", "and\u00b7re", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und ich, ich h\u00f6re sie so gern.", "tokens": ["Und", "ich", ",", "ich", "h\u00f6\u00b7re", "sie", "so", "gern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sei mir gegr\u00fc\u00dft in stiller Stunde,", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", "in", "stil\u00b7ler", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du liebes, frohes H\u00e4ndefalten!", "tokens": ["Du", "lie\u00b7bes", ",", "fro\u00b7hes", "H\u00e4n\u00b7de\u00b7fal\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Steig auf und nieder, Himmelskunde,", "tokens": ["Steig", "auf", "und", "nie\u00b7der", ",", "Him\u00b7mels\u00b7kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "PTKVZ", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich f\u00fcr das Jenseits zu gestalten.", "tokens": ["Mich", "f\u00fcr", "das", "Jen\u00b7seits", "zu", "ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Will einst der letzte Tag verschwinden,", "tokens": ["Will", "einst", "der", "letz\u00b7te", "Tag", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ruht meine Seele in dem Herrn", "tokens": ["Ruht", "mei\u00b7ne", "See\u00b7le", "in", "dem", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und wird die Heimath wiederfinden,", "tokens": ["Und", "wird", "die", "Hei\u00b7math", "wie\u00b7der\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nach der sie sucht so gern, so gern.", "tokens": ["Nach", "der", "sie", "sucht", "so", "gern", ",", "so", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "ADV", "ADV", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}