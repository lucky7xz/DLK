{"textgrid.poem.28036": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Lazarus verstirbt, wird oben er getr\u00f6stet;", "tokens": ["Als", "La\u00b7za\u00b7rus", "ver\u00b7stirbt", ",", "wird", "o\u00b7ben", "er", "ge\u00b7tr\u00f6s\u00b7tet", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,", "VAFIN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sobald der Reiche stirbt, wird unten er ger\u00f6stet;", "tokens": ["So\u00b7bald", "der", "Rei\u00b7che", "stirbt", ",", "wird", "un\u00b7ten", "er", "ge\u00b7r\u00f6s\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "VVFIN", "$,", "VAFIN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer \u00fcbel stirbt, f\u00fchlt Durst auffs Teuffels heissem Rost,", "tokens": ["Wer", "\u00fc\u00b7bel", "stirbt", ",", "f\u00fchlt", "Durst", "auffs", "Teuf\u00b7fels", "heis\u00b7sem", "Rost", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "VVFIN", "ADV", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer selig aber stirbt, in Abrahams Scho\u00df Trost.", "tokens": ["Wer", "se\u00b7lig", "a\u00b7ber", "stirbt", ",", "in", "Ab\u00b7ra\u00b7hams", "Scho\u00df", "Trost", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "VVFIN", "$,", "APPR"