{"textgrid.poem.40432": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Numero acht im Holl\u00e4nder Hof zu Heidelberg", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Schatten seh' ich schweben", "tokens": ["Zwei", "Schat\u00b7ten", "seh'", "ich", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In sp\u00e4ter, sp\u00e4ter Nacht;", "tokens": ["In", "sp\u00e4\u00b7ter", ",", "sp\u00e4\u00b7ter", "Nacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wi\u00dft ihr, wohin sie streben? \u2013", "tokens": ["Wi\u00dft", "ihr", ",", "wo\u00b7hin", "sie", "stre\u00b7ben", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u2013 Beide auf Numero acht!", "tokens": ["\u2013", "Bei\u00b7de", "auf", "Nu\u00b7me\u00b7ro", "acht", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "APPR", "NE", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Hausknecht, als es l\u00e4utet,", "tokens": ["Der", "Haus\u00b7knecht", ",", "als", "es", "l\u00e4u\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit einem Fluch erwacht;", "tokens": ["Mit", "ei\u00b7nem", "Fluch", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er wei\u00df schon, was es bedeutet:", "tokens": ["Er", "wei\u00df", "schon", ",", "was", "es", "be\u00b7deu\u00b7tet", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Beide auf Numero acht!", "tokens": ["Bei\u00b7de", "auf", "Nu\u00b7me\u00b7ro", "acht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NE", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbalt Holland steht in N\u00f6ten,", "tokens": ["\u00bb", "alt", "Hol\u00b7land", "steht", "in", "N\u00f6\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NE", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weh' uns, die wilde Jagd!", "tokens": ["Weh'", "uns", ",", "die", "wil\u00b7de", "Jagd", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weh' uns, die alten Schweden", "tokens": ["Weh'", "uns", ",", "die", "al\u00b7ten", "Schwe\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ART", "ADJA", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beide auf Numero acht!", "tokens": ["Bei\u00b7de", "auf", "Nu\u00b7me\u00b7ro", "acht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NE", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hei\u00dft das als flei\u00dfiger Schreiber", "tokens": ["Hei\u00dft", "das", "als", "flei\u00b7\u00dfi\u00b7ger", "Schrei\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDS", "KOUS", "ADJA", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Ein neues Buch gemacht,", "tokens": ["Ein", "neu\u00b7es", "Buch", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr grausamen \u00dcberkneiper", "tokens": ["Ihr", "grau\u00b7sa\u00b7men", "\u00dc\u00b7berk\u00b7nei\u00b7per"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Beide auf Numero acht?", "tokens": ["Bei\u00b7de", "auf", "Nu\u00b7me\u00b7ro", "acht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NE", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hei\u00dft das als frommer Pastor", "tokens": ["Hei\u00dft", "das", "als", "from\u00b7mer", "Pas\u00b7tor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDS", "KOUS", "ADJA", "NN"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.2": {"text": "An die Gemeinde gedacht?", "tokens": ["An", "die", "Ge\u00b7mein\u00b7de", "ge\u00b7dacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ihr s\u00fcndenharte Laster", "tokens": ["Ihr", "s\u00fcn\u00b7den\u00b7har\u00b7te", "Las\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beide auf Numero acht?!\u00ab", "tokens": ["Bei\u00b7de", "auf", "Nu\u00b7me\u00b7ro", "acht", "?!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "APPR", "NE", "CARD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Hausknecht, ungewaschen", "tokens": ["Der", "Haus\u00b7knecht", ",", "un\u00b7ge\u00b7wa\u00b7schen"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Murrt er's und ungeschlacht,", "tokens": ["Murrt", "er's", "und", "un\u00b7ge\u00b7schlacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da l\u00e4rmt's: \u00bbHe! noch zwei Flaschen,", "tokens": ["Da", "l\u00e4rmt's", ":", "\u00bb", "He", "!", "noch", "zwei", "Fla\u00b7schen", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "$(", "ITJ", "$.", "ADV", "CARD", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Beide auf Numero acht!\u00ab", "tokens": ["Bei\u00b7de", "auf", "Nu\u00b7me\u00b7ro", "acht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "APPR", "NE", "CARD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und weiter singt es und klingt es", "tokens": ["Und", "wei\u00b7ter", "singt", "es", "und", "klingt", "es"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und jubiliert und lacht,", "tokens": ["Und", "ju\u00b7bi\u00b7liert", "und", "lacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und bis zum Hausherrn dringt es:", "tokens": ["Und", "bis", "zum", "Haus\u00b7herrn", "dringt", "es", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbbeide auf Numero acht!\u00ab", "tokens": ["\u00bb", "bei\u00b7de", "auf", "Nu\u00b7me\u00b7ro", "acht", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIS", "APPR", "NE", "CARD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Der spitzt betr\u00fcbt die F\u00fc\u00dfe,", "tokens": ["Der", "spitzt", "be\u00b7tr\u00fcbt", "die", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Bettstatt seufzt und kracht;", "tokens": ["Die", "Bett\u00b7statt", "seufzt", "und", "kracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Stumm nimmt er eine Prise:", "tokens": ["Stumm", "nimmt", "er", "ei\u00b7ne", "Pri\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbbeide auf Numero acht!!\u00ab", "tokens": ["\u00bb", "bei\u00b7de", "auf", "Nu\u00b7me\u00b7ro", "acht", "!!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIS", "APPR", "NE", "CARD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}