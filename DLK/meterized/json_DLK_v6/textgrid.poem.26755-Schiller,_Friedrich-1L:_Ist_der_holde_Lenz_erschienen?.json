 "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blatt und Blume sich entf\u00e4rben,", "tokens": ["Blatt", "und", "Blu\u00b7me", "sich", "ent\u00b7f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Traurig steht der nackte Strauch,", "tokens": ["Trau\u00b7rig", "steht", "der", "nack\u00b7te", "Strauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nehm ich mir das h\u00f6chste Leben", "tokens": ["Nehm", "ich", "mir", "das", "h\u00f6chs\u00b7te", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus Vertumnus' reichem Horn,", "tokens": ["Aus", "Ver\u00b7tum\u00b7nus'", "rei\u00b7chem", "Horn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Opfernd es dem Styx zu geben,", "tokens": ["Op\u00b7fernd", "es", "dem", "Styx", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir des Samens goldnes Korn.", "tokens": ["Mir", "des", "Sa\u00b7mens", "gold\u00b7nes", "Korn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Traurend senk ichs in die Erde,", "tokens": ["Trau\u00b7rend", "senk", "ichs", "in", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "wo