{"textgrid.poem.26755": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ist der holde Lenz erschienen?", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist der holde Lenz erschienen?", "tokens": ["Ist", "der", "hol\u00b7de", "Lenz", "er\u00b7schie\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat die Erde sich verj\u00fcngt?", "tokens": ["Hat", "die", "Er\u00b7de", "sich", "ver\u00b7j\u00fcngt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die besonnten H\u00fcgel gr\u00fcnen,", "tokens": ["Die", "be\u00b7sonn\u00b7ten", "H\u00fc\u00b7gel", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des Eises Rinde springt.", "tokens": ["Und", "des", "Ei\u00b7ses", "Rin\u00b7de", "springt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus der Str\u00f6me blauem Spiegel", "tokens": ["Aus", "der", "Str\u00f6\u00b7me", "blau\u00b7em", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Lacht der unbew\u00f6lkte Zeus,", "tokens": ["Lacht", "der", "un\u00b7be\u00b7w\u00f6lk\u00b7te", "Zeus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Milder wehen Zephirs Fl\u00fcgel,", "tokens": ["Mil\u00b7der", "we\u00b7hen", "Ze\u00b7phirs", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Augen treibt das junge Reis.", "tokens": ["Au\u00b7gen", "treibt", "das", "jun\u00b7ge", "Reis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "In dem Hain erwachen Lieder,", "tokens": ["In", "dem", "Hain", "er\u00b7wa\u00b7chen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und die Oreade spricht:", "tokens": ["Und", "die", "O\u00b7rea\u00b7de", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Deine Blumen kehren wieder,", "tokens": ["Dei\u00b7ne", "Blu\u00b7men", "keh\u00b7ren", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Deine Tochter kehret nicht.", "tokens": ["Dei\u00b7ne", "Toch\u00b7ter", "keh\u00b7ret", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach! wie lang ists, da\u00df ich walle", "tokens": ["Ach", "!", "wie", "lang", "ists", ",", "da\u00df", "ich", "wal\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Suchend durch der Erde Flur,", "tokens": ["Su\u00b7chend", "durch", "der", "Er\u00b7de", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Titan, deine Strahlen alle", "tokens": ["Ti\u00b7tan", ",", "dei\u00b7ne", "Strah\u00b7len", "al\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sandt ich nach der teuren Spur;", "tokens": ["Sandt", "ich", "nach", "der", "teu\u00b7ren", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Keiner hat mir noch verk\u00fcndet", "tokens": ["Kei\u00b7ner", "hat", "mir", "noch", "ver\u00b7k\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von dem lieben Angesicht,", "tokens": ["Von", "dem", "lie\u00b7ben", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Tag, der alles findet,", "tokens": ["Und", "der", "Tag", ",", "der", "al\u00b7les", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die Verlorne fand er nicht.", "tokens": ["Die", "Ver\u00b7lor\u00b7ne", "fand", "er", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Hast du, Zeus! sie mir entrissen,", "tokens": ["Hast", "du", ",", "Zeus", "!", "sie", "mir", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NE", "$.", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Hat, von ihrem Reiz ger\u00fchrt,", "tokens": ["Hat", ",", "von", "ih\u00b7rem", "Reiz", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Zu des Orkus schwarzen Fl\u00fcssen", "tokens": ["Zu", "des", "Or\u00b7kus", "schwar\u00b7zen", "Fl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Pluto sie hinabgef\u00fchrt?", "tokens": ["Plu\u00b7to", "sie", "hin\u00b7ab\u00b7ge\u00b7f\u00fchrt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer wird nach dem d\u00fcstern Strande", "tokens": ["Wer", "wird", "nach", "dem", "d\u00fcs\u00b7tern", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Meines Grames Bote sein?", "tokens": ["Mei\u00b7nes", "Gra\u00b7mes", "Bo\u00b7te", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig st\u00f6\u00dft der Kahn vom Lande,", "tokens": ["E\u00b7wig", "st\u00f6\u00dft", "der", "Kahn", "vom", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch nur Schatten nimmt er ein.", "tokens": ["Doch", "nur", "Schat\u00b7ten", "nimmt", "er", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jedem selgen Aug verschlossen", "tokens": ["Je\u00b7dem", "sel\u00b7gen", "Aug", "ver\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bleibt das n\u00e4chtliche Gefild,", "tokens": ["Bleibt", "das", "n\u00e4cht\u00b7li\u00b7che", "Ge\u00b7fild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und solang der Styx geflossen,", "tokens": ["Und", "so\u00b7lang", "der", "Styx", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Trug er kein lebendig Bild.", "tokens": ["Trug", "er", "kein", "le\u00b7ben\u00b7dig", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Nieder f\u00fchren tausend Steige,", "tokens": ["Nie\u00b7der", "f\u00fch\u00b7ren", "tau\u00b7send", "Stei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Keiner f\u00fchrt zum Tag zur\u00fcck,", "tokens": ["Kei\u00b7ner", "f\u00fchrt", "zum", "Tag", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ihre Tr\u00e4nen bringt kein Zeuge", "tokens": ["Ih\u00b7re", "Tr\u00e4\u00b7nen", "bringt", "kein", "Zeu\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Vor der bangen Mutter Blick.", "tokens": ["Vor", "der", "ban\u00b7gen", "Mut\u00b7ter", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "M\u00fctter, die aus Pyrrhas Stamme", "tokens": ["M\u00fct\u00b7ter", ",", "die", "aus", "Pyrr\u00b7has", "Stam\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sterbliche geboren sind,", "tokens": ["Sterb\u00b7li\u00b7che", "ge\u00b7bo\u00b7ren", "sind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "D\u00fcrfen durch des Grabes Flamme", "tokens": ["D\u00fcr\u00b7fen", "durch", "des", "Gra\u00b7bes", "Flam\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Folgen dem geliebten Kind,", "tokens": ["Fol\u00b7gen", "dem", "ge\u00b7lieb\u00b7ten", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur was Jovis Haus bewohnet,", "tokens": ["Nur", "was", "Jo\u00b7vis", "Haus", "be\u00b7woh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nahet nicht dem dunkeln Strand,", "tokens": ["Na\u00b7het", "nicht", "dem", "dun\u00b7keln", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur die Seligen verschonet,", "tokens": ["Nur", "die", "Se\u00b7li\u00b7gen", "ver\u00b7scho\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Parzen, eure strenge Hand.", "tokens": ["Par\u00b7zen", ",", "eu\u00b7re", "stren\u00b7ge", "Hand", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "St\u00fcrzt mich in die Nacht der N\u00e4chte", "tokens": ["St\u00fcrzt", "mich", "in", "die", "Nacht", "der", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Aus des Himmels goldnem Saal,", "tokens": ["Aus", "des", "Him\u00b7mels", "gold\u00b7nem", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ehret nicht der G\u00f6ttin Rechte,", "tokens": ["Eh\u00b7ret", "nicht", "der", "G\u00f6t\u00b7tin", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Ach! sie sind der Mutter Qual!", "tokens": ["Ach", "!", "sie", "sind", "der", "Mut\u00b7ter", "Qual", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo sie mit dem finstern Gatten", "tokens": ["Wo", "sie", "mit", "dem", "fins\u00b7tern", "Gat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freudlos thronet, stieg' ich hin,", "tokens": ["Freud\u00b7los", "thro\u00b7net", ",", "stieg'", "ich", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00e4te mit den leisen Schatten", "tokens": ["Tr\u00e4\u00b7te", "mit", "den", "lei\u00b7sen", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leise vor die Herrscherin.", "tokens": ["Lei\u00b7se", "vor", "die", "Herr\u00b7sche\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, ihr Auge, feucht von Z\u00e4hren,", "tokens": ["Ach", ",", "ihr", "Au\u00b7ge", ",", "feucht", "von", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sucht umsonst das goldne Licht,", "tokens": ["Sucht", "um\u00b7sonst", "das", "gold\u00b7ne", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Irret nach entfernten Sph\u00e4ren,", "tokens": ["Ir\u00b7ret", "nach", "ent\u00b7fern\u00b7ten", "Sph\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf die Mutter f\u00e4llt es nicht,", "tokens": ["Auf", "die", "Mut\u00b7ter", "f\u00e4llt", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Bis die Freude sie entdecket,", "tokens": ["Bis", "die", "Freu\u00b7de", "sie", "ent\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Bis sich Brust mit Brust vereint,", "tokens": ["Bis", "sich", "Brust", "mit", "Brust", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und zum Mitgef\u00fchl erwecket,", "tokens": ["Und", "zum", "Mit\u00b7ge\u00b7f\u00fchl", "er\u00b7we\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Selbst der rauhe Orkus weint.", "tokens": ["Selbst", "der", "rau\u00b7he", "Or\u00b7kus", "weint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Eitler Wunsch! Verlorne Klagen!", "tokens": ["Eit\u00b7ler", "Wunsch", "!", "Ver\u00b7lor\u00b7ne", "Kla\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhig in dem gleichen Gleis", "tokens": ["Ru\u00b7hig", "in", "dem", "glei\u00b7chen", "Gleis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rollt des Tages sichrer Wagen,", "tokens": ["Rollt", "des", "Ta\u00b7ges", "sich\u00b7rer", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig steht der Schlu\u00df des Zeus.", "tokens": ["E\u00b7wig", "steht", "der", "Schlu\u00df", "des", "Zeus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weg von jenen Finsternissen", "tokens": ["Weg", "von", "je\u00b7nen", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wandt er sein begl\u00fccktes Haupt,", "tokens": ["Wandt", "er", "sein", "be\u00b7gl\u00fcck\u00b7tes", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Einmal in die Nacht gerissen,", "tokens": ["Ein\u00b7mal", "in", "die", "Nacht", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bleibt sie ewig mir geraubt,", "tokens": ["Bleibt", "sie", "e\u00b7wig", "mir", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Bis des dunkeln Stromes Welle", "tokens": ["Bis", "des", "dun\u00b7keln", "Stro\u00b7mes", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Von Aurorens Farben gl\u00fcht,", "tokens": ["Von", "Au\u00b7ro\u00b7rens", "Far\u00b7ben", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Iris mitten durch die H\u00f6lle", "tokens": ["I\u00b7ris", "mit\u00b7ten", "durch", "die", "H\u00f6l\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Ihren sch\u00f6nen Bogen zieht.", "tokens": ["Ih\u00b7ren", "sch\u00f6\u00b7nen", "Bo\u00b7gen", "zieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ist mir nichts von ihr geblieben,", "tokens": ["Ist", "mir", "nichts", "von", "ihr", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "APPR", "PPOSAT", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht ein s\u00fc\u00df erinnernd Pfand,", "tokens": ["Nicht", "ein", "s\u00fc\u00df", "e\u00b7rin\u00b7nernd", "Pfand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJD", "VVPP", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die Fernen sich noch lieben,", "tokens": ["Da\u00df", "die", "Fer\u00b7nen", "sich", "noch", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine Spur der teuren Hand?", "tokens": ["Kei\u00b7ne", "Spur", "der", "teu\u00b7ren", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kn\u00fcpfet sich kein Liebesknoten", "tokens": ["Kn\u00fcp\u00b7fet", "sich", "kein", "Lie\u00b7bes\u00b7kno\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PIAT", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Zwischen Kind und Mutter an?", "tokens": ["Zwi\u00b7schen", "Kind", "und", "Mut\u00b7ter", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zwischen Lebenden und Toten", "tokens": ["Zwi\u00b7schen", "Le\u00b7ben\u00b7den", "und", "To\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist kein B\u00fcndnis aufgetan?", "tokens": ["Ist", "kein", "B\u00fcnd\u00b7n