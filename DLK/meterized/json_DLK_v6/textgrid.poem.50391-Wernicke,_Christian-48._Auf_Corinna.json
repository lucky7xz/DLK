{"textgrid.poem.50391": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "48. Auf Corinna", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die weil ", "tokens": ["Die", "weil"], "token_info": ["word", "word"], "pos": ["ART", "KOUS"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Als sein Verlangen sie gestillt,", "tokens": ["Als", "sein", "Ver\u00b7lan\u00b7gen", "sie", "ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts sey, ", "tokens": ["Nichts", "sey", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,"], "meter": "++", "measure": "spondeus"}, "line.4": {"text": "O Thorheit! weil sie ihn hiedurch zu sagen zwingt;", "tokens": ["O", "Thor\u00b7heit", "!", "weil", "sie", "ihn", "hie\u00b7durch", "zu", "sa\u00b7gen", "zwingt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "KOUS", "PPER", "PPER", "PAV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dass ", "tokens": ["Dass"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}}}}}