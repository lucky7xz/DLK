{"textgrid.poem.34748": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sp\u00e4therbstnebel, kalte Tr\u00e4ume,", "tokens": ["Sp\u00e4t\u00b7herbst\u00b7ne\u00b7bel", ",", "kal\u00b7te", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00dcberfloren Berg und Tal,", "tokens": ["\u00dc\u00b7berf\u00b7lo\u00b7ren", "Berg", "und", "Tal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sturm entbl\u00e4ttert schon die B\u00e4ume,", "tokens": ["Sturm", "ent\u00b7bl\u00e4t\u00b7tert", "schon", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie schaun gespenstisch kahl.", "tokens": ["Und", "sie", "schaun", "ge\u00b7spens\u00b7tisch", "kahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur ein einz'ger, traurig schweigsam", "tokens": ["Nur", "ein", "einz'\u00b7ger", ",", "trau\u00b7rig", "schweig\u00b7sam"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "ADJA", "$,", "ADJD", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einz'ger Baum steht unentlaubt,", "tokens": ["Einz'\u00b7ger", "Baum", "steht", "un\u00b7ent\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Feucht von Wehmutstr\u00e4nen gleichsam,", "tokens": ["Feucht", "von", "Weh\u00b7muts\u00b7tr\u00e4\u00b7nen", "gleich\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00fcttelt er sein gr\u00fcnes Haupt.", "tokens": ["Sch\u00fct\u00b7telt", "er", "sein", "gr\u00fc\u00b7nes", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Ach, mein Herz gleicht dieser Wildnis,", "tokens": ["Ach", ",", "mein", "Herz", "gleicht", "die\u00b7ser", "Wild\u00b7nis", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Baum, den ich dort schau", "tokens": ["Und", "der", "Baum", ",", "den", "ich", "dort", "schau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sommergr\u00fcn, das ist dein Bildnis,", "tokens": ["Som\u00b7mer\u00b7gr\u00fcn", ",", "das", "ist", "dein", "Bild\u00b7nis", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vielgeliebte, sch\u00f6ne Frau!", "tokens": ["Viel\u00b7ge\u00b7lieb\u00b7te", ",", "sch\u00f6\u00b7ne", "Frau", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}