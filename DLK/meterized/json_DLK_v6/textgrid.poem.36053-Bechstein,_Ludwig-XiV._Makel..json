{"textgrid.poem.36053": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "XiV. Makel.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man spricht von einem Spiegel, der duldet keinen Rost;", "tokens": ["Man", "spricht", "von", "ei\u00b7nem", "Spie\u00b7gel", ",", "der", "dul\u00b7det", "kei\u00b7nen", "Rost", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und eine Blume giebt es, die knickt ein einz'ger Frost;", "tokens": ["Und", "ei\u00b7ne", "Blu\u00b7me", "giebt", "es", ",", "die", "knickt", "ein", "einz'\u00b7ger", "Frost", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein Kleinod, das nur einmal die Kunst des Meisters schuf,", "tokens": ["Ein", "Klei\u00b7nod", ",", "das", "nur", "ein\u00b7mal", "die", "Kunst", "des", "Meis\u00b7ters", "schuf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sich, ", "tokens": ["Sich", ","], "token_info": ["word", "punct"], "pos": ["PRF", "$,"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Dort steht ein Bilderkr\u00e4mer, vom allem Volk umdr\u00e4ngt;", "tokens": ["Dort", "steht", "ein", "Bil\u00b7der\u00b7kr\u00e4\u00b7mer", ",", "vom", "al\u00b7lem", "Volk", "um\u00b7dr\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "APPRART", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vom Faustus hat er Thaten im Holzschnitt ausgeh\u00e4ngt.", "tokens": ["Vom", "Faus\u00b7tus", "hat", "er", "Tha\u00b7ten", "im", "Holz\u00b7schnitt", "aus\u00b7ge\u00b7h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Auf dieser Bilder einem frisst Faust ein Fuder Heu,", "tokens": ["Auf", "die\u00b7ser", "Bil\u00b7der", "ei\u00b7nem", "frisst", "Faust", "ein", "Fu\u00b7der", "Heu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das Bild, wie Faust den Rosskamm betr\u00fcgt, h\u00e4ngt gleich dabei.", "tokens": ["Das", "Bild", ",", "wie", "Faust", "den", "Ross\u00b7kamm", "be\u00b7tr\u00fcgt", ",", "h\u00e4ngt", "gleich", "da\u00b7bei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "NN", "ART", "NN", "VVFIN", "$,", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Und Faustus geht im Zimmer ingrimmig, wild umher.", "tokens": ["Und", "Faus\u00b7tus", "geht", "im", "Zim\u00b7mer", "in\u00b7grim\u00b7mig", ",", "wild", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "APPRART", "NN", "ADJD", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbwer thut mir solchen Schimpf an, solch ungeheuern, wer?", "tokens": ["\u00bb", "wer", "thut", "mir", "sol\u00b7chen", "Schimpf", "an", ",", "solch", "un\u00b7ge\u00b7heu\u00b7ern", ",", "wer", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$,", "PIAT", "ADJA", "$,", "PWS", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich, ein gemeiner Gauner, der Gaukelk\u00fcnste treibt?", "tokens": ["Ich", ",", "ein", "ge\u00b7mei\u00b7ner", "Gau\u00b7ner", ",", "der", "Gau\u00b7kel\u00b7k\u00fcns\u00b7te", "treibt", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ich, ", "tokens": ["Ich", ","], "token_info": ["word", "punct"], "pos": ["PPER", "$,"], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "\u00bbgeh, Wagner! Eile! Fliege! Abkauf ihm seinen Kram!", "tokens": ["\u00bb", "geh", ",", "Wag\u00b7ner", "!", "Ei\u00b7le", "!", "Flie\u00b7ge", "!", "Ab\u00b7kauf", "ihm", "sei\u00b7nen", "Kram", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$.", "NN", "$.", "NN", "$.", "NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Erstick' die freche L\u00fcge! Weh! Mich erstickt die Scham!", "tokens": ["Er\u00b7stick'", "die", "fre\u00b7che", "L\u00fc\u00b7ge", "!", "Weh", "!", "Mich", "er\u00b7stickt", "die", "Scham", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$.", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mephisto!!\u00ab \u2013 und der Diener ist gleich dem Ruf zur Stell';", "tokens": ["Me\u00b7phis\u00b7to", "!!", "\u00ab", "\u2013", "und", "der", "Die\u00b7ner", "ist", "gleich", "dem", "Ruf", "zur", "Stell'", ";"], "token_info": ["word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "$(", "KON", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "\u00bbfluch Dir, Du t\u00fcckischlistger, Du teuflischer Gesell!\u00ab", "tokens": ["\u00bb", "fluch", "Dir", ",", "Du", "t\u00fc\u00b7ckischlist\u00b7ger", ",", "Du", "teuf\u00b7li\u00b7scher", "Ge\u00b7sell", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "$,", "PPER", "ADJA", "$,", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "\u00bbbewahrst Du ", "tokens": ["\u00bb", "be\u00b7wahrst", "Du"], "token_info": ["punct", "word", "word"], "pos": ["$(", "VVFIN", "PPER"], "meter": "--+", "measure": "anapaest.init"}, "line.2": {"text": "Und was mich tief erniedrigt, dess hast Du nimmer Acht!", "tokens": ["Und", "was", "mich", "tief", "er\u00b7nied\u00b7rigt", ",", "dess", "hast", "Du", "nim\u00b7mer", "Acht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VVPP", "$,", "PDS", "VAFIN", "PPER", "ADV", "CARD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Fort! Reisse den in St\u00fccken, der mir die Ehre stahl!", "tokens": ["Fort", "!", "Reis\u00b7se", "den", "in", "St\u00fc\u00b7cken", ",", "der", "mir", "die", "Eh\u00b7re", "stahl", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "ART", "APPR", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Umgarn' ihn, halt' ihn, qu\u00e4l' ihn mit Deiner \u00e4rgsten Qual!\u00ab", "tokens": ["Um\u00b7garn'", "ihn", ",", "halt'", "ihn", ",", "qu\u00e4l'", "ihn", "mit", "Dei\u00b7ner", "\u00e4rgs\u00b7ten", "Qual", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "So Faust mit gl\u00fchnden Wangen und zornigzitternd spricht,", "tokens": ["So", "Faust", "mit", "gl\u00fchn\u00b7den", "Wan\u00b7gen", "und", "zor\u00b7nig\u00b7zit\u00b7ternd", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ADJA", "NN", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch ruhig steht Mephisto; er weicht und wanket nicht.", "tokens": ["Doch", "ru\u00b7hig", "steht", "Me\u00b7phis\u00b7to", ";", "er", "weicht", "und", "wan\u00b7ket", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "NE", "$.", "PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er steht und blickt mit L\u00e4cheln auf den zornvollen Mann,", "tokens": ["Er", "steht", "und", "blickt", "mit", "L\u00e4\u00b7cheln", "auf", "den", "zorn\u00b7vol\u00b7len", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Als fessl' ihn an den Boden ein m\u00e4cht'ger Zauberbann.", "tokens": ["Als", "fessl'", "ihn", "an", "den", "Bo\u00b7den", "ein", "m\u00e4cht'\u00b7ger", "Zau\u00b7ber\u00b7bann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbbist Du der Faustus?\u00ab spricht er: \u00bbder k\u00fchn durch Wolken fliegt?", "tokens": ["\u00bb", "bist", "Du", "der", "Faus\u00b7tus", "?", "\u00ab", "spricht", "er", ":", "\u00bb", "der", "k\u00fchn", "durch", "Wol\u00b7ken", "fliegt", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ART", "NN", "$.", "$(", "VVFIN", "PPER", "$.", "$(", "ART", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der \u00fcber alle Kr\u00e4fte der Wesenmutter siegt?", "tokens": ["Der", "\u00fc\u00b7ber", "al\u00b7le", "Kr\u00e4f\u00b7te", "der", "We\u00b7sen\u00b7mut\u00b7ter", "siegt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Bist Du der Faustus? Himmel und H\u00f6lle fasst Dein Drang?", "tokens": ["Bist", "Du", "der", "Faus\u00b7tus", "?", "Him\u00b7mel", "und", "H\u00f6l\u00b7le", "fasst", "Dein", "Drang", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$.", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Und Faustus donnert zornlaut: \u00bbDess soll mich Keiner zeihn,", "tokens": ["Und", "Faus\u00b7tus", "don\u00b7nert", "zorn\u00b7laut", ":", "\u00bb", "Dess", "soll", "mich", "Kei\u00b7ner", "zeihn", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVFIN", "$.", "$(", "PDS", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mich Keiner f\u00e4hig halten solch niedrer Gaukelein!", "tokens": ["Mich", "Kei\u00b7ner", "f\u00e4\u00b7hig", "hal\u00b7ten", "solch", "nie\u00b7drer", "Gau\u00b7ke\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADJD", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Was soll die Nachwelt glauben, wer ich gewesen sei,", "tokens": ["Was", "soll", "die", "Nach\u00b7welt", "glau\u00b7ben", ",", "wer", "ich", "ge\u00b7we\u00b7sen", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$,", "PWS", "PPER", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wird ihr auf ", "tokens": ["Wird", "ihr", "auf"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "\u00bbk\u00fchn nach dem H\u00f6chsten strebt' ich mit meiner ganzen Macht;", "tokens": ["\u00bb", "k\u00fchn", "nach", "dem", "H\u00f6chs\u00b7ten", "strebt'", "ich", "mit", "mei\u00b7ner", "gan\u00b7zen", "Macht", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und dieser Gottheit hab' ich mein H\u00f6chstes dargebracht!", "tokens": ["Und", "die\u00b7ser", "Got\u00b7theit", "hab'", "ich", "mein", "H\u00f6chs\u00b7tes", "dar\u00b7ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Weh, hing sich solcher Makel an meines Namens Schall!", "tokens": ["Weh", ",", "hing", "sich", "sol\u00b7cher", "Ma\u00b7kel", "an", "mei\u00b7nes", "Na\u00b7mens", "Schall", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PRF", "PIAT", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das w\u00e4r' mein schwerster Jammer, das w\u00e4r' mein tiefster Fall!\u00ab", "tokens": ["Das", "w\u00e4r'", "mein", "schwers\u00b7ter", "Jam\u00b7mer", ",", "das", "w\u00e4r'", "mein", "tiefs\u00b7ter", "Fall", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "\u203aso s\u00e4ttigt Dich die Frucht nicht des Gl\u00fccks der Gegenwart?", "tokens": ["\u203a", "so", "s\u00e4t\u00b7tigt", "Dich", "die", "Frucht", "nicht", "des", "Gl\u00fccks", "der", "Ge\u00b7gen\u00b7wart", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Du wirbst auch um den Nachruhm und schiltst mich darum hart?", "tokens": ["Du", "wirbst", "auch", "um", "den", "Nach\u00b7ruhm", "und", "schiltst", "mich", "da\u00b7rum", "hart", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "KON", "VVFIN", "PRF", "PAV", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wohl! Nachwelt soll Dich richten, Dein Wille soll geschehn!", "tokens": ["Wohl", "!", "Nach\u00b7welt", "soll", "Dich", "rich\u00b7ten", ",", "Dein", "Wil\u00b7le", "soll", "ge\u00b7schehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "NN", "VMFIN", "PPER", "VVINF", "$,", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Bilder \u2013 will ich vernichten, es soll sie Keiner sehn!\u2039", "tokens": ["Die", "Bil\u00b7der", "\u2013", "will", "ich", "ver\u00b7nich\u00b7ten", ",", "es", "soll", "sie", "Kei\u00b7ner", "sehn", "!", "\u2039"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$(", "VMFIN", "PPER", "VVINF", "$,", "PPER", "VMFIN", "PPER", "PIS", "VVINF", "$.", "$("], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Mephisto ruft's voll Hohnes, und schwindet, Schatten gleich;", "tokens": ["Me\u00b7phis\u00b7to", "ruft's", "voll", "Hoh\u00b7nes", ",", "und", "schwin\u00b7det", ",", "Schat\u00b7ten", "gleich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "ADJA", "$,", "KON", "VVFIN", "$,", "NN", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ihm nach blickt Faustus finster, freudarm und unmuthreich.", "tokens": ["Ihm", "nach", "blickt", "Faus\u00b7tus", "fins\u00b7ter", ",", "freu\u00b7darm", "und", "un\u00b7mu\u00b7th\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "VVFIN", "NE", "NE", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbdie Nachwelt \u2013 soll mich richten? Mein Wille soll geschehn?", "tokens": ["\u00bb", "die", "Nach\u00b7welt", "\u2013", "soll", "mich", "rich\u00b7ten", "?", "Mein", "Wil\u00b7le", "soll", "ge\u00b7schehn", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$(", "VMFIN", "PRF", "VVINF", "$.", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Weh, wenn solch niedre Geschichten zu ihr einst \u00fcbergehn!\u00ab", "tokens": ["Weh", ",", "wenn", "solch", "nied\u00b7re", "Ge\u00b7schich\u00b7ten", "zu", "ihr", "einst", "\u00fc\u00b7ber\u00b7gehn", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "KOUS", "PIAT", "ADJA", "NN", "APPR", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}}, "stanza.12": {"line.1": {"text": "\u00bbfort! Fort von hier! Hier trag' ich kein Menschenantlitz mehr!", "tokens": ["\u00bb", "fort", "!", "Fort", "von", "hier", "!", "Hier", "trag'", "ich", "kein", "Men\u00b7schen\u00b7ant\u00b7litz", "mehr", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "NN", "APPR", "ADV", "$.", "ADV", "VVFIN", "PPER", "PIAT", "NN", "ADV", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mein Name l\u00e4uft besudelt von Haus zu Haus umher,", "tokens": ["Mein", "Na\u00b7me", "l\u00e4uft", "be\u00b7su\u00b7delt", "von", "Haus", "zu", "Haus", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein r\u00e4nd'ger Hund, den Bosheit und Bubenwitz entbl\u00f6sst,", "tokens": ["Ein", "r\u00e4n\u00b7d'\u00b7ger", "Hund", ",", "den", "Bos\u00b7heit", "und", "Bu\u00b7ben\u00b7witz", "ent\u00b7bl\u00f6sst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Den Jeder mit Verachtung, mit F\u00fcssen von sich st\u00f6sst!\u00ab", "tokens": ["Den", "Je\u00b7der", "mit", "Ver\u00b7ach\u00b7tung", ",", "mit", "F\u00fcs\u00b7sen", "von", "sich", "st\u00f6sst", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "$,", "APPR", "NN", "APPR", "PRF", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "\u00bbo guter Ruf, Du Perle, die sanft, bescheiden gl\u00e4nzt,", "tokens": ["\u00bb", "o", "gu\u00b7ter", "Ruf", ",", "Du", "Per\u00b7le", ",", "die", "sanft", ",", "be\u00b7schei\u00b7den", "gl\u00e4nzt", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "ADJA", "NN", "$,", "PPER", "NN", "$,", "PRELS", "ADJD", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Im Goldreif reinen Wandels bist Du fest eingegrenzt;", "tokens": ["Im", "Gold\u00b7reif", "rei\u00b7nen", "Wan\u00b7dels", "bist", "Du", "fest", "ein\u00b7ge\u00b7grenzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Wenn Frevel Dich herausbricht aus Deinem sichern Port,", "tokens": ["Wenn", "Fre\u00b7vel", "Dich", "her\u00b7aus\u00b7bricht", "aus", "Dei\u00b7nem", "si\u00b7chern", "Port", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dann gehst Du leicht verloren, und rollst f\u00fcr immer fort.\u00ab", "tokens": ["Dann", "gehst", "Du", "leicht", "ver\u00b7lo\u00b7ren", ",", "und", "rollst", "f\u00fcr", "im\u00b7mer", "fort", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVPP", "$,", "KON", "VVFIN", "APPR", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "\u00bbleicht tr\u00fcbt den Ehrenspiegel ein Fleck, der nimmer weicht;", "tokens": ["\u00bb", "leicht", "tr\u00fcbt", "den", "Eh\u00b7ren\u00b7spie\u00b7gel", "ein", "Fleck", ",", "der", "nim\u00b7mer", "weicht", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Den aus dem goldnen Buche des Lebens Keiner streicht.", "tokens": ["Den", "aus", "dem", "gold\u00b7nen", "Bu\u00b7che", "des", "Le\u00b7bens", "Kei\u00b7ner", "streicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Vom Markt ins Haus getragen sticht mich ein Skorpion,", "tokens": ["Vom", "Markt", "ins", "Haus", "ge\u00b7tra\u00b7gen", "sticht", "mich", "ein", "Skor\u00b7pi\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "VVPP", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und Jener brennt als Wundarzt die Beule mir mit Hohn.\u00ab", "tokens": ["Und", "Je\u00b7ner", "brennt", "als", "Wund\u00b7arzt", "die", "Beu\u00b7le", "mir", "mit", "Hohn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PDAT", "VVFIN", "KOUS", "NN", "ART", "NN", "PPER", "APPR", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "\u00bbwer reinigt mir die Ehre von jenem Rostfleck, wer?", "tokens": ["\u00bb", "wer", "rei\u00b7nigt", "mir", "die", "Eh\u00b7re", "von", "je\u00b7nem", "Rost\u00b7fleck", ",", "wer", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "$,", "PWS", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wer stellt verwelkte Blumen frischbl\u00fchend wieder her?", "tokens": ["Wer", "stellt", "ver\u00b7welk\u00b7te", "Blu\u00b7men", "frischbl\u00fc\u00b7hend", "wie\u00b7der", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJA", "NN", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Blumen hat vernichtet der Frechheit eis'ger Frost,", "tokens": ["Die", "Blu\u00b7men", "hat", "ver\u00b7nich\u00b7tet", "der", "Frech\u00b7heit", "eis'\u00b7ger", "Frost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und bittre Galle reicht mir des Satans Hohn zur Kost!\u00ab", "tokens": ["Und", "bitt\u00b7re", "Gal\u00b7le", "reicht", "mir", "des", "Sa\u00b7tans", "Hohn", "zur", "Kost", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}