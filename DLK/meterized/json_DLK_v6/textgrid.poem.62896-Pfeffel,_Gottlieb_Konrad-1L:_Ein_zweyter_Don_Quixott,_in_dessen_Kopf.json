{"textgrid.poem.62896": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein zweyter Don Quixott, in dessen Kopf", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein zweyter Don Quixott, in dessen Kopf", "tokens": ["Ein", "zwey\u00b7ter", "Don", "Qui\u00b7xott", ",", "in", "des\u00b7sen", "Kopf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es m\u00e4chtig spuckte, kam einst auf die Grille,", "tokens": ["Es", "m\u00e4ch\u00b7tig", "spuck\u00b7te", ",", "kam", "einst", "auf", "die", "Gril\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$,", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er w\u00e4re todt. Schon lag der arme Tropf", "tokens": ["Er", "w\u00e4\u00b7re", "todt", ".", "Schon", "lag", "der", "ar\u00b7me", "Tropf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zween Tage lang in feyerlicher Stille", "tokens": ["Zween", "Ta\u00b7ge", "lang", "in", "fey\u00b7er\u00b7li\u00b7cher", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auf seinem Kanapee. Man fasset ihn beym Schopf,", "tokens": ["Auf", "sei\u00b7nem", "Ka\u00b7na\u00b7pee", ".", "Man", "fas\u00b7set", "ihn", "beym", "Schopf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PIS", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er sinket welk zur\u00fcck; man schreyt ihm in die Ohren,", "tokens": ["Er", "sin\u00b7ket", "welk", "zu\u00b7r\u00fcck", ";", "man", "schreyt", "ihm", "in", "die", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$.", "PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Man kneipt ihn in das Kinn, man spritzt ihm ins Gesicht,", "tokens": ["Man", "kneipt", "ihn", "in", "das", "Kinn", ",", "man", "spritzt", "ihm", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PIS", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Umsonst, er h\u00f6rt und f\u00fchlet nicht.", "tokens": ["Um\u00b7sonst", ",", "er", "h\u00f6rt", "und", "f\u00fch\u00b7let", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Hier, sprach zuletzt der Arzt, ist meine Kunst verloren;", "tokens": ["Hier", ",", "sprach", "zu\u00b7letzt", "der", "Arzt", ",", "ist", "mei\u00b7ne", "Kunst", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "ADV", "ART", "NN", "$,", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Man scharre nur den armen Junker ein,", "tokens": ["Man", "schar\u00b7re", "nur", "den", "ar\u00b7men", "Jun\u00b7ker", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die Todten kann ich nicht erwecken.", "tokens": ["Die", "Tod\u00b7ten", "kann", "ich", "nicht", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der Tischler bringt den schwarzbebeizten Schrein,", "tokens": ["Der", "Tischler", "bringt", "den", "schwarz\u00b7be\u00b7beiz\u00b7ten", "Schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Er wird hineingelegt und keine Spur von Schrecken", "tokens": ["Er", "wird", "hin\u00b7ein\u00b7ge\u00b7legt", "und", "kei\u00b7ne", "Spur", "von", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "PIAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dr\u00fcckt sich in seinen Z\u00fcgen aus.", "tokens": ["Dr\u00fcckt", "sich", "in", "sei\u00b7nen", "Z\u00fc\u00b7gen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ein Bauer, der ihm Zinse bringen wollte,", "tokens": ["Ein", "Bau\u00b7er", ",", "der", "ihm", "Zin\u00b7se", "brin\u00b7gen", "woll\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Kam ungewarnt ins Sterbehaus:", "tokens": ["Kam", "un\u00b7ge\u00b7warnt", "ins", "Ster\u00b7be\u00b7haus", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Er kniete vor den Sarg; das Paternoster rollte", "tokens": ["Er", "knie\u00b7te", "vor", "den", "Sarg", ";", "das", "Pa\u00b7ter\u00b7nos\u00b7ter", "roll\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Durch seine braune Hand, nun dreht er das Gesicht:", "tokens": ["Durch", "sei\u00b7ne", "brau\u00b7ne", "Hand", ",", "nun", "dreht", "er", "das", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Gott hab ihn selig, sprach er zum Husaren,", "tokens": ["Gott", "hab", "ihn", "se\u00b7lig", ",", "sprach", "er", "zum", "Hu\u00b7sa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "$,", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Der Marschallsdienste that, gro\u00df ist der Schade nicht,", "tokens": ["Der", "Mar\u00b7schalls\u00b7diens\u00b7te", "that", ",", "gro\u00df", "ist", "der", "Scha\u00b7de", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "VAFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Denn ach! seit mehr als sieben Jahren", "tokens": ["Denn", "ach", "!", "seit", "mehr", "als", "sie\u00b7ben", "Jah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "APPR", "PIAT", "KOKOM", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "War ja der gute Herr ein Narr.", "tokens": ["War", "ja", "der", "gu\u00b7te", "Herr", "ein", "Narr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Itzt regt der Leichnam sich, die blauen Lippen beben:", "tokens": ["Itzt", "regt", "der", "Leich\u00b7nam", "sich", ",", "die", "blau\u00b7en", "Lip\u00b7pen", "be\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ha, Schurke! rief er aus, (der Kerl ward bleich und starr)", "tokens": ["Ha", ",", "Schur\u00b7ke", "!", "rief", "er", "aus", ",", "(", "der", "Kerl", "ward", "bleich", "und", "starr", ")"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$.", "VVFIN", "PPER", "PTKVZ", "$,", "$(", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "W\u00e4r ich nicht todt, bey meinem Leben,", "tokens": ["W\u00e4r", "ich", "nicht", "todt", ",", "bey", "mei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Ich w\u00fcrde dir f\u00fcnfhundert Pr\u00fcgel geben.", "tokens": ["Ich", "w\u00fcr\u00b7de", "dir", "f\u00fcnf\u00b7hun\u00b7dert", "Pr\u00fc\u00b7gel", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}