{"dta.poem.19238": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "85.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der h\u00f6chsten Liebe Bild, die Henne sieh, die br\u00fctet,", "tokens": ["Der", "h\u00f6chs\u00b7ten", "Lie\u00b7be", "Bild", ",", "die", "Hen\u00b7ne", "sieh", ",", "die", "br\u00fc\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "ART", "NN", "PTKVZ", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie mit der Fl\u00fcgel Schild sie ihre Brut beh\u00fctet.", "tokens": ["Wie", "mit", "der", "Fl\u00fc\u00b7gel", "Schild", "sie", "ih\u00b7re", "Brut", "be\u00b7h\u00fc\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie hat der K\u00fcchlein viel, doch jedes ist gez\u00e4hlt,", "tokens": ["Sie", "hat", "der", "K\u00fcch\u00b7lein", "viel", ",", "doch", "je\u00b7des", "ist", "ge\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "ADV", "PIAT", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ruhig ist sie nicht, wenn ihr nur eines fehlt.", "tokens": ["Und", "ru\u00b7hig", "ist", "sie", "nicht", ",", "wenn", "ihr", "nur", "ei\u00b7nes", "fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Versammeln unter sich wird sie den ganzen Haufen,", "tokens": ["Ver\u00b7sam\u00b7meln", "un\u00b7ter", "sich", "wird", "sie", "den", "gan\u00b7zen", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie weit auch sich von ihr die einzelnen verlaufen.", "tokens": ["Wie", "weit", "auch", "sich", "von", "ihr", "die", "ein\u00b7zel\u00b7nen", "ver\u00b7lau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "PRF", "APPR", "PPER", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie angelegen l\u00e4\u00dft sie sich es seyn, zu locken;", "tokens": ["Wie", "an\u00b7ge\u00b7le\u00b7gen", "l\u00e4\u00dft", "sie", "sich", "es", "seyn", ",", "zu", "lo\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VVFIN", "PPER", "PRF", "PPER", "VAINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kanst du, verlaufne Brut, dagegen dich verstocken?", "tokens": ["Kanst", "du", ",", "ver\u00b7lauf\u00b7ne", "Brut", ",", "da\u00b7ge\u00b7gen", "dich", "ver\u00b7sto\u00b7cken", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADJA", "NN", "$,", "PAV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und lockt dich nicht herbei der Mutterliebe Schrei,", "tokens": ["Und", "lockt", "dich", "nicht", "her\u00b7bei", "der", "Mut\u00b7ter\u00b7lie\u00b7be", "Schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schrecke dich von dort mit dem Gekreisch der Weih.", "tokens": ["So", "schre\u00b7cke", "dich", "von", "dort", "mit", "dem", "Ge\u00b7kreisch", "der", "Weih", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Kriech unter, und du bist vor dem Gekreisch geborgen,", "tokens": ["Kriech", "un\u00b7ter", ",", "und", "du", "bist", "vor", "dem", "Ge\u00b7kreisch", "ge\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "$,", "KON", "PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fcr dein Futter la\u00df der Mutter Liebe sorgen.", "tokens": ["Und", "f\u00fcr", "dein", "Fut\u00b7ter", "la\u00df", "der", "Mut\u00b7ter", "Lie\u00b7be", "sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}