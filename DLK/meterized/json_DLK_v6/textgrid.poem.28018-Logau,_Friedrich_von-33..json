{"textgrid.poem.28018": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Pallas weg von Troja ward genummen,", "tokens": ["Als", "Pal\u00b7las", "weg", "von", "Tro\u00b7ja", "ward", "ge\u00b7num\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPR", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist dessen Heil bald zum Verterben kummen.", "tokens": ["Ist", "des\u00b7sen", "Heil", "bald", "zum", "Ver\u00b7ter\u00b7ben", "kum\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Haus, darau\u00df ein redlich Weib verschieden,", "tokens": ["Ein", "Haus", ",", "dar\u00b7au\u00df", "ein", "red\u00b7lich", "Weib", "ver\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "ART", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bleibt von dem Gl\u00fccke mehrentheils vermieden.", "tokens": ["Bleibt", "von", "dem", "Gl\u00fc\u00b7cke", "meh\u00b7ren\u00b7theils", "ver\u00b7mie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}