{"dta.poem.20680": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An                  \n  Se. Hochf\u00fcrstl. Durchlaucht                  \n  den                  \n  Herzog Ferdinand von Braunschweig-  \n L\u00fcneburg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Dir, empfindungsvoller Held!                 ", "tokens": ["Wenn", "Dir", ",", "emp\u00b7fin\u00b7dungs\u00b7vol\u00b7ler", "Held", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein dankbar Herz in meinem Liede", "tokens": ["Ein", "dank\u00b7bar", "Herz", "in", "mei\u00b7nem", "Lie\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mehr als die hohe Kunst gef\u00e4llt,", "tokens": ["Mehr", "als", "die", "ho\u00b7he", "Kunst", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O dann wird mir Dein Ohr nicht m\u00fcde,", "tokens": ["O", "dann", "wird", "mir", "Dein", "Ohr", "nicht", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "ADJD", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Dann ist es meinem Saitenspiel erlaubt,", "tokens": ["Dann", "ist", "es", "mei\u00b7nem", "Sai\u00b7ten\u00b7spiel", "er\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ohn\u2019 Unterla\u00df Dir vorzut\u00f6nen,", "tokens": ["Ohn'", "Un\u00b7ter\u00b7la\u00df", "Dir", "vor\u00b7zu\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und niemals sch\u00fcttelst Du Dein lorbeerreiches Haupt,", "tokens": ["Und", "nie\u00b7mals", "sch\u00fct\u00b7telst", "Du", "Dein", "lor\u00b7beer\u00b7rei\u00b7ches", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ob Du gleich von den Musens\u00f6hnen", "tokens": ["Ob", "Du", "gleich", "von", "den", "Mu\u00b7sen\u00b7s\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "In Pindars Ton besungen bist,", "tokens": ["In", "Pin\u00b7dars", "Ton", "be\u00b7sun\u00b7gen", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und ich im Ton des kleinen Vogels singe,", "tokens": ["Und", "ich", "im", "Ton", "des", "klei\u00b7nen", "Vo\u00b7gels", "sin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Der, wenn der Tag erwacht, schon in den L\u00fcften ist,", "tokens": ["Der", ",", "wenn", "der", "Tag", "er\u00b7wacht", ",", "schon", "in", "den", "L\u00fcf\u00b7ten", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ART", "NN", "VVPP", "$,", "ADV", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Damit er dem ein Opfer bringe,", "tokens": ["Da\u00b7mit", "er", "dem", "ein", "Op\u00b7fer", "brin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Durch dessen Einflu\u00df die Natur", "tokens": ["Durch", "des\u00b7sen", "Ein\u00b7flu\u00df", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Der Menschen und der Lerchen Speise", "tokens": ["Der", "Men\u00b7schen", "und", "der", "Ler\u00b7chen", "Spei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Hervorbringt auf der Weizenflur.", "tokens": ["Her\u00b7vor\u00b7bringt", "auf", "der", "Wei\u00b7zen\u00b7flur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "O Dir gef\u00e4llt auch dieser leise", "tokens": ["O", "Dir", "ge\u00b7f\u00e4llt", "auch", "die\u00b7ser", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VVFIN", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Stets wiederholte Lobgesang,", "tokens": ["Stets", "wie\u00b7der\u00b7hol\u00b7te", "Lob\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Du h\u00f6rst ihn oftmals Tagelang", "tokens": ["Du", "h\u00f6rst", "ihn", "oft\u00b7mals", "Ta\u00b7ge\u00b7lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Im Erndtemond, wenn Dich die Rebenlaube decket", "tokens": ["Im", "Ernd\u00b7te\u00b7mond", ",", "wenn", "Dich", "die", "Re\u00b7ben\u00b7lau\u00b7be", "de\u00b7cket"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "In Deinem Garten, wo der selbstgepflanzte Kohl", "tokens": ["In", "Dei\u00b7nem", "Gar\u00b7ten", ",", "wo", "der", "selbst\u00b7ge\u00b7pflanz\u00b7te", "Kohl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Vor Deinen Augen w\u00e4chst und Dir so k\u00f6stlich schmecket,", "tokens": ["Vor", "Dei\u00b7nen", "Au\u00b7gen", "w\u00e4chst", "und", "Dir", "so", "k\u00f6st\u00b7lich", "schme\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Als jenem R\u00f6mer, den in\u2019s hohe Capitol", "tokens": ["Als", "je\u00b7nem", "R\u00f6\u00b7mer", ",", "den", "in's", "ho\u00b7he", "Ca\u00b7pi\u00b7tol"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der Siegeswagen trug, die R\u00fcbe schmecken mochte,", "tokens": ["Der", "Sie\u00b7ges\u00b7wa\u00b7gen", "trug", ",", "die", "R\u00fc\u00b7be", "schme\u00b7cken", "moch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die er zur Friedenszeit sich selbst am Heerde kochte.", "tokens": ["Die", "er", "zur", "Frie\u00b7dens\u00b7zeit", "sich", "selbst", "am", "Heer\u00b7de", "koch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein Geist belauschet Dich, erhabner Ferdinand!", "tokens": ["Mein", "Geist", "be\u00b7lau\u00b7schet", "Dich", ",", "er\u00b7hab\u00b7ner", "Fer\u00b7di\u00b7nand", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er siehet Dich Dein l\u00e4ndlich Haus bewohnen,", "tokens": ["Er", "sie\u00b7het", "Dich", "Dein", "l\u00e4nd\u00b7lich", "Haus", "be\u00b7woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da f\u00fctterst Du mit eigner Hand", "tokens": ["Da", "f\u00fct\u00b7terst", "Du", "mit", "eig\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die H\u00fchner die Dir durch ihr freundlich Kirren lohnen,", "tokens": ["Die", "H\u00fch\u00b7ner", "die", "Dir", "durch", "ihr", "freund\u00b7lich", "Kir\u00b7ren", "loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "APPR", "PPOSAT", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und Ceres sieht Dir l\u00e4chelnd zu.", "tokens": ["Und", "Ce\u00b7res", "sieht", "Dir", "l\u00e4\u00b7chelnd", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Augur, der zu Rom die heilgen H\u00fchner speiste,", "tokens": ["Kein", "Au\u00b7gur", ",", "der", "zu", "Rom", "die", "heil\u00b7gen", "H\u00fch\u00b7ner", "speis\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "APPR", "NE", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gefiel der G\u00f6ttin so wie Du,", "tokens": ["Ge\u00b7fiel", "der", "G\u00f6t\u00b7tin", "so", "wie", "Du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Denn keiner war von solchem Geiste,", "tokens": ["Denn", "kei\u00b7ner", "war", "von", "sol\u00b7chem", "Geis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So tapfer und zugleich so sanft und angenehm.", "tokens": ["So", "tap\u00b7fer", "und", "zu\u00b7gleich", "so", "sanft", "und", "an\u00b7ge\u00b7nehm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und k\u00f6nnt\u2019 ich selber so bequem", "tokens": ["Und", "k\u00f6nnt'", "ich", "sel\u00b7ber", "so", "be\u00b7quem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Als wie mein Geist die Luft durchwandern,", "tokens": ["Als", "wie", "mein", "Geist", "die", "Luft", "durch\u00b7wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Dann k\u00e4m ich oft Dir zuzusehn.", "tokens": ["Dann", "k\u00e4m", "ich", "oft", "Dir", "zu\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Herr, Deine M\u00e4\u00dfigung fehlt vielen Alexandern,", "tokens": ["Herr", ",", "Dei\u00b7ne", "M\u00e4\u00b7\u00dfi\u00b7gung", "fehlt", "vie\u00b7len", "A\u00b7lex\u00b7an\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.14": {"text": "Die Thaten, welche schon von Ihnen sind geschehn,", "tokens": ["Die", "Tha\u00b7ten", ",", "wel\u00b7che", "schon", "von", "Ih\u00b7nen", "sind", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Nicht achten, und h\u00f6chst unzufrieden leben,", "tokens": ["Nicht", "ach\u00b7ten", ",", "und", "h\u00f6chst", "un\u00b7zu\u00b7frie\u00b7den", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "KON", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Weil sie der Welt von sich nichts mehr zu reden geben.", "tokens": ["Weil", "sie", "der", "Welt", "von", "sich", "nichts", "mehr", "zu", "re\u00b7den", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PRF", "PIS", "PIS", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}