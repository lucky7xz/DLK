{"textgrid.poem.52853": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Heimt\u00fcck' und Reue", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df mit Liebe Grausamkeit verbunden", "tokens": ["Da\u00df", "mit", "Lie\u00b7be", "Grau\u00b7sam\u00b7keit", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Oftmals durch des Schicksals Toben ist,", "tokens": ["Oft\u00b7mals", "durch", "des", "Schick\u00b7sals", "To\u00b7ben", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hat bekanntlich Romeo empfunden", "tokens": ["Hat", "be\u00b7kannt\u00b7lich", "Ro\u00b7meo", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "NN", "VVPP"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sammt der Julia, die ihn gek\u00fc\u00dft.", "tokens": ["Sammt", "der", "Ju\u00b7lia", ",", "die", "ihn", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Aber schauderbar", "tokens": ["A\u00b7ber", "schau\u00b7der\u00b7bar"], "token_info": ["word", "word"], "pos": ["KON", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Die Geschichte war,", "tokens": ["Die", "Ge\u00b7schich\u00b7te", "war", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Die erlebte j\u00fcngst ein Decopist.", "tokens": ["Die", "er\u00b7leb\u00b7te", "j\u00fcngst", "ein", "De\u00b7co\u00b7pist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Michel Heidt der Zwanzigste, ein Bauer", "tokens": ["Mi\u00b7chel", "Heidt", "der", "Zwan\u00b7zigs\u00b7te", ",", "ein", "Bau\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "In der Landgemeinde Muckenwies,", "tokens": ["In", "der", "Land\u00b7ge\u00b7mein\u00b7de", "Mu\u00b7cken\u00b7wies", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "War um seine Ehefrau in Trauer,", "tokens": ["War", "um", "sei\u00b7ne", "E\u00b7he\u00b7frau", "in", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "W\u00e4hrend seine Tochter Eva hie\u00df.", "tokens": ["W\u00e4h\u00b7rend", "sei\u00b7ne", "Toch\u00b7ter", "E\u00b7va", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und vom Landgericht", "tokens": ["Und", "vom", "Land\u00b7ge\u00b7richt"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Mehrmals monatlich", "tokens": ["Mehr\u00b7mals", "mo\u00b7nat\u00b7lich"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Kam der Wilhelm Straub, das ist gewi\u00df.", "tokens": ["Kam", "der", "Wil\u00b7helm", "Straub", ",", "das", "ist", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NE", "NE", "$,", "PDS", "VAFIN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Jener Vater eines M\u00e4dchens hatte", "tokens": ["Je\u00b7ner", "Va\u00b7ter", "ei\u00b7nes", "M\u00e4d\u00b7chens", "hat\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "ART", "NN", "VAFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Diesen k\u00fchnen J\u00fcngling auf dem Strich,", "tokens": ["Die\u00b7sen", "k\u00fch\u00b7nen", "J\u00fcng\u00b7ling", "auf", "dem", "Strich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Auch die Tochter hatt' er auf der Latte", "tokens": ["Auch", "die", "Toch\u00b7ter", "hatt'", "er", "auf", "der", "Lat\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Weil sie seiner Aufsicht hold entwich,", "tokens": ["Weil", "sie", "sei\u00b7ner", "Auf\u00b7sicht", "hold", "ent\u00b7wich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und er hielt die Zwei", "tokens": ["Und", "er", "hielt", "die", "Zwei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "CARD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Nicht sowohl f\u00fcr treu,", "tokens": ["Nicht", "so\u00b7wohl", "f\u00fcr", "treu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KON", "APPR", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Sondern aber fast f\u00fcr l\u00fcderlich.", "tokens": ["Son\u00b7dern", "a\u00b7ber", "fast", "f\u00fcr", "l\u00fc\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Darum konnte man's ihm nicht verdenken,", "tokens": ["Da\u00b7rum", "konn\u00b7te", "man's", "ihm", "nicht", "ver\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PIS", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df das Paar melankatholisch ward,", "tokens": ["Da\u00df", "das", "Paar", "me\u00b7lan\u00b7ka\u00b7tho\u00b7lisch", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df sich Er herumtrieb in den Schenken,", "tokens": ["Da\u00df", "sich", "Er", "her\u00b7um\u00b7trieb", "in", "den", "Schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Und da\u00df sie verd\u00e4chtig auf ihn harrt;", "tokens": ["Und", "da\u00df", "sie", "ver\u00b7d\u00e4ch\u00b7tig", "auf", "ihn", "harrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Denn des Abends spat", "tokens": ["Denn", "des", "A\u00b7bends", "spat"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "In der Kemmenat", "tokens": ["In", "der", "Kem\u00b7me\u00b7nat"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Uebert\u00e4ubten sie die Gegenwart.", "tokens": ["Ue\u00b7ber\u00b7t\u00e4ub\u00b7ten", "sie", "die", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Doch der Vater, vor der Zukunft bange,", "tokens": ["Doch", "der", "Va\u00b7ter", ",", "vor", "der", "Zu\u00b7kunft", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Durch Vergangenheit gewitzigt schier,", "tokens": ["Durch", "Ver\u00b7gan\u00b7gen\u00b7heit", "ge\u00b7wit\u00b7zigt", "schier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "N\u00e4hrt im Busen b\u00f6sen Heimtucks Schlange,", "tokens": ["N\u00e4hrt", "im", "Bu\u00b7sen", "b\u00f6\u00b7sen", "Heim\u00b7tucks", "Schlan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Rasselt schrecklich pl\u00f6tzlich an der Th\u00fcr;", "tokens": ["Ras\u00b7selt", "schreck\u00b7lich", "pl\u00f6tz\u00b7lich", "an", "der", "Th\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und vor seines Grimm's", "tokens": ["Und", "vor", "sei\u00b7nes", "Grim\u00b7m's"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.6": {"text": "Eile, durchs Gesims", "tokens": ["Ei\u00b7le", ",", "durchs", "Ge\u00b7sims"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Schwingt der J\u00fcngling sich in's Luftrevier.", "tokens": ["Schwingt", "der", "J\u00fcng\u00b7ling", "sich", "in's", "Luf\u00b7tre\u00b7vier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Aber, wie der sorgenvolle Vater", "tokens": ["A\u00b7ber", ",", "wie", "der", "sor\u00b7gen\u00b7vol\u00b7le", "Va\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Arglist schnaubend es vorhergesehn,", "tokens": ["Arg\u00b7list", "schnau\u00b7bend", "es", "vor\u00b7her\u00b7ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Oeffnet landwirthschaftlich sich ein Krater", "tokens": ["Oeff\u00b7net", "land\u00b7wirth\u00b7schaft\u00b7lich", "sich", "ein", "Kra\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PRF", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Unten, wo des Hofes Kater stehn.", "tokens": ["Un\u00b7ten", ",", "wo", "des", "Ho\u00b7fes", "Ka\u00b7ter", "stehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "In der Grube gro\u00df", "tokens": ["In", "der", "Gru\u00b7be", "gro\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Schn\u00f6de G\u00fclle flo\u00df", "tokens": ["Schn\u00f6\u00b7de", "G\u00fcl\u00b7le", "flo\u00df"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Uebers Haupt, ach, dem Verwegenen!", "tokens": ["Ue\u00b7bers", "Haupt", ",", "ach", ",", "dem", "Ver\u00b7we\u00b7ge\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ITJ", "$,", "ART", "NN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.7": {"line.1": {"text": "Aus dem Himmel in den Pfuhl der H\u00f6lle,", "tokens": ["Aus", "dem", "Him\u00b7mel", "in", "den", "Pfuhl", "der", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Glaubt der Aermste, j\u00e4hlings sich gest\u00fcrzt,", "tokens": ["Glaubt", "der", "A\u00b7erms\u00b7te", ",", "j\u00e4h\u00b7lings", "sich", "ge\u00b7st\u00fcrzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In des schaurigen Kozytus Welle", "tokens": ["In", "des", "schau\u00b7ri\u00b7gen", "Ko\u00b7zy\u00b7tus", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Seines Lebens Faden schon gek\u00fcrzt,", "tokens": ["Sei\u00b7nes", "Le\u00b7bens", "Fa\u00b7den", "schon", "ge\u00b7k\u00fcrzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Doch er taucht empor,", "tokens": ["Doch", "er", "taucht", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Und in's frohe Ohr", "tokens": ["Und", "in's", "fro\u00b7he", "Ohr"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Braust die Welt ihm wieder, stark durchw\u00fcrzt.", "tokens": ["Braust", "die", "Welt", "ihm", "wie\u00b7der", ",", "stark", "durch\u00b7w\u00fcrzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Weh! Da stand er in dem Morgengrauen", "tokens": ["Weh", "!", "Da", "stand", "er", "in", "dem", "Mor\u00b7gen\u00b7grau\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Als ein n\u00e4chtlich dunkler Ehrenmann,", "tokens": ["Als", "ein", "n\u00e4cht\u00b7lich", "dunk\u00b7ler", "Eh\u00b7ren\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Noch vermocht er nicht das Licht zu schauen,", "tokens": ["Noch", "ver\u00b7mocht", "er", "nicht", "das", "Licht", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Doch zu d\u00e4mmern es ihm sonst begann.", "tokens": ["Doch", "zu", "d\u00e4m\u00b7mern", "es", "ihm", "sonst", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "S\u00fc\u00dfen Trost vernahm", "tokens": ["S\u00fc\u00b7\u00dfen", "Trost", "ver\u00b7nahm"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Er in Schreck und Scham,", "tokens": ["Er", "in", "Schreck", "und", "Scham", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Als sich oben Pr\u00fcgelei entspann.", "tokens": ["Als", "sich", "o\u00b7ben", "Pr\u00fc\u00b7ge\u00b7lei", "ent\u00b7spann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Denn er h\u00f6ret der Geliebten Stimme,", "tokens": ["Denn", "er", "h\u00f6\u00b7ret", "der", "Ge\u00b7lieb\u00b7ten", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ob sie auch der Vater, wild entsatzt,", "tokens": ["Ob", "sie", "auch", "der", "Va\u00b7ter", ",", "wild", "ent\u00b7satzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gleichend einem W\u00fcstenungeth\u00fcme", "tokens": ["Glei\u00b7chend", "ei\u00b7nem", "W\u00fcs\u00b7te\u00b7nun\u00b7ge\u00b7th\u00fc\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Unbarmherzig, l\u00e4chelnd, durchkarbatscht,", "tokens": ["Un\u00b7barm\u00b7her\u00b7zig", ",", "l\u00e4\u00b7chelnd", ",", "durch\u00b7kar\u00b7batscht", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ob es gleich ihm ahnt,", "tokens": ["Ob", "es", "gleich", "ihm", "ahnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Was da allerhand", "tokens": ["Was", "da", "al\u00b7ler\u00b7hand"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADV", "PIAT"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "W\u00fcrde allw\u00e4rts bald herumgeklatscht.", "tokens": ["W\u00fcr\u00b7de", "all\u00b7w\u00e4rts", "bald", "her\u00b7um\u00b7ge\u00b7klatscht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Und mit einem w\u00fcthigen Entschlusse", "tokens": ["Und", "mit", "ei\u00b7nem", "w\u00fct\u00b7hi\u00b7gen", "Ent\u00b7schlus\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Rettungslos ergreifet er die Flucht,", "tokens": ["Ret\u00b7tungs\u00b7los", "er\u00b7grei\u00b7fet", "er", "die", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Tr\u00e4umend von des Abschieds hei\u00dfem Kusse", "tokens": ["Tr\u00e4u\u00b7mend", "von", "des", "Ab\u00b7schieds", "hei\u00b7\u00dfem", "Kus\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und des Farrenschweifes kalter Wucht;", "tokens": ["Und", "des", "Far\u00b7ren\u00b7schwei\u00b7fes", "kal\u00b7ter", "Wucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Athmet wieder frei", "tokens": ["Ath\u00b7met", "wie\u00b7der", "frei"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADV", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Erst im n\u00e4chsten Mai,", "tokens": ["Erst", "im", "n\u00e4chs\u00b7ten", "Mai", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "In Australiens ferner Hafenbucht.", "tokens": ["In", "Aust\u00b7ra\u00b7li\u00b7ens", "fer\u00b7ner", "Ha\u00b7fen\u00b7bucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Ja, die treue Liebe wird belohnet", "tokens": ["Ja", ",", "die", "treu\u00b7e", "Lie\u00b7be", "wird", "be\u00b7loh\u00b7net"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN", "VAFIN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In dem herrlichen Australia,", "tokens": ["In", "dem", "herr\u00b7li\u00b7chen", "Aust\u00b7ra\u00b7lia", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Staunet nun, wie der Entwich'ne wohnet,", "tokens": ["Stau\u00b7net", "nun", ",", "wie", "der", "Ent\u00b7wich'\u00b7ne", "woh\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Einer lieblicheren Grube nah:", "tokens": ["Ei\u00b7ner", "lieb\u00b7li\u00b7che\u00b7ren", "Gru\u00b7be", "nah", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "In des Goldes Staub", "tokens": ["In", "des", "Gol\u00b7des", "Staub"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "W\u00e4lzt sich Wilhelm Straub,", "tokens": ["W\u00e4lzt", "sich", "Wil\u00b7helm", "Straub", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NE", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Stehet gl\u00e4nzend als ein Ph\u00f6nix da.", "tokens": ["Ste\u00b7het", "gl\u00e4n\u00b7zend", "als", "ein", "Ph\u00f6\u00b7nix", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Und er lebet wie ein reicher Nabob,", "tokens": ["Und", "er", "le\u00b7bet", "wie", "ein", "rei\u00b7cher", "Na\u00b7bob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "ART", "ADJD", "NN", "$,"], "meter": "+-----+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Sparsam nur der Liebe huldigend,", "tokens": ["Spar\u00b7sam", "nur", "der", "Lie\u00b7be", "hul\u00b7di\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Denn ein Brief vom Muckenwieser Jakob", "tokens": ["Denn", "ein", "Brief", "vom", "Mu\u00b7cken\u00b7wie\u00b7ser", "Ja\u00b7kob"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Meldet, da\u00df die alte Flamme brennt;", "tokens": ["Mel\u00b7det", ",", "da\u00df", "die", "al\u00b7te", "Flam\u00b7me", "brennt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Da\u00df der alte Heidt", "tokens": ["Da\u00df", "der", "al\u00b7te", "Heidt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Sei voll Traurigkeit,", "tokens": ["Sei", "voll", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"tex