{"textgrid.poem.43855": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wann ist der Fr\u00fchling wieder da?", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, lieber guter Gro\u00dfpapa,", "tokens": ["Ach", ",", "lie\u00b7ber", "gu\u00b7ter", "Gro\u00df\u00b7pa\u00b7pa", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wann ist der Fr\u00fchling wieder da?", "tokens": ["Wann", "ist", "der", "Fr\u00fch\u00b7ling", "wie\u00b7der", "da", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns Kindern wird ganz angst und bang:", "tokens": ["Uns", "Kin\u00b7dern", "wird", "ganz", "angst", "und", "bang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Winter w\u00e4hrt auch gar zu lang.", "tokens": ["Der", "Win\u00b7ter", "w\u00e4hrt", "auch", "gar", "zu", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und als wir heute so gefragt,", "tokens": ["Und", "als", "wir", "heu\u00b7te", "so", "ge\u00b7fragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da hat der Gro\u00dfpapa gesagt:", "tokens": ["Da", "hat", "der", "Gro\u00df\u00b7pa\u00b7pa", "ge\u00b7sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbei, Kinder, wollt ihr artig sein,", "tokens": ["\u00bb", "ei", ",", "Kin\u00b7der", ",", "wollt", "ihr", "ar\u00b7tig", "sein", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "$,", "VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So stellt er sich gar zeitig ein.\u00ab", "tokens": ["So", "stellt", "er", "sich", "gar", "zei\u00b7tig", "ein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der liebe Fr\u00fchling meint es gut:", "tokens": ["Der", "lie\u00b7be", "Fr\u00fch\u00b7ling", "meint", "es", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was er uns doch zu Liebe thut!", "tokens": ["Was", "er", "uns", "doch", "zu", "Lie\u00b7be", "thut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er bringt den lieben Sonnenschein", "tokens": ["Er", "bringt", "den", "lie\u00b7ben", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Vogelsang und Bl\u00fcmelein.", "tokens": ["Und", "Vo\u00b7gel\u00b7sang", "und", "Bl\u00fc\u00b7me\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Eichk\u00e4tzchen springt, die Drossel singt,", "tokens": ["Eich\u00b7k\u00e4tz\u00b7chen", "springt", ",", "die", "Dros\u00b7sel", "singt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Von fern die Weidenfl\u00f6te klingt,", "tokens": ["Von", "fern", "die", "Wei\u00b7den\u00b7fl\u00f6\u00b7te", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Kuckuck stimmet an sein Lied,", "tokens": ["Der", "Ku\u00b7ckuck", "stim\u00b7met", "an", "sein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der b\u00f6se Winter h\u00f6rt's und flieht.", "tokens": ["Der", "b\u00f6\u00b7se", "Win\u00b7ter", "h\u00f6rt's", "und", "flieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach, lieber guter Gro\u00dfpapa,", "tokens": ["Ach", ",", "lie\u00b7ber", "gu\u00b7ter", "Gro\u00df\u00b7pa\u00b7pa", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach, w\u00e4r' doch schon der Fr\u00fchling da!", "tokens": ["Ach", ",", "w\u00e4r'", "doch", "schon", "der", "Fr\u00fch\u00b7ling", "da", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir wollen auch recht artig sein", "tokens": ["Wir", "wol\u00b7len", "auch", "recht", "ar\u00b7tig", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie liebe gute Kindelein.", "tokens": ["Wie", "lie\u00b7be", "gu\u00b7te", "Kin\u00b7de\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}