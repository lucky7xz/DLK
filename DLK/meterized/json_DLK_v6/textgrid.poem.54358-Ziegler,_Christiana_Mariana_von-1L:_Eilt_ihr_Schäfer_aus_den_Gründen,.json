{"textgrid.poem.54358": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eilt ihr Sch\u00e4fer aus den Gr\u00fcnden,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eilt ihr Sch\u00e4fer aus den Gr\u00fcnden,", "tokens": ["Eilt", "ihr", "Sch\u00e4\u00b7fer", "aus", "den", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eilt zu meinem Thyrsis hin,", "tokens": ["Eilt", "zu", "mei\u00b7nem", "Thyr\u00b7sis", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, so bald ihr ihn k\u00f6nnt finden,", "tokens": ["Und", ",", "so", "bald", "ihr", "ihn", "k\u00f6nnt", "fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ADV", "PPER", "PPER", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagt, da\u00df ich ihm g\u00fcnstig bin;", "tokens": ["Sagt", ",", "da\u00df", "ich", "ihm", "g\u00fcns\u00b7tig", "bin", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sagt, was er mir mitgenommen,", "tokens": ["Sagt", ",", "was", "er", "mir", "mit\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nennt die Freyheit und mein Herz;", "tokens": ["Nennt", "die", "Frey\u00b7heit", "und", "mein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sagt, er soll auch wieder kommen,", "tokens": ["Sagt", ",", "er", "soll", "auch", "wie\u00b7der", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn man treibt damit nicht Scherz.", "tokens": ["Denn", "man", "treibt", "da\u00b7mit", "nicht", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PAV", "PTKNEG", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach! wie stellt sein holdes Wesen", "tokens": ["Ach", "!", "wie", "stellt", "sein", "hol\u00b7des", "We\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich mir in Gedanken vor;", "tokens": ["Sich", "mir", "in", "Ge\u00b7dan\u00b7ken", "vor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Thyrsis bleibet auserlesen", "tokens": ["Thyr\u00b7sis", "blei\u00b7bet", "au\u00b7ser\u00b7le\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter unserm Sch\u00e4ferchor.", "tokens": ["Un\u00b7ter", "un\u00b7serm", "Sch\u00e4\u00b7fer\u00b7chor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sagt, ihr Nymphen an dem Strande,", "tokens": ["Sagt", ",", "ihr", "Nym\u00b7phen", "an", "dem", "Stran\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo die klare Pleisse flie\u00dft,", "tokens": ["Wo", "die", "kla\u00b7re", "Pleis\u00b7se", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ob man nicht an ihm befande,", "tokens": ["Ob", "man", "nicht", "an", "ihm", "be\u00b7fan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "APPR", "PPER", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Da\u00df er liebens w\u00fcrdig ist?", "tokens": ["Da\u00df", "er", "lie\u00b7bens", "w\u00fcr\u00b7dig", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich vergesse Fluhr und Heerde,", "tokens": ["Ich", "ver\u00b7ges\u00b7se", "Fluhr", "und", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ja, ich kenn mich selbsten nicht,", "tokens": ["Ja", ",", "ich", "kenn", "mich", "selbs\u00b7ten", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ich ganz bezaubert werde,", "tokens": ["Weil", "ich", "ganz", "be\u00b7zau\u00b7bert", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn man nur vom Thyrsis spricht.", "tokens": ["Wenn", "man", "nur", "vom", "Thyr\u00b7sis", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Will der Wolf das Lamm zerreissen,", "tokens": ["Will", "der", "Wolf", "das", "Lamm", "zer\u00b7reis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NE", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So belach ich den Verlust;", "tokens": ["So", "be\u00b7lach", "ich", "den", "Ver\u00b7lust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der ist leichte zu verbeissen,", "tokens": ["Der", "ist", "leich\u00b7te", "zu", "ver\u00b7beis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Thyrsis herrscht in meiner Brust.", "tokens": ["Thyr\u00b7sis", "herrscht", "in", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denk ich noch, geliebte Seele!", "tokens": ["Denk", "ich", "noch", ",", "ge\u00b7lieb\u00b7te", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An der Stunden schnelle Flucht,", "tokens": ["An", "der", "Stun\u00b7den", "schnel\u00b7le", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich sie zur\u00fccke zehle,", "tokens": ["Wenn", "ich", "sie", "zu\u00b7r\u00fc\u00b7cke", "zeh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mein Geist vergeblich sucht;", "tokens": ["Die", "mein", "Geist", "ver\u00b7geb\u00b7lich", "sucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denk ich auch der zarten Liebe,", "tokens": ["Denk", "ich", "auch", "der", "zar\u00b7ten", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die mein Thyrsis blicken lie\u00df,", "tokens": ["Die", "mein", "Thyr\u00b7sis", "bli\u00b7cken", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die fromm und reinen Triebe,", "tokens": ["Und", "die", "fromm", "und", "rei\u00b7nen", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da er mir mein Herz entri\u00df.", "tokens": ["Da", "er", "mir", "mein", "Herz", "ent\u00b7ri\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Seufzer speisen, sehnlich Klagen,", "tokens": ["Seuf\u00b7zer", "spei\u00b7sen", ",", "sehn\u00b7lich", "Kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Macht das Herze m\u00fcd und matt;", "tokens": ["Macht", "das", "Her\u00b7ze", "m\u00fcd", "und", "matt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch mu\u00df ich dir heimlich sagen,", "tokens": ["Doch", "mu\u00df", "ich", "dir", "heim\u00b7lich", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADJD", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Was mich l\u00e4ngst bek\u00fcmmert hat;", "tokens": ["Was", "mich", "l\u00e4ngst", "be\u00b7k\u00fcm\u00b7mert", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich girr, wie die Turteltaube,", "tokens": ["Ich", "girr", ",", "wie", "die", "Tur\u00b7tel\u00b7tau\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die durch \u00e4chzen Tag und Nacht,", "tokens": ["Die", "durch", "\u00e4ch\u00b7zen", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich nach ihres Gatten Raube,", "tokens": ["Sich", "nach", "ih\u00b7res", "Gat\u00b7ten", "Rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Selbst verzehrt und kraftlos macht.", "tokens": ["Selbst", "ver\u00b7zehrt", "und", "kraft\u00b7los", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sitz ich unter Tann und Buchen,", "tokens": ["Sitz", "ich", "un\u00b7ter", "Tann", "und", "Bu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00e4llt mir auch mein Thyrsis ein;", "tokens": ["F\u00e4llt", "mir", "auch", "mein", "Thyr\u00b7sis", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diesen will ich nur da suchen,", "tokens": ["Die\u00b7sen", "will", "ich", "nur", "da", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! frag ich, wo mag er seyn?", "tokens": ["Ach", "!", "frag", "ich", ",", "wo", "mag", "er", "seyn", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "PPER", "$,", "PWAV", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da lauf ich durch Fluhr und Auen,", "tokens": ["Da", "lauf", "ich", "durch", "Fluhr", "und", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ob mein Sch\u00e4fer sich versteckt?", "tokens": ["Ob", "mein", "Sch\u00e4\u00b7fer", "sich", "ver\u00b7steckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch, ich kann der Spuhr nicht trauen,", "tokens": ["Doch", ",", "ich", "kann", "der", "Spuhr", "nicht", "trau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weil mich alles Wild erschreckt.", "tokens": ["Weil", "mich", "al\u00b7les", "Wild", "er\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nichts kann mir mehr Freude stiften,", "tokens": ["Nichts", "kann", "mir", "mehr", "Freu\u00b7de", "stif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als wenn ich oft ganz allein,", "tokens": ["Als", "wenn", "ich", "oft", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf den buntbebl\u00fcmten Triften,", "tokens": ["Auf", "den", "bunt\u00b7be\u00b7bl\u00fcm\u00b7ten", "Trif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Darf mit meinen Heerden seyn.", "tokens": ["Darf", "mit", "mei\u00b7nen", "Heer\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fliegt die Taube mit dem Haufen,", "tokens": ["Fliegt", "die", "Tau\u00b7be", "mit", "dem", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So bleibt sie doch stets gepaart,", "tokens": ["So", "bleibt", "sie", "doch", "stets", "ge\u00b7paart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Keine wird vom Gatten laufen;", "tokens": ["Kei\u00b7ne", "wird", "vom", "Gat\u00b7ten", "lau\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das ist treuer Seelen Art!", "tokens": ["Das", "ist", "treu\u00b7er", "See\u00b7len", "Art", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Mir ist weiter nichts geblieben,", "tokens": ["Mir", "ist", "wei\u00b7ter", "nichts", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als dies, da\u00df ich sagen mu\u00df,", "tokens": ["Als", "dies", ",", "da\u00df", "ich", "sa\u00b7gen", "mu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig will ich Thyrsis lieben;", "tokens": ["E\u00b7wig", "will", "ich", "Thyr\u00b7sis", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig, ist mein fester Schlu\u00df.", "tokens": ["E\u00b7wig", ",", "ist", "mein", "fes\u00b7ter", "Schlu\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sch\u00f6ner Wechsel! s\u00fcsses Leiden!", "tokens": ["Sch\u00f6\u00b7ner", "Wech\u00b7sel", "!", "s\u00fcs\u00b7ses", "Lei\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Thyrsis! h\u00f6rst du, h\u00f6rst du nicht?", "tokens": ["Thyr\u00b7sis", "!", "h\u00f6rst", "du", ",", "h\u00f6rst", "du", "nicht", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "troc