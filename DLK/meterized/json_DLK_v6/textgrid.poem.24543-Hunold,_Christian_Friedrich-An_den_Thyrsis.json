{"textgrid.poem.24543": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An den Thyrsis", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach w\u00e4re ", "tokens": ["Ach", "w\u00e4\u00b7re"], "token_info": ["word", "word"], "pos": ["ITJ", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Er fand/ indem er gieng/ an einen Baum geschrieben:", "tokens": ["Er", "fand", "/", "in\u00b7dem", "er", "gieng", "/", "an", "ei\u00b7nen", "Baum", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "VVFIN", "$(", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit einem Sch\u00e4fer sitzt die ", "tokens": ["Mit", "ei\u00b7nem", "Sch\u00e4\u00b7fer", "sitzt", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Sonne gieng bereits gew\u00fcnschet in die See.", "tokens": ["Die", "Son\u00b7ne", "gieng", "be\u00b7reits", "ge\u00b7w\u00fcn\u00b7schet", "in", "die", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es muste Sie ein Busch vor ", "tokens": ["Es", "mus\u00b7te", "Sie", "ein", "Busch", "vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein dicker Dornen-Busch: er sahe sie zwar nicht/", "tokens": ["Ein", "di\u00b7cker", "Dor\u00b7nen\u00b7Busch", ":", "er", "sa\u00b7he", "sie", "zwar", "nicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "So oft der Sch\u00e4fer stiehlt/ so treibt er in die Hecken.", "tokens": ["So", "oft", "der", "Sch\u00e4\u00b7fer", "stiehlt", "/", "so", "treibt", "er", "in", "die", "He\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}