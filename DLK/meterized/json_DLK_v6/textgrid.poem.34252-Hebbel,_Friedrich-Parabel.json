{"textgrid.poem.34252": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Parabel", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst traf ich einen alten Mann", "tokens": ["J\u00fcngst", "traf", "ich", "ei\u00b7nen", "al\u00b7ten", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hub ihm vorzusingen an,", "tokens": ["Und", "hub", "ihm", "vor\u00b7zu\u00b7sin\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch an den Mienen des Gesichts", "tokens": ["Doch", "an", "den", "Mie\u00b7nen", "des", "Ge\u00b7sichts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bemerkt' ich bald, er h\u00f6re Nichts.", "tokens": ["Be\u00b7merkt'", "ich", "bald", ",", "er", "h\u00f6\u00b7re", "Nichts", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da dachte ich: der Greis ist taub,", "tokens": ["Da", "dach\u00b7te", "ich", ":", "der", "Greis", "ist", "taub", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drum wird dein Lied des Windes Raub,", "tokens": ["Drum", "wird", "dein", "Lied", "des", "Win\u00b7des", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So thu ihm denn, nicht durch den Mund,", "tokens": ["So", "thu", "ihm", "denn", ",", "nicht", "durch", "den", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch Zeichen Die\u00df und Jenes kund.", "tokens": ["Durch", "Zei\u00b7chen", "Die\u00df", "und", "Je\u00b7nes", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDS", "KON", "PDS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich that's, doch ward mir leider klar,", "tokens": ["Ich", "that's", ",", "doch", "ward", "mir", "lei\u00b7der", "klar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df er auch schon erblindet war,", "tokens": ["Da\u00df", "er", "auch", "schon", "er\u00b7blin\u00b7det", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Denn, wie der Frosch aus seinem Sumpf,", "tokens": ["Denn", ",", "wie", "der", "Frosch", "aus", "sei\u00b7nem", "Sumpf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Hervor glotzt, sah er dumpf und stumpf,", "tokens": ["Her\u00b7vor", "glotzt", ",", "sah", "er", "dumpf", "und", "stumpf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und ungest\u00f6rt in seiner Ruh',", "tokens": ["Und", "un\u00b7ge\u00b7st\u00f6rt", "in", "sei\u00b7ner", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der Sprache meiner Finger zu.", "tokens": ["Der", "Spra\u00b7che", "mei\u00b7ner", "Fin\u00b7ger", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ich rief: mit dem steht's schlimm genug,", "tokens": ["Ich", "rief", ":", "mit", "dem", "steht's", "schlimm", "ge\u00b7nug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "APPR", "ART", "ADJA", "ADJD", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.16": {"text": "Doch m\u00f6gt' ich ihm den letzten Zug", "tokens": ["Doch", "m\u00f6gt'", "ich", "ihm", "den", "letz\u00b7ten", "Zug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Noch g\u00f6nnen aus dem Lebensquell!", "tokens": ["Noch", "g\u00f6n\u00b7nen", "aus", "dem", "Le\u00b7bens\u00b7quell", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Da reicht' ich ihm die Rose schnell,", "tokens": ["Da", "reicht'", "ich", "ihm", "die", "Ro\u00b7se", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die ich f\u00fcr meine Braut gepfl\u00fcckt,", "tokens": ["Die", "ich", "f\u00fcr", "mei\u00b7ne", "Braut", "ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Allein auch das ist schlecht gegl\u00fcckt,", "tokens": ["Al\u00b7lein", "auch", "das", "ist", "schlecht", "ge\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDS", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.21": {"text": "Ihm schien der Duft nicht mehr zu sein,", "tokens": ["Ihm", "schien", "der", "Duft", "nicht", "mehr", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Wie einem Gartengott von Stein.", "tokens": ["Wie", "ei\u00b7nem", "Gar\u00b7ten\u00b7gott", "von", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Nunmehr verlor ich die Geduld,", "tokens": ["Nun\u00b7mehr", "ver\u00b7lor", "ich", "die", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Ich dacht' an meines M\u00e4dchens Huld,", "tokens": ["Ich", "dacht'", "an", "mei\u00b7nes", "M\u00e4d\u00b7chens", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Die mir so schm\u00e4hlig jetzt entging,", "tokens": ["Die", "mir", "so", "schm\u00e4h\u00b7lig", "jetzt", "ent\u00b7ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Da sie die Rose nicht empfing,", "tokens": ["Da", "sie", "die", "Ro\u00b7se", "nicht", "emp\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und jagte ihm im ersten Zorn", "tokens": ["Und", "jag\u00b7te", "ihm", "im", "ers\u00b7ten", "Zorn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "In's dicke Fell den scharfen Dorn;", "tokens": ["In's", "di\u00b7cke", "Fell", "den", "schar\u00b7fen", "Dorn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Doch bracht' auch die\u00df ihm wenig Noth,", "tokens": ["Doch", "bracht'", "auch", "die\u00df", "ihm", "we\u00b7nig", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PDS", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Er zuckte nicht, er \u2013 war wohl todt!", "tokens": ["Er", "zuck\u00b7te", "nicht", ",", "er", "\u2013", "war", "wohl", "todt", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "$(", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}