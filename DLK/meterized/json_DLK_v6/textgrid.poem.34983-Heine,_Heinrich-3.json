{"textgrid.poem.34983": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu M\u00fcnchen in der Schlo\u00dfkapell'", "tokens": ["Zu", "M\u00fcn\u00b7chen", "in", "der", "Schlo\u00df\u00b7ka\u00b7pell'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steht eine sch\u00f6ne Madonne;", "tokens": ["Steht", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Ma\u00b7don\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie tr\u00e4gt in den Armen ihr Jesulein,", "tokens": ["Sie", "tr\u00e4gt", "in", "den", "Ar\u00b7men", "ihr", "Je\u00b7su\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der Welt und des Himmels Wonne.", "tokens": ["Der", "Welt", "und", "des", "Him\u00b7mels", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Als Ludewig von Bayerland", "tokens": ["Als", "Lu\u00b7de\u00b7wig", "von", "Bay\u00b7er\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Heiligenbild erblicket,", "tokens": ["Das", "Hei\u00b7li\u00b7gen\u00b7bild", "er\u00b7bli\u00b7cket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da kniete er nieder andachtsvoll", "tokens": ["Da", "knie\u00b7te", "er", "nie\u00b7der", "an\u00b7dachts\u00b7voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und stotterte selig verz\u00fccket:", "tokens": ["Und", "stot\u00b7ter\u00b7te", "se\u00b7lig", "ver\u00b7z\u00fc\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbmaria, Himmelsk\u00f6nigin,", "tokens": ["\u00bb", "ma\u00b7ria", ",", "Him\u00b7mels\u00b7k\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["punct", "word", "punct", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Du F\u00fcrstin sonder M\u00e4ngel!", "tokens": ["Du", "F\u00fcrs\u00b7tin", "son\u00b7der", "M\u00e4n\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus Heil'gen besteht dein Hofgesind',", "tokens": ["Aus", "Heil'\u00b7gen", "be\u00b7steht", "dein", "Hof\u00b7ge\u00b7sind'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und deine Diener sind Engel.", "tokens": ["Und", "dei\u00b7ne", "Die\u00b7ner", "sind", "En\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NE", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Gefl\u00fcgelte Pagen warten dir auf,", "tokens": ["Ge\u00b7fl\u00fc\u00b7gel\u00b7te", "Pa\u00b7gen", "war\u00b7ten", "dir", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie flechten dir Blumen und B\u00e4nder", "tokens": ["Sie", "flech\u00b7ten", "dir", "Blu\u00b7men", "und", "B\u00e4n\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ins goldene Haar, sie tragen dir nach", "tokens": ["Ins", "gol\u00b7de\u00b7ne", "Haar", ",", "sie", "tra\u00b7gen", "dir", "nach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "APPR"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Schleppe deiner Gew\u00e4nder.", "tokens": ["Die", "Schlep\u00b7pe", "dei\u00b7ner", "Ge\u00b7w\u00e4n\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Maria, reiner Morgenstern,", "tokens": ["Ma\u00b7ria", ",", "rei\u00b7ner", "Mor\u00b7gens\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Du Lilie sonder Makel,", "tokens": ["Du", "Li\u00b7lie", "son\u00b7der", "Ma\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du hast so manches Wunder getan,", "tokens": ["Du", "hast", "so", "man\u00b7ches", "Wun\u00b7der", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "So manches fromme Mirakel \u2013", "tokens": ["So", "man\u00b7ches", "from\u00b7me", "Mi\u00b7ra\u00b7kel", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Oh, la\u00df aus deiner Gnaden Born", "tokens": ["Oh", ",", "la\u00df", "aus", "dei\u00b7ner", "Gna\u00b7den", "Born"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVIMP", "APPR", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch mir ein Tr\u00f6pflein gleiten!", "tokens": ["Auch", "mir", "ein", "Tr\u00f6pf\u00b7lein", "glei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gib mir ein Zeichen deiner Huld,", "tokens": ["Gib", "mir", "ein", "Zei\u00b7chen", "dei\u00b7ner", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der hochgebenedeiten!\u00ab \u2013", "tokens": ["Der", "hoch\u00b7ge\u00b7be\u00b7ne\u00b7dei\u00b7ten", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Muttergottes bewegt sich alsbald,", "tokens": ["Die", "Mut\u00b7ter\u00b7got\u00b7tes", "be\u00b7wegt", "sich", "als\u00b7bald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sichtbar bewegt sich ihr M\u00fcndchen,", "tokens": ["Sicht\u00b7bar", "be\u00b7wegt", "sich", "ihr", "M\u00fcnd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Sie sch\u00fcttelt ungeduldig das Haupt", "tokens": ["Sie", "sch\u00fct\u00b7telt", "un\u00b7ge\u00b7dul\u00b7dig", "das", "Haupt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und spricht zu ihrem Kindchen:", "tokens": ["Und", "spricht", "zu", "ih\u00b7rem", "Kind\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbes ist ein Gl\u00fcck, da\u00df ich auf dem Arm", "tokens": ["\u00bb", "es", "ist", "ein", "Gl\u00fcck", ",", "da\u00df", "ich", "auf", "dem", "Arm"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Dich trage und nicht mehr im Bauche,", "tokens": ["Dich", "tra\u00b7ge", "und", "nicht", "mehr", "im", "Bau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Gl\u00fcck, da\u00df ich vor dem Versehn", "tokens": ["Ein", "Gl\u00fcck", ",", "da\u00df", "ich", "vor", "dem", "Ver\u00b7sehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich nicht mehr zu f\u00fcrchten brauche.", "tokens": ["Mich", "nicht", "mehr", "zu", "f\u00fcrch\u00b7ten", "brau\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "H\u00e4tt ich in meiner Schwangerschaft", "tokens": ["H\u00e4tt", "ich", "in", "mei\u00b7ner", "Schwan\u00b7ger\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erblickt den h\u00e4\u00dflichen Toren,", "tokens": ["Er\u00b7blickt", "den", "h\u00e4\u00df\u00b7li\u00b7chen", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich h\u00e4tte gewi\u00df einen Wechselbalg", "tokens": ["Ich", "h\u00e4t\u00b7te", "ge\u00b7wi\u00df", "ei\u00b7nen", "Wech\u00b7sel\u00b7balg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Statt eines Gottes geboren.\u00ab", "tokens": ["Statt", "ei\u00b7nes", "Got\u00b7tes", "ge\u00b7bo\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$.", "$("], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}