{"textgrid.poem.67493": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An den Spiegel", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blankes, k\u00fcnstliches Glas, das, wenn es kothiges Silber", "tokens": ["Blan\u00b7kes", ",", "k\u00fcnst\u00b7li\u00b7ches", "Glas", ",", "das", ",", "wenn", "es", "ko\u00b7thi\u00b7ges", "Sil\u00b7ber"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PDS", "$,", "KOUS", "PPER", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Nur von hinten bestrich, alle Gestalten uns zeigt", "tokens": ["Nur", "von", "hin\u00b7ten", "be\u00b7strich", ",", "al\u00b7le", "Ge\u00b7stal\u00b7ten", "uns", "zeigt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADV", "ADJD", "$,", "PIAT", "NN", "PPER", "VVFIN"], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und schnell alle Gestalten ohn' alle Spuren hinwegnimmt,", "tokens": ["Und", "schnell", "al\u00b7le", "Ge\u00b7stal\u00b7ten", "ohn'", "al\u00b7le", "Spu\u00b7ren", "hin\u00b7weg\u00b7nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PIAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+---+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Du, des H\u00f6flinges Bild, Spiegel der artigen Welt!", "tokens": ["Du", ",", "des", "H\u00f6f\u00b7lin\u00b7ges", "Bild", ",", "Spie\u00b7gel", "der", "ar\u00b7ti\u00b7gen", "Welt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "$,", "NN", "ART", "ADJA", "NN", "$."], "meter": "---+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Dich zum Freunde begehrt jedwede buhlende Sch\u00f6ne;", "tokens": ["Dich", "zum", "Freun\u00b7de", "be\u00b7gehrt", "jed\u00b7we\u00b7de", "buh\u00b7len\u00b7de", "Sch\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVPP", "PIAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Weder haben noch sein mag ich Dein th\u00f6richtes Ding.", "tokens": ["We\u00b7der", "ha\u00b7ben", "noch", "sein", "mag", "ich", "Dein", "th\u00f6\u00b7rich\u00b7tes", "Ding", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VAINF", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}