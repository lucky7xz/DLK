{"textgrid.poem.62349": {"metadata": {"author": {"name": "Gessner, Salomon", "birth": "N.A.", "death": "N.A."}, "title": "An den Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1750, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was hilf es mir du Fr\u00fchling,", "tokens": ["Was", "hilf", "es", "mir", "du", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn du auf alle Wiesen", "tokens": ["Wenn", "du", "auf", "al\u00b7le", "Wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die sch\u00f6nsten Blumen streuest,", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "Blu\u00b7men", "streu\u00b7est", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wann ich auf deinen Blumen,", "tokens": ["Wann", "ich", "auf", "dei\u00b7nen", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht kann mit M\u00e4dgen tanzen?", "tokens": ["Nicht", "kann", "mit", "M\u00e4d\u00b7gen", "tan\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was helfen mir die Blumen,", "tokens": ["Was", "hel\u00b7fen", "mir", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wann mir kein muntres M\u00e4dgen,", "tokens": ["Wann", "mir", "kein", "mun\u00b7tres", "M\u00e4d\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Haupt mit Kr\u00e4ntzen schm\u00fcket?", "tokens": ["Das", "Haupt", "mit", "Kr\u00e4nt\u00b7zen", "schm\u00fc\u00b7ket", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Was hilfts mir wann die B\u00e4ume,", "tokens": ["Was", "hilfts", "mir", "wann", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Jetzt junge Schatten streuen,", "tokens": ["Jetzt", "jun\u00b7ge", "Schat\u00b7ten", "streu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Wenn ich nicht neben M\u00e4dgen", "tokens": ["Wenn", "ich", "nicht", "ne\u00b7ben", "M\u00e4d\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.12": {"text": "In deinen Schatten schlumre?", "tokens": ["In", "dei\u00b7nen", "Schat\u00b7ten", "schlum\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Was hilfts mir wann du Bl\u00fcthen,", "tokens": ["Was", "hilfts", "mir", "wann", "du", "Bl\u00fc\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PWAV", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Von bunten B\u00e4umen schneyest,", "tokens": ["Von", "bun\u00b7ten", "B\u00e4u\u00b7men", "schne\u00b7yest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Wenn du an meiner Seithe,", "tokens": ["Wenn", "du", "an", "mei\u00b7ner", "Sei\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Auf keinen Busen schneyest,", "tokens": ["Auf", "kei\u00b7nen", "Bu\u00b7sen", "schne\u00b7yest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Was schieren mich die Blumen,", "tokens": ["Was", "schie\u00b7ren", "mich", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Und Schatten und die Bl\u00fcthe?", "tokens": ["Und", "Schat\u00b7ten", "und", "die", "Bl\u00fc\u00b7the", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}