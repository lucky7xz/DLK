{"dta.poem.548": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sich  Polidor  auf einem lustigen Berge  \n  diverti ret.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "N\u00e4chst/ als nach Mittags-Zeit die helle Sonne blitzte/", "tokens": ["N\u00e4chst", "/", "als", "nach", "Mit\u00b7tags\u00b7Zeit", "die", "hel\u00b7le", "Son\u00b7ne", "blitz\u00b7te", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOKOM", "APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ihr beliebter Strahl erquickte jede Brust/", "tokens": ["Da", "ihr", "be\u00b7lieb\u00b7ter", "Strahl", "er\u00b7quick\u00b7te", "je\u00b7de", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und als der Ackersmann bey seinem Pfluge schwitzte/", "tokens": ["Und", "als", "der", "A\u00b7ckers\u00b7mann", "bey", "sei\u00b7nem", "Pflu\u00b7ge", "schwitz\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erkohr ihm ", "tokens": ["Er\u00b7kohr", "ihm"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Er sahe bald den Berg/ der dem ", "tokens": ["Er", "sa\u00b7he", "bald", "den", "Berg", "/", "der", "dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$(", "ART", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Anmuth hat sich da ins gr\u00fcne Gras ", "tokens": ["Die", "An\u00b7muth", "hat", "sich", "da", "ins", "gr\u00fc\u00b7ne", "Gras"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PRF", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Desselben gr\u00fcnes Haupt bis an die Wolcken striche/", "tokens": ["Des\u00b7sel\u00b7ben", "gr\u00fc\u00b7nes", "Haupt", "bis", "an", "die", "Wol\u00b7cken", "stri\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von dem die Westen Lufft den s\u00fcssen ", "tokens": ["Von", "dem", "die", "Wes\u00b7ten", "Lufft", "den", "s\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Hier streckte ", "tokens": ["Hier", "streck\u00b7te"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "In das bebl\u00fcmmte Gras/ wo ", "tokens": ["In", "das", "be\u00b7bl\u00fcmm\u00b7te", "Gras", "/", "wo"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "PWAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Das leichte Vogel-Heer sang wunder-sch\u00f6ne Lieder/", "tokens": ["Das", "leich\u00b7te", "Vo\u00b7gel\u00b7Heer", "sang", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Lie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und stellte singend vor ein ", "tokens": ["Und", "stell\u00b7te", "sin\u00b7gend", "vor", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Der edle ", "tokens": ["Der", "ed\u00b7le"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Jhm solte dieser Ort ein rechter Labsahl seyn.", "tokens": ["Jhm", "sol\u00b7te", "die\u00b7ser", "Ort", "ein", "rech\u00b7ter", "Lab\u00b7sahl", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDAT", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es \u00fcberfiel ihm auch ein angenehmer Schlummer", "tokens": ["Es", "\u00fc\u00b7berf\u00b7iel", "ihm", "auch", "ein", "an\u00b7ge\u00b7neh\u00b7mer", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Hier fehlet ", "tokens": ["Hier", "feh\u00b7let"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Die ihm des Himmels-Gunst damahls geniessen lie\u00df/", "tokens": ["Die", "ihm", "des", "Him\u00b7mels\u00b7Gunst", "da\u00b7mahls", "ge\u00b7nies\u00b7sen", "lie\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Drum dacht er diesen Ort mit Reimen auszuschm\u00fccken", "tokens": ["Drum", "dacht", "er", "die\u00b7sen", "Ort", "mit", "Rei\u00b7men", "aus\u00b7zu\u00b7schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PDAT", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Eh ihm die braune Nacht nach Hause gehen hie\u00df.", "tokens": ["Eh", "ihm", "die", "brau\u00b7ne", "Nacht", "nach", "Hau\u00b7se", "ge\u00b7hen", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Er sprach: Begl\u00fcckter Hayn es m\u00fcssen deine Aeste", "tokens": ["Er", "sprach", ":", "Be\u00b7gl\u00fcck\u00b7ter", "Hayn", "es", "m\u00fcs\u00b7sen", "dei\u00b7ne", "A\u00b7es\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADJA", "NN", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.22": {"text": "In jenem Tempel stehn wie hoher Cedern-Pracht.", "tokens": ["In", "je\u00b7nem", "Tem\u00b7pel", "stehn", "wie", "ho\u00b7her", "Ce\u00b7dern\u00b7Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wenn man den Herren lobt am heil\u2019gen Pfingsten Feste", "tokens": ["Wenn", "man", "den", "Her\u00b7ren", "lobt", "am", "heil'\u00b7gen", "Pfings\u00b7ten", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "An dem jedweder Geist mit frohen Munde lacht.", "tokens": ["An", "dem", "jed\u00b7we\u00b7der", "Geist", "mit", "fro\u00b7hen", "Mun\u00b7de", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So gr\u00fcne immer dar du Sammel-Platz der Freuden/", "tokens": ["So", "gr\u00fc\u00b7ne", "im\u00b7mer", "dar", "du", "Sam\u00b7mel\u00b7Platz", "der", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "PPER", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und deine Gegend sey ein stetes Lust-", "tokens": ["Und", "dei\u00b7ne", "Ge\u00b7gend", "sey", "ein", "ste\u00b7tes", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Dann kan ein treuer Hirt sein Schaaf in Frieden weiden/", "tokens": ["Dann", "kan", "ein", "treu\u00b7er", "Hirt", "sein", "Schaaf", "in", "Frie\u00b7den", "wei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ich aber ehre dich/ und scheide nun von hier.", "tokens": ["Ich", "a\u00b7ber", "eh\u00b7re", "dich", "/", "und", "schei\u00b7de", "nun", "von", "hier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$(", "KON", "VVFIN", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Dis war was ", "tokens": ["Dis", "war", "was"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "PWS"], "meter": "+-+", "measure": "trochaic.di"}, "line.30": {"text": "Die an dem Wege gleich zur rechten Seiten stand/", "tokens": ["Die", "an", "dem", "We\u00b7ge", "gleich", "zur", "rech\u00b7ten", "Sei\u00b7ten", "stand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Nachdem es nun geschehn/ ging er mit sanfften Schritte", "tokens": ["Nach\u00b7dem", "es", "nun", "ge\u00b7schehn", "/", "ging", "er", "mit", "sanff\u00b7ten", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$(", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Hin nach dem Flecken zu wo sich sein Freund befand.", "tokens": ["Hin", "nach", "dem", "Fle\u00b7cken", "zu", "wo", "sich", "sein", "Freund", "be\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "PWAV", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}