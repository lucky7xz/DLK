{"dta.poem.18597": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A uf den  N iem\u00e4gischen  F rie-  \n den.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Allein die Heiligkeit ein B\u00fcndni\u00df zu erhalten/", "tokens": ["Al\u00b7lein", "die", "Hei\u00b7lig\u00b7keit", "ein", "B\u00fcn\u00b7dni\u00df", "zu", "er\u00b7hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Eydes GOttes-Furcht und der Vergleiche Treu", "tokens": ["Des", "Ey\u00b7des", "Got\u00b7tes\u00b7Furcht", "und", "der", "Ver\u00b7glei\u00b7che", "Treu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "L\u00e4st sich bey H\u00e4uptern nur/ die l\u00f6blich seyn/ verwalten/", "tokens": ["L\u00e4st", "sich", "bey", "H\u00e4up\u00b7tern", "nur", "/", "die", "l\u00f6b\u00b7lich", "seyn", "/", "ver\u00b7wal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "ADV", "$(", "ART", "ADJD", "VAINF", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es bricht ihr festes Band gar keine Macht entzwey.", "tokens": ["Es", "bricht", "ihr", "fes\u00b7tes", "Band", "gar", "kei\u00b7ne", "Macht", "ent\u00b7zwey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Recht nach den Himmel zu/ so gehn des Adlers Blicke/", "tokens": ["Recht", "nach", "den", "Him\u00b7mel", "zu", "/", "so", "gehn", "des", "Ad\u00b7lers", "Bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKZU", "$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So sol auch bey den Bund ein Hertz gerichtet seyn:", "tokens": ["So", "sol", "auch", "bey", "den", "Bund", "ein", "Hertz", "ge\u00b7rich\u00b7tet", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hat aber Joabs-Mund ein Hertze voller T\u00fccke/", "tokens": ["Hat", "a\u00b7ber", "Joab\u00b7sMund", "ein", "Hert\u00b7ze", "vol\u00b7ler", "T\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "PTKVZ", "VVFIN", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ach ja so stellet sich schon unser Fallstrick ein:", "tokens": ["Ach", "ja", "so", "stel\u00b7let", "sich", "schon", "un\u00b7ser", "Fall\u00b7strick", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADV", "VVFIN", "PRF", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hier wil Gelegenheit nicht steten Glauben ehren/", "tokens": ["Hier", "wil", "Ge\u00b7le\u00b7gen\u00b7heit", "nicht", "ste\u00b7ten", "Glau\u00b7ben", "eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "PTKNEG", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nein/ wie das Wetter ist/ l\u00e4st sich der Hahn auch h\u00f6ren.", "tokens": ["Nein", "/", "wie", "das", "Wet\u00b7ter", "ist", "/", "l\u00e4st", "sich", "der", "Hahn", "auch", "h\u00f6\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "KOKOM", "ART", "NN", "VAFIN", "$(", "VVFIN", "PRF", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}