{"textgrid.poem.54249": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich lache, wenn man will von Lieb- und Sterben reden,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lache, wenn man will von Lieb- und Sterben reden,", "tokens": ["Ich", "la\u00b7che", ",", "wenn", "man", "will", "von", "Lie\u00b7b", "und", "Ster\u00b7ben", "re\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "VMFIN", "APPR", "TRUNC", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Drum k\u00f6mmt mir auch dein Blat recht frembd und artig f\u00fcr.", "tokens": ["Drum", "k\u00f6mmt", "mir", "auch", "dein", "Blat", "recht", "frembd", "und", "ar\u00b7tig", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Liebe wird so leicht wohl keinen Menschen t\u00f6den,", "tokens": ["Die", "Lie\u00b7be", "wird", "so", "leicht", "wohl", "kei\u00b7nen", "Men\u00b7schen", "t\u00f6\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es ist ein falscher Wahn, ach! Crito, glaub es mir.", "tokens": ["Es", "ist", "ein", "fal\u00b7scher", "Wahn", ",", "ach", "!", "Cri\u00b7to", ",", "glaub", "es", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ITJ", "$.", "NE", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein Kiel versteht die Kunst dem Frauen-Volck zu schmeicheln,", "tokens": ["Dein", "Kiel", "ver\u00b7steht", "die", "Kunst", "dem", "Frau\u00b7en\u00b7Volck", "zu", "schmei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er hat ein Probe-St\u00fcck bey mir auch abgelegt;", "tokens": ["Er", "hat", "ein", "Pro\u00b7be\u00b7St\u00fcck", "bey", "mir", "auch", "ab\u00b7ge\u00b7legt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch wir verstehen auch der M\u00e4nner s\u00fcsses Heucheln,", "tokens": ["Doch", "wir", "ver\u00b7ste\u00b7hen", "auch", "der", "M\u00e4n\u00b7ner", "s\u00fcs\u00b7ses", "Heu\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wir wissen lange schon, wie viel der Seiger schl\u00e4gt.", "tokens": ["Wir", "wis\u00b7sen", "lan\u00b7ge", "schon", ",", "wie", "viel", "der", "Sei\u00b7ger", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PWAV", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du setzest, schlauer Freund, mich in der Sch\u00f6nen Orden", "tokens": ["Du", "set\u00b7zest", ",", "schlau\u00b7er", "Freund", ",", "mich", "in", "der", "Sch\u00f6\u00b7nen", "Or\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und dichtest mir so viel von Treflichkeiten an,", "tokens": ["Und", "dich\u00b7test", "mir", "so", "viel", "von", "Tref\u00b7lich\u00b7kei\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df ich, indem ichs las, recht schamroth bin geworden,", "tokens": ["Da\u00df", "ich", ",", "in\u00b7dem", "ichs", "las", ",", "recht", "scham\u00b7roth", "bin", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PIS", "VVFIN", "$,", "ADV", "ADJD", "VAFIN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil ich die Schmeicheley nicht wohl vertragen kan.", "tokens": ["Weil", "ich", "die", "Schmei\u00b7che\u00b7ley", "nicht", "wohl", "ver\u00b7tra\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jedoch dadurch l\u00e4st sich mein Hertze gar nicht beugen,", "tokens": ["Je\u00b7doch", "da\u00b7durch", "l\u00e4st", "sich", "mein", "Hert\u00b7ze", "gar", "nicht", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VVFIN", "PRF", "PPOSAT", "VVFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Ich achte keine Lieb und auch kein Schmeicheln nicht,", "tokens": ["Ich", "ach\u00b7te", "kei\u00b7ne", "Lieb", "und", "auch", "kein", "Schmei\u00b7cheln", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "ADV", "PIAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Denn wisse, da\u00df durch dein so z\u00e4rtliches Bezeigen,", "tokens": ["Denn", "wis\u00b7se", ",", "da\u00df", "durch", "dein", "so", "z\u00e4rt\u00b7li\u00b7ches", "Be\u00b7zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "APPR", "PPOSAT", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der allergr\u00f6ste Tort mir in der That geschicht.", "tokens": ["Der", "al\u00b7ler\u00b7gr\u00f6s\u00b7te", "Tort", "mir", "in", "der", "That", "ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Klagen, welche mich l\u00e4st deine Feder lesen,", "tokens": ["Die", "Kla\u00b7gen", ",", "wel\u00b7che", "mich", "l\u00e4st", "dei\u00b7ne", "Fe\u00b7der", "le\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Als h\u00e4tt ich, Crito, dir die Fesseln angelegt,", "tokens": ["Als", "h\u00e4tt", "ich", ",", "Cri\u00b7to", ",", "dir", "die", "Fes\u00b7seln", "an\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "$,", "NE", "$,", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Seynd ein erdichtetes und falsch-verstelltes Wesen,", "tokens": ["Seynd", "ein", "er\u00b7dich\u00b7te\u00b7tes", "und", "falschver\u00b7stell\u00b7tes", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.20": {"text": "Das Morpheus dir vielleicht in Traum hat eingepr\u00e4gt.", "tokens": ["Das", "Mor\u00b7pheus", "dir", "viel\u00b7leicht", "in", "Traum", "hat", "ein\u00b7ge\u00b7pr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mein! Warum solt ich dich in Band und Fesseln schlagen,", "tokens": ["Mein", "!", "Wa\u00b7rum", "solt", "ich", "dich", "in", "Band", "und", "Fes\u00b7seln", "schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$.", "PWAV", "VMFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Du hast ja nichts gethan, so viel mir wissend ist;", "tokens": ["Du", "hast", "ja", "nichts", "ge\u00b7than", ",", "so", "viel", "mir", "wis\u00b7send", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "VVPP", "$,", "ADV", "ADV", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ein Misseth\u00e4ter mu\u00df nur solch Geschmeide tragen,", "tokens": ["Ein", "Mis\u00b7set\u00b7h\u00e4\u00b7ter", "mu\u00df", "nur", "solch", "Ge\u00b7schmei\u00b7de", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "In deren Rollen du doch nicht zu setzen bist.", "tokens": ["In", "de\u00b7ren", "Rol\u00b7len", "du", "doch", "nicht", "zu", "set\u00b7zen", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dein Leiden soll von mir und meiner Sch\u00f6nheit stammen,", "tokens": ["Dein", "Lei\u00b7den", "soll", "von", "mir", "und", "mei\u00b7ner", "Sch\u00f6n\u00b7heit", "stam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Du thust, als h\u00e4tt ich dich in Gluth und Brand gesetzt,", "tokens": ["Du", "thust", ",", "als", "h\u00e4tt", "ich", "dich", "in", "Gluth", "und", "Brand", "ge\u00b7setzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich wei\u00df kein Wort davon, weil niemals unsre Flammen,", "tokens": ["Ich", "wei\u00df", "kein", "Wort", "da\u00b7von", ",", "weil", "nie\u00b7mals", "uns\u00b7re", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PAV", "$,", "KOUS", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die der Camin und Heerd erregt, ein Hertz verletzt.", "tokens": ["Die", "der", "Ca\u00b7min", "und", "Heerd", "er\u00b7regt", ",", "ein", "Hertz", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "KON", "NN", "VVPP", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Es sehnet sich dein Geist das Urtheil anzuh\u00f6ren,", "tokens": ["Es", "seh\u00b7net", "sich", "dein", "Geist", "das", "Ur\u00b7theil", "an\u00b7zu\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Das meine Feder dir bey deinen Vortrag spricht;", "tokens": ["Das", "mei\u00b7ne", "Fe\u00b7der", "dir", "bey", "dei\u00b7nen", "Vor\u00b7trag", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Allein auch dieses will die Ohnmacht mir verwehren,", "tokens": ["Al\u00b7lein", "auch", "die\u00b7ses", "will", "die", "Ohn\u00b7macht", "mir", "ver\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDS", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ich bin, du weist es ja, kein Rechts-Gelahrter nicht.", "tokens": ["Ich", "bin", ",", "du", "weist", "es", "ja", ",", "kein", "Rechts\u00b7Ge\u00b7lahr\u00b7ter", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Doch kan ich dir so viel nur in Vertrauen sagen,", "tokens": ["Doch", "kan", "ich", "dir", "so", "viel", "nur", "in", "Ver\u00b7trau\u00b7en", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADV", "ADV", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Da\u00df die, so dieses schreibt, ein Feind vom Lieben heist,", "tokens": ["Da\u00df", "die", ",", "so", "die\u00b7ses", "schreibt", ",", "ein", "Feind", "vom", "Lie\u00b7ben", "heist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ADV", "PDS", "VVFIN", "$,", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Sie wird sich mit dem Gold der edlen Freyheit tragen,", "tokens": ["Sie", "wird", "sich", "mit", "dem", "Gold", "der", "ed\u00b7len", "Frey\u00b7heit", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So lange sich noch Blut in ihren Adern weist.", "tokens": ["So", "lan\u00b7ge", "sich", "noch", "Blut", "in", "ih\u00b7ren", "A\u00b7dern", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "ADV", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Man hat die Liebe mir recht furchtbar vorgerissen,", "tokens": ["Man", "hat", "die", "Lie\u00b7be", "mir", "recht", "furcht\u00b7bar", "vor\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Cupido solte blind, bedenck es Crito, seyn;", "tokens": ["Cu\u00b7pi\u00b7do", "sol\u00b7te", "blind", ",", "be\u00b7denck", "es", "Cri\u00b7to", ",", "seyn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VMFIN", "ADJD", "$,", "VVFIN", "PPER", "NE", "$,", "VAINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.39": {"text": "Wir k\u00f6nten, liebten wir, nicht unser Ungl\u00fcck wissen,", "tokens": ["Wir", "k\u00f6n\u00b7ten", ",", "lieb\u00b7ten", "wir", ",", "nicht", "un\u00b7ser", "Un\u00b7gl\u00fcck", "wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "PPER", "$,", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wie offtermahls bricht nicht ein Blinder Hals und Bein.", "tokens": ["Wie", "off\u00b7ter\u00b7mahls", "bricht", "nicht", "ein", "Blin\u00b7der", "Hals", "und", "Bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.41": {"text": "Nein, solchem Unfall mu\u00df ein kluger Mensch entgehen,", "tokens": ["Nein", ",", "sol\u00b7chem", "Un\u00b7fall", "mu\u00df", "ein", "klu\u00b7ger", "Mensch", "ent\u00b7ge\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIAT", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Entreisse ja dein Hertz des Amors Netz und Strick;", "tokens": ["E\u00b7ntreis\u00b7se", "ja", "dein", "Hertz", "des", "A\u00b7mors", "Netz", "und", "Strick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "ART", "NE", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und da\u00df du besser kanst auf Hut und Wache stehen,", "tokens": ["Und", "da\u00df", "du", "bes\u00b7ser", "kanst", "auf", "Hut", "und", "Wa\u00b7che", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VMFIN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "So schick ich selbiges dir wiederum zur\u00fcck.", "tokens": ["So", "schick", "ich", "sel\u00b7bi\u00b7ges", "dir", "wie\u00b7de\u00b7rum", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": 