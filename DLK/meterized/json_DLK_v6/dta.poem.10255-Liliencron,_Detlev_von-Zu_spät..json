{"dta.poem.10255": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Zu sp\u00e4t.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kann das Wort nicht vergessen,             ", "tokens": ["Ich", "kann", "das", "Wort", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es klang so traurig und schwer.", "tokens": ["Es", "klang", "so", "trau\u00b7rig", "und", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dein Stimmlein h\u00f6r\u2019 ich schluchzen:", "tokens": ["Dein", "Stimm\u00b7lein", "h\u00f6r'", "ich", "schluch\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich wei\u00df, du liebst mich nicht mehr.", "tokens": ["Ich", "wei\u00df", ",", "du", "liebst", "mich", "nicht", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Der Abend sank auf die Felder,", "tokens": ["Der", "A\u00b7bend", "sank", "auf", "die", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vom Tage nur noch ein Rest.", "tokens": ["Vom", "Ta\u00b7ge", "nur", "noch", "ein", "Rest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die letzten Kr\u00e4hen flogen", "tokens": ["Die", "letz\u00b7ten", "Kr\u00e4\u00b7hen", "flo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach fernen W\u00e4ldern zu Nest.", "tokens": ["Nach", "fer\u00b7nen", "W\u00e4l\u00b7dern", "zu", "Nest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NE", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Nun sind wir weit geschieden,", "tokens": ["Nun", "sind", "wir", "weit", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf Nimmerwiederkehr.", "tokens": ["Auf", "Nim\u00b7mer\u00b7wie\u00b7der\u00b7kehr", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich kann das Wort nicht vergessen:", "tokens": ["Ich", "kann", "das", "Wort", "nicht", "ver\u00b7ges\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich wei\u00df, du liebst mich nicht mehr.", "tokens": ["Ich", "wei\u00df", ",", "du", "liebst", "mich", "nicht", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}