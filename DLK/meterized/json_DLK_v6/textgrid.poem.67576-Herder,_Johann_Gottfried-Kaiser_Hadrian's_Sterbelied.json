{"textgrid.poem.67576": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Kaiser Hadrian's Sterbelied", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, Seelchen, armes Seelchen!", "tokens": ["Ach", ",", "Seel\u00b7chen", ",", "ar\u00b7mes", "Seel\u00b7chen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So lange meine gute Freundin,", "tokens": ["So", "lan\u00b7ge", "mei\u00b7ne", "gu\u00b7te", "Freun\u00b7din", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So lange Leibes gute Wirthin,", "tokens": ["So", "lan\u00b7ge", "Lei\u00b7bes", "gu\u00b7te", "Wirt\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wohin wirst Du nun wandern?", "tokens": ["Wo\u00b7hin", "wirst", "Du", "nun", "wan\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf welcher dunklen Stra\u00dfe! \u2013", "tokens": ["Auf", "wel\u00b7cher", "dunk\u00b7len", "Stra\u00b7\u00dfe", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PWAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nackt und bleich und bla\u00df und zitternd! \u2013", "tokens": ["Nackt", "und", "bleich", "und", "bla\u00df", "und", "zit\u00b7ternd", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "KON", "ADJD", "KON", "ADJD", "KON", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wirst nimmermehr nun, wie Du pflegtest,", "tokens": ["Wirst", "nim\u00b7mer\u00b7mehr", "nun", ",", "wie", "Du", "pfleg\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So artig mit mir t\u00e4ndeln!", "tokens": ["So", "ar\u00b7tig", "mit", "mir", "t\u00e4n\u00b7deln", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}