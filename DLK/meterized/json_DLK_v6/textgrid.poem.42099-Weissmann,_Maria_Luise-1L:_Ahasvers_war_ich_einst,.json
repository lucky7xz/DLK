{"textgrid.poem.42099": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ahasvers war ich einst,", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ahasvers war ich einst,", "tokens": ["A\u00b7has\u00b7vers", "war", "ich", "einst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Tannh\u00e4users auch.", "tokens": ["Tann\u00b7h\u00e4u\u00b7sers", "auch", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich gr\u00fcnte und ich schlug", "tokens": ["Ich", "gr\u00fcn\u00b7te", "und", "ich", "schlug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wurzeln einmal und wuchs und wurde Strauch.", "tokens": ["Wur\u00b7zeln", "ein\u00b7mal", "und", "wuchs", "und", "wur\u00b7de", "Strauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "VVFIN", "KON", "VAFIN", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Nun ist die Welt bes\u00e4t", "tokens": ["Nun", "ist", "die", "Welt", "be\u00b7s\u00e4t"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit meiner Brut:", "tokens": ["Mit", "mei\u00b7ner", "Brut", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein jeder Sch\u00f6\u00dfling trieb", "tokens": ["Ein", "je\u00b7der", "Sch\u00f6\u00df\u00b7ling", "trieb"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und trieb die alte ungestillte Wut.", "tokens": ["Und", "trieb", "die", "al\u00b7te", "un\u00b7ge\u00b7still\u00b7te", "Wut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Trieb, die sich treibt, die Kraft.", "tokens": ["Trieb", ",", "die", "sich", "treibt", ",", "die", "Kraft", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "VVFIN", "$,", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wen sie befiel,", "tokens": ["Wen", "sie", "be\u00b7fiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wer mich ergriff, den trifft", "tokens": ["Wer", "mich", "er\u00b7griff", ",", "den", "trifft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ziel nur als Weg zu einem neuen Ziel.", "tokens": ["Ziel", "nur", "als", "Weg", "zu", "ei\u00b7nem", "neu\u00b7en", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOUS", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wer an mir ging, den kann", "tokens": ["Wer", "an", "mir", "ging", ",", "den", "kann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "$,", "ART", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Kein Haus verwahrn,", "tokens": ["Kein", "Haus", "ver\u00b7wahrn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Zu viele Stra\u00dfen rings,", "tokens": ["Zu", "vie\u00b7le", "Stra\u00b7\u00dfen", "rings", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die ungekannt ins Unbegangne fahrn,", "tokens": ["Die", "un\u00b7ge\u00b7kannt", "ins", "Un\u00b7be\u00b7gang\u00b7ne", "fahrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Zu wenig Schnitter f\u00fcr", "tokens": ["Zu", "we\u00b7nig", "Schnit\u00b7ter", "f\u00fcr"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die gro\u00dfe Mahd,", "tokens": ["Die", "gro\u00b7\u00dfe", "Mahd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nach allen H\u00e4nden dr\u00e4ngt", "tokens": ["Nach", "al\u00b7len", "H\u00e4n\u00b7den", "dr\u00e4ngt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und dr\u00e4ngt sich gierig die gereifte Saat:", "tokens": ["Und", "dr\u00e4ngt", "sich", "gie\u00b7rig", "die", "ge\u00b7reif\u00b7te", "Saat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wegs\u00fcchtige, die aus mir", "tokens": ["Weg\u00b7s\u00fcch\u00b7ti\u00b7ge", ",", "die", "aus", "mir"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Geboren sind", "tokens": ["Ge\u00b7bo\u00b7ren", "sind"], "token_info": ["word", "word"], "pos": ["VVPP", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und noch gefangen stehn", "tokens": ["Und", "noch", "ge\u00b7fan\u00b7gen", "stehn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und seufzend wehn unter dem weiten Wind...", "tokens": ["Und", "seuf\u00b7zend", "wehn", "un\u00b7ter", "dem", "wei\u00b7ten", "Wind", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Mi\u00dfh\u00f6r die Sehnsucht nicht,", "tokens": ["Mi\u00df\u00b7h\u00f6r", "die", "Sehn\u00b7sucht", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die um dich brennt,", "tokens": ["Die", "um", "dich", "brennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mensch, fa\u00df ein Ding und geh", "tokens": ["Mensch", ",", "fa\u00df", "ein", "Ding", "und", "geh"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ihm nach und stills und f\u00fchrs zu seinem End.", "tokens": ["Ihm", "nach", "und", "stills", "und", "f\u00fchrs", "zu", "sei\u00b7nem", "End", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "KON", "ADV", "KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}