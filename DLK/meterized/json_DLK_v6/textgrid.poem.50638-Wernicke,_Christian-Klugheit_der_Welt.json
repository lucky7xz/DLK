{"textgrid.poem.50638": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Klugheit der Welt", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast den Seneca und Plato wohl gelesen;", "tokens": ["Du", "hast", "den", "Se\u00b7ne\u00b7ca", "und", "Pla\u00b7to", "wohl", "ge\u00b7le\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NE", "KON", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein du kennest nicht der Welt verkehrtes Wesen.", "tokens": ["Al\u00b7lein", "du", "ken\u00b7nest", "nicht", "der", "Welt", "ver\u00b7kehr\u00b7tes", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Menalcas, glaub', es steckt viel Weisheit im Betrug,", "tokens": ["Me\u00b7nal\u00b7cas", ",", "glaub'", ",", "es", "steckt", "viel", "Weis\u00b7heit", "im", "Be\u00b7trug", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "PPER", "VVFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Viel Wissenschaft in den Gebehrden.", "tokens": ["Viel", "Wis\u00b7sen\u00b7schaft", "in", "den", "Ge\u00b7behr\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du hast Verstand, doch nicht Geschicklichkeit genug,", "tokens": ["Du", "hast", "Ver\u00b7stand", ",", "doch", "nicht", "Ge\u00b7schick\u00b7lich\u00b7keit", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "ADV", "PTKNEG", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auch von den Narren selbst f\u00fcr klug gesch\u00e4tzt zu werden.", "tokens": ["Auch", "von", "den", "Nar\u00b7ren", "selbst", "f\u00fcr", "klug", "ge\u00b7sch\u00e4tzt", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "APPR", "ADJD", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}