{"textgrid.poem.49324": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Neaera Mann war \u00fcber Land", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Neaera Mann war \u00fcber Land", "tokens": ["Nea\u00b7e\u00b7ra", "Mann", "war", "\u00fc\u00b7ber", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von seinem Herren ausgesandt,", "tokens": ["Von", "sei\u00b7nem", "Her\u00b7ren", "aus\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df er ein' angelegne Sach'", "tokens": ["Da\u00df", "er", "ein'", "an\u00b7ge\u00b7leg\u00b7ne", "Sach'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An fremden Orten richten mag,", "tokens": ["An", "frem\u00b7den", "Or\u00b7ten", "rich\u00b7ten", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wann er die gerichtet aus,", "tokens": ["Und", "wann", "er", "die", "ge\u00b7rich\u00b7tet", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Alsdenn bald wiedrum k\u00e4m' zu Haus", "tokens": ["Als\u00b7denn", "bald", "wie\u00b7drum", "k\u00e4m'", "zu", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und meld es seinem F\u00fcrsten an,", "tokens": ["Und", "meld", "es", "sei\u00b7nem", "F\u00fcrs\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was er gelassen und gethan.", "tokens": ["Was", "er", "ge\u00b7las\u00b7sen", "und", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Indem erh\u00f6rt man ein Ger\u00fccht',", "tokens": ["In\u00b7dem", "er\u00b7h\u00f6rt", "man", "ein", "Ge\u00b7r\u00fccht'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Als ob er nunmehr lebe nicht;", "tokens": ["Als", "ob", "er", "nun\u00b7mehr", "le\u00b7be", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Er sei ohnl\u00e4ngst bei finstrer Nacht", "tokens": ["Er", "sei", "ohn\u00b7l\u00e4ngst", "bei", "finst\u00b7rer", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Im Wald erw\u00fcrgt und umgebracht", "tokens": ["Im", "Wald", "er\u00b7w\u00fcrgt", "und", "um\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und l\u00e4g' jetzt mitten in dem Pfad,", "tokens": ["Und", "l\u00e4g'", "jetzt", "mit\u00b7ten", "in", "dem", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Zw\u00f6lf ganzer Meilen von der Stadt,", "tokens": ["Zw\u00f6lf", "gan\u00b7zer", "Mei\u00b7len", "von", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "In Regen, Hitz' und Sonnenschein;", "tokens": ["In", "Re\u00b7gen", ",", "Hitz'", "und", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Man solt' ihn schleunigst holen ein.", "tokens": ["Man", "solt'", "ihn", "schleu\u00b7nigst", "ho\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ein ander sagt, es w\u00e4r nicht wahr;", "tokens": ["Ein", "an\u00b7der", "sagt", ",", "es", "w\u00e4r", "nicht", "wahr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Herr Appius lebt' ohn Gefahr,", "tokens": ["Herr", "Ap\u00b7pius", "lebt'", "ohn", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Ja sei auch noch verletzet nicht;", "tokens": ["Ja", "sei", "auch", "noch", "ver\u00b7let\u00b7zet", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VAFIN", "ADV", "ADV", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Es sei nur L\u00fcgen und Gedicht,", "tokens": ["Es", "sei", "nur", "L\u00fc\u00b7gen", "und", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Was man von dieser Ungl\u00fccksnacht", "tokens": ["Was", "man", "von", "die\u00b7ser", "Un\u00b7gl\u00fccks\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Aus Scherz hab' auf die Bahn gebracht.", "tokens": ["Aus", "Scherz", "hab'", "auf", "die", "Bahn", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "So rief der ein', es w\u00e4r geschehn,", "tokens": ["So", "rief", "der", "ein'", ",", "es", "w\u00e4r", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIS", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Ein ander wolt' es nicht gestehn;", "tokens": ["Ein", "an\u00b7der", "wolt'", "es", "nicht", "ge\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VMFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Doch war der Immenschw\u00e4tzer Zahl", "tokens": ["Doch", "war", "der", "Im\u00b7men\u00b7schw\u00e4t\u00b7zer", "Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Viel gr\u00f6\u00dfer noch, die allzumal", "tokens": ["Viel", "gr\u00f6\u00b7\u00dfer", "noch", ",", "die", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Bezeugten fast mit Hand und Mund,", "tokens": ["Be\u00b7zeug\u00b7ten", "fast", "mit", "Hand", "und", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Da\u00df Appius noch w\u00e4r' gesund", "tokens": ["Da\u00df", "Ap\u00b7pius", "noch", "w\u00e4r'", "ge\u00b7sund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "VAFIN", "ADJD"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.29": {"text": "Und w\u00fcste gar von keiner Pein;", "tokens": ["Und", "w\u00fcs\u00b7te", "gar", "von", "kei\u00b7ner", "Pein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Er w\u00fcrde bald zugegen sein.", "tokens": ["Er", "w\u00fcr\u00b7de", "bald", "zu\u00b7ge\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Als die\u00df sein junges Weib vernahm", "tokens": ["Als", "die\u00df", "sein", "jun\u00b7ges", "Weib", "ver\u00b7nahm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Und auf den Markt gelaufen kam,", "tokens": ["Und", "auf", "den", "Markt", "ge\u00b7lau\u00b7fen", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Hilf Gott, wie rief das liebe Kind!", "tokens": ["Hilf", "Gott", ",", "wie", "rief", "das", "lie\u00b7be", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Sie warf die Hauben gar geschwind", "tokens": ["Sie", "warf", "die", "Hau\u00b7ben", "gar", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Vom Kopf herab und ri\u00df ihr Haar,", "tokens": ["Vom", "Kopf", "her\u00b7ab", "und", "ri\u00df", "ihr", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "So da\u00df es zum Erbarmen war.", "tokens": ["So", "da\u00df", "es", "zum", "Er\u00b7bar\u00b7men", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Sie schrie: \u00bbAch weh, ich armes Weib!", "tokens": ["Sie", "schrie", ":", "\u00bb", "Ach", "weh", ",", "ich", "ar\u00b7mes", "Weib", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ITJ", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Mein Schatz, wo find' ich deinen Leib?", "tokens": ["Mein", "Schatz", ",", "wo", "find'", "ich", "dei\u00b7nen", "Leib", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "O Appius, mein liebster Mann,", "tokens": ["O", "Ap\u00b7pius", ",", "mein", "liebs\u00b7ter", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Ach weh', ach weh', was fang' ich an?", "tokens": ["Ach", "weh'", ",", "ach", "weh'", ",", "was", "fang'", "ich", "an", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "ADV", "PTKVZ", "$,", "PWS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Sag' an, mein Schatz, wie weit von hier", "tokens": ["Sag'", "an", ",", "mein", "Schatz", ",", "wie", "weit", "von", "hier"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Hat dich der Tod entzogen mir?", "tokens": ["Hat", "dich", "der", "Tod", "ent\u00b7zo\u00b7gen", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Sag' an, bekenn' es ohne Scheu,", "tokens": ["Sag'", "an", ",", "be\u00b7kenn'", "es", "oh\u00b7ne", "Scheu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Ob auch noch Leben in dir sei;", "tokens": ["Ob", "auch", "noch", "Le\u00b7ben", "in", "dir", "sei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "NN", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Wo nicht, so hol' mich Arme nach.", "tokens": ["Wo", "nicht", ",", "so", "hol'", "mich", "Ar\u00b7me", "nach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Wie kan ich solches Ungemach", "tokens": ["Wie", "kan", "ich", "sol\u00b7ches", "Un\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Erdulden, o mein Appius,", "tokens": ["E\u00b7rdul\u00b7den", ",", "o", "mein", "Ap\u00b7pius", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "PPOSAT", "NE", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.48": {"text": "Da\u00df ohne dich ich leben mu\u00df?", "tokens": ["Da\u00df", "oh\u00b7ne", "dich", "ich", "le\u00b7ben", "mu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Ein finstres Grab ist mein Begehr.", "tokens": ["Ein", "finst\u00b7res", "Grab", "ist", "mein", "Be\u00b7gehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "O s\u00fc\u00dfer Tod, kom' eiligst her", "tokens": ["O", "s\u00fc\u00b7\u00dfer", "Tod", ",", "kom'", "ei\u00b7ligst", "her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN", "ADV", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Und w\u00fcrge mich von Stunden an,", "tokens": ["Und", "w\u00fcr\u00b7ge", "mich", "von", "Stun\u00b7den", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Weil ich nicht l\u00e4nger leben kan.\u00ab", "tokens": ["Weil", "ich", "nicht", "l\u00e4n\u00b7ger", "le\u00b7ben", "kan", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "So schrie das Weib; ihr Leid war gro\u00df;", "tokens": ["So", "schrie", "das", "Weib", ";", "ihr", "Leid", "war", "gro\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Manch tausend Thr\u00e4nen sie vergo\u00df,", "tokens": ["Manch", "tau\u00b7send", "Thr\u00e4\u00b7nen", "sie", "ver\u00b7go\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "CARD", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Ja wrang die H\u00e4nd' und schlug die Brust,", "tokens": ["Ja", "wrang", "die", "H\u00e4nd'", "und", "schlug", "die", "Brust", ","], "token_in