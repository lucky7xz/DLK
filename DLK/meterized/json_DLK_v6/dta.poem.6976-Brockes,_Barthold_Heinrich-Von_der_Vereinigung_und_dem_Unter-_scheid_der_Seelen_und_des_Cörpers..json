{"dta.poem.6976": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was sich in ihrem Band f\u00fcr eine Gleichheit weist,", "tokens": ["Was", "sich", "in", "ih\u00b7rem", "Band", "f\u00fcr", "ei\u00b7ne", "Gleich\u00b7heit", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die so verschiedne Dinge binden,", "tokens": ["Die", "so", "ver\u00b7schied\u00b7ne", "Din\u00b7ge", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bem\u00fchen wir uns zu ergr\u00fcnden.", "tokens": ["Be\u00b7m\u00fc\u00b7hen", "wir", "uns", "zu", "er\u00b7gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was finden wir? Die Seel\u2019 erregt und \u00fcberleget,", "tokens": ["Was", "fin\u00b7den", "wir", "?", "Die", "Seel'", "er\u00b7regt", "und", "\u00fc\u00b7ber\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "ART", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der C\u00f6rper wird beweget.", "tokens": ["Der", "C\u00f6r\u00b7per", "wird", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Man mu\u00df demnach derselben Einigkeit", "tokens": ["Man", "mu\u00df", "dem\u00b7nach", "der\u00b7sel\u00b7ben", "Ei\u00b7nig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PAV", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Auf diese beyde Gr\u00fcnde sencken:", "tokens": ["Auf", "die\u00b7se", "bey\u00b7de", "Gr\u00fcn\u00b7de", "sen\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bewegt kan dieser seyn, und jene kan gedencken,", "tokens": ["Be\u00b7wegt", "kan", "die\u00b7ser", "seyn", ",", "und", "je\u00b7ne", "kan", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VMFIN", "PDAT", "VAINF", "$,", "KON", "PDS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nach der Verbindungen Beschaffenheit.", "tokens": ["Nach", "der", "Ver\u00b7bin\u00b7dun\u00b7gen", "Be\u00b7schaf\u00b7fen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Es nimmt die Seele Theil an ihres C\u00f6rpers Stand,", "tokens": ["Es", "nimmt", "die", "See\u00b7le", "Theil", "an", "ih\u00b7res", "C\u00f6r\u00b7pers", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Durch den die Sinnen sich besch\u00e4fft\u2019gen und bereiten,", "tokens": ["Durch", "den", "die", "Sin\u00b7nen", "sich", "be\u00b7sch\u00e4fft'\u00b7gen", "und", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PRF", "VVPP", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der C\u00f6rper hat auf seiner Seiten", "tokens": ["Der", "C\u00f6r\u00b7per", "hat", "auf", "sei\u00b7ner", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Bewegungen, die unserm Geist bekannt,", "tokens": ["Be\u00b7we\u00b7gun\u00b7gen", ",", "die", "un\u00b7serm", "Geist", "be\u00b7kannt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.14": {"text": "Durch die er Schmertz und Lust empfindet,", "tokens": ["Durch", "die", "er", "Schmertz", "und", "Lust", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Weil, und damit, ihr Band sich desto fester bindet.", "tokens": ["Weil", ",", "und", "da\u00b7mit", ",", "ihr", "Band", "sich", "des\u00b7to", "fes\u00b7ter", "bin\u00b7det", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KON", "PAV", "$,", "PPOSAT", "NN", "PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nach der Gedancken Sinn mu\u00df sich der Leib bewegen,", "tokens": ["Nach", "der", "Ge\u00b7dan\u00b7cken", "Sinn", "mu\u00df", "sich", "der", "Leib", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Durch ihres C\u00f6rpers Stand kan sich der Geist erregen:", "tokens": ["Durch", "ih\u00b7res", "C\u00f6r\u00b7pers", "Stand", "kan", "sich", "der", "Geist", "er\u00b7re\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da sie nun dieser Macht sich unter sich bedienen,", "tokens": ["Da", "sie", "nun", "die\u00b7ser", "Macht", "sich", "un\u00b7ter", "sich", "be\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PDAT", "NN", "PRF", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So bleibt der Einigkeit Gesetze zwischen ihnen.", "tokens": ["So", "bleibt", "der", "Ei\u00b7nig\u00b7keit", "Ge\u00b7set\u00b7ze", "zwi\u00b7schen", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die Seele, wie gesagt, darf nur versp\u00fcren", "tokens": ["Die", "See\u00b7le", ",", "wie", "ge\u00b7sagt", ",", "darf", "nur", "ver\u00b7sp\u00fc\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "VVPP", "$,", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Die Aendrungen, die sich in ihrem C\u00f6rper r\u00fchren,", "tokens": ["Die", "A\u00b7en\u00b7drun\u00b7gen", ",", "die", "sich", "in", "ih\u00b7rem", "C\u00f6r\u00b7per", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Und unser C\u00f6rper nimmt nur an", "tokens": ["Und", "un\u00b7ser", "C\u00f6r\u00b7per", "nimmt", "nur", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Den Eindruck, den der Geist erkennen kan.", "tokens": ["Den", "Ein\u00b7druck", ",", "den", "der", "Geist", "er\u00b7ken\u00b7nen", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.24": {"text": "So stimmen \u00fcberein, das Dencken und Bewegen;", "tokens": ["So", "stim\u00b7men", "\u00fc\u00b7be\u00b7re\u00b7in", ",", "das", "Den\u00b7cken", "und", "Be\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Die Seele kan den Leib, der Leib die Seel erregen.", "tokens": ["Die", "See\u00b7le", "kan", "den", "Leib", ",", "der", "Leib", "die", "Seel", "er\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und also binden sie sich mit einander fest,", "tokens": ["Und", "al\u00b7so", "bin\u00b7den", "sie", "sich", "mit", "ein\u00b7an\u00b7der", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PRF", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ohn da\u00df im minsten sich Verwirrung sp\u00fcren l\u00e4sst,", "tokens": ["Ohn", "da\u00df", "im", "mins\u00b7ten", "sich", "Ver\u00b7wir\u00b7rung", "sp\u00fc\u00b7ren", "l\u00e4sst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "APPRART", "ADJA", "PRF", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und was sonst von Natur gantz unterschiedentlich,", "tokens": ["Und", "was", "sonst", "von", "Na\u00b7tur", "gantz", "un\u00b7ter\u00b7schie\u00b7dent\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Vereint im Menschen sich,", "tokens": ["Ver\u00b7eint", "im", "Men\u00b7schen", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Durch Handlungen, die stets gemeinsam seyn:", "tokens": ["Durch", "Hand\u00b7lun\u00b7gen", ",", "die", "stets", "ge\u00b7mein\u00b7sam", "seyn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "ADJD", "VAINF", "$."], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Aus beyden wird dadurch ein vollenkommen Ein.", "tokens": ["Aus", "bey\u00b7den", "wird", "da\u00b7durch", "ein", "vol\u00b7len\u00b7kom\u00b7men", "Ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "VAFIN", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}