{"dta.poem.7992": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die sterbenden Helden .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Begonnen hab\u2019 ich einen Festgesang", "tokens": ["Be\u00b7gon\u00b7nen", "hab'", "ich", "ei\u00b7nen", "Fest\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zum Saitenklang,", "tokens": ["Zum", "Sai\u00b7ten\u00b7klang", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Von K\u00f6nigen und Helden grauer Zeit", "tokens": ["Von", "K\u00f6\u00b7ni\u00b7gen", "und", "Hel\u00b7den", "grau\u00b7er", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In Lieb\u2019 und Streit.", "tokens": ["In", "Lieb'", "und", "Streit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Verlassen h\u00e4ngt die Harfe nun, und bang", "tokens": ["Ver\u00b7las\u00b7sen", "h\u00e4ngt", "die", "Har\u00b7fe", "nun", ",", "und", "bang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Erweckt der Winde Wehen ihren Klang.", "tokens": ["Er\u00b7weckt", "der", "Win\u00b7de", "We\u00b7hen", "ih\u00b7ren", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}