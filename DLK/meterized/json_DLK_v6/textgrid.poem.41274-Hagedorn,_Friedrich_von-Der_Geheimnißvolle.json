{"textgrid.poem.41274": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Geheimni\u00dfvolle", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Zischler Aeltester, Bisbill,", "tokens": ["Der", "Zischler", "A\u00b7el\u00b7tes\u00b7ter", ",", "Bis\u00b7bill", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lehrt heimlich, was er lehren will,", "tokens": ["Lehrt", "heim\u00b7lich", ",", "was", "er", "leh\u00b7ren", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und spricht mit allen im Vertrauen.", "tokens": ["Und", "spricht", "mit", "al\u00b7len", "im", "Ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch gestern hat er, recht erstaunt,", "tokens": ["Noch", "ge\u00b7stern", "hat", "er", ",", "recht", "er\u00b7staunt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Mir, unter uns, ins Ohr geraunt:", "tokens": ["Mir", ",", "un\u00b7ter", "uns", ",", "ins", "Ohr", "ge\u00b7raunt", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPER", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbder Preu\u00dfen K\u00f6nig wei\u00df zu siegen und zu bauen.\u00ab", "tokens": ["\u00bb", "der", "Preu\u00b7\u00dfen", "K\u00f6\u00b7nig", "wei\u00df", "zu", "sie\u00b7gen", "und", "zu", "bau\u00b7en", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Nachricht gab ich gern Geh\u00f6r,", "tokens": ["Der", "Nach\u00b7richt", "gab", "ich", "gern", "Ge\u00b7h\u00f6r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und sagt' ihm: \u00bbUnter uns! der K\u00f6nig wei\u00df noch mehr.\u00ab", "tokens": ["Und", "sagt'", "ihm", ":", "\u00bb", "Un\u00b7ter", "uns", "!", "der", "K\u00f6\u00b7nig", "wei\u00df", "noch", "mehr", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "$(", "APPR", "PPER", "$.", "ART", "NN", "VVFIN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}