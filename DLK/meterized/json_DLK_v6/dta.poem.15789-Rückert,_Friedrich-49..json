{"dta.poem.15789": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann ist ein Gleichni\u00df gut? Wenn man soweit es f\u00fchrt,", "tokens": ["Wann", "ist", "ein", "Gleich\u00b7ni\u00df", "gut", "?", "Wenn", "man", "so\u00b7weit", "es", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "$.", "KOUS", "PIS", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als sein Verm\u00f6gen reicht, und man die Wirkung sp\u00fcrt.", "tokens": ["Als", "sein", "Ver\u00b7m\u00f6\u00b7gen", "reicht", ",", "und", "man", "die", "Wir\u00b7kung", "sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn es zu fr\u00fch stehn bleibt, erscheint es schwach und zahm;", "tokens": ["Wenn", "es", "zu", "fr\u00fch", "stehn", "bleibt", ",", "er\u00b7scheint", "es", "schwach", "und", "zahm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "VVINF", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn zuweit mans treibt, wird es bekanntlich lahm.", "tokens": ["Und", "wenn", "zu\u00b7weit", "mans", "treibt", ",", "wird", "es", "be\u00b7kannt\u00b7lich", "lahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PIS", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die N\u00e4h zerst\u00f6rt den Schein, von fern ist alles gleich,", "tokens": ["Die", "N\u00e4h", "zer\u00b7st\u00f6rt", "den", "Schein", ",", "von", "fern", "ist", "al\u00b7les", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "APPR", "ADJD", "VAFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In rechter Mitte nur ist es beziehungsreich.", "tokens": ["In", "rech\u00b7ter", "Mit\u00b7te", "nur", "ist", "es", "be\u00b7zie\u00b7hungs\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-++--+--", "measure": "iambic.penta.relaxed"}}}}}