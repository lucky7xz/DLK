{"dta.poem.14637": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der G\u00f6tter Irrfahrt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Zwischen grauen Wolken-Schweifen,", "tokens": ["Zwi\u00b7schen", "grau\u00b7en", "Wol\u00b7ken\u00b7Schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die verschlafen Berg und Flut", "tokens": ["Die", "ver\u00b7schla\u00b7fen", "Berg", "und", "Flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den langen Schleiern streifen,", "tokens": ["Mit", "den", "lan\u00b7gen", "Schlei\u00b7ern", "strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoch der G\u00f6ttervater ruht.", "tokens": ["Hoch", "der", "G\u00f6t\u00b7ter\u00b7va\u00b7ter", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heut zu fischen ihn gel\u00fcstet,", "tokens": ["Heut", "zu", "fi\u00b7schen", "ihn", "ge\u00b7l\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und vom zack'gen Felsenhang", "tokens": ["Und", "vom", "zack'\u00b7gen", "Fel\u00b7sen\u00b7hang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In des Meeres gr\u00fcne W\u00fcste", "tokens": ["In", "des", "Mee\u00b7res", "gr\u00fc\u00b7ne", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Senket er die Schnur zum Fang.", "tokens": ["Sen\u00b7ket", "er", "die", "Schnur", "zum", "Fang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}