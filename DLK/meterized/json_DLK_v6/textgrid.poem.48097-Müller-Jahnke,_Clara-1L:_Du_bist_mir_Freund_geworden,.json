{"textgrid.poem.48097": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du bist mir Freund geworden,", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist mir Freund geworden,", "tokens": ["Du", "bist", "mir", "Freund", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "des trag ich Freud genug;", "tokens": ["des", "trag", "ich", "Freud", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "es rauscht in Sturmakkorden,", "tokens": ["es", "rauscht", "in", "Stur\u00b7mak\u00b7kor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "o Meer, dein Atemzug.", "tokens": ["o", "Meer", ",", "dein", "A\u00b7tem\u00b7zug", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["FM", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er haucht in meine Seele", "tokens": ["Er", "haucht", "in", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "ein Ahnen licht und gro\u00df \u2013", "tokens": ["ein", "Ah\u00b7nen", "licht", "und", "gro\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "da sinken Schuld und Fehle", "tokens": ["da", "sin\u00b7ken", "Schuld", "und", "Feh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "wie Fesseln von mir los.", "tokens": ["wie", "Fes\u00b7seln", "von", "mir", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du bist mir Freund geworden,", "tokens": ["Du", "bist", "mir", "Freund", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "des trag ich Freud genug;", "tokens": ["des", "trag", "ich", "Freud", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "mich zog zu deinen Borden", "tokens": ["mich", "zog", "zu", "dei\u00b7nen", "Bor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "ein wundersamer Zug.", "tokens": ["ein", "wun\u00b7der\u00b7sa\u00b7mer", "Zug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich lie\u00df der Palmenw\u00e4lder", "tokens": ["Ich", "lie\u00df", "der", "Pal\u00b7men\u00b7w\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "schw\u00fclduftende Tropennacht,", "tokens": ["schw\u00fcl\u00b7duf\u00b7ten\u00b7de", "Tro\u00b7pen\u00b7nacht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "ich lie\u00df der Weizenfelder", "tokens": ["ich", "lie\u00df", "der", "Wei\u00b7zen\u00b7fel\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "goldgl\u00e4nzende Aehrenpracht.", "tokens": ["gold\u00b7gl\u00e4n\u00b7zen\u00b7de", "A\u00b7eh\u00b7ren\u00b7pracht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Vergessen hab ich lange", "tokens": ["Ver\u00b7ges\u00b7sen", "hab", "ich", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "der B\u00e4ume fr\u00fcchteschwer;", "tokens": ["der", "B\u00e4u\u00b7me", "fr\u00fcch\u00b7te\u00b7schwer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ich gr\u00fc\u00dfe vom D\u00fcnenhange", "tokens": ["ich", "gr\u00fc\u00b7\u00dfe", "vom", "D\u00fc\u00b7nen\u00b7han\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "dich, vielgeliebtes Meer!", "tokens": ["dich", ",", "viel\u00b7ge\u00b7lieb\u00b7tes", "Meer", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich lockt aus blauen Feuchten", "tokens": ["Mich", "lockt", "aus", "blau\u00b7en", "Feuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "ein flimmernd Wellenspiel:", "tokens": ["ein", "flim\u00b7mernd", "Wel\u00b7len\u00b7spiel", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "eine Krone seh ich leuchten,", "tokens": ["ei\u00b7ne", "Kro\u00b7ne", "seh", "ich", "leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "die in die Tiefe fiel.", "tokens": ["die", "in", "die", "Tie\u00b7fe", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}