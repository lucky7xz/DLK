{"textgrid.poem.34801": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und der Gott sprach zu dem Teufel:", "tokens": ["Und", "der", "Gott", "sprach", "zu", "dem", "Teu\u00b7fel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbich, der Herr, kopier mich selber,", "tokens": ["\u00bb", "ich", ",", "der", "Herr", ",", "ko\u00b7pier", "mich", "sel\u00b7ber", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "ART", "NN", "$,", "VVIMP", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach der Sonne mach ich Sterne,", "tokens": ["Nach", "der", "Son\u00b7ne", "mach", "ich", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach den Ochsen mach ich K\u00e4lber,", "tokens": ["Nach", "den", "Och\u00b7sen", "mach", "ich", "K\u00e4l\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nach den L\u00f6wen mit den Tatzen", "tokens": ["Nach", "den", "L\u00f6\u00b7wen", "mit", "den", "Tat\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mach ich kleine liebe Katzen,", "tokens": ["Mach", "ich", "klei\u00b7ne", "lie\u00b7be", "Kat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nach den Menschen mach ich Affen;", "tokens": ["Nach", "den", "Men\u00b7schen", "mach", "ich", "Af\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber du kannst gar nichts schaffen.\u00ab", "tokens": ["A\u00b7ber", "du", "kannst", "gar", "nichts", "schaf\u00b7fen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "PIS", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}