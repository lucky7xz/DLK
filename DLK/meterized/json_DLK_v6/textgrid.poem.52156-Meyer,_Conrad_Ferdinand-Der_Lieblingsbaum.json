{"textgrid.poem.52156": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Lieblingsbaum", "genre": "verse", "period": "N.A.", "pub_year": 1866, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den ich pflanzte, junger Baum,", "tokens": ["Den", "ich", "pflanz\u00b7te", ",", "jun\u00b7ger", "Baum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dessen Wuchs mich freute,", "tokens": ["Des\u00b7sen", "Wuchs", "mich", "freu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Z\u00e4hl ich deine Lenze, kaum", "tokens": ["Z\u00e4hl", "ich", "dei\u00b7ne", "Len\u00b7ze", ",", "kaum"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$,", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind es zwanzig heute.", "tokens": ["Sind", "es", "zwan\u00b7zig", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "CARD", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Oft im Geist erg\u00f6tzt es mich,", "tokens": ["Oft", "im", "Geist", "er\u00b7g\u00f6tzt", "es", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber mir im Blauen,", "tokens": ["\u00dc\u00b7ber", "mir", "im", "Blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schlankes Astgebilde, dich", "tokens": ["Schlan\u00b7kes", "Ast\u00b7ge\u00b7bil\u00b7de", ",", "dich"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4chtig auszubauen.", "tokens": ["M\u00e4ch\u00b7tig", "aus\u00b7zu\u00b7bau\u00b7en", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVIZU", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Lichtdurchwirkten Schatten nur", "tokens": ["Licht\u00b7durch\u00b7wirk\u00b7ten", "Schat\u00b7ten", "nur"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Legst du auf die Matten,", "tokens": ["Legst", "du", "auf", "die", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Eh du dunkel deckst die Flur,", "tokens": ["Eh", "du", "dun\u00b7kel", "deckst", "die", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin ich selbst ein Schatten.", "tokens": ["Bin", "ich", "selbst", "ein", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Aber haschen soll mich nicht", "tokens": ["A\u00b7ber", "ha\u00b7schen", "soll", "mich", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stygisches Gesinde,", "tokens": ["Sty\u00b7gi\u00b7sches", "Ge\u00b7sin\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Weichen werd ich aus dem Licht", "tokens": ["Wei\u00b7chen", "werd", "ich", "aus", "dem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter deine Rinde.", "tokens": ["Un\u00b7ter", "dei\u00b7ne", "Rin\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Frische S\u00e4fte rieseln laut,", "tokens": ["Fri\u00b7sche", "S\u00e4f\u00b7te", "rie\u00b7seln", "laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rieseln durch die Stille.", "tokens": ["Rie\u00b7seln", "durch", "die", "Stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Um mich, in mir webt und baut", "tokens": ["Um", "mich", ",", "in", "mir", "webt", "und", "baut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "$,", "APPR", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ew'ger Lebenswille.", "tokens": ["Ew'\u00b7ger", "Le\u00b7bens\u00b7wil\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Halb bewu\u00dft und halb im Traum", "tokens": ["Halb", "be\u00b7wu\u00dft", "und", "halb", "im", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber mir im Lichten", "tokens": ["\u00dc\u00b7ber", "mir", "im", "Lich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Werd ich, mein geliebter Baum,", "tokens": ["Werd", "ich", ",", "mein", "ge\u00b7lieb\u00b7ter", "Baum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich zu Ende dichten.", "tokens": ["Dich", "zu", "En\u00b7de", "dich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}