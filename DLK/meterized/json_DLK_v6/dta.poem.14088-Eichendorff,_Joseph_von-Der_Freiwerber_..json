{"dta.poem.14088": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Freiwerber .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchmorgens durch die Winde k\u00fchl", "tokens": ["Fr\u00fch\u00b7mor\u00b7gens", "durch", "die", "Win\u00b7de", "k\u00fchl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Zwei Ritter hergeritten sind,", "tokens": ["Zwei", "Rit\u00b7ter", "her\u00b7ge\u00b7rit\u00b7ten", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Garten klingt ihr Saitenspiel,", "tokens": ["Im", "Gar\u00b7ten", "klingt", "ihr", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wach' auf, wach' auf, mein sch\u00f6nes Kind!", "tokens": ["Wach'", "auf", ",", "wach'", "auf", ",", "mein", "sch\u00f6\u00b7nes", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}