{"dta.poem.13311": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ejhr selber werdet es, Freundinnen! mir gestehn,", "tokens": ["\u201e", "jhr", "sel\u00b7ber", "wer\u00b7det", "es", ",", "Freun\u00b7din\u00b7nen", "!", "mir", "ge\u00b7stehn", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "VAFIN", "PPER", "$,", "NN", "$.", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ewie schwer das Herrschungs-Amt von Statten w\u00fcrde gehn,", "tokens": ["\u201e", "wie", "schwer", "das", "Herr\u00b7schungs\u00b7Amt", "von", "Stat\u00b7ten", "w\u00fcr\u00b7de", "gehn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJD", "ART", "NN", "APPR", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ewann man zu diesem Ziel nur eine Tugend w\u00e4hlte,", "tokens": ["\u201e", "wann", "man", "zu", "die\u00b7sem", "Ziel", "nur", "ei\u00b7ne", "Tu\u00b7gend", "w\u00e4hl\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIS", "APPR", "PDAT", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eund keine von dem Chor zur Hilff ihr beygesellte.", "tokens": ["\u201e", "und", "kei\u00b7ne", "von", "dem", "Chor", "zur", "Hilff", "ihr", "bey\u00b7ge\u00b7sell\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PIAT", "APPR", "ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewas wircken Hoheit, Schmuck, und Pracht der Majest\u00e4t,", "tokens": ["\u201e", "was", "wir\u00b7cken", "Ho\u00b7heit", ",", "Schmuck", ",", "und", "Pracht", "der", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADJA", "NN", "$,", "NN", "$,", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "330\u201dWann ihr die Weisheit nicht stets an der Seite geht?", "tokens": ["\"", "Wann", "ihr", "die", "Weis\u00b7heit", "nicht", "stets", "an", "der", "Sei\u00b7te", "geht", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "ART", "NN", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ewas kann die Tapferkeit vor Thaten unternehmen,", "tokens": ["\u201e", "was", "kann", "die", "Tap\u00b7fer\u00b7keit", "vor", "Tha\u00b7ten", "un\u00b7ter\u00b7neh\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ewann die Gerechtigkeit sich mu\u00df derselben sch\u00e4men?", "tokens": ["\u201e", "wann", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "sich", "mu\u00df", "der\u00b7sel\u00b7ben", "sch\u00e4\u00b7men", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "NN", "PRF", "VMFIN", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ewas ist das, was das Herz der Gro\u00dfmuth nicht vermag?", "tokens": ["\u201e", "was", "ist", "das", ",", "was", "das", "Herz", "der", "Gro\u00df\u00b7muth", "nicht", "ver\u00b7mag", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PDS", "$,", "PRELS", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201edoch legt die Mildigkeit ihr Wircken an den Tag.", "tokens": ["\u201e", "doch", "legt", "die", "Mil\u00b7dig\u00b7keit", "ihr", "Wir\u00b7cken", "an", "den", "Tag", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "335\u201dDie Freundlichkeit ist sch\u00f6n: wie Kraft-lo\u00df ohne Treue?", "tokens": ["\"", "Die", "Freund\u00b7lich\u00b7keit", "ist", "sch\u00f6n", ":", "wie", "Kraft\u00b7lo\u00df", "oh\u00b7ne", "Treu\u00b7e", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADJD", "$.", "PWAV", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eund die Barmherzigkeit, die sich der Gnade scheue,", "tokens": ["\u201e", "und", "die", "Barm\u00b7her\u00b7zig\u00b7keit", ",", "die", "sich", "der", "Gna\u00b7de", "scheu\u00b7e", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ART", "NN", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201ewas n\u00fczt ihr weiches Herz? die Staats-Kunst was ist sie,", "tokens": ["\u201e", "was", "n\u00fczt", "ihr", "wei\u00b7ches", "Herz", "?", "die", "Staats\u00b7Kunst", "was", "ist", "sie", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "ART", "NN", "PWS", "VAFIN", "PPER", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "\u201ewann die Bescheidenheit nicht mitwirckt? eitle M\u00fch.", "tokens": ["\u201e", "wann", "die", "Be\u00b7schei\u00b7den\u00b7heit", "nicht", "mit\u00b7wirckt", "?", "eit\u00b7le", "M\u00fch", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "NN", "PTKNEG", "VVPP", "$.", "ADJA", "NN", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.15": {"text": "\u201edie G\u00fcte sonder Rath? wer kennt nicht jenen Schaden,", "tokens": ["\u201e", "die", "G\u00fc\u00b7te", "son\u00b7der", "Rath", "?", "wer", "kennt", "nicht", "je\u00b7nen", "Scha\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "KON", "NN", "$.", "PWS", "VVFIN", "PTKNEG", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "340\u201dDer oft daher entspringt? sie mi\u00dfbraucht Huld und Gnaden.", "tokens": ["\"", "Der", "oft", "da\u00b7her", "ent\u00b7springt", "?", "sie", "mi\u00df\u00b7braucht", "Huld", "und", "Gna\u00b7den", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADV", "PAV", "VVFIN", "$.", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201edie Weisheit, spricht vielleicht die Weiseste von euch,", "tokens": ["\u201e", "die", "Weis\u00b7heit", ",", "spricht", "viel\u00b7leicht", "die", "Wei\u00b7ses\u00b7te", "von", "euch", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201eist die Vortrefflichste; wo nicht? doch allen gleich.", "tokens": ["\u201e", "ist", "die", "Vor\u00b7treff\u00b7lichs\u00b7te", ";", "wo", "nicht", "?", "doch", "al\u00b7len", "gleich", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ART", "NN", "$.", "PWAV", "PTKNEG", "$.", "ADV", "PIS", "ADV", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "\u201ees sey: was k\u00f6nnt jedoch ihr weises Herrschen n\u00fczen,", "tokens": ["\u201e", "es", "sey", ":", "was", "k\u00f6nnt", "je\u00b7doch", "ihr", "wei\u00b7ses", "Herr\u00b7schen", "n\u00fc\u00b7zen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "$.", "PWS", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201ewann nicht die Fr\u00f6mmigkeit den Zepter w\u00fcrde st\u00fczen?", "tokens": ["\u201e", "wann", "nicht", "die", "Fr\u00f6m\u00b7mig\u00b7keit", "den", "Zep\u00b7ter", "w\u00fcr\u00b7de", "st\u00fc\u00b7zen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PTKNEG", "ART", "NN", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "345\u201dGesezt auch: diese w\u00e4r zum Oberhaupt gemacht;", "tokens": ["\"", "Ge\u00b7sezt", "auch", ":", "die\u00b7se", "w\u00e4r", "zum", "O\u00b7ber\u00b7haupt", "ge\u00b7macht", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "$.", "PDS", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "\u201ewas wirckte sie, wann nicht mit ihr die Weisheit wacht?", "tokens": ["\u201e", "was", "wirck\u00b7te", "sie", ",", "wann", "nicht", "mit", "ihr", "die", "Weis\u00b7heit", "wacht", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$,", "PWAV", "PTKNEG", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "\u201egnad, Anmuth, Reiz und Huld seynd allzeit hoch gesch\u00e4zet,", "tokens": ["\u201e", "gnad", ",", "An\u00b7muth", ",", "Reiz", "und", "Huld", "seynd", "all\u00b7zeit", "hoch", "ge\u00b7sch\u00e4\u00b7zet", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NN", "$,", "NN", "KON", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "\u201ewie? wann die Redlichkeit sich nicht zu ihnen sezet.", "tokens": ["\u201e", "wie", "?", "wann", "die", "Red\u00b7lich\u00b7keit", "sich", "nicht", "zu", "ih\u00b7nen", "se\u00b7zet", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$.", "PWAV", "ART", "NN", "PRF", "PTKNEG", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}