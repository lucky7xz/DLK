{"textgrid.poem.52666": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Wiederfinden", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kam zur guten alten Stadt,", "tokens": ["Ich", "kam", "zur", "gu\u00b7ten", "al\u00b7ten", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Tag hab ich behalten,", "tokens": ["Den", "Tag", "hab", "ich", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ins Haus, wo sie gewohnet hat,", "tokens": ["Ins", "Haus", ",", "wo", "sie", "ge\u00b7woh\u00b7net", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der meine Lieder galten.", "tokens": ["Der", "mei\u00b7ne", "Lie\u00b7der", "gal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die ich geliebt, die mich geliebt,", "tokens": ["Die", "ich", "ge\u00b7liebt", ",", "die", "mich", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und der ichs nie gestanden,", "tokens": ["Und", "der", "ichs", "nie", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und die mirs nie, wie sichs begibt,", "tokens": ["Und", "die", "mirs", "nie", ",", "wie", "sichs", "be\u00b7gibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "ADV", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil wir nicht Worte fanden.", "tokens": ["Weil", "wir", "nicht", "Wor\u00b7te", "fan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bist du nun ferne, bist du nah,", "tokens": ["Bist", "du", "nun", "fer\u00b7ne", ",", "bist", "du", "nah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "$,", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat dich ein Andrer gefunden,", "tokens": ["Hat", "dich", "ein", "A\u00b7ndrer", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich wei\u00df nicht, Theure, bist du da,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "Theu\u00b7re", ",", "bist", "du", "da", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "ADJA", "$,", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bist du f\u00fcr mich verschwunden?", "tokens": ["Bist", "du", "f\u00fcr", "mich", "ver\u00b7schwun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wenn ich dich nicht wiederseh,", "tokens": ["Und", "wenn", "ich", "dich", "nicht", "wie\u00b7der\u00b7seh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will ich doch von dir tr\u00e4umen,", "tokens": ["Will", "ich", "doch", "von", "dir", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Und hab ich dich nicht in der N\u00e4h,", "tokens": ["Und", "hab", "ich", "dich", "nicht", "in", "der", "N\u00e4h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Will ich am Ort doch s\u00e4umen.", "tokens": ["Will", "ich", "am", "Ort", "doch", "s\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wie ich nun die Stiege mit", "tokens": ["Und", "wie", "ich", "nun", "die", "Stie\u00b7ge", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gelassenheit erklimme,", "tokens": ["Ge\u00b7las\u00b7sen\u00b7heit", "er\u00b7klim\u00b7me", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da h\u00f6r ich ihren leichten Schritt", "tokens": ["Da", "h\u00f6r", "ich", "ih\u00b7ren", "leich\u00b7ten", "Schritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihre helle Stimme.", "tokens": ["Und", "ih\u00b7re", "hel\u00b7le", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Als wie der Mond aus Wolken bricht,", "tokens": ["Als", "wie", "der", "Mond", "aus", "Wol\u00b7ken", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So scheu und still erschrocken,", "tokens": ["So", "scheu", "und", "still", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ergl\u00e4nzt ihr liebes Angesicht", "tokens": ["Er\u00b7gl\u00e4nzt", "ihr", "lie\u00b7bes", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus ihren dunklen Locken.", "tokens": ["Aus", "ih\u00b7ren", "dunk\u00b7len", "Lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es leuchtete so freudenklar", "tokens": ["Es", "leuch\u00b7te\u00b7te", "so", "freu\u00b7den\u00b7klar"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus ihren sch\u00f6nen Mienen,", "tokens": ["Aus", "ih\u00b7ren", "sch\u00f6\u00b7nen", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sch\u00f6ner als sie jemals war,", "tokens": ["Und", "sch\u00f6\u00b7ner", "als", "sie", "je\u00b7mals", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist sie mir heut erschienen.", "tokens": ["Ist", "sie", "mir", "heut", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Kein Sterbenswort. Sie sah mich an;", "tokens": ["Kein", "Ster\u00b7bens\u00b7wort", ".", "Sie", "sah", "mich", "an", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Busen h\u00f6rt ich pochen \u2013", "tokens": ["Den", "Bu\u00b7sen", "h\u00f6rt", "ich", "po\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein rascher H\u00e4ndedruck sodann,", "tokens": ["Ein", "ra\u00b7scher", "H\u00e4n\u00b7de\u00b7druck", "so\u00b7dann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Daraus ihr Herz gesprochen!", "tokens": ["Da\u00b7raus", "ihr", "Herz", "ge\u00b7spro\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}