{"dta.poem.6756": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Considerations sur la Vue.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem die Z\u00fcge nun, wie wir gesehn,", "tokens": ["Nach\u00b7dem", "die", "Z\u00fc\u00b7ge", "nun", ",", "wie", "wir", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zusammt den Bildungen nur eingedr\u00fccket stehn", "tokens": ["Zu\u00b7sammt", "den", "Bil\u00b7dun\u00b7gen", "nur", "ein\u00b7ge\u00b7dr\u00fc\u00b7cket", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVFIN", "VVINF"], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Auf C\u00f6rper, welche nicht belebet;", "tokens": ["Auf", "C\u00f6r\u00b7per", ",", "wel\u00b7che", "nicht", "be\u00b7le\u00b7bet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So kann man ja dahero leichtlich fassen,", "tokens": ["So", "kann", "man", "ja", "da\u00b7he\u00b7ro", "leicht\u00b7lich", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "PAV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df unser Auge blo\u00df dazu gemacht,", "tokens": ["Da\u00df", "un\u00b7ser", "Au\u00b7ge", "blo\u00df", "da\u00b7zu", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Um diese Mahlerey durch sich zu lassen,", "tokens": ["Um", "die\u00b7se", "Mah\u00b7le\u00b7rey", "durch", "sich", "zu", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "APPR", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und da\u00df durch sie die Seel\u2019 auf andre Weise nicht", "tokens": ["Und", "da\u00df", "durch", "sie", "die", "Seel'", "auf", "and\u00b7re", "Wei\u00b7se", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Empfindet, urtheilt, schliesst, als wie von ihr geschicht,", "tokens": ["Emp\u00b7fin\u00b7det", ",", "ur\u00b7theilt", ",", "schliesst", ",", "als", "wie", "von", "ihr", "ge\u00b7schicht", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "KOUS", "KOKOM", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn sie dasselbige", "tokens": ["Wenn", "sie", "das\u00b7sel\u00b7bi\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PDS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Durch C\u00f6rper, die ihr frembd, verricht.", "tokens": ["Durch", "C\u00f6r\u00b7per", ",", "die", "ihr", "frembd", ",", "ver\u00b7richt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "PPER", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}