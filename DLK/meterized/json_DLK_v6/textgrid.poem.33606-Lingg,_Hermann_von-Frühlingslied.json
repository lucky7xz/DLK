{"textgrid.poem.33606": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingslied", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling verschleiert nun wieder", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ver\u00b7schlei\u00b7ert", "nun", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Erde ganz", "tokens": ["Die", "Er\u00b7de", "ganz"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mir zartem Laubgefieder,", "tokens": ["Mir", "zar\u00b7tem", "Laub\u00b7ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Bl\u00fctenglanz;", "tokens": ["Mit", "Bl\u00fc\u00b7ten\u00b7glanz", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Nun eilet zum Tanz", "tokens": ["Nun", "ei\u00b7let", "zum", "Tanz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Hier unter dem bl\u00fchenden Flieder!", "tokens": ["Hier", "un\u00b7ter", "dem", "bl\u00fc\u00b7hen\u00b7den", "Flie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Von schwellenden Zweigen hernieder", "tokens": ["Von", "schwel\u00b7len\u00b7den", "Zwei\u00b7gen", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKVZ"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Singt sehnlich bang", "tokens": ["Singt", "sehn\u00b7lich", "bang"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Drossel so liebliche Lieder;", "tokens": ["Die", "Dros\u00b7sel", "so", "lieb\u00b7li\u00b7che", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ert\u00f6ne noch lang", "tokens": ["Er\u00b7t\u00f6\u00b7ne", "noch", "lang"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Du s\u00fc\u00dfer Gesang", "tokens": ["Du", "s\u00fc\u00b7\u00dfer", "Ge\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Hier unter dem bl\u00fchenden Flieder!", "tokens": ["Hier", "un\u00b7ter", "dem", "bl\u00fc\u00b7hen\u00b7den", "Flie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Schwerm\u00fctige Liebe, komm wieder,", "tokens": ["Schwer\u00b7m\u00fc\u00b7ti\u00b7ge", "Lie\u00b7be", ",", "komm", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Du sch\u00f6nstes Gl\u00fcck!", "tokens": ["Du", "sch\u00f6ns\u00b7tes", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Vom Dunkel der Sterne schweb nieder", "tokens": ["Vom", "Dun\u00b7kel", "der", "Ster\u00b7ne", "schweb", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Zur Erde zur\u00fcck,", "tokens": ["Zur", "Er\u00b7de", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Du sch\u00f6nstes Gl\u00fcck,", "tokens": ["Du", "sch\u00f6ns\u00b7tes", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Hier unter dem bl\u00fchenden Flieder!", "tokens": ["Hier", "un\u00b7ter", "dem", "bl\u00fc\u00b7hen\u00b7den", "Flie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}