{"textgrid.poem.67923": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "19. Der kurze Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchling w\u00e4hrt nicht immer, M\u00e4dchen,", "tokens": ["Fr\u00fch\u00b7ling", "w\u00e4hrt", "nicht", "im\u00b7mer", ",", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADV", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fchling w\u00e4hrt nicht immer.", "tokens": ["Fr\u00fch\u00b7ling", "w\u00e4hrt", "nicht", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "La\u00dft euch nicht die Zeit betr\u00fcgen,", "tokens": ["La\u00dft", "euch", "nicht", "die", "Zeit", "be\u00b7tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00dft euch nicht die Jugend t\u00e4uschen,", "tokens": ["La\u00dft", "euch", "nicht", "die", "Ju\u00b7gend", "t\u00e4u\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zeit und Jugend flechten Kr\u00e4nze", "tokens": ["Zeit", "und", "Ju\u00b7gend", "flech\u00b7ten", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus gar zarten Blumen.", "tokens": ["Aus", "gar", "zar\u00b7ten", "Blu\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Fr\u00fchling w\u00e4hrt nicht immer, M\u00e4dchen,", "tokens": ["Fr\u00fch\u00b7ling", "w\u00e4hrt", "nicht", "im\u00b7mer", ",", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADV", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fchling w\u00e4hrt nicht immer.", "tokens": ["Fr\u00fch\u00b7ling", "w\u00e4hrt", "nicht", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Leicht entfliegen unsre Jahre,", "tokens": ["Leicht", "ent\u00b7flie\u00b7gen", "uns\u00b7re", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit r\u00e4uberischem Fl\u00fcgel", "tokens": ["Und", "mit", "r\u00e4u\u00b7be\u00b7ri\u00b7schem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kommen, unser Mal zu st\u00f6ren,", "tokens": ["Kom\u00b7men", ",", "un\u00b7ser", "Mal", "zu", "st\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie, Harpyen, wieder.", "tokens": ["Sie", ",", "Har\u00b7py\u00b7en", ",", "wie\u00b7der", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Fr\u00fchling w\u00e4hrt nicht immer, M\u00e4dchen,", "tokens": ["Fr\u00fch\u00b7ling", "w\u00e4hrt", "nicht", "im\u00b7mer", ",", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADV", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fchling w\u00e4hrt nicht immer.", "tokens": ["Fr\u00fch\u00b7ling", "w\u00e4hrt", "nicht", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn ihr glaubt, da\u00df Lebensglocke", "tokens": ["Wenn", "ihr", "glaubt", ",", "da\u00df", "Le\u00b7bens\u00b7glo\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch den Morgen noch verk\u00fcndet,", "tokens": ["Euch", "den", "Mor\u00b7gen", "noch", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist es schon die Abendglocke,", "tokens": ["Ist", "es", "schon", "die", "A\u00b7bend\u00b7glo\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die die Freud' euch endet.", "tokens": ["Die", "die", "Freud'", "euch", "en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Fr\u00fchling w\u00e4hrt nicht immer, M\u00e4dchen,", "tokens": ["Fr\u00fch\u00b7ling", "w\u00e4hrt", "nicht", "im\u00b7mer", ",", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADV", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fchling w\u00e4hrt nicht immer.", "tokens": ["Fr\u00fch\u00b7ling", "w\u00e4hrt", "nicht", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Freut euch, weil ihr freun euch k\u00f6nnet,", "tokens": ["Freut", "euch", ",", "weil", "ihr", "freun", "euch", "k\u00f6n\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebet, weil man euch noch liebet,", "tokens": ["Lie\u00b7bet", ",", "weil", "man", "euch", "noch", "lie\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eh das Alter eure goldnen", "tokens": ["Eh", "das", "Al\u00b7ter", "eu\u00b7re", "gold\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Haare schnell versilbert.", "tokens": ["Haa\u00b7re", "schnell", "ver\u00b7sil\u00b7bert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}