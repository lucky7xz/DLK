{"textgrid.poem.39459": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gesch\u00e4ft und Sorge wohnt am d\u00fcrren Strande", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gesch\u00e4ft und Sorge wohnt am d\u00fcrren Strande", "tokens": ["Ge\u00b7sch\u00e4ft", "und", "Sor\u00b7ge", "wohnt", "am", "d\u00fcr\u00b7ren", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und kann dem engen Krei\u00dflauf nicht entgehen;", "tokens": ["Und", "kann", "dem", "en\u00b7gen", "Krei\u00df\u00b7lauf", "nicht", "ent\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch Phantasie lockt \u00fcber ferne Seen", "tokens": ["Doch", "Phan\u00b7ta\u00b7sie", "lockt", "\u00fc\u00b7ber", "fer\u00b7ne", "Seen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "An sel'ge Inseln, wunderbare Lande.", "tokens": ["An", "sel'\u00b7ge", "In\u00b7seln", ",", "wun\u00b7der\u00b7ba\u00b7re", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Wie freudig l\u00f6s' ich meines Schiffleins Bande,", "tokens": ["Wie", "freu\u00b7dig", "l\u00f6s'", "ich", "mei\u00b7nes", "Schif\u00b7fleins", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was Ahndung spielet, nah enth\u00fcllt zu sehen!", "tokens": ["Was", "Ahn\u00b7dung", "spie\u00b7let", ",", "nah", "ent\u00b7h\u00fcllt", "zu", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "ADJD", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Geister neugeborner Lieder wehen", "tokens": ["Die", "Geis\u00b7ter", "neu\u00b7ge\u00b7bor\u00b7ner", "Lie\u00b7der", "we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch meiner Segel schwellende Gewande.", "tokens": ["Durch", "mei\u00b7ner", "Se\u00b7gel", "schwel\u00b7len\u00b7de", "Ge\u00b7wan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Verbr\u00fcderte Gef\u00e4hrten seh' ich schweben:", "tokens": ["Ver\u00b7br\u00fc\u00b7der\u00b7te", "Ge\u00b7f\u00e4hr\u00b7ten", "seh'", "ich", "schwe\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was schreckte wohl, da\u00df ich dahinten bliebe?", "tokens": ["Was", "schreck\u00b7te", "wohl", ",", "da\u00df", "ich", "da\u00b7hin\u00b7ten", "blie\u00b7be", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es leuchten milde Sterne, droht kein Wetter.", "tokens": ["Es", "leuch\u00b7ten", "mil\u00b7de", "Ster\u00b7ne", ",", "droht", "kein", "Wet\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So leit', o s\u00fc\u00dfe Poesie! mein Leben;", "tokens": ["So", "leit'", ",", "o", "s\u00fc\u00b7\u00dfe", "Poe\u00b7sie", "!", "mein", "Le\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "ADJA", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du Jugend in der Jugend, Lieb' in Liebe,", "tokens": ["Du", "Ju\u00b7gend", "in", "der", "Ju\u00b7gend", ",", "Lieb'", "in", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Natur in der Natur, Gottheit der G\u00f6tter!", "tokens": ["Na\u00b7tur", "in", "der", "Na\u00b7tur", ",", "Got\u00b7theit", "der", "G\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}