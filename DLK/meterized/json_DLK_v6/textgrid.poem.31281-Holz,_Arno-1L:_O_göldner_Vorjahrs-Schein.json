{"textgrid.poem.31281": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: O g\u00f6ldner Vorjahrs-Schein/", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O g\u00f6ldner Vorjahrs-Schein/", "tokens": ["O", "g\u00f6ld\u00b7ner", "Vor\u00b7jahr\u00b7sSchein", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "br\u00e4chstu doch bald herein!", "tokens": ["br\u00e4chs\u00b7tu", "doch", "bald", "her\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch Ein-mahl m\u00f6cht ich sehn", "tokens": ["Noch", "Ein\u00b7mahl", "m\u00f6cht", "ich", "sehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "die Kindgens Kr\u00e4ntze drehn!", "tokens": ["die", "Kind\u00b7gens", "Kr\u00e4nt\u00b7ze", "drehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Itzt pfeifft der Wind au\u00df Pohlen/", "tokens": ["Itzt", "pfeifft", "der", "Wind", "au\u00df", "Poh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NE", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "dan dantzt man auff Violen/", "tokens": ["dan", "dantzt", "man", "auff", "Vi\u00b7o\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "APPR", "NN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "dan h\u00e4ngkt ob gr\u00fcner Au", "tokens": ["dan", "h\u00e4ngkt", "ob", "gr\u00fc\u00b7ner", "Au"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOUS", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "die Lufft Hertz-Himmelblau;", "tokens": ["die", "Lufft", "Hertz\u00b7Him\u00b7mel\u00b7blau", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "\u00fcmb bundte Kiesel schw\u00e4zzt der Bach/", "tokens": ["\u00fcmb", "bund\u00b7te", "Kie\u00b7sel", "schw\u00e4zzt", "der", "Bach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "der Gukguk rufft das Echo wach!", "tokens": ["der", "Guk\u00b7guk", "rufft", "das", "E\u00b7cho", "wach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Frau ", "tokens": ["Frau"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "dan in ihr Hifft-Horn st\u00f6h\u00dft/", "tokens": ["dan", "in", "ihr", "Hifft\u00b7Horn", "st\u00f6h\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+--++-", "measure": "dactylic.init"}, "line.3": {"text": "ihr Kleid au\u00df Doppel-Dafft", "tokens": ["ihr", "Kleid", "au\u00df", "Dop\u00b7pel\u00b7Dafft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "weht zihrlich auff-gerafft!", "tokens": ["weht", "zihr\u00b7lich", "auf\u00b7fge\u00b7rafft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie hat mich gantz besessen/", "tokens": ["Sie", "hat", "mich", "gantz", "be\u00b7ses\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "ich kan es nicht vergessen/", "tokens": ["ich", "kan", "es", "nicht", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "al\u00df ich im braunen Hahr", "tokens": ["al\u00df", "ich", "im", "brau\u00b7nen", "Hahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "noch jung und fr\u00f6lig war!", "tokens": ["noch", "jung", "und", "fr\u00f6\u00b7lig", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Da machten uns vergn\u00fcgten Sinns", "tokens": ["Da", "mach\u00b7ten", "uns", "ver\u00b7gn\u00fcg\u00b7ten", "Sinns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "die gelben Himmels-Schl\u00fcsselgins!", "tokens": ["die", "gel\u00b7ben", "Him\u00b7mels\u00b7Sch\u00b7l\u00fcs\u00b7selg\u00b7ins", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wir sa\u00dfen Hand in Hand/", "tokens": ["Wir", "sa\u00b7\u00dfen", "Hand", "in", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "manierlich und galant/", "tokens": ["ma\u00b7nier\u00b7lich", "und", "ga\u00b7lant", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "kein L\u00fcfftgen blih\u00df durchs Moos/", "tokens": ["kein", "L\u00fcfft\u00b7gen", "blih\u00df", "durchs", "Moos", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "wir lih\u00dfen uns nicht loh\u00df/", "tokens": ["wir", "lih\u00b7\u00dfen", "uns", "nicht", "loh\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "wenn das bes\u00fcsste Lallen", "tokens": ["wenn", "das", "be\u00b7s\u00fcss\u00b7te", "Lal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "der kleinen Nachtigallen", "tokens": ["der", "klei\u00b7nen", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "mich offt mit sanfftem Drang", "tokens": ["mich", "offt", "mit", "sanff\u00b7tem", "Drang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "ihr f\u00fcr die Knye zwang.", "tokens": ["ihr", "f\u00fcr", "die", "Knye", "zwang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Mein Hertzgespan/ mein Augendrohst!", "tokens": ["Mein", "Hertz\u00b7ge\u00b7span", "/", "mein", "Au\u00b7gen\u00b7drohst", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wie hat sie mich dan lihb-gekohst!", "tokens": ["Wie", "hat", "sie", "mich", "dan", "lih\u00b7b\u00b7ge\u00b7kohst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Sie hat mir manche Nacht", "tokens": ["Sie", "hat", "mir", "man\u00b7che", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "den Rihgel auff-gemacht;", "tokens": ["den", "Rih\u00b7gel", "auf\u00b7fge\u00b7macht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "wa\u00df heymlig dan geschehn \u2013", "tokens": ["wa\u00df", "heym\u00b7lig", "dan", "ge\u00b7schehn", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "kein M\u00e4ntsch hats zugesehn!", "tokens": ["kein", "M\u00e4ntsch", "hats", "zu\u00b7ge\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich lag ihr f\u00e4st am Hertzen/", "tokens": ["Ich", "lag", "ihr", "f\u00e4st", "am", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "ich pflag mit ihr zu schertzen/", "tokens": ["ich", "pflag", "mit", "ihr", "zu", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "ich lih\u00df ihr keine Ruh/", "tokens": ["ich", "lih\u00df", "ihr", "kei\u00b7ne", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "du lihbstes Seelchen du!", "tokens": ["du", "lihbs\u00b7tes", "Seel\u00b7chen", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Sie war mein ", "tokens": ["Sie", "war", "mein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "k\u00fcnt ichs \u2013 ich dh\u00e4t es noch-mahl so!", "tokens": ["k\u00fcnt", "ichs", "\u2013", "ich", "dh\u00e4t", "es", "noch\u00b7mahl", "so", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$(", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}