{"textgrid.poem.24028": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schwarze Blumen bl\u00fchten mir im Traume,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwarze Blumen bl\u00fchten mir im Traume,", "tokens": ["Schwar\u00b7ze", "Blu\u00b7men", "bl\u00fch\u00b7ten", "mir", "im", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kronenschwere, die sich nicht bewegen,", "tokens": ["Kro\u00b7nen\u00b7schwe\u00b7re", ",", "die", "sich", "nicht", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ob der Wind auch \u00fcber ihnen wandert.", "tokens": ["Ob", "der", "Wind", "auch", "\u00fc\u00b7ber", "ih\u00b7nen", "wan\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre sommerlichen D\u00fcfte stiegen", "tokens": ["Ih\u00b7re", "som\u00b7mer\u00b7li\u00b7chen", "D\u00fcf\u00b7te", "stie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wie der W\u00e4rme Wellen auf zum Himmel,", "tokens": ["Wie", "der", "W\u00e4r\u00b7me", "Wel\u00b7len", "auf", "zum", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Aber Winter war es um die Blumen.", "tokens": ["A\u00b7ber", "Win\u00b7ter", "war", "es", "um", "die", "Blu\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und es kam von ungef\u00e4hr ein M\u00e4dchen,", "tokens": ["Und", "es", "kam", "von", "un\u00b7ge\u00b7f\u00e4hr", "ein", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Flora kam, die mit dem Blumenhorne,", "tokens": ["Flo\u00b7ra", "kam", ",", "die", "mit", "dem", "Blu\u00b7men\u00b7hor\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und sie nahm die Blumen an die Br\u00fcste.", "tokens": ["Und", "sie", "nahm", "die", "Blu\u00b7men", "an", "die", "Br\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sieh: da wurden bunt die schwarzen Blumen,", "tokens": ["Sieh", ":", "da", "wur\u00b7den", "bunt", "die", "schwar\u00b7zen", "Blu\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VAFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Rot und gelb und blau, violenfarben,", "tokens": ["Rot", "und", "gelb", "und", "blau", ",", "vi\u00b7o\u00b7len\u00b7far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "ADJD", "KON", "ADJD", "$,", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Da sie starben an des M\u00e4dchens Br\u00fcsten.", "tokens": ["Da", "sie", "star\u00b7ben", "an", "des", "M\u00e4d\u00b7chens", "Br\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Ich erkannte nicht des Traums Bedeuten.", "tokens": ["Ich", "er\u00b7kann\u00b7te", "nicht", "des", "Traums", "Be\u00b7deu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Aber, als ich wach ward, sah ich leuchten", "tokens": ["A\u00b7ber", ",", "als", "ich", "wach", "ward", ",", "sah", "ich", "leuch\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Brauner Augen zwei, in deren Scheine", "tokens": ["Brau\u00b7ner", "Au\u00b7gen", "zwei", ",", "in", "de\u00b7ren", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "CARD", "$,", "APPR", "PRELAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Meine Selbstsucht starb und Liebe wurde.", "tokens": ["Mei\u00b7ne", "Selbst\u00b7sucht", "starb", "und", "Lie\u00b7be", "wur\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}