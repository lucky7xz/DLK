{"textgrid.poem.27956": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Worte gelten in der Welt,", "tokens": ["Wor\u00b7te", "gel\u00b7ten", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Viel und wenig, wie das Geld.", "tokens": ["Viel", "und", "we\u00b7nig", ",", "wie", "das", "Geld", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "PIS", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was f\u00fcr zeiten schelmisch hie\u00df,", "tokens": ["Was", "f\u00fcr", "zei\u00b7ten", "schel\u00b7misch", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heisset ehrlich, bringt Genie\u00df.", "tokens": ["Heis\u00b7set", "ehr\u00b7lich", ",", "bringt", "Ge\u00b7nie\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}