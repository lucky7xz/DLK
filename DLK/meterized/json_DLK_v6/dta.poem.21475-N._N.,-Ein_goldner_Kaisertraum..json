{"dta.poem.21475": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Ein goldner Kaisertraum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.85", "et:0.14"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Kennst du das Zaubereiland,", "tokens": ["Kennst", "du", "das", "Zau\u00b7be\u00b7rei\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das fern im S\u00fcden liegt,", "tokens": ["Das", "fern", "im", "S\u00fc\u00b7den", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das leis\u2019 in ew\u2019gen Schlummer", "tokens": ["Das", "leis'", "in", "ew'\u00b7gen", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Meereswelle wiegt?", "tokens": ["Die", "Mee\u00b7res\u00b7wel\u00b7le", "wiegt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hier bl\u00fcht noch der Orangen", "tokens": ["Hier", "bl\u00fcht", "noch", "der", "O\u00b7ran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Myrten Hain so sch\u00f6n,", "tokens": ["Und", "Myr\u00b7ten", "Hain", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hier schimmert noch so blendend wei\u00df", "tokens": ["Hier", "schim\u00b7mert", "noch", "so", "blen\u00b7dend", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Schnee auf Bergesh\u00f6h\u2019n.", "tokens": ["Der", "Schnee", "auf", "Ber\u00b7ges\u00b7h\u00f6h'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "APPR", "NE", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O siehst du, wie die Welle", "tokens": ["O", "siehst", "du", ",", "wie", "die", "Wel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als wie ein kleines Kind", "tokens": ["Als", "wie", "ein", "klei\u00b7nes", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Umkos\u2019t, umspielt das Eiland", "tokens": ["Um\u00b7kos't", ",", "um\u00b7spielt", "das", "Ei\u00b7land"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So weich, so schmeichelnd lind?", "tokens": ["So", "weich", ",", "so", "schmei\u00b7chelnd", "lind", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohl liegt der Schnee so blendend", "tokens": ["Wohl", "liegt", "der", "Schnee", "so", "blen\u00b7dend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hoch um des Aetna Firn\u2019,", "tokens": ["Hoch", "um", "des", "A\u00b7et\u00b7na", "Firn'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NE", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und doch wie Trauer, still und gro\u00df,", "tokens": ["Und", "doch", "wie", "Trau\u00b7er", ",", "still", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Umwebt\u2019s der Insel Stirn.", "tokens": ["Um\u00b7webt's", "der", "In\u00b7sel", "Stirn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wo blieb, der einst hier ragte", "tokens": ["Wo", "blieb", ",", "der", "einst", "hier", "rag\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "$,", "PRELS", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Meere, der Palast,", "tokens": ["Am", "Mee\u00b7re", ",", "der", "Pa\u00b7last", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der jeden Gott begr\u00fc\u00dfte", "tokens": ["Der", "je\u00b7den", "Gott", "be\u00b7gr\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als hochwillkomm\u2019nen Gast?", "tokens": ["Als", "hoch\u00b7will\u00b7kom\u00b7m'\u00b7nen", "Gast", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wo blieb, das ihn durchrauschte,", "tokens": ["Wo", "blieb", ",", "das", "ihn", "durc\u00b7hrauschte", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das purpurne Gewand,", "tokens": ["Das", "pur\u00b7pur\u00b7ne", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Darauf so stolz in goldnem Grund", "tokens": ["Da\u00b7rauf", "so", "stolz", "in", "gold\u00b7nem", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein rother L\u00f6we stand?", "tokens": ["Ein", "ro\u00b7ther", "L\u00f6\u00b7we", "stand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Verschollen sind die Lieder", "tokens": ["Ver\u00b7schol\u00b7len", "sind", "die", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des deutschen Minnesangs,", "tokens": ["Des", "deut\u00b7schen", "Min\u00b7ne\u00b7sangs", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verblichen auf dem Eiland", "tokens": ["Ver\u00b7bli\u00b7chen", "auf", "dem", "Ei\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Orient\u2019s M\u00e4hrchenglanz;", "tokens": ["Des", "O\u00b7ri\u00b7ent's", "M\u00e4hr\u00b7chen\u00b7glanz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der Minnehof der Sch\u00f6nheit,", "tokens": ["Der", "Min\u00b7ne\u00b7hof", "der", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Weisheit hochgelehrt,", "tokens": ["Die", "Weis\u00b7heit", "hoch\u00b7ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie wichen, seit verrostete", "tokens": ["Sie", "wi\u00b7chen", ",", "seit", "ver\u00b7ros\u00b7te\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Hohenstaufenschwert!", "tokens": ["Das", "Ho\u00b7hen\u00b7stau\u00b7fen\u00b7schwert", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und h\u00f6rst du, was die Welle", "tokens": ["Und", "h\u00f6rst", "du", ",", "was", "die", "Wel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch heute traurig singt?", "tokens": ["Noch", "heu\u00b7te", "trau\u00b7rig", "singt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was traurig wiederhallend", "tokens": ["Was", "trau\u00b7rig", "wie\u00b7der\u00b7hal\u00b7lend"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum hohen Norden klingt?", "tokens": ["Zum", "ho\u00b7hen", "Nor\u00b7den", "klingt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201ehier schlummert in zwei S\u00e4rgen", "tokens": ["\u201e", "hier", "schlum\u00b7mert", "in", "zwei", "S\u00e4r\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein goldner Kaisertraum,", "tokens": ["Ein", "gold\u00b7ner", "Kai\u00b7ser\u00b7traum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der einst umspannen wollte", "tokens": ["Der", "einst", "um\u00b7span\u00b7nen", "woll\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den ganzen Erdenraum.\u201c", "tokens": ["Den", "gan\u00b7zen", "Er\u00b7den\u00b7raum", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}