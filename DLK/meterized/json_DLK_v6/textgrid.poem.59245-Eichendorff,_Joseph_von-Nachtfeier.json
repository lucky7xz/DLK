{"textgrid.poem.59245": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachtfeier", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Decket Schlaf die weite Runde,", "tokens": ["De\u00b7cket", "Schlaf", "die", "wei\u00b7te", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df ich oft am Fenster lauschen,", "tokens": ["Mu\u00df", "ich", "oft", "am", "Fens\u00b7ter", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Str\u00f6me unten rauschen,", "tokens": ["Wie", "die", "Str\u00f6\u00b7me", "un\u00b7ten", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00e4der sausen k\u00fchl im Grunde,", "tokens": ["R\u00e4\u00b7der", "sau\u00b7sen", "k\u00fchl", "im", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mir ist so wohl zur Stunde;", "tokens": ["Und", "mir", "ist", "so", "wohl", "zur", "Stun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn hinab vom Felsenrande", "tokens": ["Denn", "hin\u00b7ab", "vom", "Fel\u00b7sen\u00b7ran\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sp\u00fcr ich Freiheit, uralt Sehnen,", "tokens": ["Sp\u00fcr", "ich", "Frei\u00b7heit", ",", "ur\u00b7alt", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fromm zerbrechend alle Bande,", "tokens": ["Fromm", "zer\u00b7bre\u00b7chend", "al\u00b7le", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "\u00dcber W\u00e4lder, Strom und Lande", "tokens": ["\u00dc\u00b7ber", "W\u00e4l\u00b7der", ",", "Strom", "und", "Lan\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Keck die gro\u00dfen Fl\u00fcgel dehnen.", "tokens": ["Keck", "die", "gro\u00b7\u00dfen", "Fl\u00fc\u00b7gel", "deh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was je Gro\u00dfes brach die Schranken,", "tokens": ["Was", "je", "Gro\u00b7\u00dfes", "brach", "die", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seh ich durch die Stille gehen,", "tokens": ["Seh", "ich", "durch", "die", "Stil\u00b7le", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Helden auf den Wolken stehen,", "tokens": ["Hel\u00b7den", "auf", "den", "Wol\u00b7ken", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ernsten Blickes, ohne Wanken,", "tokens": ["Erns\u00b7ten", "Bli\u00b7ckes", ",", "oh\u00b7ne", "Wan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und es wollen die Gedanken", "tokens": ["Und", "es", "wol\u00b7len", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit den guten Alten hausen,", "tokens": ["Mit", "den", "gu\u00b7ten", "Al\u00b7ten", "hau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich in ihr Gespr\u00e4ch vermischen,", "tokens": ["Sich", "in", "ihr", "Ge\u00b7spr\u00e4ch", "ver\u00b7mi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das da kommt in Waldesbrausen.", "tokens": ["Das", "da", "kommt", "in", "Wal\u00b7des\u00b7brau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Manchem f\u00fcllt's die Brust mit Grausen,", "tokens": ["Man\u00b7chem", "f\u00fcllt's", "die", "Brust", "mit", "Grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mich soll's laben und erfrischen!", "tokens": ["Mich", "soll's", "la\u00b7ben", "und", "er\u00b7fri\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Tag und Regung war entflohen,", "tokens": ["Tag", "und", "Re\u00b7gung", "war", "ent\u00b7flo\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcbern See nur kam Gel\u00e4ute", "tokens": ["\u00dc\u00b7bern", "See", "nur", "kam", "Ge\u00b7l\u00e4u\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "ADV", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die monderhellte Weite,", "tokens": ["Durch", "die", "mon\u00b7der\u00b7hell\u00b7te", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und rings brannten auf den hohen", "tokens": ["Und", "rings", "brann\u00b7ten", "auf", "den", "ho\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alpen still die bleichen Lohen,", "tokens": ["Al\u00b7pen", "still", "die", "blei\u00b7chen", "Lo\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ew'ge W\u00e4chter echter Weihe,", "tokens": ["Ew'\u00b7ge", "W\u00e4ch\u00b7ter", "ech\u00b7ter", "Wei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Als, erhoben vom Verderben", "tokens": ["Als", ",", "er\u00b7ho\u00b7ben", "vom", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "VVPP", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und vom Jammer, da die dreie", "tokens": ["Und", "vom", "Jam\u00b7mer", ",", "da", "die", "drei\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "$,", "KOUS", "ART", "CARD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Einsam traten in das Freie,", "tokens": ["Ein\u00b7sam", "tra\u00b7ten", "in", "das", "Frei\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Frei zu leben und zu sterben.", "tokens": ["Frei", "zu", "le\u00b7ben", "und", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und so wachen heute viele", "tokens": ["Und", "so", "wa\u00b7chen", "heu\u00b7te", "vie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam \u00fcber ihrem Kummer;", "tokens": ["Ein\u00b7sam", "\u00fc\u00b7ber", "ih\u00b7rem", "Kum\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unerquickt von falschem Schlummer,", "tokens": ["Un\u00b7er\u00b7quickt", "von", "fal\u00b7schem", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus des Wechsels wildem Spiele", "tokens": ["Aus", "des", "Wech\u00b7sels", "wil\u00b7dem", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schauend fromm nach einem Ziele.", "tokens": ["Schau\u00b7end", "fromm", "nach", "ei\u00b7nem", "Zie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch die \u00f6de, stumme Leere", "tokens": ["Durch", "die", "\u00f6\u00b7de", ",", "stum\u00b7me", "Lee\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJD", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fchl ich mich euch still verb\u00fcndet;", "tokens": ["F\u00fchl", "ich", "mich", "euch", "still", "ver\u00b7b\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ob der Tag das Recht verkehre,", "tokens": ["Ob", "der", "Tag", "das", "Recht", "ver\u00b7keh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ewig strahlt der Stern der Ehre,", "tokens": ["E\u00b7wig", "strahlt", "der", "Stern", "der", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "K\u00fchn in heil'ger Nacht entz\u00fcndet.", "tokens": ["K\u00fchn", "in", "heil'\u00b7ger", "Nacht", "ent\u00b7z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}