{"textgrid.poem.26717": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Regenstrom aus Felsenrissen,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Regenstrom aus Felsenrissen,", "tokens": ["Ein", "Re\u00b7gen\u00b7strom", "aus", "Fel\u00b7sen\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er kommt mit Donners Ungest\u00fcm,", "tokens": ["Er", "kommt", "mit", "Don\u00b7ners", "Un\u00b7ge\u00b7st\u00fcm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bergtr\u00fcmmer folgen seinen G\u00fcssen,", "tokens": ["Berg\u00b7tr\u00fcm\u00b7mer", "fol\u00b7gen", "sei\u00b7nen", "G\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Eichen st\u00fcrzen unter ihm;", "tokens": ["Und", "Ei\u00b7chen", "st\u00fcr\u00b7zen", "un\u00b7ter", "ihm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erstaunt, mit wollustvollem Grausen,", "tokens": ["Er\u00b7staunt", ",", "mit", "wol\u00b7lust\u00b7vol\u00b7lem", "Grau\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "H\u00f6rt ihn der Wanderer und lauscht,", "tokens": ["H\u00f6rt", "ihn", "der", "Wan\u00b7de\u00b7rer", "und", "lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er h\u00f6rt die Flut vom Felsen brausen,", "tokens": ["Er", "h\u00f6rt", "die", "Flut", "vom", "Fel\u00b7sen", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch wei\u00df er nicht, woher sie rauscht:", "tokens": ["Doch", "wei\u00df", "er", "nicht", ",", "wo\u00b7her", "sie", "rauscht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So str\u00f6men des Gesanges Wellen", "tokens": ["So", "str\u00f6\u00b7men", "des", "Ge\u00b7san\u00b7ges", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Hervor aus nie entdeckten Quellen.", "tokens": ["Her\u00b7vor", "aus", "nie", "ent\u00b7deck\u00b7ten", "Quel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Verb\u00fcndet mit den furchtbarn Wesen,", "tokens": ["Ver\u00b7b\u00fcn\u00b7det", "mit", "den", "furcht\u00b7barn", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die still des Lebens Faden drehn,", "tokens": ["Die", "still", "des", "Le\u00b7bens", "Fa\u00b7den", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer kann des S\u00e4ngers Zauber l\u00f6sen,", "tokens": ["Wer", "kann", "des", "S\u00e4n\u00b7gers", "Zau\u00b7ber", "l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer seinen T\u00f6nen widerstehn?", "tokens": ["Wer", "sei\u00b7nen", "T\u00f6\u00b7nen", "wi\u00b7der\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie mit dem Stab des G\u00f6tterboten", "tokens": ["Wie", "mit", "dem", "Stab", "des", "G\u00f6t\u00b7ter\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Beherrscht er das bewegte Herz,", "tokens": ["Be\u00b7herrscht", "er", "das", "be\u00b7weg\u00b7te", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er taucht es in das Reich der Toten,", "tokens": ["Er", "taucht", "es", "in", "das", "Reich", "der", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Er hebt es staunend himmelw\u00e4rts", "tokens": ["Er", "hebt", "es", "stau\u00b7nend", "him\u00b7mel\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.9": {"text": "Und wiegt es zwischen Ernst und Spiele", "tokens": ["Und", "wiegt", "es", "zwi\u00b7schen", "Ernst", "und", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Auf schwanker Leiter der Gef\u00fchle.", "tokens": ["Auf", "schwan\u00b7ker", "Lei\u00b7ter", "der", "Ge\u00b7f\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie wenn auf einmal in die Kreise", "tokens": ["Wie", "wenn", "auf", "ein\u00b7mal", "in", "die", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "APPR", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Freude, mit Gigantenschritt,", "tokens": ["Der", "Freu\u00b7de", ",", "mit", "Gi\u00b7gan\u00b7ten\u00b7schritt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geheimnisvoll nach Geisterweise", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7voll", "nach", "Geis\u00b7ter\u00b7wei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein ungeheures Schicksal tritt.", "tokens": ["Ein", "un\u00b7ge\u00b7heu\u00b7res", "Schick\u00b7sal", "tritt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da beugt sich jede Erdengr\u00f6\u00dfe", "tokens": ["Da", "beugt", "sich", "je\u00b7de", "Er\u00b7den\u00b7gr\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Fremdling aus der andern Welt,", "tokens": ["Dem", "Fremd\u00b7ling", "aus", "der", "an\u00b7dern", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Jubels nichtiges Get\u00f6se", "tokens": ["Des", "Ju\u00b7bels", "nich\u00b7ti\u00b7ges", "Ge\u00b7t\u00f6\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Verstummt, und jede Larve f\u00e4llt,", "tokens": ["Ver\u00b7stummt", ",", "und", "je\u00b7de", "Lar\u00b7ve", "f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und vor der Wahrheit m\u00e4chtgem Siege", "tokens": ["Und", "vor", "der", "Wahr\u00b7heit", "m\u00e4cht\u00b7gem", "Sie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Verschwindet jedes Werk der L\u00fcge.", "tokens": ["Ver\u00b7schwin\u00b7det", "je\u00b7des", "Werk", "der", "L\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So rafft von jeder eiteln B\u00fcrde,", "tokens": ["So", "rafft", "von", "je\u00b7der", "ei\u00b7teln", "B\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn des Gesanges Ruf erschallt,", "tokens": ["Wenn", "des", "Ge\u00b7san\u00b7ges", "Ruf", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Mensch sich auf zur Geisterw\u00fcrde", "tokens": ["Der", "Mensch", "sich", "auf", "zur", "Geis\u00b7ter\u00b7w\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und tritt in heilige Gewalt;", "tokens": ["Und", "tritt", "in", "hei\u00b7li\u00b7ge", "Ge\u00b7walt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den hohen G\u00f6ttern ist er eigen,", "tokens": ["Den", "ho\u00b7hen", "G\u00f6t\u00b7tern", "ist", "er", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihm darf nichts Irdisches sich nahn,", "tokens": ["Ihm", "darf", "nichts", "Ir\u00b7di\u00b7sches", "sich", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ADJA", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und jede andre Macht mu\u00df schweigen,", "tokens": ["Und", "je\u00b7de", "and\u00b7re", "Macht", "mu\u00df", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und kein Verh\u00e4ngnis f\u00e4llt ihn an,", "tokens": ["Und", "kein", "Ver\u00b7h\u00e4ng\u00b7nis", "f\u00e4llt", "ihn", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Es schwinden jedes Kummers Falten,", "tokens": ["Es", "schwin\u00b7den", "je\u00b7des", "Kum\u00b7mers", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Solang des Liedes Zauber walten.", "tokens": ["So\u00b7lang", "des", "Lie\u00b7des", "Zau\u00b7ber", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wie nach hoffnungslosem Sehnen,", "tokens": ["Und", "wie", "nach", "hoff\u00b7nungs\u00b7lo\u00b7sem", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach langer Trennung bitterm Schmerz,", "tokens": ["Nach", "lan\u00b7ger", "Tren\u00b7nung", "bit\u00b7term", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Kind mit hei\u00dfen Reuetr\u00e4nen", "tokens": ["Ein", "Kind", "mit", "hei\u00b7\u00dfen", "Reu\u00b7e\u00b7tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich st\u00fcrzt an seiner Mutter Herz,", "tokens": ["Sich", "st\u00fcrzt", "an", "sei\u00b7ner", "Mut\u00b7ter", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So f\u00fchrt zu seiner Jugend H\u00fctten,", "tokens": ["So", "f\u00fchrt", "zu", "sei\u00b7ner", "Ju\u00b7gend", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu seiner Unschuld reinem Gl\u00fcck,", "tokens": ["Zu", "sei\u00b7ner", "Un\u00b7schuld", "rei\u00b7nem", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vom fernen Ausland fremder Sitten", "tokens": ["Vom", "fer\u00b7nen", "Aus\u00b7land", "frem\u00b7der", "Sit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Fl\u00fcchtling der Gesang zur\u00fcck,", "tokens": ["Den", "Fl\u00fccht\u00b7ling", "der", "Ge\u00b7sang", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In der Natur getreuen Armen", "tokens": ["In", "der", "Na\u00b7tur", "ge\u00b7treu\u00b7en", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Von kalten Regeln zu erwarmen.", "tokens": ["Von", "kal\u00b7ten", "Re\u00b7geln", "zu", "er\u00b7war\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}