{"textgrid.poem.58142": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "180. Wer Eines will, ist immer still", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der eigne Will', das mancherlei Verlangen,", "tokens": ["Der", "eig\u00b7ne", "Will'", ",", "das", "man\u00b7cher\u00b7lei", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie gut es scheint, nur Unruh bringt und Pein;", "tokens": ["Wie", "gut", "es", "scheint", ",", "nur", "Un\u00b7ruh", "bringt", "und", "Pein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "$,", "ADV", "NN", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ganz lediglich vom Herren abzuhangen,", "tokens": ["Ganz", "le\u00b7di\u00b7glich", "vom", "Her\u00b7ren", "ab\u00b7zu\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "La\u00df Nacht und Tag dein Werk und Wille sein!", "tokens": ["La\u00df", "Nacht", "und", "Tag", "dein", "Werk", "und", "Wil\u00b7le", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "PPOSAT", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}