{"dta.poem.10319": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Mir tr&#228;umte einst von wildem Liebesgl&#252;hen,", "tokens": ["Mir", "tr", "&#228;", "um\u00b7te", "einst", "von", "wil\u00b7dem", "Lie\u00b7besgl", "&#252;", "hen", ","], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["NE", "NE", "$(", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$(", "VVINF", "$,"], "meter": "-+--+-+-++-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von h\u00fcbschen Locken, Myrthen und Resede,", "tokens": ["Von", "h\u00fcb\u00b7schen", "Lo\u00b7cken", ",", "Myr\u00b7then", "und", "Re\u00b7se\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von s\u00fc\u00dfen Lippen und von bittrer Rede,", "tokens": ["Von", "s\u00fc\u00b7\u00dfen", "Lip\u00b7pen", "und", "von", "bit\u00b7trer", "Re\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von d\u00fcstrer Lieder d\u00fcstern Melodien.", "tokens": ["Von", "d\u00fcst\u00b7rer", "Lie\u00b7der", "d\u00fcs\u00b7tern", "Me\u00b7lo\u00b7dien", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verblichen und verweht sind l\u00e4ngst die Tr\u00e4ume,", "tokens": ["Ver\u00b7bli\u00b7chen", "und", "ver\u00b7weht", "sind", "l\u00e4ngst", "die", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verweht ist gar mein liebstes Traumgebild!", "tokens": ["Ver\u00b7weht", "ist", "gar", "mein", "liebs\u00b7tes", "Traum\u00b7ge\u00b7bild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geblieben ist mir nur, was glutherf\u00fcllt", "tokens": ["Ge\u00b7blie\u00b7ben", "ist", "mir", "nur", ",", "was", "glut\u00b7her\u00b7f\u00fcllt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "$,", "PWS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich einst gegossen hab' in weiche Reime.", "tokens": ["Ich", "einst", "ge\u00b7gos\u00b7sen", "hab'", "in", "wei\u00b7che", "Rei\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du bleibst, verwaistes Lied! Verweh' jetzt auch,", "tokens": ["Du", "bleibst", ",", "ver\u00b7wais\u00b7tes", "Lied", "!", "Ver\u00b7weh'", "jetzt", "auch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$.", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und such' das Traumbild, das mir l\u00e4ngst entschwunden,", "tokens": ["Und", "such'", "das", "Traum\u00b7bild", ",", "das", "mir", "l\u00e4ngst", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und gr\u00fc\u00df' es mir, wenn du es aufgefunden \u2014", "tokens": ["Und", "gr\u00fc\u00df'", "es", "mir", ",", "wenn", "du", "es", "auf\u00b7ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem luft'gen Schatten send' ich luft'gen Hauch.", "tokens": ["Dem", "luft'\u00b7gen", "Schat\u00b7ten", "send'", "ich", "luft'\u00b7gen", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}