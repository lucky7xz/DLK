{"textgrid.poem.62364": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: So sind wir fr\u00f6hlich denn zum Ziel gekommen!", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So sind wir fr\u00f6hlich denn zum Ziel gekommen!", "tokens": ["So", "sind", "wir", "fr\u00f6h\u00b7lich", "denn", "zum", "Ziel", "ge\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchzogen ist ein weites, reiches Land,", "tokens": ["Durch\u00b7zo\u00b7gen", "ist", "ein", "wei\u00b7tes", ",", "rei\u00b7ches", "Land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo wir so manch lebendig Wort vernommen;", "tokens": ["Wo", "wir", "so", "manch", "le\u00b7ben\u00b7dig", "Wort", "ver\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PIAT", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es war ein tiefer Strom, an dessen Rand", "tokens": ["Es", "war", "ein", "tie\u00b7fer", "Strom", ",", "an", "des\u00b7sen", "Rand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In leichter Barke wir so froh geschwommen;", "tokens": ["In", "leich\u00b7ter", "Bar\u00b7ke", "wir", "so", "froh", "ge\u00b7schwom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch an dem holden Bl\u00fcten-Ufer stand", "tokens": ["Doch", "an", "dem", "hol\u00b7den", "Bl\u00fc\u00b7ten\u00b7U\u00b7fer", "stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Und ging ein Chor von herrlichen Gestalten \u2013", "tokens": ["Und", "ging", "ein", "Chor", "von", "herr\u00b7li\u00b7chen", "Ge\u00b7stal\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "O strebet, sie euch ewig fest zu halten!", "tokens": ["O", "stre\u00b7bet", ",", "sie", "euch", "e\u00b7wig", "fest", "zu", "hal\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "PPER", "ADJD", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vom sel'gen Anschaun ist der Blick noch trunken.", "tokens": ["Vom", "sel'\u00b7gen", "An\u00b7schaun", "ist", "der", "Blick", "noch", "trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Da lebten Bilder auf, da spr\u00fchten Funken", "tokens": ["Da", "leb\u00b7ten", "Bil\u00b7der", "auf", ",", "da", "spr\u00fch\u00b7ten", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch unsre Seelen, l\u00f6send Schlo\u00df und Riegel.", "tokens": ["Durch", "uns\u00b7re", "See\u00b7len", ",", "l\u00f6\u00b7send", "Schlo\u00df", "und", "Rie\u00b7gel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als wir in Andacht vor ihr hingesunken,", "tokens": ["Als", "wir", "in", "An\u00b7dacht", "vor", "ihr", "hin\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Entsprossen schmerzlich s\u00fc\u00df die Liebesfl\u00fcgel,", "tokens": ["Ent\u00b7spros\u00b7sen", "schmerz\u00b7lich", "s\u00fc\u00df", "die", "Lie\u00b7bes\u00b7fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Was die Platone und die Diotimen", "tokens": ["Was", "die", "Pla\u00b7to\u00b7ne", "und", "die", "Dio\u00b7ti\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "F\u00fcr aller Seligkeit Beginnen r\u00fchmen.", "tokens": ["F\u00fcr", "al\u00b7ler", "Se\u00b7lig\u00b7keit", "Be\u00b7gin\u00b7nen", "r\u00fch\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das herrliche Verm\u00f6gen, diesen Traum", "tokens": ["Das", "herr\u00b7li\u00b7che", "Ver\u00b7m\u00f6\u00b7gen", ",", "die\u00b7sen", "Traum"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verk\u00f6rpert in das Leben einzuf\u00fchren,", "tokens": ["Ver\u00b7k\u00f6r\u00b7pert", "in", "das", "Le\u00b7ben", "ein\u00b7zu\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den \u00f6den, wesenlosen, todten Raum", "tokens": ["Den", "\u00f6\u00b7den", ",", "we\u00b7sen\u00b7lo\u00b7sen", ",", "tod\u00b7ten", "Raum"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit himmlischen Gebilden auszuzieren", "tokens": ["Mit", "himm\u00b7li\u00b7schen", "Ge\u00b7bil\u00b7den", "aus\u00b7zu\u00b7zie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und fest zu halten an des Kleides Saum", "tokens": ["Und", "fest", "zu", "hal\u00b7ten", "an", "des", "Klei\u00b7des", "Saum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die G\u00f6ttin \u2013 was nur wenig Priester sp\u00fcren", "tokens": ["Die", "G\u00f6t\u00b7tin", "\u2013", "was", "nur", "we\u00b7nig", "Pries\u00b7ter", "sp\u00fc\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PWS", "ADV", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und froh bekennen als des Himmels Gunst,", "tokens": ["Und", "froh", "be\u00b7ken\u00b7nen", "als", "des", "Him\u00b7mels", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Solch Sehnen, solche Kraft, wir nannten's ", "tokens": ["Solch", "Seh\u00b7nen", ",", "sol\u00b7che", "Kraft", ",", "wir", "nann\u00b7ten's"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PPER", "NE"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Und eine Insel hob sich aus den Wellen,", "tokens": ["Und", "ei\u00b7ne", "In\u00b7sel", "hob", "sich", "aus", "den", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da weilt die ", "tokens": ["Da", "weilt", "die"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Es ruht Petrarka sinnend an den Quellen,", "tokens": ["Es", "ruht", "Pe\u00b7trar\u00b7ka", "sin\u00b7nend", "an", "den", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Lorbeer soll sich Laura ihm vereinen;", "tokens": ["Im", "Lor\u00b7beer", "soll", "sich", "Lau\u00b7ra", "ihm", "ver\u00b7ei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PRF", "NE", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ariosto will die Nacht um ihn erhellen,", "tokens": ["A\u00b7rios\u00b7to", "will", "die", "Nacht", "um", "ihn", "er\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "L\u00e4\u00dft Ritter, Damen, Zauberer erscheinen \u2013", "tokens": ["L\u00e4\u00dft", "Rit\u00b7ter", ",", "Da\u00b7men", ",", "Zau\u00b7be\u00b7rer", "er\u00b7schei\u00b7nen", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "$,", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Vor allen aber ist der Preis beschieden", "tokens": ["Vor", "al\u00b7len", "a\u00b7ber", "ist", "der", "Preis", "be\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ADV", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dem ew'gen Klang, dem Wort des M\u00e4oniden.", "tokens": ["Dem", "ew'\u00b7gen", "Klang", ",", "dem", "Wort", "des", "M\u00e4o\u00b7ni\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Schon glaubten wir die sch\u00f6ne Fahrt geendet,", "tokens": ["Schon", "glaub\u00b7ten", "wir", "die", "sch\u00f6\u00b7ne", "Fahrt", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ward noch eine G\u00f6ttin uns gesandt.", "tokens": ["Da", "ward", "noch", "ei\u00b7ne", "G\u00f6t\u00b7tin", "uns", "ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr klarer Blick war himmelan gewendet,", "tokens": ["Ihr", "kla\u00b7rer", "Blick", "war", "him\u00b7me\u00b7lan", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch Siegern gleich durchschritt sie jedes Land,", "tokens": ["Doch", "Sie\u00b7gern", "gleich", "durch\u00b7schritt", "sie", "je\u00b7des", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-----+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Vom ew'gen Recht schien sie herabgesendet,", "tokens": ["Vom", "ew'\u00b7gen", "Recht", "schien", "sie", "her\u00b7ab\u00b7ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Ein schlankes Richtmaa\u00df zierte ihre Hand,", "tokens": ["Ein", "schlan\u00b7kes", "Richt\u00b7ma\u00b7a\u00df", "zier\u00b7te", "ih\u00b7re", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Zum F\u00fchrer an verworrenen Gestaden", "tokens": ["Zum", "F\u00fch\u00b7rer", "an", "ver\u00b7wor\u00b7re\u00b7nen", "Ge\u00b7sta\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bot sie uns Ariadnen gleich den Faden. \u2013", "tokens": ["Bot", "sie", "uns", "A\u00b7riad\u00b7nen", "gleich", "den", "Fa\u00b7den", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPER", "NN", "ADV", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Dies ist das Land, wohin sich sehnt hienieden,", "tokens": ["Dies", "ist", "das", "Land", ",", "wo\u00b7hin", "sich", "sehnt", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "PRF", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wen je ein Strahl von obenher beseelet,", "tokens": ["Wen", "je", "ein", "Strahl", "von", "o\u00b7ben\u00b7her", "be\u00b7see\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das sel'ge Land, wo Streit sich l\u00f6st in Frieden,", "tokens": ["Das", "sel'\u00b7ge", "Land", ",", "wo", "Streit", "sich", "l\u00f6st", "in", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "NN", "PRF", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Sch\u00f6nheit nur der Sch\u00f6nheit sich verm\u00e4hlet;", "tokens": ["Und", "Sch\u00f6n\u00b7heit", "nur", "der", "Sch\u00f6n\u00b7heit", "sich", "ver\u00b7m\u00e4h\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch ist nicht Jedem solches Gl\u00fcck beschieden,", "tokens": ["Doch", "ist", "nicht", "Je\u00b7dem", "sol\u00b7ches", "Gl\u00fcck", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Viel sind berufen, wenig sind erw\u00e4hlet,", "tokens": ["Viel", "sind", "be\u00b7ru\u00b7fen", ",", "we\u00b7nig", "sind", "er\u00b7w\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nur frommem Kindessinn ward es bereitet,", "tokens": ["Nur", "from\u00b7mem", "Kin\u00b7des\u00b7sinn", "ward", "es", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So hat es uns der Hierophant gedeutet.", "tokens": ["So", "hat", "es", "uns", "der", "Hie\u00b7ro\u00b7phant", "ge\u00b7deu\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Vollendet hat Er, will uns nun verschwinden,", "tokens": ["Voll\u00b7en\u00b7det", "hat", "Er", ",", "will", "uns", "nun", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word