{"textgrid.poem.35238": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein betr\u00fcbter Sch\u00e4fersmann,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein betr\u00fcbter Sch\u00e4fersmann,", "tokens": ["Ein", "be\u00b7tr\u00fcb\u00b7ter", "Sch\u00e4\u00b7fers\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weidend seine Wollenheerde,", "tokens": ["Wei\u00b7dend", "sei\u00b7ne", "Wol\u00b7len\u00b7heer\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da der Felsen von der Erde", "tokens": ["Da", "der", "Fel\u00b7sen", "von", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aufstieg, gleichsam himmelan:", "tokens": ["Auf\u00b7stieg", ",", "gleich\u00b7sam", "him\u00b7me\u00b7lan", ":"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als nun seine Schafe tischten,", "tokens": ["Als", "nun", "sei\u00b7ne", "Scha\u00b7fe", "tischten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sieht er aus dem trocknen Stein", "tokens": ["Sieht", "er", "aus", "dem", "trock\u00b7nen", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wasser triefen felsenein,", "tokens": ["Was\u00b7ser", "trie\u00b7fen", "fel\u00b7sen\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Davon sich die Augen frischten.", "tokens": ["Da\u00b7von", "sich", "die", "Au\u00b7gen", "frischten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Ach, sprach er, in sich entr\u00fcst':", "tokens": ["Ach", ",", "sprach", "er", ",", "in", "sich", "ent\u00b7r\u00fcst'", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "$,", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Hoff', da nichts zu hoffen ist!", "tokens": ["Hoff'", ",", "da", "nichts", "zu", "hof\u00b7fen", "ist", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PIS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nachdem st\u00fcrmten durch das Gras", "tokens": ["Nach\u00b7dem", "st\u00fcrm\u00b7ten", "durch", "das", "Gras"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolkenwinde, Donnerblitzen,", "tokens": ["Wol\u00b7ken\u00b7win\u00b7de", ",", "Don\u00b7ner\u00b7blit\u00b7zen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als in dieses Felsens Ritzen", "tokens": ["Als", "in", "die\u00b7ses", "Fel\u00b7sens", "Rit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PDAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Turteltaube sa\u00df.", "tokens": ["Ei\u00b7ne", "Tur\u00b7tel\u00b7tau\u00b7be", "sa\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann die schweren Wetter drohen,", "tokens": ["Wann", "die", "schwe\u00b7ren", "Wet\u00b7ter", "dro\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Suchet jeder Schutz und Hut.", "tokens": ["Su\u00b7chet", "je\u00b7der", "Schutz", "und", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sie war schnell dahin geflohen,", "tokens": ["Sie", "war", "schnell", "da\u00b7hin", "ge\u00b7flo\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PAV", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Da sich sicher sitzt und ruht.", "tokens": ["Da", "sich", "si\u00b7cher", "sitzt", "und", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ach, sprach er, in sich entr\u00fcst':", "tokens": ["Ach", ",", "sprach", "er", ",", "in", "sich", "ent\u00b7r\u00fcst'", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "$,", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Hoff', da nichts zu hoffen ist!", "tokens": ["Hoff'", ",", "da", "nichts", "zu", "hof\u00b7fen", "ist", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PIS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Unter n\u00e4chstem Weidenbaum", "tokens": ["Un\u00b7ter", "n\u00e4chs\u00b7tem", "Wei\u00b7den\u00b7baum"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Trieb er, vor des Wetters Flammen,", "tokens": ["Trieb", "er", ",", "vor", "des", "Wet\u00b7ters", "Flam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Heerde bald zusammen,", "tokens": ["Sei\u00b7ne", "Heer\u00b7de", "bald", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie alle hatten Raum,", "tokens": ["Da\u00df", "sie", "al\u00b7le", "hat\u00b7ten", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich zu sch\u00fctzen vor dem Regen.", "tokens": ["Sich", "zu", "sch\u00fct\u00b7zen", "vor", "dem", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bald die Winde wurden still,", "tokens": ["Bald", "die", "Win\u00b7de", "wur\u00b7den", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Sonn' ihm kam entgegen,", "tokens": ["Und", "die", "Sonn'", "ihm", "kam", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und er sang zum Sch\u00e4ferspiel:", "tokens": ["Und", "er", "sang", "zum", "Sch\u00e4\u00b7fer\u00b7spiel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Hoffnung deine Seele frist'!", "tokens": ["Hoff\u00b7nung", "dei\u00b7ne", "See\u00b7le", "frist'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Hoff', da nichts zu hoffen ist!", "tokens": ["Hoff'", ",", "da", "nichts", "zu", "hof\u00b7fen", "ist", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PIS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, was, sagt er, nach und nach", "tokens": ["Ach", ",", "was", ",", "sagt", "er", ",", "nach", "und", "nach"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PRELS", "$,", "VVFIN", "PPER", "$,", "APPR", "KON", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denk' ich doch mit Fehlverlangen!", "tokens": ["Denk'", "ich", "doch", "mit", "Fehl\u00b7ver\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab' ich denn nicht Trost empfangen", "tokens": ["Hab'", "ich", "denn", "nicht", "Trost", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von des Felsens Thr\u00e4nenbach?", "tokens": ["Von", "des", "Fel\u00b7sens", "Thr\u00e4\u00b7nen\u00b7bach", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von der Taube sonder Gatten,", "tokens": ["Von", "der", "Tau\u00b7be", "son\u00b7der", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Welche hier in Gr\u00fcften lebt?", "tokens": ["Wel\u00b7che", "hier", "in", "Gr\u00fcf\u00b7ten", "lebt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Von der Weiden Schutz und Schatten,", "tokens": ["Von", "der", "Wei\u00b7den", "Schutz", "und", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der ob meinem Haupte schwebt?", "tokens": ["Der", "ob", "mei\u00b7nem", "Haup\u00b7te", "schwebt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ich hoff', als ein frommer Christ,", "tokens": ["Ich", "hoff'", ",", "als", "ein", "from\u00b7mer", "Christ", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Da auch nichts zu hoffen ist.", "tokens": ["Da", "auch", "nichts", "zu", "hof\u00b7fen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}