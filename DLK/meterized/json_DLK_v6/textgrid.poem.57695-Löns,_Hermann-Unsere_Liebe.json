{"textgrid.poem.57695": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Unsere Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Weidenr\u00f6schen bedecken", "tokens": ["Die", "Wei\u00b7den\u00b7r\u00f6\u00b7schen", "be\u00b7de\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+----", "measure": "unknown.measure.di"}, "line.2": {"text": "Die Bl\u00f6\u00dfe mit Purpurpracht,", "tokens": ["Die", "Bl\u00f6\u00b7\u00dfe", "mit", "Pur\u00b7pur\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Durch rote Tannenst\u00e4mme", "tokens": ["Durch", "ro\u00b7te", "Tan\u00b7nen\u00b7st\u00e4m\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die goldene Sonne lacht.", "tokens": ["Die", "gol\u00b7de\u00b7ne", "Son\u00b7ne", "lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Wind treibt goldene Wellen", "tokens": ["Der", "Wind", "treibt", "gol\u00b7de\u00b7ne", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00dcber den blauen See,", "tokens": ["\u00dc\u00b7ber", "den", "blau\u00b7en", "See", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ein gro\u00dfer goldener Vogel", "tokens": ["Ein", "gro\u00b7\u00dfer", "gol\u00b7de\u00b7ner", "Vo\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schwebt langsam auf zur H\u00f6h'.", "tokens": ["Schwebt", "lang\u00b7sam", "auf", "zur", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wir folgen ihm mit den Augen", "tokens": ["Wir", "fol\u00b7gen", "ihm", "mit", "den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und sehen uns l\u00e4chelnd an,", "tokens": ["Und", "se\u00b7hen", "uns", "l\u00e4\u00b7chelnd", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So hoch wie unsere Liebe", "tokens": ["So", "hoch", "wie", "un\u00b7se\u00b7re", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Er niemals fliegen kann.", "tokens": ["Er", "nie\u00b7mals", "flie\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}