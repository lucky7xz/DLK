{"textgrid.poem.55150": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "N\u00e4he", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie du mir oft, geliebtes Kind,", "tokens": ["Wie", "du", "mir", "oft", ",", "ge\u00b7lieb\u00b7tes", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wei\u00df nicht wie, so fremde bist,", "tokens": ["Ich", "wei\u00df", "nicht", "wie", ",", "so", "frem\u00b7de", "bist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOKOM", "$,", "ADV", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn wir im Schwarm der vielen Menschen sind,", "tokens": ["Wenn", "wir", "im", "Schwarm", "der", "vie\u00b7len", "Men\u00b7schen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das schl\u00e4gt mir alle Freude nieder.", "tokens": ["Das", "schl\u00e4gt", "mir", "al\u00b7le", "Freu\u00b7de", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch ja, wenn alles still und finster um uns ist,", "tokens": ["Doch", "ja", ",", "wenn", "al\u00b7les", "still", "und", "fins\u00b7ter", "um", "uns", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PIS", "ADJD", "KON", "ADJD", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erkenn ich dich an deinen K\u00fcssen wieder.", "tokens": ["Er\u00b7kenn", "ich", "dich", "an", "dei\u00b7nen", "K\u00fcs\u00b7sen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}