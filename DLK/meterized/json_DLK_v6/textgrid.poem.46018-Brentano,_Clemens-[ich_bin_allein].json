{"textgrid.poem.46018": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[ich bin allein]", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin allein,", "tokens": ["Ich", "bin", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Viel Sonnenschein", "tokens": ["Viel", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Liegt um das Haus,", "tokens": ["Liegt", "um", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Doch wie ein welker Blumenstrau\u00df", "tokens": ["Doch", "wie", "ein", "wel\u00b7ker", "Blu\u00b7men\u00b7strau\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mu\u00df einsam sein,", "tokens": ["Mu\u00df", "ein\u00b7sam", "sein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Bis Blatt vor Blatt", "tokens": ["Bis", "Blatt", "vor", "Blatt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ganz lebenssatt,", "tokens": ["Ganz", "le\u00b7bens\u00b7satt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Zum andern Ziel", "tokens": ["Zum", "an\u00b7dern", "Ziel"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Der leichtgesinnten L\u00fcfte Spiel", "tokens": ["Der", "leicht\u00b7ge\u00b7sinn\u00b7ten", "L\u00fcf\u00b7te", "Spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Entf\u00fchret hat.", "tokens": ["Ent\u00b7f\u00fch\u00b7ret", "hat", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "So sterb' ich hier,", "tokens": ["So", "sterb'", "ich", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Als Todeszier", "tokens": ["Als", "To\u00b7des\u00b7zier"], "token_info": ["word", "word"], "pos": ["KOUS", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Streu' ich den Zug", "tokens": ["Streu'", "ich", "den", "Zug"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Von deinem Namen in den Flug", "tokens": ["Von", "dei\u00b7nem", "Na\u00b7men", "in", "den", "Flug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der L\u00fcfte mir.", "tokens": ["Der", "L\u00fcf\u00b7te", "mir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "O gutes Kind!", "tokens": ["O", "gu\u00b7tes", "Kind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Es weht der Wind,", "tokens": ["Es", "weht", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und was mich qu\u00e4lt,", "tokens": ["Und", "was", "mich", "qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wird deinen Blumen auch erz\u00e4hlt", "tokens": ["Wird", "dei\u00b7nen", "Blu\u00b7men", "auch", "er\u00b7z\u00e4hlt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die bl\u00fchend sind.", "tokens": ["Die", "bl\u00fc\u00b7hend", "sind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Die senken drum", "tokens": ["Die", "sen\u00b7ken", "drum"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PAV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ganz still und stumm", "tokens": ["Ganz", "still", "und", "stumm"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das reine Haupt", "tokens": ["Das", "rei\u00b7ne", "Haupt"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und sprechen: Selig wer da glaubt", "tokens": ["Und", "spre\u00b7chen", ":", "Se\u00b7lig", "wer", "da", "glaubt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$.", "NE", "PWS", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der k\u00f6mmt nicht um.", "tokens": ["Der", "k\u00f6mmt", "nicht", "um", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und so auch heut,", "tokens": ["Und", "so", "auch", "heut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Da du erfreut", "tokens": ["Da", "du", "er\u00b7freut"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Im Gr\u00fcnen gehst", "tokens": ["Im", "Gr\u00fc\u00b7nen", "gehst"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Hat sich, was du in Liebe s\u00e4est", "tokens": ["Hat", "sich", ",", "was", "du", "in", "Lie\u00b7be", "s\u00e4\u00b7est"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "$,", "PWS", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ins Kreuz gestreut.", "tokens": ["Ins", "Kreuz", "ge\u00b7streut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "So zwing' ich dich,", "tokens": ["So", "zwing'", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PRF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "So schwing' ich mich", "tokens": ["So", "schwing'", "ich", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Zu deiner Huld,", "tokens": ["Zu", "dei\u00b7ner", "Huld", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Im Zeichen ewiger Geduld", "tokens": ["Im", "Zei\u00b7chen", "e\u00b7wi\u00b7ger", "Ge\u00b7duld"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vollbringe ich.", "tokens": ["Voll\u00b7brin\u00b7ge", "ich", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}