{"dta.poem.18288": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Unser Wortschatz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Rechneten Wort f\u00fcr Wort zur\u00fcck", "tokens": ["Rech\u00b7ne\u00b7ten", "Wort", "f\u00fcr", "Wort", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN", "PTKVZ"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und sahn: der Schatz des gro\u00dfen Britten", "tokens": ["Und", "sahn", ":", "der", "Schatz", "des", "gro\u00b7\u00dfen", "Brit\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Umfa\u00dfte 15,000 St\u00fcck!", "tokens": ["Um\u00b7fa\u00df\u00b7te", "15,000", "St\u00fcck", "!"], "token_info": ["word", "number", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Doch heut im neunzehnten Jahrhundert", "tokens": ["Doch", "heut", "im", "neun\u00b7zehn\u00b7ten", "Jahr\u00b7hun\u00b7dert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Dinger wie der Wind verwehn:", "tokens": ["Die", "Din\u00b7ger", "wie", "der", "Wind", "ver\u00b7wehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Droschkenkutscher braucht f\u00fcnfhundert,", "tokens": ["Ein", "Droschken\u00b7kut\u00b7scher", "braucht", "f\u00fcnf\u00b7hun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein lyrischer Dichter nur circa zehn!", "tokens": ["Ein", "ly\u00b7ri\u00b7scher", "Dich\u00b7ter", "nur", "cir\u00b7ca", "zehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "NE", "CARD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}