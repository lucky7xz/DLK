{"textgrid.poem.64878": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alles hinzugeben", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles hinzugeben", "tokens": ["Al\u00b7les", "hin\u00b7zu\u00b7ge\u00b7ben"], "token_info": ["word", "word"], "pos": ["PIS", "VVIZU"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ist der Liebe Brauch;", "tokens": ["Ist", "der", "Lie\u00b7be", "Brauch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nimm denn hin mein Leben,", "tokens": ["Nimm", "denn", "hin", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "PPOSAT", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Und mein Sterben auch!", "tokens": ["Und", "mein", "Ster\u00b7ben", "auch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Aller meiner Lieder", "tokens": ["Al\u00b7ler", "mei\u00b7ner", "Lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sanften Schmeichellaut,", "tokens": ["Sanf\u00b7ten", "Schmei\u00b7chel\u00b7laut", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die ein Eden wieder", "tokens": ["Die", "ein", "E\u00b7den", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sich aus Schutt erbaut;", "tokens": ["Sich", "aus", "Schutt", "er\u00b7baut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Alle Lichtgedanken,", "tokens": ["Al\u00b7le", "Licht\u00b7ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die an Gl\u00fcck und Leid", "tokens": ["Die", "an", "Gl\u00fcck", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "K\u00fchn sich aufw\u00e4rts ranken", "tokens": ["K\u00fchn", "sich", "auf\u00b7w\u00e4rts", "ran\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PRF", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In die Ewigkeit.", "tokens": ["In", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "All mein stilles Sehnen,", "tokens": ["All", "mein", "stil\u00b7les", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Innig dir vertraut,", "tokens": ["In\u00b7nig", "dir", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Das in sel'gen Thr\u00e4nen", "tokens": ["Das", "in", "sel'\u00b7gen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "ADJA", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Auf dich niederthaut!", "tokens": ["Auf", "dich", "nie\u00b7dert\u00b7haut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Nimm, da\u00df nichts dir fehle,", "tokens": ["Nimm", ",", "da\u00df", "nichts", "dir", "feh\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wenn die Stunde ruft,", "tokens": ["Wenn", "die", "Stun\u00b7de", "ruft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Meine ganze Seele", "tokens": ["Mei\u00b7ne", "gan\u00b7ze", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Hin als Opferduft!", "tokens": ["Hin", "als", "Op\u00b7fer\u00b7duft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}