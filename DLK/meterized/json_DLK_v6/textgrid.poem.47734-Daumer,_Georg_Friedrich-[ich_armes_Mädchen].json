{"textgrid.poem.47734": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich armes M\u00e4dchen]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich armes M\u00e4dchen,", "tokens": ["Ich", "ar\u00b7mes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ich arme Waise,", "tokens": ["Ich", "ar\u00b7me", "Wai\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Gewohnt, zu darben", "tokens": ["Ge\u00b7wohnt", ",", "zu", "dar\u00b7ben"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$,", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Im Jammergleise!", "tokens": ["Im", "Jam\u00b7mer\u00b7glei\u00b7se", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "O wenn mir noch", "tokens": ["O", "wenn", "mir", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die Mutter lebte,", "tokens": ["Die", "Mut\u00b7ter", "leb\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Ihr sanftes Herz", "tokens": ["Ihr", "sanf\u00b7tes", "Herz"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Entgegenbebte,", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7beb\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Von Trost und Treue", "tokens": ["Von", "Trost", "und", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Der reinste Quell!", "tokens": ["Der", "reins\u00b7te", "Quell", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Ach, schon so lange,", "tokens": ["Ach", ",", "schon", "so", "lan\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Bedeckt vom H\u00fcgel,", "tokens": ["Be\u00b7deckt", "vom", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Du Gute, ruhst du!", "tokens": ["Du", "Gu\u00b7te", ",", "ruhst", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Es blinkt und zittert", "tokens": ["Es", "blinkt", "und", "zit\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Auf deinem Grabe", "tokens": ["Auf", "dei\u00b7nem", "Gra\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Der Thau der Blume", "tokens": ["Der", "Thau", "der", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Wie Silber hell.", "tokens": ["Wie", "Sil\u00b7ber", "hell", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}