{"textgrid.poem.61278": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Am Bergsee", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Bergsee, wo die Wipfel steigen,", "tokens": ["Am", "Berg\u00b7see", ",", "wo", "die", "Wip\u00b7fel", "stei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bis in die Nacht hab' ich gelauscht,", "tokens": ["Bis", "in", "die", "Nacht", "hab'", "ich", "ge\u00b7lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da hat der Wald mit seinen Zweigen", "tokens": ["Da", "hat", "der", "Wald", "mit", "sei\u00b7nen", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die alte Zeit mir wach gerauscht:", "tokens": ["Die", "al\u00b7te", "Zeit", "mir", "wach", "ge\u00b7rauscht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Zeit, die nach zu kurzem Schimmer", "tokens": ["Die", "Zeit", ",", "die", "nach", "zu", "kur\u00b7zem", "Schim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie eine Sonn' hinabgegl\u00fcht,", "tokens": ["Wie", "ei\u00b7ne", "Sonn'", "hin\u00b7ab\u00b7ge\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von der ein Nachglanz mir noch immer", "tokens": ["Von", "der", "ein", "Nach\u00b7glanz", "mir", "noch", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "PPER", "ADV", "ADV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wie Sp\u00e4trot in der Seele bl\u00fcht;", "tokens": ["Wie", "Sp\u00e4t\u00b7rot", "in", "der", "See\u00b7le", "bl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Zeit, da ich mit dir geschritten,", "tokens": ["Die", "Zeit", ",", "da", "ich", "mit", "dir", "ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geliebtes Kind, im tiefen Hag,", "tokens": ["Ge\u00b7lieb\u00b7tes", "Kind", ",", "im", "tie\u00b7fen", "Hag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da ich in hoher Buchen Mitten", "tokens": ["Da", "ich", "in", "ho\u00b7her", "Bu\u00b7chen", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu deinen F\u00fc\u00dfen tr\u00e4umend lag;", "tokens": ["Zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "tr\u00e4u\u00b7mend", "lag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da du dein Aug' in meines senktest", "tokens": ["Da", "du", "dein", "Aug'", "in", "mei\u00b7nes", "senk\u00b7test"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und l\u00e4chelnd bald und weinend bald", "tokens": ["Und", "l\u00e4\u00b7chelnd", "bald", "und", "wei\u00b7nend", "bald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "KON", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir deine junge Seele schenktest,", "tokens": ["Mir", "dei\u00b7ne", "jun\u00b7ge", "See\u00b7le", "schenk\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und niemand wu\u00dft' es als der Wald;", "tokens": ["Und", "nie\u00b7mand", "wu\u00dft'", "es", "als", "der", "Wald", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da deine H\u00e4nde mich gesegnet,", "tokens": ["Da", "dei\u00b7ne", "H\u00e4n\u00b7de", "mich", "ge\u00b7seg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und deine Lippen fromm gefeit", "tokens": ["Und", "dei\u00b7ne", "Lip\u00b7pen", "fromm", "ge\u00b7feit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den meinen sanft im Ku\u00df begegnet", "tokens": ["Den", "mei\u00b7nen", "sanft", "im", "Ku\u00df", "be\u00b7geg\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sie zu reinem Lied geweiht.", "tokens": ["Und", "sie", "zu", "rei\u00b7nem", "Lied", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O Zeit der Liebe, Zeit der Lieder,", "tokens": ["O", "Zeit", "der", "Lie\u00b7be", ",", "Zeit", "der", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der stillen, gr\u00fcnen Waldeslust,", "tokens": ["Der", "stil\u00b7len", ",", "gr\u00fc\u00b7nen", "Wal\u00b7des\u00b7lust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie zog von dir ein Odem wieder", "tokens": ["Wie", "zog", "von", "dir", "ein", "O\u00b7dem", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sehns\u00fcchtig heut durch meine Brust!", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "heut", "durch", "mei\u00b7ne", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und du, die ewig mir erlesen", "tokens": ["Und", "du", ",", "die", "e\u00b7wig", "mir", "er\u00b7le\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PRELS", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In meines Herzens Tiefen ruht,", "tokens": ["In", "mei\u00b7nes", "Her\u00b7zens", "Tie\u00b7fen", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie gr\u00fc\u00dfte still mich all dein Wesen", "tokens": ["Wie", "gr\u00fc\u00df\u00b7te", "still", "mich", "all", "dein", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADJD", "PPER", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Laub und D\u00e4mmrung, Luft und Flut!", "tokens": ["Aus", "Laub", "und", "D\u00e4mm\u00b7rung", ",", "Luft", "und", "Flut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der n\u00e4chtlich tiefe Himmel blaute,", "tokens": ["Der", "n\u00e4cht\u00b7lich", "tie\u00b7fe", "Him\u00b7mel", "blau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf ging der Mond im dunklen See:", "tokens": ["Auf", "ging", "der", "Mond", "im", "dunk\u00b7len", "See", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir aber war's, dein Auge schaute", "tokens": ["Mir", "a\u00b7ber", "wa\u00b7r's", ",", "dein", "Au\u00b7ge", "schau\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu mir empor in stillem Weh.", "tokens": ["Zu", "mir", "em\u00b7por", "in", "stil\u00b7lem", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und da hinab die Bergeslehnen", "tokens": ["Und", "da", "hin\u00b7ab", "die", "Ber\u00b7ges\u00b7leh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wind den feuchten Wald durchstrich,", "tokens": ["Der", "Wind", "den", "feuch\u00b7ten", "Wald", "durch\u00b7strich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da fiel der Tau wie k\u00fchle Tr\u00e4nen,", "tokens": ["Da", "fiel", "der", "Tau", "wie", "k\u00fch\u00b7le", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}}, "stanza.10": {"line.1": {"text": "Da hielt ich's nicht. Mit wildem Klopfen", "tokens": ["Da", "hielt", "ich's", "nicht", ".", "Mit", "wil\u00b7dem", "Klop\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Unb\u00e4ndig quoll mein Herz empor,", "tokens": ["Un\u00b7b\u00e4n\u00b7dig", "quoll", "mein", "Herz", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hei\u00df vom Auge f\u00fchlt' ich's tropfen,", "tokens": ["Und", "hei\u00df", "vom", "Au\u00b7ge", "f\u00fchlt'", "ich's", "trop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie damals, da ich dich verlor.", "tokens": ["Wie", "da\u00b7mals", ",", "da", "ich", "dich", "ver\u00b7lor", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}