{"textgrid.poem.40612": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seit du dich von mir gewendet,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit du dich von mir gewendet,", "tokens": ["Seit", "du", "dich", "von", "mir", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00df ich erst, was du mir warst;", "tokens": ["Wei\u00df", "ich", "erst", ",", "was", "du", "mir", "warst", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "All der holde Zauber endet,", "tokens": ["All", "der", "hol\u00b7de", "Zau\u00b7ber", "en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Wunderring zerbarst.", "tokens": ["Und", "der", "Wun\u00b7der\u00b7ring", "zer\u00b7barst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als des Hauses gute Stunde", "tokens": ["Als", "des", "Hau\u00b7ses", "gu\u00b7te", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kamst und gingst du ein und aus,", "tokens": ["Kamst", "und", "gingst", "du", "ein", "und", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00f6hlich Wort auf heitrem Munde", "tokens": ["Fr\u00f6h\u00b7lich", "Wort", "auf", "heit\u00b7rem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchrtest du das Gl\u00fcck ins Haus.", "tokens": ["F\u00fchr\u00b7test", "du", "das", "Gl\u00fcck", "ins", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie der Lichtstrahl kamst du, Holde,", "tokens": ["Wie", "der", "Licht\u00b7strahl", "kamst", "du", ",", "Hol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der nur leuchten, w\u00e4rmen mag,", "tokens": ["Der", "nur", "leuch\u00b7ten", ",", "w\u00e4r\u00b7men", "mag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$,", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df von seinem klaren Golde", "tokens": ["Da\u00df", "von", "sei\u00b7nem", "kla\u00b7ren", "Gol\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heller noch der hellste Tag;", "tokens": ["Hel\u00b7ler", "noch", "der", "hells\u00b7te", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie das Mondlicht kamst du, S\u00fc\u00dfe,", "tokens": ["Wie", "das", "Mond\u00b7licht", "kamst", "du", ",", "S\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das nur zu verkl\u00e4ren strebt", "tokens": ["Das", "nur", "zu", "ver\u00b7kl\u00e4\u00b7ren", "strebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "PTKZU", "VVINF", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und die lichten Silbergr\u00fc\u00dfe", "tokens": ["Und", "die", "lich\u00b7ten", "Sil\u00b7ber\u00b7gr\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Still in dunkle Stunden webt;", "tokens": ["Still", "in", "dunk\u00b7le", "Stun\u00b7den", "webt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie ein Lenzhauch, mit Entz\u00fccken", "tokens": ["Wie", "ein", "Lenz\u00b7hauch", ",", "mit", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcllend Fluren und Gem\u00fcth,", "tokens": ["F\u00fcl\u00b7lend", "Flu\u00b7ren", "und", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der nicht prahlt: ich will begl\u00fccken!", "tokens": ["Der", "nicht", "prahlt", ":", "ich", "will", "be\u00b7gl\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVFIN", "$.", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der nur kommt \u2013 und Alles bl\u00fcht! \u2013 \u2013", "tokens": ["Der", "nur", "kommt", "\u2013", "und", "Al\u00b7les", "bl\u00fcht", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADV", "VVFIN", "$(", "KON", "PIS", "VVFIN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Was der G\u00f6tter Gunst verschwendet,", "tokens": ["Was", "der", "G\u00f6t\u00b7ter", "Gunst", "ver\u00b7schwen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kenn' ich jetzt, des Gl\u00fcckes bar;", "tokens": ["Kenn'", "ich", "jetzt", ",", "des", "Gl\u00fc\u00b7ckes", "bar", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seit sie sich von mir gewendet,", "tokens": ["Seit", "sie", "sich", "von", "mir", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00df ich erst, was sie mir war!", "tokens": ["Wei\u00df", "ich", "erst", ",", "was", "sie", "mir", "war", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PRELS", "PPER", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}