{"textgrid.poem.40732": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Nachruf an Presch\u00e9rn", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In w\u00fcrz'ger Luft, auf blumenbuntem Grunde", "tokens": ["In", "w\u00fcrz'\u00b7ger", "Luft", ",", "auf", "blu\u00b7men\u00b7bun\u00b7tem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ragt eine Linde neben einer Eiche,", "tokens": ["Ragt", "ei\u00b7ne", "Lin\u00b7de", "ne\u00b7ben", "ei\u00b7ner", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Zweige sanft verschr\u00e4nkt zum gr\u00fcnen Bunde,", "tokens": ["Die", "Zwei\u00b7ge", "sanft", "ver\u00b7schr\u00e4nkt", "zum", "gr\u00fc\u00b7nen", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als ob ein Freund dem Freund die H\u00e4nde reiche;", "tokens": ["Als", "ob", "ein", "Freund", "dem", "Freund", "die", "H\u00e4n\u00b7de", "rei\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ob hier das Blatt gezackt sei, dort sich's runde,", "tokens": ["Ob", "hier", "das", "Blatt", "ge\u00b7zackt", "sei", ",", "dort", "sich's", "run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "VAFIN", "$,", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Laubs und Schattens Farbe bleibt die gleiche!", "tokens": ["Des", "Laubs", "und", "Schat\u00b7tens", "Far\u00b7be", "bleibt", "die", "glei\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "VVFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Uns Nachbarkinder, spielend auf den Matten,", "tokens": ["Uns", "Nach\u00b7bar\u00b7kin\u00b7der", ",", "spie\u00b7lend", "auf", "den", "Mat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Umw\u00f6lbt des gr\u00fcnen Doms vereinter Schatten.", "tokens": ["Um\u00b7w\u00f6lbt", "des", "gr\u00fc\u00b7nen", "Doms", "ver\u00b7ein\u00b7ter", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da ward kredenzt Gluthwein vom letzten Jahre,", "tokens": ["Da", "ward", "kre\u00b7denzt", "Gluth\u00b7wein", "vom", "letz\u00b7ten", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVFIN", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Keltersegen schw\u00fcler Sonnenbr\u00e4nde,", "tokens": ["Der", "Kel\u00b7ter\u00b7se\u00b7gen", "schw\u00fc\u00b7ler", "Son\u00b7nen\u00b7br\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und als ob Feuer in die Adern fahre", "tokens": ["Und", "als", "ob", "Feu\u00b7er", "in", "die", "A\u00b7dern", "fah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOKOM", "KOUS", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Kampflust flogen an das Schwert die H\u00e4nde;", "tokens": ["In", "Kampf\u00b7lust", "flo\u00b7gen", "an", "das", "Schwert", "die", "H\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Reigen l\u00f6st das Volk, auf da\u00df sich's schaare", "tokens": ["Den", "Rei\u00b7gen", "l\u00f6st", "das", "Volk", ",", "auf", "da\u00df", "sich's", "schaa\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "APPR", "KOUS", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zur Linde hier, sich dort zur Eiche wende;", "tokens": ["Zur", "Lin\u00b7de", "hier", ",", "sich", "dort", "zur", "Ei\u00b7che", "wen\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "\u00bbhie Slave!\u00ab \u2013 \u00bbhie Germane!\u00ab scholl es grimmig", "tokens": ["\u00bb", "hie", "Sla\u00b7ve", "!", "\u00ab", "\u2013", "\u00bb", "hie", "Ger\u00b7ma\u00b7ne", "!", "\u00ab", "scholl", "es", "grim\u00b7mig"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "NE", "$.", "$(", "$(", "$(", "ADV", "NN", "$.", "$(", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Zornesworte brausten tausendstimmig.", "tokens": ["Und", "Zor\u00b7nes\u00b7wor\u00b7te", "braus\u00b7ten", "tau\u00b7sends\u00b7tim\u00b7mig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Noch schwoll der Zwist; da strich ein fl\u00fcsternd Klagen", "tokens": ["Noch", "schwoll", "der", "Zwist", ";", "da", "strich", "ein", "fl\u00fcs\u00b7ternd", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "$.", "ADV", "ADJD", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dahin durchs S\u00e4useln der Slovenenlinde,", "tokens": ["Da\u00b7hin", "durchs", "S\u00e4u\u00b7seln", "der", "Slo\u00b7ve\u00b7nen\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Zittern ging, als mocht' ein Herz ihr schlagen,", "tokens": ["Ein", "Zit\u00b7tern", "ging", ",", "als", "mocht'", "ein", "Herz", "ihr", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Stamm zum Wipfel ihr, vom Mark zur Rinde;", "tokens": ["Vom", "Stamm", "zum", "Wip\u00b7fel", "ihr", ",", "vom", "Mark", "zur", "Rin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "PPER", "$,", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von M\u00e4nnern ward ein Leichnam hergetragen,", "tokens": ["Von", "M\u00e4n\u00b7nern", "ward", "ein", "Leich\u00b7nam", "her\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie lehnten an den Stamm sein Haupt gelinde,", "tokens": ["Sie", "lehn\u00b7ten", "an", "den", "Stamm", "sein", "Haupt", "ge\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein Dichterhaupt! Dem Volke starb sein Seher;", "tokens": ["Ein", "Dich\u00b7ter\u00b7haupt", "!", "Dem", "Vol\u00b7ke", "starb", "sein", "Se\u00b7her", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ersch\u00fcttert trat ich von der Eiche n\u00e4her.", "tokens": ["Er\u00b7sch\u00fct\u00b7tert", "trat", "ich", "von", "der", "Ei\u00b7che", "n\u00e4\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er war mein Lehrer einst! Aus dumpfen Hallen", "tokens": ["Er", "war", "mein", "Leh\u00b7rer", "einst", "!", "Aus", "dum\u00b7pfen", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entf\u00fchrt' er mich zu Tiburs Musenfeste,", "tokens": ["Ent\u00b7f\u00fchrt'", "er", "mich", "zu", "Ti\u00b7burs", "Mu\u00b7sen\u00b7fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Wunderstrand, wo Maro's Helden wallen,", "tokens": ["Zum", "Wun\u00b7der\u00b7strand", ",", "wo", "Ma\u00b7ro's", "Hel\u00b7den", "wal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zur Laube, wo der Tejer Trauben pre\u00dfte,", "tokens": ["Zur", "Lau\u00b7be", ",", "wo", "der", "Te\u00b7jer", "Trau\u00b7ben", "pre\u00df\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zum Kap Sigeums, dran die Wogen prallen", "tokens": ["Zum", "Kap", "Si\u00b7geums", ",", "dran", "die", "Wo\u00b7gen", "pral\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NE", "$,", "PAV", "ART", "NN", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wie Waffentosen, bis zu Priam's Veste;", "tokens": ["Wie", "Waf\u00b7fen\u00b7to\u00b7sen", ",", "bis", "zu", "Pri\u00b7a\u00b7m's", "Ves\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "KOUS", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Sein Geisterschiff trug keine Flagg' am St\u00e4nder,", "tokens": ["Sein", "Geis\u00b7ter\u00b7schiff", "trug", "kei\u00b7ne", "Flag\u00b7g'", "am", "St\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Nicht blau-roth-wei\u00df', nicht schwarz-roth-goldne B\u00e4nder.", "tokens": ["Nicht", "blau\u00b7ro\u00b7th\u00b7wei\u00df'", ",", "nicht", "schwa\u00b7rz\u00b7ro\u00b7th\u00b7gold\u00b7ne", "B\u00e4n\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKVZ", "$,", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Wir sahn der Griechenfreiheit Todesbette,", "tokens": ["Wir", "sahn", "der", "Grie\u00b7chen\u00b7frei\u00b7heit", "To\u00b7des\u00b7bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir sahn im Blachfeld Rom und Hellas ringen,", "tokens": ["Wir", "sahn", "im", "Blach\u00b7feld", "Rom", "und", "Hel\u00b7las", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Sieger dann, sich schm\u00fcckend mit der Kette,", "tokens": ["Den", "Sie\u00b7ger", "dann", ",", "sich", "schm\u00fc\u00b7ckend", "mit", "der", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um des Besiegten Haupt den Lorbeer schlingen,", "tokens": ["Um", "des", "Be\u00b7sieg\u00b7ten", "Haupt", "den", "Lor\u00b7beer", "schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Kriegspfeil sinken vor des Marmors Gl\u00e4tte,", "tokens": ["Den", "Kriegs\u00b7pfeil", "sin\u00b7ken", "vor", "des", "Mar\u00b7mors", "Gl\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vom Hauch der mildern Sitte morsch die Klingen!", "tokens": ["Vom", "Hauch", "der", "mil\u00b7dern", "Sit\u00b7te", "morsch", "die", "Klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Im Glast zerbrochner R\u00f6merschwerter gleiten", "tokens": ["Im", "Glast", "zer\u00b7broch\u00b7ner", "R\u00f6\u00b7mer\u00b7schwer\u00b7ter", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mir Spiegelbilder sp\u00e4trer K\u00e4mpferzeiten.", "tokens": ["Mir", "Spie\u00b7gel\u00b7bil\u00b7der", "sp\u00e4t\u00b7rer", "K\u00e4mp\u00b7fer\u00b7zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Auf dieses Todten Herz, \u2013 das nie gewittert,", "tokens": ["Auf", "die\u00b7ses", "Tod\u00b7ten", "Herz", ",", "\u2013", "das", "nie", "ge\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$,", "$(", "PDS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geleuchtet nur, \u2013 leg' ich die H\u00e4nde gerne;", "tokens": ["Ge\u00b7leuch\u00b7tet", "nur", ",", "\u2013", "leg'", "ich", "die", "H\u00e4n\u00b7de", "ger\u00b7ne", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "$(", "VVIMP", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Weltenseele quillt, vom Markt zersplittert,", "tokens": ["Die", "Wel\u00b7ten\u00b7see\u00b7le", "quillt", ",", "vom", "Markt", "zer\u00b7split\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ins Dichterherz zu ruhigem klaren Kerne;", "tokens": ["Ins", "Dich\u00b7ter\u00b7herz", "zu", "ru\u00b7hi\u00b7gem", "kla\u00b7ren", "Ker\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+----+-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Das Licht, das rings verirrt in Funken zittert,", "tokens": ["Das", "Licht", ",", "das", "rings", "ver\u00b7irrt", "in", "Fun\u00b7ken", "zit\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im Dichterherzen sammelt sich's zum Sterne;", "tokens": ["Im", "Dich\u00b