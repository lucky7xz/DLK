{"textgrid.poem.56125": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Euch, die ihr nie mein Gef\u00fchl verlie\u00dft,", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Euch, die ihr nie mein Gef\u00fchl verlie\u00dft,", "tokens": ["Euch", ",", "die", "ihr", "nie", "mein", "Ge\u00b7f\u00fchl", "ver\u00b7lie\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "gr\u00fc\u00df ich, antikische Sarkophage,", "tokens": ["gr\u00fc\u00df", "ich", ",", "an\u00b7ti\u00b7ki\u00b7sche", "Sar\u00b7ko\u00b7pha\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "die das fr\u00f6hliche Wasser r\u00f6mischer Tage", "tokens": ["die", "das", "fr\u00f6h\u00b7li\u00b7che", "Was\u00b7ser", "r\u00f6\u00b7mi\u00b7scher", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "als ein wandelndes Lied durchflie\u00dft.", "tokens": ["als", "ein", "wan\u00b7deln\u00b7des", "Lied", "durch\u00b7flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Oder jene so offenen, wie das Aug", "tokens": ["O\u00b7der", "je\u00b7ne", "so", "of\u00b7fe\u00b7nen", ",", "wie", "das", "Aug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDS", "ADV", "ADJA", "$,", "PWAV", "ART", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "eines frohen erwachenden Hirten,", "tokens": ["ei\u00b7nes", "fro\u00b7hen", "er\u00b7wa\u00b7chen\u00b7den", "Hir\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "\u2013 innen voll Stille und Bienensaug \u2013", "tokens": ["\u2013", "in\u00b7nen", "voll", "Stil\u00b7le", "und", "Bie\u00b7nen\u00b7saug", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "NN", "KON", "NN", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "denen entz\u00fcckte Falter entschwirrten;", "tokens": ["de\u00b7nen", "ent\u00b7z\u00fcck\u00b7te", "Fal\u00b7ter", "ent\u00b7schwirr\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "alle, die man dem Zweifel entrei\u00dft,", "tokens": ["al\u00b7le", ",", "die", "man", "dem", "Zwei\u00b7fel", "ent\u00b7rei\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "gr\u00fc\u00df ich, die wiederge\u00f6ffneten Munde,", "tokens": ["gr\u00fc\u00df", "ich", ",", "die", "wie\u00b7der\u00b7ge\u00b7\u00f6ff\u00b7ne\u00b7ten", "Mun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "die schon wu\u00dften, was schweigen hei\u00dft.", "tokens": ["die", "schon", "wu\u00df\u00b7ten", ",", "was", "schwei\u00b7gen", "hei\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "PWS", "ADJA", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Wissen wirs, Freunde, wissen wirs nicht?", "tokens": ["Wis\u00b7sen", "wirs", ",", "Freun\u00b7de", ",", "wis\u00b7sen", "wirs", "nicht", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "NN", "$,", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Beides bildet die z\u00f6gernde Stunde", "tokens": ["Bei\u00b7des", "bil\u00b7det", "die", "z\u00f6\u00b7gern\u00b7de", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "in dem menschlichen Angesicht.", "tokens": ["in", "dem", "menschli\u00b7chen", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}