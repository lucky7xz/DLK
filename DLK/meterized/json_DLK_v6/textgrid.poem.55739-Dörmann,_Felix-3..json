{"textgrid.poem.55739": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O la\u00df mich, la\u00df mich umranken", "tokens": ["O", "la\u00df", "mich", ",", "la\u00df", "mich", "um\u00b7ran\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "VVIMP", "PPER", "VVINF"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die schmiegsam volle Gestalt,", "tokens": ["Die", "schmieg\u00b7sam", "vol\u00b7le", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "La\u00df Busen mich betten an Busen", "tokens": ["La\u00df", "Bu\u00b7sen", "mich", "bet\u00b7ten", "an", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mit st\u00fcrmischer Glutengewalt.", "tokens": ["Mit", "st\u00fcr\u00b7mi\u00b7scher", "Glu\u00b7ten\u00b7ge\u00b7walt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "O la\u00df mich in trunkener Liebe", "tokens": ["O", "la\u00df", "mich", "in", "trun\u00b7ke\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Durchw\u00fchlen Dein flimmerndes Haar,", "tokens": ["Durch\u00b7w\u00fch\u00b7len", "Dein", "flim\u00b7mern\u00b7des", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Wundk\u00fcssen die zuckenden Lippen,", "tokens": ["Wund\u00b7k\u00fcs\u00b7sen", "die", "zu\u00b7cken\u00b7den", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Der Lider k\u00fchles Paar.", "tokens": ["Der", "Li\u00b7der", "k\u00fch\u00b7les", "Paar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ich liebe Dich \u00fcbermenschlich,", "tokens": ["Ich", "lie\u00b7be", "Dich", "\u00fc\u00b7ber\u00b7menschlich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Du bleiches Medusengesicht,", "tokens": ["Du", "blei\u00b7ches", "Me\u00b7du\u00b7sen\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.11": {"text": "Und wei\u00df, da\u00df Deine Seele", "tokens": ["Und", "wei\u00df", ",", "da\u00df", "Dei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Schon f\u00fcr die meine spricht.", "tokens": ["Schon", "f\u00fcr", "die", "mei\u00b7ne", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRELS", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Was soll das t\u00f6richte Weigern,", "tokens": ["Was", "soll", "das", "t\u00f6\u00b7rich\u00b7te", "Wei\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Das halberstickte \u00bbNein\u00ab,", "tokens": ["Das", "hal\u00b7ber\u00b7stick\u00b7te", "\u00bb", "Nein", "\u00ab", ","], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "$(", "PTKANT", "$(", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Du wirst in meinen Armen", "tokens": ["Du", "wirst", "in", "mei\u00b7nen", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Noch todesz\u00e4rtlich sein!", "tokens": ["Noch", "to\u00b7des\u00b7z\u00e4rt\u00b7lich", "sein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}