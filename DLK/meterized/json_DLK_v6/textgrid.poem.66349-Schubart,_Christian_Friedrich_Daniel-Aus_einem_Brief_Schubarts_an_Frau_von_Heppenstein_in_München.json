{"textgrid.poem.66349": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Aus einem Brief Schubarts an Frau von Heppenstein in M\u00fcnchen", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fanny, das k\u00f6stlichste Gestein", "tokens": ["Fan\u00b7ny", ",", "das", "k\u00f6st\u00b7lichs\u00b7te", "Ge\u00b7stein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "im Brautschmucke der Natur", "tokens": ["im", "Brautsc\u00b7hmu\u00b7cke", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "war ihrer Mutter Lust!", "tokens": ["war", "ih\u00b7rer", "Mut\u00b7ter", "Lust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie spielte um die Winke ihrer Mutter,", "tokens": ["Sie", "spiel\u00b7te", "um", "die", "Win\u00b7ke", "ih\u00b7rer", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "wie das L\u00e4mmlein", "tokens": ["wie", "das", "L\u00e4mm\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "um den rosenbewundenen Hirtenstab.", "tokens": ["um", "den", "ro\u00b7sen\u00b7be\u00b7wun\u00b7de\u00b7nen", "Hir\u00b7ten\u00b7stab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Ein k\u00f6stliches M\u00e4dchen war Fanny!", "tokens": ["Ein", "k\u00f6st\u00b7li\u00b7ches", "M\u00e4d\u00b7chen", "war", "Fan\u00b7ny", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Beim Anblick der Gr\u00f6\u00dfe hob sich ihr Geist,", "tokens": ["Beim", "An\u00b7blick", "der", "Gr\u00f6\u00b7\u00dfe", "hob", "sich", "ihr", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "trank Aetherstr\u00f6me, sonnte sich", "tokens": ["trank", "A\u00b7e\u00b7ther\u00b7str\u00f6\u00b7me", ",", "sonn\u00b7te", "sich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NN", "$,", "VMFIN", "PRF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "im Urlichte ewiger Gr\u00f6\u00dfe.", "tokens": ["im", "Ur\u00b7lich\u00b7te", "e\u00b7wi\u00b7ger", "Gr\u00f6\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "Und nur die Thr\u00e4ne der leidenden Menschheit", "tokens": ["Und", "nur", "die", "Thr\u00e4\u00b7ne", "der", "lei\u00b7den\u00b7den", "Menschheit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "vermochte sie herunterzulocken", "tokens": ["ver\u00b7moch\u00b7te", "sie", "her\u00b7un\u00b7ter\u00b7zu\u00b7lo\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "in Erdstaub.", "tokens": ["in", "Erd\u00b7staub", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Einst trat sie auf eines Thurmes Spitze,", "tokens": ["Einst", "trat", "sie", "auf", "ei\u00b7nes", "Thur\u00b7mes", "Spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "um n\u00e4her zu sein", "tokens": ["um", "n\u00e4\u00b7her", "zu", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ADJD", "PTKZU", "VAINF"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dem blauwogigen Himmel.", "tokens": ["Dem", "blau\u00b7wo\u00b7gi\u00b7gen", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie dachte Gottes Gr\u00f6\u00dfe! \u2013", "tokens": ["Sie", "dach\u00b7te", "Got\u00b7tes", "Gr\u00f6\u00b7\u00dfe", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "NN", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und ach! die himmlische Fanny schwindelte.", "tokens": ["Und", "ach", "!", "die", "himm\u00b7li\u00b7sche", "Fan\u00b7ny", "schwin\u00b7del\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Herunter sank sie an des Thurmes", "tokens": ["Her\u00b7un\u00b7ter", "sank", "sie", "an", "des", "Thur\u00b7mes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "felsigen Rippen. Es brach ihr Gebein", "tokens": ["fel\u00b7si\u00b7gen", "Rip\u00b7pen", ".", "Es", "brach", "ihr", "Ge\u00b7bein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.8": {"text": "und Hirn und Blut bespritzte den Sand.", "tokens": ["und", "Hirn", "und", "Blut", "be\u00b7spritz\u00b7te", "den", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Und siehe! die Mutter", "tokens": ["Und", "sie\u00b7he", "!", "die", "Mut\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVIMP", "$.", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "sah die zerschmetterte Leiche Fannys", "tokens": ["sah", "die", "zer\u00b7schmet\u00b7ter\u00b7te", "Lei\u00b7che", "Fan\u00b7nys"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NE"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "und versank nicht! \u2013", "tokens": ["und", "ver\u00b7sank", "nicht", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Hoch blickte sie gen Himmel \u2013 schwieg lange \u2013", "tokens": ["Hoch", "blick\u00b7te", "sie", "gen", "Him\u00b7mel", "\u2013", "schwieg", "lan\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NN", "$(", "VVFIN", "ADV", "$("], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Dann st\u00fcrzte sie die Worte hin:", "tokens": ["Dann", "st\u00fcrz\u00b7te", "sie", "die", "Wor\u00b7te", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Wille geschehe, Jehovah!", "tokens": ["Dein", "Wil\u00b7le", "ge\u00b7sche\u00b7he", ",", "Je\u00b7ho\u00b7vah", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "NE", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Fanny's entfesselte Seele", "tokens": ["Fan\u00b7ny's", "ent\u00b7fes\u00b7sel\u00b7te", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "flog gen Himmel empor.", "tokens": ["flog", "gen", "Him\u00b7mel", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Gnadel\u00e4chelnd sprach der Ewige:", "tokens": ["Gna\u00b7de\u00b7l\u00e4\u00b7chelnd", "sprach", "der", "E\u00b7wi\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Hier bin ich, Fanny! \u2013", "tokens": ["Hier", "bin", "ich", ",", "Fan\u00b7ny", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NE", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Nun knieet sie in Sonnenstrahlen,", "tokens": ["Nun", "kni\u00b7e\u00b7et", "sie", "in", "Son\u00b7nen\u00b7strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Das himmlische Kind \u2013 und erwartet", "tokens": ["Das", "himm\u00b7li\u00b7sche", "Kind", "\u2013", "und", "er\u00b7war\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.13": {"text": "\u2013 die gr\u00f6\u00dfere Mutter.", "tokens": ["\u2013", "die", "gr\u00f6\u00b7\u00dfe\u00b7re", "Mut\u00b7ter", "."], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}