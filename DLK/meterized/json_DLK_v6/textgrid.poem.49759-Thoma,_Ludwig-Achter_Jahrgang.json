{"textgrid.poem.49759": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Achter Jahrgang", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heute ist ein Familienfest,", "tokens": ["Heu\u00b7te", "ist", "ein", "Fa\u00b7mi\u00b7li\u00b7en\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Der achte Jahrgang wird eingeweiht,", "tokens": ["Der", "ach\u00b7te", "Jahr\u00b7gang", "wird", "ein\u00b7ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Alle Verwandten sehen vergn\u00fcgt,", "tokens": ["Al\u00b7le", "Ver\u00b7wand\u00b7ten", "se\u00b7hen", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "VVPP", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wie das Kindlein so kr\u00e4ftig gedeiht.", "tokens": ["Wie", "das", "Kin\u00b7dlein", "so", "kr\u00e4f\u00b7tig", "ge\u00b7deiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Als man es einst aus der Taufe hob,", "tokens": ["Als", "man", "es", "einst", "aus", "der", "Tau\u00b7fe", "hob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da war freilich kein Pfarrer dabei,", "tokens": ["Da", "war", "frei\u00b7lich", "kein", "Pfar\u00b7rer", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "PAV", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Aber eine Deputation", "tokens": ["A\u00b7ber", "ei\u00b7ne", "De\u00b7pu\u00b7ta\u00b7ti\u00b7on"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Von der hochl\u00f6blichen Polizei.", "tokens": ["Von", "der", "hoch\u00b7l\u00f6b\u00b7li\u00b7chen", "Po\u00b7li\u00b7zei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Diese hat sich als Patin gezeigt,", "tokens": ["Die\u00b7se", "hat", "sich", "als", "Pa\u00b7tin", "ge\u00b7zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "KOUS", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wie man sie selten so eifrig sieht,", "tokens": ["Wie", "man", "sie", "sel\u00b7ten", "so", "eif\u00b7rig", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADJD", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "K\u00fcmmerte immer sich um das Kind,", "tokens": ["K\u00fcm\u00b7mer\u00b7te", "im\u00b7mer", "sich", "um", "das", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Hat sich auch oftmals daf\u00fcr bem\u00fcht.", "tokens": ["Hat", "sich", "auch", "oft\u00b7mals", "da\u00b7f\u00fcr", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Als man die r\u00fchrende Liebe sah", "tokens": ["Als", "man", "die", "r\u00fch\u00b7ren\u00b7de", "Lie\u00b7be", "sah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "VVFIN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und sie bemerkte im ganzen Land,", "tokens": ["Und", "sie", "be\u00b7merk\u00b7te", "im", "gan\u00b7zen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hat man dem Kinde im Publikum", "tokens": ["Hat", "man", "dem", "Kin\u00b7de", "im", "Pub\u00b7li\u00b7kum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "ART", "NN", "APPRART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Innige Neigung bald zugewandt.", "tokens": ["In\u00b7ni\u00b7ge", "Nei\u00b7gung", "bald", "zu\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Dies bedenket am heutigen Tag:", "tokens": ["Dies", "be\u00b7den\u00b7ket", "am", "heu\u00b7ti\u00b7gen", "Tag", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wenn das Kindlein so kr\u00e4ftig gedeiht,", "tokens": ["Wenn", "das", "Kin\u00b7dlein", "so", "kr\u00e4f\u00b7tig", "ge\u00b7deiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "M\u00fcssen wir danken zu allererst", "tokens": ["M\u00fcs\u00b7sen", "wir", "dan\u00b7ken", "zu", "al\u00b7le\u00b7rerst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "APPR", "ADV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Unserer gn\u00e4digen Obrigkeit.", "tokens": ["Un\u00b7se\u00b7rer", "gn\u00e4\u00b7di\u00b7gen", "Ob\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}