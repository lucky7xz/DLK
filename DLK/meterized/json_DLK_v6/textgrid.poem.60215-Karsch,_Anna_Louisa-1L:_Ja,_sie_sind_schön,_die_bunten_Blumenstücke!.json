{"textgrid.poem.60215": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, sie sind sch\u00f6n, die bunten Blumenst\u00fccke!", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, sie sind sch\u00f6n, die bunten Blumenst\u00fccke!", "tokens": ["Ja", ",", "sie", "sind", "sch\u00f6n", ",", "die", "bun\u00b7ten", "Blu\u00b7men\u00b7st\u00fc\u00b7cke", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Betr\u00fcgerisch f\u00fcr unsre Blicke,", "tokens": ["Be\u00b7tr\u00fc\u00b7ge\u00b7risch", "f\u00fcr", "uns\u00b7re", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wird meine Hand verf\u00fchrt, da\u00df sie nach einer langt,", "tokens": ["Wird", "mei\u00b7ne", "Hand", "ver\u00b7f\u00fchrt", ",", "da\u00df", "sie", "nach", "ei\u00b7ner", "langt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$,", "KOUS", "PPER", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die wie die sch\u00f6nste Blum in deinem Garten prangt!", "tokens": ["Die", "wie", "die", "sch\u00f6ns\u00b7te", "Blum", "in", "dei\u00b7nem", "Gar\u00b7ten", "prangt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch sch\u00f6ner sind f\u00fcr mich die St\u00fccke der Geschichte:", "tokens": ["Doch", "sch\u00f6\u00b7ner", "sind", "f\u00fcr", "mich", "die", "St\u00fc\u00b7cke", "der", "Ge\u00b7schich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "APPR", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da stirbt auf Alexanders Angesichte", "tokens": ["Da", "stirbt", "auf", "A\u00b7lex\u00b7an\u00b7ders", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Ehrgeiz, den der Held in seiner Brust verbarg;", "tokens": ["Der", "Ehr\u00b7geiz", ",", "den", "der", "Held", "in", "sei\u00b7ner", "Brust", "ver\u00b7barg", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da sieht sein Auge starr, gleich halb verloschnen Kohlen,", "tokens": ["Da", "sieht", "sein", "Au\u00b7ge", "starr", ",", "gleich", "halb", "ver\u00b7lo\u00b7schnen", "Koh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "$,", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die um ihn stehen, an; Er, der der Welt befohlen,", "tokens": ["Die", "um", "ihn", "ste\u00b7hen", ",", "an", ";", "Er", ",", "der", "der", "Welt", "be\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,", "PTKVZ", "$.", "PPER", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Stirbt, und erobert einen Sarg.", "tokens": ["Stirbt", ",", "und", "er\u00b7o\u00b7bert", "ei\u00b7nen", "Sarg", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sein Arzt kniet neben ihm, und f\u00fchlt mit seiner Rechten", "tokens": ["Sein", "Arzt", "kniet", "ne\u00b7ben", "ihm", ",", "und", "f\u00fchlt", "mit", "sei\u00b7ner", "Rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-----+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Das Herz des Kriegers, dem der Tod schon St\u00f6sse giebt,", "tokens": ["Das", "Herz", "des", "Krie\u00b7gers", ",", "dem", "der", "Tod", "schon", "St\u00f6s\u00b7se", "giebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ART", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Linke f\u00fchlt den Puls, und Alexanders Knechten", "tokens": ["Die", "Lin\u00b7ke", "f\u00fchlt", "den", "Puls", ",", "und", "A\u00b7lex\u00b7an\u00b7ders", "Knech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verk\u00fcndiget der Blick des Arztes zu betr\u00fcbt", "tokens": ["Ver\u00b7k\u00fcn\u00b7di\u00b7get", "der", "Blick", "des", "Arz\u00b7tes", "zu", "be\u00b7tr\u00fcbt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKZU", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Weltbezwingers letzte Stunde;", "tokens": ["Des", "Welt\u00b7be\u00b7zwin\u00b7gers", "letz\u00b7te", "Stun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Laut klagt ihr Herz in ihrem Munde,", "tokens": ["Laut", "klagt", "ihr", "Herz", "in", "ih\u00b7rem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und Gram auf ihre Stirn gewolket, breitet sich;", "tokens": ["Und", "Gram", "auf", "ih\u00b7re", "Stirn", "ge\u00b7wol\u00b7ket", ",", "brei\u00b7tet", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das ganze Haus, bis auf die Hunde,", "tokens": ["Das", "gan\u00b7ze", "Haus", ",", "bis", "auf", "die", "Hun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Steht angstvoll, heulet j\u00e4mmerlich!", "tokens": ["Steht", "angst\u00b7voll", ",", "heu\u00b7let", "j\u00e4m\u00b7mer\u00b7lich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zu des Erobrers Haupt, auf einem Tische, liegen", "tokens": ["Zu", "des", "E\u00b7ro\u00b7brers", "Haupt", ",", "auf", "ei\u00b7nem", "Ti\u00b7sche", ",", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "APPR", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein Helm, sein Panzer, und sein Schild,", "tokens": ["Sein", "Helm", ",", "sein", "Pan\u00b7zer", ",", "und", "sein", "Schild", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bemahlt mit Furien; sein Schwerdt, gewohnt zu siegen,", "tokens": ["Be\u00b7mahlt", "mit", "Fu\u00b7ri\u00b7en", ";", "sein", "Schwerdt", ",", "ge\u00b7wohnt", "zu", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$.", "PPOSAT", "NN", "$,", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nutzt ihm nicht mehr, er mu\u00df dem Tod itzt unterliegen;", "tokens": ["Nutzt", "ihm", "nicht", "mehr", ",", "er", "mu\u00df", "dem", "Tod", "itzt", "un\u00b7ter\u00b7lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$,", "PPER", "VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie bin ich, seufzet er, der Nichtigkeit ihr Bild?", "tokens": ["Wie", "bin", "ich", ",", "seuf\u00b7zet", "er", ",", "der", "Nich\u00b7tig\u00b7keit", "ihr", "Bild", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein Gott, und einer von des Todes Unterthanen?", "tokens": ["Ein", "Gott", ",", "und", "ei\u00b7ner", "von", "des", "To\u00b7des", "Un\u00b7ter\u00b7tha\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und stirbt, bestr\u00f6mt mit Thr\u00e4nen von Roxanen.", "tokens": ["Und", "stirbt", ",", "be\u00b7str\u00f6mt", "mit", "Thr\u00e4\u00b7nen", "von", "Ro\u00b7xa\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "APPR", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "O du, mein Vater,", "tokens": ["O", "du", ",", "mein", "Va\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Mehr, als ein St\u00fcck mit Pracht des Fr\u00fchlings ausgeziert?", "tokens": ["Mehr", ",", "als", "ein", "St\u00fcck", "mit", "Pracht", "des", "Fr\u00fch\u00b7lings", "aus\u00b7ge\u00b7ziert", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ART", "NN", "APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und, o! sieh jenes noch! da lieget auf dem Schoosse", "tokens": ["Und", ",", "o", "!", "sieh", "je\u00b7nes", "noch", "!", "da", "lie\u00b7get", "auf", "dem", "Schoos\u00b7se"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "FM", "$.", "VVIMP", "PDS", "ADV", "$.", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sch\u00f6nen, die er liebt; ein Held!", "tokens": ["Der", "Sch\u00f6\u00b7nen", ",", "die", "er", "liebt", ";", "ein", "Held", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein lockigt Haar, das weich auf seine Schultern f\u00e4llt,", "tokens": ["Sein", "lo\u00b7ckigt", "Haar", ",", "das", "weich", "auf", "sei\u00b7ne", "Schul\u00b7tern", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "NN", "$,", "PRELS", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist sch\u00f6n; doch gr\u00f6ssern Reiz enth\u00e4lt", "tokens": ["Ist", "sch\u00f6n", ";", "doch", "gr\u00f6s\u00b7sern", "Reiz", "ent\u00b7h\u00e4lt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$.", "ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein blaues Auge, das die grosse", "tokens": ["Sein", "blau\u00b7es", "Au\u00b7ge", ",", "das", "die", "gros\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und feuervolle Seele zeigt,", "tokens": ["Und", "feu\u00b7er\u00b7vol\u00b7le", "See\u00b7le", "zeigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die z\u00e4rtlich und betr\u00fcbt itzt in das Auge steigt!", "tokens": ["Die", "z\u00e4rt\u00b7lich", "und", "be\u00b7tr\u00fcbt", "itzt", "in", "das", "Au\u00b7ge", "steigt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "VVFIN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Aus Seufzern soll er Worte sammeln,", "tokens": ["Aus", "Seuf\u00b7zern", "soll", "er", "Wor\u00b7te", "sam\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um seinen Abschied herzustammeln;", "tokens": ["Um", "sei\u00b7nen", "Ab\u00b7schied", "her\u00b7zu\u00b7stam\u00b7meln", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wehm\u00fcthig reichet er sein Bildni\u00df sch\u00f6n gemahlt,", "tokens": ["Weh\u00b7m\u00fct\u00b7hig", "rei\u00b7chet", "er", "sein", "Bild\u00b7ni\u00df", "sch\u00f6n", "ge\u00b7mahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Angebetheten, die, wie Aurora strahlt;", "tokens": ["Der", "An\u00b7ge\u00b7be\u00b7the\u00b7ten", ",", "die", ",", "wie", "Au\u00b7ro\u00b7ra", "strahlt", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "PWAV", "NE", "VVFIN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Allein, an statt das Bild zu nehmen,", "tokens": ["Al\u00b7lein", ",", "an", "statt", "das", "Bild", "zu", "neh\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Greift sie nach Rosen, die ein Liebes-Gott ihr reicht;", "tokens": ["Greift", "sie", "nach", "Ro\u00b7sen", ",", "die", "ein", "Lie\u00b7bes\u00b7Gott", "ihr", "reicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Stolze! Vater ja, sie sollte sich nur sch\u00e4men!", "tokens": ["Die", "Stol\u00b7ze", "!", "Va\u00b7ter", "ja", ",", "sie", "soll\u00b7te", "sich", "nur", "sch\u00e4\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "PTKANT", "$,", "PPER", "VMFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein Held liegt da vor ihr, von Z\u00e4rtlichkeit erweicht,", "tokens": ["Ein", "Held", "liegt", "da", "vor", "ihr", ",", "von", "Z\u00e4rt\u00b7lich\u00b7keit", "er\u00b7weicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und sie allein ist Stein, und kann ihm wiederstreben,", "tokens": ["Und", "sie", "al\u00b7lein", "ist", "Stein", ",", "und", "kann", "ihm", "wie\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VAFIN", "NN", "$,", "KON", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und sie allein wird nicht erweicht?", "tokens": ["Und", "sie", "al\u00b7lein", "wird", "nicht", "er\u00b7weicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach Vater z\u00fcrne doch! sie spottet! ja! vielleicht", "tokens": ["Ach", "Va\u00b7ter", "z\u00fcr\u00b7ne", "doch", "!", "sie", "spot\u00b7tet", "!", "ja", "!", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["ITJ", "NN", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "$.", "PTKANT", "$.", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will sie dem armen Held, dem sie das Herz erweicht,", "tokens": ["Will", "sie", "dem", "ar\u00b7men", "Held", ",", "dem", "sie", "das", "Herz", "er\u00b7weicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Rose zur Erquickung geben!", "tokens": ["Die", "Ro\u00b7se", "zur", "Er\u00b7quic\u00b7kung", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Zu ihrer Rechten st\u00fcrzt ein Wasserfall, und schilt", "tokens": ["Zu", "ih\u00b7rer", "Rech\u00b7ten", "st\u00fcrzt", "ein", "Was\u00b7ser\u00b7fall", ",", "und", "schilt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Laut murmelnd, da\u00df sie so die Z\u00e4rtlichkeit vergilt,", "tokens": ["Laut", "mur\u00b7melnd", ",", "da\u00df", "sie", "so", "die", "Z\u00e4rt\u00b7lich\u00b7keit", "ver\u00b7gilt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVPP", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihm gegen\u00fcber stehn zwey Krieger, und es scheinet,", "tokens": ["Ihm", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "stehn", "zwey", "Krie\u00b7ger", ",", "und", "es", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "CARD", "NN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als ob ihr Herz mitleidig weinet;", "tokens": ["Als", "ob", "ihr", "Herz", "mit\u00b7lei\u00b7dig", "wei\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Voll ritterlicher Treu begleiteten sie ihn", "tokens": ["Voll", "rit\u00b7ter\u00b7li\u00b7cher", "Treu", "be\u00b7glei\u00b7te\u00b7ten", "sie", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, 