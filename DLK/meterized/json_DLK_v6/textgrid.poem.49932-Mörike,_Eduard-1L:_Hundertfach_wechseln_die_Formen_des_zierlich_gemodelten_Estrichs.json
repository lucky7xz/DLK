{"textgrid.poem.49932": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hundertfach wechseln die Formen des zierlich gemodelten Estrichs", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hundertfach wechseln die Formen des zierlich gemodelten Estrichs", "tokens": ["Hun\u00b7dert\u00b7fach", "wech\u00b7seln", "die", "For\u00b7men", "des", "zier\u00b7lich", "ge\u00b7mo\u00b7del\u00b7ten", "Est\u00b7richs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "ADJD", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Auf dem Flur des Dorments, r\u00f6tlich in W\u00fcrfeln gebrannt:", "tokens": ["Auf", "dem", "Flur", "des", "Dor\u00b7ments", ",", "r\u00f6t\u00b7lich", "in", "W\u00fcr\u00b7feln", "ge\u00b7brannt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Rebengewinde mit gr\u00fcner Glasur und bl\u00e4ulichen Trauben,", "tokens": ["Re\u00b7ben\u00b7ge\u00b7win\u00b7de", "mit", "gr\u00fc\u00b7ner", "Gla\u00b7sur", "und", "bl\u00e4u\u00b7li\u00b7chen", "Trau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "T\u00e4ubchen dabei, paarweis, rings in die Ecken verteilt;", "tokens": ["T\u00e4ub\u00b7chen", "da\u00b7bei", ",", "paar\u00b7weis", ",", "rings", "in", "die", "E\u00b7cken", "ver\u00b7teilt", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "$,", "FM.la", "$,", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Auch dein gotisches Blatt, Chelidonium, dessen lebendig", "tokens": ["Auch", "dein", "go\u00b7ti\u00b7sches", "Blatt", ",", "Che\u00b7li\u00b7do\u00b7ni\u00b7um", ",", "des\u00b7sen", "le\u00b7ben\u00b7dig"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,", "NE", "$,", "PRELAT", "ADJD"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Wucherndes Muster noch heut drau\u00dfen die Pfeiler begr\u00fcnt;", "tokens": ["Wu\u00b7chern\u00b7des", "Mus\u00b7ter", "noch", "heut", "drau\u00b7\u00dfen", "die", "Pfei\u00b7ler", "be\u00b7gr\u00fcnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Auch, in heraldischer Zeichnung, erscheint vielf\u00e4ltig die Lilie,", "tokens": ["Auch", ",", "in", "her\u00b7al\u00b7di\u00b7scher", "Zeich\u00b7nung", ",", "er\u00b7scheint", "viel\u00b7f\u00e4l\u00b7tig", "die", "Li\u00b7lie", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Blume der Jungfrau, wei\u00df schimmernd auf r\u00f6tlichem Grund.", "tokens": ["Blu\u00b7me", "der", "Jung\u00b7frau", ",", "wei\u00df", "schim\u00b7mernd", "auf", "r\u00f6t\u00b7li\u00b7chem", "Grund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.9": {"text": "Alles mit Sinn und Geschmack, zur Bewunderung! aber auch alles", "tokens": ["Al\u00b7les", "mit", "Sinn", "und", "Ge\u00b7schmack", ",", "zur", "Be\u00b7wun\u00b7de\u00b7rung", "!", "a\u00b7ber", "auch", "al\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "APPR", "NN", "KON", "NN", "$,", "APPRART", "NN", "$.", "ADV", "ADV", "PIS"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Fast in Tr\u00fcmmern, und nur seufzend verlie\u00df ich den Ort.", "tokens": ["Fast", "in", "Tr\u00fcm\u00b7mern", ",", "und", "nur", "seuf\u00b7zend", "ver\u00b7lie\u00df", "ich", "den", "Ort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "KON", "ADV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}