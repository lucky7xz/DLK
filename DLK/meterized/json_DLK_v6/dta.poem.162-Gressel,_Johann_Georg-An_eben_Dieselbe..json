{"dta.poem.162": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An eben Dieselbe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die Thr\u00e4nen lassen euch mein Engel gar nicht sch\u00f6n/", "tokens": ["Die", "Thr\u00e4\u00b7nen", "las\u00b7sen", "euch", "mein", "En\u00b7gel", "gar", "nicht", "sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum h\u00f6ret auf den Tod des Sperlings zu beweinen/", "tokens": ["Drum", "h\u00f6\u00b7ret", "auf", "den", "Tod", "des", "Sper\u00b7lings", "zu", "be\u00b7wei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr werdet sonst der Welt gar zu ver\u00e4chtlich scheinen/", "tokens": ["Ihr", "wer\u00b7det", "sonst", "der", "Welt", "gar", "zu", "ver\u00b7\u00e4cht\u00b7lich", "schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "PTKA", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Soll euch ein sch\u00f6nerer zu euren Diensten stehn?", "tokens": ["Soll", "euch", "ein", "sch\u00f6\u00b7ne\u00b7rer", "zu", "eu\u00b7ren", "Diens\u00b7ten", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So facht die Br\u00fcste auf um mich dadurch zu fangen/", "tokens": ["So", "facht", "die", "Br\u00fcs\u00b7te", "auf", "um", "mich", "da\u00b7durch", "zu", "fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "APPR", "PRF", "PAV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jhr werdet mich gar bald in eure Hand erlangen.", "tokens": ["Ihr", "wer\u00b7det", "mich", "gar", "bald", "in", "eu\u00b7re", "Hand", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}