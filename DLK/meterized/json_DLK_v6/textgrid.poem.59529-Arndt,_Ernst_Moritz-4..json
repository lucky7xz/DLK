{"textgrid.poem.59529": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Himmlische Auen,", "tokens": ["Himm\u00b7li\u00b7sche", "Au\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wo meines Daseins Wiege stand,", "tokens": ["Wo", "mei\u00b7nes", "Da\u00b7seins", "Wie\u00b7ge", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eh' ich zu schauen", "tokens": ["Eh'", "ich", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ging das betr\u00e4nte Erdenland,", "tokens": ["Ging", "das", "be\u00b7tr\u00e4n\u00b7te", "Er\u00b7den\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Holde Gespielen,", "tokens": ["Hol\u00b7de", "Ge\u00b7spie\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Engel des Himmels, kennt ihr mich?", "tokens": ["En\u00b7gel", "des", "Him\u00b7mels", ",", "kennt", "ihr", "mich", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "W\u00e4hnen und f\u00fchlen,", "tokens": ["W\u00e4h\u00b7nen", "und", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Tr\u00e4umen mit euch, nur das kann ich.", "tokens": ["Tr\u00e4u\u00b7men", "mit", "euch", ",", "nur", "das", "kann", "ich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "ADV", "PDS", "VMFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber die Wonne", "tokens": ["A\u00b7ber", "die", "Won\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Fl\u00fcchtig wie Schatten vor\u00fcberrauscht.", "tokens": ["Fl\u00fcch\u00b7tig", "wie", "Schat\u00b7ten", "vor\u00b7\u00fc\u00b7berr\u00b7auscht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "VVPP", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Hier, wo die Sonne", "tokens": ["Hier", ",", "wo", "die", "Son\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gleich mit der Nacht die Stunden tauscht,", "tokens": ["Gleich", "mit", "der", "Nacht", "die", "Stun\u00b7den", "tauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier, wo die Klage", "tokens": ["Hier", ",", "wo", "die", "Kla\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "\u00dcber der Todesurne schallt", "tokens": ["\u00dc\u00b7ber", "der", "To\u00b7de\u00b7sur\u00b7ne", "schallt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und mit dem Tage", "tokens": ["Und", "mit", "dem", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Sch\u00f6nheit und Jugend vor\u00fcberwallt.", "tokens": ["Sch\u00f6n\u00b7heit", "und", "Ju\u00b7gend", "vor\u00b7\u00fc\u00b7berw\u00b7allt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Traurig gefangen", "tokens": ["Trau\u00b7rig", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Schmachtet die Seele auf zum Licht,", "tokens": ["Schmach\u00b7tet", "die", "See\u00b7le", "auf", "zum", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Doch ihr Verlangen", "tokens": ["Doch", "ihr", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.4": {"text": "Stillet die Erde unten nicht;", "tokens": ["Stil\u00b7let", "die", "Er\u00b7de", "un\u00b7ten", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKNEG", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Leuchten die Sterne,", "tokens": ["Leuch\u00b7ten", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Schau' ich nach oben sehnend hin,", "tokens": ["Schau'", "ich", "nach", "o\u00b7ben", "seh\u00b7nend", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dort zu der Ferne,", "tokens": ["Dort", "zu", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Dort zu den Frommen steht mein Sinn.", "tokens": ["Dort", "zu", "den", "From\u00b7men", "steht", "mein", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Himmlische Auen,", "tokens": ["Himm\u00b7li\u00b7sche", "Au\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wo meines Daseins Wiege stand,", "tokens": ["Wo", "mei\u00b7nes", "Da\u00b7seins", "Wie\u00b7ge", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Werd' ich euch schauen", "tokens": ["Werd'", "ich", "euch", "schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Frei von dem eitlen Erdentand?", "tokens": ["Frei", "von", "dem", "eit\u00b7len", "Er\u00b7den\u00b7tand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "S\u00fc\u00dfe Gespielen", "tokens": ["S\u00fc\u00b7\u00dfe", "Ge\u00b7spie\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Himmlischer Kindheit, Engelein,", "tokens": ["Himm\u00b7li\u00b7scher", "Kind\u00b7heit", ",", "En\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Werd' ich bald spielen", "tokens": ["Werd'", "ich", "bald", "spie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Mit euch droben den Ringelreihn?", "tokens": ["Mit", "euch", "dro\u00b7ben", "den", "Rin\u00b7gel\u00b7reihn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Eija! wie fr\u00f6hlich", "tokens": ["Ei\u00b7ja", "!", "wie", "fr\u00f6h\u00b7lich"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "PWAV", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Geht mir im Busen frisch das Herz!", "tokens": ["Geht", "mir", "im", "Bu\u00b7sen", "frisch", "das", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eija! wie selig", "tokens": ["Ei\u00b7ja", "!", "wie", "se\u00b7lig"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "PWAV", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "F\u00fchl' ich versinken Erdenschmerz!", "tokens": ["F\u00fchl'", "ich", "ver\u00b7sin\u00b7ken", "Er\u00b7den\u00b7schmerz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ewige Lichter,", "tokens": ["E\u00b7wi\u00b7ge", "Lich\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Str\u00f6met ihr Lebensglut auf mich?", "tokens": ["Str\u00f6\u00b7met", "ihr", "Le\u00b7bens\u00b7glut", "auf", "mich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Engelgesichter,", "tokens": ["En\u00b7gel\u00b7ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Himmelsgespielen, gr\u00fc\u00dft ihr mich?", "tokens": ["Him\u00b7mels\u00b7ge\u00b7spie\u00b7len", ",", "gr\u00fc\u00dft", "ihr", "mich", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}