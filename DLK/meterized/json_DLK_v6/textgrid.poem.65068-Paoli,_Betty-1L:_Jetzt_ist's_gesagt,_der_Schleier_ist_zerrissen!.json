{"textgrid.poem.65068": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jetzt ist's gesagt, der Schleier ist zerrissen!", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt ist's gesagt, der Schleier ist zerrissen!", "tokens": ["Jetzt", "ist's", "ge\u00b7sagt", ",", "der", "Schlei\u00b7er", "ist", "zer\u00b7ris\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Dunkel nicht soll deine, Seele schwanken.", "tokens": ["Im", "Dun\u00b7kel", "nicht", "soll", "dei\u00b7ne", ",", "See\u00b7le", "schwan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "VMFIN", "PPOSAT", "$,", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Statt eitlem Wahne deiner Huld zu danken", "tokens": ["Statt", "eit\u00b7lem", "Wah\u00b7ne", "dei\u00b7ner", "Huld", "zu", "dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Will ich sie lieber stolzen Muth's vermissen.", "tokens": ["Will", "ich", "sie", "lie\u00b7ber", "stol\u00b7zen", "Muth's", "ver\u00b7mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch magst du auch die ", "tokens": ["Doch", "magst", "du", "auch", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Magst wissen, da\u00df ein Geist in den Gedanken", "tokens": ["Magst", "wis\u00b7sen", ",", "da\u00df", "ein", "Geist", "in", "den", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "$,", "KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Gottheit wie ein Thau vom Himmel sanken,", "tokens": ["Der", "Got\u00b7theit", "wie", "ein", "Thau", "vom", "Him\u00b7mel", "san\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu tr\u00fcben nicht von ird'schen Finsternissen.", "tokens": ["Zu", "tr\u00fc\u00b7ben", "nicht", "von", "ird'\u00b7schen", "Fins\u00b7ter\u00b7nis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "La\u00df Marmorbilder unter Schutt versinken,", "tokens": ["La\u00df", "Mar\u00b7mor\u00b7bil\u00b7der", "un\u00b7ter", "Schutt", "ver\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie werden noch nach Tausenden von Jahren", "tokens": ["Sie", "wer\u00b7den", "noch", "nach", "Tau\u00b7sen\u00b7den", "von", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Auge schimmernd hell entgegenblinken.", "tokens": ["Dem", "Au\u00b7ge", "schim\u00b7mernd", "hell", "ent\u00b7ge\u00b7gen\u00b7blin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Staub' wird der Demant den Glanz bewahren", "tokens": ["Im", "Staub'", "wird", "der", "De\u00b7mant", "den", "Glanz", "be\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und, darf er wieder Sonnenstrahlen trinken,", "tokens": ["Und", ",", "darf", "er", "wie\u00b7der", "Son\u00b7nen\u00b7strah\u00b7len", "trin\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VMFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein Lichtgeheimni\u00df flammend offenbaren.", "tokens": ["Sein", "Licht\u00b7ge\u00b7heim\u00b7ni\u00df", "flam\u00b7mend", "of\u00b7fen\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}