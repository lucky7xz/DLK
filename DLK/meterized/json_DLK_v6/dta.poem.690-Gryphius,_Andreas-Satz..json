{"dta.poem.690": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Satz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Rei\u00df Erde! rei\u00df eutzwey! Jhr Berge brecht ", "tokens": ["Rei\u00df", "Er\u00b7de", "!", "rei\u00df", "eut\u00b7zwey", "!", "Ihr", "Ber\u00b7ge", "brecht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "$.", "VVFIN", "PTKVZ", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "vnd decket ", "tokens": ["vnd", "de\u00b7cket"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Den gantz verzagten Geist. ", "tokens": ["Den", "gantz", "ver\u00b7zag\u00b7ten", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Den Blitz vnd ach vnd noth/ vnd angst/ vnd ", "tokens": ["Den", "Blitz", "vnd", "ach", "vnd", "noth", "/", "vnd", "angst", "/", "vnd"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "KON", "XY", "KON", "NN", "$(", "KON", "VVPP", "$(", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "weh' erschrecket! ", "tokens": ["weh'", "er\u00b7schre\u00b7cket", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Vnd herbe wehmutt beist!", "tokens": ["Vnd", "her\u00b7be", "weh\u00b7mutt", "beist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Jhr jmmerlichten st\u00e4tter ", "tokens": ["Ihr", "jm\u00b7mer\u00b7lich\u00b7ten", "st\u00e4t\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ach bescheinet meine glieder! ach bescheint die glieder nicht! ", "tokens": ["Ach", "be\u00b7schei\u00b7net", "mei\u00b7ne", "glie\u00b7der", "!", "ach", "be\u00b7scheint", "die", "glie\u00b7der", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Die der Donnerkeil der schmertzen/ die die krafft der ", "tokens": ["Die", "der", "Don\u00b7ner\u00b7keil", "der", "schmert\u00b7zen", "/", "die", "die", "krafft", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ART", "VVINF", "$(", "PRELS", "ART", "NN", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "zubricht! ", "tokens": ["zu\u00b7bricht", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "GoTT/ gutter GOtt! nur mir/ zu strenger Richter!", "tokens": ["GoTT", "/", "gut\u00b7ter", "Gott", "!", "nur", "mir", "/", "zu", "stren\u00b7ger", "Rich\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADJA", "NN", "$.", "ADV", "PPER", "$(", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Was l\u00e4sset mich dein grimm nicht sehen! ", "tokens": ["Was", "l\u00e4s\u00b7set", "mich", "dein", "grimm", "nicht", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "ADJD", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Was h\u00f6r ich nicht f\u00fcr spott vnd schm\u00e4hen? ", "tokens": ["Was", "h\u00f6r", "ich", "nicht", "f\u00fcr", "spott", "vnd", "schm\u00e4\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJD", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sind die augen mir verliehen ", "tokens": ["Sind", "die", "au\u00b7gen", "mir", "ver\u00b7lie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Da\u00df ich nichts al\u00df herbe plagen/ nichts al\u00df Marter schawen", "tokens": ["Da\u00df", "ich", "nichts", "al\u00df", "her\u00b7be", "pla\u00b7gen", "/", "nichts", "al\u00df", "Mar\u00b7ter", "scha\u00b7wen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIS", "KOKOM", "VVFIN", "VVFIN", "$(", "PIS", "KOKOM", "NN", "VVINF"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.16": {"text": "soll? ", "tokens": ["soll", "?"], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "+", "measure": "single.up"}, "line.17": {"text": "T\u00e4glich rufft man mir die Ohren/ ja die matte Seele voll!", "tokens": ["T\u00e4g\u00b7lich", "rufft", "man", "mir", "die", "Oh\u00b7ren", "/", "ja", "die", "mat\u00b7te", "See\u00b7le", "voll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "PPER", "ART", "NN", "$(", "ADV", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Kan ich! kan ", "tokens": ["Kan", "ich", "!", "kan"], "token_info": ["word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "$.", "VMFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Kan die Hell-besternte Nacht! ", "tokens": ["Kan", "die", "Hell\u00b7be\u00b7stern\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "erquicken? ", "tokens": ["er\u00b7qui\u00b7cken", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Sol mich jede Morgenr\u00f6tt' jeder Abendstunde dr\u00fccken? ", "tokens": ["Sol", "mich", "je\u00b7de", "Mor\u00b7gen\u00b7r\u00f6tt'", "je\u00b7der", "A\u00b7bends\u00b7tun\u00b7de", "dr\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}