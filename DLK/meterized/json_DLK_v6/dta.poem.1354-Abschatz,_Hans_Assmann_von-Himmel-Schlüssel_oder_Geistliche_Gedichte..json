{"dta.poem.1354": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl dem/ der nicht im Rath der Ungerechten wandelt/", "tokens": ["Wohl", "dem", "/", "der", "nicht", "im", "Rath", "der", "Un\u00b7ge\u00b7rech\u00b7ten", "wan\u00b7delt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$(", "ART", "PTKNEG", "APPRART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der nicht der S\u00fcnder Weg/ der Sp\u00f6tter Stul ber\u00fchrt/", "tokens": ["Der", "nicht", "der", "S\u00fcn\u00b7der", "Weg", "/", "der", "Sp\u00f6t\u00b7ter", "Stul", "be\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ART", "NN", "NN", "$(", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der GOttes Wort mit Lust und reinem Hertzen handelt/", "tokens": ["Der", "Got\u00b7tes", "Wort", "mit", "Lust", "und", "rei\u00b7nem", "Hert\u00b7zen", "han\u00b7delt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sein Gesetze Tag und Nacht im Munde f\u00fchrt.", "tokens": ["Und", "sein", "Ge\u00b7set\u00b7ze", "Tag", "und", "Nacht", "im", "Mun\u00b7de", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "KON", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der gr\u00fcnet wie ein Baum gepflantzet an den B\u00e4chen/", "tokens": ["Der", "gr\u00fc\u00b7net", "wie", "ein", "Baum", "ge\u00b7pflant\u00b7zet", "an", "den", "B\u00e4\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der seine Fr\u00fcchte bringt zu ausgesezter Zeit.", "tokens": ["Der", "sei\u00b7ne", "Fr\u00fcch\u00b7te", "bringt", "zu", "aus\u00b7ge\u00b7sez\u00b7ter", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Kein rauher Wind noch Herbst kan seine Bl\u00e4tter schw\u00e4chen/", "tokens": ["Kein", "rau\u00b7her", "Wind", "noch", "Herbst", "kan", "sei\u00b7ne", "Bl\u00e4t\u00b7ter", "schw\u00e4\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was er beginnt/ ger\u00e4th/ und was er w\u00fcnscht/ gedeyt.", "tokens": ["Was", "er", "be\u00b7ginnt", "/", "ge\u00b7r\u00e4\u00b7th", "/", "und", "was", "er", "w\u00fcnscht", "/", "ge\u00b7deyt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$(", "ADJD", "$(", "KON", "PWS", "PPER", "VVFIN", "$(", "VVPP", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "So gehts den Leuten nicht/ die sich vor GOtt nicht scheuen/", "tokens": ["So", "gehts", "den", "Leu\u00b7ten", "nicht", "/", "die", "sich", "vor", "Gott", "nicht", "scheu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKNEG", "$(", "PRELS", "PRF", "APPR", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Grund/ auff dem sie stehn/ ist ungewisser Sand/", "tokens": ["Der", "Grund", "/", "auff", "dem", "sie", "stehn", "/", "ist", "un\u00b7ge\u00b7wis\u00b7ser", "Sand", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "PRELS", "PPER", "VVINF", "$(", "VAFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie lassen sich wie Spreu und Staub durch Wind verstreuen;", "tokens": ["Sie", "las\u00b7sen", "sich", "wie", "Spreu", "und", "Staub", "durch", "Wind", "ver\u00b7streu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KOKOM", "NN", "KON", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was Gold und Eisen hie\u00df/ wird Koth und Wachs erkandt/", "tokens": ["Was", "Gold", "und", "Ei\u00b7sen", "hie\u00df", "/", "wird", "Koth", "und", "Wachs", "er\u00b7kandt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "$(", "VAFIN", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Drum kan der Falschen Sinn nicht bleiben im Gerichte/", "tokens": ["Drum", "kan", "der", "Fal\u00b7schen", "Sinn", "nicht", "blei\u00b7ben", "im", "Ge\u00b7rich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Noch/ wer ges\u00fcndigt hat/ bey reiner Schaar bestehn;", "tokens": ["Noch", "/", "wer", "ge\u00b7s\u00fcn\u00b7digt", "hat", "/", "bey", "rei\u00b7ner", "Schaar", "be\u00b7stehn", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PWS", "VVPP", "VAFIN", "$(", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Denn auff der Frommen Weg scheint GOttes Angesichte/", "tokens": ["Denn", "auff", "der", "From\u00b7men", "Weg", "scheint", "Got\u00b7tes", "An\u00b7ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wenn b\u00f6ser Leute Pfad mit Schanden mu\u00df vergehn.", "tokens": ["Wenn", "b\u00f6\u00b7ser", "Leu\u00b7te", "Pfad", "mit", "Schan\u00b7den", "mu\u00df", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NN", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}