{"dta.poem.2509": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die in den Garten spatzirende  \n Karitille.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Ach siehe dich wol f\u00fcr/ imfall du in den Garten", "tokens": ["Ach", "sie\u00b7he", "dich", "wol", "f\u00fcr", "/", "im\u00b7fall", "du", "in", "den", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVIMP", "PPER", "ADV", "APPR", "$(", "ADV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wilst aus-spatzieren gehn hertzliebste Karitill!", "tokens": ["Wilst", "aus\u00b7spat\u00b7zie\u00b7ren", "gehn", "hertz\u00b7liebs\u00b7te", "Ka\u00b7ri\u00b7till", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "VVINF", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn jener blinde Sch\u00fctz ist fertig auffzuwarten/", "tokens": ["Denn", "je\u00b7ner", "blin\u00b7de", "Sch\u00fctz", "ist", "fer\u00b7tig", "auff\u00b7zu\u00b7war\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "VAFIN", "ADJD", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auff seine Mutter selbst/ die er verwunden wil/", "tokens": ["Auff", "sei\u00b7ne", "Mut\u00b7ter", "selbst", "/", "die", "er", "ver\u00b7wun\u00b7den", "wil", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$(", "PRELS", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weil Sie ihn hat geschimpfft in seinen Liebessachen.", "tokens": ["Weil", "Sie", "ihn", "hat", "ge\u00b7schimpfft", "in", "sei\u00b7nen", "Lie\u00b7bes\u00b7sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du bist wie Venus selbst/ mein Kind/ und gleichest", "tokens": ["Du", "bist", "wie", "Ve\u00b7nus", "selbst", "/", "mein", "Kind", "/", "und", "glei\u00b7chest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "ADV", "$(", "PPOSAT", "NN", "$(", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "ihr/", "tokens": ["ihr", "/"], "token_info": ["word", "punct"], "pos": ["PPER", "$("], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Dein Leib und dessen Ziehr/ dein Reden/ Thun und La-", "tokens": ["Dein", "Leib", "und", "des\u00b7sen", "Ziehr", "/", "dein", "Re\u00b7den", "/", "Thun", "und", "La"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PRELAT", "NN", "$(", "PPOSAT", "NN", "$(", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "chen/", "tokens": ["chen", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Ist ihr nat\u00fcrlich gleich. Dr\u00fcm siehe dich wol f\u00fcr.", "tokens": ["Ist", "ihr", "na\u00b7t\u00fcr\u00b7lich", "gleich", ".", "Dr\u00fcm", "sie\u00b7he", "dich", "wol", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "$.", "NN", "VVIMP", "PPER", "ADV", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}