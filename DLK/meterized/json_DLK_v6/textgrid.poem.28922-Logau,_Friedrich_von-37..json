{"textgrid.poem.28922": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ehstes wird die b\u00f6se Zeit kummen auff die Bahre;", "tokens": ["Ehs\u00b7tes", "wird", "die", "b\u00f6\u00b7se", "Zeit", "kum\u00b7men", "auff", "die", "Bah\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ehstes werden werden jung tausend g\u00f6ldne Jahre;", "tokens": ["Ehs\u00b7tes", "wer\u00b7den", "wer\u00b7den", "jung", "tau\u00b7send", "g\u00f6ld\u00b7ne", "Jah\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "VAFIN", "ADJD", "CARD", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wie es scheint, kans auch wol seyn; dann solch Gold zu kochen,", "tokens": ["Wie", "es", "scheint", ",", "kans", "auch", "wol", "seyn", ";", "dann", "solch", "Gold", "zu", "ko\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VMFIN", "ADV", "ADV", "VAINF", "$.", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Hat zu Kohlen, Stadt und Dorff Krieg schon abgebrochen.", "tokens": ["Hat", "zu", "Koh\u00b7len", ",", "Stadt", "und", "Dorff", "Krieg", "schon", "ab\u00b7ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "NN", "ADV", "VVPP", "$."], "meter": "--+-+-++-+-+-", "measure": "anapaest.init"}}}}}