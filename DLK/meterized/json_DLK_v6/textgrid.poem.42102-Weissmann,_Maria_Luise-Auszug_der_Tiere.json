{"textgrid.poem.42102": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Auszug der Tiere", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es waren eingekreist die ahnungslosen", "tokens": ["Es", "wa\u00b7ren", "ein\u00b7ge\u00b7kreist", "die", "ah\u00b7nungs\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verirrten Tiere eh sie sich versahn", "tokens": ["Ver\u00b7irr\u00b7ten", "Tie\u00b7re", "eh", "sie", "sich", "ver\u00b7sahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KOUS", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Wand und Wand. Ganz fern im Grenzenlosen", "tokens": ["Von", "Wand", "und", "Wand", ".", "Ganz", "fern", "im", "Gren\u00b7zen\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$.", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zog noch von Himmel eine blasse Bahn.", "tokens": ["Zog", "noch", "von", "Him\u00b7mel", "ei\u00b7ne", "blas\u00b7se", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In einer Nacht war Mond in ihren Tr\u00e4umen.", "tokens": ["In", "ei\u00b7ner", "Nacht", "war", "Mond", "in", "ih\u00b7ren", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie brachen auf, gezogen in das fahle", "tokens": ["Sie", "bra\u00b7chen", "auf", ",", "ge\u00b7zo\u00b7gen", "in", "das", "fah\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "VVPP", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tr\u00fcgende Licht. Und wie ins Laub von B\u00e4umen", "tokens": ["Tr\u00fc\u00b7gen\u00b7de", "Licht", ".", "Und", "wie", "ins", "Laub", "von", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "KON", "PWAV", "APPRART", "NN", "APPR", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Stiegen sie ins Ge\u00e4st der Kathedrale.", "tokens": ["Stie\u00b7gen", "sie", "ins", "Ge\u00b7\u00e4st", "der", "Ka\u00b7thed\u00b7ra\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-++-+---+-", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Und stiegen tr\u00e4umend fort bis in das letzte", "tokens": ["Und", "stie\u00b7gen", "tr\u00e4u\u00b7mend", "fort", "bis", "in", "das", "letz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "KON", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gezweig der Giebel und erwachten kaum", "tokens": ["Ge\u00b7zweig", "der", "Gie\u00b7bel", "und", "er\u00b7wach\u00b7ten", "kaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als sich ihr Fu\u00df hinaus ins Leere setzte:", "tokens": ["Als", "sich", "ihr", "Fu\u00df", "hin\u00b7aus", "ins", "Lee\u00b7re", "setz\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "APZR", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie fanden sich verstiegen in dem Raum,", "tokens": ["Sie", "fan\u00b7den", "sich", "ver\u00b7stie\u00b7gen", "in", "dem", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Erde nicht und der nicht Himmel hie\u00df,", "tokens": ["Der", "Er\u00b7de", "nicht", "und", "der", "nicht", "Him\u00b7mel", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "KON", "ART", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ganz heimatlos. Sie starrten in des Lichts", "tokens": ["Ganz", "hei\u00b7mat\u00b7los", ".", "Sie", "starr\u00b7ten", "in", "des", "Lichts"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ziehenden Strahl bis sie der Blick verlie\u00df", "tokens": ["Zie\u00b7hen\u00b7den", "Strahl", "bis", "sie", "der", "Blick", "ver\u00b7lie\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "PPER", "ART", "NN", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und sie versteinten, irren Angesichts.", "tokens": ["Und", "sie", "ver\u00b7stein\u00b7ten", ",", "ir\u00b7ren", "An\u00b7ge\u00b7sichts", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}