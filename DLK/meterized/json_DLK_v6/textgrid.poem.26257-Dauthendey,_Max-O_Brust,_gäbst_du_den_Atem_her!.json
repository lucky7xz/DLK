{"textgrid.poem.26257": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "O Brust, g\u00e4bst du den Atem her!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Brust, g\u00e4bst du den Atem her,", "tokens": ["O", "Brust", ",", "g\u00e4bst", "du", "den", "A\u00b7tem", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du hebst doch nicht das bange Meer,", "tokens": ["Du", "hebst", "doch", "nicht", "das", "ban\u00b7ge", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Darauf sich schwer mein Heimweh wiegt.", "tokens": ["Da\u00b7rauf", "sich", "schwer", "mein", "Heim\u00b7weh", "wiegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zu Hause sind die St\u00e4dte leer,", "tokens": ["Zu", "Hau\u00b7se", "sind", "die", "St\u00e4d\u00b7te", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und viele deckt die Erde schwer.", "tokens": ["Und", "vie\u00b7le", "deckt", "die", "Er\u00b7de", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Jugend gibt ihr Blut und siegt.", "tokens": ["Die", "Ju\u00b7gend", "gibt", "ihr", "Blut", "und", "siegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Luft voll toter Helden fliegt.", "tokens": ["Die", "Luft", "voll", "to\u00b7ter", "Hel\u00b7den", "fliegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Brust, ich wei\u00df es bald nicht mehr,", "tokens": ["O", "Brust", ",", "ich", "wei\u00df", "es", "bald", "nicht", "mehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob Deutschland noch auf Erden liegt.", "tokens": ["Ob", "Deutschland", "noch", "auf", "Er\u00b7den", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}