{"textgrid.poem.40704": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "[\u00bbDas Staatsschiff\u00ab wie bezeichnend trifft]", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdas Staatsschiff\u00ab \u2013 wie bezeichnend trifft", "tokens": ["\u00bb", "das", "Staats\u00b7schiff", "\u00ab", "\u2013", "wie", "be\u00b7zeich\u00b7nend", "trifft"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "$(", "$(", "PWAV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Bild hier den Gedanken!", "tokens": ["Das", "Bild", "hier", "den", "Ge\u00b7dan\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df wir seit Langem eingeschifft,", "tokens": ["Da\u00df", "wir", "seit", "Lan\u00b7gem", "ein\u00b7ge\u00b7schifft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man f\u00fchlt's am steten Schwanken.", "tokens": ["Man", "f\u00fchlt's", "am", "ste\u00b7ten", "Schwan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}