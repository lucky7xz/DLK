{"textgrid.poem.50463": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "51. Gl\u00fcck der Unversch\u00e4mten", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einem Wirtshaus die Geehrtste,", "tokens": ["In", "ei\u00b7nem", "Wirts\u00b7haus", "die", "Ge\u00b7ehrts\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und in der Schule die Gelehrtste;", "tokens": ["Und", "in", "der", "Schu\u00b7le", "die", "Ge\u00b7lehrts\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Kl\u00fcgste nach misslungnem Raht,", "tokens": ["Die", "Kl\u00fcgs\u00b7te", "nach", "miss\u00b7lung\u00b7nem", "Raht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und tapferste nach einer That.", "tokens": ["Und", "tap\u00b7fers\u00b7te", "nach", "ei\u00b7ner", "That", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Die gr\u00f6sste Helden im Erdulden,", "tokens": ["Die", "gr\u00f6ss\u00b7te", "Hel\u00b7den", "im", "E\u00b7rdul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und beste Schl\u00e4ffer mit viel Schulden;", "tokens": ["Und", "bes\u00b7te", "Schl\u00e4f\u00b7fer", "mit", "viel", "Schul\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die weitste Wandrer in der Welt,", "tokens": ["Die", "weits\u00b7te", "Wand\u00b7rer", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als sch\u00e4rffste Spieler, ohne Geld,", "tokens": ["Als", "sch\u00e4rffs\u00b7te", "Spie\u00b7ler", ",", "oh\u00b7ne", "Geld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die G\u00fcnstling' unversuchter Weiber,", "tokens": ["Die", "G\u00fcnst\u00b7ling'", "un\u00b7ver\u00b7such\u00b7ter", "Wei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und der Lucretzen Zeitvertreiber;", "tokens": ["Und", "der", "Lu\u00b7cret\u00b7zen", "Zeit\u00b7ver\u00b7trei\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Am frembden Tisch die Ersten satt,", "tokens": ["Am", "fremb\u00b7den", "Tisch", "die", "Ers\u00b7ten", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und reich, wo niemand sonst was hatt,", "tokens": ["Und", "reich", ",", "wo", "nie\u00b7mand", "sonst", "was", "hatt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "PIS", "ADV", "PWS", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die beste Heuchler, L\u00fcgner, W\u00e4scher,", "tokens": ["Die", "bes\u00b7te", "Heuch\u00b7ler", ",", "L\u00fcg\u00b7ner", ",", "W\u00e4\u00b7scher", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wahrsager, Aertzt' und Zungendrescher;", "tokens": ["Wahr\u00b7sa\u00b7ger", ",", "A\u00b7ertzt'", "und", "Zun\u00b7gen\u00b7dre\u00b7scher", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.15": {"text": "Die Erst' im Welt- und Kirchenstand:", "tokens": ["Die", "Er\u00b7st'", "im", "Welt", "und", "Kir\u00b7chen\u00b7stand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "TRUNC", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}