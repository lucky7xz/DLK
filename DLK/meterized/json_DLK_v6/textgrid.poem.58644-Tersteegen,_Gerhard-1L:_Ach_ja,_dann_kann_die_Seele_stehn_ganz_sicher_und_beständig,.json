{"textgrid.poem.58644": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach ja, dann kann die Seele stehn ganz sicher und best\u00e4ndig,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach ja, dann kann die Seele stehn ganz sicher und best\u00e4ndig,", "tokens": ["Ach", "ja", ",", "dann", "kann", "die", "See\u00b7le", "stehn", "ganz", "si\u00b7cher", "und", "be\u00b7st\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "ADV", "VMFIN", "ART", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Wenn sie bei Gott daheime bleibt, und Gott bei ihr inwendig;", "tokens": ["Wenn", "sie", "bei", "Gott", "da\u00b7hei\u00b7me", "bleibt", ",", "und", "Gott", "bei", "ihr", "in\u00b7wen\u00b7dig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "VVFIN", "$,", "KON", "NN", "APPR", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Es f\u00fcrchtet solche Seele selbst der h\u00f6llische Tyrann,", "tokens": ["Es", "f\u00fcrch\u00b7tet", "sol\u00b7che", "See\u00b7le", "selbst", "der", "h\u00f6l\u00b7li\u00b7sche", "Ty\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Versucht er gleich, so wird sie doch nicht \u00fcberw\u00e4ltigt dann.", "tokens": ["Ver\u00b7sucht", "er", "gleich", ",", "so", "wird", "sie", "doch", "nicht", "\u00fc\u00b7berw\u00b7\u00e4l\u00b7tigt", "dann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Und ob die Seel' im Finstern ging' und schien der Herr ihr ferne,", "tokens": ["Und", "ob", "die", "Seel'", "im", "Fins\u00b7tern", "ging'", "und", "schien", "der", "Herr", "ihr", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Auch wenn die Seel' fast unterliegt, h\u00e4lt Gott sie heimlich fest,", "tokens": ["Auch", "wenn", "die", "Seel'", "fast", "un\u00b7ter\u00b7liegt", ",", "h\u00e4lt", "Gott", "sie", "heim\u00b7lich", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADV", "VVFIN", "$,", "VVFIN", "NN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Er rei\u00dft sie aus und endlich gar von aller S\u00fcnd' erl\u00f6st.", "tokens": ["Er", "rei\u00dft", "sie", "aus", "und", "end\u00b7lich", "gar", "von", "al\u00b7ler", "S\u00fcnd'", "er\u00b7l\u00f6st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "ADV", "ADV", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}