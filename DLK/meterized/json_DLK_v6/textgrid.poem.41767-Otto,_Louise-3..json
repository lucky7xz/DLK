{"textgrid.poem.41767": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir ist so froh, mir ist so leicht zu Sinnen,", "tokens": ["Mir", "ist", "so", "froh", ",", "mir", "ist", "so", "leicht", "zu", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und doch trennt uns des strengen Kerkers Gitter,", "tokens": ["Und", "doch", "trennt", "uns", "des", "stren\u00b7gen", "Ker\u00b7kers", "Git\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Und zeigt mir ganz, wie das Geschick so bitter,", "tokens": ["Und", "zeigt", "mir", "ganz", ",", "wie", "das", "Ge\u00b7schick", "so", "bit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das mich nach kurzem Gru\u00dfe treibt von hinnen.", "tokens": ["Das", "mich", "nach", "kur\u00b7zem", "Gru\u00b7\u00dfe", "treibt", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "ADJA", "NN", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das ist die Macht im selig s\u00fc\u00dfem Minnen,", "tokens": ["Das", "ist", "die", "Macht", "im", "se\u00b7lig", "s\u00fc\u00b7\u00dfem", "Min\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie es mit Dir mich eint, mein holder Ritter!", "tokens": ["Wie", "es", "mit", "Dir", "mich", "eint", ",", "mein", "hol\u00b7der", "Rit\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "PRF", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da wird der Schmerz zum fliehenden Gewitter", "tokens": ["Da", "wird", "der", "Schmerz", "zum", "flie\u00b7hen\u00b7den", "Ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von dem die Fluren Segen nur gewinnen!", "tokens": ["Von", "dem", "die", "Flu\u00b7ren", "Se\u00b7gen", "nur", "ge\u00b7win\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Himmel \u00fcber uns er bleibt uns offen,", "tokens": ["Der", "Him\u00b7mel", "\u00fc\u00b7ber", "uns", "er", "bleibt", "uns", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sonne bleibt in ihrem Glanze thronen,", "tokens": ["Die", "Son\u00b7ne", "bleibt", "in", "ih\u00b7rem", "Glan\u00b7ze", "thro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und M\u00e4rzenluft, die k\u00fcndet Fr\u00fchlingszeit!", "tokens": ["Und", "M\u00e4r\u00b7zen\u00b7luft", ",", "die", "k\u00fcn\u00b7det", "Fr\u00fch\u00b7lings\u00b7zeit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Drum la\u00df nicht ab vom Gottvertraun und Hoffen:", "tokens": ["Drum", "la\u00df", "nicht", "ab", "vom", "Gott\u00b7ver\u00b7traun", "und", "Hof\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "PTKVZ", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Liebe sch\u00f6nste Paradieseszonen", "tokens": ["Der", "Lie\u00b7be", "sch\u00f6ns\u00b7te", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7zo\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erwarten uns noch so viel Qual und Leid!", "tokens": ["Er\u00b7war\u00b7ten", "uns", "noch", "so", "viel", "Qual", "und", "Leid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}