{"dta.poem.18912": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Cardinal  \n  De Richelieu. &c.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Franckreich/ dein ist der Sig. Du bist der ", "tokens": ["Fran\u00b7ck\u00b7reich", "/", "dein", "ist", "der", "Sig", ".", "Du", "bist", "der"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$(", "PPOSAT", "VAFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ort/", "tokens": ["Ort", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Das beste Reich der Welt/ gleichlo\u00df durch Got-", "tokens": ["Das", "bes\u00b7te", "Reich", "der", "Welt", "/", "gleic\u00b7hlo\u00df", "durch", "Got"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$(", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "tes seegen;", "tokens": ["tes", "see\u00b7gen", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Zwar nicht/ weil fruchtreich du der frembden trost", "tokens": ["Zwar", "nicht", "/", "weil", "fruch\u00b7treich", "du", "der", "fremb\u00b7den", "trost"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "$(", "KOUS", "ADJD", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "vnd hort/", "tokens": ["vnd", "hort", "/"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Auch nit weil deine leut/ wie du/ gut zuth\u0169 pflegen:", "tokens": ["Auch", "nit", "weil", "dei\u00b7ne", "leut", "/", "wie", "du", "/", "gut", "zuth\u0169", "pfle\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "KOUS", "PPOSAT", "NN", "$(", "PWAV", "PPER", "$(", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+----+-+-", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht weil du der Lieb sitz; der sturmleydenden port/", "tokens": ["Nicht", "weil", "du", "der", "Lieb", "sitz", ";", "der", "sturm\u00b7ley\u00b7den\u00b7den", "port", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPER", "ART", "NN", "NE", "$.", "ART", "ADJA", "NE", "$("], "meter": "-+--++-++--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nicht deines weisen Rahts/ vnd dapfern Adels", "tokens": ["Nicht", "dei\u00b7nes", "wei\u00b7sen", "Rahts", "/", "vnd", "dap\u00b7fern", "A\u00b7dels"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NN", "$(", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wegen;", "tokens": ["we\u00b7gen", ";"], "token_info": ["word", "punct"], "pos": ["APPR", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Noch weil dein K\u00f6nig gro\u00df durch seine werck vnd", "tokens": ["Noch", "weil", "dein", "K\u00f6\u00b7nig", "gro\u00df", "durch", "sei\u00b7ne", "werck", "vnd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "ADJD", "APPR", "PPOSAT", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "wort", "tokens": ["wort"], "token_info": ["word"], "pos": ["NN"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Bezeuget/ dz jhm nichts dan dein hayl angelegen.", "tokens": ["Be\u00b7zeu\u00b7get", "/", "dz", "jhm", "nichts", "dan", "dein", "hayl", "an\u00b7ge\u00b7le\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "PPER", "PIS", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nein. Sondern weil dir wehrt ein solcher Cardinal/", "tokens": ["Nein", ".", "Son\u00b7dern", "weil", "dir", "wehrt", "ein", "sol\u00b7cher", "Car\u00b7di\u00b7nal", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "KON", "KOUS", "PPER", "VVFIN", "ART", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df jhm auch an verdienst sunst kein mensch", "tokens": ["Da\u00df", "jhm", "auch", "an", "ver\u00b7dienst", "sunst", "kein", "mensch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "gleich zu finden/", "tokens": ["gleich", "zu", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wan in der weitten welt dir Got schon geb die", "tokens": ["Wan", "in", "der", "weit\u00b7ten", "welt", "dir", "Got", "schon", "geb", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "PPER", "NN", "ADV", "VVFIN", "ART"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "wahl.", "tokens": ["wahl", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "Darumb la\u00df weder forcht noch hoffnung dich ver-", "tokens": ["Da\u00b7rumb", "la\u00df", "we\u00b7der", "forcht", "noch", "hoff\u00b7nung", "dich", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "KON", "VVFIN", "ADV", "NN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "blinden/", "tokens": ["blin\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Dieweil gantz seelig du/ durch Got vn\u0303 jhn zu mahl", "tokens": ["Die\u00b7weil", "gantz", "see\u00b7lig", "du", "/", "durch", "Got", "v\u00f1", "jhn", "zu", "mahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJD", "PPER", "$(", "APPR", "NN", "APPR", "PPER", "APPR", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kanst/ so du wilt/ die welt/ dich kein feind vber-", "tokens": ["Kanst", "/", "so", "du", "wilt", "/", "die", "welt", "/", "dich", "kein", "feind", "vber"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "$(", "ADV", "PPER", "VMFIN", "$(", "ART", "NN", "$(", "PPER", "PIAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "winden.", "tokens": ["win\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}