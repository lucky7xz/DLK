{"textgrid.poem.47678": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich m\u00f6chte wohl ein Spiegel sein]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00f6chte wohl ein Spiegel sein", "tokens": ["Ich", "m\u00f6ch\u00b7te", "wohl", "ein", "Spie\u00b7gel", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und im Gemache blinken,", "tokens": ["Und", "im", "Ge\u00b7ma\u00b7che", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Um jene reine Wohlgestalt", "tokens": ["Um", "je\u00b7ne", "rei\u00b7ne", "Wohl\u00b7ge\u00b7stalt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ohn' Unterla\u00df zu trinken,", "tokens": ["Ohn'", "Un\u00b7ter\u00b7la\u00df", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu staunen ihr, so oft sie steht,", "tokens": ["Zu", "stau\u00b7nen", "ihr", ",", "so", "oft", "sie", "steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "$,", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Brust und Locke zieret,", "tokens": ["Und", "Brust", "und", "Lo\u00b7cke", "zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wiederum ganz Aug' zu sein,", "tokens": ["Und", "wie\u00b7de\u00b7rum", "ganz", "Aug'", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So oft sie sich entschn\u00fcret.", "tokens": ["So", "oft", "sie", "sich", "ent\u00b7schn\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}