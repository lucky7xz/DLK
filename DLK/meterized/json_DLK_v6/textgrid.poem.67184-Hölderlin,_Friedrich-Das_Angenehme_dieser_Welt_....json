{"textgrid.poem.67184": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Angenehme dieser Welt ...", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Angenehme dieser Welt hab ich genossen,", "tokens": ["Das", "An\u00b7ge\u00b7neh\u00b7me", "die\u00b7ser", "Welt", "hab", "ich", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Jugendstunden sind, wie lang! wie lang! verflossen,", "tokens": ["Die", "Ju\u00b7gend\u00b7stun\u00b7den", "sind", ",", "wie", "lang", "!", "wie", "lang", "!", "ver\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PWAV", "ADJD", "$.", "PWAV", "ADJD", "$.", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "April und Mai und Julius sind ferne,", "tokens": ["Ap\u00b7ril", "und", "Mai", "und", "Ju\u00b7lius", "sind", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NE", "VAFIN", "ADV", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich bin nichts mehr, ich lebe nicht mehr gerne!", "tokens": ["Ich", "bin", "nichts", "mehr", ",", "ich", "le\u00b7be", "nicht", "mehr", "ger\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}