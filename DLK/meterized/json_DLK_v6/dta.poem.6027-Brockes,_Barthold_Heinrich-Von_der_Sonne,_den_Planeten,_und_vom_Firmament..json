{"dta.poem.6027": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann ihre Gluht von uns entfernt ist und verstecket,", "tokens": ["Wann", "ih\u00b7re", "Gluht", "von", "uns", "ent\u00b7fernt", "ist", "und", "ver\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "PPER", "ADJD", "VAFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da sie der Mohren Strand verdecket;", "tokens": ["Da", "sie", "der", "Moh\u00b7ren", "Strand", "ver\u00b7de\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erleuchtet sie uns doch", "tokens": ["Er\u00b7leuch\u00b7tet", "sie", "uns", "doch"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von jenem halben Welt-Theil noch.", "tokens": ["Von", "je\u00b7nem", "hal\u00b7ben", "Welt\u00b7T\u00b7heil", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es stellen ihren Strahl uns klar", "tokens": ["Es", "stel\u00b7len", "ih\u00b7ren", "Strahl", "uns", "klar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verschiedne runde Spiegel dar.", "tokens": ["Ver\u00b7schied\u00b7ne", "run\u00b7de", "Spie\u00b7gel", "dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es zeiget uns die Sonn\u2019 ihr helles Licht", "tokens": ["Es", "zei\u00b7get", "uns", "die", "Sonn'", "ihr", "hel\u00b7les", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Auf der Planeten Angesicht.", "tokens": ["Auf", "der", "Pla\u00b7ne\u00b7ten", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Wir haben erst gesehn, wie sie gemacht: wir kennen,", "tokens": ["Wir", "ha\u00b7ben", "erst", "ge\u00b7sehn", ",", "wie", "sie", "ge\u00b7macht", ":", "wir", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "PWAV", "PPER", "VVPP", "$.", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sam\u0303t ihrem Glantz und R\u00fcnd\u2019, auch ihr so schnelles Rennen,", "tokens": ["Sam\u0303t", "ih\u00b7rem", "Glantz", "und", "R\u00fcnd'", ",", "auch", "ihr", "so", "schnel\u00b7les", "Ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "$,", "ADV", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jhr\u2019 unterschiedne Gr\u00f6\u00df\u2019, und die verschiedne Weise", "tokens": ["Ih\u00b7r'", "un\u00b7ter\u00b7schied\u00b7ne", "Gr\u00f6\u00df'", ",", "und", "die", "ver\u00b7schied\u00b7ne", "Wei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "Des Zustands ihrer grossen Creise,", "tokens": ["Des", "Zu\u00b7stands", "ih\u00b7rer", "gros\u00b7sen", "Crei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wie der Planeten Meng\u2019 in Himmels-L\u00fcfften schwimmt,", "tokens": ["Wie", "der", "Pla\u00b7ne\u00b7ten", "Meng'", "in", "Him\u00b7mels\u00b7L\u00fcff\u00b7ten", "schwimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NE", "APPR", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Und, durch Gewicht und Gr\u00f6\u00df\u2019, gewisse Stellen nimmt,", "tokens": ["Und", ",", "durch", "Ge\u00b7wicht", "und", "Gr\u00f6\u00df'", ",", "ge\u00b7wis\u00b7se", "Stel\u00b7len", "nimmt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "KON", "NN", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So, wie wir in der Fluht an harten C\u00f6rpern sehen.", "tokens": ["So", ",", "wie", "wir", "in", "der", "Fluht", "an", "har\u00b7ten", "C\u00f6r\u00b7pern", "se\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sie treiben so, wie sie, auch in verschiednen H\u00f6hen.", "tokens": ["Sie", "trei\u00b7ben", "so", ",", "wie", "sie", ",", "auch", "in", "ver\u00b7schied\u00b7nen", "H\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "$,", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}