{"textgrid.poem.37469": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es sa\u00df der fromme Meister", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sa\u00df der fromme Meister", "tokens": ["Es", "sa\u00df", "der", "from\u00b7me", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Weib und Kind bei Tisch.", "tokens": ["Mit", "Weib", "und", "Kind", "bei", "Tisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach, seine Lebensgeister", "tokens": ["Ach", ",", "sei\u00b7ne", "Le\u00b7bens\u00b7geis\u00b7ter"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sind nicht wie sonst so frisch.", "tokens": ["Sind", "nicht", "wie", "sonst", "so", "frisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "KOKOM", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er sitzt mit krummem Nacken", "tokens": ["Er", "sitzt", "mit", "krum\u00b7mem", "Na\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor seinem Leibgericht,", "tokens": ["Vor", "sei\u00b7nem", "Leib\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er h\u00e4lt sich beide Backen,", "tokens": ["Er", "h\u00e4lt", "sich", "bei\u00b7de", "Ba\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Worin es heftig sticht.", "tokens": ["Wo\u00b7rin", "es", "hef\u00b7tig", "sticht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das brennt wie hei\u00dfe Kohlen.", "tokens": ["Das", "brennt", "wie", "hei\u00b7\u00dfe", "Koh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Au, schreit er, au, verdammt!", "tokens": ["Au", ",", "schreit", "er", ",", "au", ",", "ver\u00b7dammt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "$,", "NE", "$,", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Teufel soll sie holen,", "tokens": ["Der", "Teu\u00b7fel", "soll", "sie", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Z\u00e4hne allesamt!", "tokens": ["Die", "Z\u00e4h\u00b7ne", "al\u00b7le\u00b7samt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch gleich, wie es in N\u00f6ten", "tokens": ["Doch", "gleich", ",", "wie", "es", "in", "N\u00f6\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl \u00f6fter schon geschah,", "tokens": ["Wohl", "\u00f6f\u00b7ter", "schon", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Begann er laut zu beten:", "tokens": ["Be\u00b7gann", "er", "laut", "zu", "be\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hilf, Apollonia!", "tokens": ["Hilf", ",", "A\u00b7pol\u00b7lo\u00b7nia", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Kaum, da\u00df aus voller Seele", "tokens": ["Kaum", ",", "da\u00df", "aus", "vol\u00b7ler", "See\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er diesen Spruch getan,", "tokens": ["Er", "die\u00b7sen", "Spruch", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00e4llt aus des Mundes H\u00f6hle", "tokens": ["F\u00e4llt", "aus", "des", "Mun\u00b7des", "H\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm pl\u00f6tzlich jeder Zahn.", "tokens": ["Ihm", "pl\u00f6tz\u00b7lich", "je\u00b7der", "Zahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und schmerzlos, Dank dem Himmel,", "tokens": ["Und", "schmerz\u00b7los", ",", "Dank", "dem", "Him\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schmaust er, wie 's sonst der Brauch,", "tokens": ["Schmaust", "er", ",", "wie", "'s", "sonst", "der", "Brauch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur war es mehr Gem\u00fcmmel,", "tokens": ["Nur", "war", "es", "mehr", "Ge\u00b7m\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und lispeln t\u00e4t er auch.", "tokens": ["Und", "lis\u00b7peln", "t\u00e4t", "er", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Pohsit! Wie klingt so niedlich", "tokens": ["Poh\u00b7sit", "!", "Wie", "klingt", "so", "nied\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Meisters S\u00e4uselton.", "tokens": ["Des", "Meis\u00b7ters", "S\u00e4u\u00b7sel\u00b7ton", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er trank, entschlummert friedlich,", "tokens": ["Er", "trank", ",", "ent\u00b7schlum\u00b7mert", "fried\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und horch, da schnarcht er schon.", "tokens": ["Und", "horch", ",", "da", "schnarcht", "er", "schon", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}