{"textgrid.poem.49930": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "4. Kapitelsaal", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieder und wieder bestaun ich die Pracht der romanischen Halle,", "tokens": ["Wie\u00b7der", "und", "wie\u00b7der", "be\u00b7staun", "ich", "die", "Pracht", "der", "ro\u00b7ma\u00b7ni\u00b7schen", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Herrliche Bogen, auf kurzst\u00e4mmige S\u00e4ulen gestellt.", "tokens": ["Herr\u00b7li\u00b7che", "Bo\u00b7gen", ",", "auf", "kurz\u00b7st\u00e4m\u00b7mi\u00b7ge", "S\u00e4u\u00b7len", "ge\u00b7stellt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Rauh von Korn ist der Stein, doch nahm er willig die Zierde", "tokens": ["Rauh", "von", "Korn", "ist", "der", "Stein", ",", "doch", "nahm", "er", "wil\u00b7lig", "die", "Zier\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VAFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Auch zu der Gro\u00dfheit auf, welche die Massen beseelt.", "tokens": ["Auch", "zu", "der", "Gro\u00df\u00b7heit", "auf", ",", "wel\u00b7che", "die", "Mas\u00b7sen", "be\u00b7seelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Nur ein d\u00fcsteres Halblicht sendet der Tag durch die schmalen", "tokens": ["Nur", "ein", "d\u00fcs\u00b7te\u00b7res", "Halb\u00b7licht", "sen\u00b7det", "der", "Tag", "durch", "die", "schma\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "--+--+----+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Fenster herein und streift dort ein vergessenes Grab.", "tokens": ["Fens\u00b7ter", "her\u00b7ein", "und", "streift", "dort", "ein", "ver\u00b7ges\u00b7se\u00b7nes", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Rudolph dem Stifter, und ihr, Mechthildis, der frommen, verg\u00f6nnte", "tokens": ["Ru\u00b7dolph", "dem", "Stif\u00b7ter", ",", "und", "ihr", ",", "Mecht\u00b7hil\u00b7dis", ",", "der", "from\u00b7men", ",", "ver\u00b7g\u00f6nn\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "ART", "NN", "$,", "KON", "PPER", "$,", "NE", "$,", "ART", "ADJA", "$,", "VVFIN"], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.8": {"text": "Dankbar das Kloster, im Port seiner Geweihten zu ruhn.", "tokens": ["Dank\u00b7bar", "das", "Klos\u00b7ter", ",", "im", "Port", "sei\u00b7ner", "Ge\u00b7weih\u00b7ten", "zu", "ruhn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "APPRART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}}}}