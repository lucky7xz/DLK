{"textgrid.poem.36298": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diana, heil'ge, wo sind deine Br\u00fcste?", "tokens": ["Di\u00b7a\u00b7na", ",", "heil'\u00b7ge", ",", "wo", "sind", "dei\u00b7ne", "Br\u00fcs\u00b7te", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begeist'rung trinkt der L\u00f6we sich im Blute,", "tokens": ["Be\u00b7geist'\u00b7rung", "trinkt", "der", "L\u00f6\u00b7we", "sich", "im", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NE", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Titanen schwellt der Wein zum \u00dcbermute,", "tokens": ["Ti\u00b7ta\u00b7nen", "schwellt", "der", "Wein", "zum", "\u00dc\u00b7ber\u00b7mu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dianas Milch war Sehern wild Gel\u00fcste.", "tokens": ["Di\u00b7a\u00b7nas", "Milch", "war", "Se\u00b7hern", "wild", "Ge\u00b7l\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Umklirrt blieb still, als ob es nichts nicht w\u00fc\u00dfte,", "tokens": ["Um\u00b7klirrt", "blieb", "still", ",", "als", "ob", "es", "nichts", "nicht", "w\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKVZ", "$,", "KOKOM", "KOUS", "PPER", "PIS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das R\u00e4tselbild, wie auch der Taumel flute,", "tokens": ["Das", "R\u00e4t\u00b7sel\u00b7bild", ",", "wie", "auch", "der", "Tau\u00b7mel", "flu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis matt vom Waffentanz der Priester ruhte,", "tokens": ["Bis", "matt", "vom", "Waf\u00b7fen\u00b7tanz", "der", "Pries\u00b7ter", "ruh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der gro\u00dfen G\u00f6ttin tiefsten Saum noch k\u00fc\u00dfte.", "tokens": ["Der", "gro\u00b7\u00dfen", "G\u00f6t\u00b7tin", "tiefs\u00b7ten", "Saum", "noch", "k\u00fc\u00df\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Diana, heil'ge, reich' zum Tanz die Waffen!", "tokens": ["Di\u00b7a\u00b7na", ",", "heil'\u00b7ge", ",", "reich'", "zum", "Tanz", "die", "Waf\u00b7fen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$,", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ich der Br\u00fcste F\u00fcll' im Marmor schaute,", "tokens": ["Als", "ich", "der", "Br\u00fcs\u00b7te", "F\u00fcll'", "im", "Mar\u00b7mor", "schau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da ward von deiner Milch das Herz mir trunken;", "tokens": ["Da", "ward", "von", "dei\u00b7ner", "Milch", "das", "Herz", "mir", "trun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und ob ich gleich im Mark vor dir ergraute,", "tokens": ["Und", "ob", "ich", "gleich", "im", "Mark", "vor", "dir", "er\u00b7grau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So f\u00fchlt' ich Kraft auch, nimmer zu erschlaffen,", "tokens": ["So", "f\u00fchlt'", "ich", "Kraft", "auch", ",", "nim\u00b7mer", "zu", "er\u00b7schlaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADV", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bleib' in Mysterien ewig nun versunken.", "tokens": ["Bleib'", "in", "Mys\u00b7te\u00b7ri\u00b7en", "e\u00b7wig", "nun", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}