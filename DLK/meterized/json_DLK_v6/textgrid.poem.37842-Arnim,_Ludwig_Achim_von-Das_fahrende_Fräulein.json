{"textgrid.poem.37842": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Das fahrende Fr\u00e4ulein", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O weh der Zeit, die ich verzehrt", "tokens": ["O", "weh", "der", "Zeit", ",", "die", "ich", "ver\u00b7zehrt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit meiner Buhler Orden,", "tokens": ["Mit", "mei\u00b7ner", "Buh\u00b7ler", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nachreu ist worden mein Gef\u00e4hrt,", "tokens": ["Nach\u00b7reu", "ist", "wor\u00b7den", "mein", "Ge\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VAPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bin zur Th\u00f6rin worden.", "tokens": ["Ich", "bin", "zur", "Th\u00f6\u00b7rin", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mich reut die Schmink und falscher Flei\u00df,", "tokens": ["Mich", "reut", "die", "Schmink", "und", "fal\u00b7scher", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den ich darauf gewendet,", "tokens": ["Den", "ich", "da\u00b7rauf", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PAV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Sonne schien, ich baut auf Eis,", "tokens": ["Die", "Son\u00b7ne", "schien", ",", "ich", "baut", "auf", "Eis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So war ich schier verblendet.", "tokens": ["So", "war", "ich", "schier", "ver\u00b7blen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie wird es hei\u00df, fort zieht das Eis,", "tokens": ["Wie", "wird", "es", "hei\u00df", ",", "fort", "zieht", "das", "Eis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,", "PTKVZ", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und meine goldnen Schl\u00f6sser,", "tokens": ["Und", "mei\u00b7ne", "gold\u00b7nen", "Schl\u00f6s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie ruft es doch im Flusse leis,", "tokens": ["Wie", "ruft", "es", "doch", "im", "Flus\u00b7se", "leis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da drunten w\u00e4r es besser.", "tokens": ["Da", "drun\u00b7ten", "w\u00e4r", "es", "bes\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wie sie in das Wasser f\u00e4llt,", "tokens": ["Und", "wie", "sie", "in", "das", "Was\u00b7ser", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da hat sie fest gehalten,", "tokens": ["Da", "hat", "sie", "fest", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Liebste, dem sie nachgestellt,", "tokens": ["Der", "Liebs\u00b7te", ",", "dem", "sie", "nach\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An ihres Schleyers Falten.", "tokens": ["An", "ih\u00b7res", "Schley\u00b7ers", "Fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "La\u00df mir den Schleyer, halt mich nicht,", "tokens": ["La\u00df", "mir", "den", "Schle\u00b7yer", ",", "halt", "mich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df still mich 'nunter ziehen,", "tokens": ["La\u00df", "still", "mich", "'n\u00b7un\u00b7ter", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn mein verst\u00f6rtes Angesicht,", "tokens": ["Denn", "mein", "ver\u00b7st\u00f6r\u00b7tes", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das w\u00fcrde nach dich ziehen.", "tokens": ["Das", "w\u00fcr\u00b7de", "nach", "dich", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Strom ist stark, sein Arm zu schwach,", "tokens": ["Der", "Strom", "ist", "stark", ",", "sein", "Arm", "zu", "schwach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie will den Schleyer nicht lassen,", "tokens": ["Sie", "will", "den", "Schle\u00b7yer", "nicht", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So zieht verlorne Liebe nach,", "tokens": ["So", "zieht", "ver\u00b7lor\u00b7ne", "Lie\u00b7be", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er wollte sie nicht verlassen.", "tokens": ["Er", "woll\u00b7te", "sie", "nicht", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}