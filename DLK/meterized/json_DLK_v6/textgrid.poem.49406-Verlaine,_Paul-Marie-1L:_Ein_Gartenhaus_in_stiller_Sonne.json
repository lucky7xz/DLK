{"textgrid.poem.49406": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Gartenhaus in stiller Sonne", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Gartenhaus in stiller Sonne", "tokens": ["Ein", "Gar\u00b7ten\u00b7haus", "in", "stil\u00b7ler", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Birgt sanft und z\u00e4rtlich unsre Wonne,", "tokens": ["Birgt", "sanft", "und", "z\u00e4rt\u00b7lich", "uns\u00b7re", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Umweht von lieber Rosen Luft.", "tokens": ["Um\u00b7weht", "von", "lie\u00b7ber", "Ro\u00b7sen", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Rosen Atem s\u00fcss und linde", "tokens": ["Der", "Ro\u00b7sen", "A\u00b7tem", "s\u00fcss", "und", "lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mischt sich im leichten Sommerwinde", "tokens": ["Mischt", "sich", "im", "leich\u00b7ten", "Som\u00b7mer\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit ihres Haares holdem Duft.", "tokens": ["Mit", "ih\u00b7res", "Haa\u00b7res", "hol\u00b7dem", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr Blick war ihrer K\u00fchnheit Pfand,", "tokens": ["Ihr", "Blick", "war", "ih\u00b7rer", "K\u00fchn\u00b7heit", "Pfand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es gab mir ihrer Lippen Bl\u00fcte", "tokens": ["Es", "gab", "mir", "ih\u00b7rer", "Lip\u00b7pen", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Fieber, das bet\u00f6rend gl\u00fchte.", "tokens": ["Ein", "Fie\u00b7ber", ",", "das", "be\u00b7t\u00f6\u00b7rend", "gl\u00fch\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da Liebe nicht den Hunger bannt,", "tokens": ["Da", "Lie\u00b7be", "nicht", "den", "Hun\u00b7ger", "bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So mu\u00df Sorbet mit s\u00fcssen Dingen", "tokens": ["So", "mu\u00df", "Sor\u00b7bet", "mit", "s\u00fcs\u00b7sen", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Erquickung uns, den M\u00fcden, bringen.", "tokens": ["Er\u00b7quic\u00b7kung", "uns", ",", "den", "M\u00fc\u00b7den", ",", "brin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}