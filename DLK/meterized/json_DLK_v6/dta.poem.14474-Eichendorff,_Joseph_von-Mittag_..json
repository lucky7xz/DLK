{"dta.poem.14474": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Mittag .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie schwank' ich s\u00fcndlich,", "tokens": ["Wie", "schwank'", "ich", "s\u00fcnd\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "L\u00e4\u00dft Du von mir,", "tokens": ["L\u00e4\u00dft", "Du", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Un\u00fcberwindlich", "tokens": ["Un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich"], "token_info": ["word"], "pos": ["ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Bin ich mit Dir!", "tokens": ["Bin", "ich", "mit", "Dir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}