{"textgrid.poem.44781": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[f\u00fcr den Naturforscher Martius]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sch\u00f6n das All in Pflanze, Baum und Stein!", "tokens": ["Wie", "sch\u00f6n", "das", "All", "in", "Pflan\u00b7ze", ",", "Baum", "und", "Stein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPR", "NN", "$,", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Tierwelt nur, gesteh ich, macht mir Grauen.", "tokens": ["Die", "Tier\u00b7welt", "nur", ",", "ge\u00b7steh", "ich", ",", "macht", "mir", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch kr\u00f6net sie der Mensch mit seinem Sein und Schauen", "tokens": ["Doch", "kr\u00f6\u00b7net", "sie", "der", "Mensch", "mit", "sei\u00b7nem", "Sein", "und", "Schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und tr\u00e4gt die Liebe und das Recht hinein.", "tokens": ["Und", "tr\u00e4gt", "die", "Lie\u00b7be", "und", "das", "Recht", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}