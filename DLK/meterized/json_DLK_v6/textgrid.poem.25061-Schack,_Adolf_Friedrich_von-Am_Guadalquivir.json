{"textgrid.poem.25061": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Am Guadalquivir", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo bist du, Wunderbau der Omajaden,", "tokens": ["Wo", "bist", "du", ",", "Wun\u00b7der\u00b7bau", "der", "O\u00b7ma\u00b7ja\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Az-Zahra, zauberisch am Silberfaden", "tokens": ["Az\u00b7Zahra", ",", "zau\u00b7be\u00b7risch", "am", "Sil\u00b7ber\u00b7fa\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Des rauschenden Guadalquivir gedehnt?", "tokens": ["Des", "rau\u00b7schen\u00b7den", "Gua\u00b7dal\u00b7qui\u00b7vir", "ge\u00b7dehnt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Braut Abdurrahmans, in der Schattenk\u00fchle", "tokens": ["Braut", "Ab\u00b7dur\u00b7rah\u00b7mans", ",", "in", "der", "Schat\u00b7ten\u00b7k\u00fch\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des Mandelhaines auf die Rosenpf\u00fchle", "tokens": ["Des", "Man\u00b7del\u00b7hai\u00b7nes", "auf", "die", "Ro\u00b7sen\u00b7pf\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Uferh\u00fcgel hingelehnt?", "tokens": ["Der", "U\u00b7fer\u00b7h\u00fc\u00b7gel", "hin\u00b7ge\u00b7lehnt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo sind die Feste unter Myrtenlauben", "tokens": ["Wo", "sind", "die", "Fes\u00b7te", "un\u00b7ter", "Myr\u00b7ten\u00b7lau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bei Brunnenrieseln und Gegirr der Tauben,", "tokens": ["Bei", "Brun\u00b7nen\u00b7rie\u00b7seln", "und", "Ge\u00b7girr", "der", "Tau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bei Lampengl\u00fchn und buntem Wimpelflug,", "tokens": ["Bei", "Lam\u00b7pen\u00b7gl\u00fchn", "und", "bun\u00b7tem", "Wim\u00b7pel\u00b7flug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn auf dem Strom, in den krystallnen Tiefen", "tokens": ["Wenn", "auf", "dem", "Strom", ",", "in", "den", "krys\u00b7tall\u00b7nen", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Lorbeerschatten spaltend, den Kalifen", "tokens": ["Die", "Lor\u00b7beer\u00b7schat\u00b7ten", "spal\u00b7tend", ",", "den", "Ka\u00b7li\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die schimmernde Galeere trug?", "tokens": ["Die", "schim\u00b7mern\u00b7de", "Ga\u00b7lee\u00b7re", "trug", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Wo deine G\u00e4rten l\u00e4ngs des Uferrandes,", "tokens": ["Wo", "dei\u00b7ne", "G\u00e4r\u00b7ten", "l\u00e4ngs", "des", "Uf\u00b7er\u00b7ran\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In denen mit den Feen des Abendlandes", "tokens": ["In", "de\u00b7nen", "mit", "den", "Feen", "des", "A\u00b7bend\u00b7lan\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Arabiens Peri sich besprach,", "tokens": ["A\u00b7ra\u00b7biens", "Pe\u00b7ri", "sich", "be\u00b7sprach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PRF", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wenn auf den bl\u00fctenduftigen Terrassen", "tokens": ["Wenn", "auf", "den", "bl\u00fc\u00b7ten\u00b7duf\u00b7ti\u00b7gen", "Ter\u00b7ras\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Voll wei\u00dfer schimmernder Kiosks im blassen", "tokens": ["Voll", "wei\u00b7\u00dfer", "schim\u00b7mern\u00b7der", "Ki\u00b7osks", "im", "blas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "ADJA", "NN", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Lichtschein der Sternenhimmel lag?", "tokens": ["Licht\u00b7schein", "der", "Ster\u00b7nen\u00b7him\u00b7mel", "lag", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Und du, o Stadt der hochgew\u00f6lbten Dome,", "tokens": ["Und", "du", ",", "o", "Stadt", "der", "hoch\u00b7ge\u00b7w\u00f6lb\u00b7ten", "Do\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Milchstra\u00dfengleich mit deinem H\u00e4userstrome", "tokens": ["Milch\u00b7stra\u00b7\u00dfen\u00b7gleich", "mit", "dei\u00b7nem", "H\u00e4u\u00b7ser\u00b7stro\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf deinen Erdenhimmel hingestreckt,", "tokens": ["Auf", "dei\u00b7nen", "Er\u00b7den\u00b7him\u00b7mel", "hin\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Fanal der Gl\u00e4ubigen, des Wissens Leuchte,", "tokens": ["Fa\u00b7nal", "der", "Gl\u00e4u\u00b7bi\u00b7gen", ",", "des", "Wis\u00b7sens", "Leuch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Die hellen Strahls zuerst das Dunkel scheuchte,", "tokens": ["Die", "hel\u00b7len", "Strahls", "zu\u00b7erst", "das", "Dun\u00b7kel", "scheuch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das lang und tief die Welt bedeckt:", "tokens": ["Das", "lang", "und", "tief", "die", "Welt", "be\u00b7deckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "KON", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O Cordova! wo find' ich deine Dichter,", "tokens": ["O", "Cor\u00b7do\u00b7va", "!", "wo", "find'", "ich", "dei\u00b7ne", "Dich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo deine Sch\u00f6nen, gl\u00e4nzend wie die Lichter,", "tokens": ["Wo", "dei\u00b7ne", "Sch\u00f6\u00b7nen", ",", "gl\u00e4n\u00b7zend", "wie", "die", "Lich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die vom Serai der Nacht herniedersehn?", "tokens": ["Die", "vom", "Se\u00b7rai", "der", "Nacht", "her\u00b7nie\u00b7der\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wo sie, die mit dem Ruhm des Einig-Einen", "tokens": ["Wo", "sie", ",", "die", "mit", "dem", "Ruhm", "des", "Ei\u00b7nig\u00b7Ei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zum Himmel ragten aus den Cederhainen,", "tokens": ["Zum", "Him\u00b7mel", "rag\u00b7ten", "aus", "den", "Ce\u00b7der\u00b7hai\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Halbmondkuppeln der Moscheen?", "tokens": ["Die", "Halb\u00b7mond\u00b7kup\u00b7peln", "der", "Mo\u00b7scheen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Gest\u00fcrzt sind deine goldnen Minarete!", "tokens": ["Ge\u00b7st\u00fcrzt", "sind", "dei\u00b7ne", "gold\u00b7nen", "Mi\u00b7na\u00b7re\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Isan schweigt! Nie mehr, wenn die Drommete", "tokens": ["Der", "I\u00b7san", "schweigt", "!", "Nie", "mehr", ",", "wenn", "die", "Drom\u00b7me\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "$.", "ADV", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Gl\u00e4ubigen ermahnt zum heil'gen Kampf,", "tokens": ["Die", "Gl\u00e4u\u00b7bi\u00b7gen", "er\u00b7mahnt", "zum", "heil'\u00b7gen", "Kampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Entstr\u00f6mt das Heer der turbanbunten Mohren", "tokens": ["Ent\u00b7str\u00f6mt", "das", "Heer", "der", "tur\u00b7ban\u00b7bun\u00b7ten", "Moh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im eh'rnen Harnisch deinen hundert Thoren", "tokens": ["Im", "eh'r\u00b7nen", "Har\u00b7nisch", "dei\u00b7nen", "hun\u00b7dert", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bei Allahruf und Ro\u00dfgestampf.", "tokens": ["Bei", "Al\u00b7la\u00b7hruf", "und", "Ro\u00df\u00b7ge\u00b7stampf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Einsam inmitten deiner Tr\u00fcmmer ragen", "tokens": ["Ein\u00b7sam", "in\u00b7mit\u00b7ten", "dei\u00b7ner", "Tr\u00fcm\u00b7mer", "ra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die Pfeiler, die das hehre Dach getragen,", "tokens": ["Die", "Pfei\u00b7ler", ",", "die", "das", "heh\u00b7re", "Dach", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein wipfelreicher Marmorwald;", "tokens": ["Ein", "wip\u00b7fel\u00b7rei\u00b7cher", "Mar\u00b7mor\u00b7wald", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erloschen aber ist der Lampen Menge;", "tokens": ["Er\u00b7lo\u00b7schen", "a\u00b7ber", "ist", "der", "Lam\u00b7pen", "Men\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nie mehr wallt Allah durch die S\u00e4uleng\u00e4nge,", "tokens": ["Nie", "mehr", "wallt", "Al\u00b7lah", "durch", "die", "S\u00e4u\u00b7len\u00b7g\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.6": {"text": "Draus kein Gebet zu ihm mehr schallt;", "tokens": ["Draus", "kein", "Ge\u00b7bet", "zu", "ihm", "mehr", "schallt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ein neuer Glaube f\u00fcllt die Tempelhallen", "tokens": ["Ein", "neu\u00b7er", "Glau\u00b7be", "f\u00fcllt", "die", "Tem\u00b7pel\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Islam nun, die Stein auf Stein zerfallen,", "tokens": ["Des", "Is\u00b7lam", "nun", ",", "die", "Stein", "auf", "Stein", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "$,", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Orgelklang und Weihrauchqualm;", "tokens": ["Mit", "Or\u00b7gel\u00b7klang", "und", "Weih\u00b7rauch\u00b7qualm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald stirbt auch er; des Hochaltars Gepr\u00e4nge", "tokens": ["Bald", "stirbt", "auch", "er", ";", "des", "Hoc\u00b7hal\u00b7tars", "Ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Deckt m\u00e4hlich Staub, und matt wie Grabges\u00e4nge", "tokens": ["Deckt", "m\u00e4h\u00b7lich", "Staub", ",", "und", "matt", "wie", "Grab\u00b7ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "NN", "$,", "KON", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verklingt der letzte Christenpsalm.", "tokens": ["Ver\u00b7klingt", "der", "letz\u00b7te", "Chris\u00b7ten\u00b7psalm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}