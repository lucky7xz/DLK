{"textgrid.poem.67358": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Jupiter und Pluto", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast Du die bl\u00fchenden Horen,", "tokens": ["Hast", "Du", "die", "bl\u00fc\u00b7hen\u00b7den", "Ho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Siegsg\u00f6ttinnen gesehn?", "tokens": ["Die", "Siegs\u00b7g\u00f6t\u00b7tin\u00b7nen", "ge\u00b7sehn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie schweben im Tanz und tragen", "tokens": ["Sie", "schwe\u00b7ben", "im", "Tanz", "und", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Des ewigen Vaters goldenen Thron.", "tokens": ["Des", "e\u00b7wi\u00b7gen", "Va\u00b7ters", "gol\u00b7de\u00b7nen", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Aber er thronet milde;", "tokens": ["A\u00b7ber", "er", "thro\u00b7net", "mil\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Sein Blitz und der Adler schl\u00e4ft;", "tokens": ["Sein", "Blitz", "und", "der", "Ad\u00b7ler", "schl\u00e4ft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn nicht mit blutigem Lorbeer,", "tokens": ["Denn", "nicht", "mit", "blu\u00b7ti\u00b7gem", "Lor\u00b7beer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit dem Oelzweig kr\u00e4nzet die Weisheit ihn.", "tokens": ["Mit", "dem", "O\u00b7el\u00b7zweig", "kr\u00e4n\u00b7zet", "die", "Weis\u00b7heit", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Der Scepter in seinen H\u00e4nden", "tokens": ["Der", "Scep\u00b7ter", "in", "sei\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "War einst ein ruhiger Hirtenstab,", "tokens": ["War", "einst", "ein", "ru\u00b7hi\u00b7ger", "Hir\u00b7ten\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit dem er die V\u00f6lker besuchte,", "tokens": ["Mit", "dem", "er", "die", "V\u00f6l\u00b7ker", "be\u00b7such\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mit dem er noch jetzt die V\u00f6lker begl\u00fcckt.", "tokens": ["Mit", "dem", "er", "noch", "jetzt", "die", "V\u00f6l\u00b7ker", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Heil mir! o Vater der G\u00f6tter,", "tokens": ["Heil", "mir", "!", "o", "Va\u00b7ter", "der", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "FM", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Ich habe Dein Antlitz gl\u00e4nzen gesehn;", "tokens": ["Ich", "ha\u00b7be", "Dein", "Ant\u00b7litz", "gl\u00e4n\u00b7zen", "ge\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVINF", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es blickte zu den Aethiopen,", "tokens": ["Es", "blick\u00b7te", "zu", "den", "A\u00b7e\u00b7thio\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem friedlichen, dem unschuldigen Volk,", "tokens": ["Dem", "fried\u00b7li\u00b7chen", ",", "dem", "un\u00b7schul\u00b7di\u00b7gen", "Volk", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das Du noch gern besuchest,", "tokens": ["Das", "Du", "noch", "gern", "be\u00b7su\u00b7chest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und h\u00e4ltst mit ihnen ein fr\u00f6hliches Mahl,", "tokens": ["Und", "h\u00e4ltst", "mit", "ih\u00b7nen", "ein", "fr\u00f6h\u00b7li\u00b7ches", "Mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und zu Dir str\u00f6men die V\u00f6lker;", "tokens": ["Und", "zu", "Dir", "str\u00f6\u00b7men", "die", "V\u00f6l\u00b7ker", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Bittende kniet erh\u00f6rt vor Dir.", "tokens": ["Der", "Bit\u00b7ten\u00b7de", "kniet", "er\u00b7h\u00f6rt", "vor", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Nicht so Dein dunkeler Bruder;", "tokens": ["Nicht", "so", "Dein", "dun\u00b7ke\u00b7ler", "Bru\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Des Rechtes Zweizack ist in seiner K\u00f6nigshand,", "tokens": ["Des", "Rech\u00b7tes", "Zwei\u00b7zack", "ist", "in", "sei\u00b7ner", "K\u00f6\u00b7nigs\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und \u00e4chzende Danaiden", "tokens": ["Und", "\u00e4ch\u00b7zen\u00b7de", "Da\u00b7na\u00b7i\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit leeren Kr\u00fcgen sein Clientenhof.", "tokens": ["Mit", "lee\u00b7ren", "Kr\u00fc\u00b7gen", "sein", "Clien\u00b7ten\u00b7hof", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Die Furien seine Gesinde,", "tokens": ["Die", "Fu\u00b7ri\u00b7en", "sei\u00b7ne", "Ge\u00b7sin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mit bunten Schlangen sch\u00f6n geschm\u00fcckt,", "tokens": ["Mit", "bun\u00b7ten", "Schlan\u00b7gen", "sch\u00f6n", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tantalus, Ixion und Prometheus seine Diener,", "tokens": ["Tan\u00b7ta\u00b7lus", ",", "I\u00b7xion", "und", "Pro\u00b7me\u00b7theus", "sei\u00b7ne", "Die\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "+--++--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und Sisyphus sein Cancellar.", "tokens": ["Und", "Si\u00b7syp\u00b7hus", "sein", "Can\u00b7cel\u00b7lar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Mit weggewandtem Blicke", "tokens": ["Mit", "weg\u00b7ge\u00b7wand\u00b7tem", "Bli\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Thront neben ihm die gelbe Persephone.", "tokens": ["Thront", "ne\u00b7ben", "ihm", "die", "gel\u00b7be", "Per\u00b7se\u00b7pho\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "O Jupiter, gieb uns F\u00fcrsten,", "tokens": ["O", "Ju\u00b7pi\u00b7ter", ",", "gieb", "uns", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVIMP", "PPER", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dir \u00e4hnlich, Deinem Bruder nie!", "tokens": ["Dir", "\u00e4hn\u00b7lich", ",", "Dei\u00b7nem", "Bru\u00b7der", "nie", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}