{"textgrid.poem.59911": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie glauben mit frommem Hadern", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie glauben mit frommem Hadern", "tokens": ["Sie", "glau\u00b7ben", "mit", "from\u00b7mem", "Ha\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den Himmel zu verdienen;", "tokens": ["Den", "Him\u00b7mel", "zu", "ver\u00b7die\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Zorn schwillt ihre Adern,", "tokens": ["Der", "Zorn", "schwillt", "ih\u00b7re", "A\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Ha\u00df f\u00e4rbt ihre Mienen.", "tokens": ["Der", "Ha\u00df", "f\u00e4rbt", "ih\u00b7re", "Mie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das Mordschwert in den H\u00e4nden,", "tokens": ["Das", "Mord\u00b7schwert", "in", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verlangen sie Glauben und Bu\u00dfe", "tokens": ["Ver\u00b7lan\u00b7gen", "sie", "Glau\u00b7ben", "und", "Bu\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und glauben, sie selber st\u00e4nden", "tokens": ["Und", "glau\u00b7ben", ",", "sie", "sel\u00b7ber", "st\u00e4n\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPER", "ADV", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mir Gott auf dem besten Fu\u00dfe.", "tokens": ["Mir", "Gott", "auf", "dem", "bes\u00b7ten", "Fu\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich aber sage euch, da\u00df", "tokens": ["Ich", "a\u00b7ber", "sa\u00b7ge", "euch", ",", "da\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "KOUS"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Gott ferne solchem Getriebe!", "tokens": ["Gott", "fer\u00b7ne", "sol\u00b7chem", "Ge\u00b7trie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ung\u00f6ttlich ist der Ha\u00df", "tokens": ["Un\u00b7g\u00f6tt\u00b7lich", "ist", "der", "Ha\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und g\u00f6ttlich nur die Liebe!", "tokens": ["Und", "g\u00f6tt\u00b7lich", "nur", "die", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}