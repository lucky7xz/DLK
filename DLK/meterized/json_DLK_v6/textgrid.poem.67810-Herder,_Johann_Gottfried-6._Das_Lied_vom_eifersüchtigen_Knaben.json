{"textgrid.poem.67810": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "6. Das Lied vom eifers\u00fcchtigen Knaben", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es stehen drey Stern' am Himmel,", "tokens": ["Es", "ste\u00b7hen", "drey", "Stern'", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die geben der Lieb' ihren Schein.", "tokens": ["Die", "ge\u00b7ben", "der", "Lieb'", "ih\u00b7ren", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Gott gr\u00fc\u00df euch, sch\u00f6nes Jungfr\u00e4ulein,", "tokens": ["Gott", "gr\u00fc\u00df", "euch", ",", "sch\u00f6\u00b7nes", "Jung\u00b7fr\u00e4u\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo bind' ich mein R\u00f6sselein hin.", "tokens": ["Wo", "bind'", "ich", "mein", "R\u00f6s\u00b7sel\u00b7ein", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "\u00bbnimm du es, dein R\u00f6\u00dflein, beim Z\u00fcgel, beim Zaum,", "tokens": ["\u00bb", "nimm", "du", "es", ",", "dein", "R\u00f6\u00df\u00b7lein", ",", "beim", "Z\u00fc\u00b7gel", ",", "beim", "Zaum", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "PPER", "$,", "PPOSAT", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bind's an den Feigenbaum.", "tokens": ["Bin\u00b7d's", "an", "den", "Fei\u00b7gen\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sez dich ein' kleine Weil nieder,", "tokens": ["Sez", "dich", "ein'", "klei\u00b7ne", "Weil", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und mach mir ein kleine Kurzweil.\u00ab", "tokens": ["Und", "mach", "mir", "ein", "klei\u00b7ne", "Kurz\u00b7weil", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich kann und mag nicht sizen,", "tokens": ["Ich", "kann", "und", "mag", "nicht", "si\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mag auch nicht lustig seyn,", "tokens": ["Mag", "auch", "nicht", "lus\u00b7tig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Mein Herz ist mir betr\u00fcbet,", "tokens": ["Mein", "Herz", "ist", "mir", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Feinslieb von wegen dein.", "tokens": ["Feins\u00b7lieb", "von", "we\u00b7gen", "dein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was zog er aus der Taschen?", "tokens": ["Was", "zog", "er", "aus", "der", "Ta\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Messer, war scharf und spiz;", "tokens": ["Ein", "Mes\u00b7ser", ",", "war", "scharf", "und", "spiz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er stachs seiner Lieben durchs Herze;", "tokens": ["Er", "stachs", "sei\u00b7ner", "Lie\u00b7ben", "durchs", "Her\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "APPRART", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das rothe Blut gegen ihn sprizt.", "tokens": ["Das", "ro\u00b7the", "Blut", "ge\u00b7gen", "ihn", "sprizt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und da er's wieder herausser zog,", "tokens": ["Und", "da", "er's", "wie\u00b7der", "her\u00b7aus\u00b7ser", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von Blut war es so roth.", "tokens": ["Von", "Blut", "war", "es", "so", "roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbach reicher Gott vom Himmel,", "tokens": ["\u00bb", "ach", "rei\u00b7cher", "Gott", "vom", "Him\u00b7mel", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie bitter wird mir der Tod!\u00ab", "tokens": ["Wie", "bit\u00b7ter", "wird", "mir", "der", "Tod", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Was zog er ihr abe vom Finger?", "tokens": ["Was", "zog", "er", "ihr", "a\u00b7be", "vom", "Fin\u00b7ger", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ein rothes Goldringelein.", "tokens": ["Ein", "ro\u00b7thes", "Gold\u00b7rin\u00b7ge\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er warfs in fl\u00fcssig Wasser;", "tokens": ["Er", "warfs", "in", "fl\u00fcs\u00b7sig", "Was\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es gab seinen klaren Schein.", "tokens": ["Es", "gab", "sei\u00b7nen", "kla\u00b7ren", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Schwimm hin, schwimm her, Goldringelein!", "tokens": ["Schwimm", "hin", ",", "schwimm", "her", ",", "Gold\u00b7rin\u00b7ge\u00b7lein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ADJD", "PTKVZ", "$,", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Bis an den tiefen See!", "tokens": ["Bis", "an", "den", "tie\u00b7fen", "See", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Feinslieb ist mir gestorben;", "tokens": ["Mein", "Feins\u00b7lieb", "ist", "mir", "ge\u00b7stor\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jezt hab ich kein Feinslieb mehr.", "tokens": ["Jezt", "hab", "ich", "kein", "Feins\u00b7lieb", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "So gehts, wenn ein Maidel zwei Knaben lieb hat,", "tokens": ["So", "gehts", ",", "wenn", "ein", "Mai\u00b7del", "zwei", "Kna\u00b7ben", "lieb", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ART", "NN", "CARD", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Thut wunderselten gut;", "tokens": ["Thut", "wun\u00b7der\u00b7sel\u00b7ten", "gut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das haben wir Beid' erfahren,", "tokens": ["Das", "ha\u00b7ben", "wir", "Beid'", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was falsche Liebe thut.", "tokens": ["Was", "fal\u00b7sche", "Lie\u00b7be", "thut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}