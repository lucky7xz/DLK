{"textgrid.poem.40008": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es giengen j\u00fcngst Ergast und Belisander,", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es giengen j\u00fcngst Ergast und Belisander,", "tokens": ["Es", "gien\u00b7gen", "j\u00fcngst", "Er\u00b7gast", "und", "Be\u00b7li\u00b7san\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In einen dicht-verwachs'nen Wald,", "tokens": ["In", "ei\u00b7nen", "dicht\u00b7ver\u00b7wachs'\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur schw\u00fchlen Sommers-Zeit, vertraulich mit einander,", "tokens": ["Zur", "schw\u00fch\u00b7len", "Som\u00b7mer\u00b7sZeit", ",", "ver\u00b7trau\u00b7lich", "mit", "ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADJD", "APPR", "PRF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um, unter Eichen, B\u00fcchen, Linden,", "tokens": ["Um", ",", "un\u00b7ter", "Ei\u00b7chen", ",", "B\u00fc\u00b7chen", ",", "Lin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUI", "$,", "APPR", "NN", "$,", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der holden K\u00fchlung Aufenthalt,", "tokens": ["Der", "hol\u00b7den", "K\u00fch\u00b7lung", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der frischen Schatten Kind, zu finden.", "tokens": ["Der", "fri\u00b7schen", "Schat\u00b7ten", "Kind", ",", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie setzten sich sofort", "tokens": ["Sie", "setz\u00b7ten", "sich", "so\u00b7fort"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "An einen angenehm-erhab'nen Ort,", "tokens": ["An", "ei\u00b7nen", "an\u00b7ge\u00b7nehm\u00b7er\u00b7hab'\u00b7nen", "Ort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der sein beschattet Haupt mit krausen B\u00fcschen kr\u00e4ntzte,", "tokens": ["Der", "sein", "be\u00b7schat\u00b7tet", "Haupt", "mit", "krau\u00b7sen", "B\u00fc\u00b7schen", "kr\u00e4ntz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAINF", "VVFIN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und doppelt sch\u00f6n, im nahen Wasser, gl\u00e4ntzte,", "tokens": ["Und", "dop\u00b7pelt", "sch\u00f6n", ",", "im", "na\u00b7hen", "Was\u00b7ser", ",", "gl\u00e4ntz\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "$,", "APPRART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Das an der gr\u00fcnen H\u00f6he Fu\u00df,", "tokens": ["Das", "an", "der", "gr\u00fc\u00b7nen", "H\u00f6\u00b7he", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mit sanftem Flu\u00df,", "tokens": ["Mit", "sanf\u00b7tem", "Flu\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Wie ein lebendig Silber, rollte,", "tokens": ["Wie", "ein", "le\u00b7ben\u00b7dig", "Sil\u00b7ber", ",", "roll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und manchen feuchten Ku\u00df,", "tokens": ["Und", "man\u00b7chen", "feuch\u00b7ten", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Durch sein erquickend Na\u00df, dem fetten Ufer zollte.", "tokens": ["Durch", "sein", "er\u00b7qui\u00b7ckend", "Na\u00df", ",", "dem", "fet\u00b7ten", "U\u00b7fer", "zoll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVPP", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie sah'n die Bluhmen, auf den H\u00fcgeln,", "tokens": ["Sie", "sah'n", "die", "Bluh\u00b7men", ",", "auf", "den", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich theils in eig'nem Schmuck verliebt bespiegeln;", "tokens": ["Sich", "theils", "in", "eig'\u00b7nem", "Schmuck", "ver\u00b7liebt", "be\u00b7spie\u00b7geln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ADJA", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Theils sahen sie, um sich zu tr\u00e4ncken,", "tokens": ["Theils", "sa\u00b7hen", "sie", ",", "um", "sich", "zu", "tr\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "KOUI", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bluhmen in die Fluth die bunten H\u00e4upter sencken.", "tokens": ["Die", "Bluh\u00b7men", "in", "die", "Fluth", "die", "bun\u00b7ten", "H\u00e4up\u00b7ter", "sen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie h\u00f6r'ten, in den nahen B\u00fcschen,", "tokens": ["Sie", "h\u00f6r'\u00b7ten", ",", "in", "den", "na\u00b7hen", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gelinde Winde s\u00e4uselnd zischen.", "tokens": ["Ge\u00b7lin\u00b7de", "Win\u00b7de", "s\u00e4u\u00b7selnd", "zi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es waren hier bebl\u00fchmte H\u00f6hen,", "tokens": ["Es", "wa\u00b7ren", "hier", "be\u00b7bl\u00fchm\u00b7te", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beb\u00fcschte Th\u00e4ler dort, zu sehen.", "tokens": ["Be\u00b7b\u00fcschte", "Th\u00e4\u00b7ler", "dort", ",", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie sah'n der V\u00f6gel Schaar von Zweig auf Zweige springen.", "tokens": ["Sie", "sah'n", "der", "V\u00f6\u00b7gel", "Schaar", "von", "Zweig", "auf", "Zwei\u00b7ge", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie h\u00f6rten auf einmahl,", "tokens": ["Sie", "h\u00f6r\u00b7ten", "auf", "ein\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Von kleinen Schn\u00e4beln ohne Zahl,", "tokens": ["Von", "klei\u00b7nen", "Schn\u00e4\u00b7beln", "oh\u00b7ne", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein zwitscherndes Ger\u00e4usch, ein s\u00fc\u00df-verwirrtes Singen.", "tokens": ["Ein", "zwit\u00b7schern\u00b7des", "Ge\u00b7r\u00e4usch", ",", "ein", "s\u00fc\u00df\u00b7ver\u00b7wirr\u00b7tes", "Sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das Gurgeln der verliebten Nachtigall,", "tokens": ["Das", "Gur\u00b7geln", "der", "ver\u00b7lieb\u00b7ten", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das Murmeln von den hellen B\u00e4chen,", "tokens": ["Das", "Mur\u00b7meln", "von", "den", "hel\u00b7len", "B\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die auf dem Kiesel-Grund die Fluth gem\u00e4hlich brechen,", "tokens": ["Die", "auf", "dem", "Kie\u00b7sel\u00b7Grund", "die", "Fluth", "ge\u00b7m\u00e4h\u00b7lich", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sucht der geschwinde Wiederhall,", "tokens": ["Sucht", "der", "ge\u00b7schwin\u00b7de", "Wie\u00b7der\u00b7hall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Den Schall verdoppelnd, nachzusprechen.", "tokens": ["Den", "Schall", "ver\u00b7dop\u00b7pelnd", ",", "nach\u00b7zu\u00b7spre\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Hiedurch fast halb entz\u00fcckt, hub Belisander an,", "tokens": ["Hie\u00b7durch", "fast", "halb", "ent\u00b7z\u00fcckt", ",", "hub", "Be\u00b7li\u00b7san\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADJD", "VVPP", "$,", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dem ", "tokens": ["Dem"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Der W\u00e4lder Schmuck, wie folget, zu besingen:", "tokens": ["Der", "W\u00e4l\u00b7der", "Schmuck", ",", "wie", "fol\u00b7get", ",", "zu", "be\u00b7sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PWAV", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ach, wie viel sch\u00f6ne Farben fallen,", "tokens": ["Ach", ",", "wie", "viel", "sch\u00f6\u00b7ne", "Far\u00b7ben", "fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bey diesen fliessenden Krystallen,", "tokens": ["Bey", "die\u00b7sen", "flies\u00b7sen\u00b7den", "Krys\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die diesen dichten Wald durchwallen,", "tokens": ["Die", "die\u00b7sen", "dich\u00b7ten", "Wald", "durch\u00b7wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir nicht auf einmahl ins Gesicht!", "tokens": ["Mir", "nicht", "auf", "ein\u00b7mahl", "ins", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Ein tausendfach gef\u00e4rbtes Licht,", "tokens": ["Ein", "tau\u00b7send\u00b7fach", "ge\u00b7f\u00e4rb\u00b7tes", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein tausendfach geformter Schatten,", "tokens": ["Ein", "tau\u00b7send\u00b7fach", "ge\u00b7form\u00b7ter", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die sich bald trennen und bald gatten,", "tokens": ["Die", "sich", "bald", "tren\u00b7nen", "und", "bald", "gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "VVINF", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und tausendfach vermischet seyn;", "tokens": ["Und", "tau\u00b7send\u00b7fach", "ver\u00b7mi\u00b7schet", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Verdoppeln sich im Wieder-Schein.", "tokens": ["Ver\u00b7dop\u00b7peln", "sich", "im", "Wie\u00b7der\u00b7Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es kann mein Auge sich nicht satt,", "tokens": ["Es", "kann", "mein", "Au\u00b7ge", "sich", "nicht", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PRF", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An aller Vorw\u00fcrf' Anmuth sehn.", "tokens": ["An", "al\u00b7ler", "Vor\u00b7w\u00fcr\u00b7f'", "An\u00b7muth", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein jeder Strauch, ein jedes Blatt", "tokens": ["Ein", "je\u00b7der", "Strauch", ",", "ein", "je\u00b7des", "Blatt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "$,", "ART", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist sch\u00f6n, ist Wunder-w\u00fcrdig sch\u00f6n.", "tokens": ["Ist", "sch\u00f6n", ",", "ist", "Wun\u00b7der\u00b7w\u00fcr\u00b7dig", "sch\u00f6n", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Hier bricht ein lichtes Gr\u00fcn, in gleich-gezog'ner L\u00e4nge,", "tokens": ["Hier", "bricht", "ein", "lich\u00b7tes", "Gr\u00fcn", ",", "in", "gleich\u00b7ge\u00b7zo\u00b7g'\u00b7ner", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Durch starck beschatteter bemoo\u00dfter St\u00e4mme Menge.", "tokens": ["Durch", "starck", "be\u00b7schat\u00b7te\u00b7ter", "be\u00b7moo\u00df\u00b7ter", "St\u00e4m\u00b7me", "Men\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dort sencket sich ein dicht bepflantzter H\u00fcgel,", "tokens": ["Dort", "sen\u00b7cket", "sich", "ein", "dicht", "be\u00b7pflantz\u00b7ter", "H\u00fc\u00b7gel", ","], "token_info": ["