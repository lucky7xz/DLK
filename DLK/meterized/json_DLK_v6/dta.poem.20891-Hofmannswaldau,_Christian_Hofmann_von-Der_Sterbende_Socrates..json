{"dta.poem.20891": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Auf das wenn wir zum Tode gehn/", "tokens": ["Auf", "das", "wenn", "wir", "zum", "To\u00b7de", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "KOUS", "PPER", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und diesen Kercker lassen stehn/", "tokens": ["Und", "die\u00b7sen", "Ker\u00b7cker", "las\u00b7sen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da unser Seele muste kleben/", "tokens": ["Da", "un\u00b7ser", "See\u00b7le", "mus\u00b7te", "kle\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Weise lebe Zweiffels frey/", "tokens": ["Der", "Wei\u00b7se", "le\u00b7be", "Zweif\u00b7fels", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df gar gewi\u00df nach diesem Leben", "tokens": ["Da\u00df", "gar", "ge\u00b7wi\u00df", "nach", "die\u00b7sem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Leben noch zu hoffen sey.", "tokens": ["Ein", "Le\u00b7ben", "noch", "zu", "hof\u00b7fen", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was w\u00e4re sonst die Wollust hassen/", "tokens": ["Was", "w\u00e4\u00b7re", "sonst", "die", "Wol\u00b7lust", "has\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schlaff/ Wein und Buhlerey zu lassen/", "tokens": ["Schlaff", "/", "Wein", "und", "Buh\u00b7le\u00b7rey", "zu", "las\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "KON", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und allem diesem Feind zu seyn.", "tokens": ["Und", "al\u00b7lem", "die\u00b7sem", "Feind", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PDAT", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und durch was mehr die reinen Sinnen/", "tokens": ["Und", "durch", "was", "mehr", "die", "rei\u00b7nen", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sich nahen zu der Tugend Schein/", "tokens": ["Sich", "na\u00b7hen", "zu", "der", "Tu\u00b7gend", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als nur ein t\u00f6richtes Beginnen?", "tokens": ["Als", "nur", "ein", "t\u00f6\u00b7rich\u00b7tes", "Be\u00b7gin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Lust so ewig uns erfreut/", "tokens": ["Die", "Lust", "so", "e\u00b7wig", "uns", "er\u00b7freut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach vielerley Verdrie\u00dflichkeit/", "tokens": ["Nach", "vie\u00b7ler\u00b7ley", "Ver\u00b7drie\u00df\u00b7lich\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die nur auf dieser Erde bleibet/", "tokens": ["Die", "nur", "auf", "die\u00b7ser", "Er\u00b7de", "blei\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00e4r\u2019 eine Hoffnung die verschwind/", "tokens": ["W\u00e4r'", "ei\u00b7ne", "Hoff\u00b7nung", "die", "ver\u00b7schwind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und was man von dem Himmel schreibet/", "tokens": ["Und", "was", "man", "von", "dem", "Him\u00b7mel", "schrei\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verbliebe falscher Dampff und Wind.", "tokens": ["Ver\u00b7blie\u00b7be", "fal\u00b7scher", "Dampff", "und", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}