{"dta.poem.4102": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Des Baches Wiegenlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Thu die Augen zu!", "tokens": ["Thu", "die", "Au\u00b7gen", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wandrer, du m\u00fcder, du bist zu Haus.", "tokens": ["Wand\u00b7rer", ",", "du", "m\u00fc\u00b7der", ",", "du", "bist", "zu", "Haus", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJD", "$,", "PPER", "VAFIN", "APPR", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Die Treu' ist hier,", "tokens": ["Die", "Treu'", "ist", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Sollst liegen bei mir,", "tokens": ["Sollst", "lie\u00b7gen", "bei", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Bis das Meer will trinken die B\u00e4chlein aus.", "tokens": ["Bis", "das", "Meer", "will", "trin\u00b7ken", "die", "B\u00e4ch\u00b7lein", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "VVINF", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Will betten dich k\u00fchl,", "tokens": ["Will", "bet\u00b7ten", "dich", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Auf weichem Pf\u00fchl,", "tokens": ["Auf", "wei\u00b7chem", "Pf\u00fchl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "In dem blauen krystallenen K\u00e4mmerlein.", "tokens": ["In", "dem", "blau\u00b7en", "krys\u00b7tal\u00b7le\u00b7nen", "K\u00e4m\u00b7mer\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Heran, heran,", "tokens": ["He\u00b7ran", ",", "he\u00b7ran", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["PTKVZ", "$,", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Was wiegen kann,", "tokens": ["Was", "wie\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "VMFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Woget und wieget den Knaben mir ein!", "tokens": ["Wo\u00b7get", "und", "wie\u00b7get", "den", "Kna\u00b7ben", "mir", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn ein Jagdhorn schallt", "tokens": ["Wenn", "ein", "Jagd\u00b7horn", "schallt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Aus dem gr\u00fcnen Wald,", "tokens": ["Aus", "dem", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Will ich sausen und brausen wohl um dich her.", "tokens": ["Will", "ich", "sau\u00b7sen", "und", "brau\u00b7sen", "wohl", "um", "dich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "KON", "VVFIN", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Blickt nicht herein,", "tokens": ["Blickt", "nicht", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Blaue Bl\u00fcmelein!", "tokens": ["Blau\u00b7e", "Bl\u00fc\u00b7me\u00b7lein", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ihr macht meinem Schl\u00e4fer die Tr\u00e4ume so schwer.", "tokens": ["Ihr", "macht", "mei\u00b7nem", "Schl\u00e4\u00b7fer", "die", "Tr\u00e4u\u00b7me", "so", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Hinweg, hinweg,", "tokens": ["Hin\u00b7weg", ",", "hin\u00b7weg", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Von dem M\u00fchlensteg,", "tokens": ["Von", "dem", "M\u00fch\u00b7lens\u00b7teg", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "B\u00f6ses M\u00e4gdlein, da\u00df ihn dein Schatten nicht weckt!", "tokens": ["B\u00f6\u00b7ses", "M\u00e4gd\u00b7lein", ",", "da\u00df", "ihn", "dein", "Schat\u00b7ten", "nicht", "weckt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wirf mir herein", "tokens": ["Wirf", "mir", "her\u00b7ein"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKVZ"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Dein T\u00fcchlein fein,", "tokens": ["Dein", "T\u00fcch\u00b7lein", "fein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Da\u00df ich die Augen ihm halte bedeckt!", "tokens": ["Da\u00df", "ich", "die", "Au\u00b7gen", "ihm", "hal\u00b7te", "be\u00b7deckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Gute Nacht, gute Nacht!", "tokens": ["Gu\u00b7te", "Nacht", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Bis Alles wacht,", "tokens": ["Bis", "Al\u00b7les", "wacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Schlaf' aus deine Freude, schlaf' aus dein Leid!", "tokens": ["Schlaf'", "aus", "dei\u00b7ne", "Freu\u00b7de", ",", "schlaf'", "aus", "dein", "Leid", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+----+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Der Vollmond steigt,", "tokens": ["Der", "Voll\u00b7mond", "steigt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der Nebel weicht,", "tokens": ["Der", "Ne\u00b7bel", "weicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und der Himmel da oben, wie ist er so weit!", "tokens": ["Und", "der", "Him\u00b7mel", "da", "o\u00b7ben", ",", "wie", "ist", "er", "so", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADV", "$,", "PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}}}}