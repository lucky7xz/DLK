{"textgrid.poem.63042": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Zecher war bereit zu scheiden;", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Zecher war bereit zu scheiden;", "tokens": ["Ein", "Ze\u00b7cher", "war", "be\u00b7reit", "zu", "schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Weib bethr\u00e4nte sein Gesicht.", "tokens": ["Sein", "Weib", "be\u00b7thr\u00e4n\u00b7te", "sein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, rief er, Liebe, weine nicht;", "tokens": ["Ach", ",", "rief", "er", ",", "Lie\u00b7be", ",", "wei\u00b7ne", "nicht", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "$,", "NN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich konnte nie das Wasser leiden!", "tokens": ["Ich", "konn\u00b7te", "nie", "das", "Was\u00b7ser", "lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}