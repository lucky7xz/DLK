{"textgrid.poem.65429": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "2. Most und Wein", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du dich der Lieb' entschlagen, weil ein s\u00fc\u00dfer Rausch sie ist?", "tokens": ["Willst", "du", "dich", "der", "Lieb'", "ent\u00b7schla\u00b7gen", ",", "weil", "ein", "s\u00fc\u00b7\u00dfer", "Rausch", "sie", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ART", "NN", "VVPP", "$,", "KOUS", "ART", "ADJA", "NN", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "O du weiser Mann, bedenke, was du ohne Liebe bist!", "tokens": ["O", "du", "wei\u00b7ser", "Mann", ",", "be\u00b7den\u00b7ke", ",", "was", "du", "oh\u00b7ne", "Lie\u00b7be", "bist", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,", "VVFIN", "$,", "PWS", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wenn die Hefe liegt im Grunde, wirst du schaaler Most nur sein:", "tokens": ["Wenn", "die", "He\u00b7fe", "liegt", "im", "Grun\u00b7de", ",", "wirst", "du", "schaa\u00b7ler", "Most", "nur", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPRART", "NN", "$,", "VAFIN", "PPER", "ADJA", "NN", "ADV", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Liebe treibt sie in die H\u00f6he, und im Rausche wirst du Wein.", "tokens": ["Lie\u00b7be", "treibt", "sie", "in", "die", "H\u00f6\u00b7he", ",", "und", "im", "Rau\u00b7sche", "wirst", "du", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "APPRART", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}