{"textgrid.poem.39676": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "Ariadne auf Naxos", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Naxos Felsen weint verlassen Minos Tochter.", "tokens": ["Auf", "Na\u00b7xos", "Fel\u00b7sen", "weint", "ver\u00b7las\u00b7sen", "Mi\u00b7nos", "Toch\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sch\u00f6nheit heisses Flehn erreicht der G\u00f6tter Ohr.", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "heis\u00b7ses", "Flehn", "er\u00b7reicht", "der", "G\u00f6t\u00b7ter", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von seinem Thron herab senkt, Kronos Sohn, die Blitze,", "tokens": ["Von", "sei\u00b7nem", "Thron", "her\u00b7ab", "senkt", ",", "Kro\u00b7nos", "Sohn", ",", "die", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$,", "NE", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie zur Unsterblichkeit in Wettern aufzuziehn.", "tokens": ["Sie", "zur", "U\u00b7nsterb\u00b7lich\u00b7keit", "in", "Wet\u00b7tern", "auf\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Poseidon, Lieb entbrannt, er\u00f6ffnet schon die Arme,", "tokens": ["Po\u00b7sei\u00b7don", ",", "Lieb", "ent\u00b7brannt", ",", "er\u00b7\u00f6ff\u00b7net", "schon", "die", "Ar\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "VVFIN", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Umschlingen will er sie, mit seiner Fluthen Nacht.", "tokens": ["Um\u00b7schlin\u00b7gen", "will", "er", "sie", ",", "mit", "sei\u00b7ner", "Flut\u00b7hen", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "$,", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Soll zur Unsterblichkeit nun Minos Tochter steigen?", "tokens": ["Soll", "zur", "U\u00b7nsterb\u00b7lich\u00b7keit", "nun", "Mi\u00b7nos", "Toch\u00b7ter", "stei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "ADV", "NE", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Soll sie, den Schatten gleich, zum dunklen Orkus gehn?", "tokens": ["Soll", "sie", ",", "den", "Schat\u00b7ten", "gleich", ",", "zum", "dunk\u00b7len", "Or\u00b7kus", "gehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ART", "NN", "ADV", "$,", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ariadne z\u00f6gert nicht, sie st\u00fcrzt sich in die Fluthen:", "tokens": ["A\u00b7riad\u00b7ne", "z\u00f6\u00b7gert", "nicht", ",", "sie", "st\u00fcrzt", "sich", "in", "die", "Flut\u00b7hen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Betrogner Liebe Schmerz soll nicht unsterblich seyn!", "tokens": ["Be\u00b7trog\u00b7ner", "Lie\u00b7be", "Schmerz", "soll", "nicht", "uns\u00b7terb\u00b7lich", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zum G\u00f6tterloos hinauf mag sich der Gram nicht dr\u00e4ngen,", "tokens": ["Zum", "G\u00f6t\u00b7ter\u00b7loos", "hin\u00b7auf", "mag", "sich", "der", "Gram", "nicht", "dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VMFIN", "PRF", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Herzens Wunde h\u00fcllt sich gern in Gr\u00e4bernacht.", "tokens": ["Des", "Her\u00b7zens", "Wun\u00b7de", "h\u00fcllt", "sich", "gern", "in", "Gr\u00e4\u00b7ber\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PRF", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}