{"dta.poem.10244": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckes genug.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn sanft du mir im Arme schliefst,", "tokens": ["Wenn", "sanft", "du", "mir", "im", "Ar\u00b7me", "schliefst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich deinen Athem h\u00f6ren konnte,", "tokens": ["Ich", "dei\u00b7nen", "A\u00b7them", "h\u00f6\u00b7ren", "konn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Traum du meinen Namen riefst,", "tokens": ["Im", "Traum", "du", "mei\u00b7nen", "Na\u00b7men", "riefst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um deinen Mund ein L\u00e4cheln sonnte \u2014", "tokens": ["Um", "dei\u00b7nen", "Mund", "ein", "L\u00e4\u00b7cheln", "sonn\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wenn nach hei\u00dfem, ernstem Tag", "tokens": ["Und", "wenn", "nach", "hei\u00b7\u00dfem", ",", "erns\u00b7tem", "Tag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du mir verscheuchtest schwere Sorgen,", "tokens": ["Du", "mir", "ver\u00b7scheuch\u00b7test", "schwe\u00b7re", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ich an deinem Herzen lag,", "tokens": ["Wenn", "ich", "an", "dei\u00b7nem", "Her\u00b7zen", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nicht mehr dachte an ein Morgen \u2014", "tokens": ["Und", "nicht", "mehr", "dach\u00b7te", "an", "ein", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}