{"textgrid.poem.56406": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "27", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thrym l\u00fcftet' das Leinen", "tokens": ["Th\u00b7rym", "l\u00fcf\u00b7tet'", "das", "Lei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus Lust sie zu k\u00fcssen,", "tokens": ["Aus", "Lust", "sie", "zu", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "So weit der Saal war,", "tokens": ["So", "weit", "der", "Saal", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ward zur\u00fcck er geschreckt.", "tokens": ["Ward", "zu\u00b7r\u00fcck", "er", "ge\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "\u00bbwie sind doch furchtbar", "tokens": ["\u00bb", "wie", "sind", "doch", "furcht\u00b7bar"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "KOKOM", "VAFIN", "ADV", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Frayas Augen,", "tokens": ["Fray\u00b7as", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "D\u00fcnkte mich Feuer hervor", "tokens": ["D\u00fcnk\u00b7te", "mich", "Feu\u00b7er", "her\u00b7vor"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "PTKVZ"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Funkeln zu sehn!\u00ab", "tokens": ["Fun\u00b7keln", "zu", "sehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}}}}}