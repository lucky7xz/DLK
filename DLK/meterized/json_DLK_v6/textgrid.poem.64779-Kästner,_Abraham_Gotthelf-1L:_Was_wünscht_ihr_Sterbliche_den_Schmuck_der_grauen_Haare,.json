{"textgrid.poem.64779": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was w\u00fcnscht ihr Sterbliche den Schmuck der grauen Haare,", "genre": "verse", "period": "N.A.", "pub_year": 1743, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was w\u00fcnscht ihr Sterbliche den Schmuck der grauen Haare,", "tokens": ["Was", "w\u00fcnscht", "ihr", "Sterb\u00b7li\u00b7che", "den", "Schmuck", "der", "grau\u00b7en", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als da\u00df ihr mit Verdru\u00df die letzten Lebensjahre,", "tokens": ["Als", "da\u00df", "ihr", "mit", "Ver\u00b7dru\u00df", "die", "letz\u00b7ten", "Le\u00b7bens\u00b7jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vergessen, ungebraucht, im Weg der j\u00fcngern Welt,", "tokens": ["Ver\u00b7ges\u00b7sen", ",", "un\u00b7ge\u00b7braucht", ",", "im", "Weg", "der", "j\u00fcn\u00b7gern", "Welt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zur Last euch selbsten seyd, und vor'ges Gl\u00fcck verg\u00e4llt;", "tokens": ["Zur", "Last", "euch", "selbs\u00b7ten", "seyd", ",", "und", "vor'\u00b7ges", "Gl\u00fcck", "ver\u00b7g\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADV", "VAFIN", "$,", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wie ein alter Baum, von innen ausgezehret,", "tokens": ["So", "wie", "ein", "al\u00b7ter", "Baum", ",", "von", "in\u00b7nen", "aus\u00b7ge\u00b7zeh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "$,", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Selbst keine Frucht mehr tr\u00e4gt, und andrer Wachsthum st\u00f6ret.", "tokens": ["Selbst", "kei\u00b7ne", "Frucht", "mehr", "tr\u00e4gt", ",", "und", "an\u00b7drer", "Wach\u00b7sthum", "st\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "VVFIN", "$,", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Menschen, deren Werth im K\u00f6rper nur besteht,", "tokens": ["Die", "Men\u00b7schen", ",", "de\u00b7ren", "Werth", "im", "K\u00f6r\u00b7per", "nur", "be\u00b7steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Empfinden diesen Schmerz, wenn seine Kraft vergeht;", "tokens": ["Emp\u00b7fin\u00b7den", "die\u00b7sen", "Schmerz", ",", "wenn", "sei\u00b7ne", "Kraft", "ver\u00b7geht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der, den der Geist erhebt, wird bessers Gl\u00fcck erfahren,", "tokens": ["Der", ",", "den", "der", "Geist", "er\u00b7hebt", ",", "wird", "bes\u00b7sers", "Gl\u00fcck", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "VAFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn Geister altern nicht, sie reifen mit den Jahren.", "tokens": ["Denn", "Geis\u00b7ter", "al\u00b7tern", "nicht", ",", "sie", "rei\u00b7fen", "mit", "den", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "PTKNEG", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Nachwelt lernt von ihm der V\u00e4ter Unterricht,", "tokens": ["Die", "Nach\u00b7welt", "lernt", "von", "ihm", "der", "V\u00e4\u00b7ter", "Un\u00b7ter\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sieht, ihr Vorzug sey was mehr, als ein Gedicht,", "tokens": ["Und", "sieht", ",", "ihr", "Vor\u00b7zug", "sey", "was", "mehr", ",", "als", "ein", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "PIS", "ADV", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das nur ein Buch erz\u00e4hlt. Ger\u00fchrt durch sein Ermahnen", "tokens": ["Das", "nur", "ein", "Buch", "er\u00b7z\u00e4hlt", ".", "Ge\u00b7r\u00fchrt", "durch", "sein", "Er\u00b7mah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "NN", "VVFIN", "$.", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "H\u00f6rt ihn der Enkel Schaar, den Freund von ihren Ahnen.", "tokens": ["H\u00f6rt", "ihn", "der", "En\u00b7kel", "Schaar", ",", "den", "Freund", "von", "ih\u00b7ren", "Ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wird dann noch seine Huld die Herzen an sich ziehn,", "tokens": ["Wird", "dann", "noch", "sei\u00b7ne", "Huld", "die", "Her\u00b7zen", "an", "sich", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "NN", "ART", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird f\u00fcr sein eigen Gl\u00fcck befreyt sich zu bem\u00fchn,", "tokens": ["Wird", "f\u00fcr", "sein", "ei\u00b7gen", "Gl\u00fcck", "be\u00b7freyt", "sich", "zu", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er nur auf Andrer Wohl sich zum Vergn\u00fcgen denken:", "tokens": ["Er", "nur", "auf", "A\u00b7ndrer", "Wohl", "sich", "zum", "Ver\u00b7gn\u00fc\u00b7gen", "den\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach! dann wird sein Verlust uns niemals sp\u00e4te kr\u00e4nken.", "tokens": ["Ach", "!", "dann", "wird", "sein", "Ver\u00b7lust", "uns", "nie\u00b7mals", "sp\u00e4\u00b7te", "kr\u00e4n\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VAFIN", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So klagt man, theurer Graf, durch deinen Tod best\u00fcrzt,", "tokens": ["So", "klagt", "man", ",", "theu\u00b7rer", "Graf", ",", "durch", "dei\u00b7nen", "Tod", "be\u00b7st\u00fcrzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "ADJD", "NE", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df dir der Vorsicht Schlu\u00df dein Leben noch verk\u00fcrzt,", "tokens": ["Da\u00df", "dir", "der", "Vor\u00b7sicht", "Schlu\u00df", "dein", "Le\u00b7ben", "noch", "ver\u00b7k\u00fcrzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dir nicht das h\u00f6chste Ziel der Sterblichen verliehen;", "tokens": ["Dir", "nicht", "das", "h\u00f6chs\u00b7te", "Ziel", "der", "Sterb\u00b7li\u00b7chen", "ver\u00b7lie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch w\u00e4rst du auch dahin, du schienst uns noch zu fliehen.", "tokens": ["Doch", "w\u00e4rst", "du", "auch", "da\u00b7hin", ",", "du", "schienst", "uns", "noch", "zu", "flie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PAV", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich will mit schwacher Kraft nicht deinen Ruhm entweyhn;", "tokens": ["Ich", "will", "mit", "schwa\u00b7cher", "Kraft", "nicht", "dei\u00b7nen", "Ruhm", "ent\u00b7weyhn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch, r\u00fchmet man an dir den Staatsmann und den Weisen,", "tokens": ["Doch", ",", "r\u00fch\u00b7met", "man", "an", "dir", "den", "Staats\u00b7mann", "und", "den", "Wei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PIS", "APPR", "PPER", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "So strebt' ich, k\u00f6nnt' ich nur, den Menschenfreund zu preisen.", "tokens": ["So", "strebt'", "ich", ",", "k\u00f6nnt'", "ich", "nur", ",", "den", "Men\u00b7schen\u00b7freund", "zu", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}