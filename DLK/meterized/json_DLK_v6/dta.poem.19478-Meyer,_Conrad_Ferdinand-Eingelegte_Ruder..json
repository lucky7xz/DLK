{"dta.poem.19478": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Eingelegte Ruder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Meine eingelegten Ruder triefen,", "tokens": ["Mei\u00b7ne", "ein\u00b7ge\u00b7leg\u00b7ten", "Ru\u00b7der", "trie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Tropfen fallen langsam in die Tiefen.", "tokens": ["Trop\u00b7fen", "fal\u00b7len", "lang\u00b7sam", "in", "die", "Tie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Nichts das mich verdro\u00df! Nichts das mich freute!", "tokens": ["Nichts", "das", "mich", "ver\u00b7dro\u00df", "!", "Nichts", "das", "mich", "freu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "PPER", "VVFIN", "$.", "PIS", "ART", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Niederrinnt ein schmerzenloses Heute!", "tokens": ["Nie\u00b7der\u00b7rinnt", "ein", "schmer\u00b7zen\u00b7lo\u00b7ses", "Heu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Unter mir \u2014 ach, aus dem Licht verschwunden \u2014", "tokens": ["Un\u00b7ter", "mir", "ach", ",", "aus", "dem", "Licht", "ver\u00b7schwun\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "ITJ", "$,", "APPR", "ART", "NN", "VVPP", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Tr\u00e4umen schon die sch\u00f6nern meiner Stunden.", "tokens": ["Tr\u00e4u\u00b7men", "schon", "die", "sch\u00f6\u00b7nern", "mei\u00b7ner", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Aus der blauen Tiefe ruft das Gestern:", "tokens": ["Aus", "der", "blau\u00b7en", "Tie\u00b7fe", "ruft", "das", "Ge\u00b7stern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sind im Licht noch manche meiner Schwestern?", "tokens": ["Sind", "im", "Licht", "noch", "man\u00b7che", "mei\u00b7ner", "Schwes\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADV", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}