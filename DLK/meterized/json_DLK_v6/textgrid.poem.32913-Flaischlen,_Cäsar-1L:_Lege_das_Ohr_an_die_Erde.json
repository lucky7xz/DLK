{"textgrid.poem.32913": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lege das Ohr an die Erde", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lege das Ohr an die Erde", "tokens": ["Le\u00b7ge", "das", "Ohr", "an", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "und h\u00f6re ...", "tokens": ["und", "h\u00f6\u00b7re", "..."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "und du wirst Hufgestampf h\u00f6ren, in weiter Ferne nur, aber n\u00e4her und n\u00e4her kommend.", "tokens": ["und", "du", "wirst", "Huf\u00b7ge\u00b7stampf", "h\u00f6\u00b7ren", ",", "in", "wei\u00b7ter", "Fer\u00b7ne", "nur", ",", "a\u00b7ber", "n\u00e4\u00b7her", "und", "n\u00e4\u00b7her", "kom\u00b7mend", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "VVINF", "$,", "APPR", "ADV", "NN", "ADV", "$,", "ADV", "ADJD", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+--+--+-+--+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Es ist die Zukunft", "tokens": ["Es", "ist", "die", "Zu\u00b7kunft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "auf lichtwei\u00dfen Pferden ...", "tokens": ["auf", "licht\u00b7wei\u00b7\u00dfen", "Pfer\u00b7den", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "eine goldene Krone im blauen Banner ...", "tokens": ["ei\u00b7ne", "gol\u00b7de\u00b7ne", "Kro\u00b7ne", "im", "blau\u00b7en", "Ban\u00b7ner", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "die Krone des Menschen und seines Siegs und seines K\u00f6nigtums!", "tokens": ["die", "Kro\u00b7ne", "des", "Men\u00b7schen", "und", "sei\u00b7nes", "Siegs", "und", "sei\u00b7nes", "K\u00f6\u00b7nig\u00b7tums", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+-+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Raffe dich auf aus deinem Alltag und g\u00fcrte das Schwert um deine Lenden und k\u00e4mpfe ihr entgegen ...", "tokens": ["Raf\u00b7fe", "dich", "auf", "aus", "dei\u00b7nem", "All\u00b7tag", "und", "g\u00fcr\u00b7te", "das", "Schwert", "um", "dei\u00b7ne", "Len\u00b7den", "und", "k\u00e4mp\u00b7fe", "ihr", "ent\u00b7ge\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "+--+-+-+--+--+-+-+--+-+-+-", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "denn", "tokens": ["denn"], "token_info": ["word"], "pos": ["KON"], "meter": "-", "measure": "single.down"}}}}}