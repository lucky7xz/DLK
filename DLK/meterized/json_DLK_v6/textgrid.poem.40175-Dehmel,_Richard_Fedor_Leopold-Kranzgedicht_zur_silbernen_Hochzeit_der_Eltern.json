}, "line.43": {"text": "Da schwebt zur Erde ein entf\u00e4rbtes Blatt,", "tokens": ["Da", "schwebt", "zur", "Er\u00b7de", "ein", "ent\u00b7f\u00e4rb\u00b7tes", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "und halb schon sinkend auch die andern h\u00e4ngen:", "tokens": ["und", "halb", "schon", "sin\u00b7kend", "auch", "die", "an\u00b7dern", "h\u00e4n\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ADJD", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "der ", "tokens": ["der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.46": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.47": {"text": "ob auch entlaubt die Aeste: freundlich matt", "tokens": ["ob", "auch", "ent\u00b7laubt", "die", "A\u00b7es\u00b7te", ":", "freund\u00b7lich", "matt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "VVFIN", "ART", "NN", "$.", "ADJD", "PTKVZ"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.48": {"text": "von ihnen her die reifen Fr\u00fcchte blinken, \u2013", "tokens": ["von", "ih\u00b7nen", "her", "die", "rei\u00b7fen", "Fr\u00fcch\u00b7te", "blin\u00b7ken", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "und so wird M\u00fche Segen, That Genu\u00df ...", "tokens": ["und", "so", "wird", "M\u00fc\u00b7he", "Se\u00b7gen", ",", "That", "Ge\u00b7nu\u00df", ".