{"textgrid.poem.40175": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Kranzgedicht zur silbernen Hochzeit der Eltern", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fast scheu' ich mich, Euch diesen Kranz zu reichen,", "tokens": ["Fast", "scheu'", "ich", "mich", ",", "Euch", "die\u00b7sen", "Kranz", "zu", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PPER", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der deutungsvoll sich um zwei Leben schlingt", "tokens": ["der", "deu\u00b7tungs\u00b7voll", "sich", "um", "zwei", "Le\u00b7ben", "schlingt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PRF", "APPR", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "so liebeheilig mir, da\u00df tief im weichen", "tokens": ["so", "lie\u00b7be\u00b7hei\u00b7lig", "mir", ",", "da\u00df", "tief", "im", "wei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "$,", "KOUS", "ADJD", "APPRART", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gef\u00fchle ringend mir das Wort versinkt ...", "tokens": ["Ge\u00b7f\u00fch\u00b7le", "rin\u00b7gend", "mir", "das", "Wort", "ver\u00b7sinkt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch sch\u00f6n're Sprache, als sie je ert\u00f6nte,", "tokens": ["Doch", "sch\u00f6n'\u00b7re", "Spra\u00b7che", ",", "als", "sie", "je", "er\u00b7t\u00f6n\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "von jedem dieser lichten Bl\u00e4ttchen blinkt;", "tokens": ["von", "je\u00b7dem", "die\u00b7ser", "lich\u00b7ten", "Bl\u00e4tt\u00b7chen", "blinkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PDAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "von jedem Gl\u00fcck, das euern Weg versch\u00f6nte,", "tokens": ["von", "je\u00b7dem", "Gl\u00fcck", ",", "das", "eu\u00b7ern", "Weg", "ver\u00b7sch\u00f6n\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "von jeder Sorge spricht sie, jedem Leid,", "tokens": ["von", "je\u00b7der", "Sor\u00b7ge", "spricht", "sie", ",", "je\u00b7dem", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "mit denen Kraft und Liebe Euch vers\u00f6hnte.", "tokens": ["mit", "de\u00b7nen", "Kraft", "und", "Lie\u00b7be", "Euch", "ver\u00b7s\u00f6hn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "so seltsam ineinander Traum und Leben,", "tokens": ["so", "selt\u00b7sam", "in\u00b7ein\u00b7an\u00b7der", "Traum", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "da\u00df Augenblick und l\u00e4ngst versunkne Zeit", "tokens": ["da\u00df", "Au\u00b7gen\u00b7blick", "und", "l\u00e4ngst", "ver\u00b7sunk\u00b7ne", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "zu reichsten Wirklichkeiten sich verweben.", "tokens": ["zu", "reichs\u00b7ten", "Wirk\u00b7lich\u00b7kei\u00b7ten", "sich", "ver\u00b7we\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und so, aus dieses Kranzes Schimmerschein,", "tokens": ["Und", "so", ",", "aus", "die\u00b7ses", "Kran\u00b7zes", "Schim\u00b7mer\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Gestalten seh' ich, Bilder sacht sich heben:", "tokens": ["Ge\u00b7stal\u00b7ten", "seh'", "ich", ",", "Bil\u00b7der", "sacht", "sich", "he\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "NN", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "die Zweiglein schlank, die Bl\u00e4ttchen zierlich fein", "tokens": ["die", "Zwei\u00b7glein", "schlank", ",", "die", "Bl\u00e4tt\u00b7chen", "zier\u00b7lich", "fein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "empor zu Stamm und Laubwerk vor mir schie\u00dfen", "tokens": ["em\u00b7por", "zu", "Stamm", "und", "Laub\u00b7werk", "vor", "mir", "schie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKVZ", "APPR", "NN", "KON", "NN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "in Eures Lebens Garten tret' ich ein, \u2013", "tokens": ["in", "Eu\u00b7res", "Le\u00b7bens", "Gar\u00b7ten", "tret'", "ich", "ein", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "oh leuchtend Bild! ich mu\u00df die Augen schlie\u00dfen ...", "tokens": ["oh", "leuch\u00b7tend", "Bild", "!", "ich", "mu\u00df", "die", "Au\u00b7gen", "schlie\u00b7\u00dfen", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "NN", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Ich seh' euch wandeln. Fr\u00fchlingsrein die Luft", "tokens": ["Ich", "seh'", "euch", "wan\u00b7deln", ".", "Fr\u00fch\u00b7lings\u00b7rein", "die", "Luft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$.", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "der Hoffnung bunte Blumen um euch sprie\u00dfen,", "tokens": ["der", "Hoff\u00b7nung", "bun\u00b7te", "Blu\u00b7men", "um", "euch", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "aus allen Kelchen quillt Ein Glanz, Ein Duft,", "tokens": ["aus", "al\u00b7len", "Kel\u00b7chen", "quillt", "Ein", "Glanz", ",", "Ein", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "und vor euch liegt ein Pfad, den zu beschreiten", "tokens": ["und", "vor", "euch", "liegt", "ein", "Pfad", ",", "den", "zu", "be\u00b7schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "das V\u00f6glein Gl\u00fcck euch lieblich lockend ruft ...", "tokens": ["das", "V\u00f6\u00b7glein", "Gl\u00fcck", "euch", "lieb\u00b7lich", "lo\u00b7ckend", "ruft", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "ADJD", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Schon wallt ihr ihn; und an des Weges Seiten", "tokens": ["Schon", "wallt", "ihr", "ihn", ";", "und", "an", "des", "We\u00b7ges", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "in langer Reihe seh' ich Paar an Paar", "tokens": ["in", "lan\u00b7ger", "Rei\u00b7he", "seh'", "ich", "Paar", "an", "Paar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "fruchtschwere B\u00e4ume ihre Zweige breiten,", "tokens": ["frucht\u00b7schwe\u00b7re", "B\u00e4u\u00b7me", "ih\u00b7re", "Zwei\u00b7ge", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "als wollte Tag f\u00fcr Tag und Jahr um Jahr", "tokens": ["als", "woll\u00b7te", "Tag", "f\u00fcr", "Tag", "und", "Jahr", "um", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "der Sommer eures Wirkens mir sich k\u00fcnden.", "tokens": ["der", "Som\u00b7mer", "eu\u00b7res", "Wir\u00b7kens", "mir", "sich", "k\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Und dichter seh' ich, wie vor Sturmgefahr,", "tokens": ["Und", "dich\u00b7ter", "seh'", "ich", ",", "wie", "vor", "Sturm\u00b7ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "die starken Aeste ihren Wuchs verb\u00fcnden", "tokens": ["die", "star\u00b7ken", "A\u00b7es\u00b7te", "ih\u00b7ren", "Wuchs", "ver\u00b7b\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "zu einem schirmend festen Wetterdach,", "tokens": ["zu", "ei\u00b7nem", "schir\u00b7mend", "fes\u00b7ten", "Wet\u00b7ter\u00b7dach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "um vor des Menschenlebens Weh und S\u00fcnden,", "tokens": ["um", "vor", "des", "Men\u00b7schen\u00b7le\u00b7bens", "Weh", "und", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "vor des Geschicks gesch\u00e4ftigem Ungemach", "tokens": ["vor", "des", "Ge\u00b7schicks", "ge\u00b7sch\u00e4f\u00b7ti\u00b7gem", "Un\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.34": {"text": "die jungen Pflanzen sorglich zu beh\u00fcten,", "tokens": ["die", "jun\u00b7gen", "Pflan\u00b7zen", "sorg\u00b7lich", "zu", "be\u00b7h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "die unter Lust und Wollust, Schmerz und Ach", "tokens": ["die", "un\u00b7ter", "Lust", "und", "Wol\u00b7lust", ",", "Schmerz", "und", "Ach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "aus Eurer Liebe keimten, spro\u00dften, bl\u00fchten ...", "tokens": ["aus", "Eu\u00b7rer", "Lie\u00b7be", "ke\u00b7im\u00b7ten", ",", "spro\u00df\u00b7ten", ",", "bl\u00fch\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Und jetzt \u2013 in Nebel