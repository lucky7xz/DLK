{"textgrid.poem.42238": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "An Bruno", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcberkommt dich nun, mein holder Knabe,", "tokens": ["\u00dc\u00b7ber\u00b7kommt", "dich", "nun", ",", "mein", "hol\u00b7der", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deines Erdendaseins h\u00f6chste Gabe,", "tokens": ["Dei\u00b7nes", "Er\u00b7den\u00b7da\u00b7seins", "h\u00f6chs\u00b7te", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn die Schenkel rosig frisch dir schwellen,", "tokens": ["Wenn", "die", "Schen\u00b7kel", "ro\u00b7sig", "frisch", "dir", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn der Flaum dir um die Lippen keimt,", "tokens": ["Wenn", "der", "Flaum", "dir", "um", "die", "Lip\u00b7pen", "keimt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn dein Sehnen trotz der Sturmeswellen", "tokens": ["Wenn", "dein", "Seh\u00b7nen", "trotz", "der", "Stur\u00b7mes\u00b7wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Spielend sich zu leichten Liedern reimt \u2013", "tokens": ["Spie\u00b7lend", "sich", "zu", "leich\u00b7ten", "Lie\u00b7dern", "reimt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Pr\u00e4g dir dann f\u00fcr alle Zukunft ein:", "tokens": ["Pr\u00e4g", "dir", "dann", "f\u00fcr", "al\u00b7le", "Zu\u00b7kunft", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Deines Erdendaseins h\u00f6chste Gabe", "tokens": ["Dei\u00b7nes", "Er\u00b7den\u00b7da\u00b7seins", "h\u00f6chs\u00b7te", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "L\u00e4\u00dft dich eines nur von dreien sein,", "tokens": ["L\u00e4\u00dft", "dich", "ei\u00b7nes", "nur", "von", "drei\u00b7en", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADV", "APPR", "CARD", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Und du hast f\u00fcr eine der drei Freuden", "tokens": ["Und", "du", "hast", "f\u00fcr", "ei\u00b7ne", "der", "drei", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ART", "ART", "CARD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "In der ersten Nacht dich zu entscheiden!", "tokens": ["In", "der", "ers\u00b7ten", "Nacht", "dich", "zu", "ent\u00b7schei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}