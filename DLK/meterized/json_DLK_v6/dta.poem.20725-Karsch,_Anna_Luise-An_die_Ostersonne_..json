{"dta.poem.20725": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An die Ostersonne .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ostersonne! du bist sch\u00f6n               ", "tokens": ["Os\u00b7ter\u00b7son\u00b7ne", "!", "du", "bist", "sch\u00f6n"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VAFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Freundin aufgegangen,", "tokens": ["Mei\u00b7ner", "Freun\u00b7din", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kinder werden um sie stehn,", "tokens": ["Kin\u00b7der", "wer\u00b7den", "um", "sie", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihren Seegen zu empfangen,", "tokens": ["Ih\u00b7ren", "See\u00b7gen", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und dazu ein buntes Ei,", "tokens": ["Und", "da\u00b7zu", "ein", "bun\u00b7tes", "Ei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und ich hoffe, da\u00df sie heiter", "tokens": ["Und", "ich", "hof\u00b7fe", ",", "da\u00df", "sie", "hei\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie der Ostermorgen sey,", "tokens": ["Wie", "der", "Os\u00b7ter\u00b7mor\u00b7gen", "sey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hoffe, da\u00df sie mich noch weiter", "tokens": ["Hof\u00b7fe", ",", "da\u00df", "sie", "mich", "noch", "wei\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Lieb behalten wird, ob ich", "tokens": ["Lieb", "be\u00b7hal\u00b7ten", "wird", ",", "ob", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "VAFIN", "$,", "KOUS", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Gleich ihr Antlitz nicht mehr sehe \u2014", "tokens": ["Gleich", "ihr", "Ant\u00b7litz", "nicht", "mehr", "se\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKNEG", "ADV", "VVFIN", "$("], "meter": "+-+-+++-", "measure": "unknown.measure.penta"}, "line.11": {"text": "Osterwasser l\u00e4\u00dft Sie sich", "tokens": ["Os\u00b7ter\u00b7was\u00b7ser", "l\u00e4\u00dft", "Sie", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Wol nicht sch\u00f6pfen in der N\u00e4he", "tokens": ["Wol", "nicht", "sch\u00f6p\u00b7fen", "in", "der", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Aus der Elbe, wo du dich", "tokens": ["Aus", "der", "El\u00b7be", ",", "wo", "du", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "$,", "PWAV", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Dreymal h\u00fcpfend hast gespiegelt,", "tokens": ["Drey\u00b7mal", "h\u00fcp\u00b7fend", "hast", "ge\u00b7spie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Sie will nicht versch\u00f6nert seyn \u2014", "tokens": ["Sie", "will", "nicht", "ver\u00b7sch\u00f6\u00b7nert", "seyn"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVPP", "VAINF", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Gr\u00fc\u00dfe hat Sie fortgefl\u00fcgelt,", "tokens": ["Gr\u00fc\u00b7\u00dfe", "hat", "Sie", "fort\u00b7ge\u00b7fl\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und vielleicht ist einer mein", "tokens": ["Und", "viel\u00b7leicht", "ist", "ei\u00b7ner", "mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "PPOSAT"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.18": {"text": "Unter diesen Ostergr\u00fc\u00dfen,", "tokens": ["Un\u00b7ter", "die\u00b7sen", "Os\u00b7ter\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Und in diesem Erdenthal", "tokens": ["Und", "in", "die\u00b7sem", "Er\u00b7dent\u00b7hal"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Werd ich heute Dich genie\u00dfen", "tokens": ["Werd", "ich", "heu\u00b7te", "Dich", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Ganz gewi\u00df zum letztenmal;", "tokens": ["Ganz", "ge\u00b7wi\u00df", "zum", "letz\u00b7ten\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Denn ich darf nichts mehr versuchen", "tokens": ["Denn", "ich", "darf", "nichts", "mehr", "ver\u00b7su\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PIS", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Vom gebratnen Osterlamm,", "tokens": ["Vom", "ge\u00b7brat\u00b7nen", "Os\u00b7ter\u00b7lamm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Oder auch vom Osterkuchen;", "tokens": ["O\u00b7der", "auch", "vom", "Os\u00b7ter\u00b7ku\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Ich bin wie ein Weidenstamm,", "tokens": ["Ich", "bin", "wie", "ein", "Wei\u00b7dens\u00b7tamm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "Den der Wurm ganz hohl gefressen", "tokens": ["Den", "der", "Wurm", "ganz", "hohl", "ge\u00b7fres\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADV", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Und die Fluth halb abgesp\u00fchlt", "tokens": ["Und", "die", "Fluth", "halb", "ab\u00b7ge\u00b7sp\u00fchlt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Von dem Raum, wo er gesessen.", "tokens": ["Von", "dem", "Raum", ",", "wo", "er", "ge\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Meine Seele lebt und f\u00fchlt", "tokens": ["Mei\u00b7ne", "See\u00b7le", "lebt", "und", "f\u00fchlt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Nur noch deinen Glanz, du milde", "tokens": ["Nur", "noch", "dei\u00b7nen", "Glanz", ",", "du", "mil\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$,", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "S\u00fc\u00dfe Knospen\u00f6fnerin!", "tokens": ["S\u00fc\u00b7\u00dfe", "Knos\u00b7pe\u00b7n\u00f6f\u00b7ne\u00b7rin", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Nur mein Auge sieht noch hin", "tokens": ["Nur", "mein", "Au\u00b7ge", "sieht", "noch", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Ins bebl\u00fcmte Grasgefilde,", "tokens": ["Ins", "be\u00b7bl\u00fcm\u00b7te", "Gras\u00b7ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Bleibt noch munter, bis es bricht;", "tokens": ["Bleibt", "noch", "mun\u00b7ter", ",", "bis", "es", "bricht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Brechen wirds eh du vorhanden", "tokens": ["Bre\u00b7chen", "wirds", "eh", "du", "vor\u00b7han\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "KOUS", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Wieder bist, und singen h\u00f6rst:", "tokens": ["Wie\u00b7der", "bist", ",", "und", "sin\u00b7gen", "h\u00f6rst", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KON", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Von dem, den du h\u00fcpfend ehrst,", "tokens": ["Von", "dem", ",", "den", "du", "h\u00fcp\u00b7fend", "ehrst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.38": {"text": "Er sey auferstanden \u2014", "tokens": ["Er", "sey", "auf\u00b7er\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.39": {"text": "Auferstehen soll auch ich,", "tokens": ["Auf\u00b7er\u00b7ste\u00b7hen", "soll", "auch", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Aber ob mit diesem Leibe,", "tokens": ["A\u00b7ber", "ob", "mit", "die\u00b7sem", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Den du w\u00e4rmest, wenn ich dich", "tokens": ["Den", "du", "w\u00e4r\u00b7mest", ",", "wenn", "ich", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "VAFIN", "$,", "KOUS", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Sehe durch die Fensterscheibe,", "tokens": ["Se\u00b7he", "durch", "die", "Fens\u00b7ter\u00b7schei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Ob mit dieser welken Haut", "tokens": ["Ob", "mit", "die\u00b7ser", "wel\u00b7ken", "Haut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Und mit diesen morschen Knochen?", "tokens": ["Und", "mit", "die\u00b7sen", "mor\u00b7schen", "Kno\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Ob mein Grab wird durchgebrochen", "tokens": ["Ob", "mein", "Grab", "wird", "durch\u00b7ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Von dem Kopfe, der jezt sich", "tokens": ["Von", "dem", "Kop\u00b7fe", ",", "der", "jezt", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "In die H\u00f6he kann erheben,", "tokens": ["In", "die", "H\u00f6\u00b7he", "kann", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Wenn die Nacht dem Tage wich", "tokens": ["Wenn", "die", "Nacht", "dem", "Ta\u00b7ge", "wich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Und du Th\u00e4tigkeit gegeben", "tokens": ["Und", "du", "Th\u00e4\u00b7tig\u00b7keit", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Einer halben Welt wie mir \u2014", "tokens": ["Ei\u00b7ner", "hal\u00b7ben", "Welt", "wie", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOKOM", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Ob du mich siehst auferstehen,", "tokens": ["Ob", "du", "mich", "siehst", "auf\u00b7er\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Oder ob auch deine Zier", "tokens": ["O\u00b7der", "ob", "auch", "dei\u00b7ne", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Mit den Bergen untergehen,", "tokens": ["Mit", "den", "Ber\u00b7gen", "un\u00b7ter\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Mit den Th\u00fcrmen st\u00fcrzen mu\u00df? \u2014", "tokens": ["Mit", "den", "Th\u00fcr\u00b7men", "st\u00fcr\u00b7zen", "mu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VMFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Ach! dies kann kein Weiser sagen,", "tokens": ["Ach", "!", "dies", "kann", "kein", "Wei\u00b7ser", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PDS", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Und ich wills auch beim Genu\u00df", "tokens": ["Und", "ich", "wills", "auch", "beim", "Ge\u00b7nu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.57": {"text": "Nicht ergr\u00fcbeln, nicht erfragen,", "tokens": ["Nicht", "er\u00b7gr\u00fc\u00b7beln", ",", "nicht", "er\u00b7fra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Will genie\u00dfen deinen Glanz", "tokens": ["Will", "ge\u00b7nie\u00b7\u00dfen", "dei\u00b7nen", "Glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "In des jungen Fr\u00fchlings Tagen,", "tokens": ["In", "des", "jun\u00b7gen", "Fr\u00fch\u00b7lings", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.60": {"text": "Will mir einen Blumenkranz", "tokens": ["Will", "mir", "ei\u00b7nen", "Blu\u00b7men\u00b7kranz"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.61": {"text": "Noch um meine Schl\u00e4fe winden,", "tokens": ["Noch", "um", "mei\u00b7ne", "Schl\u00e4\u00b7fe", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.62": {"text": "Wo sich hin und wieder nur", "tokens": ["Wo", "sich", "hin", "und", "wie\u00b7der", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "PTKVZ", "KON", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.63": {"text": "L\u00e4\u00dft ein graues H\u00e4rlein finden,", "tokens": ["L\u00e4\u00dft", "ein", "grau\u00b7es", "H\u00e4r\u00b7lein", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.64": {"text": "Des gestiegnen Alters Spur. \u2014", "tokens": ["Des", "ge\u00b7stieg\u00b7nen", "Al\u00b7ters", "Spur", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.65": {"text": "Soll mir nun mein Auge brechen,", "tokens": ["Soll", "mir", "nun", "mein", "Au\u00b7ge", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.66": {"text": "Ehe noch ein Jahr entschl\u00fcpft", "tokens": ["E\u00b7he", "noch", "ein", "Jahr", "ent\u00b7schl\u00fcpft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.67": {"text": "Und von dir viel Christen sprechen,", "tokens": ["Und", "von", "dir", "viel", "Chris\u00b7ten", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.68": {"text": "Da\u00df du dreimal aufgeh\u00fcpft", "tokens": ["Da\u00df", "du", "drei\u00b7mal", "auf\u00b7ge\u00b7h\u00fcpft"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.69": {"text": "An des Ostertages Morgen,", "tokens": ["An", "des", "Os\u00b7ter\u00b7ta\u00b7ges", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.70": {"text": "Dann hab\u2019 ich den Engel lieb,", "tokens": ["Dann", "hab'", "ich", "den", "En\u00b7gel", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.71": {"text": "Der aus einer Welt voll Sorgen", "tokens": ["Der", "aus", "ei\u00b7ner", "Welt", "voll", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.72": {"text": "Mich in eine be\u00dfre trieb \u2014", "tokens": ["Mich", "in", "ei\u00b7ne", "be\u00df\u00b7re", "trieb"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}