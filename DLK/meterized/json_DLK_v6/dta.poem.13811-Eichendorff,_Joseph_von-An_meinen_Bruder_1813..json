{"dta.poem.13811": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An meinen Bruder 1813.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "en:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So wolle Gott Du flehen,", "tokens": ["So", "wol\u00b7le", "Gott", "Du", "fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df er mit meinem Blut und Leben schalte,", "tokens": ["Da\u00df", "er", "mit", "mei\u00b7nem", "Blut", "und", "Le\u00b7ben", "schal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Seele nur erhalte,", "tokens": ["Die", "See\u00b7le", "nur", "er\u00b7hal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf da\u00df wir freudig einst uns wiedersehen,", "tokens": ["Auf", "da\u00df", "wir", "freu\u00b7dig", "einst", "uns", "wie\u00b7der\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn nimmermehr hienieden:", "tokens": ["Wenn", "nim\u00b7mer\u00b7mehr", "hien\u00b7ie\u00b7den", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So dort, wo Heimath, Licht und ew'ger Frieden!", "tokens": ["So", "dort", ",", "wo", "Hei\u00b7math", ",", "Licht", "und", "ew'\u00b7ger", "Frie\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}