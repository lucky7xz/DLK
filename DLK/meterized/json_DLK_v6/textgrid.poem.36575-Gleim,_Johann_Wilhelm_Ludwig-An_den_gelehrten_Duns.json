{"textgrid.poem.36575": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An den gelehrten Duns", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df uns die Vernunft vertrinken,", "tokens": ["La\u00df", "uns", "die", "Ver\u00b7nunft", "ver\u00b7trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Grundgelehrter Duns!", "tokens": ["Grund\u00b7ge\u00b7lehr\u00b7ter", "Duns", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "La\u00df uns die Vernunft vertrinken,", "tokens": ["La\u00df", "uns", "die", "Ver\u00b7nunft", "ver\u00b7trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn was n\u00fctzt sie uns?", "tokens": ["Denn", "was", "n\u00fctzt", "sie", "uns", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Unsre neue Weisen kehren", "tokens": ["Uns\u00b7re", "neu\u00b7e", "Wei\u00b7sen", "keh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles um und um!", "tokens": ["Al\u00b7les", "um", "und", "um", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKVZ", "KON", "KOUI", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Allzuklug sind ihre Lehren,", "tokens": ["A\u00b7llzu\u00b7klug", "sind", "ih\u00b7re", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Allzuklug ist dumm!", "tokens": ["A\u00b7llzu\u00b7klug", "ist", "dumm", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Alles wollen sie ergr\u00fcbeln!", "tokens": ["Al\u00b7les", "wol\u00b7len", "sie", "er\u00b7gr\u00fc\u00b7beln", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles \u2013 Gott und Wein!", "tokens": ["Al\u00b7les", "\u2013", "Gott", "und", "Wein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$(", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Trinkern w\u00e4r' es zu ver\u00fcbeln", "tokens": ["Trin\u00b7kern", "w\u00e4r'", "es", "zu", "ver\u00b7\u00fc\u00b7beln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Allzuklug zu sein!", "tokens": ["A\u00b7llzu\u00b7klug", "zu", "sein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}