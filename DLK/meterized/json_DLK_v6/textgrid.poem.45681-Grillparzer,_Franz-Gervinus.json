{"textgrid.poem.45681": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Gervinus", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Deutschen St\u00e4mme, die gem\u00fctlich schwachen,", "tokens": ["Der", "Deut\u00b7schen", "St\u00e4m\u00b7me", ",", "die", "ge\u00b7m\u00fct\u00b7lich", "schwa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gilts sozial-\u00e4sthetisch zu entpuppen.", "tokens": ["Gilts", "so\u00b7zi\u00b7a\u00b7l\u00e4s\u00b7the\u00b7tisch", "zu", "ent\u00b7pup\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Du willst sie, scheint es, zu Spartanern machen", "tokens": ["Du", "willst", "sie", ",", "scheint", "es", ",", "zu", "Spar\u00b7ta\u00b7nern", "ma\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "APPR", "NN", "VVINF"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Und sorgst vorl\u00e4ufig drum f\u00fcr schwarze Suppen.", "tokens": ["Und", "sorgst", "vor\u00b7l\u00e4u\u00b7fig", "drum", "f\u00fcr", "schwar\u00b7ze", "Sup\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}