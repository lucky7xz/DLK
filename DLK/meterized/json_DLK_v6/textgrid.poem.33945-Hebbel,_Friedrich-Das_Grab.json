{"textgrid.poem.33945": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Grab", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir war, als m\u00fc\u00dft' ich graben", "tokens": ["Mir", "war", ",", "als", "m\u00fc\u00dft'", "ich", "gra\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und grub gar tief hinab;", "tokens": ["Und", "grub", "gar", "tief", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Grub in die L\u00e4ng' und Breite,", "tokens": ["Grub", "in", "die", "L\u00e4ng'", "und", "Brei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am Ende ward's ein Grab.", "tokens": ["Am", "En\u00b7de", "ward's", "ein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "War, wei\u00df nicht wie, gezwungen,", "tokens": ["War", ",", "wei\u00df", "nicht", "wie", ",", "ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "$,", "VVFIN", "PTKNEG", "KOKOM", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hab's nimmer gern gethan,", "tokens": ["Hab's", "nim\u00b7mer", "gern", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch sollt' ich, was ich w\u00fcnschte,", "tokens": ["Doch", "sollt'", "ich", ",", "was", "ich", "w\u00fcnschte", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zuletzt als Lohn empfah'n.", "tokens": ["Zu\u00b7letzt", "als", "Lohn", "emp\u00b7fah'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "KOUS", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das Grab war aufgeworfen,", "tokens": ["Das", "Grab", "war", "auf\u00b7ge\u00b7wor\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Matt sank mir Arm und Bein,", "tokens": ["Matt", "sank", "mir", "Arm", "und", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich hatte Nichts mehr zu w\u00fcnschen", "tokens": ["Ich", "hat\u00b7te", "Nichts", "mehr", "zu", "w\u00fcn\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "PTKZU", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und legte mich selbst hinein.", "tokens": ["Und", "leg\u00b7te", "mich", "selbst", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}