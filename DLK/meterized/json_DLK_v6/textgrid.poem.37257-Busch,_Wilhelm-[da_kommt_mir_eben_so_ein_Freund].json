{"textgrid.poem.37257": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[da kommt mir eben so ein Freund]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da kommt mir eben so ein Freund", "tokens": ["Da", "kommt", "mir", "e\u00b7ben", "so", "ein", "Freund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einem gro\u00dfen Zwicker.", "tokens": ["Mit", "ei\u00b7nem", "gro\u00b7\u00dfen", "Zwi\u00b7cker", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ei, ruft er, Freundchen, wie mir scheint,", "tokens": ["Ei", ",", "ruft", "er", ",", "Freund\u00b7chen", ",", "wie", "mir", "scheint", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie werden immer dicker.", "tokens": ["Sie", "wer\u00b7den", "im\u00b7mer", "di\u00b7cker", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ja, ja, man wei\u00df oft selbst nicht wie,", "tokens": ["Ja", ",", "ja", ",", "man", "wei\u00df", "oft", "selbst", "nicht", "wie", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PIS", "VVFIN", "ADV", "ADV", "PTKNEG", "KOKOM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So kommt man in die Jahre;", "tokens": ["So", "kommt", "man", "in", "die", "Jah\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Pardon, mein Schatz, hier haben Sie", "tokens": ["Par\u00b7don", ",", "mein", "Schatz", ",", "hier", "ha\u00b7ben", "Sie"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schon eins, zwei graue Haare! \u2013", "tokens": ["Schon", "eins", ",", "zwei", "grau\u00b7e", "Haa\u00b7re", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PIS", "$,", "CARD", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hinaus, verdammter Kritikus,", "tokens": ["Hin\u00b7aus", ",", "ver\u00b7damm\u00b7ter", "Kri\u00b7ti\u00b7kus", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sonst schmei\u00df ich dich in Scherben.", "tokens": ["Sonst", "schmei\u00df", "ich", "dich", "in", "Scher\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du Schlingel willst mir den Genu\u00df", "tokens": ["Du", "Schlin\u00b7gel", "willst", "mir", "den", "Ge\u00b7nu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Gegenwart verderben!", "tokens": ["Der", "Ge\u00b7gen\u00b7wart", "ver\u00b7der\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}