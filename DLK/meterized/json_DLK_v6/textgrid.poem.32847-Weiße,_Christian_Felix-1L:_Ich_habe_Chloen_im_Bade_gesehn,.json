{"textgrid.poem.32847": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich habe Chloen im Bade gesehn,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe Chloen im Bade gesehn,", "tokens": ["Ich", "ha\u00b7be", "Chloen", "im", "Ba\u00b7de", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sie war \u2013 \u2013 wie sehr war sie nicht sch\u00f6n!", "tokens": ["Sie", "war", "\u2013", "\u2013", "wie", "sehr", "war", "sie", "nicht", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "$(", "KOKOM", "ADV", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie stand wie eine der G\u00f6ttinnen da,", "tokens": ["Sie", "stand", "wie", "ei\u00b7ne", "der", "G\u00f6t\u00b7tin\u00b7nen", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Paris auf dem Ida sah.", "tokens": ["Die", "Pa\u00b7ris", "auf", "dem", "I\u00b7da", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie eine Lilje im reinen Crystall,", "tokens": ["Wie", "ei\u00b7ne", "Lil\u00b7je", "im", "rei\u00b7nen", "Crys\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So gl\u00e4nzte Chloe \u00fcberall.", "tokens": ["So", "gl\u00e4nz\u00b7te", "Chloe", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich sah \u2013 \u2013 doch nein, geblendet zu sehr,", "tokens": ["Ich", "sah", "\u2013", "\u2013", "doch", "nein", ",", "ge\u00b7blen\u00b7det", "zu", "sehr", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "$(", "ADV", "PTKANT", "$,", "VVPP", "APPR", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sah ich ganz au\u00dfer mir nicht mehr.", "tokens": ["Sah", "ich", "ganz", "au\u00b7\u00dfer", "mir", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}