{"dta.poem.14433": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So sag' auch ich bei jedem Fr\u00fchlingsglanze:", "tokens": ["So", "sag'", "auch", "ich", "bei", "je\u00b7dem", "Fr\u00fch\u00b7lings\u00b7glan\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du s\u00fc\u00dfe Laute! la\u00df' uns beide sterben,", "tokens": ["Du", "s\u00fc\u00b7\u00dfe", "Lau\u00b7te", "!", "la\u00df'", "uns", "bei\u00b7de", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "VVIMP", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Beklagt vom Wiederhallen zarter T\u00f6ne,", "tokens": ["Be\u00b7klagt", "vom", "Wie\u00b7der\u00b7hal\u00b7len", "zar\u00b7ter", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}