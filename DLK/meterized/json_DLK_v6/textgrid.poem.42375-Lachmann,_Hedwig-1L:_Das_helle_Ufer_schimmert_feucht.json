{"textgrid.poem.42375": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das helle Ufer schimmert feucht", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das helle Ufer schimmert feucht", "tokens": ["Das", "hel\u00b7le", "U\u00b7fer", "schim\u00b7mert", "feucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Schaum der Welle, die entwich.", "tokens": ["Vom", "Schaum", "der", "Wel\u00b7le", ",", "die", "ent\u00b7wich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In silbern flirrendem Geleucht", "tokens": ["In", "sil\u00b7bern", "flir\u00b7ren\u00b7dem", "Ge\u00b7leucht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verliert sich fern sein letzter Strich.", "tokens": ["Ver\u00b7liert", "sich", "fern", "sein", "letz\u00b7ter", "Strich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Segelboote fliegen aus \u2013", "tokens": ["Die", "Se\u00b7gel\u00b7boo\u00b7te", "flie\u00b7gen", "aus", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Mitternacht, von Norden her", "tokens": ["Von", "Mit\u00b7ter\u00b7nacht", ",", "von", "Nor\u00b7den", "her"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kommt eine Woge hoch und kraus:", "tokens": ["Kommt", "ei\u00b7ne", "Wo\u00b7ge", "hoch", "und", "kraus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geliebtes Meer, geliebtes Meer!", "tokens": ["Ge\u00b7lieb\u00b7tes", "Meer", ",", "ge\u00b7lieb\u00b7tes", "Meer", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}