{"textgrid.poem.44529": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[dort mitten in dem Acker]", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort mitten in dem Acker,", "tokens": ["Dort", "mit\u00b7ten", "in", "dem", "A\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da liegt ein Diamant,", "tokens": ["Da", "liegt", "ein", "Di\u00b7a\u00b7mant", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den, neidisch wohl dem Erben,", "tokens": ["Den", ",", "nei\u00b7disch", "wohl", "dem", "Er\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Mann verscharrt beim Sterben.", "tokens": ["Ein", "Mann", "ver\u00b7scharrt", "beim", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich eil ihn zu erwerben! \u2013", "tokens": ["Ich", "eil", "ihn", "zu", "er\u00b7wer\u00b7ben", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wer h\u00e4tte das gemeint?", "tokens": ["Wer", "h\u00e4t\u00b7te", "das", "ge\u00b7meint", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es ist ein alter Scherben,", "tokens": ["Es", "ist", "ein", "al\u00b7ter", "Scher\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den eben die Sonne bescheint.", "tokens": ["Den", "e\u00b7ben", "die", "Son\u00b7ne", "be\u00b7scheint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}