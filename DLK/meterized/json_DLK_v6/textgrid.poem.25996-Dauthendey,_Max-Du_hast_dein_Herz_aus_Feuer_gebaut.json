{"textgrid.poem.25996": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Du hast dein Herz aus Feuer gebaut", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von einem Thron purpurn und alt", "tokens": ["Von", "ei\u00b7nem", "Thron", "pur\u00b7purn", "und", "alt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Regiert sie mich, deine junge Gestalt;", "tokens": ["Re\u00b7giert", "sie", "mich", ",", "dei\u00b7ne", "jun\u00b7ge", "Ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die holde Geb\u00e4rde und Augen hold", "tokens": ["Die", "hol\u00b7de", "Ge\u00b7b\u00e4r\u00b7de", "und", "Au\u00b7gen", "hold"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Fassen meine grobe Erde in Gold.", "tokens": ["Fas\u00b7sen", "mei\u00b7ne", "gro\u00b7be", "Er\u00b7de", "in", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Du hast dein Herz aus Feuer gebaut und tief,", "tokens": ["Du", "hast", "dein", "Herz", "aus", "Feu\u00b7er", "ge\u00b7baut", "und", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gl\u00fchend schlief ich dort, wo noch keiner schlief.", "tokens": ["Gl\u00fc\u00b7hend", "schlief", "ich", "dort", ",", "wo", "noch", "kei\u00b7ner", "schlief", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ADV", "PIS", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Zierliche Dinger sind deine H\u00e4nde, doch ihre Gaben wiegen schwer,", "tokens": ["Zier\u00b7li\u00b7che", "Din\u00b7ger", "sind", "dei\u00b7ne", "H\u00e4n\u00b7de", ",", "doch", "ih\u00b7re", "Ga\u00b7ben", "wie\u00b7gen", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "+--+--+-+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Mein Leben und mein Tod liegen bei dir.", "tokens": ["Mein", "Le\u00b7ben", "und", "mein", "Tod", "lie\u00b7gen", "bei", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+---++--+", "measure": "iambic.tetra.chol"}}}}}