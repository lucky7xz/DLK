{"dta.poem.7282": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Luft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.85", "af:0.14"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Wo die Felsenlager stehen,", "tokens": ["Wo", "die", "Fel\u00b7sen\u00b7la\u00b7ger", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich des Schnees Daunen bl\u00e4hen,", "tokens": ["Sich", "des", "Schnees", "Dau\u00b7nen", "bl\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf des Chimborasso H\u00f6hen", "tokens": ["Auf", "des", "Chim\u00b7bo\u00b7ras\u00b7so", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der junge Stral erwacht;", "tokens": ["Ist", "der", "jun\u00b7ge", "Stral", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Regt und dehnt die ros'gen Glieder,", "tokens": ["Regt", "und", "dehnt", "die", "ros'\u00b7gen", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sch\u00fcttelt dann sein Goldgefieder,", "tokens": ["Sch\u00fct\u00b7telt", "dann", "sein", "Gold\u00b7ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit dem Flimmerauge nieder", "tokens": ["Mit", "dem", "Flim\u00b7mer\u00b7au\u00b7ge", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Blinzt er in des Thales Schacht.", "tokens": ["Blinzt", "er", "in", "des", "Tha\u00b7les", "Schacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Dringt zu dir im weichen Duft", "tokens": ["Dringt", "zu", "dir", "im", "wei\u00b7chen", "Duft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Nicht der Himmelsodem \u2014 Luft?", "tokens": ["Nicht", "der", "Him\u00b7mel\u00b7so\u00b7dem", "Luft", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$(", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In's frische Land der J\u00e4ger tritt:", "tokens": ["In's", "fri\u00b7sche", "Land", "der", "J\u00e4\u00b7ger", "tritt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201egegr\u00fc\u00dft du fr\u00f6hlicher Morgen!", "tokens": ["\u201e", "ge\u00b7gr\u00fc\u00dft", "du", "fr\u00f6h\u00b7li\u00b7cher", "Mor\u00b7gen", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gegr\u00fc\u00dft du Sonn', mit dem leichten Schritt", "tokens": ["Ge\u00b7gr\u00fc\u00dft", "du", "Sonn'", ",", "mit", "dem", "leich\u00b7ten", "Schritt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir Beiden ziehn ohne Sorgen.", "tokens": ["Wir", "Bei\u00b7den", "ziehn", "oh\u00b7ne", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVINF", "APPR", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Und drei Mal gegr\u00fc\u00dft mein Geselle Wind,", "tokens": ["Und", "drei", "Mal", "ge\u00b7gr\u00fc\u00dft", "mein", "Ge\u00b7sel\u00b7le", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVPP", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Der stets mir wandelt zur Seite,", "tokens": ["Der", "stets", "mir", "wan\u00b7delt", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Im Walde fl\u00fcstert durch Bl\u00e4tter lind,", "tokens": ["Im", "Wal\u00b7de", "fl\u00fcs\u00b7tert", "durch", "Bl\u00e4t\u00b7ter", "lind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Zur H\u00f6h' gibt springend Geleite.", "tokens": ["Zur", "H\u00f6h'", "gibt", "sprin\u00b7gend", "Ge\u00b7lei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Und hat die Gems, das listige Thier,", "tokens": ["Und", "hat", "die", "Gems", ",", "das", "lis\u00b7ti\u00b7ge", "Thier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Mich verlockt in ihr zackiges Felsrevier,", "tokens": ["Mich", "ver\u00b7lockt", "in", "ihr", "za\u00b7cki\u00b7ges", "Fels\u00b7re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Wie sind wir Drei dann so ganz allein,", "tokens": ["Wie", "sind", "wir", "Drei", "dann", "so", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "CARD", "ADV", "ADV", "ADV", "ADV", "$,"], "meter": "------+-+", "measure": "unknown.measure.di"}, "line.12": {"text": "Du, Luft, und ich, und der uralte Stein!", "tokens": ["Du", ",", "Luft", ",", "und", "ich", ",", "und", "der", "ur\u00b7al\u00b7te", "Stein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "KON", "PPER", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}