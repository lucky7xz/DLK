{"textgrid.poem.36099": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "12. Zuschrift", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr, o geehrtesten Patronen,", "tokens": ["Ihr", ",", "o", "ge\u00b7ehr\u00b7tes\u00b7ten", "Pat\u00b7ro\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Meiner Musen Kron' vnd Zier,", "tokens": ["Mei\u00b7ner", "Mu\u00b7sen", "Kron'", "vnd", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob ich zwar euch abzulohnen", "tokens": ["Ob", "ich", "zwar", "euch", "ab\u00b7zu\u00b7loh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir mit dem nicht nehme f\u00fcr, \u2013", "tokens": ["Mir", "mit", "dem", "nicht", "neh\u00b7me", "f\u00fcr", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "PRELS", "PTKNEG", "VVFIN", "APPR", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn wie hoch ich euch versessen,", "tokens": ["Denn", "wie", "hoch", "ich", "euch", "ver\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Werd ich nicht mit dem ermessen, \u2013", "tokens": ["Werd", "ich", "nicht", "mit", "dem", "er\u00b7mes\u00b7sen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPR", "ART", "VVINF", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch so nehmet dieses an,", "tokens": ["Doch", "so", "neh\u00b7met", "die\u00b7ses", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PDS", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Was ich euch hier geben kan.", "tokens": ["Was", "ich", "euch", "hier", "ge\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bring ich gleich nicht hohe Sachen,", "tokens": ["Bring", "ich", "gleich", "nicht", "ho\u00b7he", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die den Wolken gleiche gehn,", "tokens": ["Die", "den", "Wol\u00b7ken", "glei\u00b7che", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ber\u00fchmbt und gro\u00df mich machen,", "tokens": ["Die", "be\u00b7r\u00fchmbt", "und", "gro\u00df", "mich", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "ADJD", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dem Monden nahe stehn,", "tokens": ["Und", "dem", "Mon\u00b7den", "na\u00b7he", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die man m\u00fc\u00df' in Ledern binden", "tokens": ["Die", "man", "m\u00fc\u00df'", "in", "Le\u00b7dern", "bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VMFIN", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mit Purpurtuch umbwinden,", "tokens": ["Und", "mit", "Pur\u00b7pur\u00b7tuch", "um\u00b7bwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch so sols die Handschrift sein,", "tokens": ["Doch", "so", "sols", "die", "Hand\u00b7schrift", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df ich mich wil stellen ein.", "tokens": ["Da\u00df", "ich", "mich", "wil", "stel\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VMFIN", "VVINF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Diese wolt ihr ietzo nehmen", "tokens": ["Die\u00b7se", "wolt", "ihr", "iet\u00b7zo", "neh\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis was Be\u00dfers k\u00f6mmet ein,", "tokens": ["Bis", "was", "Be\u00b7\u00dfers", "k\u00f6m\u00b7met", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Euch nicht eures Schuldners sch\u00e4men,", "tokens": ["Euch", "nicht", "eu\u00b7res", "Schuld\u00b7ners", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sondern mir noch g\u00fcnstig sein,", "tokens": ["Son\u00b7dern", "mir", "noch", "g\u00fcns\u00b7tig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mich zu den Restanten schreiben,", "tokens": ["Mich", "zu", "den", "Res\u00b7tan\u00b7ten", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Die nichts wollen schuldig bleiben,", "tokens": ["Die", "nichts", "wol\u00b7len", "schul\u00b7dig", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ohn was das Credit getan,", "tokens": ["Ohn", "was", "das", "Cre\u00b7dit", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Das ich nicht erlegen kan.", "tokens": ["Das", "ich", "nicht", "er\u00b7le\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "La\u00dfet die\u00dfmal mich genie\u00dfen,", "tokens": ["La\u00b7\u00dfet", "die\u00df\u00b7mal", "mich", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gerne wollen dankbar sein.", "tokens": ["Ger\u00b7ne", "wol\u00b7len", "dank\u00b7bar", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kan ich f\u00f6rder euch beflie\u00dfen,", "tokens": ["Kan", "ich", "f\u00f6r\u00b7der", "euch", "be\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solches alles bringen ein:", "tokens": ["Sol\u00b7ches", "al\u00b7les", "brin\u00b7gen", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "VVINF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So sol meine Clio singen,", "tokens": ["So", "sol", "mei\u00b7ne", "Clio", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und euch bis zum Blawen bringen,", "tokens": ["Und", "euch", "bis", "zum", "Bla\u00b7wen", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da denn euer Dank sol stehn,", "tokens": ["Da", "denn", "eu\u00b7er", "Dank", "sol", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo die Pleiasstern' entstehn.", "tokens": ["Wo", "die", "Plei\u00b7ass\u00b7tern'", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}