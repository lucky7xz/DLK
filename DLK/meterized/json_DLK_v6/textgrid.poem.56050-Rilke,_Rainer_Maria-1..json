{"textgrid.poem.56050": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn er unter jene welche ", "tokens": ["Wenn", "er", "un\u00b7ter", "je\u00b7ne", "wel\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "trat: der Pl\u00f6tzliche, der ", "tokens": ["trat", ":", "der", "Pl\u00f6tz\u00b7li\u00b7che", ",", "der"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "$.", "ART", "NN", "$,", "PRELS"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "war ein Glanz wie von Gefahren", "tokens": ["war", "ein", "Glanz", "wie", "von", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "KOKOM", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "in dem ausgesparten Raum um ihn,", "tokens": ["in", "dem", "aus\u00b7ge\u00b7spar\u00b7ten", "Raum", "um", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "den er l\u00e4chelnd \u00fcberschritt, um einer", "tokens": ["den", "er", "l\u00e4\u00b7chelnd", "\u00fc\u00b7bersc\u00b7hritt", ",", "um", "ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,", "KOUI", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Herzogin den F\u00e4cher aufzuheben:", "tokens": ["Her\u00b7zo\u00b7gin", "den", "F\u00e4\u00b7cher", "auf\u00b7zu\u00b7he\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "diesen warmen F\u00e4cher, den er eben", "tokens": ["die\u00b7sen", "war\u00b7men", "F\u00e4\u00b7cher", ",", "den", "er", "e\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "wollte fallen sehen. Und wenn keiner", "tokens": ["woll\u00b7te", "fal\u00b7len", "se\u00b7hen", ".", "Und", "wenn", "kei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "VVINF", "$.", "KON", "KOUS", "PIS"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "mit ihm eintrat in die Fensternische", "tokens": ["mit", "ihm", "ein\u00b7trat", "in", "die", "Fens\u00b7ter\u00b7ni\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "(wo die Parke gleich ins Tr\u00e4umerische", "tokens": ["(", "wo", "die", "Par\u00b7ke", "gleich", "ins", "Tr\u00e4u\u00b7me\u00b7ri\u00b7sche"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "stiegen, wenn er nur nach ihnen wies),", "tokens": ["stie\u00b7gen", ",", "wenn", "er", "nur", "nach", "ih\u00b7nen", "wies", ")", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$(", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "ging er l\u00e4ssig an die Kartentische", "tokens": ["ging", "er", "l\u00e4s\u00b7sig", "an", "die", "Kar\u00b7ten\u00b7ti\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "und gewann. Und unterlie\u00df", "tokens": ["und", "ge\u00b7wann", ".", "Und", "un\u00b7ter\u00b7lie\u00df"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$.", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "nicht, die Blicke alle zu behalten,", "tokens": ["nicht", ",", "die", "Bli\u00b7cke", "al\u00b7le", "zu", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "ART", "NN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "die ihn zweifelnd oder z\u00e4rtlich trafen,", "tokens": ["die", "ihn", "zwei\u00b7felnd", "o\u00b7der", "z\u00e4rt\u00b7lich", "tra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und auch die in Spiegel fielen, galten.", "tokens": ["und", "auch", "die", "in", "Spie\u00b7gel", "fie\u00b7len", ",", "gal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ART", "APPR", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er beschlo\u00df, auch heute nicht zu schlafen", "tokens": ["Er", "be\u00b7schlo\u00df", ",", "auch", "heu\u00b7te", "nicht", "zu", "schla\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADV", "PTKNEG", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "wie die letzte lange Nacht, und bog", "tokens": ["wie", "die", "letz\u00b7te", "lan\u00b7ge", "Nacht", ",", "und", "bog"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "einen Blick mit seinem r\u00fccksichtslosen", "tokens": ["ei\u00b7nen", "Blick", "mit", "sei\u00b7nem", "r\u00fcck\u00b7sichts\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "welcher war: als h\u00e4tte er von Rosen", "tokens": ["wel\u00b7cher", "war", ":", "als", "h\u00e4t\u00b7te", "er", "von", "Ro\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAT", "VAFIN", "$.", "KOKOM", "VAFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kinder, die man irgendwo erzog.", "tokens": ["Kin\u00b7der", ",", "die", "man", "ir\u00b7gend\u00b7wo", "er\u00b7zog", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}