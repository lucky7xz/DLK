{"textgrid.poem.32870": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du rauschest, sanfter Bach, auch nicht f\u00fcr Freuden mehr?", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du rauschest, sanfter Bach, auch nicht f\u00fcr Freuden mehr?", "tokens": ["Du", "rau\u00b7schest", ",", "sanf\u00b7ter", "Bach", ",", "auch", "nicht", "f\u00fcr", "Freu\u00b7den", "mehr", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "ADV", "PTKNEG", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Bl\u00fcmchen spiegelt sich in dir:", "tokens": ["Kein", "Bl\u00fcm\u00b7chen", "spie\u00b7gelt", "sich", "in", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Schnee bedeckt, vom Eise schwer,", "tokens": ["Vom", "Schnee", "be\u00b7deckt", ",", "vom", "Ei\u00b7se", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gleichst du, ja gleichst du mir.", "tokens": ["Gleichst", "du", ",", "ja", "gleichst", "du", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du gleichest mir, so lang mir Chloe grausam ist:", "tokens": ["Du", "glei\u00b7chest", "mir", ",", "so", "lang", "mir", "Chloe", "grau\u00b7sam", "ist", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "ADJD", "PPER", "NE", "ADJD", "VAFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da gl\u00e4nzt mir keine Freude mehr,", "tokens": ["Da", "gl\u00e4nzt", "mir", "kei\u00b7ne", "Freu\u00b7de", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Herz, das alles Gl\u00fcck vergi\u00dft,", "tokens": ["Mein", "Herz", ",", "das", "al\u00b7les", "Gl\u00fcck", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Klopft kaum von Seufzern schwer.", "tokens": ["Klopft", "kaum", "von", "Seuf\u00b7zern", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch dich l\u00f6s't einst der Lenz zu neuen Freuden auf:", "tokens": ["Doch", "dich", "l\u00f6s't", "einst", "der", "Lenz", "zu", "neu\u00b7en", "Freu\u00b7den", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da singt um dich der V\u00f6gel Chor,", "tokens": ["Da", "singt", "um", "dich", "der", "V\u00f6\u00b7gel", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein murmelnder verliebter Lauf", "tokens": ["Dein", "mur\u00b7meln\u00b7der", "ver\u00b7lieb\u00b7ter", "Lauf"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lockt Blum und Laub hervor.", "tokens": ["Lockt", "Blum", "und", "Laub", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O da\u00df mir doch, wie dir, auch einst ein Lenz erschien,", "tokens": ["O", "da\u00df", "mir", "doch", ",", "wie", "dir", ",", "auch", "einst", "ein", "Lenz", "er\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ADV", "$,", "PWAV", "PPER", "$,", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erg\u00e4b sich einstens Chloe mir!", "tokens": ["Er\u00b7g\u00e4b", "sich", "eins\u00b7tens", "Chloe", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "NE", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da w\u00fcrden mir auch Freuden bl\u00fchn,", "tokens": ["Da", "w\u00fcr\u00b7den", "mir", "auch", "Freu\u00b7den", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein ewger Lenz in ihr!", "tokens": ["Ein", "ew\u00b7ger", "Lenz", "in", "ihr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}