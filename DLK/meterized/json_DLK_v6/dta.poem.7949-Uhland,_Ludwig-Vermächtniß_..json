{"dta.poem.7949": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Verm\u00e4chtni\u00df .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Ein S\u00e4nger in den frommen Rittertagen,", "tokens": ["Ein", "S\u00e4n\u00b7ger", "in", "den", "from\u00b7men", "Rit\u00b7ter\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein k\u00fchner Streiter in dem heil\u2019gen Lande,", "tokens": ["Ein", "k\u00fch\u00b7ner", "Strei\u00b7ter", "in", "dem", "heil'\u00b7gen", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durchbohrt von Pfeilen, lag er auf dem Sande,", "tokens": ["Durch\u00b7bohrt", "von", "Pfei\u00b7len", ",", "lag", "er", "auf", "dem", "San\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch konnt\u2019 er dies noch seinem Diener sagen:", "tokens": ["Doch", "konnt'", "er", "dies", "noch", "sei\u00b7nem", "Die\u00b7ner", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PDS", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u201everschleu\u00df mein Herz, wann es nun ausgeschlagen,", "tokens": ["\u201e", "ver\u00b7schleu\u00df", "mein", "Herz", ",", "wann", "es", "nun", "aus\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPOSAT", "NN", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In jener Urne, die vom Heimathstrande", "tokens": ["In", "je\u00b7ner", "Ur\u00b7ne", ",", "die", "vom", "Hei\u00b7math\u00b7stran\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich hergebracht mit manchem Liebespfande!", "tokens": ["Ich", "her\u00b7ge\u00b7bracht", "mit", "man\u00b7chem", "Lie\u00b7be\u00b7spfan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drin sollt du es zu meiner Herrin tragen!\u201c \u2014", "tokens": ["Drin", "sollt", "du", "es", "zu", "mei\u00b7ner", "Her\u00b7rin", "tra\u00b7gen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So ich, Geliebte! der nur dich gefeiert,", "tokens": ["So", "ich", ",", "Ge\u00b7lieb\u00b7te", "!", "der", "nur", "dich", "ge\u00b7fei\u00b7ert", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "NN", "$.", "ART", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verblute, fern von dir, in Liebesschmerzen,", "tokens": ["Ver\u00b7blu\u00b7te", ",", "fern", "von", "dir", ",", "in", "Lie\u00b7bes\u00b7schmer\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "PPER", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schon decket meine Wangen Todesbl\u00e4sse.", "tokens": ["Schon", "de\u00b7cket", "mei\u00b7ne", "Wan\u00b7gen", "To\u00b7des\u00b7bl\u00e4s\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wann deinen S\u00e4nger Grabesnacht umschleiert,", "tokens": ["Wann", "dei\u00b7nen", "S\u00e4n\u00b7ger", "Gra\u00b7bes\u00b7nacht", "um\u00b7schlei\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Empfange du das treuste aller Herzen", "tokens": ["Emp\u00b7fan\u00b7ge", "du", "das", "treus\u00b7te", "al\u00b7ler", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In des Sonettes goldenem Gef\u00e4sse!", "tokens": ["In", "des", "So\u00b7net\u00b7tes", "gol\u00b7de\u00b7nem", "Ge\u00b7f\u00e4s\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}