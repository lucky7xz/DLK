{"dta.poem.14495": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonntag .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Weit in das Land die Str\u00f6m' ihr Silber f\u00fchren,", "tokens": ["Weit", "in", "das", "Land", "die", "Str\u00f6m'", "ihr", "Sil\u00b7ber", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fern blau Gebirge duftig hingezogen,", "tokens": ["Fern", "blau", "Ge\u00b7bir\u00b7ge", "duf\u00b7tig", "hin\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Sonne scheint, die B\u00e4ume sanft sich r\u00fchren,", "tokens": ["Die", "Son\u00b7ne", "scheint", ",", "die", "B\u00e4u\u00b7me", "sanft", "sich", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Glockenklang kommt auf den linden Wogen;", "tokens": ["Und", "Glo\u00b7cken\u00b7klang", "kommt", "auf", "den", "lin\u00b7den", "Wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hoch in den L\u00fcften Lerchen jubiliren,", "tokens": ["Hoch", "in", "den", "L\u00fcf\u00b7ten", "Ler\u00b7chen", "ju\u00b7bi\u00b7li\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und, so weit klar sich w\u00f6lbt des Himmels Bogen,", "tokens": ["Und", ",", "so", "weit", "klar", "sich", "w\u00f6lbt", "des", "Him\u00b7mels", "Bo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ADJD", "ADJD", "PRF", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Von Arbeit ruht der Mensch rings in die Runde,", "tokens": ["Von", "Ar\u00b7beit", "ruht", "der", "Mensch", "rings", "in", "die", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Athmet zum Herren auf aus Herzensgrunde.", "tokens": ["Ath\u00b7met", "zum", "Her\u00b7ren", "auf", "aus", "Her\u00b7zens\u00b7grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "APPR", "APPR", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}