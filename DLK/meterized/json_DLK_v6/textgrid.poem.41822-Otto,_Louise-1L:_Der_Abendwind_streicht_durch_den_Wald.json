{"textgrid.poem.41822": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Abendwind streicht durch den Wald", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Abendwind streicht durch den Wald", "tokens": ["Der", "Ab\u00b7end\u00b7wind", "streicht", "durch", "den", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Und Blatt und Blatt sich s\u00e4uselnd gr\u00fc\u00dfen,", "tokens": ["Und", "Blatt", "und", "Blatt", "sich", "s\u00e4u\u00b7selnd", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Lied der Nachtigall erschallt", "tokens": ["Das", "Lied", "der", "Nach\u00b7ti\u00b7gall", "er\u00b7schallt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Liebest\u00f6nen, schmeichelnd s\u00fc\u00dfen.", "tokens": ["In", "Lie\u00b7be\u00b7st\u00f6\u00b7nen", ",", "schmei\u00b7chelnd", "s\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Noch spielt der letzte Rosenschein", "tokens": ["Noch", "spielt", "der", "letz\u00b7te", "Ro\u00b7sen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sonne um die h\u00f6chsten B\u00e4ume,", "tokens": ["Der", "Son\u00b7ne", "um", "die", "h\u00f6chs\u00b7ten", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch schl\u00e4ft schon manche Bl\u00fcte ein,", "tokens": ["Doch", "schl\u00e4ft", "schon", "man\u00b7che", "Bl\u00fc\u00b7te", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wiegt leis ihr Haupt als ob sie tr\u00e4ume.", "tokens": ["Wiegt", "leis", "ihr", "Haupt", "als", "ob", "sie", "tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "KOKOM", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Still wird die Luft, es sinkt der Thau", "tokens": ["Still", "wird", "die", "Luft", ",", "es", "sinkt", "der", "Thau"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00fcberzieht mit feuchtem Gl\u00e4nzen", "tokens": ["Und", "\u00fc\u00b7berz\u00b7ieht", "mit", "feuch\u00b7tem", "Gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die bl\u00fctenreiche, duftge An,", "tokens": ["Die", "bl\u00fc\u00b7ten\u00b7rei\u00b7che", ",", "duft\u00b7ge", "An", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als schm\u00fcck' er sie zu Elfent\u00e4nzen.", "tokens": ["Als", "schm\u00fcck", "er", "sie", "zu", "El\u00b7fen\u00b7t\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schon fliegt der erste Gl\u00fchwurm aus,", "tokens": ["Schon", "fliegt", "der", "ers\u00b7te", "Gl\u00fch\u00b7wurm", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nachtfalter schwirrend sich erheben \u2013", "tokens": ["Nacht\u00b7fal\u00b7ter", "schwir\u00b7rend", "sich", "er\u00b7he\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da tritt ", "tokens": ["Da", "tritt"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Zur Gartenth\u00fcr, mit leisem Beben.", "tokens": ["Zur", "Gar\u00b7tent\u00b7h\u00fcr", ",", "mit", "lei\u00b7sem", "Be\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Den Riegel innen schiebt sie fort,", "tokens": ["Den", "Rie\u00b7gel", "in\u00b7nen", "schiebt", "sie", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um hinter sich ihn leis zu schlie\u00dfen;", "tokens": ["Um", "hin\u00b7ter", "sich", "ihn", "leis", "zu", "schlie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "PRF", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Abendstern, der Unschuld Hort,", "tokens": ["Den", "A\u00b7bends\u00b7tern", ",", "der", "Un\u00b7schuld", "Hort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NE", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Hat ihr der erste Blick gewiesen.", "tokens": ["Hat", "ihr", "der", "ers\u00b7te", "Blick", "ge\u00b7wie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Seit Wochen hat sie hier geweilt", "tokens": ["Seit", "Wo\u00b7chen", "hat", "sie", "hier", "ge\u00b7weilt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und treu gepflegt die alte Muhme,", "tokens": ["Und", "treu", "ge\u00b7pflegt", "die", "al\u00b7te", "Muh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bis sie genas \u2013 nun einmal eilt", "tokens": ["Bis", "sie", "ge\u00b7nas", "\u2013", "nun", "ein\u00b7mal", "eilt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$(", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie zu des Waldes Heiligtume.", "tokens": ["Sie", "zu", "des", "Wal\u00b7des", "Hei\u00b7lig\u00b7tu\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "L\u00e4ngst hat sie sich dahin gesehnt,", "tokens": ["L\u00e4ngst", "hat", "sie", "sich", "da\u00b7hin", "ge\u00b7sehnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch machte ihr die Muhme bange,", "tokens": ["Doch", "mach\u00b7te", "ihr", "die", "Muh\u00b7me", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Raub und Mord im Walde w\u00e4hnt", "tokens": ["Die", "Raub", "und", "Mord", "im", "Wal\u00b7de", "w\u00e4hnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hinter Blumen selbst die Schlange.", "tokens": ["Und", "hin\u00b7ter", "Blu\u00b7men", "selbst", "die", "Schlan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie sollt' am Abend nie allein", "tokens": ["Sie", "sollt'", "am", "A\u00b7bend", "nie", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die einsam stillen Pfade gehen \u2013 \u2013", "tokens": ["Die", "ein\u00b7sam", "stil\u00b7len", "Pfa\u00b7de", "ge\u00b7hen", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVINF", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch w\u00fcrde Gott nur bei ihr sein,", "tokens": ["Doch", "w\u00fcr\u00b7de", "Gott", "nur", "bei", "ihr", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "ADV", "APPR", "PPOSAT", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was k\u00f6nnte ihr denn wohl geschehen?", "tokens": ["Was", "k\u00f6nn\u00b7te", "ihr", "denn", "wohl", "ge\u00b7sche\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Er hatte ja den Wald gebaut", "tokens": ["Er", "hat\u00b7te", "ja", "den", "Wald", "ge\u00b7baut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinen hehren Buchenhallen \u2013", "tokens": ["Mit", "sei\u00b7nen", "heh\u00b7ren", "Bu\u00b7chen\u00b7hal\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sollte denn, wer Gott vertraut,", "tokens": ["Wie", "soll\u00b7te", "denn", ",", "wer", "Gott", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "$,", "PWS", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht froh und sicher darin wallen??", "tokens": ["Nicht", "froh", "und", "si\u00b7cher", "da\u00b7rin", "wal\u00b7len", "??"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KON", "ADJD", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Die Muhme schlief, die Magd war da,", "tokens": ["Die", "Muh\u00b7me", "schlief", ",", "die", "Magd", "war", "da", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jung-Elschen konnte fort sich schleichen,", "tokens": ["Jung\u00b7El\u00b7schen", "konn\u00b7te", "fort", "sich", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PTKVZ", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und bald war sie dem Walde nah,", "tokens": ["Und", "bald", "war", "sie", "dem", "Wal\u00b7de", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er gr\u00fc\u00dfte sie mit heilgem Schweigen.", "tokens": ["Er", "gr\u00fc\u00df\u00b7te", "sie", "mit", "heil\u00b7gem", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ganz einsam war's \u2013 ein Wandersmann", "tokens": ["Ganz", "ein\u00b7sam", "wa\u00b7r's", "\u2013", "ein", "Wan\u00b7ders\u00b7mann"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "$(", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kam ihr entgegen nur geschritten,", "tokens": ["Kam", "ihr", "ent\u00b7ge\u00b7gen", "nur", "ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er hielt den Fu\u00df dicht vor ihr an,", "tokens": ["Er", "hielt", "den", "Fu\u00df", "dicht", "vor", "ihr", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um eine Gabe sie zu bitten.", "tokens": ["Um", "ei\u00b7ne", "Ga\u00b7be", "sie", "zu", "bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Sie gr\u00fc\u00dft ihn still und gab ihm mehr", "tokens": ["Sie", "gr\u00fc\u00dft", "ihn", "still", "und", "gab", "ihm", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als er gewohnt war zu erhalten \u2013", "tokens": ["Als", "er", "ge\u00b7wohnt", "war", "zu", "er\u00b7hal\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er sah sie an \u2013 und dankte sehr \u2013", "tokens": ["Er", "sah", "sie", "an", "\u2013", "und", "dank\u00b7te", "sehr", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$(", "KON", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sie sprach tr\u00f6stlich: \u00bbGott mags walten!\u00ab", "tokens": ["Und", "sie", "sprach", "tr\u00f6st\u00b7lich", ":", "\u00bb", "Gott", "mags", "wal\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$.", "$(", "NN", "VMFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Wie sch\u00f6n war's drinnen nun im Wald!", "tokens": ["Wie", "sch\u00f6n", "wa\u00b7r's", "drin\u00b7nen", "nun", "im", "Wald", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Mond begann heraufzusteigen \u2013", "tokens": ["Der", "Mond", "be\u00b7gann", "her\u00b7auf\u00b7zu\u00b7stei\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVIZU", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch pl\u00f6tzlich lauter Sang erschallt,", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "lau\u00b7ter", "Sang", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo vorher noch das tiefste Schweigen.", "tokens": ["Wo", "vor\u00b7her", "noch", "das", "tiefs\u00b7te", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "\u00bbei, guten Abend sch\u00f6nes Kind!", "tokens": ["\u00bb", "ei", ",", "gu\u00b7ten", "A\u00b7bend", "sch\u00f6\u00b7nes", "Kind", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So einsam hier im Mondenscheine? \u2013\u00ab", "tokens": ["So", "ein\u00b7sam", "hier", "im", "Mon\u00b7den\u00b7schei\u00b7ne", "?", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPRART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbder Abend ist so hold und lind \u2013", "tokens": ["\u00bb", "der", "A\u00b7bend", "ist", "so", "hold", "und", "lind", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bin im Walde gern alleine.\u00ab", "tokens": ["Ich", "bin", "im", "Wal\u00b7de", "gern", "al\u00b7lei\u00b7ne", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "\u00bbhei, abgeblitzt!\u00ab ein Andrer rief:", "tokens": ["\u00bb", "hei", ",", "ab\u00b7ge\u00b7blitzt", "!", "\u00ab", "ein", "A\u00b7ndrer", "rief", ":"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "VVPP", "$.", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdie weist uns selbst im Wald die Th\u00fcre!\u00ab", "tokens": ["\u00bb", "die", "weist", "uns", "selbst", "im", "Wald", "die", "Th\u00fc\u00b7re", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Scherzwort durch die Burschen lief \u2013", "tokens": ["Ein", "Scherz\u00b7wort", "durch", "die", "Bur\u00b7schen", "lief", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbwohl \u2013 jeder wird, was ihr geb\u00fchre.\u00ab", "tokens": ["\u00bb", "wohl", "\u2013", "je\u00b7der", "wird", ",", "was", "ihr", "ge\u00b7b\u00fch\u00b7re", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$(", "PIS", "VAFIN", "$,", "PWS", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Sie zogen fort, und l\u00e4chelnd stand", "tokens": ["Sie", "zo\u00b7gen", "fort", ",", "und", "l\u00e4\u00b7chelnd", "stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jung-Elschen da im Mondenlichte \u2013", "tokens": ["Jung\u00b7El\u00b7schen", "da", "im", "Mon\u00b7den\u00b7lich\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Strich sinnend mit der wei\u00dfen Hand", "tokens": ["Strich", "sin\u00b7nend", "mit", "der", "wei\u00b7\u00dfen", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Goldhaar aus dem Angesichte. \u2013", "tokens": ["Ihr", "Gold\u00b7haar", "aus", "dem", "An\u00b7ge\u00b7sich\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Und wieder herrschte tiefe Ruh", "tokens": ["Und", "wie\u00b7der", "herrschte", "tie\u00b7fe", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vom Teppichmoos bis zu den Sternen \u2013", "tokens": ["Vom", "Tep\u00b7pich\u00b7moos", "bis", "zu", "den", "Ster\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbo. Nacht, wie doppelt sch\u00f6n bist du,", "tokens": ["\u00bb", "o.", "Nacht", ",", "wie", "dop\u00b7pelt", "sch\u00f6n", "bist", "du", ","], "token_info": ["punct", "abbreviation", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "PWAV", "ADJD", "ADJD", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn wir im Wald dich kennen lernen!\u00ab", "tokens": ["Wenn", "wir", "im", "Wald", "dich", "ken\u00b7nen", "ler\u00b7nen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "VVINF", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Andacht durch ihre Seele zieht,", "tokens": ["An\u00b7dacht", "durch", "ih\u00b7re", "See\u00b7le", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die H\u00e4nde faltet sie zusammen,", "tokens": ["Die", "H\u00e4n\u00b7de", "fal\u00b7tet", "sie", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und betend fromm sie niederkniet,", "tokens": ["Und", "be\u00b7tend", "fromm", "sie", "nie\u00b7der\u00b7kniet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im blauen Aug' Begeistrungsflammen.", "tokens": ["Im", "blau\u00b7en", "Aug'", "Be\u00b7geis\u00b7trungs\u00b7flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Und pl\u00f6tzlich schreckt ein Fluch sie auf \u2013", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "schreckt", "ein", "Fluch", "sie", "auf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PPER", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sieht in rauhen Mannes H\u00e4nden", "tokens": ["Sie", "sieht", "in", "rau\u00b7hen", "Man\u00b7nes", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des blitzenden Gewehres Lauf", "tokens": ["Des", "blit\u00b7zen\u00b7den", "Ge\u00b7weh\u00b7res", "Lauf"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ruft erschreckt: \u00bbDas m\u00fc\u00dft Ihr wenden!\u00ab", "tokens": ["Und", "ruft", "er\u00b7schreckt", ":", "\u00bb", "Das", "m\u00fc\u00dft", "Ihr", "wen\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "VVPP", "$.", "$(", "PDS", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "\u00bbich bin kein Dieb, bin auch kein Reh,", "tokens": ["\u00bb", "ich", "bin", "kein", "Dieb", ",", "bin", "auch", "kein", "Reh", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PIAT", "NN", "$,", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Daf\u00fcr Ihr mich vielleicht gehalten \u2013", "tokens": ["Da\u00b7f\u00fcr", "Ihr", "mich", "viel\u00b7leicht", "ge\u00b7hal\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein M\u00e4dchen nur ich vor Euch steh,", "tokens": ["Ein", "M\u00e4d\u00b7chen", "nur", "ich", "vor", "Euch", "steh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dess' H\u00e4nde betend sich gefalten!\u00ab \u2013", "tokens": ["Dess'", "H\u00e4n\u00b7de", "be\u00b7tend", "sich", "ge\u00b7fal\u00b7ten", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVPP", "PRF", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.21": {"line.1": {"text": "Da sch\u00fcttelt es den b\u00f6sen Mann,", "tokens": ["Da", "sch\u00fct\u00b7telt", "es", "den", "b\u00f6\u00b7sen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den b\u00f6se That zur Flucht getrieben,", "tokens": ["Den", "b\u00f6\u00b7se", "That", "zur", "Flucht", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er ruft: \u00bbF\u00fcr mich auch bete dann!\u00ab", "tokens": ["Er", "ruft", ":", "\u00bb", "F\u00fcr", "mich", "auch", "be\u00b7te", "dann", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "APPR", "PPER", "ADV", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kehrt sich um \u2013 \u00bbZu Gott, dem lieben!\u00ab", "tokens": ["Und", "kehrt", "sich", "um", "\u2013", "\u00bb", "Zu", "Gott", ",", "dem", "lie\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "$(", "$(", "APPR", "NN", "$,", "PRELS", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.22": {"line.1": {"text": "Ruft sie mit unschuldsvollem Laut", "tokens": ["Ruft", "sie", "mit", "un\u00b7schulds\u00b7vol\u00b7lem", "Laut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Fl\u00fccht'gen nach. Dann kehrt sie wieder", "tokens": ["Dem", "Fl\u00fccht'\u00b7gen", "nach", ".", "Dann", "kehrt", "sie", "wie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zum stillen Haus, zum Garten traut,", "tokens": ["Zum", "stil\u00b7len", "Haus", ",", "zum", "Gar\u00b7ten", "traut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo s\u00fc\u00df noch duften Ros' und Flieder.", "tokens": ["Wo", "s\u00fc\u00df", "noch", "duf\u00b7ten", "Ros'", "und", "Flie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.23": {"line.1": {"text": "Am Morgen sie zur Muhme spricht:", "tokens": ["Am", "Mor\u00b7gen", "sie", "zur", "Muh\u00b7me", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbich will es ehrlich dir gestehen,", "tokens": ["\u00bb", "ich", "will", "es", "ehr\u00b7lich", "dir", "ge\u00b7ste\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich hab' im stillen Mondenlicht", "tokens": ["Ich", "hab'", "im", "stil\u00b7len", "Mon\u00b7den\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Wald mich gestern umgesehen.\u00ab", "tokens": ["Im", "Wald", "mich", "ge\u00b7stern", "um\u00b7ge\u00b7se\u00b7hen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.24": {"line.1": {"text": "Die Muhme