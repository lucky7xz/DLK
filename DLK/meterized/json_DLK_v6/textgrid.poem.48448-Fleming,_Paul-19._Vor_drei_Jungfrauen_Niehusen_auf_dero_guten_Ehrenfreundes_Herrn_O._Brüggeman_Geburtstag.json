{"textgrid.poem.48448": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "19. Vor drei Jungfrauen Niehusen auf dero guten Ehrenfreundes Herrn O. Br\u00fcggeman Geburtstag", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist denn schon nicht mehr zu finden,", "tokens": ["Ist", "denn", "schon", "nicht", "mehr", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKNEG", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "darmit wir euch k\u00f6nnen binden,", "tokens": ["dar\u00b7mit", "wir", "euch", "k\u00f6n\u00b7nen", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "als di\u00df eingelegte Band,", "tokens": ["als", "di\u00df", "ein\u00b7ge\u00b7leg\u00b7te", "Band", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "das von dreier Schwestern Hand", "tokens": ["das", "von", "drei\u00b7er", "Schwes\u00b7tern", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "CARD", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "in einander ist gewunden,", "tokens": ["in", "ein\u00b7an\u00b7der", "ist", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "so ist doch das Herze gut,", "tokens": ["so", "ist", "doch", "das", "Her\u00b7ze", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PDS", "VVFIN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "das euch diese W\u00fcrde tut.", "tokens": ["das", "euch", "die\u00b7se", "W\u00fcr\u00b7de", "tut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Seht die angenehmen Stunden,", "tokens": ["Seht", "die", "an\u00b7ge\u00b7neh\u00b7men", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "diese Stunden, die uns allen", "tokens": ["die\u00b7se", "Stun\u00b7den", ",", "die", "uns", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "machen einen Wolgefallen,", "tokens": ["ma\u00b7chen", "ei\u00b7nen", "Wol\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "tausent- und noch tausentmal!", "tokens": ["tau\u00b7sent", "und", "noch", "tau\u00b7sent\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Der gestirnte Himmelssaal", "tokens": ["Der", "ge\u00b7stirn\u00b7te", "Him\u00b7mels\u00b7saal"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "will sein Jawort auch drein geben,", "tokens": ["will", "sein", "Ja\u00b7wort", "auch", "drein", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "da\u00df ihr unbetr\u00fcbt m\u00f6gt leben.", "tokens": ["da\u00df", "ihr", "un\u00b7be\u00b7tr\u00fcbt", "m\u00f6gt", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Wie denn auch die drei Geschwister,", "tokens": ["Wie", "denn", "auch", "die", "drei", "Ge\u00b7schwis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "welche halten das Register", "tokens": ["wel\u00b7che", "hal\u00b7ten", "das", "Re\u00b7gis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "\u00fcber Leben, \u00fcber Tod,", "tokens": ["\u00fc\u00b7ber", "Le\u00b7ben", ",", "\u00fc\u00b7ber", "Tod", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "die uns unser Gl\u00fccke spinnen,", "tokens": ["die", "uns", "un\u00b7ser", "Gl\u00fc\u00b7cke", "spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "g\u00f6nnen euch, was wir euch g\u00f6nnen,", "tokens": ["g\u00f6n\u00b7nen", "euch", ",", "was", "wir", "euch", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "was euch g\u00f6nnet selbsten Gott.", "tokens": ["was", "euch", "g\u00f6n\u00b7net", "selbs\u00b7ten", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Wie viel schlechter ist das Band,", "tokens": ["Wie", "viel", "schlech\u00b7ter", "ist", "das", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "so viel besser ist der Rat", "tokens": ["so", "viel", "bes\u00b7ser", "ist", "der", "Rat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "und der dreien Schwestern Hand,", "tokens": ["und", "der", "drei\u00b7en", "Schwes\u00b7tern", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "CARD", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "die euch das geschrieben hat.", "tokens": ["die", "euch", "das", "ge\u00b7schrie\u00b7ben", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDS", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}