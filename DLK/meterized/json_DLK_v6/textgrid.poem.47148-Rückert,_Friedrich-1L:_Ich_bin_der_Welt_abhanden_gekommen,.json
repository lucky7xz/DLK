{"textgrid.poem.47148": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin der Welt abhanden gekommen,", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin der Welt abhanden gekommen,", "tokens": ["Ich", "bin", "der", "Welt", "ab\u00b7han\u00b7den", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit der ich sonst viele Zeit verdorben.", "tokens": ["Mit", "der", "ich", "sonst", "vie\u00b7le", "Zeit", "ver\u00b7dor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sie hat so lange von mir nichts vernommen,", "tokens": ["Sie", "hat", "so", "lan\u00b7ge", "von", "mir", "nichts", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie mag wohl glauben, ich sei gestorben.", "tokens": ["Sie", "mag", "wohl", "glau\u00b7ben", ",", "ich", "sei", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Es ist mir auch gar nichts daran gelegen,", "tokens": ["Es", "ist", "mir", "auch", "gar", "nichts", "da\u00b7ran", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "PIS", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob sie mich f\u00fcr gestorben h\u00e4lt;", "tokens": ["Ob", "sie", "mich", "f\u00fcr", "ge\u00b7stor\u00b7ben", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich kann auch gar nichts sagen dagegen,", "tokens": ["Ich", "kann", "auch", "gar", "nichts", "sa\u00b7gen", "da\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PIS", "VVFIN", "PAV", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Denn wirklich bin ich gestorben der Welt.", "tokens": ["Denn", "wirk\u00b7lich", "bin", "ich", "ge\u00b7stor\u00b7ben", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "VVPP", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich bin gestorben dem Weltgewimmel", "tokens": ["Ich", "bin", "ge\u00b7stor\u00b7ben", "dem", "Welt\u00b7ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und ruh' in einem stillen Gebiet.", "tokens": ["Und", "ruh'", "in", "ei\u00b7nem", "stil\u00b7len", "Ge\u00b7biet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ich leb' in mir und meinem Himmel,", "tokens": ["Ich", "leb'", "in", "mir", "und", "mei\u00b7nem", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In meinen Lieben, in meinem Lied.", "tokens": ["In", "mei\u00b7nen", "Lie\u00b7ben", ",", "in", "mei\u00b7nem", "Lied", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}