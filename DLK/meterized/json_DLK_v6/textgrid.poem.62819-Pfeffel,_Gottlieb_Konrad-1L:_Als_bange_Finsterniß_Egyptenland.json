{"textgrid.poem.62819": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als bange Finsterni\u00df Egyptenland", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als bange Finsterni\u00df Egyptenland", "tokens": ["Als", "ban\u00b7ge", "Fins\u00b7ter\u00b7ni\u00df", "E\u00b7gyp\u00b7ten\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Drey Tage lang auf Moses Wink bedeckte,", "tokens": ["Drey", "Ta\u00b7ge", "lang", "auf", "Mo\u00b7ses", "Wink", "be\u00b7deck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gab Pharao Befehl, da\u00df man am j\u00e4hen Strand", "tokens": ["Gab", "Pha\u00b7rao", "Be\u00b7fehl", ",", "da\u00df", "man", "am", "j\u00e4\u00b7hen", "Strand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "NN", "$,", "KOUS", "PIS", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Des Nils, wo seine Hofburg stand,", "tokens": ["Des", "Nils", ",", "wo", "sei\u00b7ne", "Hof\u00b7burg", "stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf einen Obelisk ein gro\u00dfes Windlicht steckte.", "tokens": ["Auf", "ei\u00b7nen", "O\u00b7be\u00b7lisk", "ein", "gro\u00b7\u00dfes", "Wind\u00b7licht", "steck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kein B\u00fcrger war, wenn er die Stra\u00dfe zog", "tokens": ["Kein", "B\u00fcr\u00b7ger", "war", ",", "wenn", "er", "die", "Stra\u00b7\u00dfe", "zog"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und schauernd die Gefahr erwog,", "tokens": ["Und", "schau\u00b7ernd", "die", "Ge\u00b7fahr", "er\u00b7wog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der den Monarchen nicht gesegnet h\u00e4tte.", "tokens": ["Der", "den", "Mon\u00b7ar\u00b7chen", "nicht", "ge\u00b7seg\u00b7net", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der Fackelschein lockt aus der dicken Nacht", "tokens": ["Der", "Fa\u00b7ckel\u00b7schein", "lockt", "aus", "der", "di\u00b7cken", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Auch einen Narrn herbey, der sich von seiner Kette", "tokens": ["Auch", "ei\u00b7nen", "Narrn", "her\u00b7bey", ",", "der", "sich", "von", "sei\u00b7ner", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$,", "PRELS", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mit wilder St\u00e4rke losgemacht.", "tokens": ["Mit", "wil\u00b7der", "St\u00e4r\u00b7ke", "los\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Er gafft sie lachend an, klimmt auf die Pyramide", "tokens": ["Er", "gafft", "sie", "la\u00b7chend", "an", ",", "klimmt", "auf", "die", "Py\u00b7ra\u00b7mi\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und nimmt sie weg. Gleich einer Eumenide", "tokens": ["Und", "nimmt", "sie", "weg", ".", "Gleich", "ei\u00b7ner", "Eu\u00b7me\u00b7ni\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Schwingt er sie durch die Luft, und steckt mit rascher Hand", "tokens": ["Schwingt", "er", "sie", "durch", "die", "Luft", ",", "und", "steckt", "mit", "ra\u00b7scher", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das ganze Schlo\u00dfquartier in Brand.", "tokens": ["Das", "gan\u00b7ze", "Schlo\u00df\u00b7quar\u00b7tier", "in", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Der Flamme falber Blitz durchstreift die schwarzen Nebel", "tokens": ["Der", "Flam\u00b7me", "fal\u00b7ber", "Blitz", "durch\u00b7streift", "die", "schwar\u00b7zen", "Ne\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und f\u00fcllt die Stadt mit Angst und Graus.", "tokens": ["Und", "f\u00fcllt", "die", "Stadt", "mit", "Angst", "und", "Graus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Nur das noch brauchten wir, schrie der ergrimmte P\u00f6bel,", "tokens": ["Nur", "das", "noch", "brauch\u00b7ten", "wir", ",", "schrie", "der", "er\u00b7grimm\u00b7te", "P\u00f6\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Verdammte Fackel, l\u00f6scht sie aus!", "tokens": ["Ver\u00b7damm\u00b7te", "Fa\u00b7ckel", ",", "l\u00f6scht", "sie", "aus", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ihr haben wir die\u00df Ungl\u00fcck zuzuschreiben!", "tokens": ["Ihr", "ha\u00b7ben", "wir", "die\u00df", "Un\u00b7gl\u00fcck", "zu\u00b7zu\u00b7schrei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PDS", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Nein, rief ein weiser Greis, die Fackel ist nicht Schuld;", "tokens": ["Nein", ",", "rief", "ein", "wei\u00b7ser", "Greis", ",", "die", "Fa\u00b7ckel", "ist", "nicht", "Schuld", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "VAFIN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Euch schenkte sie des K\u00f6nigs Huld", "tokens": ["Euch", "schenk\u00b7te", "sie", "des", "K\u00f6\u00b7nigs", "Huld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Die Finsternisse zu vertreiben.", "tokens": ["Die", "Fins\u00b7ter\u00b7nis\u00b7se", "zu", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Wie manchen irren Fu\u00df hat sie", "tokens": ["Wie", "man\u00b7chen", "ir\u00b7ren", "Fu\u00df", "hat", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Dem Strom entwarnt! Ward sie von einem Tollen", "tokens": ["Dem", "Strom", "ent\u00b7warnt", "!", "Ward", "sie", "von", "ei\u00b7nem", "Tol\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$.", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Mi\u00dfbraucht, so bindet ihn, sie h\u00e4tte nie", "tokens": ["Mi\u00df\u00b7braucht", ",", "so", "bin\u00b7det", "ihn", ",", "sie", "h\u00e4t\u00b7te", "nie"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "In solche H\u00e4nde fallen sollen.", "tokens": ["In", "sol\u00b7che", "H\u00e4n\u00b7de", "fal\u00b7len", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Freund Gottes und mein Freund, der die Religion", "tokens": ["Freund", "Got\u00b7tes", "und", "mein", "Freund", ",", "der", "die", "Re\u00b7li\u00b7gi\u00b7on"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "KON", "PPOSAT", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und ihres Stifters Ehre r\u00e4chte,", "tokens": ["Und", "ih\u00b7res", "Stif\u00b7ters", "Eh\u00b7re", "r\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn doch der Sp\u00f6tter Zunft, wie dieser Alte d\u00e4chte!", "tokens": ["Wenn", "doch", "der", "Sp\u00f6t\u00b7ter", "Zunft", ",", "wie", "die\u00b7ser", "Al\u00b7te", "d\u00e4ch\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "$,", "PWAV", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie schreibt die Bluthochzeit, die Inquisition", "tokens": ["Sie", "schreibt", "die", "Blut\u00b7hoch\u00b7zeit", ",", "die", "In\u00b7qui\u00b7si\u00b7ti\u00b7on"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und ganze Myriaden Uebel,", "tokens": ["Und", "gan\u00b7ze", "My\u00b7ria\u00b7den", "Ue\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Geburten des Betrugs, der Tyranney,", "tokens": ["Ge\u00b7bur\u00b7ten", "des", "Be\u00b7trugs", ",", "der", "Ty\u00b7ran\u00b7ney", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Dummheit und der Schw\u00e4rmerey,", "tokens": ["Der", "Dumm\u00b7heit", "und", "der", "Schw\u00e4r\u00b7me\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dreist auf die Rechnung unsrer Bibel,", "tokens": ["Dreist", "auf", "die", "Rech\u00b7nung", "uns\u00b7rer", "Bi\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die lauter Weisheit lehrt und jede That verdammt,", "tokens": ["Die", "lau\u00b7ter", "Weis\u00b7heit", "lehrt", "und", "je\u00b7de", "That", "ver\u00b7dammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "KON", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die nicht aus Menschenliebe stammt.", "tokens": ["Die", "nicht", "aus", "Men\u00b7schen\u00b7lie\u00b7be", "stammt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}