{"textgrid.poem.54450": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "An eine Geschminckte", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist an Euch/ das Ihr Ewr eigen m\u00f6get nennen?", "tokens": ["Was", "ist", "an", "Euch", "/", "das", "Ihr", "Ewr", "ei\u00b7gen", "m\u00f6\u00b7get", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "$(", "ART", "PPOSAT", "NN", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Z\u00e4hne sind durch Kunst in leeren Mund gebracht;", "tokens": ["Die", "Z\u00e4h\u00b7ne", "sind", "durch", "Kunst", "in", "lee\u00b7ren", "Mund", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Euch hat der Schmincke dunst das Antlitz sch\u00f6n gemacht/", "tokens": ["Euch", "hat", "der", "Schmin\u00b7cke", "dunst", "das", "Ant\u00b7litz", "sch\u00f6n", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df Ihr tragt frembdes Haar/ kan leicht ein jeder kennen/", "tokens": ["Da\u00df", "Ihr", "tragt", "fremb\u00b7des", "Haar", "/", "kan", "leicht", "ein", "je\u00b7der", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJA", "NN", "$(", "VMFIN", "ADJD", "ART", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vnnd da\u00df Ewr Wangen von gezwungner R\u00f6te brennen/", "tokens": ["Vnnd", "da\u00df", "Ewr", "Wan\u00b7gen", "von", "ge\u00b7zwung\u00b7ner", "R\u00f6\u00b7te", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist allen offenbahr/ de\u00df Halses falsche Pracht/", "tokens": ["Ist", "al\u00b7len", "of\u00b7fen\u00b7bahr", "/", "de\u00df", "Hal\u00b7ses", "fal\u00b7sche", "Pracht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJD", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vnd die polirte Stirn wird billich au\u00dfgelacht,", "tokens": ["Vnd", "die", "po\u00b7lir\u00b7te", "Stirn", "wird", "bil\u00b7lich", "au\u00df\u00b7ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn man die salben sich schawt vmb die Runtzeln trennen.", "tokens": ["Wenn", "man", "die", "sal\u00b7ben", "sich", "schawt", "vmb", "die", "Runt\u00b7zeln", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "VVFIN", "PRF", "VVFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Wenn di\u00df von aussen ist/ was mag wol in Euch sein/", "tokens": ["Wenn", "di\u00df", "von", "aus\u00b7sen", "ist", "/", "was", "mag", "wol", "in", "Euch", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "APPR", "VVPP", "VAFIN", "$(", "PWS", "VMFIN", "ADV", "APPR", "PPER", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Al\u00df List vnd Tr\u00fcgerey/ Ich bild mir sicher ein/", "tokens": ["Al\u00df", "List", "vnd", "Tr\u00fc\u00b7ge\u00b7rey", "/", "Ich", "bild", "mir", "si\u00b7cher", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$(", "PPER", "VVFIN", "PPER", "PRF", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df vnter einem Haupt/ das sich so falsch gezieret/", "tokens": ["Da\u00df", "vn\u00b7ter", "ei\u00b7nem", "Haupt", "/", "das", "sich", "so", "falsch", "ge\u00b7zie\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$(", "PRELS", "PRF", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auch ein falsch Hertze steh/ voll schn\u00f6der heucheley.", "tokens": ["Auch", "ein", "falsch", "Hert\u00b7ze", "steh", "/", "voll", "schn\u00f6\u00b7der", "heu\u00b7che\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "VVFIN", "VVFIN", "$(", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sambt eim geschminckten Sin vnd Glei\u00dfnerey darbey/", "tokens": ["Sambt", "eim", "ge\u00b7schminck\u00b7ten", "Sin", "vnd", "Glei\u00df\u00b7ne\u00b7rey", "dar\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "KON", "NN", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Durch welche (wer Euch trawt) wird jammerlich verf\u00fchret.", "tokens": ["Durch", "wel\u00b7che", "(", "wer", "Euch", "trawt", ")", "wird", "jam\u00b7mer\u00b7lich", "ver\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "$(", "PWS", "PPER", "VVFIN", "$(", "VAFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}