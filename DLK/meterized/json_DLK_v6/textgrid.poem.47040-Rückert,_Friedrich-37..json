{"textgrid.poem.47040": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O die du mich in deine Fesseln zwangest,", "tokens": ["O", "die", "du", "mich", "in", "dei\u00b7ne", "Fes\u00b7seln", "zwan\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRELS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie w\u00fcrde mir der Zwang, den ich empfinde,", "tokens": ["Wie", "w\u00fcr\u00b7de", "mir", "der", "Zwang", ",", "den", "ich", "emp\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Lust sich wandeln, wenn du stets so linde", "tokens": ["In", "Lust", "sich", "wan\u00b7deln", ",", "wenn", "du", "stets", "so", "lin\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PRF", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Fesseln schl\u00e4ngest, wie du heut sie schlangest,", "tokens": ["Die", "Fes\u00b7seln", "schl\u00e4n\u00b7gest", ",", "wie", "du", "heut", "sie", "schlan\u00b7gest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da du mir fesselnd Hand und Arm umrangest", "tokens": ["Da", "du", "mir", "fes\u00b7selnd", "Hand", "und", "Arm", "um\u00b7ran\u00b7gest"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit diesem Kranz, dem letzten Sp\u00e4tlingskinde", "tokens": ["Mit", "die\u00b7sem", "Kranz", ",", "dem", "letz\u00b7ten", "Sp\u00e4t\u00b7lings\u00b7kin\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Sommerflur, und zu dem Angebinde", "tokens": ["Der", "Som\u00b7mer\u00b7flur", ",", "und", "zu", "dem", "An\u00b7ge\u00b7bin\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit s\u00fc\u00dfen Blicken redetest, nein, sangest.", "tokens": ["Mit", "s\u00fc\u00b7\u00dfen", "Bli\u00b7cken", "re\u00b7de\u00b7test", ",", "nein", ",", "san\u00b7gest", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "PTKANT", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Den Blick, die Rede und des Kranzes Nelken", "tokens": ["Den", "Blick", ",", "die", "Re\u00b7de", "und", "des", "Kran\u00b7zes", "Nel\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Will ich nach Hause tragen, und bewahren", "tokens": ["Will", "ich", "nach", "Hau\u00b7se", "tra\u00b7gen", ",", "und", "be\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$,", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Kranz im Schrank, den Blick, die Red' im Herzen.", "tokens": ["Den", "Kranz", "im", "Schrank", ",", "den", "Blick", ",", "die", "Red'", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wenn der sch\u00f6ne Kranz wird m\u00fcssen welken,", "tokens": ["Und", "wenn", "der", "sch\u00f6\u00b7ne", "Kranz", "wird", "m\u00fcs\u00b7sen", "wel\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VAFIN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So soll die Rede und der Blick nach Jahren", "tokens": ["So", "soll", "die", "Re\u00b7de", "und", "der", "Blick", "nach", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "KON", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir bl\u00fchn und gl\u00fchn noch wie zwei ew'ge Kerzen.", "tokens": ["Mir", "bl\u00fchn", "und", "gl\u00fchn", "noch", "wie", "zwei", "ew'\u00b7ge", "Ker\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "ADV", "KOKOM", "CARD", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}