{"dta.poem.10198": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "( Sphinx in Rosen. )", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Aus wei\u00dfem Stein geformt, im Junigarten,", "tokens": ["Aus", "wei\u00b7\u00dfem", "Stein", "ge\u00b7formt", ",", "im", "Ju\u00b7nig\u00b7ar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Liegt eine Sphinx, die greulichste der Katzen.", "tokens": ["Liegt", "ei\u00b7ne", "Sphinx", ",", "die", "greu\u00b7lichs\u00b7te", "der", "Kat\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es k\u00fcssen ihr die zierlichsten Standarten,", "tokens": ["Es", "k\u00fcs\u00b7sen", "ihr", "die", "zier\u00b7lichs\u00b7ten", "Stand\u00b7ar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Die Rosen, windgeschaukelt, leicht die Tatzen.", "tokens": ["Die", "Ro\u00b7sen", ",", "wind\u00b7ge\u00b7schau\u00b7kelt", ",", "leicht", "die", "Tat\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Untier schweigt, die Lippen offenbarten", "tokens": ["Das", "Un\u00b7tier", "schweigt", ",", "die", "Lip\u00b7pen", "of\u00b7fen\u00b7bar\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie schon zu Ramses Zeiten \u2014 leere Fratzen.", "tokens": ["Wie", "schon", "zu", "Ram\u00b7ses", "Zei\u00b7ten", "lee\u00b7re", "Frat\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NE", "NN", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und schweigt, und schweigt, und l\u00e4\u00dft auf Antwort warten \u2014", "tokens": ["Und", "schweigt", ",", "und", "schweigt", ",", "und", "l\u00e4\u00dft", "auf", "Ant\u00b7wort", "war\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Im stillen Garten schwatzen nur die Spatzen.", "tokens": ["Im", "stil\u00b7len", "Gar\u00b7ten", "schwat\u00b7zen", "nur", "die", "Spat\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}