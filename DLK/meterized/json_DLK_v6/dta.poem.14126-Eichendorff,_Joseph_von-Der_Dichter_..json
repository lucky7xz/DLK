{"dta.poem.14126": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Dichter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts auf Erden nenn' ich mein,", "tokens": ["Nichts", "auf", "Er\u00b7den", "nenn'", "ich", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "VVFIN", "PPER", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als die Lieder meiner Laute,", "tokens": ["Als", "die", "Lie\u00b7der", "mei\u00b7ner", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch nenn' den, der freud'ger schaute", "tokens": ["Doch", "nenn'", "den", ",", "der", "freu\u00b7d'\u00b7ger", "schau\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "In die sch\u00f6ne Welt hinein!", "tokens": ["In", "die", "sch\u00f6\u00b7ne", "Welt", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles Lebens tiefste Sch\u00f6ne", "tokens": ["Al\u00b7les", "Le\u00b7bens", "tiefs\u00b7te", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Thun geheimni\u00dfvoll ja T\u00f6ne", "tokens": ["Thun", "ge\u00b7heim\u00b7ni\u00df\u00b7voll", "ja", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur dem frommen S\u00e4nger kund\u201a", "tokens": ["Nur", "dem", "from\u00b7men", "S\u00e4n\u00b7ger", "kund", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Die Gott wunderbar gelegt", "tokens": ["Die", "Gott", "wun\u00b7der\u00b7bar", "ge\u00b7legt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "In des Dichters Herzensgrund.", "tokens": ["In", "des", "Dich\u00b7ters", "Her\u00b7zens\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wenn die Welt, so wild bewegt,", "tokens": ["Wenn", "die", "Welt", ",", "so", "wild", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Aengstlich schaut nach ihren Rettern:", "tokens": ["A\u00b7engst\u00b7lich", "schaut", "nach", "ih\u00b7ren", "Ret\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ueber aller Nebel Wogen", "tokens": ["Ue\u00b7ber", "al\u00b7ler", "Ne\u00b7bel", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "W\u00f6lbt Er k\u00fchn den Friedensbogen\u201a", "tokens": ["W\u00f6lbt", "Er", "k\u00fchn", "den", "Frie\u00b7dens\u00b7bo\u00b7gen", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und\u201a wie nach verzog'nen Wettern,", "tokens": ["Und", "\u201a", "wie", "nach", "ver\u00b7zo\u00b7g'\u00b7nen", "Wet\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.16": {"text": "Rauscht die Erde wieder mild;", "tokens": ["Rauscht", "die", "Er\u00b7de", "wie\u00b7der", "mild", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Alle Knospen Bl\u00fcten treiben;", "tokens": ["Al\u00b7le", "Knos\u00b7pen", "Bl\u00fc\u00b7ten", "trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Und der Fr\u00fchling ist sein Haus\u201a", "tokens": ["Und", "der", "Fr\u00fch\u00b7ling", "ist", "sein", "Haus", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.20": {"text": "O du lieblich Frauenbild!", "tokens": ["O", "du", "lieb\u00b7lich", "Frau\u00b7en\u00b7bild", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Willst Du bei dem S\u00e4nger bleiben? \u2014", "tokens": ["Willst", "Du", "bei", "dem", "S\u00e4n\u00b7ger", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Blumen bind't ein streng Geschick:", "tokens": ["Blu\u00b7men", "bind't", "ein", "streng", "Ge\u00b7schick", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Wenn die tausend Stimmen singen\u201a", "tokens": ["Wenn", "die", "tau\u00b7send", "Stim\u00b7men", "sin\u00b7gen", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "CARD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Alle Schmerzen, alles Gl\u00fcck", "tokens": ["Al\u00b7le", "Schmer\u00b7zen", ",", "al\u00b7les", "Gl\u00fcck"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Ewig lautlos zu verschweigen.", "tokens": ["E\u00b7wig", "laut\u00b7los", "zu", "ver\u00b7schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Doch bei k\u00fchlem Mondenblick", "tokens": ["Doch", "bei", "k\u00fch\u00b7lem", "Mon\u00b7den\u00b7blick"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Regt ihr stiller Geist die Schwingen\u201a", "tokens": ["Regt", "ihr", "stil\u00b7ler", "Geist", "die", "Schwin\u00b7gen", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "M\u00f6cht' dem duft'gen Kelch entsteigen.", "tokens": ["M\u00f6cht'", "dem", "duft'\u00b7gen", "Kelch", "ent\u00b7stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}