{"textgrid.poem.61920": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1. Lied des Rattenf\u00e4ngers", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kenn' eine Weise,", "tokens": ["Ich", "kenn'", "ei\u00b7ne", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und stimm' ich mein Rohr,", "tokens": ["Und", "stimm'", "ich", "mein", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da spitzen die M\u00e4use,", "tokens": ["Da", "spit\u00b7zen", "die", "M\u00e4u\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Ratten das Ohr;", "tokens": ["Die", "Rat\u00b7ten", "das", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sie kommen gesprungen,", "tokens": ["Sie", "kom\u00b7men", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Als ging' es zum Fest,", "tokens": ["Als", "ging'", "es", "zum", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Die alten, die jungen", "tokens": ["Die", "al\u00b7ten", ",", "die", "jun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Aus jeglichem Nest;", "tokens": ["Aus", "jeg\u00b7li\u00b7chem", "Nest", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Aus Ritzen und Pf\u00fctzen, aus Keller und Dach,", "tokens": ["Aus", "Rit\u00b7zen", "und", "Pf\u00fct\u00b7zen", ",", "aus", "Kel\u00b7ler", "und", "Dach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Da h\u00fcpft es und schl\u00fcpft es und wimmelt mir nach.", "tokens": ["Da", "h\u00fcpft", "es", "und", "schl\u00fcpft", "es", "und", "wim\u00b7melt", "mir", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Und greif' ich mit Schalle", "tokens": ["Und", "greif'", "ich", "mit", "Schal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Den Triller dazu,", "tokens": ["Den", "Tril\u00b7ler", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "So scharen sich alle", "tokens": ["So", "scha\u00b7ren", "sich", "al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PIAT"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gehorsam im Nu.", "tokens": ["Ge\u00b7hor\u00b7sam", "im", "Nu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sie l\u00fcpfen, vom Zauber", "tokens": ["Sie", "l\u00fcp\u00b7fen", ",", "vom", "Zau\u00b7ber"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der T\u00f6ne gepackt,", "tokens": ["Der", "T\u00f6\u00b7ne", "ge\u00b7packt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Die Schw\u00e4nzelein sauber", "tokens": ["Die", "Schw\u00e4n\u00b7zel\u00b7ein", "sau\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APPR"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und springen im Takt.", "tokens": ["Und", "sprin\u00b7gen", "im", "Takt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Sie springen und schwingen sich hinter mir drein", "tokens": ["Sie", "sprin\u00b7gen", "und", "schwin\u00b7gen", "sich", "hin\u00b7ter", "mir", "drein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Und munter hinunter zum strudelnden Rhein.", "tokens": ["Und", "mun\u00b7ter", "hin\u00b7un\u00b7ter", "zum", "stru\u00b7deln\u00b7den", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPRART", "ADJA", "NE", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Und blas' ich dann tiefer", "tokens": ["Und", "blas'", "ich", "dann", "tie\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Fuge zum Schlu\u00df,", "tokens": ["Die", "Fu\u00b7ge", "zum", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da rennt das Geziefer", "tokens": ["Da", "rennt", "das", "Ge\u00b7zie\u00b7fer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wie toll in den Flu\u00df;", "tokens": ["Wie", "toll", "in", "den", "Flu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Da rettet kein Schnaufen,", "tokens": ["Da", "ret\u00b7tet", "kein", "Schnau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Kein Zappeln sie mehr,", "tokens": ["Kein", "Zap\u00b7peln", "sie", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Sie m\u00fcssen ersaufen", "tokens": ["Sie", "m\u00fcs\u00b7sen", "er\u00b7sau\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Wie Pharaos Heer;", "tokens": ["Wie", "Pha\u00b7raos", "Heer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Die Welle verschlingt sie mit Saus und mit Braus,", "tokens": ["Die", "Wel\u00b7le", "ver\u00b7schlingt", "sie", "mit", "Saus", "und", "mit", "Braus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Dann schwing' ich den Hut, und das Elend ist aus.", "tokens": ["Dann", "schwing'", "ich", "den", "Hut", ",", "und", "das", "E\u00b7lend", "ist", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "ART", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}