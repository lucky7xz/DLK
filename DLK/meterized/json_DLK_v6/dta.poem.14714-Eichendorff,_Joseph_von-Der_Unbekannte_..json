{"dta.poem.14714": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Unbekannte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Vor seinem Hause, unter bl\u00fch'nden Lauben", "tokens": ["Vor", "sei\u00b7nem", "Hau\u00b7se", ",", "un\u00b7ter", "bl\u00fch'n\u00b7den", "Lau\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lud ihn ein Mann zum fr\u00f6hl'chen Rasten ein,", "tokens": ["Lud", "ihn", "ein", "Mann", "zum", "fr\u00f6hl'\u00b7chen", "Ras\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die junge Frau bracht' Wein und Brot und Trauben,", "tokens": ["Die", "jun\u00b7ge", "Frau", "bracht'", "Wein", "und", "Brot", "und", "Trau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Setzt dann, umspielt vom letzten Abendschein,", "tokens": ["Setzt", "dann", ",", "um\u00b7spielt", "vom", "letz\u00b7ten", "A\u00b7bend\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sich neben ihn und blickt halb scheu, halb lose,", "tokens": ["Sich", "ne\u00b7ben", "ihn", "und", "blickt", "halb", "scheu", ",", "halb", "lo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "KON", "VVFIN", "ADJD", "ADJD", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein lockigt Kn\u00e4blein l\u00e4chelnd auf dem Schoo\u00dfe.", "tokens": ["Ein", "lo\u00b7ckigt", "Kn\u00e4\u00b7blein", "l\u00e4\u00b7chelnd", "auf", "dem", "Schoo\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}