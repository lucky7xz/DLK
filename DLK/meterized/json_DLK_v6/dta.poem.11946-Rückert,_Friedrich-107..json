{"dta.poem.11946": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "107.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl w\u00fcrde sich ein Mann in seine Lage finden,", "tokens": ["Wohl", "w\u00fcr\u00b7de", "sich", "ein", "Mann", "in", "sei\u00b7ne", "La\u00b7ge", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn den Begriff von sich er nie sich lie\u00df' entschwinden.", "tokens": ["Wenn", "den", "Be\u00b7griff", "von", "sich", "er", "nie", "sich", "lie\u00df'", "ent\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PRF", "PPER", "ADV", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Darum zufrieden ist er nie mit seiner Lage,", "tokens": ["Da\u00b7rum", "zu\u00b7frie\u00b7den", "ist", "er", "nie", "mit", "sei\u00b7ner", "La\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil er sich anders f\u00fchlt an jedem andern Tage.", "tokens": ["Weil", "er", "sich", "an\u00b7ders", "f\u00fchlt", "an", "je\u00b7dem", "an\u00b7dern", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "APPR", "PIAT", "ADJA", "N