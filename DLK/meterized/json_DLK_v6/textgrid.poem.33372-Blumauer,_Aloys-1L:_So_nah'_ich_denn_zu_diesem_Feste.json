{"textgrid.poem.33372": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "1L: So nah' ich denn zu diesem Feste", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So nah' ich denn zu diesem Feste", "tokens": ["So", "nah'", "ich", "denn", "zu", "die\u00b7sem", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Recht herzlich heute auch heran,", "tokens": ["Recht", "herz\u00b7lich", "heu\u00b7te", "auch", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00fcnsche, w\u00fcnsche alles Beste,", "tokens": ["Und", "w\u00fcn\u00b7sche", ",", "w\u00fcn\u00b7sche", "al\u00b7les", "Bes\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nun f\u00fcr die ganze Lebensbahn.", "tokens": ["Nun", "f\u00fcr", "die", "gan\u00b7ze", "Le\u00b7bens\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df sich in sch\u00f6nster Bl\u00fcth' entfalte,", "tokens": ["Da\u00df", "sich", "in", "sch\u00f6ns\u00b7ter", "Bl\u00fcth'", "ent\u00b7fal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Zephyr leise angefacht,", "tokens": ["Vom", "Ze\u00b7phyr", "lei\u00b7se", "an\u00b7ge\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Lebens-Mai, und sich gestalte", "tokens": ["Dein", "Le\u00b7bens\u00b7Mai", ",", "und", "sich", "ge\u00b7stal\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KON", "PRF", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im steten Glanz, in steter Pracht.", "tokens": ["Im", "ste\u00b7ten", "Glanz", ",", "in", "ste\u00b7ter", "Pracht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df ruhig auf dem Lebens-Meere", "tokens": ["Da\u00df", "ru\u00b7hig", "auf", "dem", "Le\u00b7bens\u00b7Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sanft gleite hin dein Lebenskahn,", "tokens": ["Sanft", "glei\u00b7te", "hin", "dein", "Le\u00b7bens\u00b7kahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Sturm sich rege, und ihn st\u00f6re", "tokens": ["Kein", "Sturm", "sich", "re\u00b7ge", ",", "und", "ihn", "st\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "PRF", "ADJA", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf seiner stillen, sch\u00f6nen Bahn.", "tokens": ["Auf", "sei\u00b7ner", "stil\u00b7len", ",", "sch\u00f6\u00b7nen", "Bahn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}