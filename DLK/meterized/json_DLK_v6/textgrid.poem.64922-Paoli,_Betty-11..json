{"textgrid.poem.64922": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wisse du, der grollt voll b\u00f6ser Ungeduld:", "tokens": ["O", "wis\u00b7se", "du", ",", "der", "grollt", "voll", "b\u00f6\u00b7ser", "Un\u00b7ge\u00b7duld", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PRELS", "ADJD", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Schicksal zahlt getreu Jedwedem seine Schuld.", "tokens": ["Das", "Schick\u00b7sal", "zahlt", "ge\u00b7treu", "Jed\u00b7we\u00b7dem", "sei\u00b7ne", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch dir gen\u00fcgt es nicht, da\u00df Zahlung du empfangest,", "tokens": ["Doch", "dir", "ge\u00b7n\u00fcgt", "es", "nicht", ",", "da\u00df", "Zah\u00b7lung", "du", "emp\u00b7fan\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist's in der M\u00fcnze nicht, die du nun just verlangest.", "tokens": ["Ist's", "in", "der", "M\u00fcn\u00b7ze", "nicht", ",", "die", "du", "nun", "just", "ver\u00b7lan\u00b7gest", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKNEG", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Viel kl\u00fcger w\u00e4r' es wohl, du n\u00e4hmest frischweg an", "tokens": ["Viel", "kl\u00fc\u00b7ger", "w\u00e4r'", "es", "wohl", ",", "du", "n\u00e4h\u00b7mest", "frischweg", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "ADV", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Die M\u00fcnzensort', in der es dich bezahlen ", "tokens": ["Die", "M\u00fcn\u00b7zen\u00b7sort'", ",", "in", "der", "es", "dich", "be\u00b7zah\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und w\u00e4r' es Scheidem\u00fcnz', nur immer angenommen;", "tokens": ["Und", "w\u00e4r'", "es", "Schei\u00b7de\u00b7m\u00fcnz'", ",", "nur", "im\u00b7mer", "an\u00b7ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "$,", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es l\u00e4\u00dft sich auch damit wohl durch das Leben kommen.", "tokens": ["Es", "l\u00e4\u00dft", "sich", "auch", "da\u00b7mit", "wohl", "durch", "das", "Le\u00b7ben", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "PAV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}