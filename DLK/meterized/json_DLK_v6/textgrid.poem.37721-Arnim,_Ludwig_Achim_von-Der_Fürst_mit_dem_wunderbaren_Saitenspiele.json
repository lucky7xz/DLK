{"textgrid.poem.37721": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der F\u00fcrst mit dem wunderbaren Saitenspiele", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der F\u00fcrst ging in den Garten", "tokens": ["Der", "F\u00fcrst", "ging", "in", "den", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit seinem Saitenspiel,", "tokens": ["Mit", "sei\u00b7nem", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und aller Augen warten,", "tokens": ["Und", "al\u00b7ler", "Au\u00b7gen", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der sch\u00f6nen Blumen viel;", "tokens": ["Der", "sch\u00f6\u00b7nen", "Blu\u00b7men", "viel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er ging vor sich spazieren", "tokens": ["Er", "ging", "vor", "sich", "spa\u00b7zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und sang bald dies, bald das,", "tokens": ["Und", "sang", "bald", "dies", ",", "bald", "das", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PDS", "$,", "ADV", "PDS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und konnt' sie alle r\u00fchren,", "tokens": ["Und", "konnt'", "sie", "al\u00b7le", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Augen wurden na\u00df.", "tokens": ["Die", "Au\u00b7gen", "wur\u00b7den", "na\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbdas Nest ist ausgenommen.", "tokens": ["\u00bb", "das", "Nest", "ist", "aus\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Adler mu\u00dfte fliehn,", "tokens": ["Der", "Ad\u00b7ler", "mu\u00df\u00b7te", "fliehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So weit ist es gekommen,", "tokens": ["So", "weit", "ist", "es", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ich mu\u00df weiter ziehn.\u00ab", "tokens": ["Und", "ich", "mu\u00df", "wei\u00b7ter", "ziehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die B\u00e4ume lie\u00dfen fallen", "tokens": ["Die", "B\u00e4u\u00b7me", "lie\u00b7\u00dfen", "fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der s\u00fc\u00dfen Fr\u00fcchte viel,", "tokens": ["Der", "s\u00fc\u00b7\u00dfen", "Fr\u00fcch\u00b7te", "viel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es mu\u00dfte wohl gefallen", "tokens": ["Es", "mu\u00df\u00b7te", "wohl", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das edle Saitenspiel.", "tokens": ["Das", "ed\u00b7le", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbkein Gl\u00fcck, das ewig w\u00e4hret,", "tokens": ["\u00bb", "kein", "Gl\u00fcck", ",", "das", "e\u00b7wig", "w\u00e4h\u00b7ret", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Thau am Mittag liegt,", "tokens": ["Kein", "Thau", "am", "Mit\u00b7tag", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Becher wird geleeret,", "tokens": ["Der", "Be\u00b7cher", "wird", "ge\u00b7lee\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der hellste Ton verfliegt.\u00ab", "tokens": ["Der", "hells\u00b7te", "Ton", "ver\u00b7fliegt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Rings sammeln sich so viele", "tokens": ["Rings", "sam\u00b7meln", "sich", "so", "vie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ADV", "PIAT"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Der V\u00f6gel allgemach,", "tokens": ["Der", "V\u00f6\u00b7gel", "all\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Auf jedem Bl\u00e4tterstiele", "tokens": ["Auf", "je\u00b7dem", "Bl\u00e4t\u00b7ter\u00b7stie\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sa\u00df einer, sang ihm nach.", "tokens": ["Sa\u00df", "ei\u00b7ner", ",", "sang", "ihm", "nach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbihr V\u00f6gel k\u00f6nnt wohl spielen,", "tokens": ["\u00bb", "ihr", "V\u00f6\u00b7gel", "k\u00f6nnt", "wohl", "spie\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr habt kein Vaterland,", "tokens": ["Ihr", "habt", "kein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was w\u00fcrdet ihr nicht f\u00fchlen,", "tokens": ["Was", "w\u00fcr\u00b7det", "ihr", "nicht", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn euer Nest in Brand.\u00ab", "tokens": ["Wenn", "eu\u00b7er", "Nest", "in", "Brand", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Vogel kommt mit Springen", "tokens": ["Ein", "Vo\u00b7gel", "kommt", "mit", "Sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und springt ins Saitenspiel,", "tokens": ["Und", "springt", "ins", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er sinkt hinein mit Singen,", "tokens": ["Er", "sinkt", "hin\u00b7ein", "mit", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wer das kann, hat noch viel.", "tokens": ["Wer", "das", "kann", ",", "hat", "noch", "viel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VMFIN", "$,", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbmein Vogel sitzt im Boden,", "tokens": ["\u00bb", "mein", "Vo\u00b7gel", "sitzt", "im", "Bo\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des regen Saitenspiels,", "tokens": ["Des", "re\u00b7gen", "Sai\u00b7ten\u00b7spiels", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Es hebet sich dein Odem", "tokens": ["Es", "he\u00b7bet", "sich", "dein", "O\u00b7dem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Voll \u00e4hnlichen Gef\u00fchls.\u00ab", "tokens": ["Voll", "\u00e4hn\u00b7li\u00b7chen", "Ge\u00b7f\u00fchls", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er singt durch's Saitengitter:", "tokens": ["Er", "singt", "durch's", "Sai\u00b7ten\u00b7git\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u00bbich ruhe wie im Nest,", "tokens": ["\u00bb", "ich", "ru\u00b7he", "wie", "im", "Nest", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hoch \u00fcber ein Gewitter", "tokens": ["Hoch", "\u00fc\u00b7ber", "ein", "Ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Durchzittert alle \u00c4st'.\u00ab", "tokens": ["Durch\u00b7zit\u00b7tert", "al\u00b7le", "\u00c4st'", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbwo ist der Hirt der Heerde,", "tokens": ["\u00bb", "wo", "ist", "der", "Hirt", "der", "Heer\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Heerde irrt herum,", "tokens": ["Die", "Heer\u00b7de", "irrt", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jegliche Beschwerde", "tokens": ["Und", "jeg\u00b7li\u00b7che", "Be\u00b7schwer\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tr\u00fcg' sie doch gern darum.\u00ab", "tokens": ["Tr\u00fcg'", "sie", "doch", "gern", "da\u00b7rum", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PAV", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Die Heerde h\u00f6rt ihn singen,", "tokens": ["Die", "Heer\u00b7de", "h\u00f6rt", "ihn", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Glocken klingen viel,", "tokens": ["Der", "Glo\u00b7cken", "klin\u00b7gen", "viel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch all' zusammen klingen,", "tokens": ["Doch", "all'", "zu\u00b7sam\u00b7men", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Just wie das Saitenspiel.", "tokens": ["Just", "wie", "das", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbo leichte Kunst Regieren,", "tokens": ["\u00bb", "o", "leich\u00b7te", "Kunst", "Re\u00b7gie\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zieht andre Saiten auf,", "tokens": ["Zieht", "and\u00b7re", "Sai\u00b7ten", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Scepter mu\u00df sie f\u00fchren,", "tokens": ["Der", "Scep\u00b7ter", "mu\u00df", "sie", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Blies er als Pfeife drauf.\u00ab", "tokens": ["Blies", "er", "als", "Pfei\u00b7fe", "drauf", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die jungen L\u00e4mmer tanzen", "tokens": ["Die", "jun\u00b7gen", "L\u00e4m\u00b7mer", "tan\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nach seinem Pfeifen gleich,", "tokens": ["Nach", "sei\u00b7nem", "Pfei\u00b7fen", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So geht die Welt im Ganzen", "tokens": ["So", "geht", "die", "Welt", "im", "Gan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und auch im gr\u00f6\u00dften Reich.", "tokens": ["Und", "auch", "im", "gr\u00f6\u00df\u00b7ten", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der F\u00fcrst lie\u00df gern ihn singen,", "tokens": ["Der", "F\u00fcrst", "lie\u00df", "gern", "ihn", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Vogel nimmt mit fort,", "tokens": ["Den", "Vo\u00b7gel", "nimmt", "mit", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Herz wollt ihm zerspringen,", "tokens": ["Das", "Herz", "wollt", "ihm", "zer\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihn sah kein andrer dort.", "tokens": ["Ihn", "sah", "kein", "an\u00b7drer", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dort str\u00f6men seine Klagen", "tokens": ["Dort", "str\u00f6\u00b7men", "sei\u00b7ne", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gleich wie ein schneller Bach,", "tokens": ["Gleich", "wie", "ein", "schnel\u00b7ler", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die raschen Wellen jagen", "tokens": ["Die", "ra\u00b7schen", "Wel\u00b7len", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und ziehn das Gr\u00fcn sich nach.", "tokens": ["Und", "ziehn", "das", "Gr\u00fcn", "sich", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbmein Sohn liegt krank darnieder", "tokens": ["\u00bb", "mein", "Sohn", "liegt", "krank", "dar\u00b7nie\u00b7der"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "ADJD", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von dieser harten Flucht,", "tokens": ["Von", "die\u00b7ser", "har\u00b7ten", "Flucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hab' ich mein Kind nur wieder,", "tokens": ["Hab'", "ich", "mein", "Kind", "nur", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So hab' ich, was ich sucht.\u00ab", "tokens": ["So", "hab'", "ich", ",", "was", "ich", "sucht", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da klang das Echo wieder:", "tokens": ["Da", "klang", "das", "E\u00b7cho", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u00bbwenn du mich hast besucht,", "tokens": ["\u00bb", "wenn", "du", "mich", "hast", "be\u00b7sucht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nichts schl\u00fcge dich mehr nieder,", "tokens": ["Nichts", "schl\u00fc\u00b7ge", "dich", "mehr", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Krankheit schlag' in Flucht.\u00ab", "tokens": ["Die", "Krank\u00b7heit", "schlag'", "in", "Flucht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbschlag' an die Schmerzenst\u00f6ne,", "tokens": ["\u00bb", "schlag'", "an", "die", "Schmer\u00b7zen\u00b7st\u00f6\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was nur dein Herz beschwert,", "tokens": ["Was", "nur", "dein", "Herz", "be\u00b7schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das athmet all vers\u00f6hnet", "tokens": ["Das", "ath\u00b7met", "all", "ver\u00b7s\u00f6h\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIAT", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom hohen Opferheerd;", "tokens": ["Vom", "ho\u00b7hen", "Op\u00b7fer\u00b7heerd", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Recht wie ein Opfer flammet", "tokens": ["Recht", "wie", "ein", "Op\u00b7fer", "flam\u00b7met"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zum Himmel f\u00fcr den Sohn,", "tokens": ["Zum", "Him\u00b7mel", "f\u00fcr", "den", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dein Lied der Brust entstammet", "tokens": ["Dein", "Lied", "der", "Brust", "ent\u00b7stam\u00b7met"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und dringet leicht zum Thron.\u00ab", "tokens": ["Und", "drin\u00b7get", "leicht", "zum", "Thron", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bbdas k\u00f6nnt ihr sch\u00f6nen T\u00f6ne,", "tokens": ["\u00bb", "das", "k\u00f6nnt", "ihr", "sch\u00f6\u00b7nen", "T\u00f6\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So ruft der Wiederhall,", "tokens": ["So", "ruft", "der", "Wie\u00b7der\u00b7hall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das kann auch mich vers\u00f6hnen,", "tokens": ["Das", "kann", "auch", "mich", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich ein blo\u00dfer Schall!\u00ab", "tokens": ["Da\u00df", "ich", "ein", "blo\u00b7\u00dfer", "Schall", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Sohn kam da gegangen", "tokens": ["Der", "Sohn", "kam", "da", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In seinem Krankenkleid,", "tokens": ["In", "sei\u00b7nem", "Kran\u00b7ken\u00b7kleid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das Fieber war vergangen,", "tokens": ["Das", "Fie\u00b7ber", "war", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er spielte neu in Freud.", "tokens": ["Er", "spiel\u00b7te", "neu", "in", "Freud", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die F\u00fcrstin kam ganz leise,", "tokens": ["Die", "F\u00fcrs\u00b7tin", "kam", "ganz", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umfa\u00dfte beide mild,", "tokens": ["Um\u00b7fa\u00df\u00b7te", "bei\u00b7de", "mild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Wald sang eine Weise,", "tokens": ["Der", "Wald", "sang", "ei\u00b7ne", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als w\u00e4r's ein Wunderbild", "tokens": ["Als", "w\u00e4r's", "ein", "Wun\u00b7der\u00b7bild"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So mu\u00df es immer bleiben", "tokens": ["So", "mu\u00df", "es", "im\u00b7mer", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hier sei ein Stillestand,", "tokens": ["Hier", "sei", "ein", "Stil\u00b7les\u00b7tand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer will die Zeit vertreiben,", "tokens": ["Wer", "will", "die", "Zeit", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die all dies Gl\u00fcck gesandt.", "tokens": ["Die", "all", "dies", "Gl\u00fcck", "ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PDS", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ein Bote kommt gegangen:", "tokens": ["Ein", "Bo\u00b7te", "kommt", "ge\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbdich h\u00f6ret unser Feind,", "tokens": ["\u00bb", "dich", "h\u00f6\u00b7ret", "un\u00b7ser", "Feind", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom s\u00fc\u00dfen Ton befangen", "tokens": ["Vom", "s\u00fc\u00b7\u00dfen", "Ton", "be\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und gar kein Schwert erscheint.\u00ab", "tokens": ["Und", "gar", "kein", "Schwert", "er\u00b7scheint", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da ward im Volk ein Singen.", "tokens": ["Da", "ward", "im", "Volk", "ein", "Sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Rufen, lebet hoch,", "tokens": ["Ein", "Ru\u00b7fen", ",", "le\u00b7bet", "hoch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df alle Saiten klingen,", "tokens": ["Da\u00df", "al\u00b7le", "Sai\u00b7ten", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Ohren klingen noch.", "tokens": ["Die", "Oh\u00b7ren", "klin\u00b7gen", "noch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Sie klingen nicht von Schw\u00e4nen,", "tokens": ["Sie", "klin\u00b7gen", "nicht", "von", "Schw\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nein, wie die \u00c4olsharf,", "tokens": ["Nein", ",", "wie", "die", "\u00c4\u00b7ols\u00b7harf", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald freudig bis zu Thr\u00e4nen,", "tokens": ["Bald", "freu\u00b7dig", "bis", "zu", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie es das Herz bedarf.", "tokens": ["Wie", "es", "das", "Herz", "be\u00b7darf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein Sturm kann sie zersprengen,", "tokens": ["Kein", "Sturm", "kann", "sie", "zer\u00b7spren\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr Ton ist nie zu schwach,", "tokens": ["Ihr", "Ton", "ist", "nie", "zu", "schwach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Sturm weicht den Ges\u00e4ngen,", "tokens": ["Der", "Sturm", "weicht", "den", "Ge\u00b7s\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die tief im Ohre wach.", "tokens": ["Die", "tief", "im", "Oh\u00b7re", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}