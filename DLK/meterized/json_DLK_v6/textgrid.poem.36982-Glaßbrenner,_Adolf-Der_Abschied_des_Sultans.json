{"textgrid.poem.36982": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "Der Abschied des Sultans", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seine Excellenz der Kriegs-Rock spie\u00dfte nun aus einem Fasse", "tokens": ["Sei\u00b7ne", "Ex\u00b7cel\u00b7lenz", "der", "Kriegs\u00b7Rock", "spie\u00df\u00b7te", "nun", "aus", "ei\u00b7nem", "Fas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Mit dem kurzen Kinders\u00e4bel eine gr\u00fcne, essignasse", "tokens": ["Mit", "dem", "kur\u00b7zen", "Kin\u00b7der\u00b7s\u00e4\u00b7bel", "ei\u00b7ne", "gr\u00fc\u00b7ne", ",", "es\u00b7sig\u00b7nas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "$,", "VVFIN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Gurke auf und steckte sie zur H\u00e4lfte in des Sultans Mund,", "tokens": ["Gur\u00b7ke", "auf", "und", "steck\u00b7te", "sie", "zur", "H\u00e4lf\u00b7te", "in", "des", "Sul\u00b7tans", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Duckte dann zu seinen F\u00fc\u00dfen, bellte, blaffte wie ein Hund,", "tokens": ["Duck\u00b7te", "dann", "zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", ",", "bell\u00b7te", ",", "blaff\u00b7te", "wie", "ein", "Hund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$,", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Sprang dann wieder auf, auf mich los, und befahl mir, stracks ein gleiches", "tokens": ["Sprang", "dann", "wie\u00b7der", "auf", ",", "auf", "mich", "los", ",", "und", "be\u00b7fahl", "mir", ",", "stracks", "ein", "glei\u00b7ches"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PTKVZ", "$,", "APPR", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "$,", "ADV", "ART", "ADJA"], "meter": "+-+-+--+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Hundebellen loszulassen vor dem h\u00f6chsten Herrn des Reiches,", "tokens": ["Hun\u00b7de\u00b7bel\u00b7len", "los\u00b7zu\u00b7las\u00b7sen", "vor", "dem", "h\u00f6chs\u00b7ten", "Herrn", "des", "Rei\u00b7ches", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Dessen Calabreserhut mit meinem ihm vom Kopf zu schmei\u00dfen,", "tokens": ["Des\u00b7sen", "Ca\u00b7lab\u00b7re\u00b7ser\u00b7hut", "mit", "mei\u00b7nem", "ihm", "vom", "Kopf", "zu", "schmei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "PPOSAT", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Und zum Schlu\u00df die halbe Gurke ihm vom Mund weg abzubei\u00dfen.", "tokens": ["Und", "zum", "Schlu\u00df", "die", "hal\u00b7be", "Gur\u00b7ke", "ihm", "vom", "Mund", "weg", "ab\u00b7zu\u00b7bei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN", "PPER", "APPRART", "NN", "ADV", "VVIZU", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Dieser Cultus-Akt, durch den sich jeder sel'ge Erdensohn,", "tokens": ["Die\u00b7ser", "Cul\u00b7tus\u00b7Akt", ",", "durch", "den", "sich", "je\u00b7der", "sel'\u00b7ge", "Er\u00b7den\u00b7sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "APPR", "PRELS", "PRF", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Der nach der Verkehrten Welt kommt, unterwerfen mu\u00df dem Thron,", "tokens": ["Der", "nach", "der", "Ver\u00b7kehr\u00b7ten", "Welt", "kommt", ",", "un\u00b7ter\u00b7wer\u00b7fen", "mu\u00df", "dem", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Ich gesteh's, ist eine furchtbar alberne Ceremonie,", "tokens": ["Ich", "ge\u00b7steh's", ",", "ist", "ei\u00b7ne", "furcht\u00b7bar", "al\u00b7ber\u00b7ne", "Ce\u00b7re\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "--+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie in den civilisirten Staaten unsrer Erde sie \u2013", "tokens": ["Wie", "in", "den", "ci\u00b7vi\u00b7lis\u00b7ir\u00b7ten", "Staa\u00b7ten", "uns\u00b7rer", "Er\u00b7de", "sie", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "PPER", "$("], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Au\u00dfer als ehrw\u00fcrdige Sitte, da und dort und dann und wann,", "tokens": ["Au\u00b7\u00dfer", "als", "ehr\u00b7w\u00fcr\u00b7di\u00b7ge", "Sit\u00b7te", ",", "da", "und", "dort", "und", "dann", "und", "wann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ADJA", "NN", "$,", "ADV", "KON", "ADV", "KON", "ADV", "KON", "PWAV", "$,"], "meter": "+--+---+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Oder bei Gelegenheiten \u2013 \u00e4hnlich ", "tokens": ["O\u00b7der", "bei", "Ge\u00b7le\u00b7gen\u00b7hei\u00b7ten", "\u2013", "\u00e4hn\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "NN", "$(", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbnun Du, Fremdling, Dummdummdummer\u00ab, sprach der Sultan, \u00bbbist geworden,", "tokens": ["\u00bb", "nun", "Du", ",", "Fremd\u00b7ling", ",", "Dumm\u00b7dumm\u00b7dum\u00b7mer", "\u00ab", ",", "sprach", "der", "Sul\u00b7tan", ",", "\u00bb", "bist", "ge\u00b7wor\u00b7den", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "$,", "NN", "$,", "NN", "$(", "$,", "VVFIN", "ART", "NN", "$,", "$(", "VAFIN", "VAPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Steck ich Dir an Deinen R\u00fccken diesen Pumpel-Pampel-Orden.", "tokens": ["Steck", "ich", "Dir", "an", "Dei\u00b7nen", "R\u00fc\u00b7cken", "die\u00b7sen", "Pum\u00b7pel\u00b7Pam\u00b7pel\u00b7Or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "APPR", "PPOSAT", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Diesen mu\u00dft Du, so ist's Vorschrift, und ich darf Dir's nicht erlassen,", "tokens": ["Die\u00b7sen", "mu\u00dft", "Du", ",", "so", "ist's", "Vor\u00b7schrift", ",", "und", "ich", "darf", "Dir's", "nicht", "er\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "$,", "ADV", "VAFIN", "NN", "$,", "KON", "PPER", "VMFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-++--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Tragen wie ein jeder Andre aus den ungereimten Klassen,", "tokens": ["Tra\u00b7gen", "wie", "ein", "je\u00b7der", "And\u00b7re", "aus", "den", "un\u00b7ge\u00b7reim\u00b7ten", "Klas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "PIAT", "PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Tragen bis Du Dir erworben irgend ein Verdienst um Mich,", "tokens": ["Tra\u00b7gen", "bis", "Du", "Dir", "er\u00b7wor\u00b7ben", "ir\u00b7gend", "ein", "Ver\u00b7dienst", "um", "Mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PPER", "VVPP", "ADV", "ART", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Und mein Pr\u00fcgel-Rock Dich freispricht \u00f6ffentlich und feierlich.", "tokens": ["Und", "mein", "Pr\u00fc\u00b7gel\u00b7Rock", "Dich", "frei\u00b7spricht", "\u00f6f\u00b7fent\u00b7lich", "und", "fei\u00b7er\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "--+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Dann erst darfst Du Dir den Orden ab vom R\u00fccken unten rei\u00dfen", "tokens": ["Dann", "erst", "darfst", "Du", "Dir", "den", "Or\u00b7den", "ab", "vom", "R\u00fc\u00b7cken", "un\u00b7ten", "rei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "PTKVZ", "APPRART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Und ihn einem Narren schenken oder auf die Stra\u00dfe schmei\u00dfen.\u00ab", "tokens": ["Und", "ihn", "ei\u00b7nem", "Nar\u00b7ren", "schen\u00b7ken", "o\u00b7der", "auf", "die", "Stra\u00b7\u00dfe", "schmei\u00b7\u00dfen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVINF", "KON", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Kaum, da\u00df mir am Schlafrock hinten hing das lange Ordensband,", "tokens": ["Kaum", ",", "da\u00df", "mir", "am", "Schla\u00b7frock", "hin\u00b7ten", "hing", "das", "lan\u00b7ge", "Or\u00b7dens\u00b7band", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPRART", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Hielt der Sultan hin mir seine allerh\u00f6chste off'ne Hand.", "tokens": ["Hielt", "der", "Sul\u00b7tan", "hin", "mir", "sei\u00b7ne", "al\u00b7ler\u00b7h\u00f6chs\u00b7te", "off'\u00b7ne", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PPER", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "\u00bbwas begehrst Du, Herr?\u00ab", "tokens": ["\u00bb", "was", "be\u00b7gehrst", "Du", ",", "Herr", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$,", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbwie? Bist Du unterrichtet nicht, da\u00df Du die", "tokens": ["\u00bb", "wie", "?", "Bist", "Du", "un\u00b7ter\u00b7rich\u00b7tet", "nicht", ",", "da\u00df", "Du", "die"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PWAV", "$.", "VAFIN", "PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Staatsgeb\u00fchren mir mu\u00dft zahlen: F\u00fcnfundzwanzig goldne Scudi?", "tokens": ["Staats\u00b7ge\u00b7b\u00fch\u00b7ren", "mir", "mu\u00dft", "zah\u00b7len", ":", "F\u00fcn\u00b7fund\u00b7zwan\u00b7zig", "gold\u00b7ne", "Scu\u00b7di", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VMFIN", "VVINF", "$.", "CARD", "ADJA", "NE", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Allerdings geschieht hier Ales ", "tokens": ["Al\u00b7ler\u00b7dings", "ge\u00b7schieht", "hier", "A\u00b7les"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ausgepr\u00e4gt nicht in verschied'nen Gold- und Silberm\u00fcnzen w\u00e4re,", "tokens": ["Aus\u00b7ge\u00b7pr\u00e4gt", "nicht", "in", "ver\u00b7schie\u00b7d'\u00b7nen", "Gold", "und", "Sil\u00b7ber\u00b7m\u00fcn\u00b7zen", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ADJA", "TRUNC", "KON", "NN", "VAFIN", "$,"], "meter": "+-+-+-+--+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "K\u00f6nnte man sie ja nicht richtig nach des Staates Interessen,", "tokens": ["K\u00f6nn\u00b7te", "man", "sie", "ja", "nicht", "rich\u00b7tig", "nach", "des", "Staa\u00b7tes", "In\u00b7ter\u00b7es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "ADV", "PTKNEG", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Nicht in ihrem eignen Werthe, nicht nach Rang und W\u00fcrde messen!", "tokens": ["Nicht", "in", "ih\u00b7rem", "eig\u00b7nen", "Wert\u00b7he", ",", "nicht", "nach", "Rang", "und", "W\u00fcr\u00b7de", "mes\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$,", "PTKNEG", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Ich, der ich die gr\u00f6\u00dfte Ehre, Drei Millionen Pfund, genie\u00dfe,", "tokens": ["Ich", ",", "der", "ich", "die", "gr\u00f6\u00df\u00b7te", "Eh\u00b7re", ",", "Drei", "Mil\u00b7lion\u00b7en", "Pfund", ",", "ge\u00b7nie\u00b7\u00dfe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ART", "ADJA", "NN", "$,", "CARD", "NN", "NN", "$,", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Was w\u00e4r' ich, wenn ich auf Worte, statt auf Steuern mich verlie\u00dfe!", "tokens": ["Was", "w\u00e4r'", "ich", ",", "wenn", "ich", "auf", "Wor\u00b7te", ",", "statt", "auf", "Steu\u00b7ern", "mich", "ver\u00b7lie\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "NN", "$,", "KOUI", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "Nein, der Dummdummdummer, Fremdling, ist reeller Realist,", "tokens": ["Nein", ",", "der", "Dumm\u00b7dumm\u00b7dum\u00b7mer", ",", "Fremd\u00b7ling", ",", "ist", "re\u00b7el\u00b7ler", "Re\u00b7a\u00b7list", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "$,", "NN", "$,", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Und nimmt nur f\u00fcr baare M\u00fcnze das, was baare M\u00fcnze ist!", "tokens": ["Und", "nimmt", "nur", "f\u00fcr", "baa\u00b7re", "M\u00fcn\u00b7ze", "das", ",", "was", "baa\u00b7re", "M\u00fcn\u00b7ze", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN", "PDS", "$,", "PWS", "ADJA", "NN", "VAFIN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Zahl' drum die Geb\u00fchren!\u00ab", "tokens": ["Zahl'", "drum", "die", "Ge\u00b7b\u00fch\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PAV", "ART", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbgerne, doch ich hab' kein Geld zur Hand;", "tokens": ["\u00bb", "ger\u00b7ne", ",", "doch", "ich", "hab'", "kein", "Geld", "zur", "Hand", ";"], "token_