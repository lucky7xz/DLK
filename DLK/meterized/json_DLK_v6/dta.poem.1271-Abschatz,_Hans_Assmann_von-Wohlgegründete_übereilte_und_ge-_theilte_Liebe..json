{"dta.poem.1271": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Wohlgegr\u00fcndete/ \u00fcbereilte und ge-  \n theilte Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wahre Glutt", "tokens": ["Wenn", "wah\u00b7re", "Glutt"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "In treuem Hertzen brennet/", "tokens": ["In", "treu\u00b7em", "Hert\u00b7zen", "bren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Grund der edlen Flamme kennet/", "tokens": ["Den", "Grund", "der", "ed\u00b7len", "Flam\u00b7me", "ken\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So taurt ihr ungef\u00e4rbter Schein/", "tokens": ["So", "taurt", "ihr", "un\u00b7ge\u00b7f\u00e4rb\u00b7ter", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bi\u00df da\u00df wir Asche seyn/", "tokens": ["Bi\u00df", "da\u00df", "wir", "A\u00b7sche", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "NN", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ohn allen Wanckelmutt;", "tokens": ["Ohn", "al\u00b7len", "Wan\u00b7ckel\u00b7mutt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es mu\u00df ihr ieder Tag verneuten Zunder geben/", "tokens": ["Es", "mu\u00df", "ihr", "ie\u00b7der", "Tag", "ver\u00b7neu\u00b7ten", "Zun\u00b7der", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "NN", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und sie der Treue Ruhm bi\u00df zu den Sternen heben.", "tokens": ["Und", "sie", "der", "Treu\u00b7e", "Ruhm", "bi\u00df", "zu", "den", "Ster\u00b7nen", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "NN", "APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn falsche Glutt", "tokens": ["Wenn", "fal\u00b7sche", "Glutt"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die Augen \u00fcbereilet/", "tokens": ["Die", "Au\u00b7gen", "\u00fc\u00b7be\u00b7rei\u00b7let", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So wird das Hertze leicht getheilet/", "tokens": ["So", "wird", "das", "Hert\u00b7ze", "leicht", "ge\u00b7thei\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "VVFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Sinnen unbegr\u00fcndter Schlu\u00df", "tokens": ["Der", "Sin\u00b7nen", "un\u00b7be\u00b7gr\u00fcnd\u00b7ter", "Schlu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gebieret Uberdru\u00df", "tokens": ["Ge\u00b7bie\u00b7ret", "U\u00b7berd\u00b7ru\u00df"], "token_info": ["word", "word"], "pos": ["VVPP", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und leichten Wanckelmutt:", "tokens": ["Und", "leich\u00b7ten", "Wan\u00b7ckel\u00b7mutt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch aber steh ich an/ ob so verg\u00e4nglich Brennen", "tokens": ["Doch", "a\u00b7ber", "steh", "ich", "an", "/", "ob", "so", "ver\u00b7g\u00e4ng\u00b7lich", "Bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$(", "KOUS", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein Feuer/ oder nur ein Irrlicht sey zu nennen.", "tokens": ["Ein", "Feu\u00b7er", "/", "o\u00b7der", "nur", "ein", "Irr\u00b7licht", "sey", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "ADV", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn gleiche Glutt", "tokens": ["Wenn", "glei\u00b7che", "Glutt"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Aus Wang\u2019 und Auge blitzet/", "tokens": ["Aus", "Wang'", "und", "Au\u00b7ge", "blit\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu einer Zeit das Hertz erhitzet/", "tokens": ["Zu", "ei\u00b7ner", "Zeit", "das", "Hertz", "er\u00b7hit\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So schmertzet uns die schwere Wahl/", "tokens": ["So", "schmert\u00b7zet", "uns", "die", "schwe\u00b7re", "Wahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und plagt mit tausend Qual", "tokens": ["Und", "plagt", "mit", "tau\u00b7send", "Qual"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Den ungewissen Mutt.", "tokens": ["Den", "un\u00b7ge\u00b7wis\u00b7sen", "Mutt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sagt/ Nymphen/ k\u00f6nt ihr denn auch so zertheilte Flam-", "tokens": ["Sagt", "/", "Nym\u00b7phen", "/", "k\u00f6nt", "ihr", "denn", "auch", "so", "zer\u00b7theil\u00b7te", "Flam"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "NN", "$(", "VMFIN", "PPER", "ADV", "ADV", "ADV", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die ihr zugleich in uns erweckt/ schlecht hin verdammen?", "tokens": ["Die", "ihr", "zu\u00b7gleich", "in", "uns", "er\u00b7weckt", "/", "schlecht", "hin", "ver\u00b7dam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "PPER", "VVPP", "$(", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}