{"textgrid.poem.66462": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du kleines blitzendes Sternelein,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kleines blitzendes Sternelein,", "tokens": ["Du", "klei\u00b7nes", "blit\u00b7zen\u00b7des", "Ster\u00b7ne\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nun sag' mir, was willst du?", "tokens": ["Nun", "sag'", "mir", ",", "was", "willst", "du", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "VMFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du kannst da oben ja selig sein,", "tokens": ["Du", "kannst", "da", "o\u00b7ben", "ja", "se\u00b7lig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Was blinzelst du mir denn zu?", "tokens": ["Was", "blin\u00b7zelst", "du", "mir", "denn", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Such du dir dein Liebstes im Sternenreihn,", "tokens": ["Such", "du", "dir", "dein", "Liebs\u00b7tes", "im", "Ster\u00b7nen\u00b7reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und la\u00df mich mit Blinzeln in Ruh,", "tokens": ["Und", "la\u00df", "mich", "mit", "Blin\u00b7zeln", "in", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich hab' auf Erden ein Sternelein,", "tokens": ["Ich", "hab'", "auf", "Er\u00b7den", "ein", "Ster\u00b7ne\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das ist viel sch\u00f6ner als du!", "tokens": ["Das", "ist", "viel", "sch\u00f6\u00b7ner", "als", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}