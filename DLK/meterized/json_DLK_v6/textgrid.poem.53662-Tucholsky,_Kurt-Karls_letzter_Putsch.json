{"textgrid.poem.53662": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Karls letzter Putsch", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein fescher Trottel mit der samtnen Kappen,", "tokens": ["Ein", "fe\u00b7scher", "Trot\u00b7tel", "mit", "der", "samt\u00b7nen", "Kap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "so m\u00f6cht das durch die Weltgeschichte tappen \u2013", "tokens": ["so", "m\u00f6cht", "das", "durch", "die", "Welt\u00b7ge\u00b7schich\u00b7te", "tap\u00b7pen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "denn au\u00dfer Kaiser hat er nichts gelernt.", "tokens": ["denn", "au\u00b7\u00dfer", "Kai\u00b7ser", "hat", "er", "nichts", "ge\u00b7lernt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Am liebsten t\u00e4t er alleweil regieren.", "tokens": ["Am", "liebs\u00b7ten", "t\u00e4t", "er", "al\u00b7le\u00b7weil", "re\u00b7gie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Geh her \u2013 man mu\u00df ihm gar net ignorieren!", "tokens": ["Geh", "her", "\u2013", "man", "mu\u00df", "ihm", "gar", "net", "ig\u00b7no\u00b7rie\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "$(", "PIS", "VMFIN", "PPER", "ADV", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nur schade, da\u00df ein Henker ihn entfernt.", "tokens": ["Nur", "scha\u00b7de", ",", "da\u00df", "ein", "Hen\u00b7ker", "ihn", "ent\u00b7fernt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das schickt Millionen in die Sch\u00fctzengr\u00e4ben,", "tokens": ["Das", "schickt", "Mil\u00b7lion\u00b7en", "in", "die", "Sch\u00fct\u00b7zen\u00b7gr\u00e4\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "schreibt faule Briefe und bleibt selber leben", "tokens": ["schreibt", "fau\u00b7le", "Brie\u00b7fe", "und", "bleibt", "sel\u00b7ber", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "KON", "VVFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "und zappelt heute noch dem Throne zu . . .", "tokens": ["und", "zap\u00b7pelt", "heu\u00b7te", "noch", "dem", "Thro\u00b7ne", "zu", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Servus du \u2013!", "tokens": ["Ser\u00b7vus", "du", "\u2013", "!"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NE", "NE", "$(", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Die Majest\u00e4t kam leider stark herunter.", "tokens": ["Die", "Ma\u00b7jes\u00b7t\u00e4t", "kam", "lei\u00b7der", "stark", "her\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einst ging die Sonne in dem Reich nicht unter.", "tokens": ["Einst", "ging", "die", "Son\u00b7ne", "in", "dem", "Reich", "nicht", "un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKNEG", "APPR", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Heut steht ein Dollar hoch am Firmament.", "tokens": ["Heut", "steht", "ein", "Dol\u00b7lar", "hoch", "am", "Fir\u00b7ma\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Krone fiel. Und f\u00e4llt und f\u00e4llt aufs neue.", "tokens": ["Die", "Kro\u00b7ne", "fiel", ".", "Und", "f\u00e4llt", "und", "f\u00e4llt", "aufs", "neu\u00b7e", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "VVFIN", "KON", "VVFIN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Wiener h\u00e4lt der Republik die Treue \u2013", "tokens": ["Der", "Wie\u00b7ner", "h\u00e4lt", "der", "Re\u00b7pub\u00b7lik", "die", "Treu\u00b7e", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und Horthy k\u00e4mpft als Karlchens Konkurrent.", "tokens": ["und", "Hor\u00b7thy", "k\u00e4mpft", "als", "Karl\u00b7chens", "Kon\u00b7kur\u00b7rent", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "KOKOM", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das Zepter, das einst so viel Macht verlieh", "tokens": ["Das", "Zep\u00b7ter", ",", "das", "einst", "so", "viel", "Macht", "ver\u00b7lieh"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "PIAT", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "schwingt Habsburgs letzter reisender Kommis.", "tokens": ["schwingt", "Habs\u00b7burgs", "letz\u00b7ter", "rei\u00b7sen\u00b7der", "Kom\u00b7mis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Karl Huckebein \u2013 Sie sind gewi\u00df dramatisch.", "tokens": ["Karl", "Huc\u00b7ke\u00b7bein", "\u2013", "Sie", "sind", "ge\u00b7wi\u00df", "dra\u00b7ma\u00b7tisch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ach, lieber Herr, du bist uns so sympathisch,", "tokens": ["Ach", ",", "lie\u00b7ber", "Herr", ",", "du", "bist", "uns", "so", "sym\u00b7pa\u00b7thisch", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "und du hast tausend Freunde auf der Welt:", "tokens": ["und", "du", "hast", "tau\u00b7send", "Freun\u00b7de", "auf", "der", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "CARD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das ganze Volk der Schl\u00e4chter und Soldaten,", "tokens": ["Das", "gan\u00b7ze", "Volk", "der", "Schl\u00e4ch\u00b7ter", "und", "Sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Blutspekulanten, arbeitslose Potentaten \u2013", "tokens": ["Bluts\u00b7pe\u00b7ku\u00b7lan\u00b7ten", ",", "ar\u00b7beits\u00b7lo\u00b7se", "Po\u00b7ten\u00b7ta\u00b7ten", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "sie warten alle, wie dein W\u00fcrfel f\u00e4llt.", "tokens": ["sie", "war\u00b7ten", "al\u00b7le", ",", "wie", "dein", "W\u00fcr\u00b7fel", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie warten alle auf die gro\u00dfen Zeiten,", "tokens": ["Sie", "war\u00b7ten", "al\u00b7le", "auf", "die", "gro\u00b7\u00dfen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "wo Generale \u00fcbers Brachfeld reiten,", "tokens": ["wo", "Ge\u00b7ne\u00b7ra\u00b7le", "\u00fc\u00b7bers", "Brach\u00b7feld", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.9": {"text": "wo Proletarier wieder stumm verrecken,", "tokens": ["wo", "Pro\u00b7le\u00b7ta\u00b7rier", "wie\u00b7der", "stumm", "ver\u00b7re\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "wo Taschen wieder voll von Geldern stecken,", "tokens": ["wo", "Ta\u00b7schen", "wie\u00b7der", "voll", "von", "Gel\u00b7dern", "ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "wo Reklamierte Siegestore mei\u00dfeln,", "tokens": ["wo", "Re\u00b7kla\u00b7mier\u00b7te", "Sie\u00b7ge\u00b7sto\u00b7re", "mei\u00b7\u00dfeln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "wo St\u00e4be wirken wie die Gottesgei\u00dfeln . . .", "tokens": ["wo", "St\u00e4\u00b7be", "wir\u00b7ken", "wie", "die", "Got\u00b7tes\u00b7gei\u00b7\u00dfeln", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "NN", "VVINF", "KOKOM", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Was sich heut grollend ins Zivil geduckt hat,", "tokens": ["Was", "sich", "heut", "grol\u00b7lend", "ins", "Zi\u00b7vil", "ge\u00b7duckt", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "ADJD", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "weils Gott der Herr im Zorne ausgespuckt hat:", "tokens": ["weils", "Gott", "der", "Herr", "im", "Zor\u00b7ne", "aus\u00b7ge\u00b7spuckt", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "das alles winkte dir voll Hoffnung zu.", "tokens": ["das", "al\u00b7les", "wink\u00b7te", "dir", "voll", "Hoff\u00b7nung", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Heil dir im Siegerkranze!", "tokens": ["Heil", "dir", "im", "Sie\u00b7ger\u00b7kran\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Servus du \u2013!", "tokens": ["Ser\u00b7vus", "du", "\u2013", "!"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NE", "NE", "$(", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}