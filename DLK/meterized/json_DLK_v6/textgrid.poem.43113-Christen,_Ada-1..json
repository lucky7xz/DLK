{"textgrid.poem.43113": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist denn mein ganzes Sein verwirrt,", "tokens": ["Ist", "denn", "mein", "gan\u00b7zes", "Sein", "ver\u00b7wirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Alles ich jetzt anders schau';", "tokens": ["Da\u00df", "Al\u00b7les", "ich", "jetzt", "an\u00b7ders", "schau'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erscheint mir doch die ganze Welt", "tokens": ["Er\u00b7scheint", "mir", "doch", "die", "gan\u00b7ze", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein schmutzig Bild nur, Grau in Grau.", "tokens": ["Ein", "schmut\u00b7zig", "Bild", "nur", ",", "Grau", "in", "Grau", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADV", "$,", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich lebte gern und lachte gern", "tokens": ["Ich", "leb\u00b7te", "gern", "und", "lach\u00b7te", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sonst ein Menschenkind \u2013", "tokens": ["Wie", "sonst", "ein", "Men\u00b7schen\u00b7kind", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch Alles glotzt so fratzenhaft \u2013", "tokens": ["Doch", "Al\u00b7les", "glotzt", "so", "frat\u00b7zen\u00b7haft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dies Grau, es macht mich blind.", "tokens": ["Dies", "Grau", ",", "es", "macht", "mich", "blind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}