{"textgrid.poem.47711": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[neulich auf der sch\u00f6nsten Aue]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Neulich auf der sch\u00f6nsten Aue", "tokens": ["Neu\u00b7lich", "auf", "der", "sch\u00f6ns\u00b7ten", "Au\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klettert' ich auf einen H\u00fcgel,", "tokens": ["Klet\u00b7tert'", "ich", "auf", "ei\u00b7nen", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blickte sehnlich in die Runde,", "tokens": ["Blick\u00b7te", "sehn\u00b7lich", "in", "die", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sp\u00e4hte scharf in alle Ferne,", "tokens": ["Sp\u00e4h\u00b7te", "scharf", "in", "al\u00b7le", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob vielleicht zu sehen w\u00e4re", "tokens": ["Ob", "viel\u00b7leicht", "zu", "se\u00b7hen", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PTKZU", "VVINF", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Irgend eine goldne Liebste,", "tokens": ["Ir\u00b7gend", "ei\u00b7ne", "gold\u00b7ne", "Liebs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mir bestimmt von einem endlich", "tokens": ["Mir", "be\u00b7stimmt", "von", "ei\u00b7nem", "end\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nach so mancher stillen Z\u00e4hre", "tokens": ["Nach", "so", "man\u00b7cher", "stil\u00b7len", "Z\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Zugewandtem Huldgeschick.", "tokens": ["Zu\u00b7ge\u00b7wand\u00b7tem", "Huld\u00b7ge\u00b7schick", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Siehe da, mit einem Male", "tokens": ["Sie\u00b7he", "da", ",", "mit", "ei\u00b7nem", "Ma\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Naheten von allen Seiten", "tokens": ["Na\u00b7he\u00b7ten", "von", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Goldne M\u00e4dchen, sch\u00f6ne Sterne,", "tokens": ["Gold\u00b7ne", "M\u00e4d\u00b7chen", ",", "sch\u00f6\u00b7ne", "Ster\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Wie geboren aus dem Strahle,", "tokens": ["Wie", "ge\u00b7bo\u00b7ren", "aus", "dem", "Strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Den die warme Sonn' im Lenze", "tokens": ["Den", "die", "war\u00b7me", "Sonn'", "im", "Len\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Sendet auf die Erde nieder,", "tokens": ["Sen\u00b7det", "auf", "die", "Er\u00b7de", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Wimmelten zu meinen F\u00fc\u00dfen", "tokens": ["Wim\u00b7mel\u00b7ten", "zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.17": {"text": "Allzuhauf im gr\u00fcnen Thale,", "tokens": ["All\u00b7zu\u00b7hauf", "im", "gr\u00fc\u00b7nen", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Schlangen allerliebste T\u00e4nze,", "tokens": ["Schlan\u00b7gen", "al\u00b7ler\u00b7liebs\u00b7te", "T\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Sangen allerliebste Lieder;", "tokens": ["San\u00b7gen", "al\u00b7ler\u00b7liebs\u00b7te", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Ja die sch\u00f6nste von den Sch\u00f6nen,", "tokens": ["Ja", "die", "sch\u00f6ns\u00b7te", "von", "den", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Welche sich versammelt hatten,", "tokens": ["Wel\u00b7che", "sich", "ver\u00b7sam\u00b7melt", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Hob sich in die Luft und schwebte", "tokens": ["Hob", "sich", "in", "die", "Luft", "und", "schweb\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Sacht herauf zu meiner H\u00f6he,", "tokens": ["Sacht", "her\u00b7auf", "zu", "mei\u00b7ner", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Ihre sanften Arme breitend", "tokens": ["Ih\u00b7re", "sanf\u00b7ten", "Ar\u00b7me", "brei\u00b7tend"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Mit der Liebe hei\u00dfem Blick.", "tokens": ["Mit", "der", "Lie\u00b7be", "hei\u00b7\u00dfem", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Welch ein Taumel des Entz\u00fcckens!", "tokens": ["Welch", "ein", "Tau\u00b7mel", "des", "Ent\u00b7z\u00fc\u00b7ckens", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Wie ich gl\u00fchte, wie ich bebte!", "tokens": ["Wie", "ich", "gl\u00fch\u00b7te", ",", "wie", "ich", "beb\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Immer n\u00e4her kam der Engel,", "tokens": ["Im\u00b7mer", "n\u00e4\u00b7her", "kam", "der", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Der geliebte; schon gedacht' ich", "tokens": ["Der", "ge\u00b7lieb\u00b7te", ";", "schon", "ge\u00b7dacht'", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$.", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Ihn zu fassen, ihn zu halten", "tokens": ["Ihn", "zu", "fas\u00b7sen", ",", "ihn", "zu", "hal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Selig in der Arme Strick;", "tokens": ["Se\u00b7lig", "in", "der", "Ar\u00b7me", "Strick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Aber in dem Augenblick \u2013", "tokens": ["A\u00b7ber", "in", "dem", "Au\u00b7gen\u00b7blick", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Denn die Nacht mit ihrem Traume", "tokens": ["Denn", "die", "Nacht", "mit", "ih\u00b7rem", "Trau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Neckte mich \u2013 da, weh, erwacht' ich,", "tokens": ["Neck\u00b7te", "mich", "\u2013", "da", ",", "weh", ",", "er\u00b7wacht'", "ich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ADV", "$,", "PTKVZ", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Und zu einem leeren Schatten,", "tokens": ["Und", "zu", "ei\u00b7nem", "lee\u00b7ren", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Und zu einem hohlen Schaume", "tokens": ["Und", "zu", "ei\u00b7nem", "hoh\u00b7len", "Schau\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "War geworden all mein Gl\u00fcck.", "tokens": ["War", "ge\u00b7wor\u00b7den", "all", "mein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VAPP", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}