{"textgrid.poem.36010": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "[schwestern, la\u00dft uns freudig singen]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwestern, la\u00dft uns freudig singen:", "tokens": ["Schwes\u00b7tern", ",", "la\u00dft", "uns", "freu\u00b7dig", "sin\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Holder Festtag, sei gegr\u00fc\u00dft!", "tokens": ["Hol\u00b7der", "Fest\u00b7tag", ",", "sei", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft uns heute Kr\u00e4nze schlingen,", "tokens": ["La\u00dft", "uns", "heu\u00b7te", "Kr\u00e4n\u00b7ze", "schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedes Bl\u00fcmchen, das da sprie\u00dft.", "tokens": ["Je\u00b7des", "Bl\u00fcm\u00b7chen", ",", "das", "da", "sprie\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Heute wollen wir uns freuen,", "tokens": ["Heu\u00b7te", "wol\u00b7len", "wir", "uns", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr jubelnd Blumen streuen", "tokens": ["Ihr", "ju\u00b7belnd", "Blu\u00b7men", "streu\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVPP", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und singen lala lala,", "tokens": ["Und", "sin\u00b7gen", "la\u00b7la", "la\u00b7la", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Es lebe Antonia.", "tokens": ["Es", "le\u00b7be", "An\u00b7to\u00b7nia", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "M\u00f6ge Gott, der Herr, dich segnen,", "tokens": ["M\u00f6\u00b7ge", "Gott", ",", "der", "Herr", ",", "dich", "seg\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "ART", "NN", "$,", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Morgens fr\u00fch und abends sp\u00e4t,", "tokens": ["Mor\u00b7gens", "fr\u00fch", "und", "a\u00b7bends", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6ge Freude dir begegnen,", "tokens": ["M\u00f6\u00b7ge", "Freu\u00b7de", "dir", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo dein Fu\u00df auf Erden geht.", "tokens": ["Wo", "dein", "Fu\u00df", "auf", "Er\u00b7den", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wir wollen uns bestreben,", "tokens": ["Und", "wir", "wol\u00b7len", "uns", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich durch Tugend zu erfreun,", "tokens": ["Dich", "durch", "Tu\u00b7gend", "zu", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ganz dir unsre Herzen geben,", "tokens": ["Ganz", "dir", "uns\u00b7re", "Her\u00b7zen", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine guten Kinder sein!", "tokens": ["Dei\u00b7ne", "gu\u00b7ten", "Kin\u00b7der", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nimm auch unsre kleinen Gaben,", "tokens": ["Nimm", "auch", "uns\u00b7re", "klei\u00b7nen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die wir heut' so freudig weihn;", "tokens": ["Die", "wir", "heut'", "so", "freu\u00b7dig", "weihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn das Beste, was wir haben,", "tokens": ["Denn", "das", "Bes\u00b7te", ",", "was", "wir", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser Herz, ist l\u00e4ngst schon dein.", "tokens": ["Un\u00b7ser", "Herz", ",", "ist", "l\u00e4ngst", "schon", "dein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "ADV", "ADV", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}