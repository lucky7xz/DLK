{"dta.poem.9114": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kein Laut!", "tokens": ["Kein", "Laut", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Nur die Pappeln fl\u00fcstern . . .", "tokens": ["Nur", "die", "Pap\u00b7peln", "fl\u00fcs\u00b7tern", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$.", "$.", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Der alte T\u00fcmpel vor mir schwarz wie Tinte,", "tokens": ["Der", "al\u00b7te", "T\u00fcm\u00b7pel", "vor", "mir", "schwarz", "wie", "Tin\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "um mich, \u00fcber mir, von allen Seiten,", "tokens": ["um", "mich", ",", "\u00fc\u00b7ber", "mir", ",", "von", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "APPR", "PPER", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "auf Fledermausfl\u00fcgeln,", "tokens": ["auf", "Fle\u00b7der\u00b7maus\u00b7fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "die Nacht,", "tokens": ["die", "Nacht", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "und nur dr\u00fcben noch,", "tokens": ["und", "nur", "dr\u00fc\u00b7ben", "noch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "zwischen den beiden Weidenst\u00fcmpfen,", "tokens": ["zwi\u00b7schen", "den", "bei\u00b7den", "Wei\u00b7den\u00b7st\u00fcmp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "die sich im Dunkeln wie Drachen dehnen,", "tokens": ["die", "sich", "im", "Dun\u00b7keln", "wie", "Dra\u00b7chen", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "APPRART", "NN", "KOKOM", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "matt, fahl, verr\u00f6chelnd,", "tokens": ["matt", ",", "fahl", ",", "ver\u00b7r\u00f6\u00b7chelnd", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "ein letzter Schwefelstreif.", "tokens": ["ein", "letz\u00b7ter", "Schwe\u00b7fel\u00b7streif", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf ihm, scharf, eine Silhouette: ein Faun, der die Fl\u00f6te bl\u00e4st.", "tokens": ["Auf", "ihm", ",", "scharf", ",", "ei\u00b7ne", "Sil\u00b7hou\u00b7et\u00b7te", ":", "ein", "Faun", ",", "der", "die", "Fl\u00f6\u00b7te", "bl\u00e4st", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "VVFIN", "$,", "ART", "NN", "$.", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Ich sehe deutlich seine Finger.", "tokens": ["Ich", "se\u00b7he", "deut\u00b7lich", "sei\u00b7ne", "Fin\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sind alle zierlich gespreizt", "tokens": ["Sie", "sind", "al\u00b7le", "zier\u00b7lich", "ge\u00b7spreizt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "ADJD", "VVPP"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "und die beiden kleinsten sogar h\u00f6chst kokett aufw\u00e4rts gebogen.", "tokens": ["und", "die", "bei\u00b7den", "kleins\u00b7ten", "so\u00b7gar", "h\u00f6chst", "ko\u00b7kett", "auf\u00b7w\u00e4rts", "ge\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "ADJA", "ADV", "ADV", "ADJD", "ADV", "VVPP", "$."], "meter": "-+--+--++--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das grazi\u00f6se R\u00f6hrchen quer in ihrer Mitte", "tokens": ["Das", "gra\u00b7zi\u00b7\u00f6\u00b7se", "R\u00f6hr\u00b7chen", "quer", "in", "ih\u00b7rer", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "schwebt fast wagerecht \u00fcber der linken Schulter.", "tokens": ["schwebt", "fast", "wa\u00b7ge\u00b7recht", "\u00fc\u00b7ber", "der", "lin\u00b7ken", "Schul\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Auch die rechte sehe ich.", "tokens": ["Auch", "die", "rech\u00b7te", "se\u00b7he", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur den Kopf nicht. Der fehlt. Der ist runtergekullert.", "tokens": ["Nur", "den", "Kopf", "nicht", ".", "Der", "fehlt", ".", "Der", "ist", "run\u00b7ter\u00b7ge\u00b7kul\u00b7lert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKNEG", "$.", "ART", "VVFIN", "$.", "PDS", "VAFIN", "VVPP", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Der liegt seit hundert Jahren schon", "tokens": ["Der", "liegt", "seit", "hun\u00b7dert", "Jah\u00b7ren", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "CARD", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "unten im T\u00fcmpel.", "tokens": ["un\u00b7ten", "im", "T\u00fcm\u00b7pel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Plitsch! \u2014? Ein Frosch.", "tokens": ["Plitsch", "!", "?", "Ein", "Frosch", "."], "token_info": ["word", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["ADJD", "$.", "$(", "$.", "ART", "NN", "$."], "meter": "+--", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Ich bin zusammengeschrocken.", "tokens": ["Ich", "bin", "zu\u00b7sam\u00b7men\u00b7ge\u00b7schro\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Der Streif dr\u00fcben erlischt,", "tokens": ["Der", "Streif", "dr\u00fc\u00b7ben", "er\u00b7lischt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "ich f\u00fchle, wie das Wasser Kreise treibt,", "tokens": ["ich", "f\u00fch\u00b7le", ",", "wie", "das", "Was\u00b7ser", "Krei\u00b7se", "treibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und die uralte Steinbank, auf der ich sitze,", "tokens": ["und", "die", "ur\u00b7al\u00b7te", "Stein\u00b7bank", ",", "auf", "der", "ich", "sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "schauert mir pl\u00f6tzlich ihre K\u00e4lte bis ins Genick hinauf.", "tokens": ["schau\u00b7ert", "mir", "pl\u00f6tz\u00b7lich", "ih\u00b7re", "K\u00e4l\u00b7te", "bis", "ins", "Ge\u00b7nick", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "APPR", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+--+-+", "measure": "iambic.septa.invert"}}, "stanza.8": {"line.1": {"text": ". . . . .?", "tokens": [".", ".", ".", ".", ".", "?"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$."]}}, "stanza.9": {"line.1": {"text": "Nein. Nichts. Nur die Pappeln.", "tokens": ["Nein", ".", "Nichts", ".", "Nur", "die", "Pap\u00b7peln", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PIS", "$.", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}