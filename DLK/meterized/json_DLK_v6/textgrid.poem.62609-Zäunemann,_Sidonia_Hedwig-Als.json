{"textgrid.poem.62609": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Als", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kaum hatte Evens Blick den Baum mit Lust gesehn;", "tokens": ["Kaum", "hat\u00b7te", "E\u00b7vens", "Blick", "den", "Baum", "mit", "Lust", "ge\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "NN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kaum war der Apfel-Bi\u00df von beyder Mund geschehn;", "tokens": ["Kaum", "war", "der", "Ap\u00b7fel\u00b7Bi\u00df", "von", "bey\u00b7der", "Mund", "ge\u00b7schehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So rief die Gottheit aus; Was habt ihr jetzt verbrochen;", "tokens": ["So", "rief", "die", "Got\u00b7theit", "aus", ";", "Was", "habt", "ihr", "jetzt", "ver\u00b7bro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$.", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die S\u00fcnde war vollbracht, die Strafe folgte gleich;", "tokens": ["Die", "S\u00fcn\u00b7de", "war", "voll\u00b7bracht", ",", "die", "Stra\u00b7fe", "folg\u00b7te", "gleich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Engel jagte sie von Lust und Gl\u00fcck und Reich;", "tokens": ["Ein", "En\u00b7gel", "jag\u00b7te", "sie", "von", "Lust", "und", "Gl\u00fcck", "und", "Reich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ja noch ein h\u00e4rter Wort ward \u00fcber sie gesprochen:", "tokens": ["Ja", "noch", "ein", "h\u00e4r\u00b7ter", "Wort", "ward", "\u00fc\u00b7ber", "sie", "ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "ADJA", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es hie\u00df: weil ihr gefehlt, und wieder mich gethan,", "tokens": ["Es", "hie\u00df", ":", "weil", "ihr", "ge\u00b7fehlt", ",", "und", "wie\u00b7der", "mich", "ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "PPER", "VVPP", "$,", "KON", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So solt ihr kr\u00e4ncklich seyn: Ihr werdet sterben m\u00fcssen.", "tokens": ["So", "solt", "ihr", "kr\u00e4n\u00b7ck\u00b7lich", "seyn", ":", "Ihr", "wer\u00b7det", "ster\u00b7ben", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$.", "PPER", "VAFIN", "VVINF", "VMINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Die\u00df scharfe Wort geht nun auch alle Menschen an,", "tokens": ["Die\u00df", "schar\u00b7fe", "Wort", "geht", "nun", "auch", "al\u00b7le", "Men\u00b7schen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "VVFIN", "ADV", "ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und dieser alte Bund vermag nichts auszuschliessen.", "tokens": ["Und", "die\u00b7ser", "al\u00b7te", "Bund", "ver\u00b7mag", "nichts", "aus\u00b7zu\u00b7schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "VVFIN", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ja dieser Bund besteht, so lang der Erden-Krei\u00df,", "tokens": ["Ja", "die\u00b7ser", "Bund", "be\u00b7steht", ",", "so", "lang", "der", "Er\u00b7den\u00b7Krei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PDAT", "NN", "VVFIN", "$,", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Menschen, Gras und Laub und Creaturen weis,", "tokens": ["Von", "Men\u00b7schen", ",", "Gras", "und", "Laub", "und", "Crea\u00b7tu\u00b7ren", "weis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und das gew\u00f6lbte Rund um seine Achsen gehet.", "tokens": ["Und", "das", "ge\u00b7w\u00f6lb\u00b7te", "Rund", "um", "sei\u00b7ne", "Ach\u00b7sen", "ge\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nun hat die h\u00f6chste Kraft mit Flei\u00df und Vorbedacht,", "tokens": ["Nun", "hat", "die", "h\u00f6chs\u00b7te", "Kraft", "mit", "Flei\u00df", "und", "Vor\u00b7be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dem Leben Stund und Zeit, und Ziel und Ma\u00df gemacht,", "tokens": ["Dem", "Le\u00b7ben", "Stund", "und", "Zeit", ",", "und", "Ziel", "und", "Ma\u00df", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,", "KON", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So, da\u00df der Schlu\u00df so fest, als wie der Bund bestehet.", "tokens": ["So", ",", "da\u00df", "der", "Schlu\u00df", "so", "fest", ",", "als", "wie", "der", "Bund", "be\u00b7ste\u00b7het", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "ADV", "PTKVZ", "$,", "KOUS", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dort setzt der W\u00e4chter-Rath das Ziel nicht \u00fcberein.", "tokens": ["Dort", "setzt", "der", "W\u00e4ch\u00b7ter\u00b7Rath", "das", "Ziel", "nicht", "\u00fc\u00b7be\u00b7re\u00b7in", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "Der zahlet seine Schuld bey Silbergrauen Haaren,", "tokens": ["Der", "zah\u00b7let", "sei\u00b7ne", "Schuld", "bey", "Sil\u00b7ber\u00b7grau\u00b7en", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dort mu\u00df ein kleines Kind ein Raub des Todes seyn,", "tokens": ["Dort", "mu\u00df", "ein", "klei\u00b7nes", "Kind", "ein", "Raub", "des", "To\u00b7des", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Oft stirbt ein J\u00fcngling auch in seinen besten Jahren.", "tokens": ["Oft", "stirbt", "ein", "J\u00fcng\u00b7ling", "auch", "in", "sei\u00b7nen", "bes\u00b7ten", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dem aber ohngeacht versuchet doch und sinnt", "tokens": ["Dem", "a\u00b7ber", "ohn\u00b7ge\u00b7acht", "ver\u00b7su\u00b7chet", "doch", "und", "sinnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADJD", "VVFIN", "ADV", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein aufgeweckter Geist, und kluges Menschen-Kind,", "tokens": ["Ein", "auf\u00b7ge\u00b7weck\u00b7ter", "Geist", ",", "und", "klu\u00b7ges", "Men\u00b7schen\u00b7Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der starken Todes-Hand mit Macht zu wiederstreben.", "tokens": ["Der", "star\u00b7ken", "To\u00b7des\u00b7Hand", "mit", "Macht", "zu", "wie\u00b7der\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es forschet alles aus, was nur die Erde tr\u00e4gt,", "tokens": ["Es", "for\u00b7schet", "al\u00b7les", "aus", ",", "was", "nur", "die", "Er\u00b7de", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKVZ", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und was so Feld als Wald vor Seltenheiten hegt,", "tokens": ["Und", "was", "so", "Feld", "als", "Wald", "vor", "Sel\u00b7ten\u00b7hei\u00b7ten", "hegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "NN", "KOUS", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das mu\u00df ihm St\u00e4rk und Kraft nach seiner Absicht geben.", "tokens": ["Das", "mu\u00df", "ihm", "St\u00e4rk", "und", "Kraft", "nach", "sei\u00b7ner", "Ab\u00b7sicht", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die W\u00fcnsche treffen zu, die theure Arzeney", "tokens": ["Die", "W\u00fcn\u00b7sche", "tref\u00b7fen", "zu", ",", "die", "theu\u00b7re", "Ar\u00b7ze\u00b7ney"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Schl\u00e4gt in der Krankheit an, die Macht des Todes weichet.", "tokens": ["Schl\u00e4gt", "in", "der", "Krank\u00b7heit", "an", ",", "die", "Macht", "des", "To\u00b7des", "wei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.9": {"text": "Kommt aber nun das Ziel und unser End herbey;", "tokens": ["Kommt", "a\u00b7ber", "nun", "das", "Ziel", "und", "un\u00b7ser", "End", "her\u00b7bey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So ist die Kunst umsonst, man stirbet und erbleichet.", "tokens": ["So", "ist", "die", "Kunst", "um\u00b7sonst", ",", "man", "stir\u00b7bet", "und", "er\u00b7blei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "$,", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur schade da\u00df der Tod ohn allen Unterschied", "tokens": ["Nur", "scha\u00b7de", "da\u00df", "der", "Tod", "ohn", "al\u00b7len", "Un\u00b7ter\u00b7schied"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOUS", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Ansehn der Person die C\u00f6rper zu sich zieht,", "tokens": ["Und", "An\u00b7sehn", "der", "Per\u00b7son", "die", "C\u00f6r\u00b7per", "zu", "sich", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und weder Wissenschaft, noch Stand, noch Tugend scheuet.", "tokens": ["Und", "we\u00b7der", "Wis\u00b7sen\u00b7schaft", ",", "noch", "Stand", ",", "noch", "Tu\u00b7gend", "scheu\u00b7et", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KON", "NN", "$,", "ADV", "NN", "$,", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erbla\u00dft ein frommer F\u00fcrst; so lebet der Tyrann;", "tokens": ["Er\u00b7bla\u00dft", "ein", "from\u00b7mer", "F\u00fcrst", ";", "so", "le\u00b7bet", "der", "Ty\u00b7rann", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Hier herrscht der Aberwitz; dort stirbt ein Weiser Mann;", "tokens": ["Hier", "herrscht", "der", "A\u00b7ber\u00b7witz", ";", "dort", "stirbt", "ein", "Wei\u00b7ser", "Mann", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hier weinet, heult und klagt, \u00e4chzt, winselt, seufzt und schreyet", "tokens": ["Hier", "wei\u00b7net", ",", "heult", "und", "klagt", ",", "\u00e4chzt", ",", "win\u00b7selt", ",", "seufzt", "und", "schre\u00b7yet"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein hart geplagter Mann nach seiner Grabes-Th\u00fcr;", "tokens": ["Ein", "hart", "ge\u00b7plag\u00b7ter", "Mann", "nach", "sei\u00b7ner", "Gra\u00b7bes\u00b7T\u00b7h\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Allein an seiner statt geht eine Sch\u00f6nheit unter.", "tokens": ["Al\u00b7lein", "an", "sei\u00b7ner", "statt", "geht", "ei\u00b7ne", "Sch\u00f6n\u00b7heit", "un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dort sinkt ein Tugend-Bild: der B\u00f6se gr\u00fcnt herf\u00fcr;", "tokens": ["Dort", "sinkt", "ein", "Tu\u00b7gen\u00b7dBild", ":", "der", "B\u00f6\u00b7se", "gr\u00fcnt", "her\u00b7f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein Jonathan f\u00e4llt um; ein Judas bleibet munter.", "tokens": ["Ein", "Jo\u00b7na\u00b7than", "f\u00e4llt", "um", ";", "ein", "Ju\u00b7das", "blei\u00b7bet", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.",