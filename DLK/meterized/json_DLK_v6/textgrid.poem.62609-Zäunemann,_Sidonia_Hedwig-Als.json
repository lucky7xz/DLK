 "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Allwo dein Geist bey Gott in Ruh und Freude lebt;", "tokens": ["All\u00b7wo", "dein", "Geist", "bey", "Gott", "in", "Ruh", "und", "Freu\u00b7de", "lebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wir aber m\u00fcssen noch den Dornen-Acker bauen.", "tokens": ["Wir", "a\u00b7ber", "m\u00fcs\u00b7sen", "noch", "den", "Dor\u00b7nen\u00b7A\u00b7cker", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}