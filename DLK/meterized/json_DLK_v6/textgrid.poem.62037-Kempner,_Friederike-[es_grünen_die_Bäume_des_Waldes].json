{"textgrid.poem.62037": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[es gr\u00fcnen die B\u00e4ume des Waldes]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gr\u00fcnen die B\u00e4ume des Waldes,", "tokens": ["Es", "gr\u00fc\u00b7nen", "die", "B\u00e4u\u00b7me", "des", "Wal\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Es k\u00fcndigt der Fr\u00fchling sich an,", "tokens": ["Es", "k\u00fcn\u00b7digt", "der", "Fr\u00fch\u00b7ling", "sich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Hinweg mit dem frostigen Winter,", "tokens": ["Hin\u00b7weg", "mit", "dem", "fros\u00b7ti\u00b7gen", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der Fr\u00fchling ist ein sanfter Mann!", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "ein", "sanf\u00b7ter", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die langen goldnen Strahlen,", "tokens": ["Die", "lan\u00b7gen", "gold\u00b7nen", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie sind wie ein langes Haar!", "tokens": ["Sie", "sind", "wie", "ein", "lan\u00b7ges", "Haar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Veilchen im tiefen Grase", "tokens": ["Die", "Veil\u00b7chen", "im", "tie\u00b7fen", "Gra\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sind blau, wie ein Augenpaar!", "tokens": ["Sind", "blau", ",", "wie", "ein", "Au\u00b7gen\u00b7paar", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}