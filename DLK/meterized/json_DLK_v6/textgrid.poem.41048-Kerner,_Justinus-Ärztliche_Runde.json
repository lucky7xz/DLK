{"textgrid.poem.41048": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "\u00c4rztliche Runde", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh' ich in der Mitternacht", "tokens": ["Geh'", "ich", "in", "der", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch der H\u00e4user enge Reihn", "tokens": ["Durch", "der", "H\u00e4u\u00b7ser", "en\u00b7ge", "Reihn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hin, wo noch ein Kranker wacht", "tokens": ["Hin", ",", "wo", "noch", "ein", "Kran\u00b7ker", "wacht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bei der Lampe mattem Schein,", "tokens": ["Bei", "der", "Lam\u00b7pe", "mat\u00b7tem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Blick' ich an die Fenster oft,", "tokens": ["Blick'", "ich", "an", "die", "Fens\u00b7ter", "oft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hinter denen fruchtlos ich", "tokens": ["Hin\u00b7ter", "de\u00b7nen", "frucht\u00b7los", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ADJD", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf Metall und Kraut gehofft,", "tokens": ["Auf", "Me\u00b7tall", "und", "Kraut", "ge\u00b7hofft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lausch' ich, und es reget sich.", "tokens": ["Lausch'", "ich", ",", "und", "es", "re\u00b7get", "sich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KON", "PPER", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und es kommt herab im Haus,", "tokens": ["Und", "es", "kommt", "her\u00b7ab", "im", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als h\u00e4tt' ich geklopfet an \u2013", "tokens": ["Als", "h\u00e4tt'", "ich", "ge\u00b7klop\u00b7fet", "an", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "VVPP", "APPR", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein Verstorbner tritt heraus,", "tokens": ["Ein", "Ver\u00b7storb\u00b7ner", "tritt", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gehet stumm mit mir die Bahn.", "tokens": ["Ge\u00b7het", "stumm", "mit", "mir", "die", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und mein H\u00fcndlein stutzt und bellt,", "tokens": ["Und", "mein", "H\u00fcnd\u00b7lein", "stutzt", "und", "bellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Will mit mir nicht weitergehn.", "tokens": ["Will", "mit", "mir", "nicht", "wei\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wolken, fliegt vom Himmelszelt!", "tokens": ["Wol\u00b7ken", ",", "fliegt", "vom", "Him\u00b7mels\u00b7zelt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df die Sterne leuchtend stehn.", "tokens": ["Da\u00df", "die", "Ster\u00b7ne", "leuch\u00b7tend", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}