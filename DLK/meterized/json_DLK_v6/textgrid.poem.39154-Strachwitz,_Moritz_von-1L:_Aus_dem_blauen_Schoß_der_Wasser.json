{"textgrid.poem.39154": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus dem blauen Scho\u00df der Wasser", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus dem blauen Scho\u00df der Wasser", "tokens": ["Aus", "dem", "blau\u00b7en", "Scho\u00df", "der", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00e4hrt Poseidon rasch und pl\u00f6tzlich;", "tokens": ["F\u00e4hrt", "Po\u00b7sei\u00b7don", "rasch", "und", "pl\u00f6tz\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um die Stirn dem Weltumfasser", "tokens": ["Um", "die", "Stirn", "dem", "Welt\u00b7um\u00b7fas\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weht das Haupthaar wild, entsetzlich;", "tokens": ["Weht", "das", "Haupt\u00b7haar", "wild", ",", "ent\u00b7setz\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von dem Bart des Mastzersplitt'rers", "tokens": ["Von", "dem", "Bart", "des", "Mast\u00b7zer\u00b7splitt'\u00b7rers"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00e4uft der Flutschaum Aphroditens,", "tokens": ["Tr\u00e4uft", "der", "Flutsc\u00b7haum", "A\u00b7phro\u00b7di\u00b7tens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Um das Ro\u00df des Erdersch\u00fctt'rers", "tokens": ["Um", "das", "Ro\u00df", "des", "Er\u00b7der\u00b7sch\u00fctt'\u00b7rers"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schw\u00e4rmt das Meervolk Amphitritens.", "tokens": ["Schw\u00e4rmt", "das", "Meer\u00b7volk", "Am\u00b7phit\u00b7ri\u00b7tens", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Das ist dein Werk, Atrytone!", "tokens": ["Das", "ist", "dein", "Werk", ",", "At\u00b7ry\u00b7to\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "NE", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Tochter du, vom Blitzversender,", "tokens": ["Toch\u00b7ter", "du", ",", "vom", "Blitz\u00b7ver\u00b7sen\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf des Weltalls Wellenzone", "tokens": ["Auf", "des", "Welt\u00b7alls", "Wel\u00b7len\u00b7zo\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Segelt der Zyklopenblender.", "tokens": ["Se\u00b7gelt", "der", "Zyk\u00b7lo\u00b7pen\u00b7blen\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "St\u00fcrmisch walle, Wogenbusen,", "tokens": ["St\u00fcr\u00b7misch", "wal\u00b7le", ",", "Wo\u00b7gen\u00b7bu\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "St\u00e4ube, Meer, in Schaum und Flocken!", "tokens": ["St\u00e4u\u00b7be", ",", "Meer", ",", "in", "Schaum", "und", "Flo\u00b7cken", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gleich den Nattern der Medusen", "tokens": ["Gleich", "den", "Nat\u00b7tern", "der", "Me\u00b7du\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sch\u00fcttle deine Silberlocken!", "tokens": ["Sch\u00fctt\u00b7le", "dei\u00b7ne", "Sil\u00b7berl\u00b7o\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf des Atlas Himmelsfirne", "tokens": ["Auf", "des", "At\u00b7las", "Him\u00b7mels\u00b7fir\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Regt der Notos sein Gefieder,", "tokens": ["Regt", "der", "No\u00b7tos", "sein", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sausend von des Berges Stirne", "tokens": ["Sau\u00b7send", "von", "des", "Ber\u00b7ges", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Meerflut st\u00fcrzt er nieder,", "tokens": ["In", "die", "Meer\u00b7flut", "st\u00fcrzt", "er", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Legt sich grimmig und zerkrallend", "tokens": ["Legt", "sich", "grim\u00b7mig", "und", "zer\u00b7kral\u00b7lend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "KON", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An Thalatta's volle Br\u00fcste,", "tokens": ["An", "Tha\u00b7lat\u00b7ta's", "vol\u00b7le", "Br\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Mit dem Fittich donnerschallend", "tokens": ["Mit", "dem", "Fit\u00b7tich", "don\u00b7ner\u00b7schal\u00b7lend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Peitscht er Hella's Felsenk\u00fcste.", "tokens": ["Peitscht", "er", "Hel\u00b7la's", "Fel\u00b7sen\u00b7k\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Rasend in des Untiers Klammern", "tokens": ["Ra\u00b7send", "in", "des", "Un\u00b7tiers", "Klam\u00b7mern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hebt die Brandung an zu pochen,", "tokens": ["Hebt", "die", "Bran\u00b7dung", "an", "zu", "po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus des Abgrunds Wogenkammern", "tokens": ["Aus", "des", "Ab\u00b7grunds", "Wo\u00b7gen\u00b7kam\u00b7mern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4\u00dft sie Strudelwellen kochen.", "tokens": ["L\u00e4\u00dft", "sie", "Stru\u00b7del\u00b7wel\u00b7len", "ko\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aufw\u00e4rts zu des Himmels Lichtung", "tokens": ["Auf\u00b7w\u00e4rts", "zu", "des", "Him\u00b7mels", "Lich\u00b7tung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wirft im Zorn sie Mast und Schiff mir,", "tokens": ["Wirft", "im", "Zorn", "sie", "Mast", "und", "Schiff", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "NN", "KON", "NN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis in tosender Vernichtung", "tokens": ["Bis", "in", "to\u00b7sen\u00b7der", "Ver\u00b7nich\u00b7tung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mast und Schiff zerschellt am Riff mir.", "tokens": ["Mast", "und", "Schiff", "zer\u00b7schellt", "am", "Riff", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "APPRART", "NN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie der Schnee auf Erymanthos,", "tokens": ["Wie", "der", "Schnee", "auf", "E\u00b7ry\u00b7man\u00b7thos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NE", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der in Morgenstrahlen leuchtet,", "tokens": ["Der", "in", "Mor\u00b7gen\u00b7strah\u00b7len", "leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Silberschwan des Xanthos,", "tokens": ["Wie", "der", "Sil\u00b7ber\u00b7schwan", "des", "Xan\u00b7thos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der im Strom die Schwinge feuchtet,", "tokens": ["Der", "im", "Strom", "die", "Schwin\u00b7ge", "feuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hebt ein Hals, vom hellsten Scheine", "tokens": ["Hebt", "ein", "Hals", ",", "vom", "hells\u00b7ten", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Blendend, sich im Wogenschlage,", "tokens": ["Blen\u00b7dend", ",", "sich", "im", "Wo\u00b7gen\u00b7schla\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Steigst du selber, Wunderreine,", "tokens": ["Steigst", "du", "sel\u00b7ber", ",", "Wun\u00b7der\u00b7rei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als Leukothea zutage.", "tokens": ["Als", "Leu\u00b7ko\u00b7thea", "zu\u00b7ta\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mit dem Schleier deiner Milde", "tokens": ["Mit", "dem", "Schlei\u00b7er", "dei\u00b7ner", "Mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast du z\u00e4rtlich mich umwoben,", "tokens": ["Hast", "du", "z\u00e4rt\u00b7lich", "mich", "um\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus dem heil'gen Meergefilde", "tokens": ["Aus", "dem", "heil'\u00b7gen", "Meer\u00b7ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rettend mich emporgehoben,", "tokens": ["Ret\u00b7tend", "mich", "em\u00b7por\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df die Wellen mit Gekose", "tokens": ["Da\u00df", "die", "Wel\u00b7len", "mit", "Ge\u00b7ko\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich an Scherias Borde trieben;", "tokens": ["Mich", "an", "Sche\u00b7ri\u00b7as", "Bor\u00b7de", "trie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "NE", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Doch du selber, Sch\u00f6ne, Lose!", "tokens": ["Doch", "du", "sel\u00b7ber", ",", "Sch\u00f6\u00b7ne", ",", "Lo\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$,", "NN", "$,", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bist im Meer zur\u00fcckgeblieben.", "tokens": ["Bist", "im", "Meer", "zu\u00b7r\u00fcck\u00b7ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}