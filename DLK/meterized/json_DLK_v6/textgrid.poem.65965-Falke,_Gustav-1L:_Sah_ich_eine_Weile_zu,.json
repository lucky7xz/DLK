{"textgrid.poem.65965": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sah ich eine Weile zu,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sah ich eine Weile zu,", "tokens": ["Sah", "ich", "ei\u00b7ne", "Wei\u00b7le", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die Funken stieben;", "tokens": ["Wie", "die", "Fun\u00b7ken", "stie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "R\u00e4der, Riemen ohne Ruh", "tokens": ["R\u00e4\u00b7der", ",", "Rie\u00b7men", "oh\u00b7ne", "Ruh"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch den Tag getrieben.", "tokens": ["Durch", "den", "Tag", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "H\u00f6rt' ich eine Weile, wie", "tokens": ["H\u00f6rt'", "ich", "ei\u00b7ne", "Wei\u00b7le", ",", "wie"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PWAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Maschinen st\u00f6hnen,", "tokens": ["Die", "Ma\u00b7schi\u00b7nen", "st\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Unter ihrer Melodie", "tokens": ["Un\u00b7ter", "ih\u00b7rer", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Pfosten dr\u00f6hnen.", "tokens": ["Al\u00b7le", "Pfos\u00b7ten", "dr\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Stampf und Sto\u00df und Surr und Summ", "tokens": ["Stampf", "und", "Sto\u00df", "und", "Surr", "und", "Summ"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NE", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Machten mich beklommen,", "tokens": ["Mach\u00b7ten", "mich", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ging zum Thor hinaus ich stumm,", "tokens": ["Ging", "zum", "Thor", "hin\u00b7aus", "ich", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "APZR", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "War so froh gekommen.", "tokens": ["War", "so", "froh", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Drau\u00dfen sah in Staub und Ru\u00df", "tokens": ["Drau\u00b7\u00dfen", "sah", "in", "Staub", "und", "Ru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich ein M\u00e4dchen stehen;", "tokens": ["Ich", "ein", "M\u00e4d\u00b7chen", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "War so eben fl\u00fcgge. Muss", "tokens": ["War", "so", "e\u00b7ben", "fl\u00fcg\u00b7ge", ".", "Muss"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "ADV", "ADV", "VVFIN", "$.", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jugend so vergehen?", "tokens": ["Ju\u00b7gend", "so", "ver\u00b7ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Fort! nur fort! Schon gr\u00fc\u00dft mich hoch", "tokens": ["Fort", "!", "nur", "fort", "!", "Schon", "gr\u00fc\u00dft", "mich", "hoch"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Freier Wipfel Brausen,", "tokens": ["Frei\u00b7er", "Wip\u00b7fel", "Brau\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Aber immer h\u00f6r' ich noch", "tokens": ["A\u00b7ber", "im\u00b7mer", "h\u00f6r'", "ich", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00e4dersurrn und -sausen.", "tokens": ["R\u00e4\u00b7der\u00b7surrn", "und", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}