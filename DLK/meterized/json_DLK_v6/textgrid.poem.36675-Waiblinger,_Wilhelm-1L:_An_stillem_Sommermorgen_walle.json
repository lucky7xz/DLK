{"textgrid.poem.36675": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: An stillem Sommermorgen walle", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An stillem Sommermorgen walle", "tokens": ["An", "stil\u00b7lem", "Som\u00b7mer\u00b7mor\u00b7gen", "wal\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So gern ich durch die Einsamkeit,", "tokens": ["So", "gern", "ich", "durch", "die", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo sich des Tempels S\u00e4ulenhalle,", "tokens": ["Wo", "sich", "des", "Tem\u00b7pels", "S\u00e4u\u00b7len\u00b7hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem G\u00f6ttervater einst geweiht,", "tokens": ["Dem", "G\u00f6t\u00b7ter\u00b7va\u00b7ter", "einst", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo sich in tausendj\u00e4hr'ger Trauer", "tokens": ["Wo", "sich", "in", "tau\u00b7send\u00b7j\u00e4hr'\u00b7ger", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Eintracht nun zertr\u00fcmmert Haus,", "tokens": ["Der", "Ein\u00b7tracht", "nun", "zer\u00b7tr\u00fcm\u00b7mert", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Kapitoles ew'ge Dauer,", "tokens": ["Des", "Ka\u00b7pi\u00b7to\u00b7les", "ew'\u00b7ge", "Dau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Aus Schutt erhebt und \u00f6dem Graus.", "tokens": ["Aus", "Schutt", "er\u00b7hebt", "und", "\u00f6\u00b7dem", "Graus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gern blick' ich, wenn der D\u00e4mm'rung Schleier", "tokens": ["Gern", "blick'", "ich", ",", "wenn", "der", "D\u00e4m\u00b7m'\u00b7rung", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die sieben H\u00fcgel schon umwebt,", "tokens": ["Die", "sie\u00b7ben", "H\u00fc\u00b7gel", "schon", "um\u00b7webt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Grabe m\u00e4chtiger und freier", "tokens": ["Dem", "Gra\u00b7be", "m\u00e4ch\u00b7ti\u00b7ger", "und", "frei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Geist des Alterthums entschwebt,", "tokens": ["Der", "Geist", "des", "Al\u00b7ter\u00b7thums", "ent\u00b7schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hinunter in die dunkeln Tiefen,", "tokens": ["Hin\u00b7un\u00b7ter", "in", "die", "dun\u00b7keln", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo mir, zum ernsten Freund erw\u00e4hlt,", "tokens": ["Wo", "mir", ",", "zum", "erns\u00b7ten", "Freund", "er\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von jenen Helden, die entschliefen,", "tokens": ["Von", "je\u00b7nen", "Hel\u00b7den", ",", "die", "ent\u00b7schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der alte Tibergott erz\u00e4hlt.", "tokens": ["Der", "al\u00b7te", "Ti\u00b7ber\u00b7gott", "er\u00b7z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gern wandl' ich auf verlassnen Wegen,", "tokens": ["Gern", "wandl'", "ich", "auf", "ver\u00b7lass\u00b7nen", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die kaum ein tr\u00fcber Schein erhellt,", "tokens": ["Die", "kaum", "ein", "tr\u00fc\u00b7ber", "Schein", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit schauderndem Gef\u00fchl entgegen", "tokens": ["Mit", "schau\u00b7dern\u00b7dem", "Ge\u00b7f\u00fchl", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Colosseums Tr\u00fcmmerwelt;", "tokens": ["Des", "Co\u00b7los\u00b7se\u00b7ums", "Tr\u00fcm\u00b7mer\u00b7welt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn furchtsam von den wilden Schrecken", "tokens": ["Wenn", "furcht\u00b7sam", "von", "den", "wil\u00b7den", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des schwarzen Ungeth\u00fcms verscheucht,", "tokens": ["Des", "schwar\u00b7zen", "Un\u00b7ge\u00b7th\u00fcms", "ver\u00b7scheucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der scheue Mond, sich zu verstecken,", "tokens": ["Der", "scheu\u00b7e", "Mond", ",", "sich", "zu", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In einer Wolke Schoo\u00df entfleucht.", "tokens": ["In", "ei\u00b7ner", "Wol\u00b7ke", "Schoo\u00df", "ent\u00b7fleucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Oft da\u00df der furchtbaren Gestalten", "tokens": ["Oft", "da\u00df", "der", "furcht\u00b7ba\u00b7ren", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ehrw\u00fcrd'ger Ernst mein Herz erf\u00fcllt,", "tokens": ["Ehr\u00b7w\u00fcrd'\u00b7ger", "Ernst", "mein", "Herz", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mir der Gottheit strafend Walten", "tokens": ["Und", "mir", "der", "Got\u00b7theit", "stra\u00b7fend", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr hoher Sehergeist enth\u00fcllt,", "tokens": ["Ihr", "ho\u00b7her", "Se\u00b7her\u00b7geist", "ent\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Michel Angelos Propheten", "tokens": ["Wenn", "Mi\u00b7chel", "An\u00b7ge\u00b7los", "Pro\u00b7phe\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gleich St\u00fcrmen aus den Himmeln wehn,", "tokens": ["Gleich", "St\u00fcr\u00b7men", "aus", "den", "Him\u00b7meln", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und bei des Weltgerichts Trompeten", "tokens": ["Und", "bei", "des", "Welt\u00b7ge\u00b7richts", "Trom\u00b7pe\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Todten aus dem Grab erstehn.", "tokens": ["Die", "Tod\u00b7ten", "aus", "dem", "Grab", "er\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Oft da\u00df ich selig mich erhebe", "tokens": ["Oft", "da\u00df", "ich", "se\u00b7lig", "mich", "er\u00b7he\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Tabors heiligem Gesicht,", "tokens": ["In", "Ta\u00b7bors", "hei\u00b7li\u00b7gem", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich dem sanften Geist erbebe,", "tokens": ["Da\u00df", "ich", "dem", "sanf\u00b7ten", "Geist", "er\u00b7be\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der \u00fcberstrahlt von reinem Licht,", "tokens": ["Der", "\u00fc\u00b7bers\u00b7trahlt", "von", "rei\u00b7nem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Gottes glanzumflossnem Sohne,", "tokens": ["Mit", "Got\u00b7tes", "glan\u00b7zum\u00b7floss\u00b7nem", "Soh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von seinen J\u00fcngern treu verehrt,", "tokens": ["Von", "sei\u00b7nen", "J\u00fcn\u00b7gern", "treu", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Angesicht vor Gottes Throne,", "tokens": ["Im", "An\u00b7ge\u00b7sicht", "vor", "Got\u00b7tes", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Erd' entschwebend, sich verkl\u00e4rt.", "tokens": ["Der", "Erd'", "ent\u00b7schwe\u00b7bend", ",", "sich", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich sah wie vom begr\u00fcnten Saume", "tokens": ["Ich", "sah", "wie", "vom", "be\u00b7gr\u00fcn\u00b7ten", "Sau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Felswand in gewalt'ger Wuth", "tokens": ["Der", "Fels\u00b7wand", "in", "ge\u00b7walt'\u00b7ger", "Wuth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dumpfdonnernd in zerst\u00e4ubtem Schaume", "tokens": ["Dum\u00b7pf\u00b7don\u00b7nernd", "in", "zer\u00b7st\u00e4ub\u00b7tem", "Schau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hinunterbraust des Anio Fluth,", "tokens": ["Hin\u00b7un\u00b7ter\u00b7braust", "des", "A\u00b7nio", "Fluth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie tief in uralt finstern Kl\u00fcften", "tokens": ["Wie", "tief", "in", "ur\u00b7alt", "fins\u00b7tern", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Meergott in den Wassern rauscht,", "tokens": ["Der", "Meer\u00b7gott", "in", "den", "Was\u00b7sern", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und oben in den milden L\u00fcften", "tokens": ["Und", "o\u00b7ben", "in", "den", "mil\u00b7den", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Im Tempel die Sibylle lauscht.", "tokens": ["Im", "Tem\u00b7pel", "die", "Si\u00b7byl\u00b7le", "lauscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn endlich an Dianens Bade", "tokens": ["Wenn", "end\u00b7lich", "an", "Di\u00b7a\u00b7nens", "Ba\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Alba's duft'gen Veilchenwald,", "tokens": ["Durch", "Al\u00b7ba's", "duft'\u00b7gen", "Veil\u00b7chen\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fernhin das blumige Gestade,", "tokens": ["Fern\u00b7hin", "das", "blu\u00b7mi\u00b7ge", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Echo Jubel wiederhallt,", "tokens": ["Das", "E\u00b7cho", "Ju\u00b7bel", "wie\u00b7der\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durchs Schattenlaub, o welch Entz\u00fccken!", "tokens": ["Durchs", "Schat\u00b7ten\u00b7laub", ",", "o", "welch", "Ent\u00b7z\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "FM", "PWAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Abends goldner Regen tr\u00e4uft,", "tokens": ["Des", "A\u00b7bends", "gold\u00b7ner", "Re\u00b7gen", "tr\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch blendend helle Bl\u00e4tterl\u00fccken", "tokens": ["Durch", "blen\u00b7dend", "hel\u00b7le", "Bl\u00e4t\u00b7ter\u00b7l\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Blick zum nahen Meere schweift, \u2013", "tokens": ["Der", "Blick", "zum", "na\u00b7hen", "Mee\u00b7re", "schweift", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch ohne Zagen, ohne Schwanken,", "tokens": ["Doch", "oh\u00b7ne", "Za\u00b7gen", ",", "oh\u00b7ne", "Schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weih' ich selbst in Elysium", "tokens": ["Weih'", "ich", "selbst", "in", "E\u00b7ly\u00b7si\u00b7um"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Nur Einem herrlichen Gedanken", "tokens": ["Nur", "Ei\u00b7nem", "herr\u00b7li\u00b7chen", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz zum treuen Heiligthum,", "tokens": ["Mein", "Herz", "zum", "treu\u00b7en", "Hei\u00b7lig\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob mir der Zauber aller Fernen", "tokens": ["Ob", "mir", "der", "Zau\u00b7ber", "al\u00b7ler", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und aller Meere sich erschlie\u00dft,", "tokens": ["Und", "al\u00b7ler", "Mee\u00b7re", "sich", "er\u00b7schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch glaub' ich, da\u00df ihn fliehn zu lernen", "tokens": ["Doch", "glaub'", "ich", ",", "da\u00df", "ihn", "fliehn", "zu", "ler\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVINF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": 