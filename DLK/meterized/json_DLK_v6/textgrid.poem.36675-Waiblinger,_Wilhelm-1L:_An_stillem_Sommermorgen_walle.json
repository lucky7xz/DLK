h wand,", "tokens": ["So", "schreck\u00b7lich", "z\u00fcr\u00b7nend", "los", "sich", "wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "PTKVZ", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verschlie\u00dfe meinem Klagetone", "tokens": ["Ver\u00b7schlie\u00b7\u00dfe", "mei\u00b7nem", "Kla\u00b7ge\u00b7to\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Ohr nicht, deutsches Vaterland!", "tokens": ["Dein", "Ohr", "nicht", ",", "deut\u00b7sches", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}