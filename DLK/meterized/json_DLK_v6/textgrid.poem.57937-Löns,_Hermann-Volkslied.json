{"textgrid.poem.57937": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Volkslied", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es steht ein Baum am Stephansstift,", "tokens": ["Es", "steht", "ein", "Baum", "am", "Ste\u00b7phans\u00b7stift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der hat drei gr\u00fcne \u00c4st',", "tokens": ["Der", "hat", "drei", "gr\u00fc\u00b7ne", "\u00c4st'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Einst standen ein'ge Tausend hier,", "tokens": ["Einst", "stan\u00b7den", "ein'\u00b7ge", "Tau\u00b7send", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist der sch\u00e4bige Rest.", "tokens": ["Das", "ist", "der", "sch\u00e4\u00b7bi\u00b7ge", "Rest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Da sitzt ein kleiner Vogel drauf,", "tokens": ["Da", "sitzt", "ein", "klei\u00b7ner", "Vo\u00b7gel", "drauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der pfeift nicht gerade sch\u00f6n,", "tokens": ["Der", "pfeift", "nicht", "ge\u00b7ra\u00b7de", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es ist ein Spatz, denn Fink und Star", "tokens": ["Es", "ist", "ein", "Spatz", ",", "denn", "Fink", "und", "Star"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind l\u00e4ngst nicht mehr zu sehn.", "tokens": ["Sind", "l\u00e4ngst", "nicht", "mehr", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Vogel sitzt in seinem Nest", "tokens": ["Der", "Vo\u00b7gel", "sitzt", "in", "sei\u00b7nem", "Nest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl auf dem letzten Baum;", "tokens": ["Wohl", "auf", "dem", "letz\u00b7ten", "Baum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach Sch\u00e4tzel, bin ich h\u00fchnerblind,", "tokens": ["Ach", "Sch\u00e4t\u00b7zel", ",", "bin", "ich", "h\u00fch\u00b7ner\u00b7blind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Oder ist es nur ein Traum.", "tokens": ["O\u00b7der", "ist", "es", "nur", "ein", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Es ist kein Traum, den sch\u00f6nen Wald,", "tokens": ["Es", "ist", "kein", "Traum", ",", "den", "sch\u00f6\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So dichte bei der Stadt,", "tokens": ["So", "dich\u00b7te", "bei", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verkaufte man und trieb ihn ab,", "tokens": ["Ver\u00b7kauf\u00b7te", "man", "und", "trieb", "ihn", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das tat der Magistrat.", "tokens": ["Das", "tat", "der", "Ma\u00b7gist\u00b7rat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Baum, der steht beim Stephansstift,", "tokens": ["Der", "Baum", ",", "der", "steht", "beim", "Ste\u00b7phans\u00b7stift", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und still mir mein Verstand,", "tokens": ["Und", "still", "mir", "mein", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Vogel pfeift ein Schelmenlied,", "tokens": ["Der", "Vo\u00b7gel", "pfeift", "ein", "Schel\u00b7men\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man h\u00f6rt's im ganzen Land.", "tokens": ["Man", "h\u00f6rt's", "im", "gan\u00b7zen", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und wer es h\u00f6rt, der denkt sein Teil", "tokens": ["Und", "wer", "es", "h\u00f6rt", ",", "der", "denkt", "sein", "Teil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und spricht: \u00bb's ist ein Skandal!", "tokens": ["Und", "spricht", ":", "\u00bb", "'s", "ist", "ein", "Skan\u00b7dal", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Woanders pflanzt man B\u00e4ume an,", "tokens": ["Wo\u00b7an\u00b7ders", "pflanzt", "man", "B\u00e4u\u00b7me", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier hackt man alles kahl.", "tokens": ["Hier", "hackt", "man", "al\u00b7les", "kahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Woanders geht man klug zu Werk", "tokens": ["Wo\u00b7an\u00b7ders", "geht", "man", "klug", "zu", "Werk"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00e4lt vern\u00fcnftig Haus,", "tokens": ["Und", "h\u00e4lt", "ver\u00b7n\u00fcnf\u00b7tig", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch hier ...\u00ab, da fliegt der Vogel weg,", "tokens": ["Doch", "hier", "...", "\u00ab", ",", "da", "fliegt", "der", "Vo\u00b7gel", "weg", ","], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "$(", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Lied ist deshalb aus.", "tokens": ["Das", "Lied", "ist", "des\u00b7halb", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}