{"dta.poem.10868": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Noch zwo dergleichen  Arien,  \n von Mr. Teleman componiret.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Stille, die den Wald erf\u00fcllt,", "tokens": ["Die", "Stil\u00b7le", ",", "die", "den", "Wald", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der holden Unschuld sanftes Bild,", "tokens": ["Der", "hol\u00b7den", "Un\u00b7schuld", "sanf\u00b7tes", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist nicht von froher Anmuth leer.", "tokens": ["Ist", "nicht", "von", "fro\u00b7her", "An\u00b7muth", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der kleinen V\u00f6gel muntres Heer", "tokens": ["Der", "klei\u00b7nen", "V\u00f6\u00b7gel", "mun\u00b7tres", "Heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "L\u00e4\u00dft tausend s\u00fcsse T\u00f6n erklingen,", "tokens": ["L\u00e4\u00dft", "tau\u00b7send", "s\u00fcs\u00b7se", "T\u00f6n", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So kann auch ein gela\u00dfnes Herz,", "tokens": ["So", "kann", "auch", "ein", "ge\u00b7la\u00df\u00b7nes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Recht, bey zugela\u00dfnem Scherz,", "tokens": ["Mit", "Recht", ",", "bey", "zu\u00b7ge\u00b7la\u00df\u00b7nem", "Scherz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gesellig lachen, fr\u00f6lich singen,", "tokens": ["Ge\u00b7sel\u00b7lig", "la\u00b7chen", ",", "fr\u00f6\u00b7lich", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Weil sonst die Tugend gr\u00e4mlich w\u00e4r.", "tokens": ["Weil", "sonst", "die", "Tu\u00b7gend", "gr\u00e4m\u00b7lich", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}