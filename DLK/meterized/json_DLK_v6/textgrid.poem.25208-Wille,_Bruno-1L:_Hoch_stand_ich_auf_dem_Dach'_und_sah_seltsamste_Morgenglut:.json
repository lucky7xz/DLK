{"textgrid.poem.25208": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hoch stand ich auf dem Dach' und sah seltsamste Morgenglut:", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch stand ich auf dem Dach' und sah seltsamste Morgenglut:", "tokens": ["Hoch", "stand", "ich", "auf", "dem", "Da\u00b7ch'", "und", "sah", "selt\u00b7sams\u00b7te", "Mor\u00b7gen\u00b7glut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Rings wogte \u00fcber die H\u00e4user hin ein Meer von Brand und Blut.", "tokens": ["Rings", "wog\u00b7te", "\u00fc\u00b7ber", "die", "H\u00e4u\u00b7ser", "hin", "ein", "Meer", "von", "Brand", "und", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Es br\u00fcllte die schwarzrot qualmende Schlacht; mit zornigem Knattern schossen", "tokens": ["Es", "br\u00fcll\u00b7te", "die", "schwarz\u00b7rot", "qual\u00b7men\u00b7de", "Schlacht", ";", "mit", "zor\u00b7ni\u00b7gem", "Knat\u00b7tern", "schos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$.", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+--+-+--+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Behelmte Feinde zu uns empor; doch es trotzten fest die Genossen", "tokens": ["Be\u00b7helm\u00b7te", "Fein\u00b7de", "zu", "uns", "em\u00b7por", ";", "doch", "es", "trotz\u00b7ten", "fest", "die", "Ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "PPER", "PTKVZ", "$.", "KON", "PPER", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+--+-+--+-+--+-", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Wie Felsen im schlagenden Hagelsturm; verheerende Bomben schwangen sie", "tokens": ["Wie", "Fel\u00b7sen", "im", "schla\u00b7gen\u00b7den", "Ha\u00b7gel\u00b7sturm", ";", "ver\u00b7hee\u00b7ren\u00b7de", "Bom\u00b7ben", "schwan\u00b7gen", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPRART", "ADJA", "NN", "$.", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+--+--+-+-+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und manchmal durch das Schlachtgetos' die Marseillaise sangen sie.", "tokens": ["Und", "manch\u00b7mal", "durch", "das", "Schlacht\u00b7ge\u00b7to\u00b7s'", "die", "Mar\u00b7seil\u00b7laise", "san\u00b7gen", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+--++-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.7": {"text": "Ihr wollust-girrendes Mordlied pfiff eine Kugel an meinem Ohr;", "tokens": ["Ihr", "wol\u00b7lust\u00b7gir\u00b7ren\u00b7des", "Mord\u00b7lied", "pfiff", "ei\u00b7ne", "Ku\u00b7gel", "an", "mei\u00b7nem", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-++-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.8": {"text": "Da b\u00e4umte sich meine Seele j\u00e4h, gleich w\u00fctiger Schlange, empor,", "tokens": ["Da", "b\u00e4um\u00b7te", "sich", "mei\u00b7ne", "See\u00b7le", "j\u00e4h", ",", "gleich", "w\u00fc\u00b7ti\u00b7ger", "Schlan\u00b7ge", ",", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "ADJD", "$,", "ADV", "ADJA", "NN", "$,", "PTKVZ", "$,"], "meter": "-+--+-+-+-+--+--+", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "Den Sprengball zuckte die krallende Faust nach den feindlich st\u00fcrmenden Massen", "tokens": ["Den", "Spreng\u00b7ball", "zuck\u00b7te", "die", "kral\u00b7len\u00b7de", "Faust", "nach", "den", "feind\u00b7lich", "st\u00fcr\u00b7men\u00b7den", "Mas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+--+--+--+-+--+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Und schmi\u00df des Todes rei\u00dfende Saat hinunter mit jauchzendem Hassen.", "tokens": ["Und", "schmi\u00df", "des", "To\u00b7des", "rei\u00b7\u00dfen\u00b7de", "Saat", "hin\u00b7un\u00b7ter", "mit", "jauch\u00b7zen\u00b7dem", "Has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "APPO", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Und dumpf ...", "tokens": ["Und", "dumpf", "..."], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$("], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Ein Rollen ... ein Peitschengeklatsch und", "tokens": ["Ein", "Rol\u00b7len", "...", "ein", "Peit\u00b7schen\u00b7ge\u00b7klatsch", "und"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "NN", "KON"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Und sieh! die Morgensonne stralt ins offene Fenster herein;", "tokens": ["Und", "sieh", "!", "die", "Mor\u00b7gen\u00b7son\u00b7ne", "stralt", "ins", "of\u00b7fe\u00b7ne", "Fens\u00b7ter", "her\u00b7ein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+--+--+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Im Bette lieg' ich; \u2013 es war ein Traum!", "tokens": ["Im", "Bet\u00b7te", "lieg'", "ich", ";", "\u2013", "es", "war", "ein", "Traum", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$.", "$(", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nicht Kugeln, \u2013 die Schwalben girren", "tokens": ["Nicht", "Ku\u00b7geln", ",", "\u2013", "die", "Schwal\u00b7ben", "gir\u00b7ren"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "$(", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und schie\u00dfen um mein l\u00e4ndliches Dach, und droben im Mattblau schwirren", "tokens": ["Und", "schie\u00b7\u00dfen", "um", "mein", "l\u00e4nd\u00b7li\u00b7ches", "Dach", ",", "und", "dro\u00b7ben", "im", "Matt\u00b7blau", "schwir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,", "KON", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+---+--+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Lichtfrohe Lerchen. Durch thauige Flur trabt munter das Pferd mit dem Wagen;", "tokens": ["Licht\u00b7fro\u00b7he", "Ler\u00b7chen", ".", "Durch", "thau\u00b7i\u00b7ge", "Flur", "trabt", "mun\u00b7ter", "das", "Pferd", "mit", "dem", "Wa\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "APPR", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+--+--+-", "measure": "iambic.octa.plus.invert"}, "line.6": {"text": "Drauf sitzt der junge Bauer und schmaucht sein Pfeifchen mit Behagen.", "tokens": ["Drauf", "sitzt", "der", "jun\u00b7ge", "Bau\u00b7er", "und", "schmaucht", "sein", "Pfei\u00b7fchen", "mit", "Be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Und f\u00e4hrt so sicher hinein in die Welt.", "tokens": ["Und", "f\u00e4hrt", "so", "si\u00b7cher", "hin\u00b7ein", "in", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ich aber, ich seufze und schwanke", "tokens": ["Ich", "a\u00b7ber", ",", "ich", "seuf\u00b7ze", "und", "schwan\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Und bin auf bangem Lager hier ein zweifelnder Gedanke.", "tokens": ["Und", "bin", "auf", "ban\u00b7gem", "La\u00b7ger", "hier", "ein", "zwei\u00b7feln\u00b7der", "Ge\u00b7dan\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Noch h\u00e4lt der zornesglutende Traum mein Herz in banger Stockung,", "tokens": ["Noch", "h\u00e4lt", "der", "zor\u00b7nes\u00b7glu\u00b7ten\u00b7de", "Traum", "mein", "Herz", "in", "ban\u00b7ger", "Sto\u00b7ckung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Und schon umschmeichelt mich so s\u00fc\u00df des Lebens liebliche Lockung.", "tokens": ["Und", "schon", "um\u00b7schmei\u00b7chelt", "mich", "so", "s\u00fc\u00df", "des", "Le\u00b7bens", "lieb\u00b7li\u00b7che", "Lo\u00b7ckung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Da schwindelt mir; Verwirrung, Scham, sie \u00fcberfluten hei\u00df mich;", "tokens": ["Da", "schwin\u00b7delt", "mir", ";", "Ver\u00b7wir\u00b7rung", ",", "Scham", ",", "sie", "\u00fc\u00b7berf\u00b7lu\u00b7ten", "hei\u00df", "mich", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "$,", "NN", "$,", "PPER", "VVFIN", "ADJD", "PPER", "$."], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.13": {"text": "O ich vermessener, armer Thor! Was ", "tokens": ["O", "ich", "ver\u00b7mes\u00b7se\u00b7ner", ",", "ar\u00b7mer", "Thor", "!", "Was"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "PPER", "ADJD", "$,", "ADJA", "NN", "$.", "PWS"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.14": {"text": "Ich bin nur ein ", "tokens": ["Ich", "bin", "nur", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Und ich wanke und schwanke in Lieb' und Ha\u00df, und mir d\u00e4ucht', ich bewege die Welt.", "tokens": ["Und", "ich", "wan\u00b7ke", "und", "schwan\u00b7ke", "in", "Lieb'", "und", "Ha\u00df", ",", "und", "mir", "d\u00e4ucht'", ",", "ich", "be\u00b7we\u00b7ge", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "KON", "NN", "$,", "KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "--+--+--+-+--+--+--+", "measure": "anapaest.tri.plus"}, "line.16": {"text": "O ich Irrtum und schw\u00e4chlicher Widerspruch! \u2013", "tokens": ["O", "ich", "Irr\u00b7tum", "und", "schw\u00e4ch\u00b7li\u00b7cher", "Wi\u00b7der\u00b7spruch", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PPER", "NN", "KON", "ADJA", "NN", "$.", "$("], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Und doch! Was hier erwacht", "tokens": ["Und", "doch", "!", "Was", "hier", "er\u00b7wacht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "PWS", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "So grimm und k\u00fchn, ist Irrtum nicht, ist Zwietracht nicht, \u2013 ist ", "tokens": ["So", "grimm", "und", "k\u00fchn", ",", "ist", "Irr\u00b7tum", "nicht", ",", "ist", "Zwiet\u00b7racht", "nicht", ",", "\u2013", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "VAFIN", "NE", "PTKNEG", "$,", "VAFIN", "NN", "PTKNEG", "$,", "$(", "VAFIN"], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.19": {"text": "Ich bin die ", "tokens": ["Ich", "bin", "die"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "So einig wie Kastanienfrucht und ihre Stachelschale.", "tokens": ["So", "ei\u00b7nig", "wie", "Kas\u00b7ta\u00b7ni\u00b7en\u00b7frucht", "und", "ih\u00b7re", "Sta\u00b7chel\u00b7scha\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.21": {"text": "Und die hassende Liebe, der liebende Ha\u00df, so in mir g\u00e4hrt und schafft,", "tokens": ["Und", "die", "has\u00b7sen\u00b7de", "Lie\u00b7be", ",", "der", "lie\u00b7ben\u00b7de", "Ha\u00df", ",", "so", "in", "mir", "g\u00e4hrt", "und", "schafft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,", "ADV", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "--+--+--+--+-+-+-+", "measure": "anapaest.tetra.plus"}, "line.22": {"text": "Das ist der ", "tokens": ["Das", "ist", "der"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Wird Arme breiten sehnsuchtsvoll und F\u00e4uste drohend ballen.", "tokens": ["Wird", "Ar\u00b7me", "brei\u00b7ten", "sehn\u00b7suchts\u00b7voll", "und", "F\u00e4us\u00b7te", "dro\u00b7hend", "bal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJA", "ADJD", "KON", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}