{"textgrid.poem.26835": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was der Griechen Kunst erschaffen,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was der Griechen Kunst erschaffen,", "tokens": ["Was", "der", "Grie\u00b7chen", "Kunst", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mag der Franke mit den Waffen", "tokens": ["Mag", "der", "Fran\u00b7ke", "mit", "den", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchren nach der ", "tokens": ["F\u00fch\u00b7ren", "nach", "der"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Und in prangenden Museen", "tokens": ["Und", "in", "pran\u00b7gen\u00b7den", "Mu\u00b7se\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.5": {"text": "Zeig er seine Siegstroph\u00e4en", "tokens": ["Zeig", "er", "sei\u00b7ne", "Siegs\u00b7trop\u00b7h\u00e4\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dem erstaunten Vaterland!", "tokens": ["Dem", "er\u00b7staun\u00b7ten", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ewig werden sie ihm schweigen,", "tokens": ["E\u00b7wig", "wer\u00b7den", "sie", "ihm", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nie von den Gestellen steigen", "tokens": ["Nie", "von", "den", "Ge\u00b7stel\u00b7len", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In des Lebens frischen Reihn.", "tokens": ["In", "des", "Le\u00b7bens", "fri\u00b7schen", "Reihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der allein besitzt die Musen,", "tokens": ["Der", "al\u00b7lein", "be\u00b7sitzt", "die", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der sie tr\u00e4gt im warmen Busen,", "tokens": ["Der", "sie", "tr\u00e4gt", "im", "war\u00b7men", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dem Vandalen sind sie Stein.", "tokens": ["Dem", "Van\u00b7da\u00b7len", "sind", "sie", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}