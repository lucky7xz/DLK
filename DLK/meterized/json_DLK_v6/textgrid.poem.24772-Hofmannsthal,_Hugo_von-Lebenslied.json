{"textgrid.poem.24772": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Lebenslied", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Erben la\u00df verschwenden", "tokens": ["Den", "Er\u00b7ben", "la\u00df", "ver\u00b7schwen\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVIMP", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An Adler, Lamm und Pfau", "tokens": ["An", "Ad\u00b7ler", ",", "Lamm", "und", "Pfau"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Salb\u00f6l aus den H\u00e4nden", "tokens": ["Das", "Sal\u00b7b\u00f6l", "aus", "den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der toten alten Frau!", "tokens": ["Der", "to\u00b7ten", "al\u00b7ten", "Frau", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Toten, die entgleiten,", "tokens": ["Die", "To\u00b7ten", ",", "die", "ent\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Wipfel in dem Weiten \u2013", "tokens": ["Die", "Wip\u00b7fel", "in", "dem", "Wei\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ihm sind sie wie das Schreiten", "tokens": ["Ihm", "sind", "sie", "wie", "das", "Schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der T\u00e4nzerinnen wert!", "tokens": ["Der", "T\u00e4n\u00b7ze\u00b7rin\u00b7nen", "wert", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er geht wie den kein Walten", "tokens": ["Er", "geht", "wie", "den", "kein", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom R\u00fccken her bedroht.", "tokens": ["Vom", "R\u00fc\u00b7cken", "her", "be\u00b7droht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APZR", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er l\u00e4chelt, wenn die Falten", "tokens": ["Er", "l\u00e4\u00b7chelt", ",", "wenn", "die", "Fal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Lebens fl\u00fcstern: Tod!", "tokens": ["Des", "Le\u00b7bens", "fl\u00fcs\u00b7tern", ":", "Tod", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihm bietet jede Stelle", "tokens": ["Ihm", "bie\u00b7tet", "je\u00b7de", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Geheimnisvoll die Schwelle;", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7voll", "die", "Schwel\u00b7le", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es gibt sich jeder Welle", "tokens": ["Es", "gibt", "sich", "je\u00b7der", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Heimatlose hin.", "tokens": ["Der", "Hei\u00b7mat\u00b7lo\u00b7se", "hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Schwarm von wilden Bienen", "tokens": ["Der", "Schwarm", "von", "wil\u00b7den", "Bie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nimmt seine Seele mit;", "tokens": ["Nimmt", "sei\u00b7ne", "See\u00b7le", "mit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Singen von Delphinen", "tokens": ["Das", "Sin\u00b7gen", "von", "Del\u00b7phi\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Befl\u00fcgelt seinen Schritt:", "tokens": ["Be\u00b7fl\u00fc\u00b7gelt", "sei\u00b7nen", "Schritt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihn tragen alle Erden", "tokens": ["Ihn", "tra\u00b7gen", "al\u00b7le", "Er\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit m\u00e4chtigen Geb\u00e4rden.", "tokens": ["Mit", "m\u00e4ch\u00b7ti\u00b7gen", "Ge\u00b7b\u00e4r\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Fl\u00fcsse Dunkelwerden", "tokens": ["Der", "Fl\u00fcs\u00b7se", "Dun\u00b7kel\u00b7wer\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Begrenzt den Hirtentag!", "tokens": ["Be\u00b7grenzt", "den", "Hir\u00b7ten\u00b7tag", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Salb\u00f6l aus den H\u00e4nden", "tokens": ["Das", "Sal\u00b7b\u00f6l", "aus", "den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der toten alten Frau", "tokens": ["Der", "to\u00b7ten", "al\u00b7ten", "Frau"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df l\u00e4chelnd ihn verschwenden", "tokens": ["La\u00df", "l\u00e4\u00b7chelnd", "ihn", "ver\u00b7schwen\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An Adler, Lamm und Pfau:", "tokens": ["An", "Ad\u00b7ler", ",", "Lamm", "und", "Pfau", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er l\u00e4chelt der Gef\u00e4hrten. \u2013", "tokens": ["Er", "l\u00e4\u00b7chelt", "der", "Ge\u00b7f\u00e4hr\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die schwebend unbeschwerten", "tokens": ["Die", "schwe\u00b7bend", "un\u00b7be\u00b7schwer\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Abgr\u00fcnde und die G\u00e4rten", "tokens": ["Ab\u00b7gr\u00fcn\u00b7de", "und", "die", "G\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ART", "NN"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Lebens tragen ihn.", "tokens": ["Des", "Le\u00b7bens", "tra\u00b7gen", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}