{"dta.poem.20379": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Auff den tod Friederich Wilhelms/  \n Churf\u00fcrstens zu Brandenburg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schau Deutschland! wo du kanst f\u00fcr blut und thr\u00e4nen sehn", "tokens": ["Schau", "Deutschland", "!", "wo", "du", "kanst", "f\u00fcr", "blut", "und", "thr\u00e4\u00b7nen", "sehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$.", "PWAV", "PPER", "VMFIN", "APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es ist um deinen sohn und deine ruh geschehn!", "tokens": ["Es", "ist", "um", "dei\u00b7nen", "sohn", "und", "dei\u00b7ne", "ruh", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn Friedrich Wilhelm f\u00e4llt und du mit ihm darnieder.", "tokens": ["Denn", "Fried\u00b7rich", "Wil\u00b7helm", "f\u00e4llt", "und", "du", "mit", "ihm", "dar\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVFIN", "KON", "PPER", "APPR", "PPER", "PAV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Beym kriege ward er jung/ beym kriege stirbt er wieder.", "tokens": ["Beym", "krie\u00b7ge", "ward", "er", "jung", "/", "beym", "krie\u00b7ge", "stirbt", "er", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "$(", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Damahls zu rechter zwar/ itzt zu verkehrter zeit:", "tokens": ["Da\u00b7mahls", "zu", "rech\u00b7ter", "zwar", "/", "itzt", "zu", "ver\u00b7kehr\u00b7ter", "zeit", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "ADV", "$(", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn wer vermisset wohl nicht seine tapfferkeit?", "tokens": ["Denn", "wer", "ver\u00b7mis\u00b7set", "wohl", "nicht", "sei\u00b7ne", "tapf\u00b7fer\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was dir in tausenden kaum wieder wird gebohren/", "tokens": ["Was", "dir", "in", "tau\u00b7sen\u00b7den", "kaum", "wie\u00b7der", "wird", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJA", "ADV", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hast du in einem hier auff einen tag verlohren.", "tokens": ["Hast", "du", "in", "ei\u00b7nem", "hier", "auff", "ei\u00b7nen", "tag", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er hat am ersten uns die augen auffgethan/", "tokens": ["Er", "hat", "am", "ers\u00b7ten", "uns", "die", "au\u00b7gen", "auff\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was/ wenn die noth anbricht/ ein deutscher Churf\u00fcrst kan.", "tokens": ["Was", "/", "wenn", "die", "noth", "an\u00b7bricht", "/", "ein", "deut\u00b7scher", "Chur\u00b7f\u00fcrst", "kan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$(", "KOUS", "ART", "NN", "VVFIN", "$(", "ART", "ADJA", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zwey reiche fiengen an f\u00fcr seiner macht zu wancken;", "tokens": ["Zwey", "rei\u00b7che", "fi\u00b7en\u00b7gen", "an", "f\u00fcr", "sei\u00b7ner", "macht", "zu", "wan\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "VVFIN", "APPR", "APPR", "PPOSAT", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Zwey kronen haben ihm auch ihre ruh zu dancken;", "tokens": ["Zwey", "kro\u00b7nen", "ha\u00b7ben", "ihm", "auch", "ih\u00b7re", "ruh", "zu", "dan\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und zwey/ die alle welt zum morden auffgeweckt/", "tokens": ["Und", "zwey", "/", "die", "al\u00b7le", "welt", "zum", "mor\u00b7den", "auff\u00b7ge\u00b7weckt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "$(", "ART", "PIAT", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Hat er/ und zwar allein/ durch seinen muth erschreckt.", "tokens": ["Hat", "er", "/", "und", "zwar", "al\u00b7lein", "/", "durch", "sei\u00b7nen", "muth", "er\u00b7schreckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "KON", "ADV", "ADV", "$(", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Viel freunde haben ihn/ er selten sie/ verlassen:", "tokens": ["Viel", "freun\u00b7de", "ha\u00b7ben", "ihn", "/", "er", "sel\u00b7ten", "sie", "/", "ver\u00b7las\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "$(", "PPER", "VVFIN", "PPER", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die feinde konten ihn/ auch wenn er schlug/ nicht hassen;", "tokens": ["Die", "fein\u00b7de", "kon\u00b7ten", "ihn", "/", "auch", "wenn", "er", "schlug", "/", "nicht", "has\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "$(", "ADV", "KOUS", "PPER", "VVFIN", "$(", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Es kennt ihn Africa/ und Stambol glaubte gar/", "tokens": ["Es", "kennt", "ihn", "A\u00b7fri\u00b7ca", "/", "und", "Stam\u00b7bol", "glaub\u00b7te", "gar", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NE", "$(", "KON", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df er was mehrers w\u00e4r/ als er doch w\u00fcrcklich war;", "tokens": ["Da\u00df", "er", "was", "meh\u00b7rers", "w\u00e4r", "/", "als", "er", "doch", "w\u00fcrck\u00b7lich", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADV", "VAFIN", "$(", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ja Franckreich wolte sich nicht eh zu felde wagen/", "tokens": ["Ja", "Fran\u00b7ck\u00b7reich", "wol\u00b7te", "sich", "nicht", "eh", "zu", "fel\u00b7de", "wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NE", "VMFIN", "PRF", "PTKNEG", "KOUS", "APPR", "NN", "VVINF", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Als bi\u00df es diesen held sah tod zu grabe tragen.", "tokens": ["Als", "bi\u00df", "es", "die\u00b7sen", "held", "sah", "tod", "zu", "gra\u00b7be", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "PDS", "VVFIN", "VVFIN", "NN", "PTKZU", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Jedoch verzweiffle nicht/ geliebtes vaterland!", "tokens": ["Je\u00b7doch", "ver\u00b7zweiff\u00b7le", "nicht", "/", "ge\u00b7lieb\u00b7tes", "va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sein geist hat sich von dir nur darum abgewand/", "tokens": ["Sein", "geist", "hat", "sich", "von", "dir", "nur", "da\u00b7rum", "ab\u00b7ge\u00b7wand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PRF", "APPR", "PPER", "ADV", "PAV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Da\u00df er/ wenn deine noth einst wird am h\u00f6chsten steigen/", "tokens": ["Da\u00df", "er", "/", "wenn", "dei\u00b7ne", "noth", "einst", "wird", "am", "h\u00f6chs\u00b7ten", "stei\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "KOUS", "PPOSAT", "NN", "ADV", "VAFIN", "APPRART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sich wie Elins kan im sohne doppelt zeigen.", "tokens": ["Sich", "wie", "E\u00b7lins", "kan", "im", "soh\u00b7ne", "dop\u00b7pelt", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KOKOM", "NE", "VMFIN", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}