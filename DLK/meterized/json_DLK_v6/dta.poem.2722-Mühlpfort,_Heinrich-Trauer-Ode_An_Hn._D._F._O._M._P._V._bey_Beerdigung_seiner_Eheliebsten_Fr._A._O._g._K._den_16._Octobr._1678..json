{"dta.poem.2722": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trauer-Ode/  \n  An Hn.   D.   F. O.   M. P. V.   bey Beerdigung  \n seiner Eheliebsten/ Fr. A. O. g. K. den 16.  \n Octobr. 1678.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hochwerther Freund/ der Musen Zier/", "tokens": ["Hoch\u00b7wert\u00b7her", "Freund", "/", "der", "Mu\u00b7sen", "Zier", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ber\u00fchmter Artzt und Podalier", "tokens": ["Be\u00b7r\u00fchm\u00b7ter", "Artzt", "und", "Po\u00b7da\u00b7lier"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df itzt sein Lorber-Baum erblassen?", "tokens": ["Mu\u00df", "itzt", "sein", "Lor\u00b7ber\u00b7Baum", "er\u00b7blas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Baum der sonst den Blitz verlacht/", "tokens": ["Der", "Baum", "der", "sonst", "den", "Blitz", "ver\u00b7lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den/ wenn der Donner gleich erkracht/", "tokens": ["Den", "/", "wenn", "der", "Don\u00b7ner", "gleich", "er\u00b7kracht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOUS", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch nicht mit seinem Keil kan fassen.", "tokens": ["Doch", "nicht", "mit", "sei\u00b7nem", "Keil", "kan", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Verwelckt sein immer-gr\u00fcnes Haar", "tokens": ["Ver\u00b7welckt", "sein", "im\u00b7mer\u00b7gr\u00fc\u00b7nes", "Haar"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das seine sch\u00f6nste Zierath war/", "tokens": ["Das", "sei\u00b7ne", "sch\u00f6ns\u00b7te", "Zie\u00b7rath", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und ihm viel Kronen hat gewunden?", "tokens": ["Und", "ihm", "viel", "Kro\u00b7nen", "hat", "ge\u00b7wun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIAT", "NN", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Stehn seine Zweige sonder Frucht/", "tokens": ["Stehn", "sei\u00b7ne", "Zwei\u00b7ge", "son\u00b7der", "Frucht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und hat der Nymfen zarte Zucht/", "tokens": ["Und", "hat", "der", "Nym\u00b7fen", "zar\u00b7te", "Zucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sich nicht bey ihm mehr eingefunden?", "tokens": ["Sich", "nicht", "bey", "ihm", "mehr", "ein\u00b7ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Ach ja selbst Ph\u00f6bus ist betr\u00fcbt/", "tokens": ["Ach", "ja", "selbst", "Ph\u00f6\u00b7bus", "ist", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADV", "NE", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Beklagt den Baum den er geliebt/", "tokens": ["Be\u00b7klagt", "den", "Baum", "den", "er", "ge\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Weil er verwandelt in Cypressen/", "tokens": ["Weil", "er", "ver\u00b7wan\u00b7delt", "in", "Cyp\u00b7res\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NE", "$("], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Es ist jetzt Wermuth hingepflantzt/", "tokens": ["Es", "ist", "jetzt", "Wer\u00b7muth", "hin\u00b7ge\u00b7pflantzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "VVPP", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.17": {"text": "Wo vor die Gratien getantzt/", "tokens": ["Wo", "vor", "die", "Gra\u00b7ti\u00b7en", "ge\u00b7tantzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und wo der Musen Volck gesessen.", "tokens": ["Und", "wo", "der", "Mu\u00b7sen", "Volck", "ge\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Hygea seufftzet in dem Haus/", "tokens": ["Hy\u00b7gea", "seufft\u00b7zet", "in", "dem", "Haus", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Ziehrt nicht den Rock mit Blumen aus/", "tokens": ["Ziehrt", "nicht", "den", "Rock", "mit", "Blu\u00b7men", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "F\u00fcllt den Gesundheits-Kelch mit Thr\u00e4nen;", "tokens": ["F\u00fcllt", "den", "Ge\u00b7sund\u00b7heits\u00b7Kelch", "mit", "Thr\u00e4\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.22": {"text": "Und Flora die von jedem Kraut/", "tokens": ["Und", "Flo\u00b7ra", "die", "von", "je\u00b7dem", "Kraut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Jhm Geist und Seele hat vertraut/", "tokens": ["Jhm", "Geist", "und", "See\u00b7le", "hat", "ver\u00b7traut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Entdeckt zugleich ihr bittres Sehnen.", "tokens": ["Ent\u00b7deckt", "zu\u00b7gleich", "ihr", "bitt\u00b7res", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Zudem/ Erhalter der Natur/", "tokens": ["Zu\u00b7dem", "/", "Er\u00b7hal\u00b7ter", "der", "Na\u00b7tur", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Wil seine sonst bewehrte Cur", "tokens": ["Wil", "sei\u00b7ne", "sonst", "be\u00b7wehr\u00b7te", "Cur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Best\u00fcrtzt den Fu\u00df zur\u00fccke ziehen;", "tokens": ["Be\u00b7st\u00fcrtzt", "den", "Fu\u00df", "zu\u00b7r\u00fc\u00b7cke", "zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Dem Tode/ den er offt bek\u00e4mpfft/", "tokens": ["Dem", "To\u00b7de", "/", "den", "er", "offt", "be\u00b7k\u00e4mpfft", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und durch Verstand und Kunst ged\u00e4mpfft/", "tokens": ["Und", "durch", "Ver\u00b7stand", "und", "Kunst", "ge\u00b7d\u00e4mpfft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Kan nun die ", "tokens": ["Kan", "nun", "die"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ADV", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.31": {"text": "Sein Schmertzen ist nur allzugro\u00df/", "tokens": ["Sein", "Schmert\u00b7zen", "ist", "nur", "all\u00b7zu\u00b7gro\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Und k\u00f6nt auch wol ein h\u00e4rter Sto\u00df", "tokens": ["Und", "k\u00f6nt", "auch", "wol", "ein", "h\u00e4r\u00b7ter", "Sto\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Jhm durch Gebein und Adern dringen?", "tokens": ["Jhm", "durch", "Ge\u00b7bein", "und", "A\u00b7dern", "drin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Ja solt auch wol ein schwerer Leid", "tokens": ["Ja", "solt", "auch", "wol", "ein", "schwe\u00b7rer", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "VMFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Jhm machen mehr Empfindligkeit/", "tokens": ["Jhm", "ma\u00b7chen", "mehr", "Emp\u00b7find\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Als dieser Trauer-Fall kan bringen?", "tokens": ["Als", "die\u00b7ser", "Trau\u00b7e\u00b7rFall", "kan", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Es hat der Monden kaum zweymal", "tokens": ["Es", "hat", "der", "Mon\u00b7den", "kaum", "zwey\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.38": {"text": "Ver\u00e4ndert seine Farb und Stral/", "tokens": ["Ver\u00b7\u00e4n\u00b7dert", "sei\u00b7ne", "Farb", "und", "Stral", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Da\u00df er den ", "tokens": ["Da\u00df", "er", "den"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.40": {"text": "Den Bruder/ Sions Ruhm und Zier/", "tokens": ["Den", "Bru\u00b7der", "/", "Si\u00b7ons", "Ruhm", "und", "Zier", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NE", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Der wenig seines gleichen hier", "tokens": ["Der", "we\u00b7nig", "sei\u00b7nes", "glei\u00b7chen", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "PPOSAT", "ADJA", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Auch lange Zeit darnach wird haben.", "tokens": ["Auch", "lan\u00b7ge", "Zeit", "dar\u00b7nach", "wird", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PAV", "VAFIN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Der noch bey der Gelehrten Welt", "tokens": ["Der", "noch", "bey", "der", "Ge\u00b7lehr\u00b7ten", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.44": {"text": "Di\u00df unverf\u00e4lschte Lob beh\u00e4lt/", "tokens": ["Di\u00df", "un\u00b7ver\u00b7f\u00e4lschte", "Lob", "be\u00b7