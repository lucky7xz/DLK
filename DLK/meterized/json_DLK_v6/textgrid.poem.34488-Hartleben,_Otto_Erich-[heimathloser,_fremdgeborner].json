{"textgrid.poem.34488": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[heimathloser, fremdgeborner]", "genre": "verse", "period": "N.A.", "pub_year": 1888, "urn": "N.A.", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heimathloser, fremdgeborner, suche, wo du Ruhe findest \u2013", "tokens": ["Hei\u00b7math\u00b7lo\u00b7ser", ",", "fremd\u00b7ge\u00b7bor\u00b7ner", ",", "su\u00b7che", ",", "wo", "du", "Ru\u00b7he", "fin\u00b7dest", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$,", "VVFIN", "$,", "PWAV", "PPER", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "wo du sehnsuchtskrank dich l\u00e4nger nicht in Zwanges Qualen windest.", "tokens": ["wo", "du", "sehn\u00b7suchts\u00b7krank", "dich", "l\u00e4n\u00b7ger", "nicht", "in", "Zwan\u00b7ges", "Qua\u00b7len", "win\u00b7dest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PPER", "ADJD", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Lehne dich an jene Br\u00fcste, lehne dich in jene Arme \u2013", "tokens": ["Leh\u00b7ne", "dich", "an", "je\u00b7ne", "Br\u00fcs\u00b7te", ",", "leh\u00b7ne", "dich", "in", "je\u00b7ne", "Ar\u00b7me", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "NN", "$,", "VVFIN", "PRF", "APPR", "PDAT", "NN", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "glaube, dass sich treue Liebe deiner w\u00fcsten Stirn erbarme!", "tokens": ["glau\u00b7be", ",", "dass", "sich", "treu\u00b7e", "Lie\u00b7be", "dei\u00b7ner", "w\u00fcs\u00b7ten", "Stirn", "er\u00b7bar\u00b7me", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PRF", "ADJA", "NN", "PPOSAT", "VVFIN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Heimathloser, fremdgeborner, tr\u00e4ume so ein leis Ermatten \u2013", "tokens": ["Hei\u00b7math\u00b7lo\u00b7ser", ",", "fremd\u00b7ge\u00b7bor\u00b7ner", ",", "tr\u00e4u\u00b7me", "so", "ein", "leis", "Er\u00b7mat\u00b7ten", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$,", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "tr\u00e4ume, dass dein Haupt sich wiege schon in s\u00fcssen Todes Schatten.", "tokens": ["tr\u00e4u\u00b7me", ",", "dass", "dein", "Haupt", "sich", "wie\u00b7ge", "schon", "in", "s\u00fcs\u00b7sen", "To\u00b7des", "Schat\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "ADV", "APPR", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}