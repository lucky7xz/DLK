{"textgrid.poem.33844": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das letzte Glas", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das letzte Glas! Wer mag es denken!", "tokens": ["Das", "letz\u00b7te", "Glas", "!", "Wer", "mag", "es", "den\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dennoch mu\u00df ein letztes sein!", "tokens": ["Und", "den\u00b7noch", "mu\u00df", "ein", "letz\u00b7tes", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich dr\u00e4ngt's, es hastig einzuschenken,", "tokens": ["Mich", "dr\u00e4ngt's", ",", "es", "has\u00b7tig", "ein\u00b7zu\u00b7schen\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00e4llt auch die Thr\u00e4ne mit hinein.", "tokens": ["F\u00e4llt", "auch", "die", "Thr\u00e4\u00b7ne", "mit", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sto\u00df an! Du stie\u00dfest gar zu heftig!", "tokens": ["Sto\u00df", "an", "!", "Du", "stie\u00b7\u00dfest", "gar", "zu", "hef\u00b7tig", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PPER", "VVFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In tausend Scherben liegt das Glas.", "tokens": ["In", "tau\u00b7send", "Scher\u00b7ben", "liegt", "das", "Glas", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein neues bringt mir schon gesch\u00e4ftig", "tokens": ["Ein", "neu\u00b7es", "bringt", "mir", "schon", "ge\u00b7sch\u00e4f\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Kellner; nochmals f\u00fcll' ich das.", "tokens": ["Der", "Kell\u00b7ner", ";", "noch\u00b7mals", "f\u00fcll'", "ich", "das", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "VVFIN", "PPER", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das letzte Glas! Wer mag es schauen!", "tokens": ["Das", "letz\u00b7te", "Glas", "!", "Wer", "mag", "es", "schau\u00b7en", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dennoch mu\u00df ein letztes sein!", "tokens": ["Und", "den\u00b7noch", "mu\u00df", "ein", "letz\u00b7tes", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du ziehst nun bald in ferne Gauen:", "tokens": ["Du", "ziehst", "nun", "bald", "in", "fer\u00b7ne", "Gau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denkst du im fremden Land noch mein?", "tokens": ["Denkst", "du", "im", "frem\u00b7den", "Land", "noch", "mein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sto\u00df an! Ich zitt're gar zu heftig!", "tokens": ["Sto\u00df", "an", "!", "Ich", "zitt'\u00b7re", "gar", "zu", "hef\u00b7tig", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PPER", "VVFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In tausend Scherben liegt das Glas.", "tokens": ["In", "tau\u00b7send", "Scher\u00b7ben", "liegt", "das", "Glas", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein neues bringt mir schon gesch\u00e4ftig", "tokens": ["Ein", "neu\u00b7es", "bringt", "mir", "schon", "ge\u00b7sch\u00e4f\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Kellner; nochmals f\u00fcll' ich das.", "tokens": ["Der", "Kell\u00b7ner", ";", "noch\u00b7mals", "f\u00fcll'", "ich", "das", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "VVFIN", "PPER", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das letzte Glas! Wer mag es trinken!", "tokens": ["Das", "letz\u00b7te", "Glas", "!", "Wer", "mag", "es", "trin\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dennoch mu\u00df ein letztes sein!", "tokens": ["Und", "den\u00b7noch", "mu\u00df", "ein", "letz\u00b7tes", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir werden neue Freunde winken,", "tokens": ["Dir", "wer\u00b7den", "neu\u00b7e", "Freun\u00b7de", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich aber bleib' hier ganz allein!", "tokens": ["Ich", "a\u00b7ber", "bleib'", "hier", "ganz", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sto\u00df an! Zu Boden werf' ich's heftig!", "tokens": ["Sto\u00df", "an", "!", "Zu", "Bo\u00b7den", "wer\u00b7f'", "ich's", "hef\u00b7tig", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "APPR", "NN", "VAFIN", "PIS", "ADJD", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Warum schon jetzt das letzte Glas!", "tokens": ["Wa\u00b7rum", "schon", "jetzt", "das", "letz\u00b7te", "Glas", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein neues bringt mir schon gesch\u00e4ftig", "tokens": ["Ein", "neu\u00b7es", "bringt", "mir", "schon", "ge\u00b7sch\u00e4f\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Kellner; nochmals f\u00fcll' ich das.", "tokens": ["Der", "Kell\u00b7ner", ";", "noch\u00b7mals", "f\u00fcll'", "ich", "das", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "VVFIN", "PPER", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das letzte Glas! Wir lassen's stehen!", "tokens": ["Das", "letz\u00b7te", "Glas", "!", "Wir", "las\u00b7sen's", "ste\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Versiegle und verschlie\u00df den Wein!", "tokens": ["Ver\u00b7sieg\u00b7le", "und", "ver\u00b7schlie\u00df", "den", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn wir dereinst uns wieder sehen,", "tokens": ["Wenn", "wir", "de\u00b7reinst", "uns", "wie\u00b7der", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So soll es unser erstes sein!", "tokens": ["So", "soll", "es", "un\u00b7ser", "ers\u00b7tes", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Komm, an den Mund press' ich dich heftig,", "tokens": ["Komm", ",", "an", "den", "Mund", "press'", "ich", "dich", "hef\u00b7tig", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als w\u00e4rst du selbst mein letztes Glas!", "tokens": ["Als", "w\u00e4rst", "du", "selbst", "mein", "letz\u00b7tes", "Glas", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was wir uns sind, das f\u00fchl' ich kr\u00e4ftig,", "tokens": ["Was", "wir", "uns", "sind", ",", "das", "f\u00fchl'", "ich", "kr\u00e4f\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VAFIN", "$,", "PDS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Jetzt geh mit Gott! Wir bleiben das!", "tokens": ["Jetzt", "geh", "mit", "Gott", "!", "Wir", "blei\u00b7ben", "das", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$.", "PPER", "VVFIN", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}