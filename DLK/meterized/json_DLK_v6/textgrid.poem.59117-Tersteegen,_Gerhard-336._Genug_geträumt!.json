{"textgrid.poem.59117": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "336. Genug getr\u00e4umt!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die kurze Lebensnacht bringst du", "tokens": ["Die", "kur\u00b7ze", "Le\u00b7bens\u00b7nacht", "bringst", "du"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Phantasier'n und Tr\u00e4umen zu.", "tokens": ["Mit", "Phan\u00b7ta\u00b7sier'n", "und", "Tr\u00e4u\u00b7men", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die tr\u00e4umend schreckten oder lachten,", "tokens": ["Die", "tr\u00e4u\u00b7mend", "schreck\u00b7ten", "o\u00b7der", "lach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was sagten sie, wie sie erwachten?", "tokens": ["Was", "sag\u00b7ten", "sie", ",", "wie", "sie", "er\u00b7wach\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}