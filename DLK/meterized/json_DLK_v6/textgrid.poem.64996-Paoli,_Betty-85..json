{"textgrid.poem.64996": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "85.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Krieger w\u00fcnscht den Tod von Jenem zu empfangen,", "tokens": ["Der", "Krie\u00b7ger", "w\u00fcnscht", "den", "Tod", "von", "Je\u00b7nem", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "PDAT", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An welchem lebend er am innigsten gehangen.", "tokens": ["An", "wel\u00b7chem", "le\u00b7bend", "er", "am", "in\u00b7nigs\u00b7ten", "ge\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJD", "PPER", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Und sieh! auch mir, die sich in manchem Kampf bew\u00e4hret,", "tokens": ["Und", "sieh", "!", "auch", "mir", ",", "die", "sich", "in", "man\u00b7chem", "Kampf", "be\u00b7w\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "ADV", "PPER", "$,", "PRELS", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ward diese letzte Gunst als Kampfespreis bescheeret.", "tokens": ["Ward", "die\u00b7se", "letz\u00b7te", "Gunst", "als", "Kamp\u00b7fe\u00b7spreis", "be\u00b7schee\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auf Lebens Schlachtfeld lag ich wund und schmerzentbrannt,", "tokens": ["Auf", "Le\u00b7bens", "Schlacht\u00b7feld", "lag", "ich", "wund", "und", "schmer\u00b7zent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da sandte Gott den Tod mir von der liebsten Hand.", "tokens": ["Da", "sand\u00b7te", "Gott", "den", "Tod", "mir", "von", "der", "liebs\u00b7ten", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ART", "NN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}