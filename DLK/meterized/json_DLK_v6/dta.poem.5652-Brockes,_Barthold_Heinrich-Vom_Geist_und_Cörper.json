{"dta.poem.5652": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So lass't uns denn von c\u00f6rperlichen Sachen,", "tokens": ["So", "lass't", "uns", "denn", "von", "c\u00f6r\u00b7per\u00b7li\u00b7chen", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Und was den Geist angeht, nie eine Mischung machen.", "tokens": ["Und", "was", "den", "Geist", "an\u00b7geht", ",", "nie", "ei\u00b7ne", "Misc\u00b7hung", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man mu\u00df der C\u00f6rper Eigenschafft", "tokens": ["Man", "mu\u00df", "der", "C\u00f6r\u00b7per", "Ei\u00b7gen\u00b7schafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Absonderlich erwegen und betrachten.", "tokens": ["Ab\u00b7son\u00b7der\u00b7lich", "er\u00b7we\u00b7gen", "und", "be\u00b7trach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wer wird sich doch dazu verm\u00f6gend achten", "tokens": ["Wer", "wird", "sich", "doch", "da\u00b7zu", "ver\u00b7m\u00f6\u00b7gend", "ach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PRF", "ADV", "PAV", "VVPP", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu messen eine Seel, zu theilen ihre Krafft?", "tokens": ["Zu", "mes\u00b7sen", "ei\u00b7ne", "Seel", ",", "zu", "thei\u00b7len", "ih\u00b7re", "Krafft", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$,", "PTKZU", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie will man, da\u00df, von der Materie,", "tokens": ["Wie", "will", "man", ",", "da\u00df", ",", "von", "der", "Ma\u00b7te\u00b7rie", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "$,", "KOUS", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Man mag sie, wie man will, ver\u00e4ndern und versetzen:", "tokens": ["Man", "mag", "sie", ",", "wie", "man", "will", ",", "ver\u00b7\u00e4n\u00b7dern", "und", "ver\u00b7set\u00b7zen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "$,", "PWAV", "PIS", "VMFIN", "$,", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man mag sie noch so viel Bewegungs-f\u00e4hig sch\u00e4tzen;", "tokens": ["Man", "mag", "sie", "noch", "so", "viel", "Be\u00b7we\u00b7gungs\u00b7f\u00e4\u00b7hig", "sch\u00e4t\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein Geist, der dencken kan, entsteh\u2019?", "tokens": ["Ein", "Geist", ",", "der", "den\u00b7cken", "kan", ",", "ent\u00b7steh'", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVINF", "VMFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wie k\u00f6nnten diese Theil\u2019 und wenn sie noch so klein,", "tokens": ["Wie", "k\u00f6nn\u00b7ten", "die\u00b7se", "Theil'", "und", "wenn", "sie", "noch", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PDAT", "NN", "KON", "KOUS", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Getheilet und geordnet seyn,", "tokens": ["Ge\u00b7thei\u00b7let", "und", "ge\u00b7ord\u00b7net", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Doch der Gedancken Wesen kriegen?", "tokens": ["Doch", "der", "Ge\u00b7dan\u00b7cken", "We\u00b7sen", "krie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wollt unser Geist sich selbst wol so betr\u00fcgen?", "tokens": ["Wollt", "un\u00b7ser", "Geist", "sich", "selbst", "wol", "so", "be\u00b7tr\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PRF", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}}}}}