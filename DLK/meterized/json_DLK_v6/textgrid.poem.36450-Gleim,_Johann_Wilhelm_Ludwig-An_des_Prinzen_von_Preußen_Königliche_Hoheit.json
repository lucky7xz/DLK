{"textgrid.poem.36450": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An des Prinzen von Preu\u00dfen K\u00f6nigliche Hoheit", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem du nachahmen sollt,", "tokens": ["Dem", "du", "nac\u00b7hah\u00b7men", "sollt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dein ", "tokens": ["Dein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Von Jugend an, gedacht,", "tokens": ["Von", "Ju\u00b7gend", "an", ",", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Einst gro\u00df zu sein, und ist, was er gewollt.", "tokens": ["Einst", "gro\u00df", "zu", "sein", ",", "und", "ist", ",", "was", "er", "ge\u00b7wollt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VAINF", "$,", "KON", "VAFIN", "$,", "PWS", "PPER", "VMPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er ist des Vaterlandes Lust,", "tokens": ["Er", "ist", "des", "Va\u00b7ter\u00b7lan\u00b7des", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Europas weiser Schiedesrichter,", "tokens": ["Eu\u00b7ro\u00b7pas", "wei\u00b7ser", "Schie\u00b7des\u00b7rich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Held, Philosoph und Dichter.", "tokens": ["Held", ",", "Phi\u00b7lo\u00b7soph", "und", "Dich\u00b7ter."], "token_info": ["word", "punct", "word", "word", "abbreviation"], "pos": ["NN", "$,", "NE", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Was Antonin, und C\u00e4sar, und August,", "tokens": ["Was", "An\u00b7to\u00b7nin", ",", "und", "C\u00e4\u00b7sar", ",", "und", "Au\u00b7gust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "NE", "$,", "KON", "NE", "$,", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Titus war, und mehr,", "tokens": ["Und", "Ti\u00b7tus", "war", ",", "und", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "$,", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das alles, ", "tokens": ["Das", "al\u00b7les", ","], "token_info": ["word", "word", "punct"], "pos": ["PDS", "PIS", "$,"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Um seinen Thron im pr\u00e4chtigen Berlin", "tokens": ["Um", "sei\u00b7nen", "Thron", "im", "pr\u00e4ch\u00b7ti\u00b7gen", "Ber\u00b7lin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "APPRART", "ADJA", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Stehn Grazien und Musen; ihren T\u00e4nzen", "tokens": ["Stehn", "Gra\u00b7zi\u00b7en", "und", "Mu\u00b7sen", ";", "ih\u00b7ren", "T\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "KON", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sieht er oft zu, sie werfen ihn,", "tokens": ["Sieht", "er", "oft", "zu", ",", "sie", "wer\u00b7fen", "ihn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Nicht ohne Neid, mit ihren Lorbeerkr\u00e4nzen!", "tokens": ["Nicht", "oh\u00b7ne", "Neid", ",", "mit", "ih\u00b7ren", "Lor\u00b7beer\u00b7kr\u00e4n\u00b7zen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sein Waffenplatz erwartet ihn; er eilt,", "tokens": ["Sein", "Waf\u00b7fen\u00b7platz", "er\u00b7war\u00b7tet", "ihn", ";", "er", "eilt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist Kriegesgott, sieht seine Fahnen fliegen;", "tokens": ["Ist", "Krie\u00b7ges\u00b7gott", ",", "sieht", "sei\u00b7ne", "Fah\u00b7nen", "flie\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ein Blick, der sie zusammen zieht und teilt,", "tokens": ["Ein", "Blick", ",", "der", "sie", "zu\u00b7sam\u00b7men", "zieht", "und", "teilt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gebeut, so schlagen sie, und siegen!", "tokens": ["Ge\u00b7beut", ",", "so", "schla\u00b7gen", "sie", ",", "und", "sie\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "$,", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch oft erholt er sich ein wenig", "tokens": ["Doch", "oft", "er\u00b7holt", "er", "sich", "ein", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "ART", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Ungemach der Monarchie;", "tokens": ["Vom", "Un\u00b7ge\u00b7mach", "der", "Mon\u00b7ar\u00b7chie", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann hat das stille Sanssouci", "tokens": ["Dann", "hat", "das", "stil\u00b7le", "Sans\u00b7sou\u00b7ci"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Philosophen, nicht den K\u00f6nig!", "tokens": ["Den", "Phi\u00b7lo\u00b7so\u00b7phen", ",", "nicht", "den", "K\u00f6\u00b7nig", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Da denkt er dann in seiner gro\u00dfen Seele", "tokens": ["Da", "denkt", "er", "dann", "in", "sei\u00b7ner", "gro\u00b7\u00dfen", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gedanken, wie die Marc-Aurele,", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "wie", "die", "Ma\u00b7rc\u00b7Au\u00b7re\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Und liest \u2013", "tokens": ["Und", "liest", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.8": {"line.1": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Und trag' in seinen B\u00fcchersaal", "tokens": ["Und", "trag'", "in", "sei\u00b7nen", "B\u00fc\u00b7cher\u00b7saal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dies Fabelbuch, dein Spiel!", "tokens": ["Dies", "Fa\u00b7bel\u00b7buch", ",", "dein", "Spiel", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O! wenn es dann dem K\u00f6nige gefiel", "tokens": ["O", "!", "wenn", "es", "dann", "dem", "K\u00f6\u00b7ni\u00b7ge", "ge\u00b7fiel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hineinzusehn! Dann h\u00f6rte dein \u00c4sop", "tokens": ["Hin\u00b7ein\u00b7zu\u00b7sehn", "!", "Dann", "h\u00f6r\u00b7te", "dein", "\u00c4\u00b7sop"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVINF", "$.", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vielleicht von Fern ein kleines Lob;", "tokens": ["Viel\u00b7leicht", "von", "Fern", "ein", "klei\u00b7nes", "Lob", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das w\u00fcrde dann mit neuem Mut", "tokens": ["Das", "w\u00fcr\u00b7de", "dann", "mit", "neu\u00b7em", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihn allsobald beseelen!", "tokens": ["Ihn", "all\u00b7so\u00b7bald", "be\u00b7see\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und du! du d\u00fcrftest nur befehlen,", "tokens": ["Und", "du", "!", "du", "d\u00fcrf\u00b7test", "nur", "be\u00b7feh\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$.", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So w\u00fcrd' er k\u00fchn, und kurz und gut,", "tokens": ["So", "w\u00fcrd'", "er", "k\u00fchn", ",", "und", "kurz", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch manche Fabel dir erz\u00e4hlen.", "tokens": ["Noch", "man\u00b7che", "Fa\u00b7bel", "dir", "er\u00b7z\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}