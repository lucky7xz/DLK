{"dta.poem.163": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als ihn die harte   Violante   im   Contra-  \n Marche   k\u00fcssen muste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Dein harter Sinn mu\u00df sich/ doch Violante brechen/", "tokens": ["Dein", "har\u00b7ter", "Sinn", "mu\u00df", "sich", "/", "doch", "Vi\u00b7o\u00b7lan\u00b7te", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PRF", "$(", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der lang versagte Ku\u00df/ der stellt sich endlich ein/", "tokens": ["Der", "lang", "ver\u00b7sag\u00b7te", "Ku\u00df", "/", "der", "stellt", "sich", "end\u00b7lich", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$(", "ART", "VVFIN", "PRF", "ADV", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Solt er auch durch das Spiel von dir erzwungen seyn.", "tokens": ["Solt", "er", "auch", "durch", "das", "Spiel", "von", "dir", "er\u00b7zwun\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Die seine grosse Macht/ durch ihren Wahnwitz schw\u00e4chen/", "tokens": ["Die", "sei\u00b7ne", "gros\u00b7se", "Macht", "/", "durch", "ih\u00b7ren", "Wahn\u00b7witz", "schw\u00e4\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "$(", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So schmeck ich nun mit Lust der Lippen s\u00fcssen Wein/", "tokens": ["So", "schmeck", "ich", "nun", "mit", "Lust", "der", "Lip\u00b7pen", "s\u00fcs\u00b7sen", "Wein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie gerne woltest du mir bittre Wermuth streun/", "tokens": ["Wie", "ger\u00b7ne", "wol\u00b7test", "du", "mir", "bitt\u00b7re", "Wer\u00b7muth", "streun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PPER", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und sehen/ da\u00df der Dorn/ bey Rosen m\u00f6ge stechen.", "tokens": ["Und", "se\u00b7hen", "/", "da\u00df", "der", "Dorn", "/", "bey", "Ro\u00b7sen", "m\u00f6\u00b7ge", "ste\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$(", "KOUS", "ART", "NN", "$(", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du sagst zwar Ja dazu/ da\u00df ich den Mund soll k\u00fcssen/", "tokens": ["Du", "sagst", "zwar", "Ja", "da\u00b7zu", "/", "da\u00df", "ich", "den", "Mund", "soll", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKANT", "PAV", "$(", "KOUS", "PPER", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Doch h\u00e4ttest du vorher die ", "tokens": ["Doch", "h\u00e4t\u00b7test", "du", "vor\u00b7her", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Die in dem Pfande-Spiel beym ", "tokens": ["Die", "in", "dem", "Pfan\u00b7de\u00b7Spiel", "beym"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "APPRART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "So h\u00e4ttest du mir nicht den s\u00fcssen Ku\u00df verg\u00fcnnt.", "tokens": ["So", "h\u00e4t\u00b7test", "du", "mir", "nicht", "den", "s\u00fcs\u00b7sen", "Ku\u00df", "ver\u00b7g\u00fcnnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich h\u00e4tte auf den Ku\u00df mein Lebe m\u00fcssen warten/", "tokens": ["Ich", "h\u00e4t\u00b7te", "auf", "den", "Ku\u00df", "mein", "Le\u00b7be", "m\u00fcs\u00b7sen", "war\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den mir jetzt Spiel/ Gl\u00fcck/ Zeit/ auf meine Lippen karten.", "tokens": ["Den", "mir", "jetzt", "Spiel", "/", "Gl\u00fcck", "/", "Zeit", "/", "auf", "mei\u00b7ne", "Lip\u00b7pen", "kar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "NN", "$(", "NN", "$(", "NN", "$(", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}