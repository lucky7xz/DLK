{"textgrid.poem.62703": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Unter das, von der Jungfer Dennern zu Hamburg, entworfene Gem\u00e4hlde, des Herrn Hofrath Weichmanns", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Pinsel eines Frauenzimmers trift ungemein, und stellt allhier", "tokens": ["Der", "Pin\u00b7sel", "ei\u00b7nes", "Frau\u00b7en\u00b7zim\u00b7mers", "trift", "un\u00b7ge\u00b7mein", ",", "und", "stellt", "all\u00b7hier"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV", "$,", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Allein den hocherhabnen Geist vermag auch in den sch\u00f6nsten Bildern", "tokens": ["Al\u00b7lein", "den", "ho\u00b7cher\u00b7hab\u00b7nen", "Geist", "ver\u00b7mag", "auch", "in", "den", "sch\u00f6ns\u00b7ten", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.4": {"text": "Kein K\u00fcnstler, noch vielweniger ein Frauenzimmer abzuschildern.", "tokens": ["Kein", "K\u00fcnst\u00b7ler", ",", "noch", "viel\u00b7we\u00b7ni\u00b7ger", "ein", "Frau\u00b7en\u00b7zim\u00b7mer", "ab\u00b7zu\u00b7schil\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "PIS", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}}}}