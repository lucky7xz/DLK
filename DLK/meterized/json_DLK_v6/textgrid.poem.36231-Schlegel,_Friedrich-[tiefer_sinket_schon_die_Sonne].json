{"textgrid.poem.36231": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[tiefer sinket schon die Sonne]", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tiefer sinket schon die Sonne,", "tokens": ["Tie\u00b7fer", "sin\u00b7ket", "schon", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es atmet alles Ruhe,", "tokens": ["Und", "es", "at\u00b7met", "al\u00b7les", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tages Arbeit ist vollendet,", "tokens": ["Ta\u00b7ges", "Ar\u00b7beit", "ist", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Kinder scherzen munter.", "tokens": ["Und", "die", "Kin\u00b7der", "scher\u00b7zen", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gr\u00fcner gl\u00e4nzt die gr\u00fcne Erde,", "tokens": ["Gr\u00fc\u00b7ner", "gl\u00e4nzt", "die", "gr\u00fc\u00b7ne", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eh' die Sonne ganz versunken;", "tokens": ["Eh'", "die", "Son\u00b7ne", "ganz", "ver\u00b7sun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Milden Balsam hauchen leise", "tokens": ["Mil\u00b7den", "Bal\u00b7sam", "hau\u00b7chen", "lei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In die L\u00fcfte nun die Blumen,", "tokens": ["In", "die", "L\u00fcf\u00b7te", "nun", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Der die Seeele zart ber\u00fchret,", "tokens": ["Der", "die", "See\u00b7e\u00b7le", "zart", "be\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Wenn die Sinne selig trunken.", "tokens": ["Wenn", "die", "Sin\u00b7ne", "se\u00b7lig", "trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Kleine V\u00f6gel, ferne Menschen,", "tokens": ["Klei\u00b7ne", "V\u00f6\u00b7gel", ",", "fer\u00b7ne", "Men\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Berge himmelan geschwungen,", "tokens": ["Ber\u00b7ge", "him\u00b7me\u00b7lan", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und der gro\u00dfe Silberstrom,", "tokens": ["Und", "der", "gro\u00b7\u00dfe", "Sil\u00b7ber\u00b7strom", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Der im Tale schlank gewunden;", "tokens": ["Der", "im", "Ta\u00b7le", "schlank", "ge\u00b7wun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADJD", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Alles scheint dem Dichter redend,", "tokens": ["Al\u00b7les", "scheint", "dem", "Dich\u00b7ter", "re\u00b7dend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Denn er hat den Sinn gefunden;", "tokens": ["Denn", "er", "hat", "den", "Sinn", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und das All ein einzig Chor,", "tokens": ["Und", "das", "All", "ein", "ein\u00b7zig", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Manches Lied aus Einem Munde.", "tokens": ["Man\u00b7ches", "Lied", "aus", "Ei\u00b7nem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}