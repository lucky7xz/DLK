{"textgrid.poem.48902": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Hempel,", "genre": "verse", "period": "N.A.", "pub_year": 1754, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Winterlandschaft, die dein Pinsel hier gebiert,", "tokens": ["Die", "Win\u00b7ter\u00b7land\u00b7schaft", ",", "die", "dein", "Pin\u00b7sel", "hier", "ge\u00b7biert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist furchtbar, wie der Winter selbst; ich seh sie an, mich friert.", "tokens": ["Ist", "furcht\u00b7bar", ",", "wie", "der", "Win\u00b7ter", "selbst", ";", "ich", "seh", "sie", "an", ",", "mich", "friert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PWAV", "ART", "NN", "ADV", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}