{"dta.poem.9587": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er sendet ihr blumen.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du wirst/ Aurora/ dich der blumen ja nicht sch\u00e4men/", "tokens": ["Du", "wirst", "/", "Au\u00b7ro\u00b7ra", "/", "dich", "der", "blu\u00b7men", "ja", "nicht", "sch\u00e4\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "NE", "$(", "PPER", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die meine schlechte faust dir zum geschencke beut;", "tokens": ["Die", "mei\u00b7ne", "schlech\u00b7te", "faust", "dir", "zum", "ge\u00b7schen\u00b7cke", "beut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie seynd ein opffer/ das man deiner sch\u00f6nheit streut/", "tokens": ["Sie", "seynd", "ein", "opf\u00b7fer", "/", "das", "man", "dei\u00b7ner", "sch\u00f6n\u00b7heit", "streut", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "PRELS", "PIS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und das die g\u00f6tter selbst von menschen h\u00e4uden nehmen/", "tokens": ["Und", "das", "die", "g\u00f6t\u00b7ter", "selbst", "von", "men\u00b7schen", "h\u00e4u\u00b7den", "neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ART", "ADJA", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer wolte sie auch nicht zu lieben sich bequemen/", "tokens": ["Wer", "wol\u00b7te", "sie", "auch", "nicht", "zu", "lie\u00b7ben", "sich", "be\u00b7que\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "PTKNEG", "PTKZU", "VVFIN", "PRF", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da sie ein meister-st\u00fcck des himmels und der zeit/", "tokens": ["Da", "sie", "ein", "meis\u00b7ter\u00b7st\u00fcck", "des", "him\u00b7mels", "und", "der", "zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und mit so reichem schmuck gezieret und bekleidt/", "tokens": ["Und", "mit", "so", "rei\u00b7chem", "schmuck", "ge\u00b7zie\u00b7ret", "und", "be\u00b7kleidt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN", "VVPP", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als ob sie aus dem schoo\u00df der Venus selber k\u00e4men?", "tokens": ["Als", "ob", "sie", "aus", "dem", "schoo\u00df", "der", "Ve\u00b7nus", "sel\u00b7ber", "k\u00e4\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ART", "VVFIN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wiewohl sie aber nun der sonnen t\u00f6chter seyn/", "tokens": ["Wie\u00b7wohl", "sie", "a\u00b7ber", "nun", "der", "son\u00b7nen", "t\u00f6ch\u00b7ter", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und auch den perlen selbst entziehen ihren scheln/", "tokens": ["Und", "auch", "den", "per\u00b7len", "selbst", "ent\u00b7zie\u00b7hen", "ih\u00b7ren", "scheln", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADV", "VVFIN", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wird ihnen doch der glantz durch deine pracht benommen/", "tokens": ["Wird", "ih\u00b7nen", "doch", "der", "glantz", "durch", "dei\u00b7ne", "pracht", "be\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und siehstu dieses nicht itzt/ da sie zu dir kommen/", "tokens": ["Und", "sieh\u00b7stu", "die\u00b7ses", "nicht", "itzt", "/", "da", "sie", "zu", "dir", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PDS", "PTKNEG", "ADV", "$(", "KOUS", "PPER", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie gerne sie gestehn/ als halb verwelckte leichen/", "tokens": ["Wie", "ger\u00b7ne", "sie", "ge\u00b7stehn", "/", "als", "halb", "ver\u00b7welck\u00b7te", "lei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVPP", "$(", "KOUS", "ADJD", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df irrd\u2019sche sch\u00f6nheit pflegt der himmlischen zu weichen?", "tokens": ["Da\u00df", "irrd'\u00b7sche", "sch\u00f6n\u00b7heit", "pflegt", "der", "himm\u00b7li\u00b7schen", "zu", "wei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}