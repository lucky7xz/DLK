{"textgrid.poem.48855": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "80. An Adelfien", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ists wahr, Adelfie, als wie man sagt vor wahr,", "tokens": ["Ists", "wahr", ",", "A\u00b7del\u00b7fie", ",", "als", "wie", "man", "sagt", "vor", "wahr", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "NE", "$,", "KOUS", "PWAV", "PIS", "VVFIN", "APPR", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "du habest, also bald ich sei von dir gezogen,", "tokens": ["du", "ha\u00b7best", ",", "al\u00b7so", "bald", "ich", "sei", "von", "dir", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADV", "ADV", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "mit eines Andern Gunst der Freundschaft so gepflogen,", "tokens": ["mit", "ei\u00b7nes", "An\u00b7dern", "Gunst", "der", "Freund\u00b7schaft", "so", "ge\u00b7pflo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df dus ihm zugesagt und nun auch Braut seist gar?", "tokens": ["da\u00df", "dus", "ihm", "zu\u00b7ge\u00b7sagt", "und", "nun", "auch", "Braut", "seist", "gar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "VVPP", "KON", "ADV", "ADV", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fcrcht' und glaub' es fast. Am allermeisten zwar,", "tokens": ["Ich", "f\u00fcrcht'", "und", "glaub'", "es", "fast", ".", "Am", "al\u00b7ler\u00b7meis\u00b7ten", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$.", "APPRART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df etwan dich hierzu mein langer Weg bewogen", "tokens": ["da\u00df", "et\u00b7wan", "dich", "hier\u00b7zu", "mein", "lan\u00b7ger", "Weg", "be\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PRF", "PAV", "PPOSAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und ein verg\u00e4lltes Maul dir etwas vorgelogen,", "tokens": ["und", "ein", "ver\u00b7g\u00e4ll\u00b7tes", "Maul", "dir", "et\u00b7was", "vor\u00b7ge\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PPER", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "damit du dich und mich so setzest in Gefahr.", "tokens": ["da\u00b7mit", "du", "dich", "und", "mich", "so", "set\u00b7zest", "in", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "KON", "PPER", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich f\u00fcrcht' und glaub es fast. Nichts wird so hoch versprochen,", "tokens": ["Ich", "f\u00fcrcht'", "und", "glaub", "es", "fast", ".", "Nichts", "wird", "so", "hoch", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$.", "PIS", "VAFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das sch\u00e4ndlich werde nicht durch Mi\u00dftren' itzt gebrochen.", "tokens": ["das", "sch\u00e4nd\u00b7lich", "wer\u00b7de", "nicht", "durch", "Mi\u00df\u00b7tren'", "itzt", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "PTKNEG", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "War, Schwester, das dein Mut, der sich so hoch verschwur?", "tokens": ["War", ",", "Schwes\u00b7ter", ",", "das", "dein", "Mut", ",", "der", "sich", "so", "hoch", "ver\u00b7schwur", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "$,", "PRELS", "PPOSAT", "NN", "$,", "PRELS", "PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Hast du mir das getan, so werd' ich einer Frauen", "tokens": ["Hast", "du", "mir", "das", "ge\u00b7tan", ",", "so", "werd'", "ich", "ei\u00b7ner", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "PDS", "VVPP", "$,", "ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "auf ihren h\u00f6chsten Eid nicht so viel k\u00fcnftig trauen.", "tokens": ["auf", "ih\u00b7ren", "h\u00f6chs\u00b7ten", "Eid", "nicht", "so", "viel", "k\u00fcnf\u00b7tig", "trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Verzeiht mirs Alle denn, die Eine macht es nur!", "tokens": ["Ver\u00b7zeiht", "mirs", "Al\u00b7le", "denn", ",", "die", "Ei\u00b7ne", "macht", "es", "nur", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PIAT", "ADV", "$,", "PRELS", "PIS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}