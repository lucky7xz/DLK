{"textgrid.poem.66632": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Immergr\u00fcn", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus d\u00fcrren Gr\u00e4sern gr\u00fc\u00dft mich Immergr\u00fcn.", "tokens": ["Aus", "d\u00fcr\u00b7ren", "Gr\u00e4\u00b7sern", "gr\u00fc\u00dft", "mich", "Im\u00b7mer\u00b7gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Rasch her damit und an die Brust gesteckt!", "tokens": ["Rasch", "her", "da\u00b7mit", "und", "an", "die", "Brust", "ge\u00b7steckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PAV", "KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch einmal sei's gewagt und hoffnungsk\u00fchn", "tokens": ["Noch", "ein\u00b7mal", "sei's", "ge\u00b7wagt", "und", "hoff\u00b7nungs\u00b7k\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "VVPP", "KON", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sei das Gespenst in seine Gruft geschreckt!", "tokens": ["Sei", "das", "Ge\u00b7spenst", "in", "sei\u00b7ne", "Gruft", "ge\u00b7schreckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nein, werter Freund, der du die Schlinge wirfst", "tokens": ["Nein", ",", "wer\u00b7ter", "Freund", ",", "der", "du", "die", "Schlin\u00b7ge", "wirfst"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nach meiner Seele sinkendem Genick,", "tokens": ["Nach", "mei\u00b7ner", "See\u00b7le", "sin\u00b7ken\u00b7dem", "Ge\u00b7nick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vampir, der meines Geistes Blut du schl\u00fcrfst,", "tokens": ["Vam\u00b7pir", ",", "der", "mei\u00b7nes", "Geis\u00b7tes", "Blut", "du", "schl\u00fcrfst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPOSAT", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Noch einmal in den Strau\u00df um mein Geschick!", "tokens": ["Noch", "ein\u00b7mal", "in", "den", "Strau\u00df", "um", "mein", "Ge\u00b7schick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zu Boden ri\u00df mich Unnatur der Zeit,", "tokens": ["Zu", "Bo\u00b7den", "ri\u00df", "mich", "Un\u00b7na\u00b7tur", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mutter Natur, reck deinen Sohn empor!", "tokens": ["Mut\u00b7ter", "Na\u00b7tur", ",", "reck", "dei\u00b7nen", "Sohn", "em\u00b7por", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Verfault, ihr Gr\u00e4ser der Vergangenheit,", "tokens": ["Ver\u00b7fault", ",", "ihr", "Gr\u00e4\u00b7ser", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und du, neu Leben, quill, o quill hervor!", "tokens": ["Und", "du", ",", "neu", "Le\u00b7ben", ",", "quill", ",", "o", "quill", "her\u00b7vor", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJD", "NN", "$,", "ADJD", "$,", "FM", "FM", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}