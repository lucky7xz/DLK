{"dta.poem.21037": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ci.  \n Einer Gan\u00df.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Der Schwingen bestes Theil f\u00fchrt manches F\u00fcrsten", "tokens": ["Der", "Schwin\u00b7gen", "bes\u00b7tes", "Theil", "f\u00fchrt", "man\u00b7ches", "F\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hand/", "tokens": ["Hand", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Die Gurgel tr\u00e4get Zwirn/ die Feder ehrt das", "tokens": ["Die", "Gur\u00b7gel", "tr\u00e4\u00b7get", "Zwirn", "/", "die", "Fe\u00b7der", "ehrt", "das"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "$(", "ART", "NN", "VVFIN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Land.", "tokens": ["Land", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Wird dieser kleine Raum auf das Privet genom-", "tokens": ["Wird", "die\u00b7ser", "klei\u00b7ne", "Raum", "auf", "das", "Pri\u00b7vet", "ge\u00b7nom"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "men/", "tokens": ["men", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.7": {"text": "So ist die Grabeschrifft zu ihrem C\u00f6rper kom-", "tokens": ["So", "ist", "die", "Gra\u00b7be\u00b7schrifft", "zu", "ih\u00b7rem", "C\u00f6r\u00b7per", "kom"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "men.", "tokens": ["men", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-", "measure": "single.down"}}}}}