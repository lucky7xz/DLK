{"textgrid.poem.51213": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "2. Die Lor' sitzt im Garten", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Lor' sitzt im Garten,", "tokens": ["Die", "Lor'", "sitzt", "im", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Kehrt den R\u00fccken zumal", "tokens": ["Kehrt", "den", "R\u00fc\u00b7cken", "zu\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "KOUS"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und verbirgt mir der Augen", "tokens": ["Und", "ver\u00b7birgt", "mir", "der", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Himmlischen Strahl.", "tokens": ["Himm\u00b7li\u00b7schen", "Strahl", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ihr goldbrauner Haarwuchs", "tokens": ["Ihr", "gold\u00b7brau\u00b7ner", "Haar\u00b7wuchs"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Weht \u00fcber den Zaun;", "tokens": ["Weht", "\u00fc\u00b7ber", "den", "Zaun", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Den Rotmund, das Wei\u00dfkinn", "tokens": ["Den", "Rot\u00b7mund", ",", "das", "Wei\u00df\u00b7kinn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Doch l\u00e4\u00dft sie nicht schaun.", "tokens": ["Doch", "l\u00e4\u00dft", "sie", "nicht", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Sie l\u00e4sset erklingen", "tokens": ["Sie", "l\u00e4s\u00b7set", "er\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ihrer Stimme Get\u00f6n;", "tokens": ["Ih\u00b7rer", "Stim\u00b7me", "Ge\u00b7t\u00f6n", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "O du boshafte Hexe,", "tokens": ["O", "du", "bos\u00b7haf\u00b7te", "He\u00b7xe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.4": {"text": "Wie klingt es so sch\u00f6n!", "tokens": ["Wie", "klingt", "es", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}