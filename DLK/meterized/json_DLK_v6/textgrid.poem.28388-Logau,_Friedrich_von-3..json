{"textgrid.poem.28388": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie th\u00f6rlich handeln doch, die manchmal so erwarmen", "tokens": ["Wie", "th\u00f6r\u00b7lich", "han\u00b7deln", "doch", ",", "die", "manch\u00b7mal", "so", "er\u00b7war\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ADV", "$,", "PRELS", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff unser Blut und Gut! sie machen uns zu Armen,", "tokens": ["Auff", "un\u00b7ser", "Blut", "und", "Gut", "!", "sie", "ma\u00b7chen", "uns", "zu", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJD", "$.", "PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auff da\u00df so Gottes Reich und Evangelium", "tokens": ["Auff", "da\u00df", "so", "Got\u00b7tes", "Reich", "und", "E\u00b7van\u00b7ge\u00b7li\u00b7um"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ADV", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von ihnn, den Reichen, weg zu uns, den Armen, kumm.", "tokens": ["Von", "ihnn", ",", "den", "Rei\u00b7chen", ",", "weg", "zu", "uns", ",", "den", "Ar\u00b7men", ",", "kumm", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "NN", "$,", "ADV", "APPR", "PPER", "$,", "ART", "NN", "$,", "ADJ