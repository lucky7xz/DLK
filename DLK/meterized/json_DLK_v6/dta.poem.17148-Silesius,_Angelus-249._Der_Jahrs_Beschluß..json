{"dta.poem.17148": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "249.  Der Jahrs Beschlu\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Es wird das alte Jahr/ das sich nu schleust/ gehalten", "tokens": ["Es", "wird", "das", "al\u00b7te", "Jahr", "/", "das", "sich", "nu", "schleust", "/", "ge\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "PRELS", "PRF", "ADV", "VVFIN", "$(", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als wanns vergangen w\u00e4r\u2019: und di\u00df ist wahr mein Krist/", "tokens": ["Als", "wanns", "ver\u00b7gan\u00b7gen", "w\u00e4r'", ":", "und", "di\u00df", "ist", "wahr", "mein", "Krist", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVPP", "VAFIN", "$.", "KON", "PDS", "VAFIN", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo du ein Neuer Mensch in GOtt geworden bist:", "tokens": ["Wo", "du", "ein", "Neu\u00b7er", "Mensch", "in", "Gott", "ge\u00b7wor\u00b7den", "bist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ists nicht; so lebstu noch wahrhafftig in dem alten.", "tokens": ["Ists", "nicht", ";", "so", "lebs\u00b7tu", "noch", "wahr\u00b7haff\u00b7tig", "in", "dem", "al\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$.", "ADV", "VVFIN", "ADV", "ADJD", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}