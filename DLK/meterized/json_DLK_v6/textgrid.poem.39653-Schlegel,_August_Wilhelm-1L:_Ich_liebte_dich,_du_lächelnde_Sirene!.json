{"textgrid.poem.39653": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich liebte dich, du l\u00e4chelnde Sirene!", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liebte dich, du l\u00e4chelnde Sirene!", "tokens": ["Ich", "lieb\u00b7te", "dich", ",", "du", "l\u00e4\u00b7cheln\u00b7de", "Si\u00b7re\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nun lebe wohl! nun lieb' ich dich nicht mehr.", "tokens": ["Nun", "le\u00b7be", "wohl", "!", "nun", "lieb'", "ich", "dich", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$.", "ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ha! brauche nur die Lockung s\u00fc\u00dfer T\u00f6ne!", "tokens": ["Ha", "!", "brau\u00b7che", "nur", "die", "Lo\u00b7ckung", "s\u00fc\u00b7\u00dfer", "T\u00f6\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kalt ist mein Sinn, und taub ist mein Geh\u00f6r.", "tokens": ["Kalt", "ist", "mein", "Sinn", ",", "und", "taub", "ist", "mein", "Ge\u00b7h\u00f6r", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.5": {"text": "Kaum hab' ich k\u00fchn den Arm erhoben,", "tokens": ["Kaum", "hab'", "ich", "k\u00fchn", "den", "Arm", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So ist dein schlaues Netz wie leichte Spreu zerstoben. \u2013", "tokens": ["So", "ist", "dein", "schlau\u00b7es", "Netz", "wie", "leich\u00b7te", "Spreu", "zer\u00b7sto\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "KOKOM", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Sohn Apolls ist der, den du geh\u00f6hnet,", "tokens": ["Ein", "Sohn", "A\u00b7polls", "ist", "der", ",", "den", "du", "ge\u00b7h\u00f6h\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Sohn Apolls, im Saitenspiel ge\u00fcbt!", "tokens": ["Ein", "Sohn", "A\u00b7polls", ",", "im", "Sai\u00b7ten\u00b7spiel", "ge\u00b7\u00fcbt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr dich nur h\u00e4tte sein Gesang get\u00f6net,", "tokens": ["F\u00fcr", "dich", "nur", "h\u00e4t\u00b7te", "sein", "Ge\u00b7sang", "ge\u00b7t\u00f6\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bacchidion, h\u00e4tt'st du ihn treu geliebt.", "tokens": ["Bac\u00b7chi\u00b7di\u00b7on", ",", "h\u00e4tt'st", "du", "ihn", "treu", "ge\u00b7liebt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch jetzt verbirg dich, fleuch mit Eile!", "tokens": ["Doch", "jetzt", "ver\u00b7birg", "dich", ",", "fleuch", "mit", "Ei\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Bogen droht, und horch! im K\u00f6cher klirren Pfeile.", "tokens": ["Mein", "Bo\u00b7gen", "droht", ",", "und", "horch", "!", "im", "K\u00f6\u00b7cher", "klir\u00b7ren", "Pfei\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KON", "VVIMP", "$.", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vernehme dann mein Lied wen eine Schlange,", "tokens": ["Ver\u00b7neh\u00b7me", "dann", "mein", "Lied", "wen", "ei\u00b7ne", "Schlan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "PWS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie diese hier, umwindet! Bei'm Apoll,", "tokens": ["Wie", "die\u00b7se", "hier", ",", "um\u00b7win\u00b7det", "!", "Bei'm", "A\u00b7poll", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADV", "$,", "VVFIN", "$.", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich will sie zeichnen, da\u00df der M\u00e4nner Wange", "tokens": ["Ich", "will", "sie", "zeich\u00b7nen", ",", "da\u00df", "der", "M\u00e4n\u00b7ner", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bei ihrem Namen zornig gl\u00fchen soll!", "tokens": ["Bei", "ih\u00b7rem", "Na\u00b7men", "zor\u00b7nig", "gl\u00fc\u00b7hen", "soll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie zeichnen, da\u00df mit kaltem Schrecken", "tokens": ["Sie", "zeich\u00b7nen", ",", "da\u00df", "mit", "kal\u00b7tem", "Schre\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Jungfrau'n, wo sie geht, ihr Angesicht verstecken! \u2013", "tokens": ["Die", "Jung\u00b7frau'n", ",", "wo", "sie", "geht", ",", "ihr", "An\u00b7ge\u00b7sicht", "ver\u00b7ste\u00b7cken", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich irrte durch entleg'ne Myrtenhaine", "tokens": ["Ich", "irr\u00b7te", "durch", "ent\u00b7leg'\u00b7ne", "Myr\u00b7ten\u00b7hai\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu einem moo\u00dfumkr\u00e4nzten Silberbach.", "tokens": ["Zu", "ei\u00b7nem", "moo\u00b7\u00dfum\u00b7kr\u00e4nz\u00b7ten", "Sil\u00b7ber\u00b7bach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich warf mich hin bei Hespers bla\u00dfem Scheine;", "tokens": ["Ich", "warf", "mich", "hin", "bei", "Hes\u00b7pers", "bla\u00b7\u00dfem", "Schei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Auge schlich des Baches Wellen nach;", "tokens": ["Mein", "Au\u00b7ge", "schlich", "des", "Ba\u00b7ches", "Wel\u00b7len", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich sank in halben Schlummer nieder", "tokens": ["Ich", "sank", "in", "hal\u00b7ben", "Schlum\u00b7mer", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und lauscht' im Arm der Ruh auf Nachtigallenlieder.", "tokens": ["Und", "lauscht'", "im", "Arm", "der", "Ruh", "auf", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da sah ich in der B\u00fcsche gr\u00fcnen Hallen", "tokens": ["Da", "sah", "ich", "in", "der", "B\u00fc\u00b7sche", "gr\u00fc\u00b7nen", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein junges Weib voll Liebreiz sich ergehn;", "tokens": ["Ein", "jun\u00b7ges", "Weib", "voll", "Lieb\u00b7reiz", "sich", "er\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich sah das Weib mit frohem Wohlgefallen,", "tokens": ["Ich", "sah", "das", "Weib", "mit", "fro\u00b7hem", "Wohl\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und konnte mich an ihr nicht m\u00fcde sehn.", "tokens": ["Und", "konn\u00b7te", "mich", "an", "ihr", "nicht", "m\u00fc\u00b7de", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPR", "PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ich warb um einen ihrer Blicke;", "tokens": ["Ich", "warb", "um", "ei\u00b7nen", "ih\u00b7rer", "Bli\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie merkt' es, und entfloh mit anmuthsvoller T\u00fccke.", "tokens": ["Sie", "merkt'", "es", ",", "und", "ent\u00b7floh", "mit", "an\u00b7muths\u00b7vol\u00b7ler", "T\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Es schlug mein Herz; ich sprang empor vom Sitze,", "tokens": ["Es", "schlug", "mein", "Herz", ";", "ich", "sprang", "em\u00b7por", "vom", "Sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und suchte lang umsonst im d\u00fcstern Hain:", "tokens": ["Und", "such\u00b7te", "lang", "um\u00b7sonst", "im", "d\u00fcs\u00b7tern", "Hain", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch pl\u00f6tzlich kam sie, gleich dem raschen Blitze,", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "kam", "sie", ",", "gleich", "dem", "ra\u00b7schen", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu mir heran, und lud mich freundlich ein.", "tokens": ["Zu", "mir", "he\u00b7ran", ",", "und", "lud", "mich", "freund\u00b7lich", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Noch sch\u00fcchtern halb, und halb verwegen,", "tokens": ["Noch", "sch\u00fcch\u00b7tern", "halb", ",", "und", "halb", "ver\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Naht' ich mich ihrem Ku\u00df; mir kam ihr Ku\u00df entgegen.", "tokens": ["Naht'", "ich", "mich", "ih\u00b7rem", "Ku\u00df", ";", "mir", "kam", "ihr", "Ku\u00df", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ihr Himmlischen! wie ich mit hei\u00dfen Z\u00fcgen", "tokens": ["Ihr", "Himm\u00b7li\u00b7schen", "!", "wie", "ich", "mit", "hei\u00b7\u00dfen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den dargebotnen Wonnebecher trank!", "tokens": ["Den", "dar\u00b7ge\u00b7bot\u00b7nen", "Won\u00b7ne\u00b7be\u00b7cher", "trank", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie lie\u00df mein Haupt an ihre Brust sich schmiegen,", "tokens": ["Sie", "lie\u00df", "mein", "Haupt", "an", "ih\u00b7re", "Brust", "sich", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sah mich an, als w\u00e4r' sie liebekrank;", "tokens": ["Und", "sah", "mich", "an", ",", "als", "w\u00e4r'", "sie", "lie\u00b7be\u00b7krank", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "KOKOM", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An meinen Lippen, meinen Wangen,", "tokens": ["An", "mei\u00b7nen", "Lip\u00b7pen", ",", "mei\u00b7nen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fchlt' ich den w\u00e4rmsten Mund, wie festgezaubert, hangen.", "tokens": ["F\u00fchlt'", "ich", "den", "w\u00e4rms\u00b7ten", "Mund", ",", "wie", "fest\u00b7ge\u00b7zau\u00b7bert", ",", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PWAV", "VVPP", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Aus meinem Herzen spr\u00fchten tausend Funken", "tokens": ["Aus", "mei\u00b7nem", "Her\u00b7zen", "spr\u00fch\u00b7ten", "tau\u00b7send", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Inbrunst durch mein wildemp\u00f6rtes Blut,", "tokens": ["Der", "In\u00b7brunst", "durch", "mein", "wil\u00b7dem\u00b7p\u00f6r\u00b7tes", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Aus meinem Herzen, das ich wonnetrunken", "tokens": ["Aus", "mei\u00b7nem", "Her\u00b7zen", ",", "das", "ich", "won\u00b7ne\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An ihr's gepress't mit niegef\u00fchlter Glut;", "tokens": ["An", "ih\u00b7r's", "ge\u00b7press't", "mit", "nie\u00b7ge\u00b7f\u00fchl\u00b7ter", "Glut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Und Seufzer, zart und innig, riefen", "tokens": ["Und", "Seuf\u00b7zer", ",", "zart", "und", "in\u00b7nig", ",", "rie\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "NN", "$,", "ADJD", "KON", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr klagendes Gegirr' aus meiner Seele Tiefen.", "tokens": ["Ihr", "kla\u00b7gen\u00b7des", "Ge\u00b7girr'", "aus", "mei\u00b7ner", "See\u00b7le", "Tie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Sie gab mir viel: o h\u00e4tt' ich mehr gefodert,", "tokens": ["Sie", "gab", "mir", "viel", ":", "o", "h\u00e4tt'", "ich", "mehr", "ge\u00b7fo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "FM", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Thr\u00e4nen, Fleh'n und Ungest\u00fcm gebraucht! \u2013", "tokens": ["Und", "Thr\u00e4\u00b7nen", ",", "Fleh'n", "und", "Un\u00b7ge\u00b7st\u00fcm", "ge\u00b7braucht", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Geist, ich schw\u00f6r's, er w\u00e4re mir entlodert,", "tokens": ["Mein", "Geist", ",", "ich", "schw\u00f6r's", ",", "er", "w\u00e4\u00b7re", "mir", "ent\u00b7lo\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor Ueberma\u00df der Freuden ausgehaucht,", "tokens": ["Vor", "Ue\u00b7ber\u00b7ma\u00df", "der", "Freu\u00b7den", "aus\u00b7ge\u00b7haucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "H\u00e4tt' ich, was Liebe nur verlangen", "tokens": ["H\u00e4tt'", "ich", ",", "was", "Lie\u00b7be", "nur", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWS", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und geben kann, von ihr in dieser Stund' empfangen.", "tokens": ["Und", "ge\u00b7ben", "kann", ",", "von", "ihr", "in", "die\u00b7ser", "Stund'", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "$,", "APPR", "PPER", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Wie Luna sich, bei'm Weh'n der Morgenk\u00fchle,", "tokens": ["Wie", "Lu\u00b7na", "sich", ",", "bei'm", "Weh'n", "der", "Mor\u00b7gen\u00b7k\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus ihres Sch\u00e4fers treuen Armen wand,", "tokens": ["Aus", "ih\u00b7res", "Sch\u00e4\u00b7fers", "treu\u00b7en", "Ar\u00b7men", "wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So floh sie jetzt nach langgepflog'nem Spiele,", "tokens": ["So", "floh", "sie", "jetzt", "nach", "lang\u00b7ge\u00b7pflo\u00b7g'\u00b7nem", "Spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und dr\u00fcckt' im Flieh'n wehm\u00fcthig mir die Hand.", "tokens": ["Und", "dr\u00fcckt'", "im", "Flieh'n", "weh\u00b7m\u00fct\u00b7hig", "mir", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADJD", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch ihrer K\u00fcsse Geister wallten", "tokens": ["Doch", "ih\u00b7rer", "K\u00fcs\u00b7se", "Geis\u00b7ter", "wall\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Um meine Phantasie in holden Traumgestalten.", "tokens": ["Um", "mei\u00b7ne", "Phan\u00b7ta\u00b7sie", "in", "hol\u00b7den", "Traum\u00b7ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Es labte sich am Gaukeln dieser Tr\u00e4ume", "tokens": ["Es", "lab\u00b7te", "sich", "am", "Gau\u00b7keln", "die\u00b7ser", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Herz, von Ahndung und Erinnrung voll,", "tokens": ["Mein", "Herz", ",", "von", "Ahn\u00b7dung", "und", "E\u00b7rinn\u00b7rung", "voll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als pl\u00f6tzlich eine Stimme durch die B\u00e4ume,", "tokens": ["Als", "pl\u00f6tz\u00b7lich", "ei\u00b7ne", "Stim\u00b7me", "durch", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbermanne dich, erwache, J\u00fcngling!\u00ab scholl.", "tokens": ["\u00bb", "er\u00b7man\u00b7ne", "dich", ",", "er\u00b7wa\u00b7che", ",", "J\u00fcng\u00b7ling", "!", "\u00ab", "scholl", "."], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "VVFIN", "$,", "NN", "$.", "$(", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gleich einem neblichten Gestirne", "tokens": ["Gleich", "ei\u00b7nem", "neb\u00b7lich\u00b7ten", "Ge\u00b7stir\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Erschien mein Genius mit grambew\u00f6lkter Stirne.", "tokens": ["Er\u00b7schien", "mein", "Ge\u00b7nius", "mit", "gram\u00b7be\u00b7w\u00f6lk\u00b7ter", "Stir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "pu