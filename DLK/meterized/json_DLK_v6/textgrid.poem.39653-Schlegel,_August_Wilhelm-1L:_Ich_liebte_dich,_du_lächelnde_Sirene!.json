["ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Indessen jener Wahnberauschte", "tokens": ["In\u00b7des\u00b7sen", "je\u00b7ner", "Wahn\u00b7be\u00b7rauschte"], "token_info": ["word", "word", "word"], "pos": ["NN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Weibes Treue nicht um eine Welt vertauschte.", "tokens": ["Des", "Wei\u00b7bes", "Treu\u00b7e", "nicht", "um", "ei\u00b7ne", "Welt", "ver\u00b7tauschte", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Jetzt l\u00e4chelt sie, wie im April die Sonne", "tokens": ["Jetzt", "l\u00e4\u00b7chelt", "sie", ",", "wie", "im", "Ap\u00b7ril", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPRART", "NN", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem Wanderer, ihm wieder; und er girrt,", "tokens": ["Dem", "Wan\u00b7de\u00b7rer", ",", "ihm", "wie\u00b7der", ";", "und", "er", "girrt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "ADV", "$.", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von ihr umarmt, und trinkt den Quell der Wonne,", "tokens": ["Von", "ihr", "um\u00b7armt", ",", "und", "trinkt", "den", "Quell", "der", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.