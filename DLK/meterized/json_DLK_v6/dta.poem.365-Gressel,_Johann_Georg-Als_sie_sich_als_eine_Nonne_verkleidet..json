{"dta.poem.365": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie sich als eine Nonne verkleidet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die Masque ist recht gut/ allein das Fleisch der Nonnen/", "tokens": ["Die", "Mas\u00b7que", "ist", "recht", "gut", "/", "al\u00b7lein", "das", "Fleisch", "der", "Non\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$(", "ADV", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das fehlet dir mein Kind/ die Wollust wohnt in dir/", "tokens": ["Das", "feh\u00b7let", "dir", "mein", "Kind", "/", "die", "Wol\u00b7lust", "wohnt", "in", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN", "$(", "ART", "NN", "VVFIN", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Liebe Perlen Safft ist offt in dir zerronnen/", "tokens": ["Der", "Lie\u00b7be", "Per\u00b7len", "Safft", "ist", "offt", "in", "dir", "zer\u00b7ron\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du suchst den Zeit-Vertreib der Nonnen nicht herf\u00fcr/", "tokens": ["Du", "suchst", "den", "Zeit\u00b7Ver\u00b7treib", "der", "Non\u00b7nen", "nicht", "her\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein wesentliches Werck soll deine Sehnsucht stillen.", "tokens": ["Ein", "we\u00b7sent\u00b7li\u00b7ches", "Werck", "soll", "dei\u00b7ne", "Sehn\u00b7sucht", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das Kleid soll nur die Brust und nicht die Brunst verh\u00fcllen.", "tokens": ["Das", "Kleid", "soll", "nur", "die", "Brust", "und", "nicht", "die", "Brunst", "ver\u00b7h\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "NN", "KON", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}