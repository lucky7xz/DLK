{"textgrid.poem.33574": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Sehnsucht und von Mitgef\u00fchl erf\u00fcllt,", "tokens": ["Von", "Sehn\u00b7sucht", "und", "von", "Mit\u00b7ge\u00b7f\u00fchl", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wird niemals dich mein Geist verlassen,", "tokens": ["Wird", "nie\u00b7mals", "dich", "mein", "Geist", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er w\u00fcrde dich auch nacht- und sturmumh\u00fcllt", "tokens": ["Er", "w\u00fcr\u00b7de", "dich", "auch", "nacht", "und", "stur\u00b7mum\u00b7h\u00fcllt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "TRUNC", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit liebender Gewalt umfassen.", "tokens": ["Mit", "lie\u00b7ben\u00b7der", "Ge\u00b7walt", "um\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und w\u00e4rst du noch so fern von mir,", "tokens": ["Und", "w\u00e4rst", "du", "noch", "so", "fern", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn dich ein Leiden tr\u00e4f', es schreckte", "tokens": ["Wenn", "dich", "ein", "Lei\u00b7den", "tr\u00e4f'", ",", "es", "schreck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie Donner mich empor und weckte", "tokens": ["Wie", "Don\u00b7ner", "mich", "em\u00b7por", "und", "weck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "PPER", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vom Schlaf mich auf und riefe mich zu dir.", "tokens": ["Vom", "Schlaf", "mich", "auf", "und", "rie\u00b7fe", "mich", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}