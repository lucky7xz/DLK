{"textgrid.poem.63779": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Mailand", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df du modern und halb franz\u00f6sisch seist,", "tokens": ["Da\u00df", "du", "mo\u00b7dern", "und", "halb", "fran\u00b7z\u00f6\u00b7sisch", "seist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "KON", "ADJD", "ADJD", "VAFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Vom Edelrost Italiens reingescheuert,", "tokens": ["Vom", "E\u00b7del\u00b7rost", "I\u00b7ta\u00b7li\u00b7ens", "rein\u00b7ge\u00b7scheu\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein blankes Klein-Paris, ward mir beteuert;", "tokens": ["Ein", "blan\u00b7kes", "Klein\u00b7Pa\u00b7ris", ",", "ward", "mir", "be\u00b7teu\u00b7ert", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Echt sei hier nur, da\u00df man Risotto speist.", "tokens": ["Echt", "sei", "hier", "nur", ",", "da\u00df", "man", "Ri\u00b7sot\u00b7to", "speist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "$,", "KOUS", "PIS", "NE", "VVFIN", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}}, "stanza.2": {"line.1": {"text": "Und doch, entschwand auch der gewalt'ge Geist,", "tokens": ["Und", "doch", ",", "ent\u00b7schwand", "auch", "der", "ge\u00b7walt'\u00b7ge", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der deine Adelsh\u00e4upter einst befeuert,", "tokens": ["Der", "dei\u00b7ne", "A\u00b7dels\u00b7h\u00e4up\u00b7ter", "einst", "be\u00b7feu\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Kampf mit \u00d6streich hast du ihn erneuert,", "tokens": ["Im", "Kampf", "mit", "\u00d6s\u00b7treich", "hast", "du", "ihn", "er\u00b7neu\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Ruhm, da\u00df nichts dich von Italien rei\u00dft.", "tokens": ["Den", "Ruhm", ",", "da\u00df", "nichts", "dich", "von", "I\u00b7ta\u00b7li\u00b7en", "rei\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PIS", "PRF", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Wo nur dein Name klingt, wird zweier Werke", "tokens": ["Wo", "nur", "dein", "Na\u00b7me", "klingt", ",", "wird", "zwei\u00b7er", "Wer\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VVFIN", "$,", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Gedacht, zu ew'gen Zierden dir errichtet,", "tokens": ["Ge\u00b7dacht", ",", "zu", "ew'\u00b7gen", "Zier\u00b7den", "dir", "er\u00b7rich\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie sch\u00f6nre nie italischen Geist erprobten.", "tokens": ["Wie", "sch\u00f6n\u00b7re", "nie", "i\u00b7ta\u00b7li\u00b7schen", "Geist", "er\u00b7prob\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Eins schuf des Lionardo heil'ge St\u00e4rke,", "tokens": ["Eins", "schuf", "des", "Li\u00b7o\u00b7nar\u00b7do", "heil'\u00b7ge", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das andre hat dein edler Sohn gedichtet:", "tokens": ["Das", "and\u00b7re", "hat", "dein", "ed\u00b7ler", "Sohn", "ge\u00b7dich\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das wundervolle Buch der zwei Verlobten.", "tokens": ["Das", "wun\u00b7der\u00b7vol\u00b7le", "Buch", "der", "zwei", "Ver\u00b7lob\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}