{"dta.poem.9919": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein gef\u00e4ngni\u00df ist viel sch\u00f6ner/ als der pr\u00e4chtigste pallast/", "tokens": ["Mein", "ge\u00b7f\u00e4ng\u00b7ni\u00df", "ist", "viel", "sch\u00f6\u00b7ner", "/", "als", "der", "pr\u00e4ch\u00b7tigs\u00b7te", "pal\u00b7last", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$(", "KOUS", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Meiner mutter liebes-eyfer ist mir eine rechte last/", "tokens": ["Mei\u00b7ner", "mut\u00b7ter", "lie\u00b7bes\u00b7ey\u00b7fer", "ist", "mir", "ei\u00b7ne", "rech\u00b7te", "last", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VAFIN", "PPER", "ART", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wenn man mich nicht ohn gefahr aus den armen ihr genom\u0303en/", "tokens": ["Wenn", "man", "mich", "nicht", "ohn", "ge\u00b7fahr", "aus", "den", "ar\u00b7men", "ihr", "ge\u00b7nom\u0303en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "PTKNEG", "APPR", "NN", "APPR", "ART", "ADJA", "PPER", "VVPP", "$("], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Pfleg zu f\u00fcrst und herren ich/ und zum gr\u00f6sten schatz zu kom\u0303en.", "tokens": ["Pfleg", "zu", "f\u00fcrst", "und", "her\u00b7ren", "ich", "/", "und", "zum", "gr\u00f6s\u00b7ten", "schatz", "zu", "kom\u0303en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKA", "ADJD", "KON", "VVFIN", "PPER", "$(", "KON", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}}}}