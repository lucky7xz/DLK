{"textgrid.poem.40780": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eine Schwalbe in den L\u00fcften, die sich nach dem S\u00fcden schwingt,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Schwalbe in den L\u00fcften, die sich nach dem S\u00fcden schwingt,", "tokens": ["Ei\u00b7ne", "Schwal\u00b7be", "in", "den", "L\u00fcf\u00b7ten", ",", "die", "sich", "nach", "dem", "S\u00fc\u00b7den", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Eine Kugel, die mit Knalle aus dem Rohr des Sch\u00fctzen springt,", "tokens": ["Ei\u00b7ne", "Ku\u00b7gel", ",", "die", "mit", "Knal\u00b7le", "aus", "dem", "Rohr", "des", "Sch\u00fct\u00b7zen", "springt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wollt' ums Ziel, wohin sie reisen, diese Zwei mein F\u00fcrwitz fragen,", "tokens": ["Wollt'", "ums", "Ziel", ",", "wo\u00b7hin", "sie", "rei\u00b7sen", ",", "die\u00b7se", "Zwei", "mein", "F\u00fcr\u00b7witz", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "$,", "PWAV", "PPER", "VVINF", "$,", "PDAT", "CARD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Eine sch\u00f6ne, lust'ge Antwort w\u00fc\u00dften beide wohl zu sagen.", "tokens": ["Ei\u00b7ne", "sch\u00f6\u00b7ne", ",", "lust'\u00b7ge", "Ant\u00b7wort", "w\u00fc\u00df\u00b7ten", "bei\u00b7de", "wohl", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "PIS", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "M\u00e4nner, die mit finstrem Mi\u00dftraun heitre Herzen ihr erf\u00fcllt,", "tokens": ["M\u00e4n\u00b7ner", ",", "die", "mit", "fins\u00b7trem", "Mi\u00df\u00b7traun", "heit\u00b7re", "Her\u00b7zen", "ihr", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Schuldlos Volk in Fesseln schmiedet, lichten Tag in Nacht verh\u00fcllt;", "tokens": ["Schuld\u00b7los", "Volk", "in", "Fes\u00b7seln", "schmie\u00b7det", ",", "lich\u00b7ten", "Tag", "in", "Nacht", "ver\u00b7h\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "VVFIN", "$,", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wollt' an euch dieselbe Frage neubegierig dreist ich wagen,", "tokens": ["Wollt'", "an", "euch", "die\u00b7sel\u00b7be", "Fra\u00b7ge", "neu\u00b7be\u00b7gie\u00b7rig", "dreist", "ich", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "PDAT", "NN", "ADJD", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "W\u00fc\u00dftet ihr solch' helle Antwort mir wohl auch darauf zu sagen?", "tokens": ["W\u00fc\u00df\u00b7tet", "ihr", "solch'", "hel\u00b7le", "Ant\u00b7wort", "mir", "wohl", "auch", "da\u00b7rauf", "zu", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "NN", "PPER", "ADV", "ADV", "PAV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "W\u00e4rt ihr nicht so fromm und sittsam, w\u00fcrd' ich fast zum Wahn gebracht,", "tokens": ["W\u00e4rt", "ihr", "nicht", "so", "fromm", "und", "sitt\u00b7sam", ",", "w\u00fcrd'", "ich", "fast", "zum", "Wahn", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "KON", "ADJD", "$,", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df verbotner Liebe pflegen, in der selbsterschaffnen Nacht,", "tokens": ["Da\u00df", "ver\u00b7bot\u00b7ner", "Lie\u00b7be", "pfle\u00b7gen", ",", "in", "der", "selbs\u00b7ter\u00b7schaff\u00b7nen", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Oder da\u00df ihr wollt im Dunkeln schleichen, Dieben gleich, nach Beute!", "tokens": ["O\u00b7der", "da\u00df", "ihr", "wollt", "im", "Dun\u00b7keln", "schlei\u00b7chen", ",", "Die\u00b7ben", "gleich", ",", "nach", "Beu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "APPRART", "ADJA", "VVINF", "$,", "PDAT", "ADV", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Doch ihr seid ja viel zu heil'ge, viel zu ehrenfeste Leute!", "tokens": ["Doch", "ihr", "seid", "ja", "viel", "zu", "heil'\u00b7ge", ",", "viel", "zu", "eh\u00b7ren\u00b7fes\u00b7te", "Leu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "APPR", "ADJA", "$,", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "W\u00e4rt ihr nicht so klug und weise, schient ihr mir beinah' zu sein", "tokens": ["W\u00e4rt", "ihr", "nicht", "so", "klug", "und", "wei\u00b7se", ",", "schient", "ihr", "mir", "bei\u00b7nah'", "zu", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "KON", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "ADV", "PTKZU", "VAINF"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Narren, die Ber\u00fchrung scheuen, gl\u00e4sern w\u00e4hnend Stei\u00df und Bein,", "tokens": ["Nar\u00b7ren", ",", "die", "Be\u00b7r\u00fch\u00b7rung", "scheu\u00b7en", ",", "gl\u00e4\u00b7sern", "w\u00e4h\u00b7nend", "Stei\u00df", "und", "Bein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVINF", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Thoren, die den ganzen Fr\u00fchling aus dem Lande wollen jagen,", "tokens": ["Tho\u00b7ren", ",", "die", "den", "gan\u00b7zen", "Fr\u00fch\u00b7ling", "aus", "dem", "Lan\u00b7de", "wol\u00b7len", "ja\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "F\u00fcrchtend, eine Bl\u00fcthenknospe k\u00f6nn' im Fallen sie erschlagen!", "tokens": ["F\u00fcrch\u00b7tend", ",", "ei\u00b7ne", "Bl\u00fc\u00b7then\u00b7knos\u00b7pe", "k\u00f6nn'", "im", "Fal\u00b7len", "sie", "er\u00b7schla\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "VMFIN", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "W\u00e4rt ihr nicht so reich und m\u00e4chtig, sternbesetzt und sammtbedeckt,", "tokens": ["W\u00e4rt", "ihr", "nicht", "so", "reich", "und", "m\u00e4ch\u00b7tig", ",", "stern\u00b7be\u00b7setzt", "und", "sammt\u00b7be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "KON", "ADJD", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "M\u00fc\u00dft' ich euch f\u00fcr Bettler halten, die das Tageslicht erschreckt,", "tokens": ["M\u00fc\u00dft'", "ich", "euch", "f\u00fcr", "Bett\u00b7ler", "hal\u00b7ten", ",", "die", "das", "Ta\u00b7ges\u00b7licht", "er\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Weil's durch schlechtgeflickte Fetzen ihre Bl\u00f6\u00dfen l\u00e4\u00dft erblicken,", "tokens": ["Weil's", "durch", "schlecht\u00b7ge\u00b7flick\u00b7te", "Fet\u00b7zen", "ih\u00b7re", "Bl\u00f6\u00b7\u00dfen", "l\u00e4\u00dft", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Oder gar vielleicht als Brandmal einen Pranger auf dem R\u00fccken!", "tokens": ["O\u00b7der", "gar", "viel\u00b7leicht", "als", "Brand\u00b7mal", "ei\u00b7nen", "Pran\u00b7ger", "auf", "dem", "R\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "KOUS", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Sagt's heraus, wohin soll's f\u00fchren? welches mag das Ziel euch sein?", "tokens": ["Sagt's", "he\u00b7raus", ",", "wo\u00b7hin", "soll's", "f\u00fch\u00b7ren", "?", "wel\u00b7ches", "mag", "das", "Ziel", "euch", "sein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PWAV", "PIS", "VVINF", "$.", "PWS", "VMFIN", "ART", "NN", "PPER", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "K\u00f6nnt ihr Red' und Antwort stehen? \u2013 o beim Himmel, nein, o nein!", "tokens": ["K\u00f6nnt", "ihr", "Red'", "und", "Ant\u00b7wort", "ste\u00b7hen", "?", "\u2013", "o", "beim", "Him\u00b7mel", ",", "nein", ",", "o", "nein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "KON", "NN", "VVINF", "$.", "$(", "FM", "APPRART", "NN", "$,", "PTKANT", "$,", "FM", "PTKANT", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Doch f\u00fcrwahr, ich kann's statt eurer! Will der Zukunft Bild entrollen,", "tokens": ["Doch", "f\u00fcr\u00b7wahr", ",", "ich", "kann's", "statt", "eu\u00b7rer", "!", "Will", "der", "Zu\u00b7kunft", "Bild", "ent\u00b7rol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VMFIN", "APPR", "PPOSAT", "$.", "VMFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wie ihr's formet, wenn's nicht fr\u00fcher gute G\u00f6tter wenden wollen!", "tokens": ["Wie", "ih\u00b7r's", "for\u00b7met", ",", "wenn's", "nicht", "fr\u00fc\u00b7her", "gu\u00b7te", "G\u00f6t\u00b7ter", "wen\u00b7den", "wol\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "KOUS", "PTKNEG", "ADJD", "ADJA", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Wir sind alle l\u00e4ngst gestorben, schlummernd in den S\u00e4rgen tief,", "tokens": ["Wir", "sind", "al\u00b7le", "l\u00e4ngst", "ge\u00b7stor\u00b7ben", ",", "schlum\u00b7mernd", "in", "den", "S\u00e4r\u00b7gen", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "VVPP", "$,", "ADJD", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "W\u00e4hrend \u00fcber unsre Gr\u00e4ber l\u00e4ngst ein neu Geschlecht schon lief,", "tokens": ["W\u00e4h\u00b7rend", "\u00fc\u00b7ber", "uns\u00b7re", "Gr\u00e4\u00b7ber", "l\u00e4ngst", "ein", "neu", "Ge\u00b7schlecht", "schon", "lief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "ADV", "ART", "ADJD", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Offnen Ohrs f\u00fcr Lug der Heuchler, Tagesscheue in den Blicken,", "tokens": ["Off\u00b7nen", "Ohrs", "f\u00fcr", "Lug", "der", "Heuch\u00b7ler", ",", "Ta\u00b7ges\u00b7scheu\u00b7e", "in", "den", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NE", "ART", "NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "F\u00fcr die Lasten seiner Herren gut gebogen seinen R\u00fccken.", "tokens": ["F\u00fcr", "die", "Las\u00b7ten", "sei\u00b7ner", "Her\u00b7ren", "gut", "ge\u00b7bo\u00b7gen", "sei\u00b7nen", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "ADJD", "VVPP", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Seiner F\u00fcrsten Zepter formte sich zum Weihbrunnsprengel um,", "tokens": ["Sei\u00b7ner", "F\u00fcrs\u00b7ten", "Zep\u00b7ter", "form\u00b7te", "sich", "zum", "Weih\u00b7brunn\u00b7spren\u00b7gel", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und ihr Purpur, der verschw\u00e4rzte sich zum m\u00f6nch'schen Pallium;", "tokens": ["Und", "ihr", "Pur\u00b7pur", ",", "der", "ver\u00b7schw\u00e4rz\u00b7te", "sich", "zum", "m\u00f6n\u00b7ch'\u00b7schen", "Pal\u00b7li\u00b7um", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "--+---+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Aus den alten Tagen mochten nur die Weihrauchf\u00e4sser bleiben,", "tokens": ["Aus", "den", "al\u00b7ten", "Ta\u00b7gen", "moch\u00b7ten", "nur", "die", "Weih\u00b7rauch\u00b7f\u00e4s\u00b7ser", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Die noch immer, lustig qualmend, obligate Wolken treiben.", "tokens": ["Die", "noch", "im\u00b7mer", ",", "lus\u00b7tig", "qual\u00b7mend", ",", "ob\u00b7li\u00b7ga\u00b7te", "Wol\u00b7ken", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "$,", "ADJD", "VVPP", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Pressen kennt man nicht im Lande, wenn auch Bengel wohl bekannt,", "tokens": ["Pres\u00b7sen", "kennt", "man", "nicht", "im", "Lan\u00b7de", ",", "wenn", "auch", "Ben\u00b7gel", "wohl", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PTKNEG", "APPRART", "NN", "$,", "KOUS", "ADV", "NE", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und vom Drucke gar wei\u00df Niemand, h\u00f6chstens nur das Volk und Land;", "tokens": ["Und", "vom", "Dru\u00b7cke", "gar", "wei\u00df", "Nie\u00b7mand", ",", "h\u00f6chs\u00b7tens", "nur", "das", "Volk", "und", "Land", ";"]